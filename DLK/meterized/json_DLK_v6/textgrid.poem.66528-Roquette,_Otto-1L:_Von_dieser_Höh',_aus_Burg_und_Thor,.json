{"textgrid.poem.66528": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dieser H\u00f6h', aus Burg und Thor,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dieser H\u00f6h', aus Burg und Thor,", "tokens": ["Von", "die\u00b7ser", "H\u00f6h'", ",", "aus", "Burg", "und", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Sang und Kreuzesfahnen,", "tokens": ["Mit", "Sang", "und", "Kreu\u00b7zes\u00b7fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zog einst die reisige Schaar hervor,", "tokens": ["Zog", "einst", "die", "rei\u00b7si\u00b7ge", "Schaar", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Geweiht zu fernen Bahnen.", "tokens": ["Ge\u00b7weiht", "zu", "fer\u00b7nen", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie zogen zu des Kaisers Heer.", "tokens": ["Sie", "zo\u00b7gen", "zu", "des", "Kai\u00b7sers", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott schenk' euch Gl\u00fcck und Wiederkehr!", "tokens": ["Gott", "schenk'", "euch", "Gl\u00fcck", "und", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So klang's mit bangem Ahnen.", "tokens": ["So", "klang's", "mit", "ban\u00b7gem", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu dieser H\u00f6h' die Kunde drang", "tokens": ["Zu", "die\u00b7ser", "H\u00f6h'", "die", "Kun\u00b7de", "drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Siegen, hart erworben,", "tokens": ["Von", "Sie\u00b7gen", ",", "hart", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Mannen, die im Kampfesdrang", "tokens": ["Von", "Man\u00b7nen", ",", "die", "im", "Kamp\u00b7fes\u00b7drang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr's heilge Land gestorben.", "tokens": ["F\u00fcr's", "heil\u00b7ge", "Land", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die edle Fraue ging herf\u00fcr:", "tokens": ["Die", "ed\u00b7le", "Frau\u00b7e", "ging", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Thut auf, thut auf die Klosterth\u00fcr,", "tokens": ["Thut", "auf", ",", "thut", "auf", "die", "Klos\u00b7tert\u00b7h\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Gl\u00fcck ist all verdorben!", "tokens": ["Mein", "Gl\u00fcck", "ist", "all", "ver\u00b7dor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf dieser H\u00f6h' das Dach zerfiel,", "tokens": ["Auf", "die\u00b7ser", "H\u00f6h'", "das", "Dach", "zer\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tr\u00fcmmer ruhn im Sande,", "tokens": ["Die", "Tr\u00fcm\u00b7mer", "ruhn", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und V\u00f6gel halten freies Spiel", "tokens": ["Und", "V\u00f6\u00b7gel", "hal\u00b7ten", "frei\u00b7es", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Blumen am Mauerrande.", "tokens": ["Und", "Blu\u00b7men", "am", "Mau\u00b7er\u00b7ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was hier verging in Gl\u00fcck und Qual,", "tokens": ["Was", "hier", "ver\u00b7ging", "in", "Gl\u00fcck", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Liedern weht's hinab zu Thal,", "tokens": ["In", "Lie\u00b7dern", "weht's", "hin\u00b7ab", "zu", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und klingt durch alle Lande.", "tokens": ["Und", "klingt", "durch", "al\u00b7le", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}