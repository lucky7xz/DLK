{"dta.poem.23355": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unfruchtbar.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du m\u00f6chtest Allen Alles sein,", "tokens": ["Du", "m\u00f6ch\u00b7test", "Al\u00b7len", "Al\u00b7les", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Conservativ und liberal,", "tokens": ["Con\u00b7ser\u00b7va\u00b7tiv", "und", "li\u00b7be\u00b7ral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aristokratisch, radical,", "tokens": ["A\u00b7ris\u00b7to\u00b7kra\u00b7tisch", ",", "ra\u00b7di\u00b7cal", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und demagogisch auch einmal.", "tokens": ["Und", "de\u00b7ma\u00b7go\u00b7gisch", "auch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du m\u00f6chtest Allen Alles sein!", "tokens": ["Du", "m\u00f6ch\u00b7test", "Al\u00b7len", "Al\u00b7les", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PIS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4rst du ein Esel oder Pferd,", "tokens": ["W\u00e4rst", "du", "ein", "E\u00b7sel", "o\u00b7der", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "So w\u00e4rst du \u00fcberall begehrt \u2014", "tokens": ["So", "w\u00e4rst", "du", "\u00fc\u00b7be\u00b7rall", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Maulesel sind zur Zucht nichts werth.", "tokens": ["Mau\u00b7le\u00b7sel", "sind", "zur", "Zucht", "nichts", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "PIS", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}