{"textgrid.poem.33431": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einsam sitzt in seiner Kammer,", "tokens": ["Wer", "ein\u00b7sam", "sitzt", "in", "sei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwere, bittre Thr\u00e4nen weint,", "tokens": ["Und", "schwe\u00b7re", ",", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wem nur gef\u00e4rbt von Noth und Jammer", "tokens": ["Wem", "nur", "ge\u00b7f\u00e4rbt", "von", "Noth", "und", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nachbarschaft umher erscheint;", "tokens": ["Die", "Nach\u00b7bar\u00b7schaft", "um\u00b7her", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer in das Bild vergangner Zeiten", "tokens": ["Wer", "in", "das", "Bild", "ver\u00b7gang\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie tief in einen Abgrund sieht,", "tokens": ["Wie", "tief", "in", "ei\u00b7nen", "Ab\u00b7grund", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In welchen ihn von allen Seiten", "tokens": ["In", "wel\u00b7chen", "ihn", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein s\u00fc\u00dfes Weh hinunter zieht; \u2013", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "Weh", "hin\u00b7un\u00b7ter", "zieht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPO", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist, als l\u00e4gen Wundersch\u00e4tze", "tokens": ["Es", "ist", ",", "als", "l\u00e4\u00b7gen", "Wun\u00b7der\u00b7sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da unten f\u00fcr ihn aufgeh\u00e4uft,", "tokens": ["Da", "un\u00b7ten", "f\u00fcr", "ihn", "auf\u00b7ge\u00b7h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach deren Schlo\u00df in wilder Hetze", "tokens": ["Nach", "de\u00b7ren", "Schlo\u00df", "in", "wil\u00b7der", "Het\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit athemloser Brust er greift.", "tokens": ["Mit", "at\u00b7hem\u00b7lo\u00b7ser", "Brust", "er", "greift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Zukunft liegt in \u00f6der D\u00fcrre", "tokens": ["Die", "Zu\u00b7kunft", "liegt", "in", "\u00f6\u00b7der", "D\u00fcr\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entsetzlich lang und bang vor ihm \u2013", "tokens": ["Ent\u00b7setz\u00b7lich", "lang", "und", "bang", "vor", "ihm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schweift umher, allein und irre,", "tokens": ["Er", "schweift", "um\u00b7her", ",", "al\u00b7lein", "und", "ir\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sucht sich selbst mit Ungest\u00fcm.", "tokens": ["Und", "sucht", "sich", "selbst", "mit", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich fall' ihm weinend in die Arme:", "tokens": ["Ich", "fall'", "ihm", "wei\u00b7nend", "in", "die", "Ar\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch mir war einst, wie dir, zu Muth,", "tokens": ["Auch", "mir", "war", "einst", ",", "wie", "dir", ",", "zu", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ich genas von meinem Harme,", "tokens": ["Doch", "ich", "ge\u00b7nas", "von", "mei\u00b7nem", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00df nun, wo man ewig ruht.", "tokens": ["Und", "wei\u00df", "nun", ",", "wo", "man", "e\u00b7wig", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dich mu\u00df, wie mich ein Wesen tr\u00f6sten,", "tokens": ["Dich", "mu\u00df", ",", "wie", "mich", "ein", "We\u00b7sen", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das innig liebte, litt und starb;", "tokens": ["Das", "in\u00b7nig", "lieb\u00b7te", ",", "litt", "und", "starb", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das selbst f\u00fcr die, die ihm am wehsten", "tokens": ["Das", "selbst", "f\u00fcr", "die", ",", "die", "ihm", "am", "wehs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gethan, mit tausend Freuden starb.", "tokens": ["Ge\u00b7than", ",", "mit", "tau\u00b7send", "Freu\u00b7den", "starb", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er starb, und dennoch alle Tage", "tokens": ["Er", "starb", ",", "und", "den\u00b7noch", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vernimmst du seine Lieb' und ihn,", "tokens": ["Ver\u00b7nimmst", "du", "sei\u00b7ne", "Lieb'", "und", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kannst getrost in jeder Lage", "tokens": ["Und", "kannst", "ge\u00b7trost", "in", "je\u00b7der", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn z\u00e4rtlich in die Arme ziehn.", "tokens": ["Ihn", "z\u00e4rt\u00b7lich", "in", "die", "Ar\u00b7me", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit ihm kommt neues Blut und Leben", "tokens": ["Mit", "ihm", "kommt", "neu\u00b7es", "Blut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dein erstorbenes Gebein \u2013", "tokens": ["In", "dein", "ers\u00b7tor\u00b7be\u00b7nes", "Ge\u00b7bein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wenn du ihm dein Herz gegeben,", "tokens": ["Und", "wenn", "du", "ihm", "dein", "Herz", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist auch seines ewig dein.", "tokens": ["So", "ist", "auch", "sei\u00b7nes", "e\u00b7wig", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was du verlohrst, hat er gefunden;", "tokens": ["Was", "du", "ver\u00b7lohrst", ",", "hat", "er", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du triffst bey ihm, was du geliebt:", "tokens": ["Du", "triffst", "bey", "ihm", ",", "was", "du", "ge\u00b7liebt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ewig bleibt mit dir verbunden,", "tokens": ["Und", "e\u00b7wig", "bleibt", "mit", "dir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was seine Hand dir wiedergiebt.", "tokens": ["Was", "sei\u00b7ne", "Hand", "dir", "wie\u00b7der\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}