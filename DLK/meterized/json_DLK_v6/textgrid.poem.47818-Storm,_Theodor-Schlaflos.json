{"textgrid.poem.47818": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schlaflos", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Tr\u00e4umen in \u00c4ngsten bin ich erwacht;", "tokens": ["Aus", "Tr\u00e4u\u00b7men", "in", "\u00c4ngs\u00b7ten", "bin", "ich", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was singt doch die Lerche so tief in der Nacht!", "tokens": ["Was", "singt", "doch", "die", "Ler\u00b7che", "so", "tief", "in", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der Tag ist gegangen, der Morgen ist fern,", "tokens": ["Der", "Tag", "ist", "ge\u00b7gan\u00b7gen", ",", "der", "Mor\u00b7gen", "ist", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aufs Kissen hernieder scheinen die Stern'.", "tokens": ["Aufs", "Kis\u00b7sen", "her\u00b7nie\u00b7der", "schei\u00b7nen", "die", "Stern'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und immer h\u00f6r ich den Lerchengesang;", "tokens": ["Und", "im\u00b7mer", "h\u00f6r", "ich", "den", "Ler\u00b7chen\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O Stimme des Tages, mein Herz ist bang.", "tokens": ["O", "Stim\u00b7me", "des", "Ta\u00b7ges", ",", "mein", "Herz", "ist", "bang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}