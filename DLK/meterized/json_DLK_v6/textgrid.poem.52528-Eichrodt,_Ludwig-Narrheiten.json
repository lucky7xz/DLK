{"textgrid.poem.52528": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Narrheiten", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wornach steht mir der Sinn?", "tokens": ["Wor\u00b7nach", "steht", "mir", "der", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zerr\u00fcttet ist mein Denken,", "tokens": ["Zer\u00b7r\u00fct\u00b7tet", "ist", "mein", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All meine Tr\u00e4ume lenken", "tokens": ["All", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einen Punkt nur hin,", "tokens": ["Auf", "ei\u00b7nen", "Punkt", "nur", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf ihren Mund, den s\u00fc\u00dfen,", "tokens": ["Auf", "ih\u00b7ren", "Mund", ",", "den", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und den zu k\u00fcssen!", "tokens": ["Und", "den", "zu", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Entweiche, Phantasie!", "tokens": ["Ent\u00b7wei\u00b7che", ",", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du stolze, tiefbesch\u00e4mte!", "tokens": ["Du", "stol\u00b7ze", ",", "tief\u00b7be\u00b7sch\u00e4m\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schwinge dir, denn nie", "tokens": ["Die", "Schwin\u00b7ge", "dir", ",", "denn", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erschufst du Reiz, so s\u00fc\u00dfen,", "tokens": ["Er\u00b7schufst", "du", "Reiz", ",", "so", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ha! sie zu k\u00fcssen!", "tokens": ["Ha", "!", "sie", "zu", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Seit ich ihr Antlitz sah,", "tokens": ["Seit", "ich", "ihr", "Ant\u00b7litz", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das wonnige, das liebe,", "tokens": ["Das", "won\u00b7ni\u00b7ge", ",", "das", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das unaussprechlich liebe,", "tokens": ["Das", "un\u00b7aus\u00b7sprech\u00b7lich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht mir der Wahnsinn nah \u2013", "tokens": ["Steht", "mir", "der", "Wahn\u00b7sinn", "nah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Antlitz, mit dem s\u00fc\u00dfen,", "tokens": ["O", "Ant\u00b7litz", ",", "mit", "dem", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Mund zum k\u00fcssen.", "tokens": ["Dem", "Mund", "zum", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "So schwebt mir dort und hier", "tokens": ["So", "schwebt", "mir", "dort", "und", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Zaubermund vor Augen,", "tokens": ["Der", "Zau\u00b7ber\u00b7mund", "vor", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will Hirn und Herz mir saugen", "tokens": ["Will", "Hirn", "und", "Herz", "mir", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles Blut aus mir:", "tokens": ["Und", "al\u00b7les", "Blut", "aus", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Raserei, den s\u00fc\u00dfen", "tokens": ["O", "Ra\u00b7se\u00b7rei", ",", "den", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mund nicht zu k\u00fcssen!", "tokens": ["Mund", "nicht", "zu", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Treff ichs nicht bald einmal", "tokens": ["Treff", "ichs", "nicht", "bald", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Lippen rasch zu kosten,", "tokens": ["Die", "Lip\u00b7pen", "rasch", "zu", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So soll mein Wille rosten", "tokens": ["So", "soll", "mein", "Wil\u00b7le", "ros\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ein entehrter Stahl.", "tokens": ["Wie", "ein", "ent\u00b7ehr\u00b7ter", "Stahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich st\u00fcrbe gern, den s\u00fc\u00dfen", "tokens": ["Ich", "st\u00fcr\u00b7be", "gern", ",", "den", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Mund zu k\u00fcssen!", "tokens": ["Den", "Mund", "zu", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}