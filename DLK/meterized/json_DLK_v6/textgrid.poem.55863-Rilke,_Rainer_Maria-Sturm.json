{"textgrid.poem.55863": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Sturm", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Wolken, von St\u00fcrmen geschlagen,", "tokens": ["Wenn", "die", "Wol\u00b7ken", ",", "von", "St\u00fcr\u00b7men", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "jagen:", "tokens": ["ja\u00b7gen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Himmel von hundert Tagen", "tokens": ["Him\u00b7mel", "von", "hun\u00b7dert", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u00fcber einem einzigen Tag \u2013:", "tokens": ["\u00fc\u00b7ber", "ei\u00b7nem", "ein\u00b7zi\u00b7gen", "Tag", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Dann f\u00fchl ich dich, Hetman, von fern", "tokens": ["Dann", "f\u00fchl", "ich", "dich", ",", "Het\u00b7man", ",", "von", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NE", "$,", "APPR", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "(der du deine Kosaken gern", "tokens": ["(", "der", "du", "dei\u00b7ne", "Ko\u00b7sa\u00b7ken", "gern"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "PPER", "PPOSAT", "NN", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "zu dem gr\u00f6\u00dfesten Herrn", "tokens": ["zu", "dem", "gr\u00f6\u00b7\u00dfes\u00b7ten", "Herrn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "f\u00fchren wolltest).", "tokens": ["f\u00fch\u00b7ren", "woll\u00b7test", ")", "."], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVINF", "VMFIN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Deinen waagrechten Nacken", "tokens": ["Dei\u00b7nen", "waag\u00b7rech\u00b7ten", "Na\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "f\u00fchl ich, Mazeppa.", "tokens": ["f\u00fchl", "ich", ",", "Ma\u00b7zep\u00b7pa", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dann bin auch ich an das rasende Rennen", "tokens": ["Dann", "bin", "auch", "ich", "an", "das", "ra\u00b7sen\u00b7de", "Ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "eines rauchenden R\u00fcckens gebunden;", "tokens": ["ei\u00b7nes", "rau\u00b7chen\u00b7den", "R\u00fc\u00b7ckens", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "alle Dinge sind mir verschwunden,", "tokens": ["al\u00b7le", "Din\u00b7ge", "sind", "mir", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "nur die Himmel kann ich erkennen:", "tokens": ["nur", "die", "Him\u00b7mel", "kann", "ich", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00dcberdunkelt und \u00fcberschienen", "tokens": ["\u00dc\u00b7berd\u00b7un\u00b7kelt", "und", "\u00fc\u00b7bersc\u00b7hie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "lieg ich flach unter ihnen,", "tokens": ["lieg", "ich", "flach", "un\u00b7ter", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wie Ebenen liegen;", "tokens": ["wie", "E\u00b7be\u00b7nen", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "meine Augen sind offen wie Teiche,", "tokens": ["mei\u00b7ne", "Au\u00b7gen", "sind", "of\u00b7fen", "wie", "Tei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "und in ihnen fl\u00fcchtet das gleiche", "tokens": ["und", "in", "ih\u00b7nen", "fl\u00fcch\u00b7tet", "das", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Fliegen.", "tokens": ["Flie\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}