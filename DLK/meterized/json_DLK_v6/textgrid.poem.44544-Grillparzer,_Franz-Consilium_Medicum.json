{"textgrid.poem.44544": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Consilium Medicum", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Poesie war krank.", "tokens": ["Frau", "Poe\u00b7sie", "war", "krank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Verwitwet schon seit manchem Jahr,", "tokens": ["Ver\u00b7wit\u00b7wet", "schon", "seit", "man\u00b7chem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wuchs scheinbar st\u00fcndlich die Gefahr.", "tokens": ["Wuchs", "schein\u00b7bar", "st\u00fcnd\u00b7lich", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Stirne hei\u00df,", "tokens": ["Die", "Stir\u00b7ne", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Zunge wei\u00df,", "tokens": ["Die", "Zun\u00b7ge", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Haut bald Frost und bald in Schwei\u00df,", "tokens": ["Die", "Haut", "bald", "Frost", "und", "bald", "in", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im ganzen Leib ein schmerzlich Jucken,", "tokens": ["Im", "gan\u00b7zen", "Leib", "ein", "schmerz\u00b7lich", "Ju\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Kr\u00e4mpfen alle Nerven zucken.", "tokens": ["Von", "Kr\u00e4mp\u00b7fen", "al\u00b7le", "Ner\u00b7ven", "zu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Obschon noch r\u00fcstig und nicht alt,", "tokens": ["Ob\u00b7schon", "noch", "r\u00fcs\u00b7tig", "und", "nicht", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Schien nah des Todes Nachtgewalt.", "tokens": ["Schien", "nah", "des", "To\u00b7des", "Nacht\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doctores kommen von allen Seiten,", "tokens": ["Doc\u00b7to\u00b7res", "kom\u00b7men", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die erst sich begr\u00fc\u00dfen und dann bestreiten,", "tokens": ["Die", "erst", "sich", "be\u00b7gr\u00fc\u00b7\u00dfen", "und", "dann", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVINF", "KON", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Hippokratisch,", "tokens": ["Hip\u00b7po\u00b7kra\u00b7tisch", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Hom\u00f6opathisch,", "tokens": ["Ho\u00b7m\u00f6o\u00b7pa\u00b7thisch", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Allopathisch,", "tokens": ["Al\u00b7lo\u00b7pa\u00b7thisch", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Hydropathisch,", "tokens": ["Hyd\u00b7ro\u00b7pa\u00b7thisch", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Antipathisch,", "tokens": ["An\u00b7ti\u00b7pa\u00b7thisch", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Philosophisch gebr\u00fcstet,", "tokens": ["Phi\u00b7lo\u00b7so\u00b7phisch", "ge\u00b7br\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Historisch ger\u00fcstet,", "tokens": ["His\u00b7to\u00b7risch", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Dogmatisch, kritisch,", "tokens": ["Dog\u00b7ma\u00b7tisch", ",", "kri\u00b7tisch", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Klassisch, britisch;", "tokens": ["Klas\u00b7sisch", ",", "bri\u00b7tisch", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Schreiben Rezepte in langen Zeilen,", "tokens": ["Schrei\u00b7ben", "Re\u00b7zep\u00b7te", "in", "lan\u00b7gen", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "Umsonst! die Kranke war nicht zu heilen.", "tokens": ["Um\u00b7sonst", "!", "die", "Kran\u00b7ke", "war", "nicht", "zu", "hei\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Da kam ein Bader vom Land herein,", "tokens": ["Da", "kam", "ein", "Ba\u00b7der", "vom", "Land", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Besieht die Kranke beim Tagesschein,", "tokens": ["Be\u00b7sieht", "die", "Kran\u00b7ke", "beim", "Ta\u00b7ges\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Erforscht den Puls, die Zunge auch,", "tokens": ["Er\u00b7forscht", "den", "Puls", ",", "die", "Zun\u00b7ge", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Bef\u00fchlt die Weichen und den Bauch,", "tokens": ["Be\u00b7f\u00fchlt", "die", "Wei\u00b7chen", "und", "den", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Zuletzt hebt er mit Lachen an:", "tokens": ["Zu\u00b7letzt", "hebt", "er", "mit", "La\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Die Wissenschaft hier wenig kann,", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "hier", "we\u00b7nig", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}