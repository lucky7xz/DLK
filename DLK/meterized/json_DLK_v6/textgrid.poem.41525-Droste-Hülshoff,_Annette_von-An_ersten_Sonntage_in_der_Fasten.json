{"textgrid.poem.41525": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An ersten Sonntage in der Fasten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsprich, da\u00df diese Steine Brode werden!", "tokens": ["\u00bb", "sprich", ",", "da\u00df", "die\u00b7se", "Stei\u00b7ne", "Bro\u00b7de", "wer\u00b7den", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "KOUS", "PDAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df dich deine Engel niedertragen!", "tokens": ["La\u00df", "dich", "dei\u00b7ne", "En\u00b7gel", "nie\u00b7der\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh die Reiche dieser ganzen Erden!", "tokens": ["Sieh", "die", "Rei\u00b7che", "die\u00b7ser", "gan\u00b7zen", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Willst du deinem Sch\u00f6pfer nicht entsagen?\u00ab", "tokens": ["Willst", "du", "dei\u00b7nem", "Sch\u00f6p\u00b7fer", "nicht", "ent\u00b7sa\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dunkler Geist, und warst du gleich befangen,", "tokens": ["Dunk\u00b7ler", "Geist", ",", "und", "warst", "du", "gleich", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da du deinen Gott und Herrn versucht,", "tokens": ["Da", "du", "dei\u00b7nen", "Gott", "und", "Herrn", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, in deinen Netzen zahllos hangen", "tokens": ["Ach", ",", "in", "dei\u00b7nen", "Net\u00b7zen", "zahl\u00b7los", "han\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sie, verloren an die t\u00fcck'sche Frucht;", "tokens": ["Sie", ",", "ver\u00b7lo\u00b7ren", "an", "die", "t\u00fc\u00b7ck'\u00b7sche", "Frucht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ehrgeiz, Hoffart, dieser Erde Freuden,", "tokens": ["Ehr\u00b7geiz", ",", "Hof\u00b7fart", ",", "die\u00b7ser", "Er\u00b7de", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00f6tzen, denen teure Seelen sterben \u2013", "tokens": ["G\u00f6t\u00b7zen", ",", "de\u00b7nen", "teu\u00b7re", "See\u00b7len", "ster\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O, mein Gott, la\u00df mich nicht ewig scheiden,", "tokens": ["O", ",", "mein", "Gott", ",", "la\u00df", "mich", "nicht", "e\u00b7wig", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "La\u00df mich selber nicht den Tod erwerben!", "tokens": ["La\u00df", "mich", "sel\u00b7ber", "nicht", "den", "Tod", "er\u00b7wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ganz verwirrt, wei\u00df ich mich nicht zu fassen,", "tokens": ["Ganz", "ver\u00b7wirrt", ",", "wei\u00df", "ich", "mich", "nicht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Drohend schwankt um mich der falsche Grund;", "tokens": ["Dro\u00b7hend", "schwankt", "um", "mich", "der", "fal\u00b7sche", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, der eignen schwachen Kraft gelassen,", "tokens": ["Ach", ",", "der", "eig\u00b7nen", "schwa\u00b7chen", "Kraft", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tret' ich sinnlos in den offnen Schlund!", "tokens": ["Tret'", "ich", "sinn\u00b7los", "in", "den", "off\u00b7nen", "Schlund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jesu mein, zu dir steigt auf mein Flehen,", "tokens": ["Je\u00b7su", "mein", ",", "zu", "dir", "steigt", "auf", "mein", "Fle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der Kreuzesleiter meine Stimme!", "tokens": ["Auf", "der", "Kreu\u00b7zes\u00b7lei\u00b7ter", "mei\u00b7ne", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du ber\u00fchrst die Meere, sie vergehen,", "tokens": ["Du", "be\u00b7r\u00fchrst", "die", "Mee\u00b7re", ",", "sie", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Berge rauchen deinem Grimme,", "tokens": ["Und", "die", "Ber\u00b7ge", "rau\u00b7chen", "dei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch mit tausend Himmelszweigen bl\u00fchet", "tokens": ["Doch", "mit", "tau\u00b7send", "Him\u00b7mels\u00b7zwei\u00b7gen", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dein unendlich Gnadenwort empor:", "tokens": ["Dein", "un\u00b7end\u00b7lich", "Gna\u00b7den\u00b7wort", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "\u00bbdu verl\u00f6schest nicht den Docht der gl\u00fchet", "tokens": ["\u00bb", "du", "ver\u00b7l\u00f6\u00b7schest", "nicht", "den", "Docht", "der", "gl\u00fc\u00b7het"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und zerbrichst nicht das geknickte Rohr.\u00ab", "tokens": ["Und", "zer\u00b7brichst", "nicht", "das", "ge\u00b7knick\u00b7te", "Rohr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Herr, ich bin ein arm und kaum noch gl\u00fchend", "tokens": ["Herr", ",", "ich", "bin", "ein", "arm", "und", "kaum", "noch", "gl\u00fc\u00b7hend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "KON", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "D\u00f6chtlein am Altare deiner Gnade,", "tokens": ["D\u00f6cht\u00b7lein", "am", "Al\u00b7ta\u00b7re", "dei\u00b7ner", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, mich l\u00f6scht ein mattes L\u00fcftchen fliehend,", "tokens": ["Sieh", ",", "mich", "l\u00f6scht", "ein", "mat\u00b7tes", "L\u00fcft\u00b7chen", "flie\u00b7hend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich ein Tropfen von der Welt Gestade!", "tokens": ["Mich", "ein", "Trop\u00b7fen", "von", "der", "Welt", "Ge\u00b7sta\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach, wenn nicht in meinem Herzen bliebe", "tokens": ["Ach", ",", "wenn", "nicht", "in", "mei\u00b7nem", "Her\u00b7zen", "blie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nur ein einzig leuchtend P\u00fcnktlein noch,", "tokens": ["Nur", "ein", "ein\u00b7zig", "leuch\u00b7tend", "P\u00fcnkt\u00b7lein", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VVPP", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Jener hei\u00dfe Funken deiner Liebe,", "tokens": ["Je\u00b7ner", "hei\u00b7\u00dfe", "Fun\u00b7ken", "dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie so ganz erstorben w\u00e4r' ich doch!", "tokens": ["Wie", "so", "ganz", "ers\u00b7tor\u00b7ben", "w\u00e4r'", "ich", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVPP", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Herr, du hast vielleicht noch viel beschlossen", "tokens": ["Herr", ",", "du", "hast", "viel\u00b7leicht", "noch", "viel", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr dies kurze ruhelose Leben,", "tokens": ["F\u00fcr", "dies", "kur\u00b7ze", "ru\u00b7he\u00b7lo\u00b7se", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ob ich soll in Qualen hingegossen,", "tokens": ["Ob", "ich", "soll", "in", "Qua\u00b7len", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ob ich soll in allen Freuden weben:", "tokens": ["Ob", "ich", "soll", "in", "al\u00b7len", "Freu\u00b7den", "we\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Darf ich w\u00e4hlen, und will Lust mich trennen,", "tokens": ["Darf", "ich", "w\u00e4h\u00b7len", ",", "und", "will", "Lust", "mich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KON", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Brenne mich in Leidensflammen rein!", "tokens": ["Bren\u00b7ne", "mich", "in", "Lei\u00b7dens\u00b7flam\u00b7men", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "O, die Not lehrt deinen Namen nennen!", "tokens": ["O", ",", "die", "Not", "lehrt", "dei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Doch die Ehre steht so gern allein.", "tokens": ["Doch", "die", "Eh\u00b7re", "steht", "so", "gern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lauscht vielleicht verborgen eine Spitze", "tokens": ["Lauscht", "viel\u00b7leicht", "ver\u00b7bor\u00b7gen", "ei\u00b7ne", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVPP", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Lob, das mir die Menschen bringen,", "tokens": ["In", "dem", "Lob", ",", "das", "mir", "die", "Men\u00b7schen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich noch die letzte Kraft besitze,", "tokens": ["Da\u00df", "ich", "noch", "die", "letz\u00b7te", "Kraft", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich zu r\u00fchmen, deinen Preis zu singen,", "tokens": ["Dich", "zu", "r\u00fch\u00b7men", ",", "dei\u00b7nen", "Preis", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sind auch hier die Netze aufgeschlagen,", "tokens": ["Sind", "auch", "hier", "die", "Net\u00b7ze", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo der Mund zu deiner Ehre schafft!", "tokens": ["Wo", "der", "Mund", "zu", "dei\u00b7ner", "Eh\u00b7re", "schafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und ich w\u00e4r' zu schwach das Lob zu tragen,", "tokens": ["Und", "ich", "w\u00e4r'", "zu", "schwach", "das", "Lob", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKA", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und es br\u00e4che meine letzte Kraft!", "tokens": ["Und", "es", "br\u00e4\u00b7che", "mei\u00b7ne", "letz\u00b7te", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Herr du wei\u00dft, wie tr\u00fcb in meiner Seele,", "tokens": ["Herr", "du", "wei\u00dft", ",", "wie", "tr\u00fcb", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie verloren die Gebete stehen,", "tokens": ["Wie", "ver\u00b7lo\u00b7ren", "die", "Ge\u00b7be\u00b7te", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich m\u00f6chte wie um gro\u00dfe Fehle", "tokens": ["Da\u00df", "ich", "m\u00f6ch\u00b7te", "wie", "um", "gro\u00b7\u00dfe", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "B\u00fc\u00dfen, da\u00df ich es gewagt zu flehen,", "tokens": ["B\u00fc\u00b7\u00dfen", ",", "da\u00df", "ich", "es", "ge\u00b7wagt", "zu", "fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mein Gebet ist wie von einem Toten,", "tokens": ["Mein", "Ge\u00b7bet", "ist", "wie", "von", "ei\u00b7nem", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist ein kalter Dunst vor deinem Thron;", "tokens": ["Ist", "ein", "kal\u00b7ter", "Dunst", "vor", "dei\u00b7nem", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Herr, du hast es selber mir geboten!", "tokens": ["Herr", ",", "du", "hast", "es", "sel\u00b7ber", "mir", "ge\u00b7bo\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und du h\u00f6rtest den verlornen Sohn.", "tokens": ["Und", "du", "h\u00f6r\u00b7test", "den", "ver\u00b7lor\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "La\u00df mich, Herr, es immerdar empfinden,", "tokens": ["La\u00df", "mich", ",", "Herr", ",", "es", "im\u00b7mer\u00b7dar", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ich tief gesunken unter allen,", "tokens": ["Wie", "ich", "tief", "ge\u00b7sun\u00b7ken", "un\u00b7ter", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "APPR", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df mich nicht zu allen meinen S\u00fcnden", "tokens": ["La\u00df", "mich", "nicht", "zu", "al\u00b7len", "mei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Noch in frevelhafte Torheit fallen!", "tokens": ["Noch", "in", "fre\u00b7vel\u00b7haf\u00b7te", "Tor\u00b7heit", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meine Pflichten stehen \u00fcber vielen,", "tokens": ["Mei\u00b7ne", "Pflich\u00b7ten", "ste\u00b7hen", "\u00fc\u00b7ber", "vie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Unter allen meiner Tugend Kraft,", "tokens": ["Un\u00b7ter", "al\u00b7len", "mei\u00b7ner", "Tu\u00b7gend", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, ich mu\u00dfte wohl die Kraft verspielen", "tokens": ["Ach", ",", "ich", "mu\u00df\u00b7te", "wohl", "die", "Kraft", "ver\u00b7spie\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In dem Spiel mit S\u00fcnd' und Leidenschaft!", "tokens": ["In", "dem", "Spiel", "mit", "S\u00fcnd'", "und", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Willst du mehr der Erdeng\u00fcter schenken,", "tokens": ["Willst", "du", "mehr", "der", "Er\u00b7den\u00b7g\u00fc\u00b7ter", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Soll ich die besessen