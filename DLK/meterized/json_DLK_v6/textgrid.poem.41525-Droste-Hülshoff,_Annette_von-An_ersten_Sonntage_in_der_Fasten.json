, "line.8": {"text": "In Entbehrung teile f\u00fcr und f\u00fcr.", "tokens": ["In", "Ent\u00b7beh\u00b7rung", "tei\u00b7le", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Selber kann ich diesen Kampf nicht wagen,", "tokens": ["Sel\u00b7ber", "kann", "ich", "die\u00b7sen", "Kampf", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Gnaden hab' ich all' verloren,", "tokens": ["Dei\u00b7ne", "Gna\u00b7den", "hab'", "ich", "all'", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn du mich verl\u00e4\u00dft, ich darf nicht klagen,", "tokens": ["Wenn", "du", "mich", "ver\u00b7l\u00e4\u00dft", ",", "ich", "darf", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hab' ich doch die Finsternis erkoren,", "tokens": ["Hab'", "ich", "doch", "die", "Fins\u00b7ter\u00b7nis", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hoffart, Ehrgeiz, dieser Erde Freuden;", "tokens": ["Hof\u00b7fart", ",", "Ehr\u00b7geiz", ",", "die\u00b7ser", "Er\u00b7de", "Freu\u00b7den", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O mein Jesu, ziehe mich zur\u00fcck!", "tokens": ["O", "mein", "Je\u00b7su", ",", "zie\u00b7he", "mich", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, was hab' ich denn um sie zu meiden,", "tokens": ["Ach", ",", "was", "hab'", "ich", "denn", "um", "sie", "zu", "mei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als zu dir den angsterf\u00fcllten Blick!", "tokens": ["Als", "zu", "dir", "den", "angs\u00b7ter\u00b7f\u00fcll\u00b7ten", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}