{"dta.poem.2861": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was giebt es hier, um was des Volks Gedr\u00e4ng sich h\u00e4uft?", "tokens": ["Was", "giebt", "es", "hier", ",", "um", "was", "des", "Volks", "Ge\u00b7dr\u00e4ng", "sich", "h\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "APPR", "PRELS", "ART", "NN", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": " frag's oder warte bis es wieder sich verl\u00e4uft.", "tokens": ["frag's", "o\u00b7der", "war\u00b7te", "bis", "es", "wie\u00b7der", "sich", "ver\u00b7l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPR", "PPER", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn du's dann erf\u00e4hrst, hast du vielleicht erfahren,", "tokens": ["Doch", "wenn", "du's", "dann", "er\u00b7f\u00e4hrst", ",", "hast", "du", "viel\u00b7leicht", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du dein Fragen und dein Warten konntest sparen.", "tokens": ["Da\u00df", "du", "dein", "Fra\u00b7gen", "und", "dein", "War\u00b7ten", "konn\u00b7test", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum lieber geh mit mir vor\u00fcber dem Geschrei,", "tokens": ["Drum", "lie\u00b7ber", "geh", "mit", "mir", "vor\u00b7\u00fc\u00b7ber", "dem", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denk im Stillen, was es wol gewesen sei?", "tokens": ["Und", "denk", "im", "Stil\u00b7len", ",", "was", "es", "wol", "ge\u00b7we\u00b7sen", "sei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PWS", "PPER", "ADV", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir k\u00f6nnen mangerlei Anl\u00e4sse dem Geschrei", "tokens": ["Wir", "k\u00f6n\u00b7nen", "man\u00b7ger\u00b7lei", "An\u00b7l\u00e4s\u00b7se", "dem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erdenken, keinen doch, der viel zu gut nicht sei.", "tokens": ["Er\u00b7den\u00b7ken", ",", "kei\u00b7nen", "doch", ",", "der", "viel", "zu", "gut", "nicht", "sei", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "ADV", "$,", "PRELS", "ADV", "PTKA", "ADJD", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}