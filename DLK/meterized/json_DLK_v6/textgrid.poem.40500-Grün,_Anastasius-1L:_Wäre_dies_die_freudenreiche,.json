{"textgrid.poem.40500": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4re dies die freudenreiche,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4re dies die freudenreiche,", "tokens": ["W\u00e4\u00b7re", "dies", "die", "freu\u00b7den\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolze Meeresk\u00f6nigin,", "tokens": ["Stol\u00b7ze", "Mee\u00b7res\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der ernsten Heldengr\u00f6\u00dfe,", "tokens": ["Mit", "der", "erns\u00b7ten", "Hel\u00b7den\u00b7gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem leichten, heitren Sinn?", "tokens": ["Mit", "dem", "leich\u00b7ten", ",", "hei\u00b7tren", "Sinn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwarze Gondeln im Kanale", "tokens": ["Schwar\u00b7ze", "Gon\u00b7deln", "im", "Ka\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwankend, ohne Liederklang!", "tokens": ["Schwan\u00b7kend", ",", "oh\u00b7ne", "Lie\u00b7der\u00b7klang", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Shifferruf nur st\u00f6hnt bisweilen", "tokens": ["Shif\u00b7fer\u00b7ruf", "nur", "st\u00f6hnt", "bis\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dumpf wie tr\u00e4ger Unkensang.", "tokens": ["Dumpf", "wie", "tr\u00e4\u00b7ger", "Un\u00b7ken\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Marmorbilder nur bewohnen", "tokens": ["Mar\u00b7mor\u00b7bil\u00b7der", "nur", "be\u00b7woh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Pal\u00e4ste, hoch gebaut,", "tokens": ["Die", "Pa\u00b7l\u00e4s\u00b7te", ",", "hoch", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Sinken und Zerfallen", "tokens": ["Und", "ihr", "Sin\u00b7ken", "und", "Zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist darin der eniz'ge Laut.", "tokens": ["Ist", "da\u00b7rin", "der", "e\u00b7ni\u00b7z'\u00b7ge", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Leer vom Volke steht San Marco,", "tokens": ["Leer", "vom", "Vol\u00b7ke", "steht", "San", "Mar\u00b7co", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Gebete Stoff gebricht!", "tokens": ["Der", "Ge\u00b7be\u00b7te", "Stoff", "ge\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klagen will es nicht das V\u00f6lklein,", "tokens": ["Kla\u00b7gen", "will", "es", "nicht", "das", "V\u00f6l\u00b7klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu danken hat es nicht.", "tokens": ["Und", "zu", "dan\u00b7ken", "hat", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Am Altar fungirt der Priester,", "tokens": ["Am", "Al\u00b7tar", "fun\u00b7girt", "der", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ohne Ernst und ohne Sinn;", "tokens": ["Oh\u00b7ne", "Ernst", "und", "oh\u00b7ne", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur damit er's nicht vergesse,", "tokens": ["Nur", "da\u00b7mit", "er's", "nicht", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Murmelt er sein Spr\u00fcchlein hin.", "tokens": ["Mur\u00b7melt", "er", "sein", "Spr\u00fcch\u00b7lein", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "L\u00e4ngst zerschellt im Arsenale", "tokens": ["L\u00e4ngst", "zer\u00b7schellt", "im", "Ar\u00b7se\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fault das alte Dogenschiff,", "tokens": ["Fault", "das", "al\u00b7te", "Do\u00b7gen\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, der eigne alte Hafen", "tokens": ["Ach", ",", "der", "eig\u00b7ne", "al\u00b7te", "Ha\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward ihm Klipp' und Todesriff!", "tokens": ["Ward", "ihm", "Klipp'", "und", "To\u00b7de\u00b7sriff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Venetianer, sagt, was deuten", "tokens": ["Ve\u00b7ne\u00b7ti\u00b7a\u00b7ner", ",", "sagt", ",", "was", "deu\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "PWS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort die hohen Maste drei?", "tokens": ["Dort", "die", "ho\u00b7hen", "Mas\u00b7te", "drei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pflanzet ihr als Vogelscheuchen", "tokens": ["Pflan\u00b7zet", "ihr", "als", "Vo\u00b7gel\u00b7scheu\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor den Dom die Stangen frei?", "tokens": ["Vor", "den", "Dom", "die", "Stan\u00b7gen", "frei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ei, ihr habt doch keine Saaten!", "tokens": ["Ei", ",", "ihr", "habt", "doch", "kei\u00b7ne", "Saa\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr hattet, sind verdorrt!", "tokens": ["Die", "ihr", "hat\u00b7tet", ",", "sind", "ver\u00b7dorrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die allerschlimmsten V\u00f6gel", "tokens": ["Und", "die", "al\u00b7ler\u00b7schlimms\u00b7ten", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheuchten sie euch doch nicht fort;", "tokens": ["Scheuch\u00b7ten", "sie", "euch", "doch", "nicht", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jene V\u00f6gel, die die Augen", "tokens": ["Je\u00b7ne", "V\u00f6\u00b7gel", ",", "die", "die", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurer Freiheit ausgepickt,", "tokens": ["Eu\u00b7rer", "Frei\u00b7heit", "aus\u00b7ge\u00b7pickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr das Schlummerlied gesungen,", "tokens": ["Ihr", "das", "Schlum\u00b7mer\u00b7lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis sie sterbend eingenickt.", "tokens": ["Bis", "sie", "ster\u00b7bend", "ein\u00b7ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In dem eh'rnen Markusl\u00f6wen", "tokens": ["In", "dem", "eh'r\u00b7nen", "Mar\u00b7kus\u00b7l\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War einst Leben, Kraft und Herz:", "tokens": ["War", "einst", "Le\u00b7ben", ",", "Kraft", "und", "Herz", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der k\u00f6nigliche W\u00e4chter", "tokens": ["Doch", "der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt nun todt, ein Aas von Erz!", "tokens": ["Liegt", "nun", "todt", ",", "ein", "Aas", "von", "Erz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "L\u00e4ngst begann ja Adlerherrschaft,", "tokens": ["L\u00e4ngst", "be\u00b7gann", "ja", "Ad\u00b7ler\u00b7herr\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit der alte Leu erlag", "tokens": ["Seit", "der", "al\u00b7te", "Leu", "er\u00b7lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter jenes Frankenadlers", "tokens": ["Un\u00b7ter", "je\u00b7nes", "Fran\u00b7ke\u00b7nad\u00b7lers"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jugendlichem Fl\u00fcgelschlag.", "tokens": ["Ju\u00b7gend\u00b7li\u00b7chem", "Fl\u00fc\u00b7gel\u00b7schlag", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Stumm und \u00f6de Platz und Stra\u00dfen", "tokens": ["Stumm", "und", "\u00f6\u00b7de", "Platz", "und", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Fluthen rings umher,", "tokens": ["Und", "die", "Flut\u00b7hen", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst die Steine reden nimmer", "tokens": ["Selbst", "die", "Stei\u00b7ne", "re\u00b7den", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Menschen l\u00e4ngst nicht mehr!", "tokens": ["Und", "die", "Men\u00b7schen", "l\u00e4ngst", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und doch w\u00fc\u00dft' ich einen Zauber,", "tokens": ["Und", "doch", "w\u00fc\u00dft'", "ich", "ei\u00b7nen", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja ein W\u00f6rtlein nur, gar klein!", "tokens": ["Ja", "ein", "W\u00f6rt\u00b7lein", "nur", ",", "gar", "klein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00e4ch's zur rechten Stund' der Rechte", "tokens": ["Spr\u00e4ch's", "zur", "rech\u00b7ten", "Stund'", "der", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spr\u00e4ng' von diesem Sarg der Stein!", "tokens": ["Spr\u00e4ng'", "von", "die\u00b7sem", "Sarg", "der", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ha, da wirft der Markusl\u00f6we", "tokens": ["Ha", ",", "da", "wirft", "der", "Mar\u00b7kus\u00b7l\u00f6\u00b7we"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seine M\u00e4hne stolz empor,", "tokens": ["Sei\u00b7ne", "M\u00e4h\u00b7ne", "stolz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcttelt wieder k\u00fchn die Fl\u00fcgel", "tokens": ["Sch\u00fct\u00b7telt", "wie\u00b7der", "k\u00fchn", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frei und kr\u00e4ftig, wie zuvor.", "tokens": ["Frei", "und", "kr\u00e4f\u00b7tig", ",", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Dreier K\u00f6nigreiche Flaggen", "tokens": ["Drei\u00b7er", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "Flag\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh'n von jenen Masten her", "tokens": ["Weh'n", "von", "je\u00b7nen", "Mas\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Lied der Gondoliere", "tokens": ["Und", "das", "Lied", "der", "Gon\u00b7do\u00b7lie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt in Ch\u00f6ren \u00fcbers Meer.", "tokens": ["T\u00f6nt", "in", "Ch\u00f6\u00b7re