{"dta.poem.17986": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "229.  Der Weise hat alles gemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise was er hat/ hat alls mit alln gemein/", "tokens": ["Der", "Wei\u00b7se", "was", "er", "hat", "/", "hat", "alls", "mit", "alln", "ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "PPER", "VAFIN", "$(", "VAFIN", "ADV", "APPR", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie da? er sch\u00e4tzet alls/ sich selbst auch nicht f\u00fcr sein.", "tokens": ["Wie", "da", "?", "er", "sch\u00e4t\u00b7zet", "alls", "/", "sich", "selbst", "auch", "nicht", "f\u00fcr", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "PPER", "VVFIN", "ADV", "$(", "PRF", "ADV", "ADV", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}