{"dta.poem.2699": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbende Alcyone  \n  Bey Beerdigung Fr. E. v. S. g. N. den  4.  \n Julii 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eslie\u00df Alcyone/ die Perle keuscher Frauen/", "tokens": ["Es\u00b7lie\u00df", "Al\u00b7cyo\u00b7ne", "/", "die", "Per\u00b7le", "keu\u00b7scher", "Frau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit heissem Seelen-Schmertz den Liebsten von sich hin;", "tokens": ["Mit", "heis\u00b7sem", "See\u00b7len\u00b7Schmertz", "den", "Liebs\u00b7ten", "von", "sich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Furcht/ sie w\u00fcrd ihn nun auffewig nicht mehr schauen/", "tokens": ["Aus", "Furcht", "/", "sie", "w\u00fcrd", "ihn", "nun", "auf\u00b7fe\u00b7wig", "nicht", "mehr", "schau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jhr Hertze schwam in Blut/ gefoltert war ihr Sinn.", "tokens": ["Ihr", "Hert\u00b7ze", "schwam", "in", "Blut", "/", "ge\u00b7fol\u00b7tert", "war", "ihr", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$(", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dennoch blieb der Schlu\u00df Ceycis muste scheiden/", "tokens": ["Und", "den\u00b7noch", "blieb", "der", "Schlu\u00df", "Cey\u00b7cis", "mus\u00b7te", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NE", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er schwur in Monatsfrist zu fassen sie in Arm.", "tokens": ["Er", "schwur", "in", "Mo\u00b7nats\u00b7frist", "zu", "fas\u00b7sen", "sie", "in", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie aber kan ein Mensch doch sein Verh\u00e4ngn\u00fc\u00df meiden?", "tokens": ["Wie", "a\u00b7ber", "kan", "ein", "Mensch", "doch", "sein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was \u00fcber ihn bestimmt/ hemmt kein vergebner Harm.", "tokens": ["Was", "\u00fc\u00b7ber", "ihn", "be\u00b7stimmt", "/", "hemmt", "kein", "ver\u00b7geb\u00b7ner", "Harm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVPP", "$(", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er mu\u00df durch Schiffbruch nur ins Meeres Scho\u00df verderben/", "tokens": ["Er", "mu\u00df", "durch", "Schiff\u00b7bruch", "nur", "ins", "Mee\u00b7res", "Scho\u00df", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sehr er widerstrebt/ der Wellen Opffer seyn.", "tokens": ["Wie", "sehr", "er", "wi\u00b7der\u00b7strebt", "/", "der", "Wel\u00b7len", "Opf\u00b7fer", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$(", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und dieser Fall der heist zugleich zwey Seelen sterben/", "tokens": ["Und", "die\u00b7ser", "Fall", "der", "heist", "zu\u00b7gleich", "zwey", "See\u00b7len", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "ADV", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Alcyone verlangt nicht mehr der Sonnen Schein.", "tokens": ["Al\u00b7cyo\u00b7ne", "ver\u00b7langt", "nicht", "mehr", "der", "Son\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Es stellt die tr\u00fcbe Nacht/ wenn sie jetzt Tr\u00e4ume s\u00e4et/", "tokens": ["Es", "stellt", "die", "tr\u00fc\u00b7be", "Nacht", "/", "wenn", "sie", "jetzt", "Tr\u00e4u\u00b7me", "s\u00e4et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und mit vergn\u00fcgter Ruh die m\u00fcden Glieder speist", "tokens": ["Und", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "Ruh", "die", "m\u00fc\u00b7den", "Glie\u00b7der", "speist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie f\u00fcr der Liebsten Bett Ceycis seufftzt und flehet/", "tokens": ["Wie", "f\u00fcr", "der", "Liebs\u00b7ten", "Bett", "Cey\u00b7cis", "seufftzt", "und", "fle\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "NE", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und seinen Untergang durch einen Schatten weist.", "tokens": ["Und", "sei\u00b7nen", "Un\u00b7ter\u00b7gang", "durch", "ei\u00b7nen", "Schat\u00b7ten", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was thut Alcyone? Sie laufft an das Gestade", "tokens": ["Was", "thut", "Al\u00b7cyo\u00b7ne", "?", "Sie", "laufft", "an", "das", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NE", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Denckt an den letzten Ku\u00df/ den sie zum Abschied gab/", "tokens": ["Denckt", "an", "den", "letz\u00b7ten", "Ku\u00df", "/", "den", "sie", "zum", "Ab\u00b7schied", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "ART", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Rufft: Find ich Himmel denn nicht weiter mehr Genade?", "tokens": ["Rufft", ":", "Find", "ich", "Him\u00b7mel", "denn", "nicht", "wei\u00b7ter", "mehr", "Ge\u00b7na\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "PPER", "NN", "ADV", "PTKNEG", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So mach ein gleicher Tod uns doch ein gleiches Grab.", "tokens": ["So", "mach", "ein", "glei\u00b7cher", "Tod", "uns", "doch", "ein", "glei\u00b7ches", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach Muster voller Zucht und Spiegel reiner Sitten!", "tokens": ["Ach", "Mus\u00b7ter", "vol\u00b7ler", "Zucht", "und", "Spie\u00b7gel", "rei\u00b7ner", "Sit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ja der verm\u00e4hlten Lieb\u2019 unsch\u00e4tzbar Ebenbild!", "tokens": ["Ja", "der", "ver\u00b7m\u00e4hl\u00b7ten", "Lieb'", "un\u00b7sch\u00e4tz\u00b7bar", "E\u00b7ben\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als unsre ", "tokens": ["Als", "uns\u00b7re"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Hat sie mit gleichem Ruhm des Lebens Ziel erf\u00fcllt.", "tokens": ["Hat", "sie", "mit", "glei\u00b7chem", "Ruhm", "des", "Le\u00b7bens", "Ziel", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Woledler/ seinen Schmertz lebhafftig vorzustellen", "tokens": ["Wo\u00b7led\u00b7ler", "/", "sei\u00b7nen", "Schmertz", "leb\u00b7haff\u00b7tig", "vor\u00b7zu\u00b7stel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mu\u00df wol Alcyone das beste Vorbild seyn", "tokens": ["Mu\u00df", "wol", "Al\u00b7cyo\u00b7ne", "das", "bes\u00b7te", "Vor\u00b7bild", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NE", "ART", "ADJA", "NN", "VAINF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Es sey ein Mensch ein Fels/ bey solchen Trauer-F\u00e4llen", "tokens": ["Es", "sey", "ein", "Mensch", "ein", "Fels", "/", "bey", "sol\u00b7chen", "Trau\u00b7e\u00b7rF\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$(", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Reist die Best\u00fcrtzung nur den besten Rathschlag ein.", "tokens": ["Reist", "die", "Be\u00b7st\u00fcrt\u00b7zung", "nur", "den", "bes\u00b7ten", "Rath\u00b7schlag", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Ein Artzt/ der vielen offt die Schmertzen hat geheilet/", "tokens": ["Ein", "Artzt", "/", "der", "vie\u00b7len", "offt", "die", "Schmert\u00b7zen", "hat", "ge\u00b7hei\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIAT", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wei\u00df/ wenn er selber liegt/ f\u00fcr seine Noth nicht Rath.", "tokens": ["Wei\u00df", "/", "wenn", "er", "sel\u00b7ber", "liegt", "/", "f\u00fcr", "sei\u00b7ne", "Noth", "nicht", "Rath", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn so ein Donnerschlag die Sinnen \u00fcbereilet/", "tokens": ["Wenn", "so", "ein", "Don\u00b7ner\u00b7schlag", "die", "Sin\u00b7nen", "\u00fc\u00b7be\u00b7rei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So glaubt/ da\u00df Seneca auch nicht mehr Pflaster hat.", "tokens": ["So", "glaubt", "/", "da\u00df", "Se\u00b7ne\u00b7ca", "auch", "nicht", "mehr", "Pflas\u00b7ter", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "NE", "ADV", "PTKNEG", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "War di\u00df der letzte Ku\u00df/ war di\u00df das letzte Reisen", "tokens": ["War", "di\u00df", "der", "letz\u00b7te", "Ku\u00df", "/", "war", "di\u00df", "das", "letz\u00b7te", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$(", "VAFIN", "PDS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als ihn Alcyone au\u00df ihren Armen lie\u00df?", "tokens": ["Als", "ihn", "Al\u00b7cyo\u00b7ne", "au\u00df", "ih\u00b7ren", "Ar\u00b7men", "lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Versprach sie nicht mehr Lust in k\u00fcnfftig zu erweisen?", "tokens": ["Ver\u00b7sprach", "sie", "nicht", "mehr", "Lust", "in", "k\u00fcnff\u00b7tig", "zu", "er\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PIAT", "NN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als Leid er jetzt empfind bey solchem Seelen-Rie\u00df.", "tokens": ["Als", "Leid", "er", "jetzt", "emp\u00b7find", "bey", "sol\u00b7chem", "See\u00b7len\u00b7Rie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "War seine Wiederkunfft nicht nur ihr eintzig hoffen?", "tokens": ["War", "sei\u00b7ne", "Wie\u00b7der\u00b7kunfft", "nicht", "nur", "ihr", "eint\u00b7zi