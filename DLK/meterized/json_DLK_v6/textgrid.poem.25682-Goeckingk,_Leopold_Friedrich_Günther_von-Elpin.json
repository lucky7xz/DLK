{"textgrid.poem.25682": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Elpin", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, Elpin sey sehr in Sorgen,", "tokens": ["Man", "sagt", ",", "El\u00b7pin", "sey", "sehr", "in", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "NE", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich nicht durch Betrug mein Geld verl\u00f6re.", "tokens": ["Da\u00df", "ich", "nicht", "durch", "Be\u00b7trug", "mein", "Geld", "ver\u00b7l\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dem so w\u00e4re,", "tokens": ["Wenn", "dem", "so", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "K\u00e4m' er wohl nicht, um Geld von mir zu borgen.", "tokens": ["K\u00e4m'", "er", "wohl", "nicht", ",", "um", "Geld", "von", "mir", "zu", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUI", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}