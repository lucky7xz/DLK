{"textgrid.poem.37426": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er kriegte Geld. Die Sorge wich,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kriegte Geld. Die Sorge wich,", "tokens": ["Er", "krieg\u00b7te", "Geld", ".", "Die", "Sor\u00b7ge", "wich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihn bisher beklommen.", "tokens": ["Die", "ihn", "bis\u00b7her", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat die Jungfer Fr\u00f6hlich sich", "tokens": ["Er", "hat", "die", "Jung\u00b7fer", "Fr\u00f6h\u00b7lich", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Schatz genommen.", "tokens": ["Zu", "sei\u00b7nem", "Schatz", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie tranken Wein, sie a\u00dfen fein,", "tokens": ["Sie", "tran\u00b7ken", "Wein", ",", "sie", "a\u00b7\u00dfen", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sangen zum Klaviere;", "tokens": ["Sie", "san\u00b7gen", "zum", "Kla\u00b7vie\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch wie sie sich so recht erfreun,", "tokens": ["Doch", "wie", "sie", "sich", "so", "recht", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da klopft es an die T\u00fcre.", "tokens": ["Da", "klopft", "es", "an", "die", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die alte Sorge war's, o weh,", "tokens": ["Die", "al\u00b7te", "Sor\u00b7ge", "wa\u00b7r's", ",", "o", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "FM", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die magerste der Sorgen.", "tokens": ["Die", "ma\u00b7gers\u00b7te", "der", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Sie setzte sich ins Kanapee", "tokens": ["Sie", "setz\u00b7te", "sich", "ins", "Ka\u00b7na\u00b7pee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnschte guten Morgen.", "tokens": ["Und", "w\u00fcnschte", "gu\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}}}}