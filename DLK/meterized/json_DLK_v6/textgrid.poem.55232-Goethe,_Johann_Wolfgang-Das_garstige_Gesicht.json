{"textgrid.poem.55232": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Das garstige Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einen w\u00fcrdigen Biedermann,", "tokens": ["Wenn", "ei\u00b7nen", "w\u00fcr\u00b7di\u00b7gen", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pastorn oder Ratsherrn lobesan,", "tokens": ["Pas\u00b7torn", "o\u00b7der", "Rats\u00b7herrn", "lo\u00b7be\u00b7san", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Wittib l\u00e4\u00dft in Kupfer stechen", "tokens": ["Die", "Wit\u00b7tib", "l\u00e4\u00dft", "in", "Kup\u00b7fer", "ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und drunter ein Verslein radebrechen,", "tokens": ["Und", "drun\u00b7ter", "ein", "Ver\u00b7slein", "ra\u00b7de\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da hei\u00dft's: \u00bbSeht hier mit Kopf und Ohren", "tokens": ["Da", "hei\u00dft's", ":", "\u00bb", "Seht", "hier", "mit", "Kopf", "und", "Oh\u00b7ren"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$.", "$(", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Herrn, Ehrw\u00fcrdig, Wohlgeboren!", "tokens": ["Den", "Herrn", ",", "Ehr\u00b7w\u00fcr\u00b7dig", ",", "Wohl\u00b7ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Seht seine Augen und seine Stirn;", "tokens": ["Seht", "sei\u00b7ne", "Au\u00b7gen", "und", "sei\u00b7ne", "Stirn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Aber sein verst\u00e4ndig Gehirn,", "tokens": ["A\u00b7ber", "sein", "ver\u00b7st\u00e4n\u00b7dig", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "So manch Verdienst ums gemeine Wesen", "tokens": ["So", "manch", "Ver\u00b7dienst", "ums", "ge\u00b7mei\u00b7ne", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "K\u00f6nnt ihr ihm nicht an der Nase lesen.\u00ab", "tokens": ["K\u00f6nnt", "ihr", "ihm", "nicht", "an", "der", "Na\u00b7se", "le\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "So, liebe Lotte! hei\u00dft's auch hier:", "tokens": ["So", ",", "lie\u00b7be", "Lot\u00b7te", "!", "hei\u00dft's", "auch", "hier", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "NE", "$.", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schicke da mein Bildnis dir.", "tokens": ["Ich", "schi\u00b7cke", "da", "mein", "Bild\u00b7nis", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Magst wohl die ernste Stirne sehen,", "tokens": ["Magst", "wohl", "die", "erns\u00b7te", "Stir\u00b7ne", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Augen Glut, der Locken Wehen;", "tokens": ["Der", "Au\u00b7gen", "Glut", ",", "der", "Lo\u00b7cken", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "'s ist ungef\u00e4hr das garst'ge Gesicht:", "tokens": ["'s", "ist", "un\u00b7ge\u00b7f\u00e4hr", "das", "gar\u00b7st'\u00b7ge", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Aber meine Liebe siehst du nicht.", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Lie\u00b7be", "siehst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}