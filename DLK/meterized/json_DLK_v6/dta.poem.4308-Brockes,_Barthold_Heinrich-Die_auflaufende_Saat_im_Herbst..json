{"dta.poem.4308": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n auflaufende Saat im Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich von dem Gang im Garten durchs Korn-Feld", "tokens": ["Nach\u00b7dem", "ich", "von", "dem", "Gang", "im", "Gar\u00b7ten", "durchs", "Korn\u00b7Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "APPRART", "NN"], "meter": "---+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "einen Weg gezogen,", "tokens": ["ei\u00b7nen", "Weg", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jm Herbst, wie eben erst das Feld", "tokens": ["Jm", "Herbst", ",", "wie", "e\u00b7ben", "erst", "das", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gepfl\u00fcget war, geegt, bes\u00e4et und bestellt,", "tokens": ["Ge\u00b7pfl\u00fc\u00b7get", "war", ",", "geegt", ",", "be\u00b7s\u00e4et", "und", "be\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ich, nach wenig Tagen nur, durch einen sch\u00f6nen Tag", "tokens": ["Und", "ich", ",", "nach", "we\u00b7nig", "Ta\u00b7gen", "nur", ",", "durch", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "PIAT", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "bewogen,", "tokens": ["be\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Auf selbigem spatzieren ging; erblickt\u2019 ich, mit ger\u00fchrter", "tokens": ["Auf", "sel\u00b7bi\u00b7gem", "spat\u00b7zie\u00b7ren", "ging", ";", "er\u00b7blickt'", "ich", ",", "mit", "ge\u00b7r\u00fchr\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "VVINF", "VVFIN", "$.", "VVFIN", "PPER", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Brust,", "tokens": ["Brust", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Zur unverhofften Augen-Lust,", "tokens": ["Zur", "un\u00b7ver\u00b7hoff\u00b7ten", "Au\u00b7gen\u00b7Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie aus des Ackers ebner Fl\u00e4chen,", "tokens": ["Wie", "aus", "des", "A\u00b7ckers", "eb\u00b7ner", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von dem schon aufgelaufnen Korn, sich Millionen Spitzen", "tokens": ["Von", "dem", "schon", "auf\u00b7ge\u00b7lauf\u00b7nen", "Korn", ",", "sich", "Mil\u00b7lion\u00b7en", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "PRF", "NN", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "stechen,", "tokens": ["ste\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die theils ein gelbliches, und theils ein r\u00f6htlich Gr\u00fcn,", "tokens": ["Die", "theils", "ein", "gelb\u00b7li\u00b7ches", ",", "und", "theils", "ein", "r\u00f6ht\u00b7lich", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "$,", "KON", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In recht lebend\u2019gen Farben, schm\u00fcckte, und reichen Segen", "tokens": ["In", "recht", "le\u00b7ben\u00b7d'\u00b7gen", "Far\u00b7ben", ",", "schm\u00fcck\u00b7te", ",", "und", "rei\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "----+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "schon versprechen.", "tokens": ["schon", "ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der Sonnen lieblichs Licht, so durch die Bl\u00e4tter schien,", "tokens": ["Der", "Son\u00b7nen", "lieb\u00b7lichs", "Licht", ",", "so", "durch", "die", "Bl\u00e4t\u00b7ter", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhoben durch das graue Land, und durch der Furchen", "tokens": ["Er\u00b7ho\u00b7ben", "durch", "das", "grau\u00b7e", "Land", ",", "und", "durch", "der", "Fur\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "braunen Grund,", "tokens": ["brau\u00b7nen", "Grund", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "In welchen, als in Linien, das junge Korn recht zierlich", "tokens": ["In", "wel\u00b7chen", ",", "als", "in", "Li\u00b7ni\u00b7en", ",", "das", "jun\u00b7ge", "Korn", "recht", "zier\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "KOUS", "APPR", "NE", "$,", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "stund,", "tokens": ["stund", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Versch\u00f6nerte noch mehr, was an sich selbst schon sch\u00f6n,", "tokens": ["Ver\u00b7sch\u00f6\u00b7ner\u00b7te", "noch", "mehr", ",", "was", "an", "sich", "selbst", "schon", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PRELS", "APPR", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch ihren Farben-reichen Glanz.", "tokens": ["Durch", "ih\u00b7ren", "Fa\u00b7rben\u00b7rei\u00b7chen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was nahe stund, war einzeln noch zu seh\u2019n,", "tokens": ["Was", "na\u00b7he", "stund", ",", "war", "ein\u00b7zeln", "noch", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VAFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn das, so weiter lag, bereits ein gr\u00fcnlich Ganz", "tokens": ["Wenn", "das", ",", "so", "wei\u00b7ter", "lag", ",", "be\u00b7reits", "ein", "gr\u00fcn\u00b7lich", "Ganz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "ADV", "ADV", "VVFIN", "$,", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem Blick bem\u00fcht schien vorzustellen. Hier\u00fcber war ein", "tokens": ["Dem", "Blick", "be\u00b7m\u00fcht", "schien", "vor\u00b7zu\u00b7stel\u00b7len", ".", "Hier\u00b7\u00fc\u00b7ber", "war", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VVFIN", "VVIZU", "$.", "PAV", "VAFIN", "ART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "bunt-gef\u00e4rbtes Glas,", "tokens": ["bunt\u00b7ge\u00b7f\u00e4rb\u00b7tes", "Glas", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Um dieses Mahlwerk der Natur noch desto sch\u00f6ner auszu-", "tokens": ["Um", "die\u00b7ses", "Mahl\u00b7werk", "der", "Na\u00b7tur", "noch", "des\u00b7to", "sch\u00f6\u00b7ner", "aus\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "NN", "ART", "NN", "ADV", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "schm\u00fccken,", "tokens": ["schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In einem gl\u00e4nzenden Gewebe von bunten F\u00e4den zu", "tokens": ["In", "ei\u00b7nem", "gl\u00e4n\u00b7zen\u00b7den", "Ge\u00b7we\u00b7be", "von", "bun\u00b7ten", "F\u00e4\u00b7den", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "erblicken.", "tokens": ["er\u00b7bli\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wovon noch ungewi\u00df, ob Spinnen es gesponnen,", "tokens": ["Wo\u00b7von", "noch", "un\u00b7ge\u00b7wi\u00df", ",", "ob", "Spin\u00b7nen", "es", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie, oder ob es sonst entsteht. Es sind im hellen Strahl", "tokens": ["Wie", ",", "o\u00b7der", "ob", "es", "sonst", "ent\u00b7steht", ".", "Es", "sind", "im", "hel\u00b7len", "Strahl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "der Sonnen", "tokens": ["der", "Son\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Des sch\u00f6nen Regen-Bogens Farben, in einem noch vermehr-", "tokens": ["Des", "sch\u00f6\u00b7nen", "Re\u00b7gen\u00b7Bo\u00b7gens", "Far\u00b7ben", ",", "in", "ei\u00b7nem", "noch", "ver\u00b7mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "APPR", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "tern Schein,", "tokens": ["tern", "Schein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Wie lauter Prismata, zu seh\u2019n. Mit noch vergr\u00f6ssertem", "tokens": ["Wie", "lau\u00b7ter", "Pris\u00b7ma\u00b7ta", ",", "zu", "seh'", "n.", "Mit", "noch", "ver\u00b7gr\u00f6s\u00b7ser\u00b7tem"], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "APPR", "NE", "NE", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.11": {"text": "Vergn\u00fcgen", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Sieht man, vor andern, noch die Stellen, die unterm Strahl", "tokens": ["Sieht", "man", ",", "vor", "an\u00b7dern", ",", "noch", "die", "Stel\u00b7len", ",", "die", "un\u00b7term", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "APPR", "PIS", "$,", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "der Sonnen liegen,", "tokens": ["der", "Son\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "So hell, so wunderw\u00fcrdig schimmern, nicht anders, als", "tokens": ["So", "hell", ",", "so", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "schim\u00b7mern", ",", "nicht", "an\u00b7ders", ",", "als"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$,", "PTKNEG", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "wenn auf der Fluht", "tokens": ["wenn", "auf", "der", "Fluht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Der Sonnen Bild, im flachen Strahl, als eine Feuer-", "tokens": ["Der", "Son\u00b7nen", "Bild", ",", "im", "fla\u00b7chen", "Strahl", ",", "als", "ei\u00b7ne", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPRART", "ADJA", "NN", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Seule ruht.", "tokens": ["Seu\u00b7le", "ruht", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Durch ihr best\u00e4ndiges Bewegen", "tokens": ["Durch", "ihr", "be\u00b7st\u00e4n\u00b7di\u00b7ges", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Schien jede Farbe, wandelbar, sich stets im neuen Glanz", "tokens": ["Schien", "je\u00b7de", "Far\u00b7be", ",", "wan\u00b7del\u00b7bar", ",", "sich", "stets", "im", "neu\u00b7en", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "ADJD", "$,", "PRF", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "zu regen.", "tokens": ["zu", "re\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Mich nahm, mit ungemeiner Freuden, die wunderw\u00fcrd\u2019ge", "tokens": ["Mich", "nahm", ",", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "Freu\u00b7den", ",", "die", "wun\u00b7der\u00b7w\u00fcrd'\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sch\u00f6nheit ein.", "tokens": ["Sch\u00f6n\u00b7heit", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich dankte GOtt, Der, auch im Herbst, nicht nur das Feld", "tokens": ["Ich", "dank\u00b7te", "Gott", ",", "Der", ",", "auch", "im", "Herbst", ",", "nicht", "nur", "das", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "ART", "$,", "ADV", "APPRART", "NN", "$,", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so lieblich zieret,", "tokens": ["so", "lieb\u00b7lich", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nein, Der uns auch, in solcher Zierde, von dem, was uns", "tokens": ["Nein", ",", "Der", "uns", "auch", ",", "in", "sol\u00b7cher", "Zier\u00b7de", ",", "von", "dem", ",", "was", "uns"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "APPR", "ART", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "