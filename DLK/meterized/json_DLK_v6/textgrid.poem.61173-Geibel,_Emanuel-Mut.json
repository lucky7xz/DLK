{"textgrid.poem.61173": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Mut", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herz, la\u00df ab zu zagen", "tokens": ["O", "Herz", ",", "la\u00df", "ab", "zu", "za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und von dir wirf das Joch!", "tokens": ["Und", "von", "dir", "wirf", "das", "Joch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast so viel getragen,", "tokens": ["Du", "hast", "so", "viel", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du tr\u00e4gst auch dieses noch.", "tokens": ["Du", "tr\u00e4gst", "auch", "die\u00b7ses", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tritt auf in blanken Waffen,", "tokens": ["Tritt", "auf", "in", "blan\u00b7ken", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Geist, und werde frei!", "tokens": ["Mein", "Geist", ",", "und", "wer\u00b7de", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gilt noch mehr zu schaffen,", "tokens": ["Es", "gilt", "noch", "mehr", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als einen Liebesmai.", "tokens": ["Als", "ei\u00b7nen", "Lie\u00b7bes\u00b7mai", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ob die Brust auch blutet,", "tokens": ["Und", "ob", "die", "Brust", "auch", "blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur vorw\u00e4rts in die Bahn!", "tokens": ["Nur", "vor\u00b7w\u00e4rts", "in", "die", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wei\u00dft, am vollsten flutet", "tokens": ["Du", "wei\u00dft", ",", "am", "volls\u00b7ten", "flu\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesang dem ", "tokens": ["Ge\u00b7sang", "dem"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}