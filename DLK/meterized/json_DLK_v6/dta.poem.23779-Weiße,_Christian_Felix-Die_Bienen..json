{"dta.poem.23779": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Bienen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Tragt nur in die Zellen ein,", "tokens": ["Tragt", "nur", "in", "die", "Zel\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleine Honigsammlerinnen:", "tokens": ["Klei\u00b7ne", "Ho\u00b7ni\u00b7gsamm\u00b7le\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt bey warmen Sonnenschein", "tokens": ["Jetzt", "bey", "war\u00b7men", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sucht Jhr Sch\u00e4tze zu gewinnen:", "tokens": ["Sucht", "Ihr", "Sch\u00e4t\u00b7ze", "zu", "ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fc\u00dfigg\u00e4nger ha\u00dft man hier;", "tokens": ["M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", "ha\u00dft", "man", "hier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flei\u00df und Arbeit sind Euch Freude,", "tokens": ["Flei\u00df", "und", "Ar\u00b7beit", "sind", "Euch", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das Beste sammelt Jhr", "tokens": ["Und", "das", "Bes\u00b7te", "sam\u00b7melt", "Ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der blumenvollen Weide.", "tokens": ["Auf", "der", "blu\u00b7men\u00b7vol\u00b7len", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn dann einst der rauhe Nord", "tokens": ["Wenn", "dann", "einst", "der", "rau\u00b7he", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber jene H\u00fcgel streichet", "tokens": ["Ue\u00b7ber", "je\u00b7ne", "H\u00fc\u00b7gel", "strei\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Flora Kinder dort,", "tokens": ["Und", "der", "Flo\u00b7ra", "Kin\u00b7der", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der bunten Flur verscheuchet;", "tokens": ["Von", "der", "bun\u00b7ten", "Flur", "ver\u00b7scheu\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann sitzt Jhr in Sicherheit:", "tokens": ["Dann", "sitzt", "Ihr", "in", "Si\u00b7cher\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Voll sind Eure Vorrathskammern,", "tokens": ["Voll", "sind", "Eu\u00b7re", "Vor\u00b7raths\u00b7kam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Euch lehrt die D\u00fcrftigkeit", "tokens": ["Und", "Euch", "lehrt", "die", "D\u00fcrf\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht vor andern Th\u00fcren jammern.", "tokens": ["Nicht", "vor", "an\u00b7dern", "Th\u00fc\u00b7ren", "jam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch Jhr sorgt nicht nur f\u00fcr Euch:", "tokens": ["Doch", "Ihr", "sorgt", "nicht", "nur", "f\u00fcr", "Euch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, bey Eurem s\u00fc\u00dfen Flei\u00dfe", "tokens": ["Nein", ",", "bey", "Eu\u00b7rem", "s\u00fc\u00b7\u00dfen", "Flei\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seyd Jhr auch f\u00fcr andre reich,", "tokens": ["Seyd", "Ihr", "auch", "f\u00fcr", "and\u00b7re", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "APPR", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dankbegierig, milde, weise:", "tokens": ["Dank\u00b7be\u00b7gie\u00b7rig", ",", "mil\u00b7de", ",", "wei\u00b7se", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr verzinn\u00dft das kleine Haus", "tokens": ["Ihr", "ver\u00b7zinn\u00dft", "das", "klei\u00b7ne", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reichlich dem, der es erbauet,", "tokens": ["Reich\u00b7lich", "dem", ",", "der", "es", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der leiht mit Wucher aus,", "tokens": ["Und", "der", "leiht", "mit", "Wu\u00b7cher", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Euch in der Theurung trauet.", "tokens": ["Der", "Euch", "in", "der", "Theu\u00b7rung", "trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Euer bl\u00fchendes Geschlecht", "tokens": ["Eu\u00b7er", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6ge j\u00e4hrlich sich vermehren,", "tokens": ["M\u00f6\u00b7ge", "j\u00e4hr\u00b7lich", "sich", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das weise B\u00fcrgerrecht", "tokens": ["Und", "das", "wei\u00b7se", "B\u00fcr\u00b7ger\u00b7recht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie ein falscher Fremdling st\u00f6ren!", "tokens": ["Nie", "ein", "fal\u00b7scher", "Fremd\u00b7ling", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blumen will ich pflanzen, hier", "tokens": ["Blu\u00b7men", "will", "ich", "pflan\u00b7zen", ",", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedes Bl\u00fcmchens sorgsam schonen,", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chens", "sorg\u00b7sam", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Jhr sollet mich daf\u00fcr", "tokens": ["Und", "Ihr", "sol\u00b7let", "mich", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PRF", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Einst mit Honigseim belohnen.", "tokens": ["Einst", "mit", "Ho\u00b7ni\u00b7gseim", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}