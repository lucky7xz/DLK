{"textgrid.poem.46011": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o Trost in letzten Stunden]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Trost in letzten Stunden", "tokens": ["O", "Trost", "in", "letz\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr heiligen f\u00fcnf Wunden", "tokens": ["Ihr", "hei\u00b7li\u00b7gen", "f\u00fcnf", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter la\u00dft gesunden", "tokens": ["Die", "Mut\u00b7ter", "la\u00dft", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Euch ja k\u00f6mmt das Heil", "tokens": ["Von", "Euch", "ja", "k\u00f6mmt", "das", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So fleht der Kinder Jammer", "tokens": ["So", "fleht", "der", "Kin\u00b7der", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da klingt der Pforte Hammer", "tokens": ["Da", "klingt", "der", "Pfor\u00b7te", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da naht der Schmerzenkammer", "tokens": ["Da", "naht", "der", "Schmer\u00b7zen\u00b7kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Tod mit seinem Pfeil.", "tokens": ["Der", "Tod", "mit", "sei\u00b7nem", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es mahnt der Schrei der Eule", "tokens": ["Es", "mahnt", "der", "Schrei", "der", "Eu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es kracht des Hauses S\u00e4ule", "tokens": ["Es", "kracht", "des", "Hau\u00b7ses", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein klagendes Geheule", "tokens": ["Ein", "kla\u00b7gen\u00b7des", "Ge\u00b7heu\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erhebt der treue Hund", "tokens": ["Er\u00b7hebt", "der", "treu\u00b7e", "Hund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da fleht die Mutter leise,", "tokens": ["Da", "fleht", "die", "Mut\u00b7ter", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Herr zur dunklen Reise", "tokens": ["O", "Herr", "zur", "dunk\u00b7len", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sehnt mich's nach heil'ger Speise", "tokens": ["Sehnt", "mich's", "nach", "heil'\u00b7ger", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Aus deinem Gnadenbund.", "tokens": ["Aus", "dei\u00b7nem", "Gna\u00b7den\u00b7bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da kam der Arzt gegangen", "tokens": ["Da", "kam", "der", "Arzt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kinder flehn mit Bangen", "tokens": ["Die", "Kin\u00b7der", "flehn", "mit", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jammerndem Verlangen", "tokens": ["Und", "jam\u00b7mern\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Herr brich unsre Not!", "tokens": ["O", "Herr", "brich", "uns\u00b7re", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sah mit Tr\u00e4nenb\u00e4chen", "tokens": ["Er", "sah", "mit", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Mutter Augen brechen", "tokens": ["Der", "Mut\u00b7ter", "Au\u00b7gen", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wagt nicht auszusprechen", "tokens": ["Und", "wagt", "nicht", "aus\u00b7zu\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott helf', ich seh' den Tod.", "tokens": ["Gott", "hel\u00b7f'", ",", "ich", "seh'", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da hat er Rat gefunden,", "tokens": ["Da", "hat", "er", "Rat", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sah des Heilands Wunden", "tokens": ["Er", "sah", "des", "Hei\u00b7lands", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Trost in letzten Stunden", "tokens": ["Den", "Trost", "in", "letz\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gemalet an die Wand,", "tokens": ["Ge\u00b7ma\u00b7let", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dahin den Blick erhoben", "tokens": ["Da\u00b7hin", "den", "Blick", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zeigt ruhig er nach oben", "tokens": ["Zeigt", "ru\u00b7hig", "er", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und spricht, die Hand da droben", "tokens": ["Und", "spricht", ",", "die", "Hand", "da", "dro\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die hilft, die Gotteshand.", "tokens": ["Die", "hilft", ",", "die", "Got\u00b7tes\u00b7hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich selbst kann hier nichts geben,", "tokens": ["Ich", "selbst", "kann", "hier", "nichts", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Wein sucht bei den Reben", "tokens": ["Den", "Wein", "sucht", "bei", "den", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben bei dem Leben", "tokens": ["Das", "Le\u00b7ben", "bei", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Heilands Heilhand Heil,", "tokens": ["In", "Hei\u00b7lands", "Heil\u00b7hand", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu diesem Arzte tretet", "tokens": ["Zu", "die\u00b7sem", "Arz\u00b7te", "tre\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er sieht euch so ihr betet.", "tokens": ["Er", "sieht", "euch", "so", "ihr", "be\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und als er so geredet", "tokens": ["Und", "als", "er", "so", "ge\u00b7re\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verlie\u00df er sie in Eil.", "tokens": ["Ver\u00b7lie\u00df", "er", "sie", "in", "Eil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als er so geschieden,", "tokens": ["Und", "als", "er", "so", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "All andre H\u00fclfe mieden", "tokens": ["All", "and\u00b7re", "H\u00fcl\u00b7fe", "mie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder fromm zufrieden,", "tokens": ["Die", "Kin\u00b7der", "fromm", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie folgten seinem Rat,", "tokens": ["Sie", "folg\u00b7ten", "sei\u00b7nem", "Rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn von dem Trost belebet", "tokens": ["Denn", "von", "dem", "Trost", "be\u00b7le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Haupt die Mutter hebet", "tokens": ["Das", "Haupt", "die", "Mut\u00b7ter", "he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und spricht, ihr Lieben gebet", "tokens": ["Und", "spricht", ",", "ihr", "Lie\u00b7ben", "ge\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was er geordnet hat.", "tokens": ["Was", "er", "ge\u00b7ord\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es kehrt nach zweien Tagen", "tokens": ["Es", "kehrt", "nach", "zwei\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Arzt mit mildem Zagen", "tokens": ["Der", "Arzt", "mit", "mil\u00b7dem", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Kindern nachzufragen", "tokens": ["Den", "Kin\u00b7dern", "nach\u00b7zu\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieses fromme Haus,", "tokens": ["In", "die\u00b7ses", "from\u00b7me", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da h\u00f6rt er Lieder klingen", "tokens": ["Da", "h\u00f6rt", "er", "Lie\u00b7der", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und feierlich lobsingen,", "tokens": ["Und", "fei\u00b7er\u00b7lich", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dachte, ach sie bringen", "tokens": ["Und", "dach\u00b7te", ",", "ach", "sie", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "XY", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Leiche nun heraus.", "tokens": ["Die", "Lei\u00b7che", "nun", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sein Herz wollt' Gott da lenken,", "tokens": ["Sein", "Herz", "wollt'", "Gott", "da", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Waisen zu bedenken,", "tokens": ["Die", "Wai\u00b7sen", "zu", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Kleinen will er schenken", "tokens": ["Den", "Klei\u00b7nen", "will", "er", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Vater sich zur Stund", "tokens": ["Als", "Va\u00b7ter", "sich", "zur", "Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sah ins Haus gegangen", "tokens": ["Und", "sah", "ins", "Haus", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Hals der Mutter hangen", "tokens": ["Am", "Hals", "der", "Mut\u00b7ter", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Kinder, sie lobsangen,", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "lob\u00b7san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Mutter war gesund.", "tokens": ["Die", "Mut\u00b7ter", "war", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie eilten ihm entgegen", "tokens": ["Sie", "eil\u00b7ten", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und riefen: Gottessegen", "tokens": ["Und", "rie\u00b7fen", ":", "Got\u00b7tes\u00b7se\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf allen deinen Wegen", "tokens": ["Auf", "al