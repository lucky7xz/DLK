ord"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lieb' warf vom Farbenstaube", "tokens": ["Lieb'", "warf", "vom", "Far\u00b7ben\u00b7stau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Heilandshand hinein", "tokens": ["Der", "Hei\u00b7lands\u00b7hand", "hin\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schau auf nach den f\u00fcnf Wunden", "tokens": ["Schau", "auf", "nach", "den", "f\u00fcnf", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "ART", "CARD", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Die eine ist verschwunden", "tokens": ["Die", "ei\u00b7ne", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es trank sie, zu gesunden", "tokens": ["Es", "trank", "sie", ",", "zu", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Mutter in dem Wein.", "tokens": ["Die", "Mut\u00b7ter", "in", "dem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da sah der Arzt das Wunder", "tokens": ["Da", "sah", "der", "Arzt", "das", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ging sein Wissen 