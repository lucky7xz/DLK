{"dta.poem.6502": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.42", "da:0.42", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Une Lumiere moder&#233;e,", "tokens": ["U\u00b7ne", "Lu\u00b7mie\u00b7re", "mo\u00b7der", "&#233;", "e", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Qui par un moindre mouvement", "tokens": ["Qui", "par", "un", "mo\u00b7ind\u00b7re", "mou\u00b7ve\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Cause sur notre Organe un leger pressement,", "tokens": ["Cau\u00b7se", "sur", "not\u00b7re", "Or\u00b7ga\u00b7ne", "un", "le\u00b7ger", "pres\u00b7se\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+---+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Nous rejou\u00eft, & nous recr\u00e9e;", "tokens": ["Nous", "re\u00b7jou\u00eft", ",", "&", "nous", "re\u00b7cr\u00e9e", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "$(", "FM.fr", "FM.fr", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mais lorsque pars l\u2019Activit\u00e9", "tokens": ["Mais", "lors\u00b7que", "pars", "l'\u00b7Ac\u00b7ti\u00b7vit\u00e9"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Des traits per\u00e7ans de la Clart\u00e9,", "tokens": ["Des", "traits", "per\u00e7ans", "de", "la", "Clar\u00b7t\u00e9", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Notre v\u00fbe est trop \u00e9branl\u00e9e,", "tokens": ["Not\u00b7re", "v\u00fbe", "est", "trop", "\u00e9br\u00b7an\u00b7l\u00e9e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ils nous causent de la douleur,", "tokens": ["Ils", "nous", "cau\u00b7sent", "de", "la", "dou\u00b7leur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Comme feroit sur notre main br\u00fbl\u00e9e", "tokens": ["Com\u00b7me", "fe\u00b7roit", "sur", "not\u00b7re", "main", "br\u00fbl\u00e9e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Une violente Chaleur.", "tokens": ["U\u00b7ne", "vi\u00b7o\u00b7len\u00b7te", "Cha\u00b7leur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}}}}