{"textgrid.poem.25200": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: An meinem Lager h\u00e4lt die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An meinem Lager h\u00e4lt die Nacht", "tokens": ["An", "mei\u00b7nem", "La\u00b7ger", "h\u00e4lt", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schweigend ihre Leichenwacht.", "tokens": ["Schwei\u00b7gend", "ih\u00b7re", "Lei\u00b7chen\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur drau\u00dfen \u00fcber H\u00e4userd\u00e4cher streift", "tokens": ["Nur", "drau\u00b7\u00dfen", "\u00fc\u00b7ber", "H\u00e4u\u00b7ser\u00b7d\u00e4\u00b7cher", "streift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein ruheloser Luftgeist, \u2013", "tokens": ["Ein", "ru\u00b7he\u00b7lo\u00b7ser", "Luft\u00b7geist", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Trauergewandung", "tokens": ["Wie", "Trau\u00b7er\u00b7ge\u00b7wan\u00b7dung"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00dcber Sargesdeckel schleift.", "tokens": ["\u00dc\u00b7ber", "Sar\u00b7ges\u00b7de\u00b7ckel", "schleift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter den D\u00e4chern", "tokens": ["Un\u00b7ter", "den", "D\u00e4\u00b7chern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Modert es zahllos", "tokens": ["Mo\u00b7dert", "es", "zahl\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie unter herbstlichen B\u00e4umen", "tokens": ["Wie", "un\u00b7ter", "herbst\u00b7li\u00b7chen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gestorbenes Laub ...", "tokens": ["Ge\u00b7stor\u00b7be\u00b7nes", "Laub", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die V\u00f6lker sind tot!", "tokens": ["Die", "V\u00f6l\u00b7ker", "sind", "tot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wohl sickert warmes Blut", "tokens": ["Wohl", "si\u00b7ckert", "war\u00b7mes", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Durch ihre Adern,", "tokens": ["Durch", "ih\u00b7re", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wohl heben sie im Morgengrau", "tokens": ["Wohl", "he\u00b7ben", "sie", "im", "Mor\u00b7gen\u00b7grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Augenlider und H\u00e4upter:", "tokens": ["Au\u00b7gen\u00b7li\u00b7der", "und", "H\u00e4up\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Doch m\u00fcrrisch wie Gefangne;", "tokens": ["Doch", "m\u00fcr\u00b7risch", "wie", "Ge\u00b7fang\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und m\u00fcrrisch str\u00f6mt es durch die Stra\u00dfe", "tokens": ["Und", "m\u00fcr\u00b7risch", "str\u00f6mt", "es", "durch", "die", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu kerkerhaften Mauern,", "tokens": ["Zu", "ker\u00b7ker\u00b7haf\u00b7ten", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo Menschenleiber sich wandeln", "tokens": ["Wo", "Men\u00b7schen\u00b7lei\u00b7ber", "sich", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Zu R\u00e4derwerk und Balken,", "tokens": ["Zu", "R\u00e4\u00b7der\u00b7werk", "und", "Bal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Zu stumpfen Riesenmaschinen,", "tokens": ["Zu", "stump\u00b7fen", "Rie\u00b7sen\u00b7ma\u00b7schi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die stampfen und schaffen und stampfen,", "tokens": ["Die", "stamp\u00b7fen", "und", "schaf\u00b7fen", "und", "stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Bis drau\u00dfen der sonnige Tag", "tokens": ["Bis", "drau\u00b7\u00dfen", "der", "son\u00b7ni\u00b7ge", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Wehm\u00fctigen Blicks zur Neige geht.", "tokens": ["Weh\u00b7m\u00fc\u00b7ti\u00b7gen", "Blicks", "zur", "Nei\u00b7ge", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und wieder auf die Stra\u00dfe str\u00f6mt es,", "tokens": ["Und", "wie\u00b7der", "auf", "die", "Stra\u00b7\u00dfe", "str\u00f6mt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Aufthun sich die dumpfigen H\u00e4users\u00e4rge,", "tokens": ["Auf\u00b7thun", "sich", "die", "dump\u00b7fi\u00b7gen", "H\u00e4u\u00b7ser\u00b7s\u00e4r\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Die V\u00f6lker strecken sich nieder", "tokens": ["Die", "V\u00f6l\u00b7ker", "stre\u00b7cken", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und liegen tot.", "tokens": ["Und", "lie\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nur heimlich in den H\u00e4upten", "tokens": ["Nur", "heim\u00b7lich", "in", "den", "H\u00e4up\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Keimen Tr\u00e4ume, \u2013", "tokens": ["Kei\u00b7men", "Tr\u00e4u\u00b7me", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wie krankhaft bleiche Keime", "tokens": ["Wie", "krank\u00b7haft", "blei\u00b7che", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Wurzelknollen, die im Keller lagern,", "tokens": ["An", "Wur\u00b7zel\u00b7knol\u00b7len", ",", "die", "im", "Kel\u00b7ler", "la\u00b7gern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sehnlich tasten", "tokens": ["Sehn\u00b7lich", "tas\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nach warmem Sonnenbade. \u2013", "tokens": ["Nach", "war\u00b7mem", "Son\u00b7nen\u00b7ba\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "An meinem Lager h\u00e4lt die Nacht", "tokens": ["An", "mei\u00b7nem", "La\u00b7ger", "h\u00e4lt", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Finster ihre Leichenwacht.", "tokens": ["Fins\u00b7ter", "ih\u00b7re", "Lei\u00b7chen\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch drau\u00dfen \u00fcber die D\u00e4cher", "tokens": ["Doch", "drau\u00b7\u00dfen", "\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geht ein Seufzen;", "tokens": ["Geht", "ein", "Seuf\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Das wird zum St\u00f6hnen,", "tokens": ["Das", "wird", "zum", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zu murrender Klage;", "tokens": ["Zu", "mur\u00b7ren\u00b7der", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Zornig st\u00f6\u00dft ein Wind das Haus,", "tokens": ["Zor\u00b7nig", "st\u00f6\u00dft", "ein", "Wind", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein andrer Wind heult auf,", "tokens": ["Ein", "an\u00b7drer", "Wind", "heult", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und heran st\u00fcrmt es", "tokens": ["Und", "he\u00b7ran", "st\u00fcrmt", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Bedrohlich brausend,", "tokens": ["Be\u00b7droh\u00b7lich", "brau\u00b7send", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Wie tobende Aufruhrrotten.", "tokens": ["Wie", "to\u00b7ben\u00b7de", "Auf\u00b7ruhr\u00b7rot\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Th\u00fcre schlottert, Fenster rasselt,", "tokens": ["Th\u00fc\u00b7re", "schlot\u00b7tert", ",", "Fens\u00b7ter", "ras\u00b7selt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Luke klappt, Dachsparren knarren,", "tokens": ["Lu\u00b7ke", "klappt", ",", "Dach\u00b7spar\u00b7ren", "knar\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Losgel\u00f6ste Ziegel scharren", "tokens": ["Los\u00b7ge\u00b7l\u00f6s\u00b7te", "Zie\u00b7gel", "schar\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcbers Dach und krachen auf das Pflaster.", "tokens": ["\u00dc\u00b7bers", "Dach", "und", "kra\u00b7chen", "auf", "das", "Pflas\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aus schnarchendem Schlaf in Federn", "tokens": ["Aus", "schnar\u00b7chen\u00b7dem", "Schlaf", "in", "Fe\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schrickt der B\u00fcrger empor", "tokens": ["Schrickt", "der", "B\u00fcr\u00b7ger", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und horcht,", "tokens": ["Und", "horcht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Wie's im Kamine schaurig heult", "tokens": ["Wie's", "im", "Ka\u00b7mi\u00b7ne", "schau\u00b7rig", "heult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und durch den Th\u00fcrspalt zischt:", "tokens": ["Und", "durch", "den", "Th\u00fcr\u00b7spalt", "zischt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbherbei, und schl\u00fcpft in die Kammer!", "tokens": ["\u00bb", "her\u00b7bei", ",", "und", "schl\u00fcpft", "in", "die", "Kam\u00b7mer", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Blaset den Narren, blaset!\u00ab", "tokens": ["Bla\u00b7set", "den", "Nar\u00b7ren", ",", "bla\u00b7set", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Und wie am Kirchthurm droben", "tokens": ["Und", "wie", "am", "Kirch\u00b7thurm", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Wetterfahne \u00e4ngstlich kreischt, \u2013", "tokens": ["Die", "Wet\u00b7ter\u00b7fah\u00b7ne", "\u00e4ngst\u00b7lich", "kreischt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis ein wuchtiger Windsto\u00df", "tokens": ["Bis", "ein", "wuch\u00b7ti\u00b7ger", "Wind\u00b7sto\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Von verbogener Stange", "tokens": ["Von", "ver\u00b7bo\u00b7ge\u00b7ner", "Stan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Die Rostige abbricht;", "tokens": ["Die", "Ros\u00b7ti\u00b7ge", "ab\u00b7bricht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sie schollert \u00fcbers Kirchendach", "tokens": ["Sie", "schol\u00b7lert", "\u00fc\u00b7bers", "Kir\u00b7chen\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und prasselt auf das Pflaster", "tokens": ["Und", "pras\u00b7selt", "auf", "das", "Pflas\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Dem 