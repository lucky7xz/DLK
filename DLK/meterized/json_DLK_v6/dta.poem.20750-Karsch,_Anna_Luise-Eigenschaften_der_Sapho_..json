{"dta.poem.20750": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Eigenschaften der Sapho .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aufblicken ist ja kein Verbrechen;", "tokens": ["Auf\u00b7bli\u00b7cken", "ist", "ja", "kein", "Ver\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier denken kann ich schon, und dort mit Thyrsis", "tokens": ["Hier", "den\u00b7ken", "kann", "ich", "schon", ",", "und", "dort", "mit", "Thyr\u00b7sis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "ADV", "$,", "KON", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sprechen.", "tokens": ["spre\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}