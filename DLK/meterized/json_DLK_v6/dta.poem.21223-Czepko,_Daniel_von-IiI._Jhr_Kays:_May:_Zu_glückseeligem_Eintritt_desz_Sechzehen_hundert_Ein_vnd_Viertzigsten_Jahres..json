{"dta.poem.21223": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Jhr Kays: May:  \n Zu gl\u00fcckseeligem Eintritt desz Sechzehen  \n hundert Ein vnd Viertzigsten  \n Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ein newes Jahr ist da. Vnd weil sich Phoebus lenckt", "tokens": ["Ein", "ne\u00b7wes", "Jahr", "ist", "da", ".", "Vnd", "weil", "sich", "Phoe\u00b7bus", "lenckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$.", "KON", "KOUS", "PRF", "NE", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Auff Phryxus seine bahn; Wie kan/ als weit sein Wagen", "tokens": ["Auff", "Phry\u00b7xus", "sei\u00b7ne", "bahn", ";", "Wie", "kan", "/", "als", "weit", "sein", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "PPOSAT", "NN", "$.", "PWAV", "VMFIN", "$(", "KOUS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von beyden Angeln fehrt/ ein Volck was fr\u00f6mers tragen/", "tokens": ["Von", "bey\u00b7den", "An\u00b7geln", "fehrt", "/", "ein", "Volck", "was", "fr\u00f6\u00b7mers", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$(", "ART", "NN", "PWS", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wann es trewe w\u00fcnsch/ auff Janus Taffel henckt?", "tokens": ["Als", "wann", "es", "tre\u00b7we", "w\u00fcnsch", "/", "auff", "Ja\u00b7nus", "Taf\u00b7fel", "henckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "VVFIN", "ADJD", "$(", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O vnser HERR vnd Haupt. Der Sterne giebel gl\u00fchn/", "tokens": ["O", "vn\u00b7ser", "HeRR", "vnd", "Haupt", ".", "Der", "Ster\u00b7ne", "gie\u00b7bel", "gl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "$.", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich seh ein Ewigs Reich vor Frewd vnd Andacht brennen:", "tokens": ["Ich", "seh", "ein", "E\u00b7wigs", "Reich", "vor", "Frewd", "vnd", "An\u00b7dacht", "bren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die V\u00f6lcker wollen ", "tokens": ["Die", "V\u00f6l\u00b7cker", "wol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vnd vor die G\u00f6ttligkeit mit heilgen Opffern knihn.", "tokens": ["Vnd", "vor", "die", "G\u00f6tt\u00b7lig\u00b7keit", "mit", "heil\u00b7gen", "Opf\u00b7fern", "knihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vnd obgleich ", "tokens": ["Vnd", "ob\u00b7gleich"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df es den ", "tokens": ["Da\u00df", "es", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch ist Vns minder hie/ der ", "tokens": ["Doch", "ist", "Vns", "min\u00b7der", "hie", "/", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als in der reinen Lufft der Sonnen s\u00fcsses Licht.", "tokens": ["Als", "in", "der", "rei\u00b7nen", "Lufft", "der", "Son\u00b7nen", "s\u00fcs\u00b7ses", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O hohe Majest\u00e4t: Es m\u00fcsse Ph&#339;bus Jhr/", "tokens": ["O", "ho\u00b7he", "Ma\u00b7jes\u00b7t\u00e4t", ":", "Es", "m\u00fcs\u00b7se", "Ph", "&#339;", "bus", "Ihr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VMFIN", "NE", "$(", "NE", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Thirkrei\u00df Jahr vnd Tag ", "tokens": ["Den", "Thir\u00b7krei\u00df", "Jahr", "vnd", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es m\u00fcsse durch die Welt ein Ewger ", "tokens": ["Es", "m\u00fcs\u00b7se", "durch", "die", "Welt", "ein", "Ew\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd sehet ein ", "tokens": ["Vnd", "se\u00b7het", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}}}}}