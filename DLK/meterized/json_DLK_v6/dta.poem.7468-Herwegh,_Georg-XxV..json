{"dta.poem.7468": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am sch\u00f6nsten Tag um einen Wunsch betrogen,", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "Tag", "um", "ei\u00b7nen", "Wunsch", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und eine Niete jede, jede Karte,", "tokens": ["Und", "ei\u00b7ne", "Nie\u00b7te", "je\u00b7de", ",", "je\u00b7de", "Kar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An meinem Schwerte Scharte nur an Scharte,", "tokens": ["An", "mei\u00b7nem", "Schwer\u00b7te", "Schar\u00b7te", "nur", "an", "Schar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn einmal aus der Scheide ich's gezogen.", "tokens": ["Wenn", "ein\u00b7mal", "aus", "der", "Schei\u00b7de", "ich's", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch halt' ich mutig \u00fcber allen Wogen", "tokens": ["Doch", "halt'", "ich", "mu\u00b7tig", "\u00fc\u00b7ber", "al\u00b7len", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Poesie, die leuchtende Standarte,", "tokens": ["Die", "Poe\u00b7sie", ",", "die", "leuch\u00b7ten\u00b7de", "Stand\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch sie vers\u00f6hn' ich mein Geschick, das harte,", "tokens": ["Durch", "sie", "ver\u00b7s\u00f6hn'", "ich", "mein", "Ge\u00b7schick", ",", "das", "har\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den rauhsten Sturm mit ihrem Regenbogen.", "tokens": ["Den", "rauhs\u00b7ten", "Sturm", "mit", "ih\u00b7rem", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nie t\u00f6nte meine Leier Tod und Fluch,", "tokens": ["Nie", "t\u00f6n\u00b7te", "mei\u00b7ne", "Lei\u00b7er", "Tod", "und", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nie schnitt ich aus des Hyperioniden", "tokens": ["Nie", "schnitt", "ich", "aus", "des", "Hy\u00b7pe\u00b7ri\u00b7o\u00b7ni\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Purpur ein traurig-d\u00fcstres Leichentuch;", "tokens": ["Pur\u00b7pur", "ein", "trau\u00b7rig\u00b7d\u00fcst\u00b7res", "Lei\u00b7chen\u00b7tuch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Der Herr hat mir ein frommes Herz beschieden,", "tokens": ["Der", "Herr", "hat", "mir", "ein", "from\u00b7mes", "Herz", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt ist mir ein heilig, heilig Buch,", "tokens": ["Die", "Welt", "ist", "mir", "ein", "hei\u00b7lig", ",", "hei\u00b7lig", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drin alle Bl\u00e4tter fl\u00fcstern: Frieden! Frieden!", "tokens": ["Drin", "al\u00b7le", "Bl\u00e4t\u00b7ter", "fl\u00fcs\u00b7tern", ":", "Frie\u00b7den", "!", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}