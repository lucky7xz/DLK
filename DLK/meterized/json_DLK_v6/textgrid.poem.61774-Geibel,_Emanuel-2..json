{"textgrid.poem.61774": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Walther, dessen Ruhm erklungen,", "tokens": ["Herr", "Walt\u00b7her", ",", "des\u00b7sen", "Ruhm", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So weit die deutschen Str\u00f6me gehn,", "tokens": ["So", "weit", "die", "deut\u00b7schen", "Str\u00f6\u00b7me", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er sich Land und Leut' ersungen,", "tokens": ["Als", "er", "sich", "Land", "und", "Leut'", "er\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da jauchzt' er auf in Liedeszungen:", "tokens": ["Da", "jauchzt'", "er", "auf", "in", "Lie\u00b7des\u00b7zun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab' ein Lehn! Ich hab' ein Lehn!", "tokens": ["Ich", "hab'", "ein", "Lehn", "!", "Ich", "hab'", "ein", "Lehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Walther von der Vogelweide,", "tokens": ["Herr", "Walt\u00b7her", "von", "der", "Vo\u00b7gel\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fc\u00dftet Ihr, was mir geschehn,", "tokens": ["Und", "w\u00fc\u00df\u00b7tet", "Ihr", ",", "was", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ich zu Freuden kam aus Leide,", "tokens": ["Wie", "ich", "zu", "Freu\u00b7den", "kam", "aus", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr h\u00f6rtet singen mich mit Neide:", "tokens": ["Ihr", "h\u00f6r\u00b7tet", "sin\u00b7gen", "mich", "mit", "Nei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab' ein Lehn! Ich hab' ein Lehn!", "tokens": ["Ich", "hab'", "ein", "Lehn", "!", "Ich", "hab'", "ein", "Lehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Lehn sind eitel rote Rosen,", "tokens": ["Mein", "Lehn", "sind", "ei\u00b7tel", "ro\u00b7te", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tag und Nacht in Bl\u00fcte stehn,", "tokens": ["Die", "Tag", "und", "Nacht", "in", "Bl\u00fc\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Minne lie\u00df es mich erlosen,", "tokens": ["Frau", "Min\u00b7ne", "lie\u00df", "es", "mich", "er\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Scherz bestell' ich's und Liebkosen:", "tokens": ["Mit", "Scherz", "be\u00b7stell'", "ich's", "und", "Lieb\u00b7ko\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "KON", "NN", "$."], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Ich hab' ein Lehn! Ich hab' ein Lehn!", "tokens": ["Ich", "hab'", "ein", "Lehn", "!", "Ich", "hab'", "ein", "Lehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}