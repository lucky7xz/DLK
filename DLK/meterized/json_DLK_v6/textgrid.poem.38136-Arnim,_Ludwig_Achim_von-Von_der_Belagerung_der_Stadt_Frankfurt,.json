{"textgrid.poem.38136": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Von der Belagerung der Stadt Frankfurt,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "(die unterstrichene Worte sind Namen von Schanzen und Gesch\u00fctz.)", "tokens": ["(", "die", "un\u00b7ter\u00b7stri\u00b7che\u00b7ne", "Wor\u00b7te", "sind", "Na\u00b7men", "von", "Schan\u00b7zen", "und", "Ge\u00b7sch\u00fctz", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die Sonn mit klarem Scheine", "tokens": ["Die", "Sonn", "mit", "kla\u00b7rem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erglastet \u00fcberall,", "tokens": ["Er\u00b7glas\u00b7tet", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die k\u00fchlen Br\u00fcnnlein reine", "tokens": ["Die", "k\u00fch\u00b7len", "Br\u00fcnn\u00b7lein", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erlusten Berg und Thal,", "tokens": ["Er\u00b7lus\u00b7ten", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Viel s\u00fc\u00dfer L\u00fcftlein G\u00fcte", "tokens": ["Viel", "s\u00fc\u00b7\u00dfer", "L\u00fcft\u00b7lein", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Auf- und Niedergang,", "tokens": ["Von", "Auf", "und", "Nie\u00b7der\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Aus freyer Stimm, Gem\u00fcthe,", "tokens": ["Aus", "frey\u00b7er", "Stimm", ",", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der hell Waldv\u00f6glein Bl\u00fcthe", "tokens": ["Der", "hell", "Wald\u00b7v\u00f6\u00b7glein", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Frau Nachtigall erklang.", "tokens": ["Frau", "Nach\u00b7ti\u00b7gall", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Walds, der Bl\u00fcmlein Ziere", "tokens": ["Des", "Walds", ",", "der", "Bl\u00fcm\u00b7lein", "Zie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gab Wonn und Freudigkeit,", "tokens": ["Gab", "Wonn", "und", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deutschem Landreviere", "tokens": ["In", "deut\u00b7schem", "Land\u00b7re\u00b7vie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War stille Sicherheit.", "tokens": ["War", "stil\u00b7le", "Si\u00b7cher\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der g\u00fctig Herr und Gotte", "tokens": ["Der", "g\u00fc\u00b7tig", "Herr", "und", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sohn, Vater, heilger Geist", "tokens": ["Sohn", ",", "Va\u00b7ter", ",", "heil\u00b7ger", "Geist"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erl\u00f6st aus aller Nothe,", "tokens": ["Er\u00b7l\u00f6st", "aus", "al\u00b7ler", "No\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus Teufels Macht und Tode", "tokens": ["Aus", "Teu\u00b7fels", "Macht", "und", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sein g\u00f6ttlich Gnad uns rei\u00dft.", "tokens": ["Sein", "g\u00f6tt\u00b7lich", "Gnad", "uns", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Stadt Frankfurt an dem Mayne!", "tokens": ["Stadt", "Frank\u00b7furt", "an", "dem", "May\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dein Lob ist weit und breit,", "tokens": ["Dein", "Lob", "ist", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Treu, Ehr und Glauben reine,", "tokens": ["Treu", ",", "Ehr", "und", "Glau\u00b7ben", "rei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mannliche Redlichkeit", "tokens": ["Mann\u00b7li\u00b7che", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Hast du mit deinem Blute", "tokens": ["Hast", "du", "mit", "dei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erhalten ritterlich.", "tokens": ["Er\u00b7hal\u00b7ten", "rit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vertrau dem Herrn, du Gute,", "tokens": ["Ver\u00b7trau", "dem", "Herrn", ",", "du", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er hilft unschuldgem Blute,", "tokens": ["Er", "hilft", "un\u00b7schuld\u00b7gem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Des sollst du freuen dich.", "tokens": ["Des", "sollst", "du", "freu\u00b7en", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich ritt an einem Morgen", "tokens": ["Ich", "ritt", "an", "ei\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Lust in gr\u00fcnem Wald,", "tokens": ["Mit", "Lust", "in", "gr\u00fc\u00b7nem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Wildes Spur ohn Sorgen,", "tokens": ["Nach", "Wil\u00b7des", "Spur", "ohn", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da sah ich mannichfalt", "tokens": ["Da", "sah", "ich", "man\u00b7nich\u00b7falt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von fernen einherbrechen", "tokens": ["Von", "fer\u00b7nen", "ein\u00b7her\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel Reuter und Landsknecht gut,", "tokens": ["Viel", "Reu\u00b7ter", "und", "Lands\u00b7knecht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mit Schie\u00dfen, Rennen, Stechen,", "tokens": ["Mit", "Schie\u00b7\u00dfen", ",", "Ren\u00b7nen", ",", "Ste\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df mancher zahlt die Zechen", "tokens": ["Da\u00df", "man\u00b7cher", "zahlt", "die", "Ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gar theuer mit seinem Blut.", "tokens": ["Gar", "theu\u00b7er", "mit", "sei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Stadt sie th\u00e4ten beschie\u00dfen,", "tokens": ["Die", "Stadt", "sie", "th\u00e4\u00b7ten", "be\u00b7schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des achten wir gar klein,", "tokens": ["Des", "ach\u00b7ten", "wir", "gar", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man lie\u00df sie's wieder genie\u00dfen,", "tokens": ["Man", "lie\u00df", "sie's", "wie\u00b7der", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schenkt ihnen tapfer ein.", "tokens": ["Schenkt", "ih\u00b7nen", "tap\u00b7fer", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus St\u00fccken, neuen und firnen", "tokens": ["Aus", "St\u00fc\u00b7cken", ",", "neu\u00b7en", "und", "fir\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "KON", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Hie\u00df sie Gott willkomm seyn;", "tokens": ["Hie\u00df", "sie", "Gott", "will\u00b7komm", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Es gab K\u00f6pf, Bein und Hirnen,", "tokens": ["Es", "gab", "K\u00f6pf", ",", "Bein", "und", "Hir\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich mag nicht solcher Birnen,", "tokens": ["Ich", "mag", "nicht", "sol\u00b7cher", "Bir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gott helf ihnen all aus Pein!", "tokens": ["Gott", "helf", "ih\u00b7nen", "all", "aus", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "M\u00e4nnlichen richtet auf,", "tokens": ["M\u00e4nn\u00b7li\u00b7chen", "rich\u00b7tet", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zerstie\u00df manch harte Stirne", "tokens": ["Zer\u00b7stie\u00df", "manch", "har\u00b7te", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So fern in schnellem Lauf.", "tokens": ["So", "fern", "in", "schnel\u00b7lem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Auf seinem Zweiglein sch\u00f6n,", "tokens": ["Auf", "sei\u00b7nem", "Zwei\u00b7glein", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Th\u00e4t manchen Vogel krauen,", "tokens": ["Th\u00e4t", "man\u00b7chen", "Vo\u00b7gel", "krau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df er sich mu\u00dfte rauen,", "tokens": ["Da\u00df", "er", "sich", "mu\u00df\u00b7te", "rau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Federn lassen gehn.", "tokens": ["Die", "Fe\u00b7dern", "las\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Jetzt wehr dich unser ", "tokens": ["Jetzt", "wehr", "dich", "un\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "O Bruder und Schwester gerne", "tokens": ["O", "Bru\u00b7der", "und", "Schwes\u00b7ter", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist Beystand euch gethan,", "tokens": ["Ist", "Beys\u00b7tand", "euch", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es fliehen ", "tokens": ["Es", "flie\u00b7hen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Viel scharfer Nadeln geschwind,", "tokens": ["Viel", "schar\u00b7fer", "Na\u00b7deln", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die alte ", "tokens": ["Die", "al\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Thut 's ", "tokens": ["Thut", "'s"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Her, her ihr b\u00f6sen Kind!", "tokens": ["Her", ",", "her", "ihr", "b\u00f6\u00b7sen", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ihres Liedleins Anefang", "tokens": ["Ih\u00b7res", "Lied\u00b7leins", "A\u00b7ne\u00b7fang"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt man am ", "tokens": ["H\u00f6rt", "man", "am"]