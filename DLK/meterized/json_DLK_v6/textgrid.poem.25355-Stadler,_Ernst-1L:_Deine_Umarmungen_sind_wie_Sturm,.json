{"textgrid.poem.25355": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deine Umarmungen sind wie Sturm,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Umarmungen sind wie Sturm,", "tokens": ["Dei\u00b7ne", "Um\u00b7ar\u00b7mun\u00b7gen", "sind", "wie", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "der uns \u00fcber Weltenabgr\u00fcnde schwenkt,", "tokens": ["der", "uns", "\u00fc\u00b7ber", "Wel\u00b7ten\u00b7ab\u00b7gr\u00fcn\u00b7de", "schwenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deine Umarmungen sind wie wildduftender Regen,", "tokens": ["Dei\u00b7ne", "Um\u00b7ar\u00b7mun\u00b7gen", "sind", "wie", "wild\u00b7duf\u00b7ten\u00b7der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "der das Blut mit Traum und Irrsein tr\u00e4nkt.", "tokens": ["der", "das", "Blut", "mit", "Traum", "und", "Irr\u00b7sein", "tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber dann ist Tag. Nachtschwere Augen brechen auf,", "tokens": ["A\u00b7ber", "dann", "ist", "Tag", ".", "Nacht\u00b7schwe\u00b7re", "Au\u00b7gen", "bre\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$.", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "herwankend aus goldner Vernichtung und Tod,", "tokens": ["her\u00b7wan\u00b7kend", "aus", "gold\u00b7ner", "Ver\u00b7nich\u00b7tung", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Durch Str\u00f6me dunklen Bluts rausch ich zur\u00fcck", "tokens": ["Durch", "Str\u00f6\u00b7me", "dunk\u00b7len", "Bluts", "rausch", "ich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "wie Ebbe, f\u00fchle schneidend eine Not,", "tokens": ["wie", "Eb\u00b7be", ",", "f\u00fch\u00b7le", "schnei\u00b7dend", "ei\u00b7ne", "Not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "H\u00f6re deines Herzens Schlag an meinem Herzen klopfen", "tokens": ["H\u00f6\u00b7re", "dei\u00b7nes", "Her\u00b7zens", "Schlag", "an", "mei\u00b7nem", "Her\u00b7zen", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "und wei\u00df doch: du bist ganz fern und weit.", "tokens": ["und", "wei\u00df", "doch", ":", "du", "bist", "ganz", "fern", "und", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "F\u00fchle: \u00fcberm Feuer dieser Lust, die wir entfacht,", "tokens": ["F\u00fch\u00b7le", ":", "\u00fc\u00b7berm", "Feu\u00b7er", "die\u00b7ser", "Lust", ",", "die", "wir", "ent\u00b7facht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "weht eine Traurigkeit,", "tokens": ["weht", "ei\u00b7ne", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "N\u00e4her an dir! Gew\u00f6lk, das meinem stillern", "tokens": ["N\u00e4\u00b7her", "an", "dir", "!", "Ge\u00b7w\u00f6lk", ",", "das", "mei\u00b7nem", "stil\u00b7lern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$.", "NN", "$,", "PRELS", "PPOSAT", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Tagverlangen dein Gesicht entzieht,", "tokens": ["Tag\u00b7ver\u00b7lan\u00b7gen", "dein", "Ge\u00b7sicht", "ent\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Fremdes, darein du fl\u00fcchtest, drin sich deine Inbrunst,", "tokens": ["Frem\u00b7des", ",", "da\u00b7rein", "du", "fl\u00fcch\u00b7test", ",", "drin", "sich", "dei\u00b7ne", "In\u00b7brunst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "PPER", "VVFIN", "$,", "ADV", "PRF", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "ferne Liebeslitaneien betend, niederkniet,", "tokens": ["fer\u00b7ne", "Lie\u00b7bes\u00b7li\u00b7ta\u00b7nei\u00b7en", "be\u00b7tend", ",", "nie\u00b7der\u00b7kniet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Herzblut, das tropft, verschollene Worte,", "tokens": ["Herz\u00b7blut", ",", "das", "tropft", ",", "ver\u00b7schol\u00b7le\u00b7ne", "Wor\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Streichen \u00fcber hei\u00dfe Stirn, Finger gefaltet,", "tokens": ["Strei\u00b7chen", "\u00fc\u00b7ber", "hei\u00b7\u00dfe", "Stirn", ",", "Fin\u00b7ger", "ge\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-++--+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Blicke z\u00e4rtlich tauend, die ich nie gekannt \u2013", "tokens": ["Bli\u00b7cke", "z\u00e4rt\u00b7lich", "tau\u00b7end", ",", "die", "ich", "nie", "ge\u00b7kannt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "Grenzenloses streckt sich wie ein undurchdringlich", "tokens": ["Gren\u00b7zen\u00b7lo\u00b7ses", "streckt", "sich", "wie", "ein", "un\u00b7durch\u00b7dring\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "KOKOM", "ART", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "tiefes, d\u00e4mmerunggef\u00fclltes Land,", "tokens": ["tie\u00b7fes", ",", "d\u00e4m\u00b7me\u00b7rung\u00b7ge\u00b7f\u00fcll\u00b7tes", "Land", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "G\u00e4rten, zugewachsen, die ins Fr\u00fchlicht eingebl\u00fcht", "tokens": ["G\u00e4r\u00b7ten", ",", "zu\u00b7ge\u00b7wach\u00b7sen", ",", "die", "ins", "Fr\u00fch\u00b7licht", "ein\u00b7ge\u00b7bl\u00fcht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "$,", "PRELS", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "bei deiner Seele stehn \u2013", "tokens": ["bei", "dei\u00b7ner", "See\u00b7le", "stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Ich wei\u00df: du m\u00fc\u00dftest \u00fcber hundert Br\u00fccken,", "tokens": ["Ich", "wei\u00df", ":", "du", "m\u00fc\u00df\u00b7test", "\u00fc\u00b7ber", "hun\u00b7dert", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "weite zugesperrte Stra\u00dfen gehn,", "tokens": ["wei\u00b7te", "zu\u00b7ge\u00b7sperr\u00b7te", "Stra\u00b7\u00dfen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "R\u00fcckw\u00e4rts,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "in dein M\u00e4dchenland zur\u00fcck,", "tokens": ["in", "dein", "M\u00e4d\u00b7chen\u00b7land", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "M\u00fc\u00dftest deine Hand", "tokens": ["M\u00fc\u00df\u00b7test", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "mir geben und das lange St\u00fcck", "tokens": ["mir", "ge\u00b7ben", "und", "das", "lan\u00b7ge", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Mit mir durchwandern,", "tokens": ["Mit", "mir", "durch\u00b7wan\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "bis Erinnerung, Lust und Wehe dir entschw\u00e4nden,", "tokens": ["bis", "E\u00b7rin\u00b7ne\u00b7rung", ",", "Lust", "und", "We\u00b7he", "dir", "ent\u00b7schw\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Und wir in morgendlich begr\u00fcnten Furchen", "tokens": ["Und", "wir", "in", "mor\u00b7gend\u00b7lich", "be\u00b7gr\u00fcn\u00b7ten", "Fur\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "vor dem Tal des neuen Aufgangs st\u00e4nden ...", "tokens": ["vor", "dem", "Tal", "des", "neu\u00b7en", "Auf\u00b7gangs", "st\u00e4n\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Aber du blickst zur\u00fcck. Schrickst auf und schauerst.", "tokens": ["A\u00b7ber", "du", "blickst", "zu\u00b7r\u00fcck", ".", "Schrickst", "auf", "und", "schau\u00b7erst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$.", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "L\u00e4chelst. Und deine Lippen sinken,", "tokens": ["L\u00e4\u00b7chelst", ".", "Und", "dei\u00b7ne", "Lip\u00b7pen", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Gefl\u00fcgel wilder Schw\u00e4ne, \u00fcber meinen Mund,", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel", "wil\u00b7der", "Schw\u00e4\u00b7ne", ",", "\u00fc\u00b7ber", "mei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "als wollten sie sich um Erwachen", "tokens": ["als", "woll\u00b7ten", "sie", "sich", "um", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "und Besinnung trinken.", "tokens": ["und", "Be\u00b7sin\u00b7nung", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}