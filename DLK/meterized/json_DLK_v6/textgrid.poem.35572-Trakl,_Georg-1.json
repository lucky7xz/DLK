{"textgrid.poem.35572": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter trug das Kindlein im wei\u00dfen Mond,", "tokens": ["Mut\u00b7ter", "trug", "das", "Kin\u00b7dlein", "im", "wei\u00b7\u00dfen", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Im Schatten des Nu\u00dfbaums, uralten Hollunders,", "tokens": ["Im", "Schat\u00b7ten", "des", "Nu\u00df\u00b7baums", ",", "ur\u00b7al\u00b7ten", "Hol\u00b7lun\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Trunken vom Safte des Mohns, der Klage der Drossel;", "tokens": ["Trun\u00b7ken", "vom", "Saf\u00b7te", "des", "Mohns", ",", "der", "Kla\u00b7ge", "der", "Dros\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und stille", "tokens": ["Und", "stil\u00b7le"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Neigte in Mitleid sich \u00fcber jene ein b\u00e4rtiges Antlitz", "tokens": ["Neig\u00b7te", "in", "Mit\u00b7leid", "sich", "\u00fc\u00b7ber", "je\u00b7ne", "ein", "b\u00e4r\u00b7ti\u00b7ges", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PRF", "APPR", "PDAT", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Leise im Dunkel des Fensters; und altes Hausger\u00e4t", "tokens": ["Lei\u00b7se", "im", "Dun\u00b7kel", "des", "Fens\u00b7ters", ";", "und", "al\u00b7tes", "Haus\u00b7ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$.", "KON", "ADJA", "NN"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Der V\u00e4ter", "tokens": ["Der", "V\u00e4\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Lag im Verfall; Liebe und herbstliche Tr\u00e4umerei.", "tokens": ["Lag", "im", "Ver\u00b7fall", ";", "Lie\u00b7be", "und", "herbst\u00b7li\u00b7che", "Tr\u00e4u\u00b7me\u00b7rei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Also dunkel der Tag des Jahrs, traurige Kindheit,", "tokens": ["Al\u00b7so", "dun\u00b7kel", "der", "Tag", "des", "Jahrs", ",", "trau\u00b7ri\u00b7ge", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da der Knabe leise zu k\u00fchlen Wassern, silbernen Fischen hinabstieg,", "tokens": ["Da", "der", "Kna\u00b7be", "lei\u00b7se", "zu", "k\u00fch\u00b7len", "Was\u00b7sern", ",", "sil\u00b7ber\u00b7nen", "Fi\u00b7schen", "hin\u00b7ab\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ruh und Antlitz;", "tokens": ["Ruh", "und", "Ant\u00b7litz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da er steinern sich vor rasende Rappen warf,", "tokens": ["Da", "er", "stei\u00b7nern", "sich", "vor", "ra\u00b7sen\u00b7de", "Rap\u00b7pen", "warf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In grauer Nacht sein Stern \u00fcber ihn kam;", "tokens": ["In", "grau\u00b7er", "Nacht", "sein", "Stern", "\u00fc\u00b7ber", "ihn", "kam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Oder wenn er an der frierenden Hand der Mutter", "tokens": ["O\u00b7der", "wenn", "er", "an", "der", "frie\u00b7ren\u00b7den", "Hand", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Abends \u00fcber Sankt Peters herbstlichen Friedhof ging,", "tokens": ["A\u00b7bends", "\u00fc\u00b7ber", "Sankt", "Pe\u00b7ters", "herbst\u00b7li\u00b7chen", "Fried\u00b7hof", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+----+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein zarter Leichnam stille im Dunkel der Kammer lag", "tokens": ["Ein", "zar\u00b7ter", "Leich\u00b7nam", "stil\u00b7le", "im", "Dun\u00b7kel", "der", "Kam\u00b7mer", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und jener die kalten Lider \u00fcber ihn aufhob.", "tokens": ["Und", "je\u00b7ner", "die", "kal\u00b7ten", "Li\u00b7der", "\u00fc\u00b7ber", "ihn", "auf\u00b7hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Er aber war ein kleiner Vogel im kahlen Ge\u00e4st,", "tokens": ["Er", "a\u00b7ber", "war", "ein", "klei\u00b7ner", "Vo\u00b7gel", "im", "kah\u00b7len", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Glocke lang im Abendnovember,", "tokens": ["Die", "Glo\u00b7cke", "lang", "im", "A\u00b7bend\u00b7no\u00b7vem\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Vaters Stille, da er im Schlaf die d\u00e4mmernde Wendeltreppe hinabstieg.", "tokens": ["Des", "Va\u00b7ters", "Stil\u00b7le", ",", "da", "er", "im", "Schlaf", "die", "d\u00e4m\u00b7mern\u00b7de", "Wen\u00b7del\u00b7trep\u00b7pe", "hin\u00b7ab\u00b7stieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}}}}}