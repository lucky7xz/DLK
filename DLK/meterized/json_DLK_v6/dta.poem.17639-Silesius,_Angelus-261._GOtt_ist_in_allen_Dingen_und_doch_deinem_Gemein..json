{"dta.poem.17639": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "261.  GOtt ist in allen Dingen/ und doch  \n deinem Gemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das wesen Gottes macht sich keinem Ding gemein:", "tokens": ["Das", "we\u00b7sen", "Got\u00b7tes", "macht", "sich", "kei\u00b7nem", "Ding", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00df nothwendig doch auch in den Teuffeln seyn.", "tokens": ["Und", "mu\u00df", "noth\u00b7wen\u00b7dig", "doch", "auch", "in", "den", "Teuf\u00b7feln", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}