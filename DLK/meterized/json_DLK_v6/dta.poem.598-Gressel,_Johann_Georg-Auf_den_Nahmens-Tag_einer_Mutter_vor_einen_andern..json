{"dta.poem.598": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer Mutter/  \n vor einen andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Geehrteste/ nun ist es Zeit", "tokens": ["Ge\u00b7ehr\u00b7tes\u00b7te", "/", "nun", "ist", "es", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich auf Gebeht und Wunsch zu schicken/", "tokens": ["Sich", "auf", "Ge\u00b7beht", "und", "Wunsch", "zu", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heut ist der Tag der Lieblichkeit/", "tokens": ["Heut", "ist", "der", "Tag", "der", "Lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit den angenehmsten Blicken/", "tokens": ["Der", "mit", "den", "an\u00b7ge\u00b7nehms\u00b7ten", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit des vollen Lichtes Pracht", "tokens": ["Und", "mit", "des", "vol\u00b7len", "Lich\u00b7tes", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den werthen Nahmen kundbahr macht/", "tokens": ["Den", "wert\u00b7hen", "Nah\u00b7men", "kund\u00b7bahr", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heut ist der Tag/ nun ist es Zeit.", "tokens": ["Heut", "ist", "der", "Tag", "/", "nun", "ist", "es", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}