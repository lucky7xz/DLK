{"textgrid.poem.63534": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie so gerne", "tokens": ["Ach", ",", "wie", "so", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bleib' ich euch ferne,", "tokens": ["Bleib'", "ich", "euch", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schimmernde S\u00e4le, von Kerzen erhellt!", "tokens": ["Schim\u00b7mern\u00b7de", "S\u00e4\u00b7le", ",", "von", "Ker\u00b7zen", "er\u00b7hellt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Da\u00df mir im Dunkeln", "tokens": ["Da\u00df", "mir", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Zwei Augen funkeln,", "tokens": ["Zwei", "Au\u00b7gen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ist meine Wonne, ist meine Welt!", "tokens": ["Ist", "mei\u00b7ne", "Won\u00b7ne", ",", "ist", "mei\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sucht' ich doch allen", "tokens": ["Sucht'", "ich", "doch", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Einst zu gefallen,", "tokens": ["Einst", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Habe verstohlen die Netze gestellt.", "tokens": ["Ha\u00b7be", "ver\u00b7stoh\u00b7len", "die", "Net\u00b7ze", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Einem mich schm\u00fccken,", "tokens": ["Ei\u00b7nem", "mich", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PRF", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Einen begl\u00fccken,", "tokens": ["Ei\u00b7nen", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ward meine Wonne, ward meine Welt!", "tokens": ["Ward", "mei\u00b7ne", "Won\u00b7ne", ",", "ward", "mei\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Einsam im stillen", "tokens": ["Ein\u00b7sam", "im", "stil\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Um seinetwillen", "tokens": ["Um", "sei\u00b7net\u00b7wil\u00b7len"], "token_info": ["word", "word"], "pos": ["KOUI", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Pocht mir das Herz, von Sehnsucht geschwellt:", "tokens": ["Pocht", "mir", "das", "Herz", ",", "von", "Sehn\u00b7sucht", "ge\u00b7schwellt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihn zu umfangen,", "tokens": ["Ihn", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "An ihm zu hangen,", "tokens": ["An", "ihm", "zu", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bis mir in Wonnen schwindet die Welt!", "tokens": ["Bis", "mir", "in", "Won\u00b7nen", "schwin\u00b7det", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}