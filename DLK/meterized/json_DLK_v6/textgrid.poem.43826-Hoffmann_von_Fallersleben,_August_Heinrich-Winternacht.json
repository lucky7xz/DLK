{"textgrid.poem.43826": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist so herrlich die Winternacht!", "tokens": ["Wie", "ist", "so", "herr\u00b7lich", "die", "Win\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es gl\u00e4nzt der Mond in voller Pracht", "tokens": ["Es", "gl\u00e4nzt", "der", "Mond", "in", "vol\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit den silbernen Sternen am Himmelszelt.", "tokens": ["Mit", "den", "sil\u00b7ber\u00b7nen", "Ster\u00b7nen", "am", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Es zieht der Frost durch Wald und Feld", "tokens": ["Es", "zieht", "der", "Frost", "durch", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberspinnet jedes Reis", "tokens": ["Und", "\u00fc\u00b7bers\u00b7pin\u00b7net", "je\u00b7des", "Reis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Halme silberwei\u00df.", "tokens": ["Und", "al\u00b7le", "Hal\u00b7me", "sil\u00b7ber\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er hauchet \u00fcber dem See, und im Nu,", "tokens": ["Er", "hau\u00b7chet", "\u00fc\u00b7ber", "dem", "See", ",", "und", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "APPRART", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch eh' wir's denken, friert er zu.", "tokens": ["Noch", "eh'", "wir's", "den\u00b7ken", ",", "friert", "er", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So hat der Winter auch unser gedacht", "tokens": ["So", "hat", "der", "Win\u00b7ter", "auch", "un\u00b7ser", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und \u00fcber Nacht uns Freude gebracht.", "tokens": ["Und", "\u00fc\u00b7ber", "Nacht", "uns", "Freu\u00b7de", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nun wollen wir auch dem Winter nicht grollen", "tokens": ["Nun", "wol\u00b7len", "wir", "auch", "dem", "Win\u00b7ter", "nicht", "grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ihm auch Lieder des Dankes zollen.", "tokens": ["Und", "ihm", "auch", "Lie\u00b7der", "des", "Dan\u00b7kes", "zol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}