{"dta.poem.16194": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "248.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der gr\u00f6\u00dfre Bruder soll die kleinern \u00fcberwachen,", "tokens": ["Der", "gr\u00f6\u00df\u00b7re", "Bru\u00b7der", "soll", "die", "klei\u00b7nern", "\u00fc\u00b7ber\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diese sollen ihn zu ihrem Vorbild machen.", "tokens": ["Und", "die\u00b7se", "sol\u00b7len", "ihn", "zu", "ih\u00b7rem", "Vor\u00b7bild", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So tritt er halb und halb schon an des Vaters Statt,", "tokens": ["So", "tritt", "er", "halb", "und", "halb", "schon", "an", "des", "Va\u00b7ters", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihnen er vielleicht einst zu vertreten hat.", "tokens": ["Die", "ih\u00b7nen", "er", "viel\u00b7leicht", "einst", "zu", "ver\u00b7tre\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}