{"textgrid.poem.57960": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Maienlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mai ist gekommen,", "tokens": ["Der", "Mai", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Torfmann br\u00fcllt aus,", "tokens": ["Der", "Torf\u00b7mann", "br\u00fcllt", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da bleibe, wer Lust hat,", "tokens": ["Da", "blei\u00b7be", ",", "wer", "Lust", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In Hannover zu Haus.", "tokens": ["In", "Han\u00b7no\u00b7ver", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Kirschenfritz schreit bald,", "tokens": ["Der", "Kir\u00b7schen\u00b7fritz", "schreit", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df mir 's Trommelfell gellt,", "tokens": ["Da\u00df", "mir", "'s", "Trom\u00b7mel\u00b7fell", "gellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ich glaub', ich verdufte", "tokens": ["Ich", "glaub'", ",", "ich", "ver\u00b7duf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Nach ausw\u00e4rts in die Welt.", "tokens": ["Nach", "aus\u00b7w\u00e4rts", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Vater, Frau Mutter,", "tokens": ["Herr", "Va\u00b7ter", ",", "Frau", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es wird mir zu dumm,", "tokens": ["Es", "wird", "mir", "zu", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf dem Nachbar sein'm Grundst\u00fcck", "tokens": ["Auf", "dem", "Nach\u00b7bar", "sein'm", "Grund\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dreht 'n Karussell sich 'rum,", "tokens": ["Dreht", "'n", "Ka\u00b7rus\u00b7sell", "sich", "'rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es wimmert die Orgel,", "tokens": ["Es", "wim\u00b7mert", "die", "Or\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich halt's nicht mehr aus,", "tokens": ["Ich", "halt's", "nicht", "mehr", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es treibt zur Eilenriede", "tokens": ["Es", "treibt", "zur", "Ei\u00b7len\u00b7rie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich m\u00e4chtig hinaus.", "tokens": ["Mich", "m\u00e4ch\u00b7tig", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "O weh, es ist Sonntag,", "tokens": ["O", "weh", ",", "es", "ist", "Sonn\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich bin sch\u00f6n lackiert,", "tokens": ["Ich", "bin", "sch\u00f6n", "la\u00b7ckiert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In jedem Lokale", "tokens": ["In", "je\u00b7dem", "Lo\u00b7ka\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wird drauf los konzertiert,", "tokens": ["Wird", "drauf", "los", "kon\u00b7zer\u00b7tiert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von der List bis nach D\u00f6hren", "tokens": ["Von", "der", "List", "bis", "nach", "D\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ist \u00fcberall Musik,", "tokens": ["Ist", "\u00fc\u00b7be\u00b7rall", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich mache mich d\u00fcnne,", "tokens": ["Ich", "ma\u00b7che", "mich", "d\u00fcn\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Denn ich habe es dick.", "tokens": ["Denn", "ich", "ha\u00b7be", "es", "dick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "O Wandern, o Wandern,", "tokens": ["O", "Wan\u00b7dern", ",", "o", "Wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du frohe Burschenlust,", "tokens": ["Du", "fro\u00b7he", "Bur\u00b7schen\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich blieb' gern zu Hause,", "tokens": ["Ich", "blieb'", "gern", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Doch hab' ich fortgemu\u00dft.", "tokens": ["Doch", "hab'", "ich", "fort\u00b7ge\u00b7mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie die Wolken wandern", "tokens": ["Wie", "die", "Wol\u00b7ken", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Am blauen Himmel 'rum,", "tokens": ["Am", "blau\u00b7en", "Him\u00b7mel", "'rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So mach' ich es gerade,", "tokens": ["So", "mach'", "ich", "es", "ge\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn zu Hause ist's zu dumm.", "tokens": ["Denn", "zu", "Hau\u00b7se", "ist's", "zu", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}