{"dta.poem.7315": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der zu fr\u00fch geborene Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Acht Tage z\u00e4hlt' er schon, eh ihn", "tokens": ["Acht", "Ta\u00b7ge", "z\u00e4hlt'", "er", "schon", ",", "eh", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Amme konnte stillen,", "tokens": ["Die", "Am\u00b7me", "konn\u00b7te", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein W\u00fcrmchen, saugend k\u00fcmmerlich", "tokens": ["Ein", "W\u00fcrm\u00b7chen", ",", "sau\u00b7gend", "k\u00fcm\u00b7mer\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Zucker und Kamillen,", "tokens": ["An", "Zu\u00b7cker", "und", "Ka\u00b7mil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Statt Nagel nur ein H\u00e4utchen lind,", "tokens": ["Statt", "Na\u00b7gel", "nur", "ein", "H\u00e4ut\u00b7chen", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "D\u00e4umlein wie Vogelsporen,", "tokens": ["D\u00e4um\u00b7lein", "wie", "Vo\u00b7gel\u00b7spo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und Jeder sagte: \u201earmes Kind!", "tokens": ["Und", "Je\u00b7der", "sag\u00b7te", ":", "\u201e", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist zu fr\u00fch geboren!\u201c", "tokens": ["Es", "ist", "zu", "fr\u00fch", "ge\u00b7bo\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wuchs er auf, und mit der Zeit", "tokens": ["Doch", "wuchs", "er", "auf", ",", "und", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Leben sich entwickelt,", "tokens": ["Hat", "Le\u00b7ben", "sich", "ent\u00b7wi\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr als der Doktor prophezeit,", "tokens": ["Mehr", "als", "der", "Dok\u00b7tor", "pro\u00b7phe\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4tt' er ihn zerst\u00fcckelt;", "tokens": ["Und", "h\u00e4tt'", "er", "ihn", "zer\u00b7st\u00fc\u00b7ckelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im z\u00e4hen K\u00f6rper zeigte sich", "tokens": ["Im", "z\u00e4\u00b7hen", "K\u00f6r\u00b7per", "zeig\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Z\u00e4h wilder Seele Streben;", "tokens": ["Z\u00e4h", "wil\u00b7der", "See\u00b7le", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Einmal erfa\u00dft \u2014 dann sicherlich", "tokens": ["Ein\u00b7mal", "er\u00b7fa\u00dft", "dann", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$(", "ADV", "PRF"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Hielt er, auf Tod und Leben.", "tokens": ["Hielt", "er", ",", "auf", "Tod", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In B\u00fcchern hat er sich studirt", "tokens": ["In", "B\u00fc\u00b7chern", "hat", "er", "sich", "stu\u00b7dirt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hohl\u00e4ugig und zu Schanden,", "tokens": ["Hoh\u00b7l\u00e4u\u00b7gig", "und", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch sein gl\u00fches Hirn gef\u00fchrt", "tokens": ["Und", "durch", "sein", "gl\u00fc\u00b7hes", "Hirn", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zahllose Liederbanden.", "tokens": ["Zahl\u00b7lo\u00b7se", "Lie\u00b7der\u00b7ban\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ein steter Drang \u2014 hinauf! hinauf!", "tokens": ["Ein", "ste\u00b7ter", "Drang", "hin\u00b7auf", "!", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ringsum keine Palme;", "tokens": ["Und", "ring\u00b7sum", "kei\u00b7ne", "Pal\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So klomm er an der Weide auf", "tokens": ["So", "klomm", "er", "an", "der", "Wei\u00b7de", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jauchzte in die Alme.", "tokens": ["Und", "jauchz\u00b7te", "in", "die", "Al\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zwar d\u00fcnkt ihn oft, bei tr\u00fcbem Muth,", "tokens": ["Zwar", "d\u00fcnkt", "ihn", "oft", ",", "bei", "tr\u00fc\u00b7bem", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Baldachin von Laube", "tokens": ["Sein", "Bal\u00b7da\u00b7ch\u00b7in", "von", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So k\u00f6stlich wie ein alter Hut,", "tokens": ["So", "k\u00f6st\u00b7lich", "wie", "ein", "al\u00b7ter", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie 'ne zerrissne Haube;", "tokens": ["Wie", "'ne", "zer\u00b7riss\u00b7ne", "Hau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein dies schalt man \u201eeitlen Drang,", "tokens": ["Al\u00b7lein", "dies", "schalt", "man", "\u201e", "eit\u00b7len", "Drang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PIS", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit W\u00fcrde abzutrumpfen!\u201c", "tokens": ["Mit", "W\u00fcr\u00b7de", "ab\u00b7zu\u00b7trump\u00b7fen", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVIZU", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Alles was er sah, das sang", "tokens": ["Und", "Al\u00b7les", "was", "er", "sah", ",", "das", "sang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Herab vom Weidenstumpfen.", "tokens": ["Her\u00b7ab", "vom", "Wei\u00b7den\u00b7stump\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So ward denn eine werthe Zeit", "tokens": ["So", "ward", "denn", "ei\u00b7ne", "wert\u00b7he", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertr\u00f6delt und verstammelt,", "tokens": ["Ver\u00b7tr\u00f6\u00b7delt", "und", "ver\u00b7stam\u00b7melt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lichtblonde Liederlein juchheit,", "tokens": ["Licht\u00b7blon\u00b7de", "Lie\u00b7der\u00b7lein", "juch\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und Weidenduft gesammelt;", "tokens": ["Und", "Wei\u00b7den\u00b7duft", "ge\u00b7sam\u00b7melt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohl fielen Thr\u00e4nen in den Flaum", "tokens": ["Wohl", "fie\u00b7len", "Thr\u00e4\u00b7nen", "in", "den", "Flaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schimmerten am Raine,", "tokens": ["Und", "schim\u00b7mer\u00b7ten", "am", "Rai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erfa\u00dfte ihn der gl\u00fche Traum", "tokens": ["Er\u00b7fa\u00df\u00b7te", "ihn", "der", "gl\u00fc\u00b7he", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem Palmenhaine.", "tokens": ["Von", "ei\u00b7nem", "Pal\u00b7men\u00b7hai\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als das Leben ausgebrannt", "tokens": ["Und", "als", "das", "Le\u00b7ben", "aus\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchlte sich vergehen,", "tokens": ["Und", "f\u00fchl\u00b7te", "sich", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sollt' wie Moses er das Land", "tokens": ["Da", "sollt'", "wie", "Mo\u00b7ses", "er", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "KOKOM", "NE", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gottverhei\u00dfung sehen;", "tokens": ["Der", "Gott\u00b7ver\u00b7hei\u00b7\u00dfung", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sah, er sah sie Schaft an Schaft", "tokens": ["Er", "sah", ",", "er", "sah", "sie", "Schaft", "an", "Schaft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die heil'gen Kronen tragen,", "tokens": ["Die", "heil'\u00b7gen", "Kro\u00b7nen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und drunter all die frische Kraft", "tokens": ["Und", "drun\u00b7ter", "all", "die", "fri\u00b7sche", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der edlen Sprossen ragen.", "tokens": ["Der", "ed\u00b7len", "Spros\u00b7sen", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Lieder h\u00f6rt' er, Melodien,", "tokens": ["Und", "Lie\u00b7der", "h\u00f6rt'", "er", ",", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ihm im Traum geklungen,", "tokens": ["Wie", "ihm", "im", "Traum", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ein Kristall der Gletscher schien,", "tokens": ["Wenn", "ein", "Kris\u00b7tall", "der", "Glet\u00b7scher", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Adler sich geschwungen;", "tokens": ["Und", "Ad\u00b7ler", "sich", "ge\u00b7schwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch das smaragdne Riesenlaub", "tokens": ["Durch", "das", "sma\u00b7ragd\u00b7ne", "Rie\u00b7sen\u00b7laub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sah er die Lyra blinken,", "tokens": ["Sah", "er", "die", "Ly\u00b7ra", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und \u00fcber sie gleich goldnem Staub", "tokens": ["Und", "\u00fc\u00b7ber", "sie", "gleich", "gold\u00b7nem", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Levante's Aether sinken.", "tokens": ["Le\u00b7van\u00b7te's", "A\u00b7e\u00b7ther", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O, wie zusammen da im Fall", "tokens": ["O", ",", "wie", "zu\u00b7sam\u00b7men", "da", "im", "Fall"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alten T\u00f6ne schwirrten,", "tokens": ["Die", "al\u00b7ten", "T\u00f6\u00b7ne", "schwirr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Busen die Gefangnen all", "tokens": ["Im", "Bu\u00b7sen", "die", "Ge\u00b7fang\u00b7nen", "all"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren Ketten klirrten!", "tokens": ["Mit", "ih\u00b7ren", "Ket\u00b7ten", "klirr\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eha, Leben, Jahre! und mein Sitz", "tokens": ["\u201e", "ha", ",", "Le\u00b7ben", ",", "Jah\u00b7re", "!", "und", "mein", "Sitz"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "NN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist in den S\u00e4ulenw\u00e4nden,", "tokens": ["Ist", "in", "den", "S\u00e4u\u00b7len\u00b7w\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch meine Lyra soll den Blitz", "tokens": ["Auch", "mei\u00b7ne", "Ly\u00b7ra", "soll", "den", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch die Smaragden senden!\u201c", "tokens": ["Durch", "die", "Sma\u00b7rag\u00b7den", "sen\u00b7den", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.9": {"line.1": {"text": "Ach, arme Frist, an solchem Schaft", "tokens": ["Ach", ",", "ar\u00b7me", "Frist", ",", "an", "sol\u00b7chem", "Schaft"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit mattem Fu\u00df zu klimmen,", "tokens": ["Mit", "mat\u00b7tem", "Fu\u00df", "zu", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sehne seiner Jugendkraft,", "tokens": ["Die", "Seh\u00b7ne", "sei\u00b7ner", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vermag er sie zu stimmen?", "tokens": ["Ver\u00b7mag", "er", "sie", "zu", "stim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und bald erseufzt er: \u201ehin ist hin!", "tokens": ["Und", "bald", "er\u00b7seufzt", "er", ":", "\u201e", "hin", "ist", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vertr\u00f6delt ist verloren!", "tokens": ["Ver\u00b7tr\u00f6\u00b7delt", "ist", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Scholle winkt, weh mir, ich bin", "tokens": ["Die", "Schol\u00b7le", "winkt", ",", "weh", "mir", ",", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu fr\u00fch, zu fr\u00fch geboren!", "tokens": ["Zu", "fr\u00fch", ",", "zu", "fr\u00fch", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}