{"textgrid.poem.33905": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Das Haus im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin im Walde gegangen,", "tokens": ["Ich", "bin", "im", "Wal\u00b7de", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da traf ich ein kleines Haus,", "tokens": ["Da", "traf", "ich", "ein", "klei\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort gingen die Engel Gottes", "tokens": ["Dort", "gin\u00b7gen", "die", "En\u00b7gel", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sichtbarlich ein und aus.", "tokens": ["Sicht\u00b7bar\u00b7lich", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das G\u00e4rtchen, umher gezogen,", "tokens": ["Das", "G\u00e4rt\u00b7chen", ",", "um\u00b7her", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bot Aepfel und Birnen genug,", "tokens": ["Bot", "A\u00b7e\u00b7pfel", "und", "Bir\u00b7nen", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein Weinstock spann sich durch's Fenster,", "tokens": ["Ein", "Wein\u00b7stock", "spann", "sich", "durch's", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der duftige Trauben trug.", "tokens": ["Der", "duf\u00b7ti\u00b7ge", "Trau\u00b7ben", "trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Mutter s\u00e4ugte den Knaben,", "tokens": ["Die", "Mut\u00b7ter", "s\u00e4ug\u00b7te", "den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie neigte sich \u00fcber ihn,", "tokens": ["Sie", "neig\u00b7te", "sich", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ihre rosige Wange", "tokens": ["Da\u00df", "ih\u00b7re", "ro\u00b7si\u00b7ge", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Abglanz der seinigen schien.", "tokens": ["Ein", "Ab\u00b7glanz", "der", "sei\u00b7ni\u00b7gen", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Nun pfl\u00fcckt sie die schwerste der Trauben,", "tokens": ["Nun", "pfl\u00fcckt", "sie", "die", "schwers\u00b7te", "der", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die selbst die Schulter ihr tickt,", "tokens": ["Die", "selbst", "die", "Schul\u00b7ter", "ihr", "tickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Rebe will sie erquicken,", "tokens": ["Die", "Re\u00b7be", "will", "sie", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie sie ihr Kind erquickt.", "tokens": ["Wie", "sie", "ihr", "Kind", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und vor ihr, auf dem Tische,", "tokens": ["Und", "vor", "ihr", ",", "auf", "dem", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht eine Flasche Wein,", "tokens": ["Steht", "ei\u00b7ne", "Fla\u00b7sche", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Becher dabei, die werden", "tokens": ["Ein", "Be\u00b7cher", "da\u00b7bei", ",", "die", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PAV", "$,", "PRELS", "VAINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl f\u00fcr den Gatten sein!", "tokens": ["Wohl", "f\u00fcr", "den", "Gat\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ger\u00e4usch! \u2013 \u00bbDein Vater, Knabe!\u00ab", "tokens": ["Ge\u00b7r\u00e4usch", "!", "\u2013", "\u00bb", "Dein", "Va\u00b7ter", ",", "Kna\u00b7be", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "$(", "PPOSAT", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie schenkt den Becher voll.", "tokens": ["Sie", "schenkt", "den", "Be\u00b7cher", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch nicht! Die Birne fiel nur,", "tokens": ["Noch", "nicht", "!", "Die", "Bir\u00b7ne", "fiel", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sie ihm reichen soll.", "tokens": ["Die", "sie", "ihm", "rei\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich m\u00f6gte vor sie treten,", "tokens": ["Ich", "m\u00f6g\u00b7te", "vor", "sie", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist noch eben Zeit,", "tokens": ["Es", "ist", "noch", "e\u00b7ben", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprechen: la\u00df mich trinken,", "tokens": ["Und", "spre\u00b7chen", ":", "la\u00df", "mich", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich habe noch so weit!", "tokens": ["Ich", "ha\u00b7be", "noch", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie w\u00fcrde den Trunk mir reichen,", "tokens": ["Sie", "w\u00fcr\u00b7de", "den", "Trunk", "mir", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der ihm beschieden war,", "tokens": ["Der", "ihm", "be\u00b7schie\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir w\u00fcrde sein, als b\u00f6t' ihn", "tokens": ["Mir", "w\u00fcr\u00b7de", "sein", ",", "als", "b\u00f6t'", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VAINF", "$,", "KOUS", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Friede selbst mir dar.", "tokens": ["Der", "Frie\u00b7de", "selbst", "mir", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch nein, ich will mich wenden,", "tokens": ["Doch", "nein", ",", "ich", "will", "mich", "wen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wald ist dick und wild,", "tokens": ["Der", "Wald", "ist", "dick", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will in den Wald mich verlieren,", "tokens": ["Ich", "will", "in", "den", "Wald", "mich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wer tritt hinein in ein Bild!", "tokens": ["Wer", "tritt", "hin\u00b7ein", "in", "ein", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}