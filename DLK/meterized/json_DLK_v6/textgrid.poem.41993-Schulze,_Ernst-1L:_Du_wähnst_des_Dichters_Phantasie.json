{"textgrid.poem.41993": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du w\u00e4hnst des Dichters Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du w\u00e4hnst des Dichters Phantasie", "tokens": ["Du", "w\u00e4hnst", "des", "Dich\u00b7ters", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begn\u00fcg', um lodernd zu entbrennen,", "tokens": ["Be\u00b7gn\u00fcg'", ",", "um", "lo\u00b7dernd", "zu", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem blo\u00dfen Blick sich nie?", "tokens": ["Mit", "ei\u00b7nem", "blo\u00b7\u00dfen", "Blick", "sich", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, du mu\u00dft deinen Blick nicht kennen!", "tokens": ["Ach", ",", "du", "mu\u00dft", "dei\u00b7nen", "Blick", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind Lieb' und Lieder nicht verwandt?", "tokens": ["Sind", "Lieb'", "und", "Lie\u00b7der", "nicht", "ver\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Apollo, den wir Vater nennen,", "tokens": ["A\u00b7pol\u00b7lo", ",", "den", "wir", "Va\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Geht mit der Anmuth Hand in Hand,", "tokens": ["Geht", "mit", "der", "An\u00b7muth", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sollt' er deinen Blick nicht kennen?", "tokens": ["Wie", "sollt'", "er", "dei\u00b7nen", "Blick", "nicht", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Band das sich um Beide schlingt", "tokens": ["Das", "Band", "das", "sich", "um", "Bei\u00b7de", "schlingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PRF", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kaltes Herz nur kann es trennen;", "tokens": ["Ein", "kal\u00b7tes", "Herz", "nur", "kann", "es", "tren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer liebt und nicht von Liebe singt,", "tokens": ["Wer", "liebt", "und", "nicht", "von", "Lie\u00b7be", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, der mu\u00df deinen Blick nicht kennen!", "tokens": ["Ach", ",", "der", "mu\u00df", "dei\u00b7nen", "Blick", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Liebe naht so s\u00fc\u00df und still,", "tokens": ["Die", "Lie\u00b7be", "naht", "so", "s\u00fc\u00df", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wollt' ihr nicht ein Pl\u00e4tzchen g\u00f6nnen?", "tokens": ["Wer", "wollt'", "ihr", "nicht", "ein", "Pl\u00e4tz\u00b7chen", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer sie nie behausen will,", "tokens": ["Doch", "wer", "sie", "nie", "be\u00b7hau\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, der mu\u00df deinen Blick nicht kennen!", "tokens": ["Ach", ",", "der", "mu\u00df", "dei\u00b7nen", "Blick", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}