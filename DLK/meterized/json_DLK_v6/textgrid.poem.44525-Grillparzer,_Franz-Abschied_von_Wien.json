{"textgrid.poem.44525": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Wien", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb wohl, du stolze Kaiserstadt,", "tokens": ["Leb", "wohl", ",", "du", "stol\u00b7ze", "Kai\u00b7ser\u00b7stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar nicht auf lange, denk ich;", "tokens": ["Zwar", "nicht", "auf", "lan\u00b7ge", ",", "denk", "ich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ADV", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu andern Grenzen, lebensmatt,", "tokens": ["Zu", "an\u00b7dern", "Gren\u00b7zen", ",", "le\u00b7bens\u00b7matt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die irren Schritte lenk ich.", "tokens": ["Die", "ir\u00b7ren", "Schrit\u00b7te", "lenk", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n bist du, doch gef\u00e4hrlich auch,", "tokens": ["Sch\u00f6n", "bist", "du", ",", "doch", "ge\u00b7f\u00e4hr\u00b7lich", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Sch\u00fcler wie dem Meister,", "tokens": ["Dem", "Sch\u00fc\u00b7ler", "wie", "dem", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entnervend weht dein Sommerhauch,", "tokens": ["Ent\u00b7ner\u00b7vend", "weht", "dein", "Som\u00b7mer\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Du Kapua der Geister.", "tokens": ["Du", "Ka\u00b7pua", "der", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf deinen Fluren geht sichs weich,", "tokens": ["Auf", "dei\u00b7nen", "Flu\u00b7ren", "geht", "sichs", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Berg und W\u00e4lder breiten", "tokens": ["Und", "Berg", "und", "W\u00e4l\u00b7der", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rings um dich her ein Zauberreich,", "tokens": ["Rings", "um", "dich", "her", "ein", "Zau\u00b7ber\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Durch das die Str\u00f6me gleiten.", "tokens": ["Durch", "das", "die", "Str\u00f6\u00b7me", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Weithin Musik, wie wenn im Baum", "tokens": ["Weit\u00b7hin", "Mu\u00b7sik", ",", "wie", "wenn", "im", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "KOKOM", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der V\u00f6gel Chor erwachte,", "tokens": ["Der", "V\u00f6\u00b7gel", "Chor", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man spricht nicht, denkt wohl etwa kaum", "tokens": ["Man", "spricht", "nicht", ",", "denkt", "wohl", "et\u00b7wa", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlt das Halb-Gedachte.", "tokens": ["Und", "f\u00fchlt", "das", "Halb\u00b7Ge\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dazu dein Volk, ein wackres Herz,", "tokens": ["Da\u00b7zu", "dein", "Volk", ",", "ein", "wack\u00b7res", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstand, und vom gesunden,", "tokens": ["Ver\u00b7stand", ",", "und", "vom", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sich mit M\u00e4rchen und mit Scherz", "tokens": ["Das", "sich", "mit", "M\u00e4r\u00b7chen", "und", "mit", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wahrheit Bild umwunden.", "tokens": ["Der", "Wahr\u00b7heit", "Bild", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Man lebt in halber Poesie,", "tokens": ["Man", "lebt", "in", "hal\u00b7ber", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gef\u00e4hrlich f\u00fcr die ganze,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "f\u00fcr", "die", "gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist ein Dichter, ob man nie", "tokens": ["Und", "ist", "ein", "Dich\u00b7ter", ",", "ob", "man", "nie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Vers gedacht und Stanze.", "tokens": ["An", "Vers", "ge\u00b7dacht", "und", "Stan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch weil, von so viel Sch\u00f6nheit voll,", "tokens": ["Doch", "weil", ",", "von", "so", "viel", "Sch\u00f6n\u00b7heit", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "APPR", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir nur zu atmen brauchen,", "tokens": ["Wir", "nur", "zu", "at\u00b7men", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00dft man, was zum Herzen quoll,", "tokens": ["Ver\u00b7gi\u00dft", "man", ",", "was", "zum", "Her\u00b7zen", "quoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch wieder auszuhauchen:", "tokens": ["Auch", "wie\u00b7der", "aus\u00b7zu\u00b7hau\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Tafel bleibt, die Leinwand leer.", "tokens": ["Die", "Ta\u00b7fel", "bleibt", ",", "die", "Lein\u00b7wand", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum fort aus diesen Gr\u00fcnden!", "tokens": ["Drum", "fort", "aus", "die\u00b7sen", "Gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob von der Reiselast Beschwer", "tokens": ["Ob", "von", "der", "Rei\u00b7se\u00b7last", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich festre Bilder r\u00fcnden.", "tokens": ["Sich", "fest\u00b7re", "Bil\u00b7der", "r\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}