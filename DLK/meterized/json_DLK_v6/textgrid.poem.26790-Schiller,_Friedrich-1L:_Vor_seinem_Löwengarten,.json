{"textgrid.poem.26790": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor seinem L\u00f6wengarten,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor seinem L\u00f6wengarten,", "tokens": ["Vor", "sei\u00b7nem", "L\u00f6\u00b7wen\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Kampfspiel zu erwarten,", "tokens": ["Das", "Kampf\u00b7spiel", "zu", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sa\u00df K\u00f6nig Franz,", "tokens": ["Sa\u00df", "K\u00f6\u00b7nig", "Franz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und um ihn die Gro\u00dfen der Krone,", "tokens": ["Und", "um", "ihn", "die", "Gro\u00b7\u00dfen", "der", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und rings auf hohem Balkone", "tokens": ["Und", "rings", "auf", "ho\u00b7hem", "Bal\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Damen in sch\u00f6nem Kranz.", "tokens": ["Die", "Da\u00b7men", "in", "sch\u00f6\u00b7nem", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wie er winkt mit dem Finger,", "tokens": ["Und", "wie", "er", "winkt", "mit", "dem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf tut sich der weite Zwinger,", "tokens": ["Auf", "tut", "sich", "der", "wei\u00b7te", "Zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hinein mit bed\u00e4chtigem Schritt", "tokens": ["Und", "hin\u00b7ein", "mit", "be\u00b7d\u00e4ch\u00b7ti\u00b7gem", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ein L\u00f6we tritt,", "tokens": ["Ein", "L\u00f6\u00b7we", "tritt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und sieht sich stumm", "tokens": ["Und", "sieht", "sich", "stumm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Rings um,", "tokens": ["Rings", "um", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Mit langem G\u00e4hnen,", "tokens": ["Mit", "lan\u00b7gem", "G\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und sch\u00fcttelt die M\u00e4hnen,", "tokens": ["Und", "sch\u00fct\u00b7telt", "die", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und streckt die Glieder,", "tokens": ["Und", "streckt", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und legt sich nieder.", "tokens": ["Und", "legt", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und der K\u00f6nig winkt wieder,", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "winkt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da \u00f6ffnet sich behend", "tokens": ["Da", "\u00f6ff\u00b7net", "sich", "be\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein zweites Tor,", "tokens": ["Ein", "zwei\u00b7tes", "Tor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Daraus rennt", "tokens": ["Da\u00b7raus", "rennt"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Mit wildem Sprunge", "tokens": ["Mit", "wil\u00b7dem", "Sprun\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein Tiger hervor.", "tokens": ["Ein", "Ti\u00b7ger", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie der den L\u00f6wen erschaut,", "tokens": ["Wie", "der", "den", "L\u00f6\u00b7wen", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Br\u00fcllt er laut,", "tokens": ["Br\u00fcllt", "er", "laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Schl\u00e4gt mit dem Schweif", "tokens": ["Schl\u00e4gt", "mit", "dem", "Schweif"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Einen furchtbaren Reif,", "tokens": ["Ei\u00b7nen", "furcht\u00b7ba\u00b7ren", "Reif", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Und recket die Zunge,", "tokens": ["Und", "re\u00b7cket", "die", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Und im Kreise scheu", "tokens": ["Und", "im", "Krei\u00b7se", "scheu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Umgeht er den Leu", "tokens": ["Um\u00b7geht", "er", "den", "Leu"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Grimmig schnurrend;", "tokens": ["Grim\u00b7mig", "schnur\u00b7rend", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Drauf streckt er sich murrend", "tokens": ["Drauf", "streckt", "er", "sich", "mur\u00b7rend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Zur Seite nieder.", "tokens": ["Zur", "Sei\u00b7te", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Und der K\u00f6nig winkt wieder,", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "winkt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Da speit das doppelt ge\u00f6ffnete Haus", "tokens": ["Da", "speit", "das", "dop\u00b7pelt", "ge\u00b7\u00f6ff\u00b7ne\u00b7te", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Zwei Leoparden auf einmal aus,", "tokens": ["Zwei", "Le\u00b7o\u00b7par\u00b7den", "auf", "ein\u00b7mal", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Die st\u00fcrzen mit mutiger Kampfbegier", "tokens": ["Die", "st\u00fcr\u00b7zen", "mit", "mu\u00b7ti\u00b7ger", "Kampf\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Auf das Tigertier,", "tokens": ["Auf", "das", "Ti\u00b7ger\u00b7tier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Das packt sie mit seinen grimmigen Tatzen,", "tokens": ["Das", "packt", "sie", "mit", "sei\u00b7nen", "grim\u00b7mi\u00b7gen", "Tat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und der Leu mit Gebr\u00fcll", "tokens": ["Und", "der", "Leu", "mit", "Ge\u00b7br\u00fcll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Richtet sich auf, da wirds still,", "tokens": ["Rich\u00b7tet", "sich", "auf", ",", "da", "wirds", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Und herum im Kreis,", "tokens": ["Und", "he\u00b7rum", "im", "Kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.26": {"text": "Von Mordsucht hei\u00df,", "tokens": ["Von", "Mord\u00b7sucht", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Lagern die greulichen Katzen.", "tokens": ["La\u00b7gern", "die", "greu\u00b7li\u00b7chen", "Kat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Da f\u00e4llt von des Altans Rand", "tokens": ["Da", "f\u00e4llt", "von", "des", "Al\u00b7tans", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Handschuh von sch\u00f6ner Hand", "tokens": ["Ein", "Hand\u00b7schuh", "von", "sch\u00f6\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zwischen den Tiger und den Leun", "tokens": ["Zwi\u00b7schen", "den", "Ti\u00b7ger", "und", "den", "Leun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mitten hinein.", "tokens": ["Mit\u00b7ten", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und zu Ritter Delorges spottenderweis", "tokens": ["Und", "zu", "Rit\u00b7ter", "De\u00b7lor\u00b7ges", "spot\u00b7ten\u00b7der\u00b7weis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wendet sich Fr\u00e4ulein Kunigund:", "tokens": ["Wen\u00b7det", "sich", "Fr\u00e4u\u00b7lein", "Ku\u00b7ni\u00b7gund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbherr Ritter, ist Eure Lieb so hei\u00df,", "tokens": ["\u00bb", "herr", "Rit\u00b7ter", ",", "ist", "Eu\u00b7re", "Lieb", "so", "hei\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Ihr mirs schw\u00f6rt zu jeder Stund,", "tokens": ["Wie", "Ihr", "mirs", "schw\u00f6rt", "zu", "je\u00b7der", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ei, so hebt mir den Handschuh auf.\u00ab", "tokens": ["Ei", ",", "so", "hebt", "mir", "den", "Hand\u00b7schuh", "auf", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Und der Ritter in schnellem Lauf", "tokens": ["Und", "der", "Rit\u00b7ter", "in", "schnel\u00b7lem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steigt hinab in den furchtbarn Zwinger", "tokens": ["Steigt", "hin\u00b7ab", "in", "den", "furcht\u00b7barn", "Zwin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit festem Schritte,", "tokens": ["Mit", "fes\u00b7tem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und aus der Ungeheuer Mitte", "tokens": ["Und", "aus", "der", "Un\u00b7ge\u00b7heu\u00b7er", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimmt er den Handschuh mit keckem Finger.", "tokens": ["Nimmt", "er", "den", "Hand\u00b7schuh", "mit", "ke\u00b7ckem", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Und mit Erstaunen und mit Grauen", "tokens": ["Und", "mit", "Er\u00b7stau\u00b7nen", "und", "mit", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sehens die Ritter und Edelfrauen,", "tokens": ["Se\u00b7hens", "die", "Rit\u00b7ter", "und", "E\u00b7del\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und gelassen bringt er den Handschuh zur\u00fcck.", "tokens": ["Und", "ge\u00b7las\u00b7sen", "bringt", "er", "den", "Hand\u00b7schuh", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da schallt ihm sein Lob aus jedem Munde,", "tokens": ["Da", "schallt", "ihm", "sein", "Lob", "aus", "je\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aber mit z\u00e4rtlichem Liebesblick \u2013", "tokens": ["A\u00b7ber", "mit", "z\u00e4rt\u00b7li\u00b7chem", "Lie\u00b7bes\u00b7blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Er verhei\u00dft ihm sein nahes Gl\u00fcck \u2013", "tokens": ["Er", "ver\u00b7hei\u00dft", "ihm", "sein", "na\u00b7hes", "Gl\u00fcck", "\u2013"], "token_info": ["word", "w