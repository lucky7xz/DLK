{"textgrid.poem.35267": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Geburt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor unsrer Geburt, in der gr\u00fcnen S\u00fcdsee platzte die Erde und das Wasser,", "tokens": ["Vor", "uns\u00b7rer", "Ge\u00b7burt", ",", "in", "der", "gr\u00fc\u00b7nen", "S\u00fcd\u00b7see", "platz\u00b7te", "die", "Er\u00b7de", "und", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-+-+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Tausend Menschen sa\u00dfen wie Schnecken auf gro\u00dfen Bl\u00e4ttern in H\u00fctten und versanken keuchend.", "tokens": ["Tau\u00b7send", "Men\u00b7schen", "sa\u00b7\u00dfen", "wie", "Schne\u00b7cken", "auf", "gro\u00b7\u00dfen", "Bl\u00e4t\u00b7tern", "in", "H\u00fct\u00b7ten", "und", "ver\u00b7san\u00b7ken", "keu\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "KOKOM", "NN", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+--+--+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Vor Marseille fielen die roten Schiffe um, das Meer schlug vom Mond herab.", "tokens": ["Vor", "Mar\u00b7seil\u00b7le", "fie\u00b7len", "die", "ro\u00b7ten", "Schif\u00b7fe", "um", ",", "das", "Meer", "schlug", "vom", "Mond", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+--+--+-+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Dampfer schnurrten in den Abgrund, l\u00e4cherliche Insekten.", "tokens": ["Die", "Damp\u00b7fer", "schnurr\u00b7ten", "in", "den", "Ab\u00b7grund", ",", "l\u00e4\u00b7cher\u00b7li\u00b7che", "In\u00b7sek\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Als wir geboren wurden, zog Feuer durch die Luft.", "tokens": ["Als", "wir", "ge\u00b7bo\u00b7ren", "wur\u00b7den", ",", "zog", "Feu\u00b7er", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die Schw\u00e4rme des Feuers flogen um die Erde.", "tokens": ["Die", "Schw\u00e4r\u00b7me", "des", "Feu\u00b7ers", "flo\u00b7gen", "um", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wehe, wer nicht sehen wollte!", "tokens": ["We\u00b7he", ",", "wer", "nicht", "se\u00b7hen", "woll\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tausend Menschen, stillhockende Schnecken, waren zu Staub zerplatzt.", "tokens": ["Tau\u00b7send", "Men\u00b7schen", ",", "still\u00b7ho\u00b7cken\u00b7de", "Schne\u00b7cken", ",", "wa\u00b7ren", "zu", "Staub", "zer\u00b7platzt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJA", "NN", "$,", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Die Tage erblichen f\u00fcr die gl\u00fchenden Abende.", "tokens": ["Die", "Ta\u00b7ge", "er\u00b7bli\u00b7chen", "f\u00fcr", "die", "gl\u00fc\u00b7hen\u00b7den", "A\u00b7ben\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die N\u00e4chte schwangen rote Palmblattflammen \u00fcber Berlin,", "tokens": ["Die", "N\u00e4ch\u00b7te", "schwan\u00b7gen", "ro\u00b7te", "Palm\u00b7blatt\u00b7flam\u00b7men", "\u00fc\u00b7ber", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Die Abende waren gelbe Tiere \u00fcber der Friedrichstra\u00dfe.", "tokens": ["Die", "A\u00b7ben\u00b7de", "wa\u00b7ren", "gel\u00b7be", "Tie\u00b7re", "\u00fc\u00b7ber", "der", "Fried\u00b7rich\u00b7stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Berlin, aus spitzen Pl\u00e4tzen, grauen Nebenstra\u00dfen, quoll das Blau der Vulkane.", "tokens": ["Ber\u00b7lin", ",", "aus", "spit\u00b7zen", "Pl\u00e4t\u00b7zen", ",", "grau\u00b7en", "Ne\u00b7ben\u00b7stra\u00b7\u00dfen", ",", "quoll", "das", "Blau", "der", "Vul\u00b7ka\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus"}, "line.13": {"text": "Die Frauen waren alle allein, die M\u00e4nner reckten sich auf,", "tokens": ["Die", "Frau\u00b7en", "wa\u00b7ren", "al\u00b7le", "al\u00b7lein", ",", "die", "M\u00e4n\u00b7ner", "reck\u00b7ten", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Die Schenkel liefen durch Berlin, hei\u00dfe Haarberge bogen hoch.", "tokens": ["Die", "Schen\u00b7kel", "lie\u00b7fen", "durch", "Ber\u00b7lin", ",", "hei\u00b7\u00dfe", "Haar\u00b7ber\u00b7ge", "bo\u00b7gen", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$,", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Die Sonne ging immer unter. Die Abendstrahlen, hei\u00df, quollen aus den M\u00e4nnern.", "tokens": ["Die", "Son\u00b7ne", "ging", "im\u00b7mer", "un\u00b7ter", ".", "Die", "A\u00b7bend\u00b7strah\u00b7len", ",", "hei\u00df", ",", "quol\u00b7len", "aus", "den", "M\u00e4n\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "$.", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "Die H\u00e4user waren kalkig und bleich. Durch dunkle Zimmer wankte die Stadt, die Blinde.", "tokens": ["Die", "H\u00e4u\u00b7ser", "wa\u00b7ren", "kal\u00b7kig", "und", "bleich", ".", "Durch", "dunk\u00b7le", "Zim\u00b7mer", "wank\u00b7te", "die", "Stadt", ",", "die", "Blin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir wurden geboren, Strahlenlicht kreiste abends \u00fcber unseren M\u00fcndern,", "tokens": ["Wir", "wur\u00b7den", "ge\u00b7bo\u00b7ren", ",", "Strah\u00b7len\u00b7licht", "kreis\u00b7te", "a\u00b7bends", "\u00fc\u00b7ber", "un\u00b7se\u00b7ren", "M\u00fcn\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Gr\u00fcne S\u00fcdsafth\u00fcgel hingen vom Mond \u00fcber uns;", "tokens": ["Gr\u00fc\u00b7ne", "S\u00fcd\u00b7saft\u00b7h\u00fc\u00b7gel", "hin\u00b7gen", "vom", "Mond", "\u00fc\u00b7ber", "uns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wir rissen unsere Augen von unserem Blut auf.", "tokens": ["Wir", "ris\u00b7sen", "un\u00b7se\u00b7re", "Au\u00b7gen", "von", "un\u00b7se\u00b7rem", "Blut", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--++", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Himmel flog \u00fcber alle Stra\u00dfen der Stadt.", "tokens": ["Der", "Him\u00b7mel", "flog", "\u00fc\u00b7ber", "al\u00b7le", "Stra\u00b7\u00dfen", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "In der Vorstra\u00dfe aus Zaun und Stein wartete die grauhaarige Mauerdirne auf die Soldaten.", "tokens": ["In", "der", "Vor\u00b7stra\u00b7\u00dfe", "aus", "Zaun", "und", "Stein", "war\u00b7te\u00b7te", "die", "grau\u00b7ha\u00b7a\u00b7ri\u00b7ge", "Mau\u00b7er\u00b7dir\u00b7ne", "auf", "die", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-++--+-+-+--+-+--+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Wir wu\u00dften, da\u00df es andere L\u00e4nder gibt.", "tokens": ["Wir", "wu\u00df\u00b7ten", ",", "da\u00df", "es", "an\u00b7de\u00b7re", "L\u00e4n\u00b7der", "gibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "In m\u00f6blierten Zimmern sannen russische Stirnen \u00fcber Bombenattentaten.", "tokens": ["In", "m\u00f6b\u00b7lier\u00b7ten", "Zim\u00b7mern", "san\u00b7nen", "rus\u00b7si\u00b7sche", "Stir\u00b7nen", "\u00fc\u00b7ber", "Bom\u00b7be\u00b7nat\u00b7ten\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "In den Variet\u00e9s wurden die f\u00fcnf englischen Puppenm\u00e4dchen geliebt.", "tokens": ["In", "den", "Va\u00b7ri\u00b7e\u00b7t\u00e9s", "wur\u00b7den", "die", "f\u00fcnf", "eng\u00b7li\u00b7schen", "Pup\u00b7pen\u00b7m\u00e4d\u00b7chen", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VAFIN", "ART", "CARD", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--++--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Die Menschen sitzen in schwarzen R\u00f6cken, essen und werden alt.", "tokens": ["Die", "Men\u00b7schen", "sit\u00b7zen", "in", "schwar\u00b7zen", "R\u00f6\u00b7cken", ",", "es\u00b7sen", "und", "wer\u00b7den", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVINF", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Die hohlen H\u00e4userw\u00e4nde waren lose und grau.", "tokens": ["Die", "hoh\u00b7len", "H\u00e4u\u00b7ser\u00b7w\u00e4n\u00b7de", "wa\u00b7ren", "lo\u00b7se", "und", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Kamerad, Sie liefen die Stra\u00dfe auf und nieder, Sie waren bla\u00df vor dem heiligen Panoptikumsbau.", "tokens": ["Ka\u00b7me\u00b7rad", ",", "Sie", "lie\u00b7fen", "die", "Stra\u00b7\u00dfe", "auf", "und", "nie\u00b7der", ",", "Sie", "wa\u00b7ren", "bla\u00df", "vor", "dem", "hei\u00b7li\u00b7gen", "Pa\u00b7nop\u00b7ti\u00b7kums\u00b7bau", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+--+-+--+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Luft stand mit Sternen in Ihnen, der Tag war noch hell.", "tokens": ["Die", "Luft", "stand", "mit", "Ster\u00b7nen", "in", "Ih\u00b7nen", ",", "der", "Tag", "war", "noch", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PPER", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Die Menschen waren dick und rauchten Zigarren.", "tokens": ["Die", "Men\u00b7schen", "wa\u00b7ren", "dick", "und", "rauch\u00b7ten", "Zi\u00b7gar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Niemand sah sie an.", "tokens": ["Nie\u00b7mand", "sah", "sie", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die Stadt schwebte, es war still im Abendbrand, die H\u00e4user zerfielen unten.", "tokens": ["Die", "Stadt", "schweb\u00b7te", ",", "es", "war", "still", "im", "A\u00b7bend\u00b7brand", ",", "die", "H\u00e4u\u00b7ser", "zer\u00b7fie\u00b7len", "un\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-++-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Die Menschen gingen schwer.", "tokens": ["Die", "Men\u00b7schen", "gin\u00b7gen", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kamerad, Sie waren allein. Niemand hatte das Licht gesehen.", "tokens": ["Ka\u00b7me\u00b7rad", ",", "Sie", "wa\u00b7ren", "al\u00b7lein", ".", "Nie\u00b7mand", "hat\u00b7te", "das", "Licht", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "$.", "PIS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Um die Erde spr\u00fchte der s\u00fcdliche Schwei\u00df des Vulkans.", "tokens": ["Um", "die", "Er\u00b7de", "spr\u00fch\u00b7te", "der", "s\u00fcd\u00b7li\u00b7che", "Schwei\u00df", "des", "Vul\u00b7kans", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Niemand sah. Berlin schmatzte rollend.", "tokens": ["Nie\u00b7mand", "sah", ".", "Ber\u00b7lin", "schmatz\u00b7te", "rol\u00b7lend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "NE", "VVFIN", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Es war nicht mehr Licht durch buntes Abendglas,", "tokens": ["Es", "war", "nicht", "mehr", "Licht", "durch", "bun\u00b7tes", "A\u00b7bend\u00b7glas", ","], "token_info": ["word", 