{"textgrid.poem.65936": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Meine Erde", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu gro\u00dfe Sonne fand ich auf den Wegen,", "tokens": ["Zu", "gro\u00b7\u00dfe", "Son\u00b7ne", "fand", "ich", "auf", "den", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns holde Erde, hin zur offnen See;", "tokens": ["Uns", "hol\u00b7de", "Er\u00b7de", ",", "hin", "zur", "off\u00b7nen", "See", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In deinen Grotten schrie ich auf, vor Weh,", "tokens": ["In", "dei\u00b7nen", "Grot\u00b7ten", "schrie", "ich", "auf", ",", "vor", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch bald betraf mich sachter Sternensegen.", "tokens": ["Doch", "bald", "be\u00b7traf", "mich", "sach\u00b7ter", "Ster\u00b7nen\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir bleibt an sp\u00e4ter Wiederkunft gelegen:", "tokens": ["Mir", "bleibt", "an", "sp\u00e4\u00b7ter", "Wie\u00b7der\u00b7kunft", "ge\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben lieb ich, wo ich Kinder seh,", "tokens": ["Das", "Le\u00b7ben", "lieb", "ich", ",", "wo", "ich", "Kin\u00b7der", "seh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch Wald und Wesen \u2013 ach, zumal das Reh;", "tokens": ["Auch", "Wald", "und", "We\u00b7sen", "\u2013", "ach", ",", "zu\u00b7mal", "das", "Reh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$(", "XY", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wann d\u00fcrft ich traumreich meinen Garten hegen?", "tokens": ["Wann", "d\u00fcrft", "ich", "traum\u00b7reich", "mei\u00b7nen", "Gar\u00b7ten", "he\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du sollst, bei Blumen, dich f\u00fcr Sterne sammeln,", "tokens": ["Du", "sollst", ",", "bei", "Blu\u00b7men", ",", "dich", "f\u00fcr", "Ster\u00b7ne", "sam\u00b7meln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "NN", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sind noch f\u00fcr die Erde fast zu jung,", "tokens": ["Wir", "sind", "noch", "f\u00fcr", "die", "Er\u00b7de", "fast", "zu", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verm\u00f6gen kaum, Versprechen wahr zu stammeln!", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "kaum", ",", "Ver\u00b7spre\u00b7chen", "wahr", "zu", "stam\u00b7meln", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Warum die Furcht vor unserm Hadessprung?", "tokens": ["Wa\u00b7rum", "die", "Furcht", "vor", "un\u00b7serm", "Ha\u00b7des\u00b7sprung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Sternendrachen mag ich mich verrammeln,", "tokens": ["Vor", "Ster\u00b7nen\u00b7dra\u00b7chen", "mag", "ich", "mich", "ver\u00b7ram\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Mutter, gib mir tief Beruhigung!", "tokens": ["O", "Mut\u00b7ter", ",", "gib", "mir", "tief", "Be\u00b7ru\u00b7hi\u00b7gung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}