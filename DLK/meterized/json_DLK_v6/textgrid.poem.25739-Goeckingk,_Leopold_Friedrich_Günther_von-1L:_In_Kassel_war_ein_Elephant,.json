{"textgrid.poem.25739": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Kassel war ein Elephant,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Kassel war ein Elephant,", "tokens": ["In", "Kas\u00b7sel", "war", "ein", "E\u00b7le\u00b7phant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der an Gehalt im Jahr',", "tokens": ["Der", "an", "Ge\u00b7halt", "im", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gerade gleich mit mir sich stand,", "tokens": ["Ge\u00b7ra\u00b7de", "gleich", "mit", "mir", "sich", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und den ein jeder witzig fand,", "tokens": ["Und", "den", "ein", "je\u00b7der", "wit\u00b7zig", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil er ein S\u00e4ufer war.", "tokens": ["Weil", "er", "ein", "S\u00e4u\u00b7fer", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ein gr\u00f6\u00dfres Schauspielhaus;", "tokens": ["Ein", "gr\u00f6\u00df\u00b7res", "Schau\u00b7spiel\u00b7haus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das alt' ist beides denn: zu klein", "tokens": ["Das", "alt'", "ist", "bei\u00b7des", "denn", ":", "zu", "klein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "PIS", "ADV", "$.", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und gro\u00df; denn Niemand geht hinein,", "tokens": ["Und", "gro\u00df", ";", "denn", "Nie\u00b7mand", "geht", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KON", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und Niemand geht heraus.", "tokens": ["Und", "Nie\u00b7mand", "geht", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ein Thor erbaut der Magistrat", "tokens": ["Ein", "Thor", "er\u00b7baut", "der", "Ma\u00b7gist\u00b7rat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Von Bettlern wird man in der That,", "tokens": ["Von", "Bett\u00b7lern", "wird", "man", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bis auch ein Arbeitshaus der Rath", "tokens": ["Bis", "auch", "ein", "Ar\u00b7beits\u00b7haus", "der", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wird baun, fast umgebracht.", "tokens": ["Wird", "baun", ",", "fast", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Der Dom in ", "tokens": ["Der", "Dom", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Und sch\u00f6n erneut, mein Sohn.", "tokens": ["Und", "sch\u00f6n", "er\u00b7neut", ",", "mein", "Sohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Siehst gern Ruinen? Manches Haus", "tokens": ["Siehst", "gern", "Ru\u00b7i\u00b7nen", "?", "Man\u00b7ches", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "$.", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Liegt j\u00e4mmerlich in Schutt und Graus", "tokens": ["Liegt", "j\u00e4m\u00b7mer\u00b7lich", "in", "Schutt", "und", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein Seculum dort schon!", "tokens": ["Ein", "Se\u00b7cu\u00b7lum", "dort", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ein Thor der Festung ", "tokens": ["Ein", "Thor", "der", "Fes\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Zu wissen: da\u00df die Stadt", "tokens": ["Zu", "wis\u00b7sen", ":", "da\u00df", "die", "Stadt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Drei Hundert Jahre nach der Flut,", "tokens": ["Drei", "Hun\u00b7dert", "Jah\u00b7re", "nach", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die einst verschlang der Vorwelt Brut,", "tokens": ["Die", "einst", "ver\u00b7schlang", "der", "Vor\u00b7welt", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Das Zeitwort ", "tokens": ["Das", "Zeit\u00b7wort"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Schreibt das dein Cantor ", "tokens": ["Schreibt", "das", "dein", "Can\u00b7tor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Was? ga-lop-pi-ren? Pfi doch, pfi!", "tokens": ["Was", "?", "ga\u00b7lop\u00b7pi\u00b7ren", "?", "Pfi", "doch", ",", "pfi", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$.", "NN", "$.", "NE", "ADV", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So schreibt's der Rath in ", "tokens": ["So", "schreibt's", "der", "Rath", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Mit aller Weltgelehrsamkeit,", "tokens": ["Mit", "al\u00b7ler", "Welt\u00b7ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Mit Witz und mit Geschmack,", "tokens": ["Mit", "Witz", "und", "mit", "Ge\u00b7schmack", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Bringst du, mein Sohn, es niemals weit;", "tokens": ["Bringst", "du", ",", "mein", "Sohn", ",", "es", "nie\u00b7mals", "weit", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Drum sinne du bei guter Zeit", "tokens": ["Drum", "sin\u00b7ne", "du", "bei", "gu\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Auf neuen Schnupftobak.", "tokens": ["Auf", "neu\u00b7en", "Schnupf\u00b7to\u00b7bak", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Den Mainzern gleich", "tokens": ["Den", "Main\u00b7zern", "gleich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Reichsgrafen gleich hat er gepra\u00dft,", "tokens": ["Reichs\u00b7gra\u00b7fen", "gleich", "hat", "er", "ge\u00b7pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.37": {"text": "Und einen f\u00fcrstlichen Pallast", "tokens": ["Und", "ei\u00b7nen", "f\u00fcrst\u00b7li\u00b7chen", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Erbaut, zu ", "tokens": ["Er\u00b7baut", ",", "zu"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Die Milit\u00e4r-Akademie", "tokens": ["Die", "Mi\u00b7li\u00b7t\u00e4\u00b7rAka\u00b7de\u00b7mie"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.41": {"text": "Vielleicht der Reise werth, um sie", "tokens": ["Viel\u00b7leicht", "der", "Rei\u00b7se", "werth", ",", "um", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "$,", "KOUI", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Zu sehn; doch dahin reise nie!", "tokens": ["Zu", "sehn", ";", "doch", "da\u00b7hin", "rei\u00b7se", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "ADV", "PAV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Denn Niemand kommt hinein", "tokens": ["Denn", "Nie\u00b7mand", "kommt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Die Milit\u00e4r-Akademie", "tokens": ["Die", "Mi\u00b7li\u00b7t\u00e4\u00b7rAka\u00b7de\u00b7mie"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.46": {"text": "So viel du willst, so spat als fr\u00fch,", "tokens": ["So", "viel", "du", "willst", ",", "so", "spat", "als", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und unzufrieden wirst du nie", "tokens": ["Und", "un\u00b7zu\u00b7frie\u00b7den", "wirst", "du", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Aus ihr nach Hause gehn", "tokens": ["Aus", "ihr", "nach", "Hau\u00b7se", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "APPR", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Wie, denkst du wohl, da\u00df aus Gefahr", "tokens": ["Wie", ",", "denkst", "du", "wohl", ",", "da\u00df", "aus", "Ge\u00b7fahr"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Zur\u00fcck ein Krieger kehrt?", "tokens": ["Zu\u00b7r\u00fcck", "ein", "Krie\u00b7ger", "kehrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Sehr nett frisirt, und nicht ein Haar", "tokens": ["Sehr", "nett", "fri\u00b7sirt", ",", "und", "nicht", "ein", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "An der Frisur versehrt.", "tokens": ["An", "der", "Fri\u00b7sur", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Du wei\u00dft, wie sch\u00f6n im Mondenschein'", "tokens": ["Du", "wei\u00dft", ",", "wie", "sch\u00f6n", "im", "Mon\u00b7den\u00b7schein'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Spatzieren gehen sey.", "tokens": ["Spat\u00b7zie\u00b7ren", "ge\u00b7hen", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.56": {"text": "Begleitet von Laternen seyn", "tokens": ["Be\u00b7glei\u00b7tet", "von", "La\u00b7ter\u00b7nen", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Das hei\u00df' ich Polizei!", "tokens": ["Das", "hei\u00df'", "ich", "Po\u00b7li\u00b7zei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Wer lahm und blind ist, der vertrau'", "tokens": ["Wer", "lahm", "und", "blind", "ist", ",", "der", "ver\u00b7trau'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PTKVZ", "KON", "ADJD", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Dem Bad' in ", "tokens": ["Dem", "Bad'", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.60": {"text": "Doch keine Adelstolze Frau,", "tokens": ["Doch", "kei\u00b7ne", "A\u00b7del\u00b7stol\u00b7ze", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Denn die wird nur zu ", "tokens": ["Denn", "die", "wird", "nur", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.62": {"text": "Geheilt von ihrem Staar.", "tokens": ["Ge\u00b7heilt", "von", "ih\u00b7rem", "Staar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}