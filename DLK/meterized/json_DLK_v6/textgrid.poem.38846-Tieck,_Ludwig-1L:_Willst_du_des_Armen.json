{"textgrid.poem.38846": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du des Armen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du des Armen", "tokens": ["Willst", "du", "des", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dich gn\u00e4dig erbarmen?", "tokens": ["Dich", "gn\u00e4\u00b7dig", "er\u00b7bar\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So ist es kein Traum?", "tokens": ["So", "ist", "es", "kein", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie rieseln die Quellen,", "tokens": ["Wie", "rie\u00b7seln", "die", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie t\u00f6nen die Wellen,", "tokens": ["Wie", "t\u00f6\u00b7nen", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie rauschet der Baum!", "tokens": ["Wie", "rau\u00b7schet", "der", "Baum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Tief lag ich in bangen", "tokens": ["Tief", "lag", "ich", "in", "ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Gem\u00e4uern gefangen,", "tokens": ["Ge\u00b7m\u00e4u\u00b7ern", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nun gr\u00fc\u00dft mich das Licht!", "tokens": ["Nun", "gr\u00fc\u00dft", "mich", "das", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie spielen die Strahlen!", "tokens": ["Wie", "spie\u00b7len", "die", "Strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie blenden und mahlen", "tokens": ["Sie", "blen\u00b7den", "und", "mah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mein sch\u00fcchtern Gesicht.", "tokens": ["Mein", "sch\u00fcch\u00b7tern", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und soll ich es glauben?", "tokens": ["Und", "soll", "ich", "es", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wird keiner mir rauben", "tokens": ["Wird", "kei\u00b7ner", "mir", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den k\u00f6stlichen Wahn?", "tokens": ["Den", "k\u00f6st\u00b7li\u00b7chen", "Wahn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch Tr\u00e4ume entschweben,", "tokens": ["Doch", "Tr\u00e4u\u00b7me", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nur lieben hei\u00dft Leben:", "tokens": ["Nur", "lie\u00b7ben", "hei\u00dft", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Willkommene Bahn!", "tokens": ["Will\u00b7kom\u00b7me\u00b7ne", "Bahn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wie frei und wie heiter!", "tokens": ["Wie", "frei", "und", "wie", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht eile nun weiter,", "tokens": ["Nicht", "ei\u00b7le", "nun", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Pilgerstab fort!", "tokens": ["Der", "Pil\u00b7ger\u00b7stab", "fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Du hast \u00fcberwunden,", "tokens": ["Du", "hast", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Du hast ihn gefunden,", "tokens": ["Du", "hast", "ihn", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Den seligsten Ort!", "tokens": ["Den", "se\u00b7ligs\u00b7ten", "Ort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}