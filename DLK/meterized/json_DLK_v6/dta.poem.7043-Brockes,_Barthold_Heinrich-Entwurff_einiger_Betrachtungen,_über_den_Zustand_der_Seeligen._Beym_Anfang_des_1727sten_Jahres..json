{"dta.poem.7043": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bau\u2019, o GOTT, dann hier zu diesem Neuen Jahr,", "tokens": ["Ich", "bau'", ",", "o", "GoTT", ",", "dann", "hier", "zu", "die\u00b7sem", "Neu\u00b7en", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NE", "$,", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir einen Danck- und Lob-Altar.", "tokens": ["Dir", "ei\u00b7nen", "Dan\u00b7ck", "und", "Lob\u00b7Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mein Vorsatz ist, beym Wechsel unsrer Zeit,", "tokens": ["Mein", "Vor\u00b7satz", "ist", ",", "beym", "Wech\u00b7sel", "uns\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch einst auf das, was folgt, wenn keine Zeit nicht mehr,", "tokens": ["Auch", "einst", "auf", "das", ",", "was", "folgt", ",", "wenn", "kei\u00b7ne", "Zeit", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "$,", "PWS", "VVFIN", "$,", "KOUS", "PIAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf jene seel\u2019ge Ewigkeit", "tokens": ["Auf", "je\u00b7ne", "seel'\u00b7ge", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Seelen Krafft zu lencken,", "tokens": ["Der", "See\u00b7len", "Krafft", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, dem Allm\u00e4chtigen, liebreichen GOTT, zur Ehr\u2019,", "tokens": ["Und", ",", "dem", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", ",", "lieb\u00b7rei\u00b7chen", "GoTT", ",", "zur", "Ehr'", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "An jenen Zustand zu gedencken,", "tokens": ["An", "je\u00b7nen", "Zu\u00b7stand", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den GOTT, als der die ew\u2019ge Liebe,", "tokens": ["Den", "GoTT", ",", "als", "der", "die", "ew'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dereinst uns geben wird: wovon Er allbereit,", "tokens": ["De\u00b7reinst", "uns", "ge\u00b7ben", "wird", ":", "wo\u00b7von", "Er", "all\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VAFIN", "$.", "PWAV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So gar in dieser Sterblichkeit,", "tokens": ["So", "gar", "in", "die\u00b7ser", "Sterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Uns einigen Begriff in unsre Seele schriebe.", "tokens": ["Uns", "ei\u00b7ni\u00b7gen", "Be\u00b7griff", "in", "uns\u00b7re", "See\u00b7le", "schrie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}