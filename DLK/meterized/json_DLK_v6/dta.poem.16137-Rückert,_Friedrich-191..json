{"dta.poem.16137": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "191.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was schlichtet, Herz, den Streit, der dich mit dir entzweit?", "tokens": ["Was", "schlich\u00b7tet", ",", "Herz", ",", "den", "Streit", ",", "der", "dich", "mit", "dir", "ent\u00b7zweit", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NN", "$,", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gottesfurcht, die dich von aller Furcht befreit;", "tokens": ["Die", "Got\u00b7tes\u00b7furcht", ",", "die", "dich", "von", "al\u00b7ler", "Furcht", "be\u00b7freit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von aller Furcht der Welt und weltlicher Geschicke,", "tokens": ["Von", "al\u00b7ler", "Furcht", "der", "Welt", "und", "welt\u00b7li\u00b7cher", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "KON", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von aller Furcht vor dir, dem qu\u00e4lendsten der Stricke.", "tokens": ["Von", "al\u00b7ler", "Furcht", "vor", "dir", ",", "dem", "qu\u00e4\u00b7lends\u00b7ten", "der", "Stri\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Verst\u00f6ren kann dich nichts, wenn du dich nicht verst\u00f6rst,", "tokens": ["Ver\u00b7st\u00f6\u00b7ren", "kann", "dich", "nichts", ",", "wenn", "du", "dich", "nicht", "ver\u00b7st\u00f6rst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PIS", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frei nur f\u00fchlst du dich, wenn du dem Herrn geh\u00f6rst.", "tokens": ["Und", "frei", "nur", "f\u00fchlst", "du", "dich", ",", "wenn", "du", "dem", "Herrn", "ge\u00b7h\u00f6rst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie sch\u00f6n ists einen Herrn statt vieler Herrn zu haben,", "tokens": ["Wie", "sch\u00f6n", "ists", "ei\u00b7nen", "Herrn", "statt", "vie\u00b7ler", "Herrn", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der seine Diener kann mit Herrlichkeit begaben.", "tokens": ["Der", "sei\u00b7ne", "Die\u00b7ner", "kann", "mit", "Herr\u00b7lich\u00b7keit", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}