{"textgrid.poem.52637": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lieb ich dich, o Dahner Thal,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb ich dich, o Dahner Thal,", "tokens": ["Wie", "lieb", "ich", "dich", ",", "o", "Dah\u00b7ner", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "$,", "FM", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im alten Wasgenwald,", "tokens": ["Im", "al\u00b7ten", "Was\u00b7gen\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bist du sch\u00f6n im Morgenstrahl,", "tokens": ["Wie", "bist", "du", "sch\u00f6n", "im", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Vogelsang durchschallt!", "tokens": ["Vom", "Vo\u00b7gel\u00b7sang", "durch\u00b7schallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von deinen H\u00f6hen winkt ein Gru\u00df,", "tokens": ["Von", "dei\u00b7nen", "H\u00f6\u00b7hen", "winkt", "ein", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der zieht den Wandrer hin,", "tokens": ["Der", "zieht", "den", "Wand\u00b7rer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wei\u00df nicht, wie ihm geschieht, er mu\u00df", "tokens": ["Wei\u00df", "nicht", ",", "wie", "ihm", "ge\u00b7schieht", ",", "er", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch diese Berge ziehn!", "tokens": ["Durch", "die\u00b7se", "Ber\u00b7ge", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da schaut vom Felsen hoch und schroff", "tokens": ["Da", "schaut", "vom", "Fel\u00b7sen", "hoch", "und", "schroff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab manch grauer Thurm,", "tokens": ["Her\u00b7ab", "manch", "grau\u00b7er", "Thurm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf den umsonst die Wolke troff", "tokens": ["Auf", "den", "um\u00b7sonst", "die", "Wol\u00b7ke", "troff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im kalten Nebelsturm.", "tokens": ["Im", "kal\u00b7ten", "Ne\u00b7bel\u00b7sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drin hau\u00dfete ein k\u00fchn Geschlecht,", "tokens": ["Drin", "hau\u00b7\u00dfe\u00b7te", "ein", "k\u00fchn", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Heut ist es Moder nur,", "tokens": ["Heut", "ist", "es", "Mo\u00b7der", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch deine Jugendkraft ist \u00e4cht,", "tokens": ["Doch", "dei\u00b7ne", "Ju\u00b7gend\u00b7kraft", "ist", "\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unsterbliche Natur!", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Windhauch wie s\u00fc\u00dfer Odem weht", "tokens": ["Wind\u00b7hauch", "wie", "s\u00fc\u00b7\u00dfer", "O\u00b7dem", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zum d\u00fcstern Rothelstein,", "tokens": ["Zum", "d\u00fcs\u00b7tern", "Ro\u00b7thel\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ist vor allen hocherh\u00f6ht", "tokens": ["Der", "ist", "vor", "al\u00b7len", "ho\u00b7cher\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Himmelblau hinein.", "tokens": ["Ins", "Him\u00b7mel\u00b7blau", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du rufst hinaus mit hellem Schall,", "tokens": ["Du", "rufst", "hin\u00b7aus", "mit", "hel\u00b7lem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinaus ins Sonnenthal,", "tokens": ["Hin\u00b7aus", "ins", "Son\u00b7nen\u00b7thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da rufen euch die Berge all,", "tokens": ["Da", "ru\u00b7fen", "euch", "die", "Ber\u00b7ge", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Berge sonder Zahl.", "tokens": ["Die", "Ber\u00b7ge", "son\u00b7der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Erst t\u00f6nt es hohl am Lindelbronn,", "tokens": ["Erst", "t\u00f6nt", "es", "hohl", "am", "Lin\u00b7del\u00b7bronn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein stolzes Schlo\u00df,", "tokens": ["Das", "ist", "ein", "stol\u00b7zes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann klingt es feierlich, davon", "tokens": ["Dann", "klingt", "es", "fei\u00b7er\u00b7lich", ",", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwacht der Barbaro\u00df.", "tokens": ["Er\u00b7wacht", "der", "Bar\u00b7ba\u00b7ro\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ruht im Trifels ferne, fern,", "tokens": ["Der", "ruht", "im", "Tri\u00b7fels", "fer\u00b7ne", ",", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADJA", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ritt er hin bei Nacht,", "tokens": ["Da", "ritt", "er", "hin", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum Haus, wo er den Tag so gern", "tokens": ["Zum", "Haus", ",", "wo", "er", "den", "Tag", "so", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Leben zugebracht.", "tokens": ["Im", "Le\u00b7ben", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohlauf, Genossen, wandern wir", "tokens": ["Wohl\u00b7auf", ",", "Ge\u00b7nos\u00b7sen", ",", "wan\u00b7dern", "wir"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin mit Sang und Klang!", "tokens": ["Da\u00b7hin", "mit", "Sang", "und", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frau Wirthin hat gut Wein und Bier,", "tokens": ["Frau", "Wirt\u00b7hin", "hat", "gut", "Wein", "und", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wissen wir schon lang.", "tokens": ["Das", "wis\u00b7sen", "wir", "schon", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu Heidelberg im faulen Pelz", "tokens": ["Zu", "Hei\u00b7del\u00b7berg", "im", "fau\u00b7len", "Pelz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist nicht so traut Quartier,", "tokens": ["Ist", "nicht", "so", "traut", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor\u00fcber denn am Drachenfels,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "denn", "am", "Dra\u00b7chen\u00b7fels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Trifels wandern wir!", "tokens": ["Zum", "Tri\u00b7fels", "wan\u00b7dern", "wir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "F\u00fcrwahr, das ist ein Kaiserschlo\u00df!", "tokens": ["F\u00fcr\u00b7wahr", ",", "das", "ist", "ein", "Kai\u00b7ser\u00b7schlo\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00fcht purpurn himmelw\u00e4rts!", "tokens": ["Gl\u00fcht", "pur\u00b7purn", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Seht doch ins tiefste Erdgescho\u00df!", "tokens": ["Seht", "doch", "ins", "tiefs\u00b7te", "Erd\u00b7ge\u00b7scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort sa\u00df der L\u00f6wenherz.", "tokens": ["Dort", "sa\u00df", "der", "L\u00f6\u00b7wen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon d\u00e4mmerts k\u00fchl, der Tag war lang,", "tokens": ["Schon", "d\u00e4m\u00b7merts", "k\u00fchl", ",", "der", "Tag", "war", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir rasten hier zur Nacht!", "tokens": ["Wir", "ras\u00b7ten", "hier", "zur", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Freund Blondel stimmt in unsern Sang", "tokens": ["Freund", "Blon\u00b7del", "stimmt", "in", "un\u00b7sern", "Sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Getrost mit ein, gebt Acht!", "tokens": ["Ge\u00b7trost", "mit", "ein", ",", "gebt", "Acht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$,", "VVFIN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie lieb ich dich, du holdes Thal,", "tokens": ["Wie", "lieb", "ich", "dich", ",", "du", "hol\u00b7des", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im alten Wasgenwald,", "tokens": ["Im", "al\u00b7ten", "Was\u00b7gen\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bist du sch\u00f6n im Abendstrahl,", "tokens": ["Wie", "bist", "du", "sch\u00f6n", "im", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Wandersang durchschallt!", "tokens": ["Vom", "Wan\u00b7der\u00b7sang", "durch\u00b7schallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von allen H\u00f6hen winkt ein Gru\u00df", "tokens": ["Von", "al\u00b7len", "H\u00f6\u00b7hen", "winkt", "ein", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herab, hinaus zum Rhein,", "tokens": ["Her\u00b7ab", ",", "hin\u00b7aus", "zum", "Rhein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPRART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O gr\u00fc\u00dfe wieder, trauter Flu\u00df,", "tokens": ["O", "gr\u00fc\u00b7\u00dfe", "wie\u00b7der", ",", "trau\u00b7ter", "Flu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Blick auf im Vollmondschein!", "tokens": ["Blick", "auf", "im", "Voll\u00b7mond\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}