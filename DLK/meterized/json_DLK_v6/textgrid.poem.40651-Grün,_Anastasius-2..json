{"textgrid.poem.40651": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam der Herbst. Zu jedem Sonnenstrahle", "tokens": ["Es", "kam", "der", "Herbst", ".", "Zu", "je\u00b7dem", "Son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprach ich: Was lachst du mir? Zieh' hin, verm\u00e4hle,", "tokens": ["Sprach", "ich", ":", "Was", "lachst", "du", "mir", "?", "Zieh'", "hin", ",", "ver\u00b7m\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Klarer, dich der kranken Freundesseele,", "tokens": ["Du", "Kla\u00b7rer", ",", "dich", "der", "kran\u00b7ken", "Freun\u00b7des\u00b7see\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm keltre du den Heiltrank in die Schale.", "tokens": ["Ihm", "kelt\u00b7re", "du", "den", "Heil\u00b7trank", "in", "die", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Winter kam. Ich bat ihn: Mir nicht male", "tokens": ["Der", "Win\u00b7ter", "kam", ".", "Ich", "bat", "ihn", ":", "Mir", "nicht", "ma\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$.", "PPER", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wangen roth, nicht mir die Sehnen st\u00e4hle!", "tokens": ["Die", "Wan\u00b7gen", "roth", ",", "nicht", "mir", "die", "Seh\u00b7nen", "st\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PTKNEG", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den kranken Freund dir zur Verj\u00fcngung w\u00e4hle,", "tokens": ["Den", "kran\u00b7ken", "Freund", "dir", "zur", "Ver\u00b7j\u00fcn\u00b7gung", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4rt' ihm den Leib, der R\u00fcstung gleich von Stahle.", "tokens": ["H\u00e4rt'", "ihm", "den", "Leib", ",", "der", "R\u00fcs\u00b7tung", "gleich", "von", "Stah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es kam der Lenz. Ich sprach: Nicht mich umschmeichle!", "tokens": ["Es", "kam", "der", "Lenz", ".", "Ich", "sprach", ":", "Nicht", "mich", "um\u00b7schmeich\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "PTKNEG", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schwarzen Locken aus den Augen streichle", "tokens": ["Die", "schwar\u00b7zen", "Lo\u00b7cken", "aus", "den", "Au\u00b7gen", "streich\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem kranken Freund, und seine Stirne k\u00fchle!", "tokens": ["Dem", "kran\u00b7ken", "Freund", ",", "und", "sei\u00b7ne", "Stir\u00b7ne", "k\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Sch\u00f6nste deiner Flur sollst du erlesen,", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "dei\u00b7ner", "Flur", "sollst", "du", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ans Herz ihm legen Blumen der Gef\u00fchle,", "tokens": ["Ans", "Herz", "ihm", "le\u00b7gen", "Blu\u00b7men", "der", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kann er's, wird an ihnen er genesen.", "tokens": ["Und", "kann", "er's", ",", "wird", "an", "ih\u00b7nen", "er", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "$,", "VAFIN", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}}}}