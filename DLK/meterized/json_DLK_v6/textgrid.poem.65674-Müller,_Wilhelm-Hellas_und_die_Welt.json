{"textgrid.poem.65674": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Hellas und die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohne die Freiheit, was w\u00e4rest du, Hellas?", "tokens": ["Oh\u00b7ne", "die", "Frei\u00b7heit", ",", "was", "w\u00e4\u00b7rest", "du", ",", "Hel\u00b7las", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ohne dich, Hellas, was w\u00e4re die Welt?", "tokens": ["Oh\u00b7ne", "dich", ",", "Hel\u00b7las", ",", "was", "w\u00e4\u00b7re", "die", "Welt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "$,", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt, ihr V\u00f6lker aller Zonen,", "tokens": ["Kommt", ",", "ihr", "V\u00f6l\u00b7ker", "al\u00b7ler", "Zo\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht die Br\u00fcste,", "tokens": ["Seht", "die", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die euch s\u00e4ugten", "tokens": ["Die", "euch", "s\u00e4ug\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit der reinen Milch der Weisheit! \u2013", "tokens": ["Mit", "der", "rei\u00b7nen", "Milch", "der", "Weis\u00b7heit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollen Barbaren sie zerfleischen?", "tokens": ["Sol\u00b7len", "Bar\u00b7ba\u00b7ren", "sie", "zer\u00b7flei\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Seht die Augen,", "tokens": ["Seht", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die euch erleuchteten", "tokens": ["Die", "euch", "er\u00b7leuch\u00b7te\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit dem himmlischen Strahle der Sch\u00f6nheit! \u2013", "tokens": ["Mit", "dem", "himm\u00b7li\u00b7schen", "Strah\u00b7le", "der", "Sch\u00f6n\u00b7heit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Sollen sie Barbaren blenden?", "tokens": ["Sol\u00b7len", "sie", "Bar\u00b7ba\u00b7ren", "blen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Seht die Flamme,", "tokens": ["Seht", "die", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die euch w\u00e4rmte", "tokens": ["Die", "euch", "w\u00e4rm\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Durch und durch im tiefen Busen,", "tokens": ["Durch", "und", "durch", "im", "tie\u00b7fen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihr f\u00fchltet,", "tokens": ["Da\u00df", "ihr", "f\u00fchl\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wer ihr seid,", "tokens": ["Wer", "ihr", "seid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Was ihr wollt,", "tokens": ["Was", "ihr", "wollt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Was ihr sollt,", "tokens": ["Was", "ihr", "sollt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Eurer Menschheit hohen Adel,", "tokens": ["Eu\u00b7rer", "Menschheit", "ho\u00b7hen", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Eure Freiheit! \u2013", "tokens": ["Eu\u00b7re", "Frei\u00b7heit", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Sollen Barbaren sie ersticken?", "tokens": ["Sol\u00b7len", "Bar\u00b7ba\u00b7ren", "sie", "er\u00b7sti\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Kommt, ihr V\u00f6lker aller Zonen,", "tokens": ["Kommt", ",", "ihr", "V\u00f6l\u00b7ker", "al\u00b7ler", "Zo\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kommt und helfet frei sie machen,", "tokens": ["Kommt", "und", "hel\u00b7fet", "frei", "sie", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die euch alle frei gemacht!", "tokens": ["Die", "euch", "al\u00b7le", "frei", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ohne die Freiheit, was w\u00e4rest du, Hellas?", "tokens": ["Oh\u00b7ne", "die", "Frei\u00b7heit", ",", "was", "w\u00e4\u00b7rest", "du", ",", "Hel\u00b7las", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ohne dich, Hellas, was w\u00e4re die Welt?", "tokens": ["Oh\u00b7ne", "dich", ",", "Hel\u00b7las", ",", "was", "w\u00e4\u00b7re", "die", "Welt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "$,", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}