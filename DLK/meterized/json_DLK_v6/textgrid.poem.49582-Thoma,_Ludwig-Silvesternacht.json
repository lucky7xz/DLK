{"textgrid.poem.49582": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Silvesternacht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun, wenn alle Uhren schlagen,", "tokens": ["Und", "nun", ",", "wenn", "al\u00b7le", "Uh\u00b7ren", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So haben wir uns was zu sagen,", "tokens": ["So", "ha\u00b7ben", "wir", "uns", "was", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was feierlich und hoffnungsvoll", "tokens": ["Was", "fei\u00b7er\u00b7lich", "und", "hoff\u00b7nungs\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ernste Stunde weihen soll.", "tokens": ["Die", "erns\u00b7te", "Stun\u00b7de", "wei\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zuerst ein Prosit in der Runde!", "tokens": ["Zu\u00b7erst", "ein", "Pro\u00b7sit", "in", "der", "Run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein helles, und aus frohem Munde!", "tokens": ["Ein", "hel\u00b7les", ",", "und", "aus", "fro\u00b7hem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ward nicht erreicht ein jedes Ziel,", "tokens": ["Ward", "nicht", "er\u00b7reicht", "ein", "je\u00b7des", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir ", "tokens": ["Wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Noch einen Blick dem alten Jahre,", "tokens": ["Noch", "ei\u00b7nen", "Blick", "dem", "al\u00b7ten", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann legt es auf die Totenbahre!", "tokens": ["Dann", "legt", "es", "auf", "die", "To\u00b7ten\u00b7bah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neues gr\u00fcnt im vollen Saft!", "tokens": ["Ein", "neu\u00b7es", "gr\u00fcnt", "im", "vol\u00b7len", "Saft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm gelte unsre ganze Kraft!", "tokens": ["Ihm", "gel\u00b7te", "uns\u00b7re", "gan\u00b7ze", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir fragen nicht: Was wird es bringen?", "tokens": ["Wir", "fra\u00b7gen", "nicht", ":", "Was", "wird", "es", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel lieber wollen wir es zwingen,", "tokens": ["Viel", "lie\u00b7ber", "wol\u00b7len", "wir", "es", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df es mit uns nach vorne treibt,", "tokens": ["Da\u00df", "es", "mit", "uns", "nach", "vor\u00b7ne", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht r\u00fcckw\u00e4rts geht, nicht stehen bleibt.", "tokens": ["Nicht", "r\u00fcck\u00b7w\u00e4rts", "geht", ",", "nicht", "ste\u00b7hen", "bleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$,", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht schw\u00e4chlich, was sie bringt, zu tragen,", "tokens": ["Nicht", "schw\u00e4ch\u00b7lich", ",", "was", "sie", "bringt", ",", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit zu lenken, la\u00dft uns wagen!", "tokens": ["Die", "Zeit", "zu", "len\u00b7ken", ",", "la\u00dft", "uns", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann hat es weiter nicht Gefahr.", "tokens": ["Dann", "hat", "es", "wei\u00b7ter", "nicht", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem Sinne: Prost Neujahr!", "tokens": ["In", "die\u00b7sem", "Sin\u00b7ne", ":", "Prost", "Neu\u00b7jahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}