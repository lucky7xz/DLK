{"dta.poem.21298": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und fehlt der Pfaffenseegen dabei,", "tokens": ["Und", "fehlt", "der", "Pfaf\u00b7fen\u00b7see\u00b7gen", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Ehe wird g\u00fcltig nicht minder \u2013", "tokens": ["Die", "E\u00b7he", "wird", "g\u00fcl\u00b7tig", "nicht", "min\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKNEG", "ADV", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es lebe Br\u00e4utigam und Braut,", "tokens": ["Es", "le\u00b7be", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und ihre zuk\u00fcnftigen Kinder!", "tokens": ["Und", "ih\u00b7re", "zu\u00b7k\u00fcnf\u00b7ti\u00b7gen", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ein Hochzeitkarmen ist mein Lied,", "tokens": ["Ein", "Hoch\u00b7zeit\u00b7kar\u00b7men", "ist", "mein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das bessere, das neue!", "tokens": ["Das", "bes\u00b7se\u00b7re", ",", "das", "neu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In meiner Seele gehen auf", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "ge\u00b7hen", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sterne der h\u00f6chsten Weihe \u2013", "tokens": ["Die", "Ster\u00b7ne", "der", "h\u00f6chs\u00b7ten", "Wei\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Begeisterte Sterne, sie lodern wild,", "tokens": ["Be\u00b7geis\u00b7ter\u00b7te", "Ster\u00b7ne", ",", "sie", "lo\u00b7dern", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zerflie\u00dfen in Flammenb\u00e4chen \u2013", "tokens": ["Zer\u00b7flie\u00b7\u00dfen", "in", "Flam\u00b7men\u00b7b\u00e4\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich f\u00fchle mich wunderbar erstarkt,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "wun\u00b7der\u00b7bar", "er\u00b7starkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich k\u00f6nnte Eichen zerbrechen!", "tokens": ["Ich", "k\u00f6nn\u00b7te", "Ei\u00b7chen", "zer\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Seit ich auf deutsche Erde trat", "tokens": ["Seit", "ich", "auf", "deut\u00b7sche", "Er\u00b7de", "trat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchstr\u00f6men mich Zaubers\u00e4fte \u2013", "tokens": ["Durch\u00b7str\u00f6\u00b7men", "mich", "Zau\u00b7bers\u00b7\u00e4f\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Riese hat wieder die Mutter ber\u00fchrt,", "tokens": ["Der", "Rie\u00b7se", "hat", "wie\u00b7der", "die", "Mut\u00b7ter", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und es wuchsen ihm neu die Kr\u00e4fte.", "tokens": ["Und", "es", "wuch\u00b7sen", "ihm", "neu", "die", "Kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}