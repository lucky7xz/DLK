{"dta.poem.23615": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aria  \n  eines sehr gering besoldeten und doch k\u00f6niglichen  \n Professors am Vorabend seines 25j\u00e4hrigen  \n Dienstjubil\u00e4ums.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Dornen seh' ich Rosen bl\u00fchen,", "tokens": ["Aus", "Dor\u00b7nen", "seh'", "ich", "Ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O bl\u00fchte so mein Gl\u00fcck doch auch,", "tokens": ["O", "bl\u00fch\u00b7te", "so", "mein", "Gl\u00fcck", "doch", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn meines Lebens Sorg' und M\u00fchen", "tokens": ["Denn", "mei\u00b7nes", "Le\u00b7bens", "Sor\u00b7g'", "und", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sind mehr noch als ein Dornenstrauch.", "tokens": ["Sind", "mehr", "noch", "als", "ein", "Dor\u00b7nen\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Fr\u00fchling, Fr\u00fchling, denke mein,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", ",", "den\u00b7ke", "mein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df Gl\u00fcck und Rosen eins nur sein!", "tokens": ["La\u00df", "Gl\u00fcck", "und", "Ro\u00b7sen", "eins", "nur", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann mag verwelken und verschwinden", "tokens": ["Dann", "mag", "ver\u00b7wel\u00b7ken", "und", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch mit den Rosen mir mein Gl\u00fcck,", "tokens": ["Auch", "mit", "den", "Ro\u00b7sen", "mir", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es wird sich immer wiederfinden,", "tokens": ["Es", "wird", "sich", "im\u00b7mer", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn mit den Rosen kehrt's zur\u00fcck.", "tokens": ["Denn", "mit", "den", "Ro\u00b7sen", "kehrt's", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Fr\u00fchling, Fr\u00fchling, denke mein,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", ",", "den\u00b7ke", "mein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df Gl\u00fcck und Rosen eins nur sein!", "tokens": ["La\u00df", "Gl\u00fcck", "und", "Ro\u00b7sen", "eins", "nur", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}