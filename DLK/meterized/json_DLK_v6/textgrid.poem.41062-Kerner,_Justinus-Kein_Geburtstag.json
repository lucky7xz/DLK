{"textgrid.poem.41062": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Kein Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann du geboren, wei\u00df ich nicht,", "tokens": ["Wann", "du", "ge\u00b7bo\u00b7ren", ",", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will's wissen nicht, wenn ich's auch f\u00e4nde,", "tokens": ["Will's", "wis\u00b7sen", "nicht", ",", "wenn", "ich's", "auch", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei mir ein Kreis, ein ew'ges Licht,", "tokens": ["Sei", "mir", "ein", "Kreis", ",", "ein", "ew'\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ohne Anfang, so ohn' Ende!", "tokens": ["Wie", "oh\u00b7ne", "An\u00b7fang", ",", "so", "ohn'", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}