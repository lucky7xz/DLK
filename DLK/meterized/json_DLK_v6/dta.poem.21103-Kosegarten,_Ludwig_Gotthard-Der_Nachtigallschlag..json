{"dta.poem.21103": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtigallschlag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rest du wohl, wie die Nachtigall schl&#228;gt! Wie               ", "tokens": ["H\u00f6\u00b7rest", "du", "wohl", ",", "wie", "die", "Nach\u00b7ti\u00b7gall", "schl", "&#228;", "gt", "!", "Wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$(", "VVFIN", "$.", "PWAV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "ihr schmetternder Wirbel", "tokens": ["ihr", "schmet\u00b7tern\u00b7der", "Wir\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die ambrosische Nacht leiser und", "tokens": ["Durch", "die", "am\u00b7bro\u00b7si\u00b7sche", "Nacht", "lei\u00b7ser", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "leiser vert\u00f6nt?", "tokens": ["lei\u00b7ser", "ver\u00b7t\u00f6nt", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Meine Jugend, ich denk' an dich. Verjauchzt und", "tokens": ["Mei\u00b7ne", "Ju\u00b7gend", ",", "ich", "denk'", "an", "dich", ".", "Ver\u00b7jauchzt", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$.", "VVPP", "KON"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "verschmettert", "tokens": ["ver\u00b7schmet\u00b7tert"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Bist du. Melodischer nun t\u00f6ne mein Leben", "tokens": ["Bist", "du", ".", "Me\u00b7lo\u00b7di\u00b7scher", "nun", "t\u00f6\u00b7ne", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$.", "NE", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "dahin.", "tokens": ["da\u00b7hin", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}}}}}