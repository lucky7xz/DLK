{"dta.poem.59": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Kaiser-Krone .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seh ich eine Kaiser-Krone,", "tokens": ["Seh", "ich", "ei\u00b7ne", "Kai\u00b7ser\u00b7Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des hohen Stengels Throne,", "tokens": ["Auf", "des", "ho\u00b7hen", "Sten\u00b7gels", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie sich in ihrer Pracht,", "tokens": ["Wie", "sie", "sich", "in", "ih\u00b7rer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Untern Blumen herrlich macht:", "tokens": ["Un\u00b7tern", "Blu\u00b7men", "herr\u00b7lich", "macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So bewegt mich ihre Blume,", "tokens": ["So", "be\u00b7wegt", "mich", "ih\u00b7re", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu des grossen Sch\u00f6pfers Ruhme.", "tokens": ["Zu", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie steht auf den Blumen-Beeten", "tokens": ["Sie", "steht", "auf", "den", "Blu\u00b7men\u00b7Bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als auf sammtnen Kunsttapeten", "tokens": ["Als", "auf", "sammt\u00b7nen", "Kunst\u00b7ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Natur, da auf dem Gr\u00fcn,", "tokens": ["Der", "Na\u00b7tur", ",", "da", "auf", "dem", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um sie her viel Blumen bl\u00fchn,", "tokens": ["Um", "sie", "her", "viel", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die gleichsam zu ihren F\u00fcssen,", "tokens": ["Die", "gleich\u00b7sam", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als ganz niedrig wachsen m\u00fcssen.", "tokens": ["Als", "ganz", "nied\u00b7rig", "wach\u00b7sen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nahme, Bildung, andre Zeichen,", "tokens": ["Nah\u00b7me", ",", "Bil\u00b7dung", ",", "and\u00b7re", "Zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehren mich sie zu vergleichen", "tokens": ["Leh\u00b7ren", "mich", "sie", "zu", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den F\u00fcrsten dieser Welt,", "tokens": ["Mit", "den", "F\u00fcrs\u00b7ten", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die auf Thronen sind gestellt,", "tokens": ["Die", "auf", "Thro\u00b7nen", "sind", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die vor andern auf der Erden,", "tokens": ["Die", "vor", "an\u00b7dern", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsern Augen herrlich werden.", "tokens": ["Un\u00b7sern", "Au\u00b7gen", "herr\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich seh um sie Bl\u00e4tter hangen,", "tokens": ["Ich", "seh", "um", "sie", "Bl\u00e4t\u00b7ter", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die wie scharfe Schwerdter prangen,", "tokens": ["Die", "wie", "schar\u00b7fe", "Schwerd\u00b7ter", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Blik der gab mir ein,", "tokens": ["Und", "der", "Blik", "der", "gab", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie wie Trabanten seyn,", "tokens": ["Da\u00df", "sie", "wie", "Tra\u00b7ban\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die diejenigen besch\u00fctzen,", "tokens": ["Die", "die\u00b7je\u00b7ni\u00b7gen", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die auf hohen Thronen sitzen.", "tokens": ["Die", "auf", "ho\u00b7hen", "Thro\u00b7nen", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhre rothe Purpur-Bl\u00fcthe,", "tokens": ["Ih\u00b7re", "ro\u00b7the", "Pur\u00b7pur\u00b7B\u00b7l\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcckte mir in das Gem\u00fcthe,", "tokens": ["Dr\u00fcck\u00b7te", "mir", "in", "das", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein recht Lehrreich Sinnbild ein,", "tokens": ["Ein", "recht", "Lehr\u00b7reich", "Sinn\u00b7bild", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die F\u00fcrsten m\u00fcssen seyn:", "tokens": ["Wie", "die", "F\u00fcrs\u00b7ten", "m\u00fcs\u00b7sen", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich sah sie zu ihren Zweigen,", "tokens": ["Ich", "sah", "sie", "zu", "ih\u00b7ren", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Gesicht herunter neigen.", "tokens": ["Das", "Ge\u00b7sicht", "her\u00b7un\u00b7ter", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn die F\u00fcrsten von den H\u00f6hen,", "tokens": ["Wenn", "die", "F\u00fcrs\u00b7ten", "von", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Unterthanen sehen,", "tokens": ["Auf", "die", "Un\u00b7ter\u00b7tha\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Gnade Angesicht;", "tokens": ["Mit", "der", "Gna\u00b7de", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So erf\u00fcllen sie die Pflicht,", "tokens": ["So", "er\u00b7f\u00fcl\u00b7len", "sie", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sie nach den weisen Schl\u00fcssen", "tokens": ["Die", "sie", "nach", "den", "wei\u00b7sen", "Schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhres Sch\u00f6pfers, leisten m\u00fcssen.", "tokens": ["Ih\u00b7res", "Sch\u00f6p\u00b7fers", ",", "leis\u00b7ten", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "An den Kaiser-Kronen Spizzen,", "tokens": ["An", "den", "Kai\u00b7ser\u00b7Kro\u00b7nen", "Spiz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich einen B\u00fcschel sizzen,", "tokens": ["Sah", "ich", "ei\u00b7nen", "B\u00fc\u00b7schel", "siz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der gleich einem gr\u00fcnen Gra\u00df,", "tokens": ["Der", "gleich", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Gra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Daran hing ein tr\u00f6h felnd Na\u00df,", "tokens": ["Da\u00b7ran", "hing", "ein", "tr\u00f6h", "felnd", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das beim Strahl der heitren Sonnen,", "tokens": ["Das", "beim", "Strahl", "der", "hei\u00b7tren", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieblich kam herab geronnen.", "tokens": ["Lieb\u00b7lich", "kam", "her\u00b7ab", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dieser Thau, da er zerflossen,", "tokens": ["Die\u00b7ser", "Thau", ",", "da", "er", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fiel gar sanfte auf die Sprossen", "tokens": ["Fiel", "gar", "sanf\u00b7te", "auf", "die", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Derer niedren Blumen hin,", "tokens": ["De\u00b7rer", "nied\u00b7ren", "Blu\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dabei kam mir in den Sin", "tokens": ["Da\u00b7bei", "kam", "mir", "in", "den", "Sin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Wunsch: Ach! m\u00f6cht auf Erden,", "tokens": ["Die\u00b7ser", "Wunsch", ":", "Ach", "!", "m\u00f6cht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "ITJ", "$.", "VMFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder F\u00fcrst ihr \u00e4hnlich werden!", "tokens": ["Je\u00b7der", "F\u00fcrst", "ihr", "\u00e4hn\u00b7lich", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "M\u00f6chten sie des Himmels Seegen,", "tokens": ["M\u00f6ch\u00b7ten", "sie", "des", "Him\u00b7mels", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch auf Unterthanen legen,", "tokens": ["Auch", "auf", "Un\u00b7ter\u00b7tha\u00b7nen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der von den bestirnten Zelt,", "tokens": ["Der", "von", "den", "bes\u00b7tirn\u00b7ten", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die hohen H\u00e4upter f\u00e4llt:", "tokens": ["Auf", "die", "ho\u00b7hen", "H\u00e4up\u00b7ter", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alsdenn w\u00fcrden im Gedeien,", "tokens": ["Als\u00b7denn", "w\u00fcr\u00b7den", "im", "Ge\u00b7dei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Niedre sich der Hohen freuen.", "tokens": ["Nied\u00b7re", "sich", "der", "Ho\u00b7hen", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich besah die innre R\u00fcnde,", "tokens": ["Ich", "be\u00b7sah", "die", "inn\u00b7re", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich in der Blume f\u00fcnde;", "tokens": ["Was", "ich", "in", "der", "Blu\u00b7me", "f\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gukte in den Kelch hinein,", "tokens": ["Guk\u00b7te", "in", "den", "Kelch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da cristalne Augen seyn,", "tokens": ["Da", "cris\u00b7tal\u00b7ne", "Au\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure":