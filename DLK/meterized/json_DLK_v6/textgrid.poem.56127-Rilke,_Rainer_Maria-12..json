{"textgrid.poem.56127": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dem Geist, der uns verbinden mag;", "tokens": ["Heil", "dem", "Geist", ",", "der", "uns", "ver\u00b7bin\u00b7den", "mag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "denn wir leben wahrhaft in Figuren.", "tokens": ["denn", "wir", "le\u00b7ben", "wahr\u00b7haft", "in", "Fi\u00b7gu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit kleinen Schritten gehn die Uhren", "tokens": ["Und", "mit", "klei\u00b7nen", "Schrit\u00b7ten", "gehn", "die", "Uh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "neben unserm eigentlichen Tag.", "tokens": ["ne\u00b7ben", "un\u00b7serm", "ei\u00b7gent\u00b7li\u00b7chen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ohne unsern wahren Platz zu kennen,", "tokens": ["Oh\u00b7ne", "un\u00b7sern", "wah\u00b7ren", "Platz", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "handeln wir aus wirklichem Bezug.", "tokens": ["han\u00b7deln", "wir", "aus", "wirk\u00b7li\u00b7chem", "Be\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Antennen f\u00fchlen die Antennen,", "tokens": ["Die", "An\u00b7ten\u00b7nen", "f\u00fch\u00b7len", "die", "An\u00b7ten\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und die leere Ferne trug...", "tokens": ["und", "die", "lee\u00b7re", "Fer\u00b7ne", "trug", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reine Spannung. O Musik der Kr\u00e4fte!", "tokens": ["Rei\u00b7ne", "Span\u00b7nung", ".", "O", "Mu\u00b7sik", "der", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist nicht durch die l\u00e4\u00dflichen Gesch\u00e4fte", "tokens": ["Ist", "nicht", "durch", "die", "l\u00e4\u00df\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "jede St\u00f6rung von dir abgelenkt?", "tokens": ["je\u00b7de", "St\u00f6\u00b7rung", "von", "dir", "ab\u00b7ge\u00b7lenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Selbst wenn sich der Bauer sorgt und handelt,", "tokens": ["Selbst", "wenn", "sich", "der", "Bau\u00b7er", "sorgt", "und", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wo die Saat in Sommer sich verwandelt,", "tokens": ["wo", "die", "Saat", "in", "Som\u00b7mer", "sich", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "reicht er niemals hin. Die Erde ", "tokens": ["reicht", "er", "nie\u00b7mals", "hin", ".", "Die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}