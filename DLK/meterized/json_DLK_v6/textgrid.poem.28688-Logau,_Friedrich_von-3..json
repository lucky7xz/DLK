{"textgrid.poem.28688": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treue F\u00fcrstin, unsrer Welt", "tokens": ["Treu\u00b7e", "F\u00fcrs\u00b7tin", ",", "uns\u00b7rer", "Welt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist auffs neue zugestellt", "tokens": ["Ist", "auffs", "neu\u00b7e", "zu\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "VVPP"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Von der Sonn ein edler Ring,", "tokens": ["Von", "der", "Sonn", "ein", "ed\u00b7ler", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie iedes Jahr empfing.", "tokens": ["Wie", "sie", "ie\u00b7des", "Jahr", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! es wolle diesem Ringe", "tokens": ["Ach", "!", "es", "wol\u00b7le", "die\u00b7sem", "Rin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seyn verpflichtet di\u00df Gedinge:", "tokens": ["Seyn", "ver\u00b7pflich\u00b7tet", "di\u00df", "Ge\u00b7din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er steh zu sichrem Pfande", "tokens": ["Da\u00df", "er", "steh", "zu", "sich\u00b7rem", "Pfan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eurem Gl\u00fcck und Segens-Stande,", "tokens": ["Eu\u00b7rem", "Gl\u00fcck", "und", "Se\u00b7gens\u00b7Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So wie Ihr, Ihr F\u00fcrsten-Gold,", "tokens": ["So", "wie", "Ihr", ",", "Ihr", "F\u00fcrs\u00b7ten\u00b7Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Haupt und Gliedern Heil und Hold", "tokens": ["Haupt", "und", "Glie\u00b7dern", "Heil", "und", "Hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Gabt durch den Verm\u00e4hlungs-Ring,", "tokens": ["Gabt", "durch", "den", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7Ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Den mein Printz von Euch empfing!", "tokens": ["Den", "mein", "Printz", "von", "Euch", "emp\u00b7fing", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Krieg, weich ab und neme Scheue", "tokens": ["Krieg", ",", "weich", "ab", "und", "ne\u00b7me", "Scheu\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "PTKVZ", "KON", "ADJA", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "F\u00fcr de\u00df Friedens fromer Treue!", "tokens": ["F\u00fcr", "de\u00df", "Frie\u00b7dens", "fro\u00b7mer", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "B\u00f6se T\u00fcck und Triegligkeiten", "tokens": ["B\u00f6\u00b7se", "T\u00fcck", "und", "Trieg\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lauffen f\u00fcr den Redligheiten!", "tokens": ["Lauf\u00b7fen", "f\u00fcr", "den", "Red\u00b7lig\u00b7hei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Altes Arg sterb alles hin,", "tokens": ["Al\u00b7tes", "Arg", "sterb", "al\u00b7les", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Neues Wol blieb immer gr\u00fcn!", "tokens": ["Neu\u00b7es", "Wol", "blieb", "im\u00b7mer", "gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Altes Unrecht, alter Drang,", "tokens": ["Al\u00b7tes", "Un\u00b7recht", ",", "al\u00b7ter", "Drang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Geh zur H\u00f6lle schnellsten Gang!", "tokens": ["Geh", "zur", "H\u00f6l\u00b7le", "schnells\u00b7ten", "Gang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Heldin, Euren tapffren Sinnen", "tokens": ["Hel\u00b7din", ",", "Eu\u00b7ren", "tapf\u00b7fren", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Fehle nimmer kein Beginnen!", "tokens": ["Feh\u00b7le", "nim\u00b7mer", "kein", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Eurem Willen m\u00fcsse lachen,", "tokens": ["Eu\u00b7rem", "Wil\u00b7len", "m\u00fcs\u00b7se", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Was sich sonst pflegt ernst zu machen!", "tokens": ["Was", "sich", "sonst", "pflegt", "ernst", "zu", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Eurem schaffen m\u00fcsse stehn,", "tokens": ["Eu\u00b7rem", "schaf\u00b7fen", "m\u00fcs\u00b7se", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Was von dannen sonst wil gehn!", "tokens": ["Was", "von", "dan\u00b7nen", "sonst", "wil", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Eurem wincken kumme her,", "tokens": ["Eu\u00b7rem", "win\u00b7cken", "kum\u00b7me", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Was sonst blieben sonst wo w\u00e4r!", "tokens": ["Was", "sonst", "blie\u00b7ben", "sonst", "wo", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "PWAV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Ach, da\u00df Eure reine Sch\u00f6ne", "tokens": ["Ach", ",", "da\u00df", "Eu\u00b7re", "rei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Keine Schwachheit nie verh\u00f6hne!", "tokens": ["Kei\u00b7ne", "Schwach\u00b7heit", "nie", "ver\u00b7h\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Da\u00df stets Eure Liebligkeiten", "tokens": ["Da\u00df", "stets", "Eu\u00b7re", "Lieb\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Opffer nemen von den Zeiten!", "tokens": ["Opf\u00b7fer", "ne\u00b7men", "von", "den", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.33": {"text": "Da\u00df stets Eure Fr\u00f6migkeit", "tokens": ["Da\u00df", "stets", "Eu\u00b7re", "Fr\u00f6\u00b7mig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Sey ein Gifft f\u00fcr Ha\u00df und Neid!", "tokens": ["Sey", "ein", "Gifft", "f\u00fcr", "Ha\u00df", "und", "Neid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df der ungef\u00e4lschte Mut", "tokens": ["Da\u00df", "der", "un\u00b7ge\u00b7f\u00e4lschte", "Mut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Sey f\u00fcr List und Vorthel gut!", "tokens": ["Sey", "f\u00fcr", "List", "und", "Vor\u00b7thel", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Was Euch sonsten ist bescheiden", "tokens": ["Was", "Euch", "sons\u00b7ten", "ist", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Von dem Himmel, m\u00fcsse neiden", "tokens": ["Von", "dem", "Him\u00b7mel", ",", "m\u00fcs\u00b7se", "nei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Ieder, der au\u00df schwartzer T\u00fccke", "tokens": ["Ie\u00b7der", ",", "der", "au\u00df", "schwart\u00b7zer", "T\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Nagt de\u00df andren redlich Gl\u00fccke.", "tokens": ["Nagt", "de\u00df", "an\u00b7dren", "red\u00b7lich", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Fr\u00f6lich mussen dr\u00fcber seyn,", "tokens": ["Fr\u00f6\u00b7lich", "mus\u00b7sen", "dr\u00fc\u00b7ber", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PAV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Die sich liessen schreiben ein", "tokens": ["Die", "sich", "lies\u00b7sen", "schrei\u00b7ben", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "VVINF", "VVFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "In den Biedermannes Bund,", "tokens": ["In", "den", "Bie\u00b7der\u00b7man\u00b7nes", "Bund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Da kein Dupelman nie stund!", "tokens": ["Da", "kein", "Du\u00b7pel\u00b7man", "nie", "stund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Lange, lange m\u00fcssen laben", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "m\u00fcs\u00b7sen", "la\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Meinen Herrscher Eure Gaben!", "tokens": ["Mei\u00b7nen", "Herr\u00b7scher", "Eu\u00b7re", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Lange, lange m\u00fcst ihr leben,", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "m\u00fcst", "ihr", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Diese Gaben au\u00dfzugeben,", "tokens": ["Die\u00b7se", "Ga\u00b7ben", "au\u00df\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "So da\u00df die gepaarte Treu", "tokens": ["So", "da\u00df", "die", "ge\u00b7paar\u00b7te", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.50": {"text": "Immer bleibe frisch und neu!", "tokens": ["Im\u00b7mer", "blei\u00b7be", "frisch", "und", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Bi\u00df Piastus alter Baum", "tokens": ["Bi\u00df", "Pi\u00b7as\u00b7tus", "al\u00b7ter", "Baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Wieder kumm in ersten Raum,", "tokens": ["Wie\u00b7der", "kumm", "in", "ers\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Da\u00df er mit gevielten Zweigen", "tokens": ["Da\u00df", "er", "mit", "ge\u00b7viel\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "M\u00fcge bi\u00df zun Sternen steigen,", "tokens": ["M\u00fc\u00b7ge", "bi\u00df", "zun", "Ster\u00b7nen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Da\u00df er unser Land bebreite", "tokens": ["Da\u00df", "er", "un\u00b7ser", "Land", "be\u00b7brei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Mit de\u00df Schatens gr\u00fcner Weite,", "tokens": ["Mit", "de\u00df", "Scha\u00b7tens", "gr\u00fc\u00b7ner", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Da\u00df der lechzend Unterthan", "tokens": ["Da\u00df", "der", "lech\u00b7zend", "Un\u00b7ter\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Drunter sich erfrischen kan,", "tokens": ["Drun\u00b7ter", "sich", "er\u00b7fri\u00b7schen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Da\u00df er kan von seiner Frucht", "tokens": ["Da\u00df", "er", "kan", "von", "sei\u00b7ner", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Niessen, was er darff und sucht!", "tokens": ["Nies\u00b7sen", ",", "was", "er", "darff", "und", "sucht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VMFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Ich, so ich mich darff vermessen,", "tokens": ["Ich", ",", "so", "ich", "mich", "darff", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "PPER", "PRF", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Meiner selbst nicht zu vergessen,", "tokens": ["Mei\u00b7ner", "selbst", "nicht", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "W\u00fcntsche mir zu meinem Theile:", "tokens": ["W\u00fcnt\u00b7sche", "mir", "zu", "mei\u00b7nem", "Thei\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Da\u00df mir ietzt und alle weile", "tokens": ["Da\u00df", "mir", "ietzt", "und", "al\u00b7le", "wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Meine Herrschafft traue zu,", "tokens": ["Mei\u00b7ne", "Herr\u00b7schafft", "trau\u00b7e", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.66": {"text": "Da\u00df ich nimmer spar und ruh,", "tokens": ["Da\u00df", "ich", "nim\u00b7mer", "spar", "und", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text