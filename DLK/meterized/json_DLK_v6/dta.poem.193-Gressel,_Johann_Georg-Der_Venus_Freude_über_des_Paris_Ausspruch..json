{"dta.poem.193": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der  Venus  Freude \u00fcber des  Paris  \n Ausspruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.85", "da:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt Amourette, kommt/ helfft stoltzen Schmuck", "tokens": ["Kommt", "A\u00b7mou\u00b7ret\u00b7te", ",", "kommt", "/", "helfft", "stolt\u00b7zen", "Schmuck"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "$(", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bereiten/", "tokens": ["be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bringt weitgehohte W\u00fcrtz/ und theuren ", "tokens": ["Bringt", "weit\u00b7ge\u00b7hoh\u00b7te", "W\u00fcrtz", "/", "und", "theu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "$(", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Als Zimmet-Oele ist/ und Balsam kan bestreiten/", "tokens": ["Als", "Zim\u00b7met\u00b7O\u00b7ele", "ist", "/", "und", "Bal\u00b7sam", "kan", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$(", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Jhr Wolcken thauet Safft/ das dem ", "tokens": ["Ihr", "Wol\u00b7cken", "thau\u00b7et", "Safft", "/", "das", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$(", "PDS", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und am Geruch die Krafft des ", "tokens": ["Und", "am", "Ge\u00b7ruch", "die", "Krafft", "des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Mein Ansehn hat jetztmehr als sonsten zugenommen/", "tokens": ["Mein", "An\u00b7sehn", "hat", "jetzt\u00b7mehr", "als", "sons\u00b7ten", "zu\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KOKOM", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es streut die gantze Welt mir Weyrauch aufs Altar/", "tokens": ["Es", "streut", "die", "gant\u00b7ze", "Welt", "mir", "Wey\u00b7rauch", "aufs", "Al\u00b7tar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Den g\u00fcldnen Apffel hat mir ", "tokens": ["Den", "g\u00fcld\u00b7nen", "Apf\u00b7fel", "hat", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Es hat mein nackter Leib den andern obgesiegt/", "tokens": ["Es", "hat", "mein", "nack\u00b7ter", "Leib", "den", "an\u00b7dern", "ob\u00b7ge\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Minerv-und Junens Pracht mir zu den F\u00fcssen liegt/", "tokens": ["Mi\u00b7ner\u00b7v\u00b7und", "Ju\u00b7nens", "Pracht", "mir", "zu", "den", "F\u00fcs\u00b7sen", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Mein Leib hat sich jetzt mehr durch Sch\u00f6nheit auffgeschwellet/", "tokens": ["Mein", "Leib", "hat", "sich", "jetzt", "mehr", "durch", "Sch\u00f6n\u00b7heit", "auff\u00b7ge\u00b7schwel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Zunder s\u00fcsser Lust mit mehrer Anmuth glimmt/", "tokens": ["Der", "Zun\u00b7der", "s\u00fcs\u00b7ser", "Lust", "mit", "meh\u00b7rer", "An\u00b7muth", "glimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da meiner Glieder-Pracht die andern \u00fcberstimmt/", "tokens": ["Da", "mei\u00b7ner", "Glie\u00b7der\u00b7Pracht", "die", "an\u00b7dern", "\u00fc\u00b7bers\u00b7timmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein Sch\u00f6n-seyn br\u00fcstet sich mit gr\u00f6ssern Zierlichkeiten/", "tokens": ["Mein", "Sch\u00f6n\u00b7seyn", "br\u00fcs\u00b7tet", "sich", "mit", "gr\u00f6s\u00b7sern", "Zier\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Als da ich aus der Fluht des bittern Meeres kahm/", "tokens": ["Als", "da", "ich", "aus", "der", "Fluht", "des", "bit\u00b7tern", "Mee\u00b7res", "kahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und mich mein ", "tokens": ["Und", "mich", "mein"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Mir/ als der Sch\u00f6nesten/ z\u00fcndt jeder Opffer an/", "tokens": ["Mir", "/", "als", "der", "Sch\u00f6\u00b7nes\u00b7ten", "/", "z\u00fcndt", "je\u00b7der", "Opf\u00b7fer", "an", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "ART", "NN", "$(", "VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Und diese Ehre wird mit Recht mir angethan/", "tokens": ["Und", "die\u00b7se", "Eh\u00b7re", "wird", "mit", "Recht", "mir", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich bin die n\u00fctzlichste/ und allen vorzuziehen/", "tokens": ["Ich", "bin", "die", "n\u00fctz\u00b7lichs\u00b7te", "/", "und", "al\u00b7len", "vor\u00b7zu\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$(", "KON", "PIS", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das Reich-seyn hilfft gar nichts/ die Sch\u00e4tze find nur", "tokens": ["Das", "Reich\u00b7seyn", "hilfft", "gar", "nichts", "/", "die", "Sch\u00e4t\u00b7ze", "find", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIS", "$(", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.28": {"text": "Dunst/", "tokens": ["Dunst", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Die Weisheit/ die zerf\u00e4llt/ es gehet ein die Kunst;", "tokens": ["Die", "Weis\u00b7heit", "/", "die", "zer\u00b7f\u00e4llt", "/", "es", "ge\u00b7het", "ein", "die", "Kunst", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "$(", "PPER", "VVFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn keine Menschen mehr zu unsern F\u00fcssen bl\u00fchen/", "tokens": ["Wenn", "kei\u00b7ne", "Men\u00b7schen", "mehr", "zu", "un\u00b7sern", "F\u00fcs\u00b7sen", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Durch mich besteht die Welt/ mein Wesen brennt den Geist/", "tokens": ["Durch", "mich", "be\u00b7steht", "die", "Welt", "/", "mein", "We\u00b7sen", "brennt", "den", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$(", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Durch meine Macht entsteht was Brunst und Liebe heist.", "tokens": ["Durch", "mei\u00b7ne", "Macht", "ent\u00b7steht", "was", "Brunst", "und", "Lie\u00b7be", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vor lag was Lieben heist in d\u00fcstrer Grufft vergraben/", "tokens": ["Vor", "lag", "was", "Lie\u00b7ben", "heist", "in", "d\u00fcst\u00b7rer", "Grufft", "ver\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PWS", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Anmuth war noch nicht der Erden anverm\u00e4hlt/", "tokens": ["Die", "An\u00b7muth", "war", "noch", "nicht", "der", "Er\u00b7den", "an\u00b7ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Welt war \u00f6d und w\u00fcst/ der Mensch stund als entseelt/", "tokens": ["Die", "Welt", "war", "\u00f6d", "und", "w\u00fcst", "/", "der", "Mensch", "stund", "als", "ent\u00b7seelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$(", "ART", "NN", "ADJD", "KOKOM", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bis da\u00df ich aus der See kam mit den Liebes-Gaben.", "tokens": ["Bis", "da\u00df", "ich", "aus", "der", "See", "kam", "mit", "den", "Lie\u00b7bes\u00b7Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Kaum aber hatte mich das feste Land erblickt/", "tokens": ["Kaum", "a\u00b7ber", "hat\u00b7te", "mich", "das", "fes\u00b7te", "Land", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als schon die Creatur sich sch\u00e4tzete begl\u00fcckt.", "tokens": ["Als", "schon", "die", "Crea\u00b7tur", "sich", "sch\u00e4t\u00b7ze\u00b7te", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.39": {"text": "Die Menschen lebten auf/ es liebte was gestorben/", "tokens": ["Die", "Men\u00b7schen", "leb\u00b7ten", "auf", "/", "es", "lieb\u00b7te", "was", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "PPER", "VVFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die V\u00f6gel k\u00fc\u00dft en sich/ die Fische trieben Schertz/", "tokens": ["Die", "V\u00f6\u00b7gel", "k\u00fc\u00dft", "en", "sich", "/", "die", "Fi\u00b7sche", "trie\u00b7ben", "Schertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "FM", "PRF", "$(", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es war da lauter Lust/ wo vormahls d\u00fcstrer Schmertz/", "tokens": ["Es", "war", "da", "lau\u00b7ter", "Lust", "/", "wo", "vor\u00b7mahls", "d\u00fcst\u00b7rer", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$(", "PWAV", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Vor Hitze schwitzte das/ was durch den Frost verdorben/", "tokens": ["Vor", "Hit\u00b7ze", "schwitz\u00b7te", "das", "/", "was", "durch", "den", "Frost", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "$(", "PWS", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die Menschen paarten sich/ zwey nennten sich ein Leib/", "tokens": ["Die", "Men\u00b7schen", "paar\u00b7ten", "sich", "/", "zwey", "nenn\u00b7ten", "sich", "ein", "Leib", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "CARD", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da dieses eins doch zwey zusammen Mann und Weib.", "tokens": ["Da", "die\u00b7ses", "eins", "doch", "zwey", "zu\u00b7sam\u00b7men", "Mann", "und", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PIS", "ADV", "CARD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "An Sch\u00f6nheit ", "tokens": ["An", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.46": {"text": "Wer bringt das sch\u00f6ne Volck/ die Jungfern wol herf\u00fcr?", "tokens": ["Wer", "bringt", "das", "sch\u00f6\u00b7ne", "Volck", "/", "die", "Jung\u00b7fern", "wol", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Bin ichs/ die ", "tokens": ["Bin", "ichs", "/", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "PIS", "$(", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.48": {"text": "Ich kan durch ihren Blick das h\u00e4rtste Hertz erweichen/", "tokens": ["Ich", "kan", "durch", "ih\u00b7ren", "Blick", "das", "h\u00e4rts\u00b7te", "Hertz", "er\u00b7wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Alleine dis Geschlecht reicht Kron und Palmen her", "tokens": ["Al\u00b7l