{"dta.poem.10228": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Abels Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Ritter, mit schwarzem Panzer bewehrt,", "tokens": ["Zwei", "Rit\u00b7ter", ",", "mit", "schwar\u00b7zem", "Pan\u00b7zer", "be\u00b7wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Page g\u00fcrtet dem K\u00f6nig das Schwert", "tokens": ["Der", "Pa\u00b7ge", "g\u00fcr\u00b7tet", "dem", "K\u00f6\u00b7nig", "das", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Lager l\u00e4rmt es. Des Himmels Zier", "tokens": ["Im", "La\u00b7ger", "l\u00e4rmt", "es", ".", "Des", "Him\u00b7mels", "Zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edie Hengste vor. Der Friesenstier", "tokens": ["\u201e", "die", "Hengs\u00b7te", "vor", ".", "Der", "Fries\u00b7en\u00b7stier"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig ruft es, die Sonne glitzt,", "tokens": ["Der", "K\u00f6\u00b7nig", "ruft", "es", ",", "die", "Son\u00b7ne", "glitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des K\u00f6nigs goldene R\u00fcstung blitzt,", "tokens": ["Des", "K\u00f6\u00b7nigs", "gol\u00b7de\u00b7ne", "R\u00fcs\u00b7tung", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dicht dr\u00e4ngt Wulff Bokwoldt den Schecken heran,", "tokens": ["Dicht", "dr\u00e4ngt", "Wulff", "Bok\u00b7woldt", "den", "Sche\u00b7cken", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heut wird er ein Ritter, heut wird er ein Mann,", "tokens": ["Heut", "wird", "er", "ein", "Rit\u00b7ter", ",", "heut", "wird", "er", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Die Friesen k\u00e4mpfen f\u00fcr Herd und Weib,", "tokens": ["Die", "Frie\u00b7sen", "k\u00e4mp\u00b7fen", "f\u00fcr", "Herd", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die schwarzen Ritter strecken den Leib,", "tokens": ["Die", "schwar\u00b7zen", "Rit\u00b7ter", "stre\u00b7cken", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Der K\u00f6nig allein, er irrt auf dem Deich,", "tokens": ["Der", "K\u00f6\u00b7nig", "al\u00b7lein", ",", "er", "irrt", "auf", "dem", "Deich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ringsum der Feind. Keinen S\u00fcnder bleich,", "tokens": ["Ring\u00b7sum", "der", "Feind", ".", "Kei\u00b7nen", "S\u00fcn\u00b7der", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "In die Friesen trug er sein Schwert Hilfnot,", "tokens": ["In", "die", "Frie\u00b7sen", "trug", "er", "sein", "Schwert", "Hilf\u00b7not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wessel Hummer aus Pellworm schlug ihn tot", "tokens": ["Wes\u00b7sel", "Hum\u00b7mer", "aus", "Pell\u00b7worm", "schlug", "ihn", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Page, wo blieb der Page klein,", "tokens": ["Der", "Pa\u00b7ge", ",", "wo", "blieb", "der", "Pa\u00b7ge", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um seine wei\u00dfen Glieder fein", "tokens": ["Um", "sei\u00b7ne", "wei\u00b7\u00dfen", "Glie\u00b7der", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}