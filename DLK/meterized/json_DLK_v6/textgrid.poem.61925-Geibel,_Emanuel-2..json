{"textgrid.poem.61925": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngling mit dem goldnen Bogen,", "tokens": ["J\u00fcng\u00b7ling", "mit", "dem", "gold\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner Gott der Poesie,", "tokens": ["Sch\u00f6\u00b7ner", "Gott", "der", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Oftmals warst du mir gewogen,", "tokens": ["Oft\u00b7mals", "warst", "du", "mir", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch so dankt' ich's dir noch nie.", "tokens": ["Doch", "so", "dankt'", "ich's", "dir", "noch", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn in nie gehofften Flammen", "tokens": ["Denn", "in", "nie", "ge\u00b7hoff\u00b7ten", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchrtest du aus \u00f6der Nacht,", "tokens": ["F\u00fchr\u00b7test", "du", "aus", "\u00f6\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoher, mich mit ", "tokens": ["Ho\u00b7her", ",", "mich", "mit"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PRF", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die mich jung und selig macht.", "tokens": ["Die", "mich", "jung", "und", "se\u00b7lig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hat ein Mitleid ohnegleichen", "tokens": ["Hat", "ein", "Mit\u00b7leid", "oh\u00b7ne\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein olympisch Herz bewegt,", "tokens": ["Dein", "o\u00b7lym\u00b7pisch", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du pl\u00f6tzlich diesen reichen", "tokens": ["Da\u00df", "du", "pl\u00f6tz\u00b7lich", "die\u00b7sen", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schatz in meinen Arm gelegt?", "tokens": ["Schatz", "in", "mei\u00b7nen", "Arm", "ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oder hast du nur in Eile,", "tokens": ["O\u00b7der", "hast", "du", "nur", "in", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh die Senne dir entrauscht,", "tokens": ["Eh", "die", "Sen\u00b7ne", "dir", "ent\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen Pfeil mit Eros' Pfeile,", "tokens": ["Dei\u00b7nen", "Pfeil", "mit", "E\u00b7ros'", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, zu meinem Gl\u00fcck vertauscht?", "tokens": ["Ach", ",", "zu", "mei\u00b7nem", "Gl\u00fcck", "ver\u00b7tauscht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}