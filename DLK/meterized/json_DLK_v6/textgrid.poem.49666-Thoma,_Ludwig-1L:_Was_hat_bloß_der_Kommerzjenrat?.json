{"textgrid.poem.49666": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hat blo\u00df der Kommerzjenrat?", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hat blo\u00df der Kommerzjenrat?", "tokens": ["Was", "hat", "blo\u00df", "der", "Kom\u00b7merz\u00b7jen\u00b7rat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sieht doch, da\u00df er etwas hat!", "tokens": ["Man", "sieht", "doch", ",", "da\u00df", "er", "et\u00b7was", "hat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er denket nach; er denket tief;", "tokens": ["Er", "den\u00b7ket", "nach", ";", "er", "den\u00b7ket", "tief", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein ganzes Wesen ist pensiv;", "tokens": ["Sein", "gan\u00b7zes", "We\u00b7sen", "ist", "pen\u00b7siv", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Auge blicket feucht empor;", "tokens": ["Sein", "Au\u00b7ge", "bli\u00b7cket", "feucht", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es kommt ihm etwas heilig vor.", "tokens": ["Es", "kommt", "ihm", "et\u00b7was", "hei\u00b7lig", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach freilich! Ja! \u2013 Das Vaterland!", "tokens": ["Ach", "frei\u00b7lich", "!", "Ja", "!", "\u2013", "Das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PTKANT", "$.", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es trauert noch im Witwenstand,", "tokens": ["Es", "trau\u00b7ert", "noch", "im", "Wit\u00b7wen\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indem es keinen K\u00f6nig hat.", "tokens": ["In\u00b7dem", "es", "kei\u00b7nen", "K\u00f6\u00b7nig", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das schmerzet den Kommerzjenrat.", "tokens": ["Das", "schmer\u00b7zet", "den", "Kom\u00b7merz\u00b7jen\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Seit Sechsundachtzig trinken wir", "tokens": ["Seit", "Sech\u00b7sun\u00b7dacht\u00b7zig", "trin\u00b7ken", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In der Regentschaft unser Bier.", "tokens": ["In", "der", "Re\u00b7gent\u00b7schaft", "un\u00b7ser", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Kinder, wo geboren wer'n,", "tokens": ["Die", "Kin\u00b7der", ",", "wo", "ge\u00b7bo\u00b7ren", "wer'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sind unter einem Ungl\u00fccksstern.", "tokens": ["Sind", "un\u00b7ter", "ei\u00b7nem", "Un\u00b7gl\u00fccks\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Vater, der wo sie erzeugt,", "tokens": ["Der", "Va\u00b7ter", ",", "der", "wo", "sie", "er\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bew\u00e4ltigt dieses gramgebeugt.", "tokens": ["Be\u00b7w\u00e4l\u00b7tigt", "die\u00b7ses", "gram\u00b7ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die Mutter, wo sie still gebiert,", "tokens": ["Die", "Mut\u00b7ter", ",", "wo", "sie", "still", "ge\u00b7biert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ist auch nicht heiter disponiert.", "tokens": ["Ist", "auch", "nicht", "hei\u00b7ter", "dis\u00b7po\u00b7niert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und \u00fcberhaupts, die Bayern sind", "tokens": ["Und", "\u00fc\u00b7ber\u00b7haupts", ",", "die", "Bay\u00b7ern", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Verlassen wie ein Waisenkind.", "tokens": ["Ver\u00b7las\u00b7sen", "wie", "ein", "Wai\u00b7sen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sie fragen sich und seufzen schwer:", "tokens": ["Sie", "fra\u00b7gen", "sich", "und", "seuf\u00b7zen", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Bleibt denn der Thron noch immer leer?", "tokens": ["Bleibt", "denn", "der", "Thron", "noch", "im\u00b7mer", "leer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das dr\u00fcckt den Herrn Kommerzjenrat.", "tokens": ["Das", "dr\u00fcckt", "den", "Herrn", "Kom\u00b7merz\u00b7jen\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Das sind die Sorgen, die er hat.", "tokens": ["Das", "sind", "die", "Sor\u00b7gen", ",", "die", "er", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da tritt er nun entschlossen vor", "tokens": ["Da", "tritt", "er", "nun", "ent\u00b7schlos\u00b7sen", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und ruft zur Residenz empor:", "tokens": ["Und", "ruft", "zur", "Re\u00b7si\u00b7denz", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die K\u00f6nigstreu' im Bayernland", "tokens": ["Die", "K\u00f6\u00b7nigs\u00b7treu'", "im", "Bay\u00b7ern\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Braucht endlich einen Gegenstand!", "tokens": ["Braucht", "end\u00b7lich", "ei\u00b7nen", "Ge\u00b7gen\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}