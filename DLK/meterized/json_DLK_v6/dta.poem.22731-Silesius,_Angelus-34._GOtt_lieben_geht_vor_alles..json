{"dta.poem.22731": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "34. GOtt lieben geht vor alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df einen alle Lust der gantzen Welt geniessen/", "tokens": ["La\u00df", "ei\u00b7nen", "al\u00b7le", "Lust", "der", "gant\u00b7zen", "Welt", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Un\u0303 einen dreymal mehr als ", "tokens": ["U\u00f1", "ei\u00b7nen", "drey\u00b7mal", "mehr", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADV", "PIAT", "KOKOM"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "La\u00df einen Sch\u00f6ner sein als ", "tokens": ["La\u00df", "ei\u00b7nen", "Sch\u00f6\u00b7ner", "sein", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VAINF", "KOKOM"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Gieb einen der mehr St\u00e4rk\u2019 und Macht hat als", "tokens": ["Gieb", "ei\u00b7nen", "der", "mehr", "St\u00e4rk'", "und", "Macht", "hat", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ART", "PIAT", "NN", "KON", "NN", "VAFIN", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Simson;", "tokens": ["Sim\u00b7son", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und einen der mehr Gold als ", "tokens": ["Und", "ei\u00b7nen", "der", "mehr", "Gold", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "PIAT", "NN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und noch der alles kan wie ", "tokens": ["Und", "noch", "der", "al\u00b7les", "kan", "wie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "PIS", "VMFIN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja der di\u00df alles ist: So sag ich doch gantz frey:", "tokens": ["Ja", "der", "di\u00df", "al\u00b7les", "ist", ":", "So", "sag", "ich", "doch", "gantz", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "PDS", "PIS", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df auch ein schlechter Mann der Gott liebt besser sey.", "tokens": ["Da\u00df", "auch", "ein", "schlech\u00b7ter", "Mann", "der", "Gott", "liebt", "bes\u00b7ser", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}