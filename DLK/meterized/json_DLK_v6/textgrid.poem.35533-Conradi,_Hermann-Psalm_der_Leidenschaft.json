{"textgrid.poem.35533": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Psalm der Leidenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie du mich lange, lange verlassen hattest,", "tokens": ["Wie", "du", "mich", "lan\u00b7ge", ",", "lan\u00b7ge", "ver\u00b7las\u00b7sen", "hat\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "$,", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Meiner Phantasie und meiner Kraft gewaltige Tochter,", "tokens": ["Mei\u00b7ner", "Phan\u00b7ta\u00b7sie", "und", "mei\u00b7ner", "Kraft", "ge\u00b7wal\u00b7ti\u00b7ge", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Leidenschaft! \u2013", "tokens": ["Lei\u00b7den\u00b7schaft", "!", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Die du von mir gewichen warest und von mir geflohen", "tokens": ["Die", "du", "von", "mir", "ge\u00b7wi\u00b7chen", "wa\u00b7rest", "und", "von", "mir", "ge\u00b7flo\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "KON", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "In D\u00e4mmertiefen und Nebelgr\u00fcnde \u2013", "tokens": ["In", "D\u00e4m\u00b7mer\u00b7tie\u00b7fen", "und", "Ne\u00b7bel\u00b7gr\u00fcn\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die du mich hattest verdorren lassen und kl\u00e4glich verk\u00fcmmern \u2013", "tokens": ["Die", "du", "mich", "hat\u00b7test", "ver\u00b7dor\u00b7ren", "las\u00b7sen", "und", "kl\u00e4g\u00b7lich", "ver\u00b7k\u00fcm\u00b7mern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "VVINF", "VVINF", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wiederum nun nach langem Entbehren", "tokens": ["Wie\u00b7de\u00b7rum", "nun", "nach", "lan\u00b7gem", "Ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "+----+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Feire ich in st\u00fcrmisch klopfender Brust", "tokens": ["Fei\u00b7re", "ich", "in", "st\u00fcr\u00b7misch", "klop\u00b7fen\u00b7der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Deine dithyrambische Einkehr!", "tokens": ["Dei\u00b7ne", "dit\u00b7hy\u00b7ram\u00b7bi\u00b7sche", "Ein\u00b7kehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Leidenschaft! Voll bin ich deiner und deiner trunken \u2013", "tokens": ["Lei\u00b7den\u00b7schaft", "!", "Voll", "bin", "ich", "dei\u00b7ner", "und", "dei\u00b7ner", "trun\u00b7ken", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VAFIN", "PPER", "PPOSAT", "KON", "PPOSAT", "ADJD", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Alles Verstaubte tief, tief versunken \u2013", "tokens": ["Al\u00b7les", "Ver\u00b7staub\u00b7te", "tief", ",", "tief", "ver\u00b7sun\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,", "ADJD", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Alle Gew\u00f6hnlichkeit glitt dahin \u2013", "tokens": ["Al\u00b7le", "Ge\u00b7w\u00f6hn\u00b7lich\u00b7keit", "glitt", "da\u00b7hin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PAV", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Begeisterung! Ich schl\u00fcrfe dich liebegl\u00fchend, Erl\u00f6serin!", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "!", "Ich", "schl\u00fcr\u00b7fe", "dich", "lie\u00b7be\u00b7gl\u00fc\u00b7hend", ",", "Er\u00b7l\u00f6\u00b7se\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+--+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Voll bin ich deiner! In starker Erregung \u2013", "tokens": ["Voll", "bin", "ich", "dei\u00b7ner", "!", "In", "star\u00b7ker", "Er\u00b7re\u00b7gung", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPOSAT", "$.", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+----", "measure": "dactylic.tri.plus"}, "line.2": {"text": "In gl\u00fcher Bewegung", "tokens": ["In", "gl\u00fc\u00b7her", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ist all mein Sein!", "tokens": ["Ist", "all", "mein", "Sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Leidenschaft, ich bin dein!", "tokens": ["Lei\u00b7den\u00b7schaft", ",", "ich", "bin", "dein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Und hat mich erw\u00e4hlt \u2013", "tokens": ["Und", "hat", "mich", "er\u00b7w\u00e4hlt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und eine Beute von s\u00fc\u00dfen, k\u00f6stlichen Siegen", "tokens": ["Und", "ei\u00b7ne", "Beu\u00b7te", "von", "s\u00fc\u00b7\u00dfen", ",", "k\u00f6st\u00b7li\u00b7chen", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hab' ich ihr wieder und wieder erz\u00e4hlt,", "tokens": ["Hab'", "ich", "ihr", "wie\u00b7der", "und", "wie\u00b7der", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wie sie all mein Suchen und all mein Sinnen", "tokens": ["Wie", "sie", "all", "mein", "Su\u00b7chen", "und", "all", "mein", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Einzig begreift \u2013", "tokens": ["Ein\u00b7zig", "be\u00b7greift", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ob Tage, ob Wochen, ob Monde verrinnen \u2013", "tokens": ["Ob", "Ta\u00b7ge", ",", "ob", "Wo\u00b7chen", ",", "ob", "Mon\u00b7de", "ver\u00b7rin\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NE", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Meine Liebe bleibt und reift ...", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "bleibt", "und", "reift", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leidenschaft! Du erf\u00fcllst mich so ganz!", "tokens": ["Lei\u00b7den\u00b7schaft", "!", "Du", "er\u00b7f\u00fcllst", "mich", "so", "ganz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit magischem Schein \u00fcberstr\u00f6mt mich dein Glanz,", "tokens": ["Mit", "ma\u00b7gi\u00b7schem", "Schein", "\u00fc\u00b7ber\u00b7str\u00f6mt", "mich", "dein", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In deinen Wirbeln so ganz verloren", "tokens": ["In", "dei\u00b7nen", "Wir\u00b7beln", "so", "ganz", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ward ich wiedergeboren!", "tokens": ["Ward", "ich", "wie\u00b7der\u00b7ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Wie so anders nun leuchtet mir Leben und Welt!", "tokens": ["Wie", "so", "an\u00b7ders", "nun", "leuch\u00b7tet", "mir", "Le\u00b7ben", "und", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wie so heimisch nun ward mir mein irdisch Gezelt!", "tokens": ["Wie", "so", "hei\u00b7misch", "nun", "ward", "mir", "mein", "ir\u00b7disch", "Ge\u00b7zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Stunden oft rast' ich und rege mich kaum ...", "tokens": ["Stun\u00b7den", "oft", "rast'", "ich", "und", "re\u00b7ge", "mich", "kaum", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und mich erf\u00fcllt namenlos gl\u00fccklicher Traum \u2013", "tokens": ["Und", "mich", "er\u00b7f\u00fcllt", "na\u00b7men\u00b7los", "gl\u00fcck\u00b7li\u00b7cher", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Alles gew\u00e4hrend, Leidenschaft, bist du genaht!", "tokens": ["Al\u00b7les", "ge\u00b7w\u00e4h\u00b7rend", ",", "Lei\u00b7den\u00b7schaft", ",", "bist", "du", "ge\u00b7naht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$,", "NN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Siehe, ein Trunkener wandelt der Liebe rosentriefenden Sonnenpfad!", "tokens": ["Sie\u00b7he", ",", "ein", "Trun\u00b7ke\u00b7ner", "wan\u00b7delt", "der", "Lie\u00b7be", "ro\u00b7sen\u00b7trie\u00b7fen\u00b7den", "Son\u00b7nen\u00b7pfad", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Die du mich lange, lange verlassen hattest, Leidenschaft \u2013", "tokens": ["Die", "du", "mich", "lan\u00b7ge", ",", "lan\u00b7ge", "ver\u00b7las\u00b7sen", "hat\u00b7test", ",", "Lei\u00b7den\u00b7schaft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "$,", "ADV", "VVPP", "VAFIN", "$,", "NN", "$("], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Neu haftet's in mir von deinen Geschossen!", "tokens": ["Neu", "haf\u00b7tet's", "in", "mir", "von", "dei\u00b7nen", "Ge\u00b7schos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Neu dampft meiner Seele geb\u00e4rende Kraft \u2013", "tokens": ["Neu", "dampft", "mei\u00b7ner", "See\u00b7le", "ge\u00b7b\u00e4\u00b7ren\u00b7de", "Kraft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Und trunken ward ich von all dem hei\u00dfen Genie\u00dfen,", "tokens": ["Und", "trun\u00b7ken", "ward", "ich", "von", "all", "dem", "hei\u00b7\u00dfen", "Ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rosen nur seh' ich, nur Rosen sprie\u00dfen", "tokens": ["Ro\u00b7sen", "nur", "seh'", "ich", ",", "nur", "Ro\u00b7sen", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "ADV", "NN", "VVINF"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Je und je \u2013", "tokens": ["Je", "und", "je", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Berauschende D\u00fcfte wehen mir zu die Winde,", "tokens": ["Be\u00b7rau\u00b7schen\u00b7de", "D\u00fcf\u00b7te", "we\u00b7hen", "mir", "zu", "die", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und mir seligem Kinde", "tokens": ["Und", "mir", "se\u00b7li\u00b7gem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Schweigt das Weh ...", "tokens": ["Schweigt", "das", "Weh", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Sturm nur erf\u00fcllt mich und k\u00fchneres Wollen \u2013", "tokens": ["Sturm", "nur", "er\u00b7f\u00fcllt", "mich", "und", "k\u00fch\u00b7ne\u00b7res", "Wol\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "KON", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ob Tage, ob Wochen, ob Monde verrollen,", "tokens": ["Ob", "Ta\u00b7ge", ",", "ob", "Wo\u00b7chen", ",", "ob", "Mon\u00b7de", "ver\u00b7rol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NE", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was k\u00fcmmert's mich!", "tokens": ["Was", "k\u00fcm\u00b7mert's", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "La\u00df sie in bl\u00f6dem Plunder verfliegen \u2013", "tokens": ["La\u00df", "sie", "in", "bl\u00f6\u00b7dem", "Plun\u00b7der", "ver\u00b7flie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die du zu mir herniedergestiegen,", "tokens": ["Die", "du", "zu", "mir", "her\u00b7nie\u00b7der\u00b7ges\u00b7tie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Leidenschaft! Liebe! ich halte dich!", "tokens": ["Lei\u00b7den\u00b7schaft", "!", "Lie\u00b7be", "!", "ich", "hal\u00b7te", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Halte dich \u2013 ob auch nur eine Stunde \u2013:", "tokens": ["Hal\u00b7te", "dich", "\u2013", "ob", "auch", "nur", "ei\u00b7ne", "Stun\u00b7de", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "ADV", "ADV", "ART", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Saat gedeiht!", "tokens": ["Dei\u00b7ne", "Saat", "ge\u00b7deiht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Geheimnisvoll mit dir im Bunde", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "mit", "dir", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueberwind' ich die zehrende Zeit ...", "tokens": ["Ue\u00b7ber\u00b7wind'", "ich", "die", "zeh\u00b7ren\u00b7de", "Zeit", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}