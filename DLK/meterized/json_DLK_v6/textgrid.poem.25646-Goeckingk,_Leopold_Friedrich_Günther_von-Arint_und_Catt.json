{"textgrid.poem.25646": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Arint und Catt", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolz ist es, da\u00df Arint uns seinen Stammbaum zeigt,", "tokens": ["Stolz", "ist", "es", ",", "da\u00df", "A\u00b7rint", "uns", "sei\u00b7nen", "Stamm\u00b7baum", "zeigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "NE", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sechszehn Ahnen hat;", "tokens": ["Der", "sechs\u00b7zehn", "Ah\u00b7nen", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch stolzer ist der edle Catt,", "tokens": ["Noch", "stol\u00b7zer", "ist", "der", "ed\u00b7le", "Catt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der zwanzig z\u00e4hlt und davon schweigt.", "tokens": ["Der", "zwan\u00b7zig", "z\u00e4hlt", "und", "da\u00b7von", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VVFIN", "KON", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}