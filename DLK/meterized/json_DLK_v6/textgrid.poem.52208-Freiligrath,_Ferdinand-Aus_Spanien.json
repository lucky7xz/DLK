{"textgrid.poem.52208": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Aus Spanien", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Platz ist leer, das Volk hat sich verlaufen,", "tokens": ["Der", "Platz", "ist", "leer", ",", "das", "Volk", "hat", "sich", "ver\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dampf verflog, die Sch\u00fcsse sind verhallt;", "tokens": ["Der", "Dampf", "ver\u00b7flog", ",", "die", "Sch\u00fcs\u00b7se", "sind", "ver\u00b7hallt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur hier und dort steht einsam noch ein Haufen,", "tokens": ["Nur", "hier", "und", "dort", "steht", "ein\u00b7sam", "noch", "ein", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Auge Zorn, die H\u00e4nde starr geballt;", "tokens": ["Im", "Au\u00b7ge", "Zorn", ",", "die", "H\u00e4n\u00b7de", "starr", "ge\u00b7ballt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Husaren ziehn; \u2013 ein Tag der Schmach war euer!", "tokens": ["Hu\u00b7sa\u00b7ren", "ziehn", ";", "\u2013", "ein", "Tag", "der", "Schmach", "war", "eu\u00b7er", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "$(", "ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ihr go\u00dft das Blei, das seine", "tokens": ["Ihr", "go\u00dft", "das", "Blei", ",", "das", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr scho\u00dft es ab! Euch galt sein Wort: \u00bbGebt Feuer!", "tokens": ["Ihr", "scho\u00dft", "es", "ab", "!", "Euch", "galt", "sein", "Wort", ":", "\u00bb", "Gebt", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$(", "VVIMP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": ". . . . ", "tokens": [".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$."]}}, "stanza.2": {"line.1": {"text": "\u00bbgebt Feuer!\u00ab \u2013 ja, das hat er oft gesprochen,", "tokens": ["\u00bb", "gebt", "Feu\u00b7er", "!", "\u00ab", "\u2013", "ja", ",", "das", "hat", "er", "oft", "ge\u00b7spro\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$.", "$(", "$(", "PTKANT", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er zu Ro\u00df durch eure Reihen flog;", "tokens": ["Wenn", "er", "zu", "Ro\u00df", "durch", "eu\u00b7re", "Rei\u00b7hen", "flog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn zu der Hufe ungeduld'gem Pochen", "tokens": ["Wenn", "zu", "der", "Hu\u00b7fe", "un\u00b7ge\u00b7duld'\u00b7gem", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er nun sein Schwert, das makellose, zog!", "tokens": ["Er", "nun", "sein", "Schwert", ",", "das", "ma\u00b7kel\u00b7lo\u00b7se", ",", "zog", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr Spaniens Heil, f\u00fcr eurer Waffen Ehre,", "tokens": ["F\u00fcr", "Spa\u00b7ni\u00b7ens", "Heil", ",", "f\u00fcr", "eu\u00b7rer", "Waf\u00b7fen", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Wie hat er stets zu f\u00fchren euch gewu\u00dft!", "tokens": ["Wie", "hat", "er", "stets", "zu", "f\u00fch\u00b7ren", "euch", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Heut lenkt' er wieder Feuerr\u00f6hre,", "tokens": ["Heut", "lenkt'", "er", "wie\u00b7der", "Feu\u00b7er\u00b7r\u00f6h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u2013 O Gott, auf seine eigne Brust!", "tokens": ["\u2013", "O", "Gott", ",", "auf", "sei\u00b7ne", "eig\u00b7ne", "Brust", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wer verdammt ihn? \u2013 Er, der jetzt das Ruder", "tokens": ["Und", "wer", "ver\u00b7dammt", "ihn", "?", "\u2013", "Er", ",", "der", "jetzt", "das", "Ru\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "$(", "PPER", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des morschen Staats in ehrnen H\u00e4nden h\u00e4lt!", "tokens": ["Des", "mor\u00b7schen", "Staats", "in", "ehr\u00b7nen", "H\u00e4n\u00b7den", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Waffenbruder seinen Waffenbruder!", "tokens": ["Der", "Waf\u00b7fen\u00b7bru\u00b7der", "sei\u00b7nen", "Waf\u00b7fen\u00b7bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht wahr \u2013 sie schliefen in demselben Zelt?", "tokens": ["Nicht", "wahr", "\u2013", "sie", "schlie\u00b7fen", "in", "dem\u00b7sel\u00b7ben", "Zelt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$(", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr saht sie rasten oft in einer Scheuer?", "tokens": ["Ihr", "saht", "sie", "ras\u00b7ten", "oft", "in", "ei\u00b7ner", "Scheu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus einem Becher tranken sie? \u2013 Gewi\u00df!", "tokens": ["Aus", "ei\u00b7nem", "Be\u00b7cher", "tran\u00b7ken", "sie", "?", "\u2013", "Ge\u00b7wi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr saht es oft! \u2013 O Gott, und heute? \u2013 \u00bbFeuer!", "tokens": ["Ihr", "saht", "es", "oft", "!", "\u2013", "O", "Gott", ",", "und", "heu\u00b7te", "?", "\u2013", "\u00bb", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "NE", "NN", "$,", "KON", "ADV", "$.", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": ". . . . ", "tokens": [".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$."]}}, "stanza.4": {"line.1": {"text": "Ja, la\u00dft mich steigen auf mein liebstes Pferd!", "tokens": ["Ja", ",", "la\u00dft", "mich", "stei\u00b7gen", "auf", "mein", "liebs\u00b7tes", "Pferd", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch einmal gern s\u00e4h' ich mein Schwert erblitzen,", "tokens": ["Noch", "ein\u00b7mal", "gern", "s\u00e4h'", "ich", "mein", "Schwert", "er\u00b7blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie es Reitern aus der Scheide f\u00e4hrt!", "tokens": ["So", "wie", "es", "Rei\u00b7tern", "aus", "der", "Schei\u00b7de", "f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den ich im Kampf erblickt auf tausend Seiten,", "tokens": ["Den", "ich", "im", "Kampf", "er\u00b7blickt", "auf", "tau\u00b7send", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem ich seit Jahren dreist die Stirne bot,", "tokens": ["Dem", "ich", "seit", "Jah\u00b7ren", "dreist", "die", "Stir\u00b7ne", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auch jetzt dem Tod m\u00f6cht' ich entgegen", "tokens": ["Auch", "jetzt", "dem", "Tod", "m\u00f6cht'", "ich", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VMFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gern st\u00fcrb' ich einen Reiterstod!\u00ab", "tokens": ["Gern", "st\u00fcrb'", "ich", "ei\u00b7nen", "Rei\u00b7ter\u00b7stod", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er starb ihn nicht \u2013 er ward hinaus", "tokens": ["Er", "starb", "ihn", "nicht", "\u2013", "er", "ward", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "PPER", "VAFIN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesenkten Halses blieb daheim sein Ro\u00df;", "tokens": ["Ge\u00b7senk\u00b7ten", "Hal\u00b7ses", "blieb", "da\u00b7heim", "sein", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dicht lag der Staub auf seinen M\u00e4hnenhaaren,", "tokens": ["Dicht", "lag", "der", "Staub", "auf", "sei\u00b7nen", "M\u00e4h\u00b7nen\u00b7haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indes man drau\u00dfen seinen Herrn erscho\u00df!", "tokens": ["In\u00b7des", "man", "drau\u00b7\u00dfen", "sei\u00b7nen", "Herrn", "er\u00b7scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Einf\u00f6rm'gen Hufschlags trat es sein Gem\u00e4uer \u2013", "tokens": ["Ein\u00b7f\u00f6r\u00b7m'\u00b7gen", "Huf\u00b7schlags", "trat", "es", "sein", "Ge\u00b7m\u00e4u\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ha, lieber wahrlich knirscht' es ins Gebi\u00df", "tokens": ["Ha", ",", "lie\u00b7ber", "wahr\u00b7lich", "knirscht'", "es", "ins", "Ge\u00b7bi\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und stampfte wiehernd in den Zuruf: \u2013 \u00bbFeuer!", "tokens": ["Und", "stampf\u00b7te", "wie\u00b7hernd", "in", "den", "Zu\u00b7ruf", ":", "\u2013", "\u00bb", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "ART", "NN", "$.", "$(", "$(", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": ".... ", "tokens": ["...."], "token_info": ["punct"], "pos": ["$."]}}, "stanza.6": {"line.1": {"text": "Schlank, hoch und herrlich trat er aus dem Wagen;", "tokens": ["Schlank", ",", "hoch", "und", "herr\u00b7lich", "trat", "er", "aus", "dem", "Wa\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann k\u00fc\u00dft' er br\u00fcnstig ein Marienblid.", "tokens": ["Dann", "k\u00fc\u00dft'", "er", "br\u00fcns\u00b7tig", "ein", "Ma\u00b7ri\u00b7en\u00b7blid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbin allen Schlachten hab' ich dich getragen:", "tokens": ["\u00bb", "in", "al\u00b7len", "Schlach\u00b7ten", "hab'", "ich", "dich", "ge\u00b7tra\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was du vermochtest, hast du treu erf\u00fcllt!", "tokens": ["Was", "du", "ver\u00b7moch\u00b7test", ",", "hast", "du", "treu", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die dich mir gab, mein Weib hat dich gesegnet;", "tokens": ["Die", "dich", "mir", "gab", ",", "mein", "Weib", "hat", "dich", "ge\u00b7seg\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geh zu ihr heim \u2013 getan ist deine Pflicht!", "tokens": ["Geh", "zu", "ihr", "heim", "\u2013", "ge\u00b7tan", "ist", "dei\u00b7ne", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "$(", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du lenkst die Kugeln, so die Walstatt regnet,", "tokens": ["Du", "lenkst", "die", "Ku\u00b7geln", ",", "so", "die", "Wals\u00b7tatt", "reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Richtstatt Kugeln lenkst du nicht!\u00ab \u2013", "tokens": ["Der", "Richt\u00b7statt", "Ku\u00b7geln", "lenkst", "du", "nicht", "!", "\u00ab", "\u2013"], "token_