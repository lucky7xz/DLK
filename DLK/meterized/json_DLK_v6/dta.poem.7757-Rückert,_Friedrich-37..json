{"dta.poem.7757": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hier die Nachbarn hat, die stets mit ihm zufrieden", "tokens": ["Wer", "hier", "die", "Nach\u00b7barn", "hat", ",", "die", "stets", "mit", "ihm", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "APPR", "PPER", "ADJD"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gewesen sind, dem ist ein Platz bei Gott beschieden.", "tokens": ["Ge\u00b7we\u00b7sen", "sind", ",", "dem", "ist", "ein", "Platz", "bei", "Gott", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer hier nicht Frieden kann mit seinen Nachbarn halten,", "tokens": ["Wer", "hier", "nicht", "Frie\u00b7den", "kann", "mit", "sei\u00b7nen", "Nach\u00b7barn", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den nimmt man dort nicht auf, wo ew'ge Frieden walten.", "tokens": ["Den", "nimmt", "man", "dort", "nicht", "auf", ",", "wo", "ew'\u00b7ge", "Frie\u00b7den", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "PTKNEG", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer Zwietracht zwischen dir und deinem Nachbar stiftet,", "tokens": ["Wer", "Zwiet\u00b7racht", "zwi\u00b7schen", "dir", "und", "dei\u00b7nem", "Nach\u00b7bar", "stif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat zwischen euch den Brunn, den beid' ihr trinkt, vergiftet.", "tokens": ["Hat", "zwi\u00b7schen", "euch", "den", "Brunn", ",", "den", "beid'", "ihr", "trinkt", ",", "ver\u00b7gif\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer dich nicht kr\u00e4nkt, ist drum kein guter Nachbar noch;", "tokens": ["Wer", "dich", "nicht", "kr\u00e4nkt", ",", "ist", "drum", "kein", "gu\u00b7ter", "Nach\u00b7bar", "noch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VAFIN", "PAV", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist es, der, von dir gekr\u00e4nkt, es bleibet doch.", "tokens": ["Der", "ist", "es", ",", "der", ",", "von", "dir", "ge\u00b7kr\u00e4nkt", ",", "es", "blei\u00b7bet", "doch", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "$,", "APPR", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Ueberlieferung sagt: Wer sinnet aufs Verderben", "tokens": ["Die", "Ue\u00b7berl\u00b7ie\u00b7fe\u00b7rung", "sagt", ":", "Wer", "sin\u00b7net", "aufs", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Nachbars, dessen Haus l\u00e4\u00dft Gott den Nachbar erben.", "tokens": ["Des", "Nach\u00b7bars", ",", "des\u00b7sen", "Haus", "l\u00e4\u00dft", "Gott", "den", "Nach\u00b7bar", "er\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "VVFIN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es hei\u00dft auch im Gebet: Bewahr' uns Gott in Gnaden", "tokens": ["Es", "hei\u00dft", "auch", "im", "Ge\u00b7bet", ":", "Be\u00b7wahr'", "uns", "Gott", "in", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$.", "NN", "PPER", "NN", "APPR", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor Nachbars Aug' und Ohr an Thor und Fensterladen.", "tokens": ["Vor", "Nach\u00b7bars", "Aug'", "und", "Ohr", "an", "Thor", "und", "Fens\u00b7ter\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Er sieht dir durch die Wand bis in des Hauses Mitte,", "tokens": ["Er", "sieht", "dir", "durch", "die", "Wand", "bis", "in", "des", "Hau\u00b7ses", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus- und eingehn sieht er deine Tritt' und Schritte.", "tokens": ["Und", "aus", "und", "ein\u00b7gehn", "sieht", "er", "dei\u00b7ne", "Tritt'", "und", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das Gute das er sieht, das macht das Herz ihm wund,", "tokens": ["Das", "Gu\u00b7te", "das", "er", "sieht", ",", "das", "macht", "das", "Herz", "ihm", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was er B\u00f6ses sieht, macht er den Leuten kund.", "tokens": ["Und", "was", "er", "B\u00f6\u00b7ses", "sieht", ",", "macht", "er", "den", "Leu\u00b7ten", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NE", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ein leider Nachbar ist ein Leid, dem du nie fliehst,", "tokens": ["Ein", "lei\u00b7der", "Nach\u00b7bar", "ist", "ein", "Leid", ",", "dem", "du", "nie", "fliehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das leider jeden Tag du durch dein Fenster siehst.", "tokens": ["Das", "lei\u00b7der", "je\u00b7den", "Tag", "du", "durch", "dein", "Fens\u00b7ter", "siehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Was hilft es, magst du Kraut in deinem Garten baun,", "tokens": ["Was", "hilft", "es", ",", "magst", "du", "Kraut", "in", "dei\u00b7nem", "Gar\u00b7ten", "baun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dir der Nachbar wirft sein Unkraut \u00fcbern Zaun.", "tokens": ["Wenn", "dir", "der", "Nach\u00b7bar", "wirft", "sein", "Un\u00b7kraut", "\u00fc\u00b7bern", "Zaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Warum verkaufest du dein Haus? fragt man den Mann.", "tokens": ["Wa\u00b7rum", "ver\u00b7kau\u00b7fest", "du", "dein", "Haus", "?", "fragt", "man", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich den Nachbar nicht, sprach er, verkaufen kann.", "tokens": ["Weil", "ich", "den", "Nach\u00b7bar", "nicht", ",", "sprach", "er", ",", "ver\u00b7kau\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "$,", "VVFIN", "PPER", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}