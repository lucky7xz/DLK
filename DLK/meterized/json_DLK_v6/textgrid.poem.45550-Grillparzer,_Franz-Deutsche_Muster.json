{"textgrid.poem.45550": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Muster", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sollte von euch lernen?", "tokens": ["Ich", "soll\u00b7te", "von", "euch", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da bin ich weit entfernt;", "tokens": ["Da", "bin", "ich", "weit", "ent\u00b7fernt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh lieber zu den Fernen,", "tokens": ["Geh", "lie\u00b7ber", "zu", "den", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von denen ihr gelernt.", "tokens": ["Von", "de\u00b7nen", "ihr", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}