{"textgrid.poem.67096": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Baum", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "... und die ewigen Bahnen", "tokens": ["...", "und", "die", "e\u00b7wi\u00b7gen", "Bah\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "L\u00e4chelnd \u00fcber uns hin z\u00f6gen die Herrscher der Welt,", "tokens": ["L\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "uns", "hin", "z\u00f6\u00b7gen", "die", "Herr\u00b7scher", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sonne und Mond und Sterne, und auch die Blitze der Wolken", "tokens": ["Son\u00b7ne", "und", "Mond", "und", "Ster\u00b7ne", ",", "und", "auch", "die", "Blit\u00b7ze", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+-+----+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Spielten, des Augenblicks feurige Kinder, um uns,", "tokens": ["Spiel\u00b7ten", ",", "des", "Au\u00b7gen\u00b7blicks", "feu\u00b7ri\u00b7ge", "Kin\u00b7der", ",", "um", "uns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJA", "NN", "$,", "KOUI", "PPER", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Aber in unsrem Innern, ein Bild der F\u00fcrsten des Himmels,", "tokens": ["A\u00b7ber", "in", "uns\u00b7rem", "In\u00b7nern", ",", "ein", "Bild", "der", "F\u00fcrs\u00b7ten", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wandelte neidlos der Gott unserer Liebe dahin,", "tokens": ["Wan\u00b7del\u00b7te", "neid\u00b7los", "der", "Gott", "un\u00b7se\u00b7rer", "Lie\u00b7be", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "PAV", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und er mischte den Duft, die reine, heilige Seele,", "tokens": ["Und", "er", "mischte", "den", "Duft", ",", "die", "rei\u00b7ne", ",", "hei\u00b7li\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Die, von des Fr\u00fchlinges silberner Stunde gen\u00e4hrt,", "tokens": ["Die", ",", "von", "des", "Fr\u00fch\u00b7lin\u00b7ges", "sil\u00b7ber\u00b7ner", "Stun\u00b7de", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-++-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Oft \u00fcberstr\u00f6mte, hinaus ins gl\u00e4nzende Meer des Tages,", "tokens": ["Oft", "\u00fc\u00b7ber\u00b7str\u00f6m\u00b7te", ",", "hin\u00b7aus", "ins", "gl\u00e4n\u00b7zen\u00b7de", "Meer", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und in das Abendrot und in die Wogen der Nacht,", "tokens": ["Und", "in", "das", "A\u00b7ben\u00b7drot", "und", "in", "die", "Wo\u00b7gen", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Ach! wir lebten so frei im innig unendlichen Leben,", "tokens": ["Ach", "!", "wir", "leb\u00b7ten", "so", "frei", "im", "in\u00b7nig", "un\u00b7end\u00b7li\u00b7chen", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "ADJD", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Unbek\u00fcmmert und still, selber ein seliger Traum,", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", "und", "still", ",", "sel\u00b7ber", "ein", "se\u00b7li\u00b7ger", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Jetzt uns selber genug und jetzt ins Weite verfliegend,", "tokens": ["Jetzt", "uns", "sel\u00b7ber", "ge\u00b7nug", "und", "jetzt", "ins", "Wei\u00b7te", "ver\u00b7flie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADV", "KON", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aber im Innersten doch immer lebendig und eins.", "tokens": ["A\u00b7ber", "im", "In\u00b7ners\u00b7ten", "doch", "im\u00b7mer", "le\u00b7ben\u00b7dig", "und", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADV", "ADJD", "KON", "PIS", "$."], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Gl\u00fccklicher Baum! wie lange, wie lange k\u00f6nnt ich noch singen", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "Baum", "!", "wie", "lan\u00b7ge", ",", "wie", "lan\u00b7ge", "k\u00f6nnt", "ich", "noch", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PWAV", "ADV", "$,", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und vergehen im Blick auf dein erbebendes Haupt,", "tokens": ["Und", "ver\u00b7ge\u00b7hen", "im", "Blick", "auf", "dein", "er\u00b7be\u00b7ben\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Aber siehe! dort regt sichs, es wandeln in Schleiern die Jungfraun", "tokens": ["A\u00b7ber", "sie\u00b7he", "!", "dort", "regt", "sichs", ",", "es", "wan\u00b7deln", "in", "Schlei\u00b7ern", "die", "Jung\u00b7fraun"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Und wer wei\u00df es, vielleicht w\u00e4re mein M\u00e4dchen dabei;", "tokens": ["Und", "wer", "wei\u00df", "es", ",", "viel\u00b7leicht", "w\u00e4\u00b7re", "mein", "M\u00e4d\u00b7chen", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "La\u00df mich, la\u00df mich, ich mu\u00df \u2013 lebwohl! es rei\u00dft mich ins Leben,", "tokens": ["La\u00df", "mich", ",", "la\u00df", "mich", ",", "ich", "mu\u00df", "\u2013", "leb\u00b7wohl", "!", "es", "rei\u00dft", "mich", "ins", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPER", "$,", "PPER", "VMFIN", "$(", "ADJD", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Da\u00df ich im kindischen Gang folge der lieblichen Spur,", "tokens": ["Da\u00df", "ich", "im", "kin\u00b7di\u00b7schen", "Gang", "fol\u00b7ge", "der", "lieb\u00b7li\u00b7chen", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Aber du Guter, dich will, dich will ich nimmer vergessen,", "tokens": ["A\u00b7ber", "du", "Gu\u00b7ter", ",", "dich", "will", ",", "dich", "will", "ich", "nim\u00b7mer", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Ewig bist du und bleibst meiner Geliebtesten Bild.", "tokens": ["E\u00b7wig", "bist", "du", "und", "bleibst", "mei\u00b7ner", "Ge\u00b7lieb\u00b7tes\u00b7ten", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Und k\u00e4m einmal ein Tag, wo sie die meinige w\u00e4re,", "tokens": ["Und", "k\u00e4m", "ein\u00b7mal", "ein", "Tag", ",", "wo", "sie", "die", "mei\u00b7ni\u00b7ge", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "ART", "PPOSS", "VAFIN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "O! dann ruht ich mit ihr, unter dir, Freundlicher, aus", "tokens": ["O", "!", "dann", "ruht", "ich", "mit", "ihr", ",", "un\u00b7ter", "dir", ",", "Freund\u00b7li\u00b7cher", ",", "aus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "NN", "$,", "APPR"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Und du z\u00fcrnetest nicht, du g\u00f6ssest Schatten und D\u00fcfte", "tokens": ["Und", "du", "z\u00fcr\u00b7ne\u00b7test", "nicht", ",", "du", "g\u00f6s\u00b7sest", "Schat\u00b7ten", "und", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und ein rauschendes Lied \u00fcber die Gl\u00fccklichen aus.", "tokens": ["Und", "ein", "rau\u00b7schen\u00b7des", "Lied", "\u00fc\u00b7ber", "die", "Gl\u00fcck\u00b7li\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}