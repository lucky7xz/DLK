{"dta.poem.10910": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seh ich hier die wilden K\u00e4uler, durch verworrne Hecken, dringen;", "tokens": ["Seh", "ich", "hier", "die", "wil\u00b7den", "K\u00e4u\u00b7ler", ",", "durch", "ver\u00b7worr\u00b7ne", "He\u00b7cken", ",", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seh ich sie in schneller Fahrt so ergrimmt als fl\u00fcchtig springen:", "tokens": ["Seh", "ich", "sie", "in", "schnel\u00b7ler", "Fahrt", "so", "er\u00b7grimmt", "als", "fl\u00fcch\u00b7tig", "sprin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Kann mein Blick kaum stille stehn, sondern eilt mit ihnen fort,", "tokens": ["Kann", "mein", "Blick", "kaum", "stil\u00b7le", "stehn", ",", "son\u00b7dern", "eilt", "mit", "ih\u00b7nen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADJA", "VVINF", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und mich deucht, ich h\u00f6r zugleich Grunzen, Rascheln, Schreyen,", "tokens": ["Und", "mich", "deucht", ",", "ich", "h\u00f6r", "zu\u00b7gleich", "Grun\u00b7zen", ",", "Ra\u00b7scheln", ",", "Schre\u00b7yen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Aber seh ich ungefehr die gezackten Fichten dort,", "tokens": ["A\u00b7ber", "seh", "ich", "un\u00b7ge\u00b7fehr", "die", "ge\u00b7zack\u00b7ten", "Fich\u00b7ten", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "In so wirklicher Gestalt: Stutz ich, und la\u00df jene laufen,", "tokens": ["In", "so", "wirk\u00b7li\u00b7cher", "Ge\u00b7stalt", ":", "Stutz", "ich", ",", "und", "la\u00df", "je\u00b7ne", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$.", "NN", "PPER", "$,", "KON", "VVIMP", "PDS", "VVINF", "$,"], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Dem, der alles schafft, zum Ruhm, seh ich hier, in Thier und", "tokens": ["Dem", ",", "der", "al\u00b7les", "schafft", ",", "zum", "Ruhm", ",", "seh", "ich", "hier", ",", "in", "Thier", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "KON"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Eine k\u00fcnstliche Copey der Natur, in zweyen Reichen.", "tokens": ["Ei\u00b7ne", "k\u00fcnst\u00b7li\u00b7che", "Co\u00b7pey", "der", "Na\u00b7tur", ",", "in", "zwe\u00b7yen", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "VVFIN", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}