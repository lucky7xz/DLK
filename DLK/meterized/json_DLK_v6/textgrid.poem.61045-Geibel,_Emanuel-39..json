{"textgrid.poem.61045": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl springet aus dem Kiesel", "tokens": ["Wohl", "sprin\u00b7get", "aus", "dem", "Kie\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Funk' in lichter Glut,", "tokens": ["Der", "Funk'", "in", "lich\u00b7ter", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl quillet aus der Traube", "tokens": ["Wohl", "quil\u00b7let", "aus", "der", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hei\u00dfe Rebenblut,", "tokens": ["Das", "hei\u00b7\u00dfe", "Re\u00b7ben\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch aus dem dunkeln Auge,", "tokens": ["Doch", "aus", "dem", "dun\u00b7keln", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem holden Auge dein,", "tokens": ["Dem", "hol\u00b7den", "Au\u00b7ge", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da quillet nichts als Liebe", "tokens": ["Da", "quil\u00b7let", "nichts", "als", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir tief ins Herz hinein.", "tokens": ["Mir", "tief", "ins", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seit du zum ersten Male", "tokens": ["Seit", "du", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich angesehen hast,", "tokens": ["Mich", "an\u00b7ge\u00b7se\u00b7hen", "hast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schw\u00e4rmen meine Gedanken", "tokens": ["Da", "schw\u00e4r\u00b7men", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und haben nicht Ruh' noch Rast;", "tokens": ["Und", "ha\u00b7ben", "nicht", "Ruh'", "noch", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie schw\u00e4rmen wie wilde V\u00f6gel", "tokens": ["Sie", "schw\u00e4r\u00b7men", "wie", "wil\u00b7de", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch Feld und Waldrevier", "tokens": ["Durch", "Feld", "und", "Wald\u00b7re\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber Busch und Wipfel", "tokens": ["Und", "\u00fc\u00b7ber", "Busch", "und", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein zu dir, zu dir.", "tokens": ["Al\u00b7lein", "zu", "dir", ",", "zu", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und w\u00fcrden die Berge zu Golde,", "tokens": ["Und", "w\u00fcr\u00b7den", "die", "Ber\u00b7ge", "zu", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und w\u00fcrde das Meer zu Wein:", "tokens": ["Und", "w\u00fcr\u00b7de", "das", "Meer", "zu", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So wollt' ich doch lieber, du Holde,", "tokens": ["So", "wollt'", "ich", "doch", "lie\u00b7ber", ",", "du", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "$,", "PPER", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du solltest mein eigen sein!", "tokens": ["Du", "soll\u00b7test", "mein", "ei\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}