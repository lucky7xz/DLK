{"dta.poem.14223": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied und Wiedersehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "In s\u00fc\u00dfen Spielen unter nun gegangen", "tokens": ["In", "s\u00fc\u00b7\u00dfen", "Spie\u00b7len", "un\u00b7ter", "nun", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Liebchens Augen, und sie athmet linde,", "tokens": ["Sind", "Lieb\u00b7chens", "Au\u00b7gen", ",", "und", "sie", "ath\u00b7met", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "$,", "KON", "PPER", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stilllauschend sitz' ich bei dem holden Kinde,", "tokens": ["Still\u00b7lau\u00b7schend", "sitz'", "ich", "bei", "dem", "hol\u00b7den", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Locken streichelnd ihr von Stirn und Wangen.", "tokens": ["Die", "Lo\u00b7cken", "strei\u00b7chelnd", "ihr", "von", "Stirn", "und", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}