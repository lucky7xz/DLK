{"textgrid.poem.41218": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Sohn! sprach Gertraud schwer von Jahren:", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn! sprach Gertraud schwer von Jahren:", "tokens": ["Mein", "Sohn", "!", "sprach", "Ger\u00b7traud", "schwer", "von", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "NE", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du zeuchst von mir.", "tokens": ["Du", "zeuchst", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf Reisen wirst du viel erfahren.", "tokens": ["Auf", "Rei\u00b7sen", "wirst", "du", "viel", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, merk' es dir!", "tokens": ["Ach", ",", "merk'", "es", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Lehren aus der Mutter Munde", "tokens": ["Die", "Leh\u00b7ren", "aus", "der", "Mut\u00b7ter", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schlag' nicht in Wind!", "tokens": ["Schlag'", "nicht", "in", "Wind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du bist (gesagt zur guten Stunde!)", "tokens": ["Du", "bist", "(", "ge\u00b7sagt", "zur", "gu\u00b7ten", "Stun\u00b7de", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$(", "VVPP", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Sonntagskind.", "tokens": ["Ein", "Sonn\u00b7tags\u00b7kind", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "F\u00fchrt einst um Mitternacht die Reise", "tokens": ["F\u00fchrt", "einst", "um", "Mit\u00b7ter\u00b7nacht", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich durch den Wald,", "tokens": ["Dich", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So kreuze dich, und horche leise,", "tokens": ["So", "kreu\u00b7ze", "dich", ",", "und", "hor\u00b7che", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenns Hifthorn schallt.", "tokens": ["Wenns", "Hif\u00b7thorn", "schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dann mu\u00dft du dich zur Erde werfen.", "tokens": ["Dann", "mu\u00dft", "du", "dich", "zur", "Er\u00b7de", "wer\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur nicht verzagt!", "tokens": ["Nur", "nicht", "ver\u00b7zagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So wird sie dir nicht schaden d\u00e4rfen", "tokens": ["So", "wird", "sie", "dir", "nicht", "scha\u00b7den", "d\u00e4r\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die wilde Jagd.", "tokens": ["Die", "wil\u00b7de", "Jagd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der B\u00f6se treibt bei manchen Fl\u00fcssen", "tokens": ["Der", "B\u00f6\u00b7se", "treibt", "bei", "man\u00b7chen", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch oft sein Spiel!", "tokens": ["Auch", "oft", "sein", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drum wirst du dich h\u00fcbsch kreuzen m\u00fcssen,", "tokens": ["Drum", "wirst", "du", "dich", "h\u00fcbsch", "kreu\u00b7zen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie wirds zu viel.", "tokens": ["Nie", "wirds", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKA", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er schreit (so sagt mir meine Baase.", "tokens": ["Er", "schreit", "(", "so", "sagt", "mir", "mei\u00b7ne", "Baa\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was willst du mehr?)", "tokens": ["Was", "willst", "du", "mehr", "?", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er schreit gewaltig durch die Nase", "tokens": ["Er", "schreit", "ge\u00b7wal\u00b7tig", "durch", "die", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Ufer her.", "tokens": ["Am", "U\u00b7fer", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Oft f\u00e4hrt er dir auf off'nen Strassen", "tokens": ["Oft", "f\u00e4hrt", "er", "dir", "auf", "off'\u00b7nen", "Stras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Post, mein Sohn!", "tokens": ["Die", "Post", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du mu\u00dft ihn immer fahren lassen.", "tokens": ["Du", "mu\u00dft", "ihn", "im\u00b7mer", "fah\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur weit davon!", "tokens": ["Nur", "weit", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Man h\u00f6rt der Peitsche leibhaft Knallen,", "tokens": ["Man", "h\u00f6rt", "der", "Peit\u00b7sche", "leib\u00b7haft", "Knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Posthorn's Ton.", "tokens": ["Des", "Post\u00b7horn's", "Ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wer frevelt, mu\u00df in S\u00fcmpfe fallen.", "tokens": ["Wer", "fre\u00b7velt", ",", "mu\u00df", "in", "S\u00fcmp\u00b7fe", "fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du nicht mein Sohn!", "tokens": ["Du", "nicht", "mein", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sollst du bei Nacht durch Felder reisen,", "tokens": ["Sollst", "du", "bei", "Nacht", "durch", "Fel\u00b7der", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm dich in Acht!", "tokens": ["Nimm", "dich", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da pflegt ein Licht den Weg zu weisen,", "tokens": ["Da", "pflegt", "ein", "Licht", "den", "Weg", "zu", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das irre macht.", "tokens": ["Das", "ir\u00b7re", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Oft tanzen sie in bunter Menge", "tokens": ["Oft", "tan\u00b7zen", "sie", "in", "bun\u00b7ter", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ums Hochgericht.", "tokens": ["Ums", "Hoch\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Entferne dich von dem Gepr\u00e4nge,", "tokens": ["Ent\u00b7fer\u00b7ne", "dich", "von", "dem", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und frevle nicht!", "tokens": ["Und", "frev\u00b7le", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Auf Aeckern wird dir oft begegnen", "tokens": ["Auf", "A\u00b7e\u00b7ckern", "wird", "dir", "oft", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Feuermann.", "tokens": ["Ein", "Feu\u00b7er\u00b7mann", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann hilft kein Beten, und kein Segnen!", "tokens": ["Dann", "hilft", "kein", "Be\u00b7ten", ",", "und", "kein", "Seg\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er rauscht heran.", "tokens": ["Er", "rauscht", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du mu\u00dft ein ander Mittel suchen", "tokens": ["Du", "mu\u00dft", "ein", "an\u00b7der", "Mit\u00b7tel", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser Noth.", "tokens": ["In", "die\u00b7ser", "Noth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ein Feuermann weicht nur durch Fluchen.", "tokens": ["Ein", "Feu\u00b7er\u00b7mann", "weicht", "nur", "durch", "Flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Verzeih dirs Gott!", "tokens": ["Ver\u00b7zeih", "dirs", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Aus zweien sieht man einen werden,", "tokens": ["Aus", "zwei\u00b7en", "sieht", "man", "ei\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "VVFIN", "PIS", "ART", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus einem zwei,", "tokens": ["Aus", "ei\u00b7nem", "zwei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die st\u00fcrzen mit des Zorns Geberden", "tokens": ["Die", "st\u00fcr\u00b7zen", "mit", "des", "Zorns", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kampf, herbei.", "tokens": ["Zum", "Kampf", ",", "her\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es flammt! o welche Gegenwehre!", "tokens": ["Es", "flammt", "!", "o", "wel\u00b7che", "Ge\u00b7gen\u00b7weh\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "FM", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O welcher Kampf!", "tokens": ["O", "wel\u00b7cher", "Kampf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der laue Christ (den Gott bekehre)", "tokens": ["Der", "lau\u00b7e", "Christ", "(", "den", "Gott", "be\u00b7keh\u00b7re", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nennts einen Dampf.", "tokens": ["Nennts", "ei\u00b7nen", "Dampf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Erscheint dir ein Pallast von weiten", "tokens": ["Er\u00b7scheint", "dir", "ein", "Pal\u00b7last", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In vollem Glanz,", "tokens": ["In", "vol\u00b7lem", "Glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es ist (o la\u00df dich nicht verleiten!)", "tokens": ["Es", "ist", "(", "o", "la\u00df", "dich", "nicht", "ver\u00b7lei\u00b7ten", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$(", "FM", "VVIMP", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Hexentanz.", "tokens": ["Ein", "He\u00b7xen\u00b7tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Denn w\u00fcrde dir dabei entrinnen", "tokens": ["Denn", "w\u00fcr\u00b7de", "dir", "da\u00b7bei", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PAV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wort von Gott,", "tokens": ["Ein", "Wort", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Schnell w\u00e4re der Pallast von hinnen,", "tokens": ["Schnell", "w\u00e4\u00b7re", "der", "Pal\u00b7last", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Du tief im Koth.", "tokens": ["Du", "tief", "im", "Koth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Im Gasthof' ist der Alp zu scheuen.", "tokens": ["Im", "Gast\u00b7hof'", "ist", "der", "Alp", "zu", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er schleicht daher.", "tokens": ["Er", "schleicht", "da\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und liegt er nun, man kann nicht schreien.", "tokens": ["Und", "liegt", "er", "nun", ",", "man", "kann", "nicht", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PIS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er dr\u00fcckt so sehr.", "tokens": ["Er", "dr\u00fcckt", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wirf, wenn er k\u00f6mmt, geschwind ein Kissen", "tokens": ["Wirf", ",", "wenn", "er", "k\u00f6mmt", ",", "ge\u00b7schwind", "ein", "Kis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Bett' ihm zu!", "tokens": ["Vom", "Bett'", "ihm", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er wird darauf sich setzen m\u00fcssen,", "tokens": ["Er", "wird", "da\u00b7rauf", "sich", "set\u00b7zen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann hast du Ruh.", "tokens": ["Dann", "hast", "du", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Mein Sohn! ich will dir nichts vergr\u00f6\u00dfern,", "tokens": ["Mein", "Sohn", "!", "ich", "will", "dir", "nichts", "ver\u00b7gr\u00f6\u00b7\u00dfern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sey bedacht!", "tokens": ["Doch", "sey", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jetzt sag' ich dir von alten Schl\u00f6ssern.", "tokens": ["Jetzt", "sag'", "ich", "dir", "von", "al\u00b7ten", "Schl\u00f6s\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort spuckt's bei Nacht.", "tokens": ["Dort", "spuckt's", "bei", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gespenster ohne Kopf erscheinen", "tokens": ["Ge\u00b7spens\u00b7ter", "oh\u00b7ne", "Kopf", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sechs Ellen lang.", "tokens": ["Sechs", "El\u00b7len", "lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Man h\u00f6rt um zw\u00f6lf Uhr kl\u00e4glich weinen", "tokens": ["Man", "h\u00f6rt", "um", "zw\u00f6lf", "Uhr", "kl\u00e4g\u00b7lich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "CARD", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im \u00f6den Gang.", "tokens": ["Im", "\u00f6\u00b7den", "Gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Oft h\u00f6rt man an der Th\u00fcre klopfen.", "tokens": ["Oft", "h\u00f6rt", "man", "an", "der", "Th\u00fc\u00b7re", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wird es seyn!", "tokens": ["Wer", "wird", "es", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Man bebt und schwitzet gro\u00dfe Tropfen,", "tokens": ["Man", "bebt", "und", "schwit\u00b7zet", "gro\u00b7\u00dfe", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagt \u2013 Herein!", "tokens": ["Und", "sagt", "\u2013", "Her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da k\u00f6mmts, als ein Barbier, ins Zimmer,", "tokens": ["Da", "k\u00f6mmts", ",", "als", "ein", "Bar\u00b7bier", ",", "ins", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und man erstarrt", "tokens": ["Und", "man", "er\u00b7starrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mein Urgro\u00dfvater sagt' es immer.", "tokens": ["Mein", "Ur\u00b7gro\u00df\u00b7va\u00b7ter", "sagt'", "es", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm schor's den Bart.", "tokens": ["Ihm", "schor's", "den", "Bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Wie wollen wir's auch anders haben?", "tokens": ["Wie", "wol\u00b7len", "wir's", "auch", "an\u00b7ders", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur die\u00df bedenkt!", "tokens": ["Nur", "die\u00df", "be\u00b7denkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So mancher Schatz liegt hier begraben,", "tokens": ["So", "man\u00b7cher", "Schatz", "liegt", "hier", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tief versenkt.", "tokens": ["Und", "tief", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Drum hat der B\u00f6se seine Possen,", "tokens": ["Drum", "hat", "der", "B\u00f6\u00b7se", "sei\u00b7ne", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schreckt uns ab.", "tokens": ["Und", "schreckt", "uns", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch liebes Kind! bleib unverdrossen,", "tokens": ["Doch", "lie\u00b7bes", "Kind", "!", "bleib", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und such' und grab'!", "tokens": ["Und", "such'", "und", "grab'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Oft liegt ein Schatz auf freien Wegen,", "tokens": ["Oft", "liegt", "ein", "Schatz", "auf", "frei\u00b7en", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und brennt, wie Glut.", "tokens": ["Und", "brennt", ",", "wie", "Glut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sei flink, ein Heilthum drauf zu legen!", "tokens": ["Sei", "flink", ",", "ein", "Heilt\u00b7hum", "drauf", "zu", "le\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ist das Gut!", "tokens": ["Dein", "ist", "das", "Gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Studirte wissen noch ein Mittel,", "tokens": ["Stu\u00b7dir\u00b7te", "wis\u00b7sen", "noch", "ein", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein sch\u00f6n Gebet.", "tokens": ["Ein", "sch\u00f6n", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.7": {"text": "Ich glaube fast da\u00df auf dem Titel", "tokens": ["Ich", "glau\u00b7be", "fast", "da\u00df", "auf", "dem", "Ti\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sanct Christoph steht.", "tokens": ["Sanct", "Chris\u00b7toph", "steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "J\u00fcngst hab' ich dir vom schweren Wagen,", "tokens": ["J\u00fcngst", "hab'", "ich", "dir", "vom", "schwe\u00b7ren", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Sohn! erz\u00e4hlt.", "tokens": ["Mein", "Sohn", "!", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich soll dir noch vom Teufel sagen,", "tokens": ["Ich", "soll", "dir", "noch", "vom", "Teu\u00b7fel", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schildwach' h\u00e4lt,", "tokens": ["Der", "Schild\u00b7wach'", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von schwarzen Katzen mit drei F\u00fc\u00dfen \u2013", "tokens": ["Von", "schwar\u00b7zen", "Kat\u00b7zen", "mit", "drei", "F\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch sey's genug!", "tokens": ["Doch", "sey's", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du wirst es selbst erfahren m\u00fcssen,", "tokens": ["Du", "wirst", "es", "selbst", "er\u00b7fah\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wirst du klug.", "tokens": ["So", "wirst", "du", "klug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Traun! Mutter! sprach der Sohn vermessen:", "tokens": ["Traun", "!", "Mut\u00b7ter", "!", "sprach", "der", "Sohn", "ver\u00b7mes\u00b7sen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr seid gereist!", "tokens": ["Ihr", "seid", "ge\u00b7reist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch habt ihr einen Geist vergessen,", "tokens": ["Doch", "habt", "ihr", "ei\u00b7nen", "Geist", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Branntweingeist.", "tokens": ["Den", "Brannt\u00b7wein\u00b7geist", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich seh', er f\u00e4hrt aus eurem Munde.", "tokens": ["Ich", "seh'", ",", "er", "f\u00e4hrt", "aus", "eu\u00b7rem", "Mun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin nicht blind,", "tokens": ["Ich", "bin", "nicht", "blind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und w\u00e4r' ich auch (zur guten Stunde)", "tokens": ["Und", "w\u00e4r'", "ich", "auch", "(", "zur", "gu\u00b7ten", "Stun\u00b7de", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$(", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Sonntagskind.", "tokens": ["Kein", "Sonn\u00b7tags\u00b7kind", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}