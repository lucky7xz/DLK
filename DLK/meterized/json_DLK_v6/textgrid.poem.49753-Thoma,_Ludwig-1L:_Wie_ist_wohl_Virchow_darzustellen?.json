{"textgrid.poem.49753": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist wohl Virchow darzustellen?", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist wohl Virchow darzustellen?", "tokens": ["Wie", "ist", "wohl", "Vir\u00b7chow", "dar\u00b7zu\u00b7stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herr K\u00fcnstler, nun bedenken Sie,", "tokens": ["Herr", "K\u00fcnst\u00b7ler", ",", "nun", "be\u00b7den\u00b7ken", "Sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVINF", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es soll zun\u00e4chsten klar erhellen:", "tokens": ["Es", "soll", "zu\u00b7n\u00e4chs\u00b7ten", "klar", "er\u00b7hel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer war der Mann? Und zweitens wie?", "tokens": ["Wer", "war", "der", "Mann", "?", "Und", "zwei\u00b7tens", "wie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "KON", "ADV", "KOKOM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er war im Leben Mediziner,", "tokens": ["Er", "war", "im", "Le\u00b7ben", "Me\u00b7di\u00b7zi\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun gut, so zeigen wir ihn wohl", "tokens": ["Nun", "gut", ",", "so", "zei\u00b7gen", "wir", "ihn", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als solchen, \u00fcberhaupt als Diener", "tokens": ["Als", "sol\u00b7chen", ",", "\u00fc\u00b7ber\u00b7haupt", "als", "Die\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIAT", "$,", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wissenschaft durch ein Symbol.", "tokens": ["Der", "Wis\u00b7sen\u00b7schaft", "durch", "ein", "Sym\u00b7bol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Spritze, welche zum Klistieren", "tokens": ["Die", "Sprit\u00b7ze", ",", "wel\u00b7che", "zum", "Klis\u00b7tie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Arzt best\u00e4ndig bei sich hat,", "tokens": ["Der", "Arzt", "be\u00b7st\u00e4n\u00b7dig", "bei", "sich", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann seinen Stand uns deklarieren.", "tokens": ["Kann", "sei\u00b7nen", "Stand", "uns", "de\u00b7kla\u00b7ri\u00b7e\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es findet so kein Zweifel statt.", "tokens": ["Es", "fin\u00b7det", "so", "kein", "Zwei\u00b7fel", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu noch verst\u00e4rkter Unterscheidung", "tokens": ["Zu", "noch", "ver\u00b7st\u00e4rk\u00b7ter", "Un\u00b7ter\u00b7schei\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Helden gibt man ihm ein Buch,", "tokens": ["Vom", "Hel\u00b7den", "gibt", "man", "ihm", "ein", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch b\u00fcrgerliche Sonntagskleidung;", "tokens": ["Auch", "b\u00fcr\u00b7ger\u00b7li\u00b7che", "Sonn\u00b7tags\u00b7klei\u00b7dung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gehrock macht der Arzt Besuch!", "tokens": ["Im", "Ge\u00b7hrock", "macht", "der", "Arzt", "Be\u00b7such", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Die Hosenr\u00f6hre sei geb\u00fcgelt,", "tokens": ["Die", "Ho\u00b7sen\u00b7r\u00f6h\u00b7re", "sei", "ge\u00b7b\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Antlitz zeige \u00c4hnlichkeit!", "tokens": ["Das", "Ant\u00b7litz", "zei\u00b7ge", "\u00c4hn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ist die Freude ungez\u00fcgelt,", "tokens": ["Dann", "ist", "die", "Freu\u00b7de", "un\u00b7ge\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird er enth\u00fcllt und eingeweiht.", "tokens": ["Wird", "er", "ent\u00b7h\u00fcllt", "und", "ein\u00b7ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir rufen unterm Festzylinder:", "tokens": ["Wir", "ru\u00b7fen", "un\u00b7term", "Fest\u00b7zy\u00b7lin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbhier steht der echte Virchow da!\u00ab", "tokens": ["\u00bb", "hier", "steht", "der", "ech\u00b7te", "Vir\u00b7chow", "da", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NE", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schluchzend sagen seine Kinder:", "tokens": ["Und", "schluch\u00b7zend", "sa\u00b7gen", "sei\u00b7ne", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbach Gott! Er ist es, der Papa!\u00ab", "tokens": ["\u00bb", "ach", "Gott", "!", "Er", "ist", "es", ",", "der", "Pa\u00b7pa", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NN", "$.", "PPER", "VAFIN", "PPER", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}