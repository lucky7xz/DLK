{"dta.poem.18840": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Esist gut .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Bey Mondenschein im Paradeis", "tokens": ["Bey", "Mon\u00b7den\u00b7schein", "im", "Pa\u00b7ra\u00b7deis"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fand Jehova im Schlafe tief", "tokens": ["Fand", "Je\u00b7ho\u00b7va", "im", "Schla\u00b7fe", "tief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Adam versunken, legte leis\u2019", "tokens": ["A\u00b7dam", "ver\u00b7sun\u00b7ken", ",", "leg\u00b7te", "leis'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "VVFIN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zur Seit\u2019 ein Evchen, das auch entschlief.", "tokens": ["Zur", "Seit'", "ein", "Ev\u00b7chen", ",", "das", "auch", "ent\u00b7schlief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da lagen nun in Erdeschranken", "tokens": ["Da", "la\u00b7gen", "nun", "in", "Er\u00b7de\u00b7schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gottes zwey lieblichste Gedanken. \u2014", "tokens": ["Got\u00b7tes", "zwey", "lieb\u00b7lichs\u00b7te", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$.", "$("], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Gut!!! rief er sich zum Meisterlohn,", "tokens": ["Gut", "!!!", "rief", "er", "sich", "zum", "Meis\u00b7ter\u00b7lohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er ging sogar nicht gern davon.", "tokens": ["Er", "ging", "so\u00b7gar", "nicht", "gern", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Wunder dass es uns ber\u00fcckt,", "tokens": ["Kein", "Wun\u00b7der", "dass", "es", "uns", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Auge frisch in Auge blickt,", "tokens": ["Wenn", "Au\u00b7ge", "frisch", "in", "Au\u00b7ge", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als h\u00e4tten wir\u2019s so weit gebracht", "tokens": ["Als", "h\u00e4t\u00b7ten", "wir's", "so", "weit", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PIS", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey dem zu seyn der uns gedacht.", "tokens": ["Bey", "dem", "zu", "seyn", "der", "uns", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKZU", "VAINF", "PRELS", "PPER", "VVPP", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ruft er uns, wohlan! es sey!", "tokens": ["Und", "ruft", "er", "uns", ",", "wo\u00b7hlan", "!", "es", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ADV", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur, das beding\u2019 ich, alle zwey.", "tokens": ["Nur", ",", "das", "be\u00b7ding'", "ich", ",", "al\u00b7le", "zwey", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PDS", "VVFIN", "PPER", "$,", "PIAT", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich halten dieser Arme Schranken,", "tokens": ["Dich", "hal\u00b7ten", "die\u00b7ser", "Ar\u00b7me", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Liebster von allen Gottes-Gedanken.", "tokens": ["Liebs\u00b7ter", "von", "al\u00b7len", "Got\u00b7tes\u00b7Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}