{"dta.poem.16311": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Anfang hofft ein Mensch mit gl\u00fccklichem Erdreisten", "tokens": ["Im", "An\u00b7fang", "hofft", "ein", "Mensch", "mit", "gl\u00fcck\u00b7li\u00b7chem", "Er\u00b7dreis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unerh\u00f6rtes unvergleichliches zu leisten.", "tokens": ["Was", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "un\u00b7ver\u00b7gleich\u00b7li\u00b7ches", "zu", "leis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald sieht er sich entt\u00e4uscht, von Schranken eingehemmt,", "tokens": ["Bald", "sieht", "er", "sich", "ent\u00b7t\u00e4uscht", ",", "von", "Schran\u00b7ken", "ein\u00b7ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergebens da\u00df er noch die Kraft entgegenstemmt.", "tokens": ["Ver\u00b7ge\u00b7bens", "da\u00df", "er", "noch", "die", "Kraft", "ent\u00b7ge\u00b7gens\u00b7temmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er f\u00fchlt es wohl, und sucht sichs aus dem Sinn zu schlagen,", "tokens": ["Er", "f\u00fchlt", "es", "wohl", ",", "und", "sucht", "sichs", "aus", "dem", "Sinn", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df auf der Welt heraus nichts kommt mit seinen Plagen.", "tokens": ["Da\u00df", "auf", "der", "Welt", "he\u00b7raus", "nichts", "kommt", "mit", "sei\u00b7nen", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APZR", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch zur Gewohnheit ward ihm seine Plage so,", "tokens": ["Doch", "zur", "Ge\u00b7wohn\u00b7heit", "ward", "ihm", "sei\u00b7ne", "Pla\u00b7ge", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur durch die Plage wird er noch des Lebens froh.", "tokens": ["Nur", "durch", "die", "Pla\u00b7ge", "wird", "er", "noch", "des", "Le\u00b7bens", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}