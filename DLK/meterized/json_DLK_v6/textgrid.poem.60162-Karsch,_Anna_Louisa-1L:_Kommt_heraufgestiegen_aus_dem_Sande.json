{"textgrid.poem.60162": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt heraufgestiegen aus dem Sande", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt heraufgestiegen aus dem Sande", "tokens": ["Kommt", "her\u00b7auf\u00b7ge\u00b7stie\u00b7gen", "aus", "dem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr Gebeine, die ihr in dem Lande", "tokens": ["Ihr", "Ge\u00b7bei\u00b7ne", ",", "die", "ihr", "in", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Meiner Jugend, eure Ruhe habt!", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", ",", "eu\u00b7re", "Ru\u00b7he", "habt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Theurer Grei\u00df, belebe deine Glieder", "tokens": ["Theu\u00b7rer", "Grei\u00df", ",", "be\u00b7le\u00b7be", "dei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und ihr Lippen redet einmahl wieder,", "tokens": ["Und", "ihr", "Lip\u00b7pen", "re\u00b7det", "ein\u00b7mahl", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die ihr mir der Lehren Honig gabt!", "tokens": ["Die", "ihr", "mir", "der", "Leh\u00b7ren", "Ho\u00b7nig", "gabt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Oder du, auf des Olympus H\u00f6he", "tokens": ["O\u00b7der", "du", ",", "auf", "des", "O\u00b7lym\u00b7pus", "H\u00f6\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weisser Schatten, siehe! wo ich gehe;", "tokens": ["Weis\u00b7ser", "Schat\u00b7ten", ",", "sie\u00b7he", "!", "wo", "ich", "ge\u00b7he", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "$.", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hinter Rindern auf der Weide nicht.", "tokens": ["Hin\u00b7ter", "Rin\u00b7dern", "auf", "der", "Wei\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Blick' auf diese feinern Menschen", "tokens": ["Blick'", "auf", "die\u00b7se", "fei\u00b7nern", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle reden deiner Nichte Lieder;", "tokens": ["Al\u00b7le", "re\u00b7den", "dei\u00b7ner", "Nich\u00b7te", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00f6r auf ihr Gespr\u00e4ch, dein Lobgedicht!", "tokens": ["H\u00f6r", "auf", "ihr", "Ge\u00b7spr\u00e4ch", ",", "dein", "Lob\u00b7ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ewig gr\u00fcnen mu\u00df die breite Linde", "tokens": ["E\u00b7wig", "gr\u00fc\u00b7nen", "mu\u00df", "die", "brei\u00b7te", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ich, gleich des besten Vaters Kinde", "tokens": ["Wo", "ich", ",", "gleich", "des", "bes\u00b7ten", "Va\u00b7ters", "Kin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Z\u00e4rtlich dir an deinem Halse hieng,", "tokens": ["Z\u00e4rt\u00b7lich", "dir", "an", "dei\u00b7nem", "Hal\u00b7se", "hieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn dich, m\u00fcde von des Tages L\u00e4nge,", "tokens": ["Wenn", "dich", ",", "m\u00fc\u00b7de", "von", "des", "Ta\u00b7ges", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wie den Schnitter von der Arbeit Menge,", "tokens": ["Wie", "den", "Schnit\u00b7ter", "von", "der", "Ar\u00b7beit", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn dich matt die Rasenbank empfing.", "tokens": ["Wenn", "dich", "matt", "die", "Ra\u00b7sen\u00b7bank", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Unter jenem Dache gr\u00fcner Bl\u00e4tter", "tokens": ["Un\u00b7ter", "je\u00b7nem", "Da\u00b7che", "gr\u00fc\u00b7ner", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wiederholt' ich von dem Gott der G\u00f6tter", "tokens": ["Wie\u00b7der\u00b7holt'", "ich", "von", "dem", "Gott", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zwanzig unverstandne Stellen dir!", "tokens": ["Zwan\u00b7zig", "un\u00b7ver\u00b7stand\u00b7ne", "Stel\u00b7len", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus der Christen hochgehaltnem Buche", "tokens": ["Aus", "der", "Chris\u00b7ten", "hoch\u00b7ge\u00b7halt\u00b7nem", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sagt' ich dir von manchem dunkeln Spruche", "tokens": ["Sagt'", "ich", "dir", "von", "man\u00b7chem", "dun\u00b7keln", "Spru\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Frommer Mann! und du erkl\u00e4rtest mir.", "tokens": ["From\u00b7mer", "Mann", "!", "und", "du", "er\u00b7kl\u00e4r\u00b7test", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Gleich den M\u00e4nnern, die in schwarzen R\u00f6cken", "tokens": ["Gleich", "den", "M\u00e4n\u00b7nern", ",", "die", "in", "schwar\u00b7zen", "R\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der hohen Canzel uns entdecken", "tokens": ["Auf", "der", "ho\u00b7hen", "Can\u00b7zel", "uns", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welcher Weg zum Leben richtig ist,", "tokens": ["Wel\u00b7cher", "Weg", "zum", "Le\u00b7ben", "rich\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn du von dem Fall und Gnadenbunde", "tokens": ["Wenn", "du", "von", "dem", "Fall", "und", "Gna\u00b7den\u00b7bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sagtest, o dann wurden deinem Munde", "tokens": ["Sag\u00b7test", ",", "o", "dann", "wur\u00b7den", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "FM", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Alle Worte z\u00e4rtlich aufgek\u00fc\u00dft!", "tokens": ["Al\u00b7le", "Wor\u00b7te", "z\u00e4rt\u00b7lich", "auf\u00b7ge\u00b7k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Du Bewohner einer Himmels-Sph\u00e4re!", "tokens": ["Du", "Be\u00b7woh\u00b7ner", "ei\u00b7ner", "Him\u00b7mels\u00b7Sph\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Siehe, meiner Freuden stille Z\u00e4hre", "tokens": ["Sie\u00b7he", ",", "mei\u00b7ner", "Freu\u00b7den", "stil\u00b7le", "Z\u00e4h\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fliesset \u00fcber meine Wangen oft.", "tokens": ["Flies\u00b7set", "\u00fc\u00b7ber", "mei\u00b7ne", "Wan\u00b7gen", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kanst du reden theurer Schatten? sage", "tokens": ["Kanst", "du", "re\u00b7den", "theu\u00b7rer", "Schat\u00b7ten", "?", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "VVINF", "ADJD", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ob dein Herz f\u00fcr meine Lebenstage", "tokens": ["Ob", "dein", "Herz", "f\u00fcr", "mei\u00b7ne", "Le\u00b7bens\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gl\u00fcck und Ehre dazumahl gehofft,", "tokens": ["Gl\u00fcck", "und", "Eh\u00b7re", "da\u00b7zu\u00b7mahl", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn mein Auge, liegend auf dem Blatte,", "tokens": ["Wenn", "mein", "Au\u00b7ge", ",", "lie\u00b7gend", "auf", "dem", "Blat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "T\u00e4glich weisre Schriften vor sich hatte,", "tokens": ["T\u00e4g\u00b7lich", "weis\u00b7re", "Schrif\u00b7ten", "vor", "sich", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "PRF", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich auf der Wiese Bl\u00fcmchen la\u00df,", "tokens": ["Wenn", "ich", "auf", "der", "Wie\u00b7se", "Bl\u00fcm\u00b7chen", "la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sie in meinen kleinen H\u00e4nden brachte,", "tokens": ["Sie", "in", "mei\u00b7nen", "klei\u00b7nen", "H\u00e4n\u00b7den", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sie zur Zierde deiner Haare machte", "tokens": ["Sie", "zur", "Zier\u00b7de", "dei\u00b7ner", "Haa\u00b7re", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und auf Rosen l\u00e4chelnd bey dir sa\u00df?", "tokens": ["Und", "auf", "Ro\u00b7sen", "l\u00e4\u00b7chelnd", "bey", "dir", "sa\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Sey mir dreymahl mehr mit Licht bekleidet;", "tokens": ["Sey", "mir", "drey\u00b7mahl", "mehr", "mit", "Licht", "be\u00b7klei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Gottheit Blicken mehr geweidet", "tokens": ["Mit", "der", "Got\u00b7theit", "Bli\u00b7cken", "mehr", "ge\u00b7wei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als die andern Seelen um dich her!", "tokens": ["Als", "die", "an\u00b7dern", "See\u00b7len", "um", "dich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr die Tropfen alle die mir werden", "tokens": ["F\u00fcr", "die", "Trop\u00b7fen", "al\u00b7le", "die", "mir", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "PRELS", "PPER", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus dem Freuden-Becher hier auf Erden.", "tokens": ["Aus", "dem", "Freu\u00b7den\u00b7Be\u00b7cher", "hier", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Tr\u00e4nke dich der Seligkeiten Meer!", "tokens": ["Tr\u00e4n\u00b7ke", "dich", "der", "Se\u00b7lig\u00b7kei\u00b7ten", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}