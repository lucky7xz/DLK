{"textgrid.poem.33190": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2136.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine hand hat euch gestellt seiner kirch zur s\u00e4ule, und zum salz der armen welt gegen ihre f\u00e4ule;", "tokens": ["Sei\u00b7ne", "hand", "hat", "euch", "ge\u00b7stellt", "sei\u00b7ner", "kirch", "zur", "s\u00e4u\u00b7le", ",", "und", "zum", "salz", "der", "ar\u00b7men", "welt", "ge\u00b7gen", "ih\u00b7re", "f\u00e4u\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "PPOSAT", "NN", "APPRART", "VVFIN", "$,", "KON", "APPRART", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Zeiget euch der ewigkeit liebs-registraturen, und ihr seht f\u00fcr unsre zeit seines willens spuren.", "tokens": ["Zei\u00b7get", "euch", "der", "e\u00b7wig\u00b7keit", "liebs\u00b7re\u00b7gis\u00b7tra\u00b7tu\u00b7ren", ",", "und", "ihr", "seht", "f\u00fcr", "uns\u00b7re", "zeit", "sei\u00b7nes", "wil\u00b7lens", "spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Stehet seinem hause f\u00fcr mit verstand und gnade: mit dem segen gehet ihr auf dem schmalen pfade.", "tokens": ["Ste\u00b7het", "sei\u00b7nem", "hau\u00b7se", "f\u00fcr", "mit", "ver\u00b7stand", "und", "gna\u00b7de", ":", "mit", "dem", "se\u00b7gen", "ge\u00b7het", "ihr", "auf", "dem", "schma\u00b7len", "pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPR", "VVFIN", "KON", "VVFIN", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr zieht ihm sein uhr-werk auf, da\u00df es richtig gehe: s\u00e4hen wir nur alle drauf. Amen! es geschehe.", "tokens": ["Ihr", "zieht", "ihm", "sein", "uhr\u00b7werk", "auf", ",", "da\u00df", "es", "rich\u00b7tig", "ge\u00b7he", ":", "s\u00e4\u00b7hen", "wir", "nur", "al\u00b7le", "drauf", ".", "A\u00b7men", "!", "es", "ge\u00b7sche\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$.", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-+-+---+--+---+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Sitzet in der wechsel-bank, und zehlt die talente, damit krigt der Herr mit dank capital und rente.", "tokens": ["Sit\u00b7zet", "in", "der", "wech\u00b7sel\u00b7bank", ",", "und", "zehlt", "die", "ta\u00b7len\u00b7te", ",", "da\u00b7mit", "krigt", "der", "Herr", "mit", "dank", "ca\u00b7pi\u00b7tal", "und", "ren\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "$,", "PAV", "VVFIN", "ART", "NN", "APPR", "APPR", "NE", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+----+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihr wagts in Jehovah st\u00e4rk, wie der Nehemias, und greifft an das grosse werk, aufs wort Zacharias.", "tokens": ["Ihr", "wagts", "in", "Je\u00b7ho\u00b7vah", "st\u00e4rk", ",", "wie", "der", "Ne\u00b7he\u00b7mi\u00b7as", ",", "und", "greifft", "an", "das", "gros\u00b7se", "werk", ",", "aufs", "wort", "Zac\u00b7ha\u00b7ri\u00b7as", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Esras drang auf ehrbarkeit unter seinem volke; ihr auf evangelsche freud bey der zeugen-wolke.", "tokens": ["Es\u00b7ras", "drang", "auf", "ehr\u00b7bar\u00b7keit", "un\u00b7ter", "sei\u00b7nem", "vol\u00b7ke", ";", "ihr", "auf", "e\u00b7van\u00b7gel\u00b7sche", "freud", "bey", "der", "zeu\u00b7gen\u00b7wol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Daniel, der treue mann, muste k\u00f6n'ge lehren; was habt ihr nicht schon gethan, den am creuz zu ehren.", "tokens": ["Da\u00b7ni\u00b7el", ",", "der", "treu\u00b7e", "mann", ",", "mus\u00b7te", "k\u00f6n'\u00b7ge", "leh\u00b7ren", ";", "was", "habt", "ihr", "nicht", "schon", "ge\u00b7than", ",", "den", "am", "creuz", "zu", "eh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VMFIN", "VMFIN", "VVINF", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,", "PRELS", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+--+-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Wie dort Paul, besorget ihr alle die Gemeinen, und", "tokens": ["Wie", "dort", "Paul", ",", "be\u00b7sor\u00b7get", "ihr", "al\u00b7le", "die", "Ge\u00b7mei\u00b7nen", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "NE", "$,", "VVFIN", "PPER", "PIS", "ART", "NN", "$,", "KON"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Mit dem evangelio f\u00fcllt ihr alle lande; und seyd bey dem Lamme froh in ehr oder schande.", "tokens": ["Mit", "dem", "e\u00b7van\u00b7ge\u00b7lio", "f\u00fcllt", "ihr", "al\u00b7le", "lan\u00b7de", ";", "und", "seyd", "bey", "dem", "Lam\u00b7me", "froh", "in", "ehr", "o\u00b7der", "schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "PPER", "PIAT", "NN", "$.", "KON", "VAFIN", "APPR", "ART", "NN", "ADJD", "APPR", "NN", "KON", "ADJA", "$."], "meter": "--+---+-+-+--++-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Dank sey dir, du Gottes-sohn, da\u00df dergleichen helden, aufs Vaters vocation, sich zum streite melden.", "tokens": ["Dank", "sey", "dir", ",", "du", "Got\u00b7tes\u00b7sohn", ",", "da\u00df", "derg\u00b7lei\u00b7chen", "hel\u00b7den", ",", "aufs", "Va\u00b7ters", "vo\u00b7ca\u00b7ti\u00b7on", ",", "sich", "zum", "strei\u00b7te", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "$,", "PPER", "NN", "$,", "KOUS", "PIS", "VVINF", "$,", "APPRART", "NN", "NE", "$,", "PRF", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+--+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "La\u00df die deinige mit kraft derer adler fliegen, und in ihrer zeugenschaft mit der h\u00f6lle kriegen.", "tokens": ["La\u00df", "die", "dei\u00b7ni\u00b7ge", "mit", "kraft", "de\u00b7rer", "ad\u00b7ler", "flie\u00b7gen", ",", "und", "in", "ih\u00b7rer", "zeu\u00b7gen\u00b7schaft", "mit", "der", "h\u00f6l\u00b7le", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "PPOSS", "APPR", "NN", "PDS", "NN", "VVINF", "$,", "KON", "APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}