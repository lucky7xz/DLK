{"textgrid.poem.25900": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Das Geisterhaus", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Geisterhaus, das aus Ger\u00fcchen aufgebaut,", "tokens": ["Das", "Geis\u00b7ter\u00b7haus", ",", "das", "aus", "Ge\u00b7r\u00fc\u00b7chen", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft nah, da\u00df ich neu wohne in l\u00e4ngst Altem.", "tokens": ["Oft", "nah", ",", "da\u00df", "ich", "neu", "woh\u00b7ne", "in", "l\u00e4ngst", "Al\u00b7tem", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort wusch man einst die Leiche meiner Mutter,", "tokens": ["Dort", "wusch", "man", "einst", "die", "Lei\u00b7che", "mei\u00b7ner", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Garten lernten mich die Blumen kennen,", "tokens": ["Im", "Gar\u00b7ten", "lern\u00b7ten", "mich", "die", "Blu\u00b7men", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gartenblumen, die besonnen bl\u00fchen.", "tokens": ["Die", "Gar\u00b7ten\u00b7blu\u00b7men", ",", "die", "be\u00b7son\u00b7nen", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und drau\u00dfen stand behaglich Korn und Klee", "tokens": ["Und", "drau\u00b7\u00dfen", "stand", "be\u00b7hag\u00b7lich", "Korn", "und", "Klee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und duftete Begehr, und heute wei\u00df ich,", "tokens": ["Und", "duf\u00b7te\u00b7te", "Be\u00b7gehr", ",", "und", "heu\u00b7te", "wei\u00df", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df alle D\u00fcfte \u00fcber Feld und G\u00e4rten", "tokens": ["Da\u00df", "al\u00b7le", "D\u00fcf\u00b7te", "\u00fc\u00b7ber", "Feld", "und", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Liebeslieder all der Blumen sind.", "tokens": ["Die", "Lie\u00b7bes\u00b7lie\u00b7der", "all", "der", "Blu\u00b7men", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch damals unverstanden gingen Fr\u00fchlingsn\u00e4chte,", "tokens": ["Doch", "da\u00b7mals", "un\u00b7ver\u00b7stan\u00b7den", "gin\u00b7gen", "Fr\u00fch\u00b7lings\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch kindlich schlief der Mond im wei\u00dfen Baum.", "tokens": ["Noch", "kind\u00b7lich", "schlief", "der", "Mond", "im", "wei\u00b7\u00dfen", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nur reich entsinn' ich k\u00f6rperlose Freuden,", "tokens": ["Nur", "reich", "ent\u00b7sinn'", "ich", "k\u00f6r\u00b7per\u00b7lo\u00b7se", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wenn dumpfe Wolken an den Himmel stiegen,", "tokens": ["Wenn", "dump\u00b7fe", "Wol\u00b7ken", "an", "den", "Him\u00b7mel", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein Augenblick scho\u00df aus den Ewigkeiten,", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "scho\u00df", "aus", "den", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Er zeigte klein die Menschen, gro\u00df den Himmel.", "tokens": ["Er", "zeig\u00b7te", "klein", "die", "Men\u00b7schen", ",", "gro\u00df", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Winter, wenn die Tage blind geworden,", "tokens": ["Im", "Win\u00b7ter", ",", "wenn", "die", "Ta\u00b7ge", "blind", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wuchsen die Menschen breit im sichern Hause,", "tokens": ["Wuch\u00b7sen", "die", "Men\u00b7schen", "breit", "im", "si\u00b7chern", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das bilderreiche Feuer w\u00e4rmte Tr\u00e4ume,", "tokens": ["Das", "bil\u00b7der\u00b7rei\u00b7che", "Feu\u00b7er", "w\u00e4rm\u00b7te", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Tr\u00e4ume wurden Sonnen langen N\u00e4chten.", "tokens": ["Und", "Tr\u00e4u\u00b7me", "wur\u00b7den", "Son\u00b7nen", "lan\u00b7gen", "N\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und viel noch wei\u00df ich von Geheimnisdingen,", "tokens": ["Und", "viel", "noch", "wei\u00df", "ich", "von", "Ge\u00b7heim\u00b7nis\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn mehr verwandter als die Menschenherzen", "tokens": ["Denn", "mehr", "ver\u00b7wand\u00b7ter", "als", "die", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Waren die Herzen mir der Tiere und der Pflanzen.", "tokens": ["Wa\u00b7ren", "die", "Her\u00b7zen", "mir", "der", "Tie\u00b7re", "und", "der", "Pflan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "In Sommern\u00e4chten, wenn die Grillen spuken,", "tokens": ["In", "Som\u00b7mer\u00b7n\u00e4ch\u00b7ten", ",", "wenn", "die", "Gril\u00b7len", "spu\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn ganze Heere eine Nacht besangen ...", "tokens": ["Wenn", "gan\u00b7ze", "Hee\u00b7re", "ei\u00b7ne", "Nacht", "be\u00b7san\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "... Die furchtbar stummen Katzen in verlassnen Kammern,", "tokens": ["...", "Die", "furcht\u00b7bar", "stum\u00b7men", "Kat\u00b7zen", "in", "ver\u00b7lass\u00b7nen", "Kam\u00b7mern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die durch verschlossne T\u00fcren j\u00e4h verschwinden,", "tokens": ["Die", "durch", "ver\u00b7schloss\u00b7ne", "T\u00fc\u00b7ren", "j\u00e4h", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit Augen, die entsetzlich Fremdes wissen,", "tokens": ["Mit", "Au\u00b7gen", ",", "die", "ent\u00b7setz\u00b7lich", "Frem\u00b7des", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sie haben mehr ersp\u00e4ht als alle Menschen.", "tokens": ["Sie", "ha\u00b7ben", "mehr", "er\u00b7sp\u00e4ht", "als", "al\u00b7le", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Schmetterlinge, die im Himmel wohnen,", "tokens": ["Und", "Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "die", "im", "Him\u00b7mel", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Sie, die versargt gewesen in den Puppen,", "tokens": ["Sie", ",", "die", "ver\u00b7sargt", "ge\u00b7we\u00b7sen", "in", "den", "Pup\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVPP", "VAPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie kamen oft zu mir dicht auf die Erde", "tokens": ["Sie", "ka\u00b7men", "oft", "zu", "mir", "dicht", "auf", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und legten lichtbestaubt die Baldachine", "tokens": ["Und", "leg\u00b7ten", "licht\u00b7be\u00b7staubt", "die", "Bal\u00b7da\u00b7chi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Flach in die Sonne, sprechend zu der Sonne.", "tokens": ["Flach", "in", "die", "Son\u00b7ne", ",", "spre\u00b7chend", "zu", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die Tage wurden so unirdisch lang,", "tokens": ["Die", "Ta\u00b7ge", "wur\u00b7den", "so", "un\u00b7ir\u00b7disch", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Mit tausend Fl\u00fcgeln sangen die Insekten.", "tokens": ["Mit", "tau\u00b7send", "Fl\u00fc\u00b7geln", "san\u00b7gen", "die", "In\u00b7sek\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ich lebte mit der flinken Eintagfliege", "tokens": ["Ich", "leb\u00b7te", "mit", "der", "flin\u00b7ken", "Ein\u00b7tag\u00b7flie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die sechzig Jahre in der einen Stunde.", "tokens": ["Die", "sech\u00b7zig", "Jah\u00b7re", "in", "der", "ei\u00b7nen", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Doch sp\u00e4ter k\u00fcrzten sich im Haus die Jahre,", "tokens": ["Doch", "sp\u00e4\u00b7ter", "k\u00fcrz\u00b7ten", "sich", "im", "Haus", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die Falten der Gesichter lehrten z\u00e4hlen,", "tokens": ["Die", "Fal\u00b7ten", "der", "Ge\u00b7sich\u00b7ter", "lehr\u00b7ten", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sie kamen n\u00e4her, n\u00e4her und verwandter,", "tokens": ["Sie", "ka\u00b7men", "n\u00e4\u00b7her", ",", "n\u00e4\u00b7her", "und", "ver\u00b7wand\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch sehe ich auf sie, die abgeerntet haben,", "tokens": ["Doch", "se\u00b7he", "ich", "auf", "sie", ",", "die", "ab\u00b7geern\u00b7tet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "VVPP", "VAINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.27": {"text": "Ungl\u00e4ubig noch, mit jenen unersch\u00f6pften Augen,", "tokens": ["Un\u00b7gl\u00e4u\u00b7big", "noch", ",", "mit", "je\u00b7nen", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die voll Unsterblichkeit heiliger Jugend.", "tokens": ["Die", "voll", "U\u00b7nsterb\u00b7lich\u00b7keit", "hei\u00b7li\u00b7ger", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}}}}}