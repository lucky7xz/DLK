{"textgrid.poem.37378": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachdem er am Sonntagmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem er am Sonntagmorgen", "tokens": ["Nach\u00b7dem", "er", "am", "Sonn\u00b7tag\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor seinem Spiegel gestanden,", "tokens": ["Vor", "sei\u00b7nem", "Spie\u00b7gel", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verschwanden die letzten Sorgen", "tokens": ["Ver\u00b7schwan\u00b7den", "die", "letz\u00b7ten", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Zweifel, die noch vorhanden.", "tokens": ["Und", "Zwei\u00b7fel", ",", "die", "noch", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er wurde so verwegen,", "tokens": ["Er", "wur\u00b7de", "so", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er nicht l\u00e4nger schwankte.", "tokens": ["Da\u00df", "er", "nicht", "l\u00e4n\u00b7ger", "schwank\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schrieb ihr. Sie dagegen", "tokens": ["Er", "schrieb", "ihr", ".", "Sie", "da\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwidert: Nein! Sie dankte.", "tokens": ["Er\u00b7wi\u00b7dert", ":", "Nein", "!", "Sie", "dank\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "PTKANT", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Schreck, den er da hatte,", "tokens": ["Der", "Schreck", ",", "den", "er", "da", "hat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4tt' ihn fast umgeschmissen,", "tokens": ["H\u00e4tt'", "ihn", "fast", "um\u00b7ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00e4tt' ihn eine Ratte", "tokens": ["Als", "h\u00e4tt'", "ihn", "ei\u00b7ne", "Rat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pl\u00f6tzlich ins Herz gebissen.", "tokens": ["Pl\u00f6tz\u00b7lich", "ins", "Herz", "ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}