{"dta.poem.19233": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen wollen doch von Werken der Natur,", "tokens": ["Die", "Men\u00b7schen", "wol\u00b7len", "doch", "von", "Wer\u00b7ken", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihnen Nutzen bringt, am meisten r\u00fchmen nur;", "tokens": ["Was", "ih\u00b7nen", "Nut\u00b7zen", "bringt", ",", "am", "meis\u00b7ten", "r\u00fch\u00b7men", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,", "APPRART", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Entweder was sie selbst zu f\u00fcttern dient, zu kleiden,", "tokens": ["Ent\u00b7we\u00b7der", "was", "sie", "selbst", "zu", "f\u00fct\u00b7tern", "dient", ",", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch oder wenigstens ihr zahmes Vieh zu weiden.", "tokens": ["Doch", "o\u00b7der", "we\u00b7nigs\u00b7tens", "ihr", "zah\u00b7mes", "Vieh", "zu", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schrieb' auch ein Vogel nun einmal Naturgeschichte;", "tokens": ["Schrieb'", "auch", "ein", "Vo\u00b7gel", "nun", "ein\u00b7mal", "Na\u00b7tur\u00b7ge\u00b7schich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, meint ihr, lauteten vom Menschen die Berichte?", "tokens": ["Wie", ",", "meint", "ihr", ",", "lau\u00b7te\u00b7ten", "vom", "Men\u00b7schen", "die", "Be\u00b7rich\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df unter allem, was zu Vogelschirm und Schutze", "tokens": ["Da\u00df", "un\u00b7ter", "al\u00b7lem", ",", "was", "zu", "Vo\u00b7gel\u00b7schirm", "und", "Schut\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIS", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschaffen Gott, der Mensch sei vom geringsten Nutze;", "tokens": ["Ge\u00b7schaf\u00b7fen", "Gott", ",", "der", "Mensch", "sei", "vom", "ge\u00b7rings\u00b7ten", "Nut\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja recht zum Ungemach, Verderben und Entsetzen,", "tokens": ["Ja", "recht", "zum", "Un\u00b7ge\u00b7mach", ",", "Ver\u00b7der\u00b7ben", "und", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "APPRART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit R\u00e4nken tausendfach, Nachstellungen und Netzen.", "tokens": ["Mit", "R\u00e4n\u00b7ken", "tau\u00b7send\u00b7fach", ",", "Nach\u00b7stel\u00b7lun\u00b7gen", "und", "Net\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und nichts sei gut an ihm, als da\u00df mit seltnem Triebe", "tokens": ["Und", "nichts", "sei", "gut", "an", "ihm", ",", "als", "da\u00df", "mit", "selt\u00b7nem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "APPR", "PPER", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er B\u00e4ume pflanze, zwar dem Vogel nicht zu Liebe,", "tokens": ["Er", "B\u00e4u\u00b7me", "pflan\u00b7ze", ",", "zwar", "dem", "Vo\u00b7gel", "nicht", "zu", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$,", "ADV", "ART", "NE", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Von denen doch alsdann ein Vogel dann und wann,", "tokens": ["Von", "de\u00b7nen", "doch", "als\u00b7dann", "ein", "Vo\u00b7gel", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "ART", "NN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihn der Mensch nicht scheucht, die Fr\u00fcchte picken kann.", "tokens": ["Wenn", "ihn", "der", "Mensch", "nicht", "scheucht", ",", "die", "Fr\u00fcch\u00b7te", "pi\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}