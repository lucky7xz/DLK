{"textgrid.poem.55723": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wieder durchwindet den hei\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder durchwindet den hei\u00dfen,", "tokens": ["Und", "wie\u00b7der", "durch\u00b7win\u00b7det", "den", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gemarterten K\u00f6rper ein Krampf,", "tokens": ["Ge\u00b7mar\u00b7ter\u00b7ten", "K\u00f6r\u00b7per", "ein", "Krampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Z\u00e4hne die Lippen zerbei\u00dfen,", "tokens": ["Die", "Z\u00e4h\u00b7ne", "die", "Lip\u00b7pen", "zer\u00b7bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Weit offen die Augen erglei\u00dfen,", "tokens": ["Weit", "of\u00b7fen", "die", "Au\u00b7gen", "er\u00b7glei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie schauen nur r\u00f6tlichen Dampf.", "tokens": ["Sie", "schau\u00b7en", "nur", "r\u00f6t\u00b7li\u00b7chen", "Dampf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die tobenden Nerven erlechzen", "tokens": ["Die", "to\u00b7ben\u00b7den", "Ner\u00b7ven", "er\u00b7lech\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Wollust Opiumwein,", "tokens": ["Der", "Wol\u00b7lust", "O\u00b7pium\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das liebeverlangende \u00c4chzen,", "tokens": ["Das", "lie\u00b7be\u00b7ver\u00b7lan\u00b7gen\u00b7de", "\u00c4ch\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun wird es zum Fauchen und Kr\u00e4chzen \u2013", "tokens": ["Nun", "wird", "es", "zum", "Fau\u00b7chen", "und", "Kr\u00e4ch\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Jetzt mu\u00dft Du zu Willen mir sein.", "tokens": ["Jetzt", "mu\u00dft", "Du", "zu", "Wil\u00b7len", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "PPER", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.3": {"line.1": {"text": "Dem seligsten Sterben ergeben,", "tokens": ["Dem", "se\u00b7ligs\u00b7ten", "Ster\u00b7ben", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Verschlungen, verbissen, verkrallt,", "tokens": ["Ver\u00b7schlun\u00b7gen", ",", "ver\u00b7bis\u00b7sen", ",", "ver\u00b7krallt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die brennenden Leiber erbeben \u2013", "tokens": ["Die", "bren\u00b7nen\u00b7den", "Lei\u00b7ber", "er\u00b7be\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und rasend flutet das Leben", "tokens": ["Und", "ra\u00b7send", "flu\u00b7tet", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und w\u00fctet der Liebe Gewalt.", "tokens": ["Und", "w\u00fc\u00b7tet", "der", "Lie\u00b7be", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}