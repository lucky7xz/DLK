{"textgrid.poem.56902": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich einer versunkenen Melodie", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich einer versunkenen Melodie", "tokens": ["Gleich", "ei\u00b7ner", "ver\u00b7sun\u00b7ke\u00b7nen", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "h\u00f6r ich vergangene Tage", "tokens": ["h\u00f6r", "ich", "ver\u00b7gan\u00b7ge\u00b7ne", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "mich umklingen.", "tokens": ["mich", "um\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hei\u00df von Tr\u00e4nen", "tokens": ["Hei\u00df", "von", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "wird mir die Wange,", "tokens": ["wird", "mir", "die", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und von wehm\u00fctigen Seufzern", "tokens": ["und", "von", "weh\u00b7m\u00fc\u00b7ti\u00b7gen", "Seuf\u00b7zern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "schluchzt mir die Brust,", "tokens": ["schluchzt", "mir", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "an der du \u2013", "tokens": ["an", "der", "du", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "ach ", "tokens": ["ach"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "einst dein blondes,", "tokens": ["einst", "dein", "blon\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "ergl\u00fchendes K\u00f6pfchen bargst,", "tokens": ["er\u00b7gl\u00fc\u00b7hen\u00b7des", "K\u00f6pf\u00b7chen", "bargst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "o Geliebte!", "tokens": ["o", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}