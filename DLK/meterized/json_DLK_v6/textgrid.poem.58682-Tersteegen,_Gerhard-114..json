{"textgrid.poem.58682": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine bange L\u00f6wengrub' ist diese Welt den Frommen,", "tokens": ["Wie", "ei\u00b7ne", "ban\u00b7ge", "L\u00f6\u00b7wen\u00b7grub'", "ist", "die\u00b7se", "Welt", "den", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Worin sie in Versuchung stets und viel Gefahren kommen;", "tokens": ["Wo\u00b7rin", "sie", "in", "Ver\u00b7su\u00b7chung", "stets", "und", "viel", "Ge\u00b7fah\u00b7ren", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Der Satan gehet stets umher,", "tokens": ["Der", "Sa\u00b7tan", "ge\u00b7het", "stets", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der grimme Weltgeist br\u00fcllet sehr,", "tokens": ["Der", "grim\u00b7me", "Welt\u00b7geist", "br\u00fcl\u00b7let", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zornmacht, S\u00fcnd' und Eigenheit", "tokens": ["Die", "Zorn\u00b7macht", ",", "S\u00fcnd'", "und", "Ei\u00b7gen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als b\u00f6se Tiere sind nicht weit.", "tokens": ["Als", "b\u00f6\u00b7se", "Tie\u00b7re", "sind", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie soll ich bl\u00f6des Sch\u00e4flein doch entfliehen den Gefahren,", "tokens": ["Wie", "soll", "ich", "bl\u00f6\u00b7des", "Sch\u00e4f\u00b7lein", "doch", "ent\u00b7flie\u00b7hen", "den", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Denn ich durch eigne Kraft und Witz mich nimmer kann bewahren? \u2013", "tokens": ["Denn", "ich", "durch", "eig\u00b7ne", "Kraft", "und", "Witz", "mich", "nim\u00b7mer", "kann", "be\u00b7wah\u00b7ren", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Ich lass' mich selbst und Kreatur,", "tokens": ["Ich", "lass'", "mich", "selbst", "und", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dir, meinem Gott, vertrau' ich nur,", "tokens": ["Dir", ",", "mei\u00b7nem", "Gott", ",", "ver\u00b7trau'", "ich", "nur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Besch\u00fctz' mich in dem finstern Haus", "tokens": ["Be\u00b7sch\u00fctz'", "mich", "in", "dem", "fins\u00b7tern", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und zieh mich ohne Schaden draus!", "tokens": ["Und", "zieh", "mich", "oh\u00b7ne", "Scha\u00b7den", "draus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}