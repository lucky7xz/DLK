{"dta.poem.12528": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Opera posthuma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enichts als Schreiben!\u201c \u2014 Ja, zum Henker!", "tokens": ["\u201e", "nichts", "als", "Schrei\u00b7ben", "!", "\u201c", "Ja", ",", "zum", "Hen\u00b7ker", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIS", "KOKOM", "NN", "$.", "$(", "$(", "PTKANT", "$,", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Doch was rechtet Ihr mit mir?", "tokens": ["Doch", "was", "rech\u00b7tet", "Ihr", "mit", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Machtet Ihr nicht so viel Lumpen,", "tokens": ["Mach\u00b7tet", "Ihr", "nicht", "so", "viel", "Lum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "h\u00e4tt' Ich nicht so viel Papier.", "tokens": ["h\u00e4tt'", "Ich", "nicht", "so", "viel", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber, streichen wir die H\u00e4lfte \u2014", "tokens": ["A\u00b7ber", ",", "strei\u00b7chen", "wir", "die", "H\u00e4lf\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201emit dem Rest, was willst du machen?\u201c", "tokens": ["\u201e", "mit", "dem", "Rest", ",", "was", "willst", "du", "ma\u00b7chen", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nichts, ihr Herrn! es macht mein Knabe", "tokens": ["Nichts", ",", "ihr", "Herrn", "!", "es", "macht", "mein", "Kna\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "einst daraus noch \u2014 einen Drachen.", "tokens": ["einst", "da\u00b7raus", "noch", "ei\u00b7nen", "Dra\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "$(", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}