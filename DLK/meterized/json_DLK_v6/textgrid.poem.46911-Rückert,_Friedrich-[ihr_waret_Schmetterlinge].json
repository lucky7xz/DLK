{"textgrid.poem.46911": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ihr waret Schmetterlinge]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr waret Schmetterlinge", "tokens": ["Ihr", "wa\u00b7ret", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit lichtbegl\u00e4nzter Schwinge,", "tokens": ["Mit", "licht\u00b7be\u00b7gl\u00e4nz\u00b7ter", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hatte keine Luft", "tokens": ["Es", "hat\u00b7te", "kei\u00b7ne", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Genascht an eurem Duft.", "tokens": ["Ge\u00b7nascht", "an", "eu\u00b7rem", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wo ihr das Gefieder", "tokens": ["Und", "wo", "ihr", "das", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch lassen mochtet nieder,", "tokens": ["Auch", "las\u00b7sen", "moch\u00b7tet", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war, wie eu'r Gem\u00fcth,", "tokens": ["Da", "war", ",", "wie", "eu'r", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Rein eine Blum' erbl\u00fcht.", "tokens": ["Rein", "ei\u00b7ne", "Blum'", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr habt nur Duft gesogen,", "tokens": ["Ihr", "habt", "nur", "Duft", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seid in Duft entflogen,", "tokens": ["Und", "seid", "in", "Duft", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Bl\u00fcthenraub gelebt,", "tokens": ["Von", "Bl\u00fc\u00b7then\u00b7raub", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Bl\u00fcthenstaub entschwebt.", "tokens": ["Wie", "Bl\u00fc\u00b7then\u00b7staub", "ent\u00b7schwebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr braucht nicht umzuschweifen", "tokens": ["Ihr", "braucht", "nicht", "um\u00b7zu\u00b7schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Erdstoff abzustreifen;", "tokens": ["Um", "Erd\u00b7stoff", "ab\u00b7zu\u00b7strei\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Euch tr\u00e4gt in gradem Zug", "tokens": ["Euch", "tr\u00e4gt", "in", "gra\u00b7dem", "Zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Himmel euer Flug.", "tokens": ["Zum", "Him\u00b7mel", "eu\u00b7er", "Flug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir andern werden m\u00fcssen", "tokens": ["Wir", "an\u00b7dern", "wer\u00b7den", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "VAINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erst Erdenschuld abb\u00fc\u00dfen,", "tokens": ["Erst", "Er\u00b7den\u00b7schuld", "ab\u00b7b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Eh' wir hin d\u00fcrfen nahn,", "tokens": ["Eh'", "wir", "hin", "d\u00fcr\u00b7fen", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo ihr uns sollt empfahn.", "tokens": ["Wo", "ihr", "uns", "sollt", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}