{"textgrid.poem.33524": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich m\u00f6chte in Nacht mich bergen,", "genre": "verse", "period": "N.A.", "pub_year": 1910, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte in Nacht mich bergen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "in", "Nacht", "mich", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nackt und scheu,", "tokens": ["Nackt", "und", "scheu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und um die Glieder Dunkelheit decken", "tokens": ["Und", "um", "die", "Glie\u00b7der", "Dun\u00b7kel\u00b7heit", "de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und warmen Glanz ...", "tokens": ["Und", "war\u00b7men", "Glanz", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich m\u00f6chte weit hinter die H\u00fcgel der Erde wandern \u2013", "tokens": ["Ich", "m\u00f6ch\u00b7te", "weit", "hin\u00b7ter", "die", "H\u00fc\u00b7gel", "der", "Er\u00b7de", "wan\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-++--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Tief hinter die gleitenden Meere,", "tokens": ["Tief", "hin\u00b7ter", "die", "glei\u00b7ten\u00b7den", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Vorbei den singenden Winden ...", "tokens": ["Vor\u00b7bei", "den", "sin\u00b7gen\u00b7den", "Win\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Dort treff ich die stillen Sterne,", "tokens": ["Dort", "treff", "ich", "die", "stil\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die tragen den Raum durch die Zeit", "tokens": ["Die", "tra\u00b7gen", "den", "Raum", "durch", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Und wohnen am Tode des Seins,", "tokens": ["Und", "woh\u00b7nen", "am", "To\u00b7de", "des", "Seins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Und zwischen ihnen sind graue,", "tokens": ["Und", "zwi\u00b7schen", "ih\u00b7nen", "sind", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Einsame Dinge ...", "tokens": ["Ein\u00b7sa\u00b7me", "Din\u00b7ge", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Welke Bewegung vielleicht", "tokens": ["Wel\u00b7ke", "Be\u00b7we\u00b7gung", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Von Welten, die lange verwesten \u2013", "tokens": ["Von", "Wel\u00b7ten", ",", "die", "lan\u00b7ge", "ver\u00b7wes\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Verlorener Laut \u2013", "tokens": ["Ver\u00b7lo\u00b7re\u00b7ner", "Laut", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "APPR", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Wer will das wissen ...", "tokens": ["Wer", "will", "das", "wis\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Mein blinder Traum wacht fern den W\u00fcnschen der Erde.", "tokens": ["Mein", "blin\u00b7der", "Traum", "wacht", "fern", "den", "W\u00fcn\u00b7schen", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}