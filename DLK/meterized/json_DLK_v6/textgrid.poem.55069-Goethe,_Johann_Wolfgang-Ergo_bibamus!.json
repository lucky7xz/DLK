{"textgrid.poem.55069": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ergo bibamus!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sind wir versammelt zu l\u00f6blichem Tun,", "tokens": ["Hier", "sind", "wir", "ver\u00b7sam\u00b7melt", "zu", "l\u00f6b\u00b7li\u00b7chem", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Drum, Br\u00fcderchen! Ergo bibamus.", "tokens": ["Drum", ",", "Br\u00fc\u00b7der\u00b7chen", "!", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "NE", "NE", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Gl\u00e4ser, sie klingen, Gespr\u00e4che, sie ruhn,", "tokens": ["Die", "Gl\u00e4\u00b7ser", ",", "sie", "klin\u00b7gen", ",", "Ge\u00b7spr\u00e4\u00b7che", ",", "sie", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "$,", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Beherziget Ergo bibamus.", "tokens": ["Be\u00b7her\u00b7zi\u00b7get", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Das hei\u00dft noch ein altes, ein t\u00fcchtiges Wort:", "tokens": ["Das", "hei\u00dft", "noch", "ein", "al\u00b7tes", ",", "ein", "t\u00fcch\u00b7ti\u00b7ges", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es passet zum ersten und passet so fort,", "tokens": ["Es", "pas\u00b7set", "zum", "ers\u00b7ten", "und", "pas\u00b7set", "so", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und schallet ein Echo vom festlichen Ort,", "tokens": ["Und", "schal\u00b7let", "ein", "E\u00b7cho", "vom", "fest\u00b7li\u00b7chen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ein herrliches Ergo bibamus.", "tokens": ["Ein", "herr\u00b7li\u00b7ches", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ich hatte mein freundliches Liebchen gesehn,", "tokens": ["Ich", "hat\u00b7te", "mein", "freund\u00b7li\u00b7ches", "Lieb\u00b7chen", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da dacht ich mir: Ergo bibamus.", "tokens": ["Da", "dacht", "ich", "mir", ":", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und nahte mich traulich; da lie\u00df sie mich stehn.", "tokens": ["Und", "nah\u00b7te", "mich", "trau\u00b7lich", ";", "da", "lie\u00df", "sie", "mich", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich half mir und dachte: Bibamus.", "tokens": ["Ich", "half", "mir", "und", "dach\u00b7te", ":", "Bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$.", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wenn sie vers\u00f6hnet euch herzet und k\u00fc\u00dft", "tokens": ["Und", "wenn", "sie", "ver\u00b7s\u00f6h\u00b7net", "euch", "her\u00b7zet", "und", "k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und wenn ihr das Herzen und K\u00fcssen vermi\u00dft,", "tokens": ["Und", "wenn", "ihr", "das", "Her\u00b7zen", "und", "K\u00fcs\u00b7sen", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "So bleibet nur, bis ihr was Besseres wi\u00dft,", "tokens": ["So", "blei\u00b7bet", "nur", ",", "bis", "ihr", "was", "Bes\u00b7se\u00b7res", "wi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PIS", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Beim tr\u00f6stlichen Ergo bibamus.", "tokens": ["Beim", "tr\u00f6st\u00b7li\u00b7chen", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Mich ruft mein Geschick von den Freunden hinweg;", "tokens": ["Mich", "ruft", "mein", "Ge\u00b7schick", "von", "den", "Freun\u00b7den", "hin\u00b7weg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr Redlichen! Ergo bibamus.", "tokens": ["Ihr", "Red\u00b7li\u00b7chen", "!", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NE", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich scheide von hinnen mit leichtem Gep\u00e4ck;", "tokens": ["Ich", "schei\u00b7de", "von", "hin\u00b7nen", "mit", "leich\u00b7tem", "Ge\u00b7p\u00e4ck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drum doppeltes Ergo bibamus.", "tokens": ["Drum", "dop\u00b7pel\u00b7tes", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und was auch der Filz von dem Leibe sich schmorgt,", "tokens": ["Und", "was", "auch", "der", "Filz", "von", "dem", "Lei\u00b7be", "sich", "schmorgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So bleibt f\u00fcr den Heitern doch immer gesorgt,", "tokens": ["So", "bleibt", "f\u00fcr", "den", "Hei\u00b7tern", "doch", "im\u00b7mer", "ge\u00b7sorgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Weil immer dem Frohen der Fr\u00f6hliche borgt;", "tokens": ["Weil", "im\u00b7mer", "dem", "Fro\u00b7hen", "der", "Fr\u00f6h\u00b7li\u00b7che", "borgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Drum, Br\u00fcderchen! Ergo bibamus.", "tokens": ["Drum", ",", "Br\u00fc\u00b7der\u00b7chen", "!", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "NE", "NE", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Was sollen wir sagen zum heutigen Tag!", "tokens": ["Was", "sol\u00b7len", "wir", "sa\u00b7gen", "zum", "heu\u00b7ti\u00b7gen", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich d\u00e4chte nur: Ergo bibamus.", "tokens": ["Ich", "d\u00e4ch\u00b7te", "nur", ":", "Er\u00b7go", "bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er ist nun einmal von besonderem Schlag;", "tokens": ["Er", "ist", "nun", "ein\u00b7mal", "von", "be\u00b7son\u00b7de\u00b7rem", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Drum immer aufs neue: Bibamus.", "tokens": ["Drum", "im\u00b7mer", "aufs", "neu\u00b7e", ":", "Bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ADV", "APPRART", "ADJA", "$.", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er f\u00fchret die Freude durchs offene Tor,", "tokens": ["Er", "f\u00fch\u00b7ret", "die", "Freu\u00b7de", "durchs", "of\u00b7fe\u00b7ne", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es gl\u00e4nzen die Wolken, es teilt sich der Flor,", "tokens": ["Es", "gl\u00e4n\u00b7zen", "die", "Wol\u00b7ken", ",", "es", "teilt", "sich", "der", "Flor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Da scheint uns ein Bildchen, ein g\u00f6ttliches, vor;", "tokens": ["Da", "scheint", "uns", "ein", "Bild\u00b7chen", ",", "ein", "g\u00f6tt\u00b7li\u00b7ches", ",", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wir klingen und singen: Bibamus.", "tokens": ["Wir", "klin\u00b7gen", "und", "sin\u00b7gen", ":", "Bi\u00b7ba\u00b7mus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}