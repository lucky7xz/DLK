{"dta.poem.22756": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "61. Die verlohrne und wider gefun-  \n dene Drey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Der Groschen/ Sohn/ unds Schaaff/ bin ich mit", "tokens": ["Der", "Gro\u00b7schen", "/", "Sohn", "/", "unds", "Schaaff", "/", "bin", "ich", "mit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "VAFIN", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Geist/ Leib/ Seele.", "tokens": ["Geist", "/", "Leib", "/", "See\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Verlohrn in frembdem Land/ in einer W\u00fcst\u2019/ un\u0303 H\u00f6le.", "tokens": ["Ver\u00b7lohrn", "in", "fremb\u00b7dem", "Land", "/", "in", "ei\u00b7ner", "W\u00fcst'", "/", "u\u00f1", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$(", "APPR", "ART", "NN", "$(", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die heilige Dreyfalt kombt un\u0303 sucht mich alle stunde\u0303:", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Drey\u00b7falt", "kombt", "u\u00f1", "sucht", "mich", "al\u00b7le", "stun\u00b7d\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Den Groschen sindt der Geist/ der Vatter nimbt den", "tokens": ["Den", "Gro\u00b7schen", "sindt", "der", "Geist", "/", "der", "Vat\u00b7ter", "nimbt", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sohn/", "tokens": ["Sohn", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der Hirte JEsus tr\u00e4gt da\u00df Schaaff mit sich davon.", "tokens": ["Der", "Hir\u00b7te", "Je\u00b7sus", "tr\u00e4gt", "da\u00df", "Schaaff", "mit", "sich", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "KOUS", "NN", "APPR", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schau wie ich Dreyfach bin verlohren und gefunden!", "tokens": ["Schau", "wie", "ich", "Drey\u00b7fach", "bin", "ver\u00b7loh\u00b7ren", "und", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "PAV", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}}}}