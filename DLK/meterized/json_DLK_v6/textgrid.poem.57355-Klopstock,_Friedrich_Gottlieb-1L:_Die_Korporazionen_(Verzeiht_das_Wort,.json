{"textgrid.poem.57355": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Korporazionen (Verzeiht das Wort,", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Korporazionen (Verzeiht das Wort,", "tokens": ["Die", "Kor\u00b7po\u00b7ra\u00b7zi\u00b7o\u00b7nen", "(", "Ver\u00b7zeiht", "das", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das schlecht ist, wie die Sache.) vernichtete", "tokens": ["Das", "schlecht", "ist", ",", "wie", "die", "Sa\u00b7che", ".", ")", "ver\u00b7nich\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["PDS", "ADJD", "VAFIN", "$,", "PWAV", "ART", "NN", "$.", "$(", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das freye Frankreich; durchgehauen,", "tokens": ["Das", "frey\u00b7e", "Fran\u00b7kreich", ";", "durch\u00b7ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$.", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuckten im Sande die kleinen Schlangen.", "tokens": ["Zuck\u00b7ten", "im", "San\u00b7de", "die", "klei\u00b7nen", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und doch erhob sich neben den liegenden", "tokens": ["Und", "doch", "er\u00b7hob", "sich", "ne\u00b7ben", "den", "lie\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Korporazion, der Jakoberklob!", "tokens": ["Die", "Kor\u00b7po\u00b7ra\u00b7zi\u00b7on", ",", "der", "Ja\u00b7ko\u00b7ber\u00b7klob", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Kopf durchrast Paris, und ihre", "tokens": ["Ihr", "Kopf", "durc\u00b7hrast", "Pa\u00b7ris", ",", "und", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$,", "KON", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4ngelung windet sich durch ganz Frankreich.", "tokens": ["Schl\u00e4n\u00b7ge\u00b7lung", "win\u00b7det", "sich", "durch", "ganz", "Fran\u00b7kreich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADV", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ha, t\u00e4ubet euch denn Taubheit? vernehmt ihr nicht,", "tokens": ["Ha", ",", "t\u00e4u\u00b7bet", "euch", "denn", "Taub\u00b7heit", "?", "ver\u00b7nehmt", "ihr", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie sie aus ihrem scheusslichen Innersten", "tokens": ["Wie", "sie", "aus", "ih\u00b7rem", "scheuss\u00b7li\u00b7chen", "In\u00b7ners\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Musik begint, die selten zweymal", "tokens": ["Mu\u00b7sik", "be\u00b7gint", ",", "die", "sel\u00b7ten", "zwey\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ADJD", "ADV"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.4": {"text": "H\u00f6rte der Wanderer? wie sie klappert?", "tokens": ["H\u00f6r\u00b7te", "der", "Wan\u00b7de\u00b7rer", "?", "wie", "sie", "klap\u00b7pert", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Treibt ihr die Riesenschlang' in die H\u00f6hle nicht", "tokens": ["Treibt", "ihr", "die", "Rie\u00b7sen\u00b7schlang'", "in", "die", "H\u00f6h\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur\u00fcck, und w\u00e4lzt nicht Felsen dem Schlunde vor;", "tokens": ["Zu\u00b7r\u00fcck", ",", "und", "w\u00e4lzt", "nicht", "Fel\u00b7sen", "dem", "Schlun\u00b7de", "vor", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So wird ihr Geiferbiss die Freyheit,", "tokens": ["So", "wird", "ihr", "Gei\u00b7fer\u00b7biss", "die", "Frey\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welch' ihr erschuft, in den Staub euch st\u00fcrzen.", "tokens": ["Welch'", "ihr", "er\u00b7schuft", ",", "in", "den", "Staub", "euch", "st\u00fcr\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}