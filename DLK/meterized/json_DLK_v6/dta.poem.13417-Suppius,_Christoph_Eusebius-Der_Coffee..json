{"dta.poem.13417": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Coffee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Edler Saft! dich soll ich missen!", "tokens": ["Ed\u00b7ler", "Saft", "!", "dich", "soll", "ich", "mis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bohnen-Trank! du wirst mir nun", "tokens": ["Boh\u00b7nen\u00b7Trank", "!", "du", "wirst", "mir", "nun"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf wie lange Zeit entrissen,", "tokens": ["Auf", "wie", "lan\u00b7ge", "Zeit", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O! wie werd ich \u00e4ngstlich thun!", "tokens": ["O", "!", "wie", "werd", "ich", "\u00e4ngst\u00b7lich", "thun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey dem bangen Seigerschlage", "tokens": ["Bey", "dem", "ban\u00b7gen", "Sei\u00b7ger\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird die wiederholte Klage", "tokens": ["Wird", "die", "wie\u00b7der\u00b7hol\u00b7te", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Einsamkeit allein", "tokens": ["In", "der", "Ein\u00b7sam\u00b7keit", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein betr\u00fcbter Tr\u00f6ster seyn!", "tokens": ["Mein", "be\u00b7tr\u00fcb\u00b7ter", "Tr\u00f6s\u00b7ter", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Braunes Labsal! Balsam-Tropfen!", "tokens": ["Brau\u00b7nes", "Lab\u00b7sal", "!", "Bal\u00b7sam\u00b7Trop\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Seelen Elexir!", "tokens": ["Mei\u00b7ner", "See\u00b7len", "El\u00b7e\u00b7xir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Machst du andern Herzens-Klopfen,", "tokens": ["Machst", "du", "an\u00b7dern", "Her\u00b7zens\u00b7Klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O! wie wohl bekamst du mir!", "tokens": ["O", "!", "wie", "wohl", "be\u00b7kamst", "du", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich, sobald ich dich getrunken,", "tokens": ["Gleich", ",", "so\u00b7bald", "ich", "dich", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "War auch alles Leid versunken,", "tokens": ["War", "auch", "al\u00b7les", "Leid", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil dein Wasser immerdar", "tokens": ["Weil", "dein", "Was\u00b7ser", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Meiner Grillen S\u00fcndfluth war.", "tokens": ["Mei\u00b7ner", "Gril\u00b7len", "S\u00fcnd\u00b7fluth", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie entz\u00fcckend war das Sehen,", "tokens": ["Wie", "ent\u00b7z\u00fc\u00b7ckend", "war", "das", "Se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich mir die ganze Welt,", "tokens": ["Wenn", "ich", "mir", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey dem Auf- und Niedergehen,", "tokens": ["Bey", "dem", "Auf", "und", "Nie\u00b7der\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jm Coffee-Tisch f\u00fcrgestellt!", "tokens": ["Jm", "Cof\u00b7fee\u00b7Tisch", "f\u00fcr\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Du gefielst mir auf die Weise,", "tokens": ["Du", "ge\u00b7fielst", "mir", "auf", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Ulyssen nach der Reise", "tokens": ["Wie", "U\u00b7lys\u00b7sen", "nach", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Jener Rauch aus Jthaka,", "tokens": ["Je\u00b7ner", "Rauch", "aus", "Jtha\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wenn ich deine D\u00fcnste sah.", "tokens": ["Wenn", "ich", "dei\u00b7ne", "D\u00fcns\u00b7te", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jhr seyd Ursach, liebste Bohnen!", "tokens": ["Ihr", "seyd", "Ur\u00b7sach", ",", "liebs\u00b7te", "Boh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich \u00f6fters nachgedacht,", "tokens": ["Da\u00df", "ich", "\u00f6f\u00b7ters", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo doch jene V\u00f6lker wohnen,", "tokens": ["Wo", "doch", "je\u00b7ne", "V\u00f6l\u00b7ker", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da man euch hervorgebracht;", "tokens": ["Da", "man", "euch", "her\u00b7vor\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Araber und kluge T\u00fcrken!", "tokens": ["A\u00b7ra\u00b7ber", "und", "klu\u00b7ge", "T\u00fcr\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "H\u00f6rt, was eure Fr\u00fcchte wirken!", "tokens": ["H\u00f6rt", ",", "was", "eu\u00b7re", "Fr\u00fcch\u00b7te", "wir\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ihr Saft, wenn sie gebrannt,", "tokens": ["Denn", "ihr", "Saft", ",", "wenn", "sie", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchret mich in euer Land.", "tokens": ["F\u00fch\u00b7ret", "mich", "in", "eu\u00b7er", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Habet Dank, ihr lieben Leute,", "tokens": ["Ha\u00b7bet", "Dank", ",", "ihr", "lie\u00b7ben", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr euch f\u00fcr mich bem\u00fcht;", "tokens": ["Die", "ihr", "euch", "f\u00fcr", "mich", "be\u00b7m\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure Wohlthat sp\u00fchr ich heute", "tokens": ["Eu\u00b7re", "Wohlt\u00b7hat", "sp\u00fchr", "ich", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem wallenden Gebl\u00fct.", "tokens": ["In", "dem", "wal\u00b7len\u00b7den", "Ge\u00b7bl\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, ihr Freunde! la\u00dft euch rathen!", "tokens": ["Doch", ",", "ihr", "Freun\u00b7de", "!", "la\u00dft", "euch", "ra\u00b7then", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schickt ihr euch zu neuen Saaten,", "tokens": ["Schickt", "ihr", "euch", "zu", "neu\u00b7en", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So la\u00dft einen Acker leer,", "tokens": ["So", "la\u00dft", "ei\u00b7nen", "A\u00b7cker", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Ich will keine Bohnen mehr.", "tokens": ["Ich", "will", "kei\u00b7ne", "Boh\u00b7nen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Die ihr auf beschwerten Bretern", "tokens": ["Die", "ihr", "auf", "be\u00b7schwer\u00b7ten", "Bre\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche wundersch\u00f6ne Frucht,", "tokens": ["Sol\u00b7che", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Furcht vor Donnerwettern,", "tokens": ["Oh\u00b7ne", "Furcht", "vor", "Don\u00b7ner\u00b7wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Morgenl\u00e4ndern sucht,", "tokens": ["In", "den", "Mor\u00b7gen\u00b7l\u00e4n\u00b7dern", "sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr habt mich bisher versehen,", "tokens": ["Ihr", "habt", "mich", "bis\u00b7her", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "La\u00dft euch nun die Lust vergehen,", "tokens": ["La\u00dft", "euch", "nun", "die", "Lust", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Macht das Schiff nicht mehr so voll,", "tokens": ["Macht", "das", "Schiff", "nicht", "mehr", "so", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil ich k\u00fcnstig fasten soll.", "tokens": ["Weil", "ich", "k\u00fcns\u00b7tig", "fas\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist es m\u00f6glich? soll ich glauben,", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", "?", "soll", "ich", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df P \u2012 \u2012 mich dieser Lust", "tokens": ["Da\u00df", "P", "\u2012", "\u2012", "mich", "die\u00b7ser", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$(", "$(", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denkt auf ewig zu berauben?", "tokens": ["Denkt", "auf", "e\u00b7wig", "zu", "be\u00b7rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O h\u00e4tt ich das Ding gewu\u00dft!", "tokens": ["O", "h\u00e4tt", "ich", "das", "Ding", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eolus, der Herr der Winde,", "tokens": ["E\u00b7o\u00b7lus", ",", "der", "Herr", "der", "Win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4tte mir, als seinem Kinde,", "tokens": ["H\u00e4t\u00b7te", "mir", ",", "als", "sei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einen lauen West geschickt,", "tokens": ["Ei\u00b7nen", "lau\u00b7en", "West", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der mich in mein Land ger\u00fcckt.", "tokens": ["Der", "mich", "in", "mein", "Land", "ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Pfeife, rauchender Gefehrde!", "tokens": ["Pfei\u00b7fe", ",", "rau\u00b7chen\u00b7der", "Ge\u00b7fehr\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O! wie wird es um uns stehn,", "tokens": ["O", "!", "wie", "wird", "es", "um", "uns", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich mit dir schelten werde,", "tokens": ["Wenn", "ich", "mit", "dir", "schel\u00b7ten", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da ich dich soll m\u00fcssig sehn!", "tokens": ["Da", "ich", "dich", "soll", "m\u00fcs\u00b7sig", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, ich kenne deinen Willen,", "tokens": ["Doch", ",", "ich", "ken\u00b7ne", "dei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du vertriebest mir die Grillen;", "tokens": ["Du", "ver\u00b7trie\u00b7best", "mir", "die", "Gril\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber, denk auf Fall und Sturz,", "tokens": ["A\u00b7ber", ",", "denk", "auf", "Fall", "und", "Sturz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Brich! mir fehlet Coffee! kurz!", "tokens": ["Brich", "!", "mir", "feh\u00b7let", "Cof\u00b7fee", "!", "kurz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "NN", "$.", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Leute! macht kein solch Get\u00f6se!", "tokens": ["Leu\u00b7te", "!", "macht", "kein", "solch", "Ge\u00b7t\u00f6\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PIAT", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute bin ich voll Verdru\u00df,", "tokens": ["Heu\u00b7te", "bin", "ich", "voll", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eigensinnig, zornig, b\u00f6se,", "tokens": ["Ei\u00b7gen\u00b7sin\u00b7nig", ",", "zor\u00b7nig", ",", "b\u00f6\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich Coffee missen mu\u00df;", "tokens": ["Weil", "ich", "Cof\u00b7fee", "mis\u00b7sen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freunde! haltet mir zu gute,", "tokens": ["Freun\u00b7de", "!", "hal\u00b7tet", "mir", "zu", "gu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn bey unterdr\u00fccktem Muthe", "tokens": ["Wenn", "bey", "un\u00b7ter\u00b7dr\u00fcck\u00b7tem", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein sonst aufgeweckter Geist", "tokens": ["Mein", "sonst", "auf\u00b7ge\u00b7weck\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Euch die schwarzen Z\u00e4hne weist.", "tokens": ["Euch", "die", "schwar\u00b7zen", "Z\u00e4h\u00b7ne", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fort, ihr B\u00fccher! weg Gedanken,", "tokens": ["Fort", ",", "ihr", "B\u00fc\u00b7cher", "!", "weg", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr mich sonst aufgeweckt!", "tokens": ["Die", "ihr", "mich", "sonst", "auf\u00b7ge\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir geschieht wie denen Kranken,", "tokens": ["Mir", "ge\u00b7schieht", "wie", "de\u00b7nen", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PDS", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die der Kummer hingestreckt!", "tokens": ["Die", "der", "Kum\u00b7mer", "hin\u00b7ge\u00b7streckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4nge, Leyer! springt, ihr Sayten!", "tokens": ["H\u00e4n\u00b7ge", ",", "Le\u00b7yer", "!", "springt", ",", "ihr", "Say\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich gel\u00fcstet nach den Zeiten,", "tokens": ["Mich", "ge\u00b7l\u00fcs\u00b7tet", "nach", "den", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da ich mit Coffee erqvickt,", "tokens": ["Da", "ich", "mit", "Cof\u00b7fee", "er\u00b7qvickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Was das Herzeleid erstickt.", "tokens": ["Was", "das", "Her\u00b7ze\u00b7leid", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie? was steigt mir in die Nasen?", "tokens": ["Wie", "?", "was", "steigt", "mir", "in", "die", "Na\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein lieblicher Geruch", "tokens": ["Welch", "ein", "lieb\u00b7li\u00b7cher", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirkt in mir ein k\u00fctzelnd Rasen?", "tokens": ["Wirkt", "in", "mir", "ein", "k\u00fct\u00b7zelnd", "Ra\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O! itzt \u00e4ndert sich der Spruch!", "tokens": ["O", "!", "itzt", "\u00e4n\u00b7dert", "sich", "der", "Spruch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Angenehme Botschafts-F\u00fcsse!", "tokens": ["An\u00b7ge\u00b7neh\u00b7me", "Bot\u00b7schafts\u00b7F\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt, und macht die Stunden s\u00fcsse,", "tokens": ["Kommt", ",", "und", "macht", "die", "Stun\u00b7den", "s\u00fcs\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gnug, mein letzter Groschen klingt,", "tokens": ["Gnug", ",", "mein", "letz\u00b7ter", "Gro\u00b7schen", "klingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da ihr mir mein Labsal bringt.", "tokens": ["Da", "ihr", "mir", "mein", "Lab\u00b7sal", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "H\u00f6rt ihrs, Br\u00fcder! lasst es bleiben!", "tokens": ["H\u00f6rt", "ihrs", ",", "Br\u00fc\u00b7der", "!", "lasst", "es", "blei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$.", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasst die Aecker, wie sie sind!", "tokens": ["Lasst", "die", "A\u00b7e\u00b7cker", ",", "wie", "sie", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihr Handlung pflegt zu treiben,", "tokens": ["Die", "ihr", "Hand\u00b7lung", "pflegt", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch verfolge guter Wind;", "tokens": ["Euch", "ver\u00b7fol\u00b7ge", "gu\u00b7ter", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Anders hab ich mich besonnen,", "tokens": ["An\u00b7ders", "hab", "ich", "mich", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil die Hoffnung Krast gewonnen,", "tokens": ["Weil", "die", "Hoff\u00b7nung", "Krast", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich, was mir Guts gethan,", "tokens": ["Da\u00df", "ich", ",", "was", "mir", "Guts", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fcnftig noch geniessen kann.", "tokens": ["K\u00fcnf\u00b7tig", "noch", "ge\u00b7nies\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Kommt, ihr B\u00fccher! lustig, Grillen!", "tokens": ["Kommt", ",", "ihr", "B\u00fc\u00b7cher", "!", "lus\u00b7tig", ",", "Gril\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schw\u00e4rmet! neue Pfeifen her!", "tokens": ["Schw\u00e4r\u00b7met", "!", "neu\u00b7e", "Pfei\u00b7fen", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gram und Kummer l\u00e4sst sich stillen", "tokens": ["Gram", "und", "Kum\u00b7mer", "l\u00e4sst", "sich", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die\u00df fliessende Gewehr!", "tokens": ["Durch", "die\u00df", "flies\u00b7sen\u00b7de", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey Verlust des Theils am Erbe!", "tokens": ["Bey", "Ver\u00b7lust", "des", "Theils", "am", "Er\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kinder! wenn ich einsten sterbe!", "tokens": ["Kin\u00b7der", "!", "wenn", "ich", "eins\u00b7ten", "ster\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lasst mein Grab und Leichenstein", "tokens": ["Lasst", "mein", "Grab", "und", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Rund, wie der Coffeetisch seyn.", "tokens": ["Rund", ",", "wie", "der", "Cof\u00b7fee\u00b7tisch", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Sollt ich auch die Str\u00fcmpfe missen,", "tokens": ["Sollt", "ich", "auch", "die", "Str\u00fcmp\u00b7fe", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gnug, der Coffee schmeckt mir nun!", "tokens": ["Gnug", ",", "der", "Cof\u00b7fee", "schmeckt", "mir", "nun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es hie und da zerrissen,", "tokens": ["Ist", "es", "hie", "und", "da", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ich mir nur g\u00fctlich thun!", "tokens": ["Kann", "ich", "mir", "nur", "g\u00fct\u00b7lich", "thun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Findet bey dem Glockenschlage", "tokens": ["Fin\u00b7det", "bey", "dem", "Glo\u00b7cken\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich in Zukunft Noth und Plage,", "tokens": ["Sich", "in", "Zu\u00b7kunft", "Noth", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gebt mir Coffee! der allein", "tokens": ["Gebt", "mir", "Cof\u00b7fee", "!", "der", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "$.", "ART", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll mein bester Tr\u00f6ster seyn.", "tokens": ["Soll", "mein", "bes\u00b7ter", "Tr\u00f6s\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}