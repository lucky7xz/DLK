{"textgrid.poem.55667": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Buch Suleika", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte dieses Buch wohl gern zusammensch\u00fcrzen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "die\u00b7ses", "Buch", "wohl", "gern", "zu\u00b7sam\u00b7men\u00b7sch\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es den andern w\u00e4re gleich geschn\u00fcrt.", "tokens": ["Da\u00df", "es", "den", "an\u00b7dern", "w\u00e4\u00b7re", "gleich", "ge\u00b7schn\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Allein wie willst du Wort und Blatt verk\u00fcrzen,", "tokens": ["Al\u00b7lein", "wie", "willst", "du", "Wort", "und", "Blatt", "ver\u00b7k\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Liebeswahnsinn dich ins Weite f\u00fchrt?", "tokens": ["Wenn", "Lie\u00b7bes\u00b7wahn\u00b7sinn", "dich", "ins", "Wei\u00b7te", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}