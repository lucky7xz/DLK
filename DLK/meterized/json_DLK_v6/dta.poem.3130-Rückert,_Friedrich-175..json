{"dta.poem.3130": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "175.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ungelesen ich zu lassen mir erlaube?", "tokens": ["Was", "un\u00b7ge\u00b7le\u00b7sen", "ich", "zu", "las\u00b7sen", "mir", "er\u00b7lau\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein B\u00fcchlein, das mir will beweisen, was ich glaube.", "tokens": ["Ein", "B\u00fcch\u00b7lein", ",", "das", "mir", "will", "be\u00b7wei\u00b7sen", ",", "was", "ich", "glau\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sollt' ich, was ich glaub', erst mir beweisen lassen?", "tokens": ["Wie", "sollt'", "ich", ",", "was", "ich", "glaub'", ",", "erst", "mir", "be\u00b7wei\u00b7sen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Derweilen kann ich mich mit N\u00fctzlicherm befassen.", "tokens": ["Der\u00b7wei\u00b7len", "kann", "ich", "mich", "mit", "N\u00fctz\u00b7li\u00b7cherm", "be\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich denke, solches Buch ist nicht f\u00fcr mich geschrieben,", "tokens": ["Ich", "den\u00b7ke", ",", "sol\u00b7ches", "Buch", "ist", "nicht", "f\u00fcr", "mich", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIAT", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist f\u00fcr andre, die bis jetzt unglaubig blieben.", "tokens": ["Es", "ist", "f\u00fcr", "and\u00b7re", ",", "die", "bis", "jetzt", "un\u00b7glau\u00b7big", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Allein auch diese wird es nicht zum Glauben treiben;", "tokens": ["Al\u00b7lein", "auch", "die\u00b7se", "wird", "es", "nicht", "zum", "Glau\u00b7ben", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum ohne Schaden konnt' es ungeschrieben bleiben.", "tokens": ["Drum", "oh\u00b7ne", "Scha\u00b7den", "konnt'", "es", "un\u00b7ge\u00b7schrie\u00b7ben", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}