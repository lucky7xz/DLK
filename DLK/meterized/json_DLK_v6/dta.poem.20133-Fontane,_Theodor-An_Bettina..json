{"dta.poem.20133": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Bettina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Geist nimmt wie auf Lerchenschwingen", "tokens": ["Dein", "Geist", "nimmt", "wie", "auf", "Ler\u00b7chen\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "APPR", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Tief in den Himmel seinen Zug,", "tokens": ["Tief", "in", "den", "Him\u00b7mel", "sei\u00b7nen", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und freudig lausch\u2019 ich seinem Singen,", "tokens": ["Und", "freu\u00b7dig", "lausch'", "ich", "sei\u00b7nem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und freudig folg\u2019 ich seinem Flug.", "tokens": ["Und", "freu\u00b7dig", "fol\u00b7g'", "ich", "sei\u00b7nem", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Doch wie die Lerch\u2019 auf ihren Z\u00fcgen", "tokens": ["Doch", "wie", "die", "Ler\u00b7ch'", "auf", "ih\u00b7ren", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Oftmals im Aether mir verschwimmt,", "tokens": ["Oft\u00b7mals", "im", "A\u00b7e\u00b7ther", "mir", "ver\u00b7schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "---++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "So auch Dein Geist auf seinen Fl\u00fcgen,", "tokens": ["So", "auch", "Dein", "Geist", "auf", "sei\u00b7nen", "Fl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er zu hoch in\u2019s Blaue klimmt.", "tokens": ["Wenn", "er", "zu", "hoch", "in's", "Blau\u00b7e", "klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}