{"textgrid.poem.62347": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Die Dauben", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh M\u00e4dchen sieh die Dauben,", "tokens": ["Sieh", "M\u00e4d\u00b7chen", "sieh", "die", "Dau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort auf dem Ast in Bl\u00e4ttern,", "tokens": ["Dort", "auf", "dem", "Ast", "in", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh wie die Daube seufzend", "tokens": ["Sieh", "wie", "die", "Dau\u00b7be", "seuf\u00b7zend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich an den D\u00e4uber schmieget,", "tokens": ["Sich", "an", "den", "D\u00e4u\u00b7ber", "schmie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sieh wie der D\u00e4uber sanfte", "tokens": ["Sieh", "wie", "der", "D\u00e4u\u00b7ber", "sanf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie mit den Fl\u00fcgeln schl\u00e4get.", "tokens": ["Sie", "mit", "den", "Fl\u00fc\u00b7geln", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sieh M\u00e4dgen, ach! sie schn\u00e4beln!", "tokens": ["Sieh", "M\u00e4d\u00b7gen", ",", "ach", "!", "sie", "schn\u00e4\u00b7beln", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ITJ", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und jezt, ach! sieh \u2013 \u2013 die Daube!", "tokens": ["Und", "jezt", ",", "ach", "!", "sieh", "\u2013", "\u2013", "die", "Dau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ITJ", "$.", "VVIMP", "$(", "$(", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Du spr\u00f6des b\u00f6ses M\u00e4dgen.", "tokens": ["Du", "spr\u00f6\u00b7des", "b\u00f6\u00b7ses", "M\u00e4d\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}