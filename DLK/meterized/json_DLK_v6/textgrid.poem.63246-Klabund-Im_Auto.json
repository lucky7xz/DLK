{"textgrid.poem.63246": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Im Auto", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gut und fahr im Gl\u00fcck.", "tokens": ["Ich", "bin", "gut", "und", "fahr", "im", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den nassen Scheiben", "tokens": ["Von", "den", "nas\u00b7sen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Klatschen Blicke dumpf zur\u00fcck,", "tokens": ["Klat\u00b7schen", "Bli\u00b7cke", "dumpf", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die wie V\u00f6gel treiben.", "tokens": ["Die", "wie", "V\u00f6\u00b7gel", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alles rollt an mir vorbei.", "tokens": ["Al\u00b7les", "rollt", "an", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber die Kan\u00e4le", "tokens": ["\u00dc\u00b7ber", "die", "Ka\u00b7n\u00e4\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Irr ich wie ein b\u00f6ser Schrei,", "tokens": ["Irr", "ich", "wie", "ein", "b\u00f6\u00b7ser", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich mir verhehle.", "tokens": ["Den", "ich", "mir", "ver\u00b7heh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich bin ich nicht mehr da.", "tokens": ["Pl\u00f6tz\u00b7lich", "bin", "ich", "nicht", "mehr", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Motor platzt im Dunkeln.", "tokens": ["Mo\u00b7tor", "platzt", "im", "Dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich sehe sausend nah,", "tokens": ["Und", "ich", "se\u00b7he", "sau\u00b7send", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tod, dein Auge funkeln.", "tokens": ["Tod", ",", "dein", "Au\u00b7ge", "fun\u00b7keln", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}