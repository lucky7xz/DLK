{"textgrid.poem.33442": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einmal, Mutter, dich erblickt,", "tokens": ["Wer", "ein\u00b7mal", ",", "Mut\u00b7ter", ",", "dich", "er\u00b7blickt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird vom Verderben nie bestrickt,", "tokens": ["Wird", "vom", "Ver\u00b7der\u00b7ben", "nie", "be\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trennung von dir mu\u00df ihn betr\u00fcben,", "tokens": ["Tren\u00b7nung", "von", "dir", "mu\u00df", "ihn", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewig wird er dich br\u00fcnstig lieben", "tokens": ["E\u00b7wig", "wird", "er", "dich", "br\u00fcns\u00b7tig", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und deiner Huld Erinnerung", "tokens": ["Und", "dei\u00b7ner", "Huld", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bleibt fortan seines Geistes h\u00f6chster Schwung.", "tokens": ["Bleibt", "for\u00b7tan", "sei\u00b7nes", "Geis\u00b7tes", "h\u00f6chs\u00b7ter", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich mein' es herzlich gut mit dir.", "tokens": ["Ich", "mein'", "es", "herz\u00b7lich", "gut", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wir gebricht, siehst du in mir.", "tokens": ["Was", "wir", "ge\u00b7bricht", ",", "siehst", "du", "in", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df, s\u00fc\u00dfe Mutter, dich erweichen,", "tokens": ["La\u00df", ",", "s\u00fc\u00b7\u00dfe", "Mut\u00b7ter", ",", "dich", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einmal gieb mir ein frohes Zeichen.", "tokens": ["Ein\u00b7mal", "gieb", "mir", "ein", "fro\u00b7hes", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Mein ganzes Daseyn ruht in dir,", "tokens": ["Mein", "gan\u00b7zes", "Da\u00b7seyn", "ruht", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur einen Augenblick sey du bei mir.", "tokens": ["Nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "sey", "du", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oft, wenn ich tr\u00e4umte, sah ich dich", "tokens": ["Oft", ",", "wenn", "ich", "tr\u00e4um\u00b7te", ",", "sah", "ich", "dich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00f6n, so herzensinniglich,", "tokens": ["So", "sch\u00f6n", ",", "so", "her\u00b7zen\u00b7sin\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der kleine Gott auf deinen Armen", "tokens": ["Der", "klei\u00b7ne", "Gott", "auf", "dei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wollt' des Gespielen sich erbarmen;", "tokens": ["Wollt'", "des", "Ge\u00b7spie\u00b7len", "sich", "er\u00b7bar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Du aber hobst den hehren Blick", "tokens": ["Du", "a\u00b7ber", "hobst", "den", "heh\u00b7ren", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gingst in tiefe Wolkenpracht zur\u00fcck:", "tokens": ["Und", "gingst", "in", "tie\u00b7fe", "Wol\u00b7ken\u00b7pracht", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was hab' ich, Armer, dir gethan?", "tokens": ["Was", "hab'", "ich", ",", "Ar\u00b7mer", ",", "dir", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch bet' ich dich voll Sehnsucht an,", "tokens": ["Noch", "bet'", "ich", "dich", "voll", "Sehn\u00b7sucht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind deine heiligen Kapellen", "tokens": ["Sind", "dei\u00b7ne", "hei\u00b7li\u00b7gen", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht meines Lebens Ruhestellen?", "tokens": ["Nicht", "mei\u00b7nes", "Le\u00b7bens", "Ru\u00b7he\u00b7stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gebenedeite K\u00f6niginn", "tokens": ["Ge\u00b7be\u00b7ne\u00b7dei\u00b7te", "K\u00f6\u00b7ni\u00b7ginn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimm dieses Herz mit diesem Leben hin.", "tokens": ["Nimm", "die\u00b7ses", "Herz", "mit", "die\u00b7sem", "Le\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du wei\u00dft, geliebte K\u00f6niginn,", "tokens": ["Du", "wei\u00dft", ",", "ge\u00b7lieb\u00b7te", "K\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich so ganz dein eigen bin.", "tokens": ["Wie", "ich", "so", "ganz", "dein", "ei\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ich nicht schon seit langen Jahren", "tokens": ["Hab'", "ich", "nicht", "schon", "seit", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Stillen deine Huld erfahren?", "tokens": ["Im", "Stil\u00b7len", "dei\u00b7ne", "Huld", "er\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als ich kaum meiner noch bewu\u00dft,", "tokens": ["Als", "ich", "kaum", "mei\u00b7ner", "noch", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sog ich schon Milch aus deiner selgen Brust.", "tokens": ["Sog", "ich", "schon", "Milch", "aus", "dei\u00b7ner", "sel\u00b7gen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Unz\u00e4hligmal standst du bei mir,", "tokens": ["Un\u00b7z\u00e4h\u00b7lig\u00b7mal", "standst", "du", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mit Kindeslust sah ich nach dir,", "tokens": ["Mit", "Kin\u00b7des\u00b7lust", "sah", "ich", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Kindlein gab mir seine H\u00e4nde,", "tokens": ["Dein", "Kin\u00b7dlein", "gab", "mir", "sei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es dereinst mich wieder f\u00e4nde;", "tokens": ["Da\u00df", "es", "de\u00b7reinst", "mich", "wie\u00b7der", "f\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du l\u00e4cheltest voll Z\u00e4rtlichkeit", "tokens": ["Du", "l\u00e4\u00b7chel\u00b7test", "voll", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fc\u00dftest mich, o himmels\u00fc\u00dfe Zeit!", "tokens": ["Und", "k\u00fc\u00df\u00b7test", "mich", ",", "o", "him\u00b7mel\u00b7s\u00fc\u00b7\u00dfe", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Fern steht nun diese selge Welt,", "tokens": ["Fern", "steht", "nun", "die\u00b7se", "sel\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gram hat sich l\u00e4ngst zu mir gesellt,", "tokens": ["Gram", "hat", "sich", "l\u00e4ngst", "zu", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betr\u00fcbt bin ich umher gegangen,", "tokens": ["Be\u00b7tr\u00fcbt", "bin", "ich", "um\u00b7her", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hab' ich mich denn so schwer vergangen?", "tokens": ["Hab'", "ich", "mich", "denn", "so", "schwer", "ver\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kindlich ber\u00fchr' ich deinen Saum,", "tokens": ["Kind\u00b7lich", "be\u00b7r\u00fchr'", "ich", "dei\u00b7nen", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Erwecke mich aus diesem schweren Traum.", "tokens": ["Er\u00b7we\u00b7cke", "mich", "aus", "die\u00b7sem", "schwe\u00b7ren", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Darf nur ein Kind dein Antlitz schaun,", "tokens": ["Darf", "nur", "ein", "Kind", "dein", "Ant\u00b7litz", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deinem Beistand fest vertraun,", "tokens": ["Und", "dei\u00b7nem", "Bei\u00b7stand", "fest", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So l\u00f6se doch des Alters Binde", "tokens": ["So", "l\u00f6\u00b7se", "doch", "des", "Al\u00b7ters", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mache mich zu deinem Kinde:", "tokens": ["Und", "ma\u00b7che", "mich", "zu", "dei\u00b7nem", "Kin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kindeslieb' und Kindestreu", "tokens": ["Die", "Kin\u00b7des\u00b7lieb'", "und", "Kin\u00b7des\u00b7treu"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohnt mir von jener goldnen Zeit noch bei.", "tokens": ["Wohnt", "mir", "von", "je\u00b7ner", "gold\u00b7nen", "Zeit", "noch", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}