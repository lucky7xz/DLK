{"dta.poem.23500": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von rc. beeden  \n Fr\u00e4wlin rc. Agnes vnd  \n Anna Hertzoginen zu Wir-  \n temberg rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Agnes/ Ewer lob/ prei\u00df vnd ehr", "tokens": ["Ag\u00b7nes", "/", "E\u00b7wer", "lob", "/", "prei\u00df", "vnd", "ehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "PPOSAT", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wollen wir Nymfen mehr vnd mehr", "tokens": ["Wol\u00b7len", "wir", "Nym\u00b7fen", "mehr", "vnd", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ADV", "KON", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nach ewerm verdienst stehts erklinge\u0303/", "tokens": ["Nach", "e\u00b7werm", "ver\u00b7dienst", "stehts", "er\u00b7kling\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Vnd nach vnsr er schuldigkeit singen/", "tokens": ["Vnd", "nach", "vnsr", "er", "schul\u00b7dig\u00b7keit", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "ADJD", "VVINF", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dan Jhr so sch\u00f6n vnd tugentreich/", "tokens": ["Dan", "Ihr", "so", "sch\u00f6n", "vnd", "tu\u00b7gen\u00b7treich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Euch keine Nymf zuvergleichen/", "tokens": ["Das", "Euch", "kei\u00b7ne", "Nymf", "zu\u00b7ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dan Ewreschwester die Euch gleich/", "tokens": ["Dan", "E\u00b7wresc\u00b7hwes\u00b7ter", "die", "Euch", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Deren all andre Nymfen weichen.", "tokens": ["De\u00b7ren", "all", "and\u00b7re", "Nym\u00b7fen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}