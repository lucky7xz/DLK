{"textgrid.poem.41074": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das Seltenste", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig in der Erde Nacht!", "tokens": ["Steig", "in", "der", "Er\u00b7de", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wohl manchen edlen Stein", "tokens": ["Wohl", "man\u00b7chen", "ed\u00b7len", "Stein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Findst du in stillem Schacht,", "tokens": ["Findst", "du", "in", "stil\u00b7lem", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der unversehrt und rein.", "tokens": ["Der", "un\u00b7ver\u00b7sehrt", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tauch' in des Meeres Grund,", "tokens": ["Tauch'", "in", "des", "Mee\u00b7res", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Such' am einsamen Riff!", "tokens": ["Such'", "am", "ein\u00b7sa\u00b7men", "Riff", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Manch Perle rein und rund", "tokens": ["Manch", "Per\u00b7le", "rein", "und", "rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hascht ein geschickter Griff.", "tokens": ["Hascht", "ein", "ge\u00b7schick\u00b7ter", "Griff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Geh hin, wo sich ohn' Ruh'", "tokens": ["Geh", "hin", ",", "wo", "sich", "ohn'", "Ruh'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PRF", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Menschenmarkt bewegt, \u2013", "tokens": ["Der", "Men\u00b7schen\u00b7markt", "be\u00b7wegt", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das keine Narbe tr\u00e4gt.", "tokens": ["Das", "kei\u00b7ne", "Nar\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}