{"dta.poem.9350": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Darinnen  Dimeter hypercatalectus  und  acatalectus  \n mit einander geschrencket werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr frischen wasser und jhr steine/", "tokens": ["Ihr", "fri\u00b7schen", "was\u00b7ser", "und", "jhr", "stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "jhr beume/ felder/ thal und wald/", "tokens": ["jhr", "beu\u00b7me", "/", "fel\u00b7der", "/", "thal", "und", "wald", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ach h\u00f6rt! wie seuftz\u2019 ich mannigfalt;", "tokens": ["ach", "h\u00f6rt", "!", "wie", "seuft\u00b7z'", "ich", "man\u00b7nig\u00b7falt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schreibt auf mein Testament/ jhr Haine/", "tokens": ["Schreibt", "auf", "mein", "Tes\u00b7ta\u00b7ment", "/", "jhr", "Hai\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "seyd meine Schreiber/ wie ich weine/", "tokens": ["seyd", "mei\u00b7ne", "Schrei\u00b7ber", "/", "wie", "ich", "wei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "grabts in die rinden/ da\u00df es bald", "tokens": ["grabts", "in", "die", "rin\u00b7den", "/", "da\u00df", "es", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bekleibe/ wenn ich werde kalt", "tokens": ["be\u00b7klei\u00b7be", "/", "wenn", "ich", "wer\u00b7de", "kalt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und leblo\u00df \u00e4hnlich bin dem scheine;", "tokens": ["und", "le\u00b7blo\u00df", "\u00e4hn\u00b7lich", "bin", "dem", "schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sterb\u2019 aus lauter grausamkeit", "tokens": ["Ich", "sterb'", "aus", "lau\u00b7ter", "grau\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Liebsten/ welche mich (ach leid!)", "tokens": ["der", "Liebs\u00b7ten", "/", "wel\u00b7che", "mich", "(", "ach", "leid", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "$(", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hatt niemahls wollen lieb gewinnen.", "tokens": ["Hatt", "nie\u00b7mahls", "wol\u00b7len", "lieb", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun gute Nacht/ jhr meine Lust/", "tokens": ["Nun", "gu\u00b7te", "Nacht", "/", "jhr", "mei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "jhr W\u00e4lder/ die jhr mier bewust", "tokens": ["jhr", "W\u00e4l\u00b7der", "/", "die", "jhr", "mier", "be\u00b7wust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und offt erfrischt die matten Sinnen!", "tokens": ["und", "offt", "er\u00b7frischt", "die", "mat\u00b7ten", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}