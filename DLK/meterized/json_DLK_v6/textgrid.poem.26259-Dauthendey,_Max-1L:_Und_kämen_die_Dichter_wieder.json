{"textgrid.poem.26259": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und k\u00e4men die Dichter wieder", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und k\u00e4men die Dichter wieder", "tokens": ["Und", "k\u00e4\u00b7men", "die", "Dich\u00b7ter", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die je auf Erden gesungen,", "tokens": ["Die", "je", "auf", "Er\u00b7den", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu bringen heut neue Lieder", "tokens": ["Zu", "brin\u00b7gen", "heut", "neu\u00b7e", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In allen den V\u00f6lkerzungen, \u2013", "tokens": ["In", "al\u00b7len", "den", "V\u00f6l\u00b7ker\u00b7zun\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie h\u00e4tten nicht genug T\u00f6ne,", "tokens": ["Sie", "h\u00e4t\u00b7ten", "nicht", "ge\u00b7nug", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu singen vom Mut der Heere,", "tokens": ["Zu", "sin\u00b7gen", "vom", "Mut", "der", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der dem Tod geweihten S\u00f6hne,", "tokens": ["Der", "dem", "Tod", "ge\u00b7weih\u00b7ten", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die fielen im Feld der Ehre.", "tokens": ["Die", "fie\u00b7len", "im", "Feld", "der", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Schl\u00fcg man die W\u00e4lder auf Erden", "tokens": ["Schl\u00fcg", "man", "die", "W\u00e4l\u00b7der", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "ART", "NN", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Da\u00df Ehrentempel und Hallen", "tokens": ["Da\u00df", "Eh\u00b7ren\u00b7tem\u00b7pel", "und", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Heeren der Helden werden,", "tokens": ["Den", "Hee\u00b7ren", "der", "Hel\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die vor den Feinden gefallen, \u2013", "tokens": ["Die", "vor", "den", "Fein\u00b7den", "ge\u00b7fal\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Das Holz, das w\u00fcrde nicht reichen", "tokens": ["Das", "Holz", ",", "das", "w\u00fcr\u00b7de", "nicht", "rei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein w\u00fcrdiges Haus zu richten,", "tokens": ["Ein", "w\u00fcr\u00b7di\u00b7ges", "Haus", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nicht alle W\u00e4lder der Eichen.", "tokens": ["Nicht", "al\u00b7le", "W\u00e4l\u00b7der", "der", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nicht alle W\u00e4lder der Fichten.", "tokens": ["Nicht", "al\u00b7le", "W\u00e4l\u00b7der", "der", "Fich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was k\u00f6nnen wir opfern ihnen", "tokens": ["Was", "k\u00f6n\u00b7nen", "wir", "op\u00b7fern", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die frisch ihr Blut hingegeben?", "tokens": ["Die", "frisch", "ihr", "Blut", "hin\u00b7ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Zum Dank, den diese verdienen,", "tokens": ["Zum", "Dank", ",", "den", "die\u00b7se", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PDS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dazu reicht nicht unser Leben.", "tokens": ["Da\u00b7zu", "reicht", "nicht", "un\u00b7ser", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Menschheit hat es geduldet.", "tokens": ["Die", "Menschheit", "hat", "es", "ge\u00b7dul\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kann sie den Dank jemals zahlen?", "tokens": ["Kann", "sie", "den", "Dank", "je\u00b7mals", "zah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Menschheit bleibt tief verschuldet", "tokens": ["Die", "Menschheit", "bleibt", "tief", "ver\u00b7schul\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem letzten Mann, der gefallen.", "tokens": ["Dem", "letz\u00b7ten", "Mann", ",", "der", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}