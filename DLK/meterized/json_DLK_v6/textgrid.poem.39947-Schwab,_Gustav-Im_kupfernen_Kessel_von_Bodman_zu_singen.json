{"textgrid.poem.39947": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Im kupfernen Kessel von Bodman zu singen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Kessel zu Bodman, da steh' ich zur Stund,", "tokens": ["Im", "Kes\u00b7sel", "zu", "Bod\u00b7man", ",", "da", "steh'", "ich", "zur", "Stund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Soll leeren den Becher bis auf den Grund,", "tokens": ["Soll", "lee\u00b7ren", "den", "Be\u00b7cher", "bis", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Becher, gef\u00fcllet mit K\u00f6nigswein,", "tokens": ["Den", "Be\u00b7cher", ",", "ge\u00b7f\u00fcl\u00b7let", "mit", "K\u00f6\u00b7nigs\u00b7wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Herr Karol ihn pflanzt' auf dem Felsengestein.", "tokens": ["Herr", "Ka\u00b7rol", "ihn", "pflanzt'", "auf", "dem", "Fel\u00b7sen\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und was gezogen der m\u00e4chtige Frank',", "tokens": ["Und", "was", "ge\u00b7zo\u00b7gen", "der", "m\u00e4ch\u00b7ti\u00b7ge", "Frank'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein freier Schwabe jetzt erntet's mit Dank,", "tokens": ["Ein", "frei\u00b7er", "Schwa\u00b7be", "jetzt", "ern\u00b7tet's", "mit", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er sperrt's in den Keller nicht feindlich ein,", "tokens": ["Er", "sperrt's", "in", "den", "Kel\u00b7ler", "nicht", "feind\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Er ruft den Fremdling zum Trunk herein.", "tokens": ["Er", "ruft", "den", "Fremd\u00b7ling", "zum", "Trunk", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wie in den Becher mein Auge schaut,", "tokens": ["Und", "wie", "in", "den", "Be\u00b7cher", "mein", "Au\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Dunkel der alten Geschichten ihm graut,", "tokens": ["Das", "Dun\u00b7kel", "der", "al\u00b7ten", "Ge\u00b7schich\u00b7ten", "ihm", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wie der Wein an die Lippe mir schwillt,", "tokens": ["Und", "wie", "der", "Wein", "an", "die", "Lip\u00b7pe", "mir", "schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sage hervor schon, die sprudelnde, quillt.", "tokens": ["Die", "Sa\u00b7ge", "her\u00b7vor", "schon", ",", "die", "spru\u00b7deln\u00b7de", ",", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "ADV", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Sie sa\u00dfen zu Bodman beim fr\u00f6hlichen Mahl,", "tokens": ["Sie", "sa\u00b7\u00dfen", "zu", "Bod\u00b7man", "beim", "fr\u00f6h\u00b7li\u00b7chen", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Vater, die Mutter, die Kinder im Saal,", "tokens": ["Der", "Va\u00b7ter", ",", "die", "Mut\u00b7ter", ",", "die", "Kin\u00b7der", "im", "Saal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die S\u00f6hne, die T\u00f6chter, wie Rosen und Schnee,", "tokens": ["Die", "S\u00f6h\u00b7ne", ",", "die", "T\u00f6ch\u00b7ter", ",", "wie", "Ro\u00b7sen", "und", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das edelste, sch\u00f6nste Geschlecht am See.", "tokens": ["Das", "e\u00b7dels\u00b7te", ",", "sch\u00f6ns\u00b7te", "Ge\u00b7schlecht", "am", "See", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Viel G\u00e4ste begl\u00e4nzet vom Sonnenschein,", "tokens": ["Viel", "G\u00e4s\u00b7te", "be\u00b7gl\u00e4n\u00b7zet", "vom", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie tranken und sangen bei'm K\u00f6nigswein,", "tokens": ["Sie", "tran\u00b7ken", "und", "san\u00b7gen", "bei'm", "K\u00f6\u00b7nigs\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So wie ich heut trink' und heut singe mein Lied:", "tokens": ["So", "wie", "ich", "heut", "trink'", "und", "heut", "sin\u00b7ge", "mein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Abend von festlicher Lust sie nicht schied.", "tokens": ["Der", "A\u00b7bend", "von", "fest\u00b7li\u00b7cher", "Lust", "sie", "nicht", "schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Die Nacht kam heran mit Wetter und Wind,", "tokens": ["Die", "Nacht", "kam", "he\u00b7ran", "mit", "Wet\u00b7ter", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des st\u00fcrmischen Sees verstohlenem Kind,", "tokens": ["Des", "st\u00fcr\u00b7mi\u00b7schen", "Sees", "ver\u00b7stoh\u00b7le\u00b7nem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Wolken sammeln sich \u00fcber dem Haus,", "tokens": ["Die", "Wol\u00b7ken", "sam\u00b7meln", "sich", "\u00fc\u00b7ber", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch gehen die Lampen im Schlosse nicht aus.", "tokens": ["Doch", "ge\u00b7hen", "die", "Lam\u00b7pen", "im", "Schlos\u00b7se", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Die G\u00e4ste sie tanzen th\u00fcraus und th\u00fcrein,", "tokens": ["Die", "G\u00e4s\u00b7te", "sie", "tan\u00b7zen", "th\u00fc\u00b7raus", "und", "th\u00fc\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Wolken auch f\u00fchren den n\u00e4chtlichen Reihn,", "tokens": ["Die", "Wol\u00b7ken", "auch", "f\u00fch\u00b7ren", "den", "n\u00e4cht\u00b7li\u00b7chen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es spr\u00fchen die Fackeln in Gang und Saal,", "tokens": ["Es", "spr\u00fc\u00b7hen", "die", "Fa\u00b7ckeln", "in", "Gang", "und", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Blitze die sp\u00e4hen mit bleichem Stral.", "tokens": ["Die", "Blit\u00b7ze", "die", "sp\u00e4\u00b7hen", "mit", "blei\u00b7chem", "Stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Und in der Schalmei und der Fl\u00f6te Gesang", "tokens": ["Und", "in", "der", "Schal\u00b7mei", "und", "der", "Fl\u00f6\u00b7te", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Spielt heimlich des Donners begleitender Klang,", "tokens": ["Spielt", "heim\u00b7lich", "des", "Don\u00b7ners", "be\u00b7glei\u00b7ten\u00b7der", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Noch rauschet im Saale das Spiel und der Witz,", "tokens": ["Noch", "rau\u00b7schet", "im", "Saa\u00b7le", "das", "Spiel", "und", "der", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da schl\u00e4gt durch die Decke der zackigte Blitz.", "tokens": ["Da", "schl\u00e4gt", "durch", "die", "De\u00b7cke", "der", "za\u00b7ckig\u00b7te", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Und Flammen umw\u00f6lken den m\u00e4chtigen Saal,", "tokens": ["Und", "Flam\u00b7men", "um\u00b7w\u00f6l\u00b7ken", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ersticken die G\u00e4ste, verzehren das Mahl,", "tokens": ["Er\u00b7sti\u00b7cken", "die", "G\u00e4s\u00b7te", ",", "ver\u00b7zeh\u00b7ren", "das", "Mahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O Wasser und Himmel, wie gl\u00e4nzt ihr so hell,", "tokens": ["O", "Was\u00b7ser", "und", "Him\u00b7mel", ",", "wie", "gl\u00e4nzt", "ihr", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "O herrlich Geschlecht, wie vergehst du so schnell!", "tokens": ["O", "herr\u00b7lich", "Ge\u00b7schlecht", ",", "wie", "ver\u00b7gehst", "du", "so", "schnell", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Der Vater, die Mutter, sie liegen schon;", "tokens": ["Der", "Va\u00b7ter", ",", "die", "Mut\u00b7ter", ",", "sie", "lie\u00b7gen", "schon", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ach, dringt zu der Th\u00fcre kein bl\u00fchender Sohn?", "tokens": ["Ach", ",", "dringt", "zu", "der", "Th\u00fc\u00b7re", "kein", "bl\u00fc\u00b7hen\u00b7der", "Sohn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die zuckende Flamme l\u00e4\u00dft keinen hinaus,", "tokens": ["Die", "zu\u00b7cken\u00b7de", "Flam\u00b7me", "l\u00e4\u00dft", "kei\u00b7nen", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "APZR", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es f\u00e4llt auf die Leichen das wankende Haus.", "tokens": ["Es", "f\u00e4llt", "auf", "die", "Lei\u00b7chen", "das", "wan\u00b7ken\u00b7de", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Da dringt durch Flammen und Feuers Schwall,", "tokens": ["Da", "dringt", "durch", "Flam\u00b7men", "und", "Feu\u00b7ers", "Schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Amme, die treue, heraus auf den Wall,", "tokens": ["Die", "Am\u00b7me", ",", "die", "treu\u00b7e", ",", "he\u00b7raus", "auf", "den", "Wall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie hat es enthoben der Wiege geschwind,", "tokens": ["Sie", "hat", "es", "ent\u00b7ho\u00b7ben", "der", "Wie\u00b7ge", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie tr\u00e4gt auf den Armen ein wimmerndes Kind.", "tokens": ["Sie", "tr\u00e4gt", "auf", "den", "Ar\u00b7men", "ein", "wim\u00b7mern\u00b7des", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Sie st\u00f6\u00dft einen Kessel durch Glut und Flamm',", "tokens": ["Sie", "st\u00f6\u00dft", "ei\u00b7nen", "Kes\u00b7sel", "durch", "Glut", "und", "Flamm'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", 