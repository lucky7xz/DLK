{"dta.poem.8504": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Mutassin .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst vor dem Thron Mutassin des Kaliphen", "tokens": ["Einst", "vor", "dem", "Thron", "Mu\u00b7tas\u00b7sin", "des", "Ka\u00b7li\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ART", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "In Fesseln klirrend ein Verbrecher stand,", "tokens": ["In", "Fes\u00b7seln", "klir\u00b7rend", "ein", "Ver\u00b7bre\u00b7cher", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem, als vom Trunk bet\u00e4ubt die Wachen schliefen,", "tokens": ["Dem", ",", "als", "vom", "Trunk", "be\u00b7t\u00e4ubt", "die", "Wa\u00b7chen", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "APPRART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Herrschers eigne Hand den Dolch entwandt;", "tokens": ["Des", "Herr\u00b7schers", "eig\u00b7ne", "Hand", "den", "Dolch", "ent\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon traf die l\u00e4ss\u2019gen S\u00f6ldner das Gericht,", "tokens": ["Schon", "traf", "die", "l\u00e4ss'\u00b7gen", "S\u00f6ld\u00b7ner", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie es sie traf, die Sage k\u00fcndet\u2019s nicht,", "tokens": ["Wie", "es", "sie", "traf", ",", "die", "Sa\u00b7ge", "k\u00fcn\u00b7det's", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur dieses sagt sie, da\u00df an jenem Tag", "tokens": ["Nur", "die\u00b7ses", "sagt", "sie", ",", "da\u00df", "an", "je\u00b7nem", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein schaudernd Schweigen \u00fcber Bagdad lag,", "tokens": ["Ein", "schau\u00b7dernd", "Schwei\u00b7gen", "\u00fc\u00b7ber", "Bag\u00b7dad", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und da\u00df, als man den Hochverr\u00e4ther f\u00fchrte", "tokens": ["Und", "da\u00df", ",", "als", "man", "den", "Hoch\u00b7ver\u00b7r\u00e4\u00b7ther", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zum Spruch, im Saal sich keine Wimper r\u00fchrte,", "tokens": ["Zum", "Spruch", ",", "im", "Saal", "sich", "kei\u00b7ne", "Wim\u00b7per", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und da\u00df des Herrschers Blick, zum Grund gewandt,", "tokens": ["Und", "da\u00df", "des", "Herr\u00b7schers", "Blick", ",", "zum", "Grund", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Blumen aus dem Teppich schier gebrannt.", "tokens": ["Die", "Blu\u00b7men", "aus", "dem", "Tep\u00b7pich", "schier", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Throne stand ein Becher mit Scherbet,", "tokens": ["Am", "Thro\u00b7ne", "stand", "ein", "Be\u00b7cher", "mit", "Scher\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Gaumen des Kaliphen d\u00f6rrten Gluthen,", "tokens": ["Den", "Gau\u00b7men", "des", "Ka\u00b7li\u00b7phen", "d\u00f6rr\u00b7ten", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er f\u00fchlte seine Menschlichkeit verbluten", "tokens": ["Er", "f\u00fchl\u00b7te", "sei\u00b7ne", "Menschlich\u00b7keit", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Dolche der bedrohten Majest\u00e4t.", "tokens": ["Am", "Dol\u00b7che", "der", "be\u00b7droh\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer gibt ihm seiner N\u00e4chte Schlaf zur\u00fcck?", "tokens": ["Wer", "gibt", "ihm", "sei\u00b7ner", "N\u00e4ch\u00b7te", "Schlaf", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer seinen Muth zum Schaffen und zum Lieben,", "tokens": ["Wer", "sei\u00b7nen", "Muth", "zum", "Schaf\u00b7fen", "und", "zum", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPRART", "NN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wer das Vertrauen auf sein altes Gl\u00fcck?", "tokens": ["Wer", "das", "Ver\u00b7trau\u00b7en", "auf", "sein", "al\u00b7tes", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Alles stand in seinem Blick geschrieben.", "tokens": ["Das", "Al\u00b7les", "stand", "in", "sei\u00b7nem", "Blick", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Frevler zittert, da\u00df die Fessel klirrt;", "tokens": ["Der", "Frev\u00b7ler", "zit\u00b7tert", ",", "da\u00df", "die", "Fes\u00b7sel", "klirrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als noch der Lohn ihm w\u00e4sserte den Mund", "tokens": ["Als", "noch", "der", "Lohn", "ihm", "w\u00e4s\u00b7ser\u00b7te", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein kecker Fuchs, und jetzt ein feiger Hund,", "tokens": ["Ein", "ke\u00b7cker", "Fuchs", ",", "und", "jetzt", "ein", "fei\u00b7ger", "Hund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00fcrd\u2019 er sich doppelten Verraths nicht sch\u00e4men;", "tokens": ["W\u00fcrd'", "er", "sich", "dop\u00b7pel\u00b7ten", "Ver\u00b7raths", "nicht", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Doch sieht er deutlich, Keiner will ihn nehmen", "tokens": ["Doch", "sieht", "er", "deut\u00b7lich", ",", "Kei\u00b7ner", "will", "ihn", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Becher, da\u00df er ihm zur Labe wird;", "tokens": ["Den", "Be\u00b7cher", ",", "da\u00df", "er", "ihm", "zur", "La\u00b7be", "wird", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Z\u00e4hnknirschend schaut er zum Kaliphen auf,", "tokens": ["Z\u00e4hn\u00b7knir\u00b7schend", "schaut", "er", "zum", "Ka\u00b7li\u00b7phen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Wimper zuckt, es dr\u00e4ngt ein Schrei sich auf,", "tokens": ["Die", "Wim\u00b7per", "zuckt", ",", "es", "dr\u00e4ngt", "ein", "Schrei", "sich", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wie im Strauch die kranke Schlange pfeift,", "tokens": ["Und", "wie", "im", "Strauch", "die", "kran\u00b7ke", "Schlan\u00b7ge", "pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In einem schweren Krampf will er ersticken;", "tokens": ["In", "ei\u00b7nem", "schwe\u00b7ren", "Krampf", "will", "er", "er\u00b7sti\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O Allah, wird er sich dem Pfahl entr\u00fccken!", "tokens": ["O", "Al\u00b7lah", ",", "wird", "er", "sich", "dem", "Pfahl", "ent\u00b7r\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und st\u00fcrmisch der Kaliph zum Becher greift,", "tokens": ["Und", "st\u00fcr\u00b7misch", "der", "Ka\u00b7liph", "zum", "Be\u00b7cher", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gie\u00dft mit den eignen H\u00e4nden den Scherbet", "tokens": ["Gie\u00dft", "mit", "den", "eig\u00b7nen", "H\u00e4n\u00b7den", "den", "Scher\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Ihm in die Kehle, bis der Krampf vergeht.", "tokens": ["Ihm", "in", "die", "Keh\u00b7le", ",", "bis", "der", "Krampf", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Farbe kehrt, er athmet schwer und tief,", "tokens": ["Die", "Far\u00b7be", "kehrt", ",", "er", "ath\u00b7met", "schwer", "und", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das Auge, irr zuerst, dann fest und k\u00fchn,", "tokens": ["Das", "Au\u00b7ge", ",", "irr", "zu\u00b7erst", ",", "dann", "fest", "und", "k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "L\u00e4\u00dft lange er auf dem Beherrscher gl\u00fch\u2019n,", "tokens": ["L\u00e4\u00dft", "lan\u00b7ge", "er", "auf", "dem", "Be\u00b7herr\u00b7scher", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Dann spricht er ernst: \u201elang lebe der Kaliph!", "tokens": ["Dann", "spricht", "er", "ernst", ":", "\u201e", "lang", "le\u00b7be", "der", "Ka\u00b7liph", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Was er beschlie\u00dft, das kommt von Allah\u2019s Hand,", "tokens": ["Was", "er", "be\u00b7schlie\u00dft", ",", "das", "kommt", "von", "Al\u00b7lah's", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der will es nicht, da\u00df er vom Zorn entflammt", "tokens": ["Der", "will", "es", "nicht", ",", "da\u00df", "er", "vom", "Zorn", "ent\u00b7flammt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Zum Marterpfahle einen Gast verdammt,", "tokens": ["Zum", "Mar\u00b7ter\u00b7pfah\u00b7le", "ei\u00b7nen", "Gast", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Dem seinen eignen Becher er gesandt.\u201c", "tokens": ["Dem", "sei\u00b7nen", "eig\u00b7nen", "Be\u00b7cher", "er", "ge\u00b7sandt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da wird Mutassin bleich vor innrer Qual,", "tokens": ["Da", "wird", "Mu\u00b7tas\u00b7sin", "bleich", "vor", "inn\u00b7rer", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zittern sah ihn sein Hof zum ersten Mal,", "tokens": ["Zit\u00b7tern", "sah", "ihn", "sein", "Hof", "zum", "ers\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dann wie die Sonne ward sein Auge hell,", "tokens": ["Dann", "wie", "die", "Son\u00b7ne", "ward", "sein", "Au\u00b7ge", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hochgetragnen Hauptes rief er schnell:", "tokens": ["Und", "hoch\u00b7ge\u00b7trag\u00b7nen", "Haup\u00b7tes", "rief", "er", "schnell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.p