{"textgrid.poem.24847": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Enth\u00fclltes Geheimnis", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von meinem Auge sank es wie ein Schleier,", "tokens": ["Von", "mei\u00b7nem", "Au\u00b7ge", "sank", "es", "wie", "ein", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich zuerst dich fand. Mir war,", "tokens": ["Da", "ich", "zu\u00b7erst", "dich", "fand", ".", "Mir", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als w\u00fcrd' im Tempel mir bei heil'ger Feier", "tokens": ["Als", "w\u00fcrd'", "im", "Tem\u00b7pel", "mir", "bei", "heil'\u00b7ger", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPRART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein g\u00f6ttliches Geheimnis klar.", "tokens": ["Ein", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7heim\u00b7nis", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und in die Seele kam mir tiefes Schweigen;", "tokens": ["Und", "in", "die", "See\u00b7le", "kam", "mir", "tie\u00b7fes", "Schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Staunen, wie zum erstenmal,", "tokens": ["Mit", "Stau\u00b7nen", ",", "wie", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah ich die hocherhabne Sonne steigen,", "tokens": ["Sah", "ich", "die", "ho\u00b7cher\u00b7hab\u00b7ne", "Son\u00b7ne", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Mondes milden D\u00e4mmerstrahl.", "tokens": ["Des", "Mon\u00b7des", "mil\u00b7den", "D\u00e4m\u00b7mer\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erst nun ist alles, alles mir erschlossen,", "tokens": ["Erst", "nun", "ist", "al\u00b7les", ",", "al\u00b7les", "mir", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "$,", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stimmen all von Wald und Flur", "tokens": ["Die", "Stim\u00b7men", "all", "von", "Wald", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versteh' ich nun, das Welken und das Sprossen", "tokens": ["Ver\u00b7steh'", "ich", "nun", ",", "das", "Wel\u00b7ken", "und", "das", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ewig waltenden Natur.", "tokens": ["Der", "e\u00b7wig", "wal\u00b7ten\u00b7den", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was der Weisen Lehren nicht gelungen,", "tokens": ["Und", "was", "der", "Wei\u00b7sen", "Leh\u00b7ren", "nicht", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur durch der Liebe Zaubermacht,", "tokens": ["Nur", "durch", "der", "Lie\u00b7be", "Zau\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die feur'ger redet, als mit Engelzungen,", "tokens": ["Die", "feur'\u00b7ger", "re\u00b7det", ",", "als", "mit", "En\u00b7gel\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hast du es, fast noch Kind, vollbracht.", "tokens": ["Hast", "du", "es", ",", "fast", "noch", "Kind", ",", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "ADV", "ADV", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}