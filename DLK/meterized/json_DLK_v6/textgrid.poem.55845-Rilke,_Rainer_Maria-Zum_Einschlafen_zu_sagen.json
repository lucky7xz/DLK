{"textgrid.poem.55845": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Zum Einschlafen zu sagen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte jemanden einsingen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "je\u00b7man\u00b7den", "ein\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bei jemandem sitzen und sein.", "tokens": ["bei", "je\u00b7man\u00b7dem", "sit\u00b7zen", "und", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "KON", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich m\u00f6chte dich wiegen und kleinsingen", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "wie\u00b7gen", "und", "klein\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "KON", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und begleiten schlafaus und schlafein.", "tokens": ["und", "be\u00b7glei\u00b7ten", "schla\u00b7faus", "und", "schla\u00b7fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ich m\u00f6chte der Einzige sein im Haus,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "der", "Ein\u00b7zi\u00b7ge", "sein", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "VAINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "der w\u00fc\u00dfte: die Nacht war kalt.", "tokens": ["der", "w\u00fc\u00df\u00b7te", ":", "die", "Nacht", "war", "kalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und m\u00f6chte horchen herein und hinaus", "tokens": ["Und", "m\u00f6ch\u00b7te", "hor\u00b7chen", "her\u00b7ein", "und", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVINF", "PTKVZ", "KON", "XY"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "in dich, in die Welt, in den Wald.", "tokens": ["in", "dich", ",", "in", "die", "Welt", ",", "in", "den", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Die Uhren rufen sich schlagend an,", "tokens": ["Die", "Uh\u00b7ren", "ru\u00b7fen", "sich", "schla\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "und man sieht der Zeit auf den Grund.", "tokens": ["und", "man", "sieht", "der", "Zeit", "auf", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Und unten geht noch ein fremder Mann", "tokens": ["Und", "un\u00b7ten", "geht", "noch", "ein", "frem\u00b7der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "und st\u00f6rt einen fremden Hund.", "tokens": ["und", "st\u00f6rt", "ei\u00b7nen", "frem\u00b7den", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Dahinter wird Stille. Ich habe gro\u00df", "tokens": ["Da\u00b7hin\u00b7ter", "wird", "Stil\u00b7le", ".", "Ich", "ha\u00b7be", "gro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VAFIN", "NN", "$.", "PPER", "VAFIN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "die Augen auf dich gelegt;", "tokens": ["die", "Au\u00b7gen", "auf", "dich", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "und sie halten dich sanft und lassen dich los,", "tokens": ["und", "sie", "hal\u00b7ten", "dich", "sanft", "und", "las\u00b7sen", "dich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.16": {"text": "wenn ein Ding sich im Dunkel bewegt.", "tokens": ["wenn", "ein", "Ding", "sich", "im", "Dun\u00b7kel", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}