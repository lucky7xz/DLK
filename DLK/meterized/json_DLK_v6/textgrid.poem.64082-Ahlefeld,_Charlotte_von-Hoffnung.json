{"textgrid.poem.64082": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Wallfahrt durch dies dunkle Leben", "tokens": ["Auf", "der", "Wall\u00b7fahrt", "durch", "dies", "dunk\u00b7le", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leuchten uns nur selten milde Sterne;", "tokens": ["Leuch\u00b7ten", "uns", "nur", "sel\u00b7ten", "mil\u00b7de", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eingeh\u00fcllt in tr\u00fcbe Wolken schweben", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "in", "tr\u00fc\u00b7be", "Wol\u00b7ken", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie dem Blicke unerreichbar ferne.", "tokens": ["Sie", "dem", "Bli\u00b7cke", "un\u00b7er\u00b7reich\u00b7bar", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn der Hoffnung goldnes Licht nicht w\u00e4re,", "tokens": ["Wenn", "der", "Hoff\u00b7nung", "gold\u00b7nes", "Licht", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das des Kummers \u00f6de Nacht durchbricht,", "tokens": ["Das", "des", "Kum\u00b7mers", "\u00f6\u00b7de", "Nacht", "durch\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So ertr\u00fcge dieses Daseyns Schwere", "tokens": ["So", "er\u00b7tr\u00fc\u00b7ge", "die\u00b7ses", "Da\u00b7seyns", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der gepresste, bange Busen nicht.", "tokens": ["Der", "ge\u00b7press\u00b7te", ",", "ban\u00b7ge", "Bu\u00b7sen", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Weiche nimmer aus dem matten Herzen,", "tokens": ["Wei\u00b7che", "nim\u00b7mer", "aus", "dem", "mat\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das des Trostes Balsam oft entbehrt,", "tokens": ["Das", "des", "Tros\u00b7tes", "Bal\u00b7sam", "oft", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und umringt von nahmenlosen Schmerzen,", "tokens": ["Und", "um\u00b7ringt", "von", "nah\u00b7men\u00b7lo\u00b7sen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In vergebner Sehnsucht sich verzehrt.", "tokens": ["In", "ver\u00b7geb\u00b7ner", "Sehn\u00b7sucht", "sich", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach so vieles hat es schon verlohren \u2013", "tokens": ["Ach", "so", "vie\u00b7les", "hat", "es", "schon", "ver\u00b7loh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einsam f\u00fchlt' sich's \u2013 traurig und allein,", "tokens": ["Ein\u00b7sam", "f\u00fchlt'", "sich's", "\u2013", "trau\u00b7rig", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "$(", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Tanz der jugendlichen Horen", "tokens": ["Und", "der", "Tanz", "der", "ju\u00b7gend\u00b7li\u00b7chen", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bringt ihm Regen nur statt Sonnenschein.", "tokens": ["Bringt", "ihm", "Re\u00b7gen", "nur", "statt", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Doch bleibst Du ihm treu zur Seite stehen,", "tokens": ["Doch", "bleibst", "Du", "ihm", "treu", "zur", "Sei\u00b7te", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du, o Hoffnung! holdes G\u00f6tterkind,", "tokens": ["Du", ",", "o", "Hoff\u00b7nung", "!", "hol\u00b7des", "G\u00f6t\u00b7ter\u00b7kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann vernimmt es in der St\u00fcrme Wehen", "tokens": ["Dann", "ver\u00b7nimmt", "es", "in", "der", "St\u00fcr\u00b7me", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur des Lenzes milden Abendwind.", "tokens": ["Nur", "des", "Len\u00b7zes", "mil\u00b7den", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}