{"textgrid.poem.62951": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Japan, wo viel edle Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Japan, wo viel edle Seelen", "tokens": ["In", "Ja\u00b7pan", ",", "wo", "viel", "ed\u00b7le", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PWAV", "PIAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und holde M\u00e4dchen sind,", "tokens": ["Und", "hol\u00b7de", "M\u00e4d\u00b7chen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War, wie die Schiffer uns erzehlen,", "tokens": ["War", ",", "wie", "die", "Schif\u00b7fer", "uns", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein armes Hirtenkind.", "tokens": ["Ein", "ar\u00b7mes", "Hir\u00b7ten\u00b7kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verborgen, wie die Mayenrose", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "wie", "die", "Ma\u00b7yen\u00b7ro\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im dunkeln Busche gl\u00fcht,", "tokens": ["Im", "dun\u00b7keln", "Bu\u00b7sche", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War Zacchis Jugend in dem Schoo\u00dfe", "tokens": ["War", "Zac\u00b7chis", "Ju\u00b7gend", "in", "dem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Unschuld aufgebl\u00fcht.", "tokens": ["Der", "Un\u00b7schuld", "auf\u00b7ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lebte sie bey ihrer Mutter,", "tokens": ["So", "leb\u00b7te", "sie", "bey", "ih\u00b7rer", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von Harm und Liebe frey,", "tokens": ["Von", "Harm", "und", "Lie\u00b7be", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr nichts besorgt, als f\u00fcr das Futter", "tokens": ["F\u00fcr", "nichts", "be\u00b7sorgt", ",", "als", "f\u00fcr", "das", "Fut\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVPP", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleinen Sch\u00e4ferey.", "tokens": ["Der", "klei\u00b7nen", "Sch\u00e4\u00b7fe\u00b7rey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einst band sie auf bebl\u00fcmter Erde", "tokens": ["Einst", "band", "sie", "auf", "be\u00b7bl\u00fcm\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich einen Veilchenstraus;", "tokens": ["Sich", "ei\u00b7nen", "Veil\u00b7chen\u00b7straus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam ein junger Mann zu Pferde", "tokens": ["Da", "kam", "ein", "jun\u00b7ger", "Mann", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den nahen Wald heraus.", "tokens": ["Den", "na\u00b7hen", "Wald", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Cubo wars. Mit Speer und Keule", "tokens": ["Der", "Cu\u00b7bo", "wars", ".", "Mit", "Speer", "und", "Keu\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bekriegt er auf der Jagd", "tokens": ["Be\u00b7kriegt", "er", "auf", "der", "Jagd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im armen Wild die lange Weile,", "tokens": ["Im", "ar\u00b7men", "Wild", "die", "lan\u00b7ge", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die seine Seele plagt.", "tokens": ["Die", "sei\u00b7ne", "See\u00b7le", "plagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sieht das M\u00e4dchen: ihre Blicke", "tokens": ["Er", "sieht", "das", "M\u00e4d\u00b7chen", ":", "ih\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fcnden seine Lust,", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcllen pl\u00f6tzlich jede L\u00fccke", "tokens": ["Und", "f\u00fcl\u00b7len", "pl\u00f6tz\u00b7lich", "je\u00b7de", "L\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner \u00f6den Brust.", "tokens": ["In", "sei\u00b7ner", "\u00f6\u00b7den", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6n war der Cubo, gro\u00df und bieder", "tokens": ["Sch\u00f6n", "war", "der", "Cu\u00b7bo", ",", "gro\u00df", "und", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NE", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War Usges rauher Muth,", "tokens": ["War", "Us\u00b7ges", "rau\u00b7her", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er setzt zu ihr ins Gras sich nieder", "tokens": ["Er", "setzt", "zu", "ihr", "ins", "Gras", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und malt ihr seine Glut.", "tokens": ["Und", "malt", "ihr", "sei\u00b7ne", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie staunt, die Rosen ihrer Wangen", "tokens": ["Sie", "staunt", ",", "die", "Ro\u00b7sen", "ih\u00b7rer", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entflammen zu Karmin.", "tokens": ["Ent\u00b7flam\u00b7men", "zu", "Kar\u00b7min", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00fc\u00dft sie, reicht ihr seine Spangen", "tokens": ["Er", "k\u00fc\u00dft", "sie", ",", "reicht", "ihr", "sei\u00b7ne", "Span\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Helm. Sie will entfliehn.", "tokens": ["Vom", "Helm", ".", "Sie", "will", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Itzt nennt er sich; sie zagt, sie bebet", "tokens": ["Itzt", "nennt", "er", "sich", ";", "sie", "zagt", ",", "sie", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00fcrzt auf seinen Schoo\u00df.", "tokens": ["Und", "st\u00fcrzt", "auf", "sei\u00b7nen", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entz\u00fcckt umschlingt er sie und hebet", "tokens": ["Ent\u00b7z\u00fcckt", "um\u00b7schlingt", "er", "sie", "und", "he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kosend auf sein Ro\u00df.", "tokens": ["Sie", "ko\u00b7send", "auf", "sein", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie folgt ihm \u2013 (eines Cubo Blicken", "tokens": ["Sie", "folgt", "ihm", "\u2013", "(", "ei\u00b7nes", "Cu\u00b7bo", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "$(", "ART", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gehorcht selbst die Natur)", "tokens": ["Ge\u00b7horcht", "selbst", "die", "Na\u00b7tur", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verstummt, wie auf des W\u00fcrgers R\u00fccken", "tokens": ["Ver\u00b7stummt", ",", "wie", "auf", "des", "W\u00fcr\u00b7gers", "R\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lamm, durch Hayn und Flur.", "tokens": ["Das", "Lamm", ",", "durch", "Hayn", "und", "Flur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Schon deckt ihn mit der sch\u00f6nen Beute", "tokens": ["Schon", "deckt", "ihn", "mit", "der", "sch\u00f6\u00b7nen", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hofburg stolzes Dach,", "tokens": ["Der", "Hof\u00b7burg", "stol\u00b7zes", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Amor giebt ihm das Geleite", "tokens": ["Und", "A\u00b7mor", "giebt", "ihm", "das", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins goldne Brautgemach.", "tokens": ["Ins", "gold\u00b7ne", "Braut\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Tag erwacht. Die holde Dirne", "tokens": ["Der", "Tag", "er\u00b7wacht", ".", "Die", "hol\u00b7de", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umwallt ein F\u00fcrstenkleid,", "tokens": ["Um\u00b7wallt", "ein", "F\u00fcrs\u00b7ten\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Usge schm\u00fcckt ihr Arm und Stirne", "tokens": ["Und", "Us\u00b7ge", "schm\u00fcckt", "ihr", "Arm", "und", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit blitzendem Geschmeid.", "tokens": ["Mit", "blit\u00b7zen\u00b7dem", "Ge\u00b7schmeid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Doch unget\u00e4uscht von Pracht und F\u00fclle", "tokens": ["Doch", "un\u00b7ge\u00b7t\u00e4uscht", "von", "Pracht", "und", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt sie noch Sch\u00e4ferin,", "tokens": ["Bleibt", "sie", "noch", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und oft schwingt sich in ernster Stille", "tokens": ["Und", "oft", "schwingt", "sich", "in", "erns\u00b7ter", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihr Geist zur Mutter hin.", "tokens": ["Ihr", "Geist", "zur", "Mut\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie w\u00e4hlt von ihrem Brautgeschmeide", "tokens": ["Sie", "w\u00e4hlt", "von", "ih\u00b7rem", "Braut\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sch\u00f6nste Kleinod aus,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Klei\u00b7nod", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schickt mit eines Engels Freude", "tokens": ["Und", "schickt", "mit", "ei\u00b7nes", "En\u00b7gels", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es insgeheim nach Haus.", "tokens": ["Es", "ins\u00b7ge\u00b7heim", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch kaum ist unter Ku\u00df und Spielen", "tokens": ["Doch", "kaum", "ist", "un\u00b7ter", "Ku\u00df", "und", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der zehnte Tag vorbey,", "tokens": ["Der", "zehn\u00b7te", "Tag", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So f\u00e4ngt ihr Herz schon an zu f\u00fchlen,", "tokens": ["So", "f\u00e4ngt", "ihr", "Herz", "schon", "an", "zu", "f\u00fch\u00b7len", ","], "token_info"