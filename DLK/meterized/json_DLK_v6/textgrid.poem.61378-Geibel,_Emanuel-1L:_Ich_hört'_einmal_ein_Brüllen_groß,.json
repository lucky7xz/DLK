{"textgrid.poem.61378": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00f6rt' einmal ein Br\u00fcllen gro\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rt' einmal ein Br\u00fcllen gro\u00df,", "tokens": ["Ich", "h\u00f6rt'", "ein\u00b7mal", "ein", "Br\u00fcl\u00b7len", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon dacht' ich: \u00bbHimmlischer Vater!", "tokens": ["Schon", "dacht'", "ich", ":", "\u00bb", "Himm\u00b7li\u00b7scher", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ist ein Leu!\u00ab Doch fand ich blo\u00df", "tokens": ["Das", "ist", "ein", "Leu", "!", "\u00ab", "Doch", "fand", "ich", "blo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen ganz gew\u00f6hnlichen Kater.", "tokens": ["Ei\u00b7nen", "ganz", "ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "Ka\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mag man immer den L\u00f6wenton", "tokens": ["Mag", "man", "im\u00b7mer", "den", "L\u00f6\u00b7wen\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dem putzigen Tierchen verstatten!", "tokens": ["Dem", "put\u00b7zi\u00b7gen", "Tier\u00b7chen", "ver\u00b7stat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die B\u00e4ren und Panther l\u00e4\u00dft es schon", "tokens": ["Die", "B\u00e4\u00b7ren", "und", "Pan\u00b7ther", "l\u00e4\u00dft", "es", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und f\u00e4ngt uns die M\u00e4us' und die Ratten.", "tokens": ["Und", "f\u00e4ngt", "uns", "die", "M\u00e4us'", "und", "die", "Rat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}