{"textgrid.poem.60299": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Erster Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau d\u00fcstre Felsen sah ich trotzig ragen", "tokens": ["Grau", "d\u00fcst\u00b7re", "Fel\u00b7sen", "sah", "ich", "trot\u00b7zig", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus eines Tales stillen Finsternissen,", "tokens": ["Aus", "ei\u00b7nes", "Ta\u00b7les", "stil\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wollten k\u00fchn den Himmel sie verjagen,", "tokens": ["Als", "woll\u00b7ten", "k\u00fchn", "den", "Him\u00b7mel", "sie", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ADJD", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem sie den Schleier vom Gesicht gerissen.", "tokens": ["Dem", "sie", "den", "Schlei\u00b7er", "vom", "Ge\u00b7sicht", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Abgr\u00fcnde, ihre Riesengr\u00e4ber, lauern", "tokens": ["Ab\u00b7gr\u00fcn\u00b7de", ",", "ih\u00b7re", "Rie\u00b7sen\u00b7gr\u00e4\u00b7ber", ",", "lau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In sicherer Geduld zu ihren F\u00fc\u00dfen.", "tokens": ["In", "si\u00b7che\u00b7rer", "Ge\u00b7duld", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Kein Vogelsang, kein Bach, kein Waldesschauern;", "tokens": ["Kein", "Vo\u00b7gel\u00b7sang", ",", "kein", "Bach", ",", "kein", "Wal\u00b7des\u00b7schau\u00b7ern", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kein Klageton entf\u00e4hrt dem finstern Tale;", "tokens": ["Kein", "Kla\u00b7ge\u00b7ton", "ent\u00b7f\u00e4hrt", "dem", "fins\u00b7tern", "Ta\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nur stummes, unerme\u00dflich wildes Trauern.", "tokens": ["Nur", "stum\u00b7mes", ",", "un\u00b7er\u00b7me\u00df\u00b7lich", "wil\u00b7des", "Trau\u00b7ern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Einsam verk\u00fcmmert steht der Strauch, der kahle,", "tokens": ["Ein\u00b7sam", "ver\u00b7k\u00fcm\u00b7mert", "steht", "der", "Strauch", ",", "der", "kah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Hat Regen nur und Sturm und Frost erlebt,", "tokens": ["Hat", "Re\u00b7gen", "nur", "und", "Sturm", "und", "Frost", "er\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Stirbt ungeliebt vom s\u00fc\u00dfen Sonnenstrahle.", "tokens": ["Stirbt", "un\u00b7ge\u00b7liebt", "vom", "s\u00fc\u00b7\u00dfen", "Son\u00b7nen\u00b7strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "An seinen \u00c4sten, windgef\u00e4chelt, bebt", "tokens": ["An", "sei\u00b7nen", "\u00c4s\u00b7ten", ",", "wind\u00b7ge\u00b7f\u00e4\u00b7chelt", ",", "bebt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Wolle eines Lamms in stummer Klage,", "tokens": ["Die", "Wol\u00b7le", "ei\u00b7nes", "Lamms", "in", "stum\u00b7mer", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und des zerri\u00dfnen Blut am Boden klebt.", "tokens": ["Und", "des", "zer\u00b7ri\u00df\u00b7nen", "Blut", "am", "Bo\u00b7den", "klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Dort fliegt mit leisem, sattem Fl\u00fcgelschlage", "tokens": ["Dort", "fliegt", "mit", "lei\u00b7sem", ",", "sat\u00b7tem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ein Geier seinem Felsenhorste zu.", "tokens": ["Ein", "Gei\u00b7er", "sei\u00b7nem", "Fel\u00b7sen\u00b7hors\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Auf gr\u00fcner Trift, erquickt vom Sommertage,", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Trift", ",", "er\u00b7quickt", "vom", "Som\u00b7mer\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Schuldloses Lamm, wie fr\u00f6hlich irrtest du", "tokens": ["Schuld\u00b7lo\u00b7ses", "Lamm", ",", "wie", "fr\u00f6h\u00b7lich", "irr\u00b7test", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Mit deiner Weide friedlichen Genossen,", "tokens": ["Mit", "dei\u00b7ner", "Wei\u00b7de", "fried\u00b7li\u00b7chen", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Indes auf dich aus heitrer L\u00fcfte Ruh", "tokens": ["In\u00b7des", "auf", "dich", "aus", "hei\u00b7trer", "L\u00fcf\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Vormordend Geierblicke niederschossen!", "tokens": ["Vor\u00b7mor\u00b7dend", "Gei\u00b7er\u00b7bli\u00b7cke", "nie\u00b7der\u00b7schos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Der Geier, st\u00fcrzend sich in seinen Blick,", "tokens": ["Der", "Gei\u00b7er", ",", "st\u00fcr\u00b7zend", "sich", "in", "sei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Kommt pl\u00f6tzlich auf das Lamm herabgesto\u00dfen", "tokens": ["Kommt", "pl\u00f6tz\u00b7lich", "auf", "das", "Lamm", "her\u00b7ab\u00b7ge\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und rei\u00dft es fort aus seinem Jugendgl\u00fcck.", "tokens": ["Und", "rei\u00dft", "es", "fort", "aus", "sei\u00b7nem", "Ju\u00b7gend\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Hoch \u00fcber W\u00e4lder, Tale, Felsenriffe", "tokens": ["Hoch", "\u00fc\u00b7ber", "W\u00e4l\u00b7der", ",", "Ta\u00b7le", ",", "Fel\u00b7sen\u00b7rif\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Fliegt er damit in seine Nacht zur\u00fcck.", "tokens": ["Fliegt", "er", "da\u00b7mit", "in", "sei\u00b7ne", "Nacht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Es zittert, wimmert; doch mit festrem Griffe", "tokens": ["Es", "zit\u00b7tert", ",", "wim\u00b7mert", ";", "doch", "mit", "fest\u00b7rem", "Grif\u00b7fe"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Umklammert ers, ob sich am Angstgeschrei", "tokens": ["Um\u00b7klam\u00b7mert", "ers", ",", "ob", "sich", "am", "Angst\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Die scharfe Gier des M\u00f6rders sch\u00e4rfer schliffe. \u2013", "tokens": ["Die", "schar\u00b7fe", "Gier", "des", "M\u00f6r\u00b7ders", "sch\u00e4r\u00b7fer", "schlif\u00b7fe", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Nun drang ich tiefer, an dem Strauch vorbei,", "tokens": ["Nun", "drang", "ich", "tie\u00b7fer", ",", "an", "dem", "Strauch", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und wilder immer ward des Tales Grund,", "tokens": ["Und", "wil\u00b7der", "im\u00b7mer", "ward", "des", "Ta\u00b7les", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Die dunkle Wiege der Melancholei.", "tokens": ["Die", "dunk\u00b7le", "Wie\u00b7ge", "der", "Me\u00b7lan\u00b7cho\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Da bricht aus dornumstarrtem Felsenmund", "tokens": ["Da", "bricht", "aus", "dor\u00b7num\u00b7starr\u00b7tem", "Fel\u00b7sen\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Ein Quell hervor, die bange Ruh zu st\u00f6ren,", "tokens": ["Ein", "Quell", "her\u00b7vor", ",", "die", "ban\u00b7ge", "Ruh", "zu", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und braust hinunter in den offnen Schlund.", "tokens": ["Und", "braust", "hin\u00b7un\u00b7ter", "in", "den", "off\u00b7nen", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Unheimlich ist und grausenvoll zu h\u00f6ren", "tokens": ["Un\u00b7heim\u00b7lich", "ist", "und", "grau\u00b7sen\u00b7voll", "zu", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Das hohle Tosen in den Steinverliesen,", "tokens": ["Das", "hoh\u00b7le", "To\u00b7sen", "in", "den", "Stein\u00b7ver\u00b7lie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Wo murmelnd Nacht und Tod sich Treue schw\u00f6ren.", "tokens": ["Wo", "mur\u00b7melnd", "Nacht", "und", "Tod", "sich", "Treu\u00b7e", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Wie, trauernd nach verlernen Paradiesen,", "tokens": ["Wie", ",", "trau\u00b7ernd", "nach", "ver\u00b7ler\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Des Freundes Haupt ans Herz des Freundes f\u00e4llt,", "tokens": ["Des", "Freun\u00b7des", "Haupt", "ans", "Herz", "des", "Freun\u00b7des", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Umarmen sich die ernsten Felsenriesen.", "tokens": ["Um\u00b7ar\u00b7men", "sich", "die", "erns\u00b7ten", "Fel\u00b7sen\u00b7rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und weiter drang ich, \u2013 d\u00e4mmerlich erhellt", "tokens": ["Und", "wei\u00b7ter", "drang", "ich", ",", "\u2013", "d\u00e4m\u00b7mer\u00b7lich", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "$(", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "War mir die Schlucht; es fiel ein leiser Regen;", "tokens": ["War", "mir", "die", "Schlucht", ";", "es", "fiel", "ein", "lei\u00b7ser", "Re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Der Himmel Blitze durch die Felsen schnellt',", "tokens": ["Der", "Him\u00b7mel", "Blit\u00b7ze", "durch", "die", "Fel\u00b7sen", "schnellt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und fernher klangs von dumpfen Donnerschl\u00e4gen.", "tokens": ["Und", "fern\u00b7her", "klangs", "von", "dum\u00b7pfen", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Gar seltsam bleich erschien mir das Gesicht", "tokens": ["Gar", "selt\u00b7sam", "bleich", "er\u00b7schien", "mir", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Des Eremiten, der mir trat entgegen.", "tokens": ["Des", "E\u00b7re\u00b7mi\u00b7ten", ",", "der", "mir", "trat", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Es wankt um ihn ein zweife