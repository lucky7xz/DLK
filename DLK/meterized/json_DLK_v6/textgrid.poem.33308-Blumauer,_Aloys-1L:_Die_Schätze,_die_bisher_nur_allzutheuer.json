{"textgrid.poem.33308": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sch\u00e4tze, die bisher nur allzutheuer", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00e4tze, die bisher nur allzutheuer", "tokens": ["Die", "Sch\u00e4t\u00b7ze", ",", "die", "bis\u00b7her", "nur", "all\u00b7zu\u00b7theu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich die Natur von uns bezahlen lie\u00df,", "tokens": ["Sich", "die", "Na\u00b7tur", "von", "uns", "be\u00b7zah\u00b7len", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die der Mensch ihr nur durch Gift und Feuer,", "tokens": ["Und", "die", "der", "Mensch", "ihr", "nur", "durch", "Gift", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und durch Gewalt mit lahmer Hand entri\u00df,", "tokens": ["Und", "durch", "Ge\u00b7walt", "mit", "lah\u00b7mer", "Hand", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die schenkt sie dir \u2013 zum sichern Unterpfand,", "tokens": ["Die", "schenkt", "sie", "dir", "\u2013", "zum", "si\u00b7chern", "Un\u00b7ter\u00b7pfand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df du ihr Liebling bist \u2013 auf einen Druck der Hand", "tokens": ["Da\u00df", "du", "ihr", "Lieb\u00b7ling", "bist", "\u2013", "auf", "ei\u00b7nen", "Druck", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$(", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}