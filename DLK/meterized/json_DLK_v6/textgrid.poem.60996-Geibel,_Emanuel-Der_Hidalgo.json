{"textgrid.poem.60996": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Hidalgo", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist so s\u00fc\u00df, zu scherzen", "tokens": ["Es", "ist", "so", "s\u00fc\u00df", ",", "zu", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Liedern und mit Herzen", "tokens": ["Mit", "Lie\u00b7dern", "und", "mit", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem ernsten Streit.", "tokens": ["Und", "mit", "dem", "erns\u00b7ten", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ergl\u00e4nzt des Mondes Schimmer,", "tokens": ["Er\u00b7gl\u00e4nzt", "des", "Mon\u00b7des", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da treibt's mich fort vom Zimmer", "tokens": ["Da", "treibt's", "mich", "fort", "vom", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Platz und Gassen weit;", "tokens": ["Durch", "Platz", "und", "Gas\u00b7sen", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da bin zur Lieb' ich immer", "tokens": ["Da", "bin", "zur", "Lieb'", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Wie zum Gefecht bereit.", "tokens": ["Wie", "zum", "Ge\u00b7fecht", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00f6nen von Sevilla", "tokens": ["Die", "Sch\u00f6\u00b7nen", "von", "Se\u00b7vil\u00b7la"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit F\u00e4cher und Mantilla", "tokens": ["Mit", "F\u00e4\u00b7cher", "und", "Man\u00b7til\u00b7la"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Blicken den Strom entlang;", "tokens": ["Bli\u00b7cken", "den", "Strom", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sie lauschen mit Gefallen,", "tokens": ["Sie", "lau\u00b7schen", "mit", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn meine Lieder schallen", "tokens": ["Wenn", "mei\u00b7ne", "Lie\u00b7der", "schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Mandolinenklang,", "tokens": ["Zum", "Man\u00b7do\u00b7li\u00b7nen\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und dunkle Rosen fallen", "tokens": ["Und", "dunk\u00b7le", "Ro\u00b7sen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir vom Balkon zum Dank.", "tokens": ["Mir", "vom", "Bal\u00b7kon", "zum", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich trage, wenn ich singe,", "tokens": ["Ich", "tra\u00b7ge", ",", "wenn", "ich", "sin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zither und die Klinge", "tokens": ["Die", "Zi\u00b7ther", "und", "die", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Toledanischem Stahl.", "tokens": ["Von", "To\u00b7le\u00b7da\u00b7ni\u00b7schem", "Stahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich sing' an manchem Gitter", "tokens": ["Ich", "sing'", "an", "man\u00b7chem", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und h\u00f6hne manchen Ritter", "tokens": ["Und", "h\u00f6h\u00b7ne", "man\u00b7chen", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit keckem Lied zumal.", "tokens": ["Mit", "ke\u00b7ckem", "Lied", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Dame gilt die Zither,", "tokens": ["Der", "Da\u00b7me", "gilt", "die", "Zi\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Klinge dem Rival.", "tokens": ["Die", "Klin\u00b7ge", "dem", "Ri\u00b7val", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf denn zum Abenteuer!", "tokens": ["Auf", "denn", "zum", "A\u00b7bent\u00b7eu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon losch der Sonne Feuer", "tokens": ["Schon", "losch", "der", "Son\u00b7ne", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinter den Bergen aus;", "tokens": ["Hin\u00b7ter", "den", "Ber\u00b7gen", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Mondnacht D\u00e4mmerstunden,", "tokens": ["Der", "Mond\u00b7nacht", "D\u00e4m\u00b7mer\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie bringen Liebeskunden,", "tokens": ["Sie", "brin\u00b7gen", "Lie\u00b7bes\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie bringen blut'gen Strau\u00df;", "tokens": ["Sie", "brin\u00b7gen", "blut'\u00b7gen", "Strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Blumen oder Wunden", "tokens": ["Und", "Blu\u00b7men", "o\u00b7der", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Trag' morgen ich nach Haus.", "tokens": ["Trag'", "mor\u00b7gen", "ich", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}