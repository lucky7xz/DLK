{"textgrid.poem.63728": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Tage so golden verfliegen,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Tage so golden verfliegen,", "tokens": ["Wie", "die", "Ta\u00b7ge", "so", "gol\u00b7den", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wie die Nacht sich so selig vertr\u00e4umt,", "tokens": ["Wie", "die", "Nacht", "sich", "so", "se\u00b7lig", "ver\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wo am Felsen mit Wogen und Wiegen", "tokens": ["Wo", "am", "Fel\u00b7sen", "mit", "Wo\u00b7gen", "und", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die gelandete Welle versch\u00e4umt,", "tokens": ["Die", "ge\u00b7lan\u00b7de\u00b7te", "Wel\u00b7le", "ver\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wo sich Blumen und Fr\u00fcchte gesellen,", "tokens": ["Wo", "sich", "Blu\u00b7men", "und", "Fr\u00fcch\u00b7te", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Da\u00df das Herz dir in Staunen entbrennt \u2013", "tokens": ["Da\u00df", "das", "Herz", "dir", "in", "Stau\u00b7nen", "ent\u00b7brennt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "O du schimmernde Bl\u00fcte der Wellen,", "tokens": ["O", "du", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7te", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sei gegr\u00fc\u00dft, du mein sch\u00f6nes Sorrent!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mein", "sch\u00f6\u00b7nes", "Sor\u00b7rent", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die Nacht, wenn so s\u00fc\u00df Luisella", "tokens": ["Und", "die", "Nacht", ",", "wenn", "so", "s\u00fc\u00df", "Lu\u00b7i\u00b7sel\u00b7la"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "ADV", "ADJD", "NE"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihre lachenden Lieder uns singt", "tokens": ["Ih\u00b7re", "la\u00b7chen\u00b7den", "Lie\u00b7der", "uns", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und der Wirbel der Lust, Tarantella,", "tokens": ["Und", "der", "Wir\u00b7bel", "der", "Lust", ",", "Ta\u00b7ran\u00b7tel\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Fl\u00e4mmchen im Sturme sie schwingt.", "tokens": ["Wie", "ein", "Fl\u00e4mm\u00b7chen", "im", "Stur\u00b7me", "sie", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "An der Bucht sich die G\u00e4rten erhellen", "tokens": ["An", "der", "Bucht", "sich", "die", "G\u00e4r\u00b7ten", "er\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "ART", "NN", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Unterm leuchtenden Nachtfirmament \u2013", "tokens": ["Un\u00b7term", "leuch\u00b7ten\u00b7den", "Nacht\u00b7fir\u00b7ma\u00b7ment", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "O du schimmernde Bl\u00fcte der Wellen,", "tokens": ["O", "du", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7te", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sei gegr\u00fc\u00dft, du mein sch\u00f6nes Sorrent!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mein", "sch\u00f6\u00b7nes", "Sor\u00b7rent", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hier entrinnst du der Sorgen Getriebe,", "tokens": ["Hier", "ent\u00b7rinnst", "du", "der", "Sor\u00b7gen", "Ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und es tr\u00e4gt dich auf H\u00e4nden die Lust,", "tokens": ["Und", "es", "tr\u00e4gt", "dich", "auf", "H\u00e4n\u00b7den", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und sogar das Ged\u00e4chtnis der Liebe,", "tokens": ["Und", "so\u00b7gar", "das", "Ge\u00b7d\u00e4cht\u00b7nis", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Hier beschleicht es gelinder die Brust.", "tokens": ["Hier", "be\u00b7schleicht", "es", "ge\u00b7lin\u00b7der", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und du tauchst in die heilenden Quellen,", "tokens": ["Und", "du", "tauchst", "in", "die", "hei\u00b7len\u00b7den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "In des heiligen Meers Element \u2013", "tokens": ["In", "des", "hei\u00b7li\u00b7gen", "Meers", "E\u00b7le\u00b7ment", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "O du schimmernde Bl\u00fcte der Wellen,", "tokens": ["O", "du", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7te", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sei gegr\u00fc\u00dft, du mein sch\u00f6nes Sorrent!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mein", "sch\u00f6\u00b7nes", "Sor\u00b7rent", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Auch der tobenden St\u00fcrme Get\u00fcmmel,", "tokens": ["Auch", "der", "to\u00b7ben\u00b7den", "St\u00fcr\u00b7me", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Hier belebt es nur Bl\u00fcten zuhauf,", "tokens": ["Hier", "be\u00b7lebt", "es", "nur", "Bl\u00fc\u00b7ten", "zu\u00b7hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und es l\u00f6sen die Wetter am Himmel", "tokens": ["Und", "es", "l\u00f6\u00b7sen", "die", "Wet\u00b7ter", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "In ein fruchtbar Geriesel sich auf.", "tokens": ["In", "ein", "frucht\u00b7bar", "Ge\u00b7rie\u00b7sel", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wenn die Fr\u00fcchte, die herbstlichen, schwellen,", "tokens": ["Wenn", "die", "Fr\u00fcch\u00b7te", ",", "die", "herbst\u00b7li\u00b7chen", ",", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "VVINF", "$,", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Ach, wie weit, ach, wie bin ich getrennt!", "tokens": ["Ach", ",", "wie", "weit", ",", "ach", ",", "wie", "bin", "ich", "ge\u00b7trennt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "$,", "ITJ", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Dann ade, o du Bl\u00fcte der Wellen,", "tokens": ["Dann", "a\u00b7de", ",", "o", "du", "Bl\u00fc\u00b7te", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann ade, du mein sch\u00f6nes Sorrent!", "tokens": ["Dann", "a\u00b7de", ",", "du", "mein", "sch\u00f6\u00b7nes", "Sor\u00b7rent", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}