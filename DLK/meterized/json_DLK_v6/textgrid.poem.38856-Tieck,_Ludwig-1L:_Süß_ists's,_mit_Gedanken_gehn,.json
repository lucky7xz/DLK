{"textgrid.poem.38856": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00df ists's, mit Gedanken gehn,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df ists's, mit Gedanken gehn,", "tokens": ["S\u00fc\u00df", "ist\u00b7s's", ",", "mit", "Ge\u00b7dan\u00b7ken", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die uns zur Geliebten leiten,", "tokens": ["Die", "uns", "zur", "Ge\u00b7lieb\u00b7ten", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo von blumbewachsnen H\u00f6hn,", "tokens": ["Wo", "von", "blum\u00b7be\u00b7wachs\u00b7nen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnenstrahlen sich verbreiten.", "tokens": ["Son\u00b7nen\u00b7strah\u00b7len", "sich", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lilien sagen: unser Licht", "tokens": ["Li\u00b7li\u00b7en", "sa\u00b7gen", ":", "un\u00b7ser", "Licht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$.", "PPOSAT", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ist es, was die Wange schm\u00fccket;", "tokens": ["Ist", "es", ",", "was", "die", "Wan\u00b7ge", "schm\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsern Schein die Liebste blicket:", "tokens": ["Un\u00b7sern", "Schein", "die", "Liebs\u00b7te", "bli\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So das blaue Veilchen spricht.", "tokens": ["So", "das", "blau\u00b7e", "Veil\u00b7chen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mit sanfter R\u00f6the l\u00e4cheln", "tokens": ["Und", "mit", "sanf\u00b7ter", "R\u00f6\u00b7the", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen ob dem Uebermuth,", "tokens": ["Ro\u00b7sen", "ob", "dem", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchle Abendwinde f\u00e4cheln", "tokens": ["K\u00fch\u00b7le", "Ab\u00b7end\u00b7win\u00b7de", "f\u00e4\u00b7cheln"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die liebevolle Gluth.", "tokens": ["Durch", "die", "lie\u00b7be\u00b7vol\u00b7le", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "All' ihr s\u00fc\u00dfen Bl\u00fcmelein,", "tokens": ["All'", "ihr", "s\u00fc\u00b7\u00dfen", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei es Farbe, sei's Gestalt,", "tokens": ["Sei", "es", "Far\u00b7be", ",", "sei's", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mahlt mit liebender Gewalt", "tokens": ["Mahlt", "mit", "lie\u00b7ben\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Liebsten hellen Schein,", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zankt nicht, zarte Bl\u00fcmelein.", "tokens": ["Zankt", "nicht", ",", "zar\u00b7te", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rosen, duftende Narzissen,", "tokens": ["Ro\u00b7sen", ",", "duf\u00b7ten\u00b7de", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Blumen sch\u00f6ner prangen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "sch\u00f6\u00b7ner", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie ihren Busen k\u00fcssen", "tokens": ["Wenn", "sie", "ih\u00b7ren", "Bu\u00b7sen", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder in den Locken hangen,", "tokens": ["O\u00b7der", "in", "den", "Lo\u00b7cken", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blaue Veilchen, bunte Nelken,", "tokens": ["Blau\u00b7e", "Veil\u00b7chen", ",", "bun\u00b7te", "Nel\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sie sie zur Zierde pfl\u00fcckt,", "tokens": ["Wenn", "sie", "sie", "zur", "Zier\u00b7de", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fcssen gern als Putz verwelken,", "tokens": ["M\u00fcs\u00b7sen", "gern", "als", "Putz", "ver\u00b7wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch den s\u00fc\u00dfen Tod begl\u00fcckt.", "tokens": ["Durch", "den", "s\u00fc\u00b7\u00dfen", "Tod", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lehrer sind mir diese Bl\u00fcthen,", "tokens": ["Leh\u00b7rer", "sind", "mir", "die\u00b7se", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich thue wie sie thun,", "tokens": ["Und", "ich", "thue", "wie", "sie", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "PPER", "VVINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Folge ihnen, wie sie riethen,", "tokens": ["Fol\u00b7ge", "ih\u00b7nen", ",", "wie", "sie", "riet\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! ich will gern alles bieten,", "tokens": ["Ach", "!", "ich", "will", "gern", "al\u00b7les", "bie\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ich ihr am Busen ruhn.", "tokens": ["Kann", "ich", "ihr", "am", "Bu\u00b7sen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht auf Jahre sie erwerben,", "tokens": ["Nicht", "auf", "Jah\u00b7re", "sie", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, nur kurze, kleine Zeit,", "tokens": ["Nein", ",", "nur", "kur\u00b7ze", ",", "klei\u00b7ne", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann in ihren Armen sterben,", "tokens": ["Dann", "in", "ih\u00b7ren", "Ar\u00b7men", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterben ohne Wunsch und Neid.", "tokens": ["Ster\u00b7ben", "oh\u00b7ne", "Wunsch", "und", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach! wie manche Blume klaget", "tokens": ["Ach", "!", "wie", "man\u00b7che", "Blu\u00b7me", "kla\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam hier im stillen Thal,", "tokens": ["Ein\u00b7sam", "hier", "im", "stil\u00b7len", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie verwelket eh es taget,", "tokens": ["Sie", "ver\u00b7wel\u00b7ket", "eh", "es", "ta\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stirbt beim ersten Sonnenstrahl:", "tokens": ["Stirbt", "beim", "ers\u00b7ten", "Son\u00b7nen\u00b7strahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! so bitter herzlich naget", "tokens": ["Ach", "!", "so", "bit\u00b7ter", "herz\u00b7lich", "na\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ADJD", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch an mir die scharfe Qual,", "tokens": ["Auch", "an", "mir", "die", "schar\u00b7fe", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da\u00df ich sie und all mein Gl\u00fccke,", "tokens": ["Da\u00df", "ich", "sie", "und", "all", "mein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimmer, nimmermehr erblicke.", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer\u00b7mehr", "er\u00b7bli\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}