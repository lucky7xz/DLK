{"textgrid.poem.36658": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schlachtfeld von Tagliacozzo", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thal der Imele, dich gr\u00fc\u00df' ich! In deinen weiten Gefilden", "tokens": ["Thal", "der", "I\u00b7me\u00b7le", ",", "dich", "gr\u00fc\u00df'", "ich", "!", "In", "dei\u00b7nen", "wei\u00b7ten", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sank im verzweifelten Kampf unser Heroengeschlecht!", "tokens": ["Sank", "im", "ver\u00b7zwei\u00b7fel\u00b7ten", "Kampf", "un\u00b7ser", "He\u00b7ro\u00b7en\u00b7ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drum verehr' ich dich auch, du bist sch\u00f6n, doch gefielst du mir besser,", "tokens": ["Drum", "ver\u00b7ehr'", "ich", "dich", "auch", ",", "du", "bist", "sch\u00f6n", ",", "doch", "ge\u00b7fielst", "du", "mir", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "H\u00e4tte Conradin hier, h\u00e4tte der Deutsche gesiegt.", "tokens": ["H\u00e4t\u00b7te", "Con\u00b7ra\u00b7din", "hier", ",", "h\u00e4t\u00b7te", "der", "Deut\u00b7sche", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}