{"textgrid.poem.66635": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Schwermut", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau liegt die Luft, der Wind fliegt bang,", "tokens": ["Grau", "liegt", "die", "Luft", ",", "der", "Wind", "fliegt", "bang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Regen rinnt, den Wald entlang", "tokens": ["Der", "Re\u00b7gen", "rinnt", ",", "den", "Wald", "ent\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht Seufzerzug, singt Grabgesang ...", "tokens": ["Zieht", "Seuf\u00b7zer\u00b7zug", ",", "singt", "Grab\u00b7ge\u00b7sang", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun streut die Schwermut ihre Keime", "tokens": ["Nun", "streut", "die", "Schwer\u00b7mut", "ih\u00b7re", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In angstgefurchte Herzen ein,", "tokens": ["In", "angst\u00b7ge\u00b7furch\u00b7te", "Her\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dunkel abget\u00f6nte Reime", "tokens": ["In", "dun\u00b7kel", "ab\u00b7ge\u00b7t\u00f6n\u00b7te", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verh\u00fcllt der Dichter seine Pein.", "tokens": ["Ver\u00b7h\u00fcllt", "der", "Dich\u00b7ter", "sei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, wer sein Weh zu Rhythmen flicht,", "tokens": ["Ach", ",", "wer", "sein", "Weh", "zu", "Rhyth\u00b7men", "flicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der ist noch lang der \u00c4rmste nicht;", "tokens": ["Der", "ist", "noch", "lang", "der", "\u00c4rms\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Doch wer um Gl\u00fcck und Lust betrogen", "tokens": ["Doch", "wer", "um", "Gl\u00fcck", "und", "Lust", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Stirn an Fensterscheiben pre\u00dft,", "tokens": ["Die", "Stirn", "an", "Fens\u00b7ter\u00b7schei\u00b7ben", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer grauenschwer hinabgezogen", "tokens": ["Wer", "grau\u00b7en\u00b7schwer", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sich tief und tiefer treiben l\u00e4\u00dft;", "tokens": ["Sich", "tief", "und", "tie\u00b7fer", "trei\u00b7ben", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wem Kraft und Wille treu selband", "tokens": ["Wem", "Kraft", "und", "Wil\u00b7le", "treu", "sel\u00b7band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In Unkraft und Verzweiflung schwand;", "tokens": ["In", "Un\u00b7kraft", "und", "Ver\u00b7zwei\u00b7flung", "schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wer schon zu m\u00fcd, den Feind zu fassen,", "tokens": ["Wer", "schon", "zu", "m\u00fcd", ",", "den", "Feind", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKA", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der ihn erw\u00fcrgt, zum Tod gelassen,", "tokens": ["Der", "ihn", "er\u00b7w\u00fcrgt", ",", "zum", "Tod", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Verkohlend sich in Asche schiebt", "tokens": ["Ver\u00b7koh\u00b7lend", "sich", "in", "A\u00b7sche", "schiebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und nicht mehr leuchtet, nicht mehr liebt \u2013", "tokens": ["Und", "nicht", "mehr", "leuch\u00b7tet", ",", "nicht", "mehr", "liebt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "$,", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Kein Klang rei\u00dft die zerstampfte Seele", "tokens": ["Kein", "Klang", "rei\u00dft", "die", "zer\u00b7stampf\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Aus ihrer dumpfen Kerkerh\u00f6hle ...", "tokens": ["Aus", "ih\u00b7rer", "dum\u00b7pfen", "Ker\u00b7ker\u00b7h\u00f6h\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Luft liegt grau, der Wind fliegt bang,", "tokens": ["Die", "Luft", "liegt", "grau", ",", "der", "Wind", "fliegt", "bang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Regen spinnt, den Wald entlang", "tokens": ["Der", "Re\u00b7gen", "spinnt", ",", "den", "Wald", "ent\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Zieht Seufzerzug, singt Grabgesang ...", "tokens": ["Zieht", "Seuf\u00b7zer\u00b7zug", ",", "singt", "Grab\u00b7ge\u00b7sang", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}