{"textgrid.poem.31324": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Marquise von O ...", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser Roman ist nicht f\u00fcr dich, meine Tochter. In Ohnmacht!", "tokens": ["Die\u00b7ser", "Ro\u00b7man", "ist", "nicht", "f\u00fcr", "dich", ",", "mei\u00b7ne", "Toch\u00b7ter", ".", "In", "Ohn\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schamlose Posse! Sie hielt, wei\u00df ich, die Augen blo\u00df zu.", "tokens": ["Scham\u00b7lo\u00b7se", "Pos\u00b7se", "!", "Sie", "hielt", ",", "wei\u00df", "ich", ",", "die", "Au\u00b7gen", "blo\u00df", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}