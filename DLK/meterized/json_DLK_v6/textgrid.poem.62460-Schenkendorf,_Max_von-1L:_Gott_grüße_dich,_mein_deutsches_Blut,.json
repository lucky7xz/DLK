{"textgrid.poem.62460": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott gr\u00fc\u00dfe dich, mein deutsches Blut,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott gr\u00fc\u00dfe dich, mein deutsches Blut,", "tokens": ["Gott", "gr\u00fc\u00b7\u00dfe", "dich", ",", "mein", "deut\u00b7sches", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Siegeslust und Ehren,", "tokens": ["Mit", "Sie\u00b7ges\u00b7lust", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er wolle dir den Heldenmuth", "tokens": ["Er", "wol\u00b7le", "dir", "den", "Hel\u00b7den\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit edler Speise n\u00e4hren.", "tokens": ["Mit", "ed\u00b7ler", "Spei\u00b7se", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wandle muthig weiter fort", "tokens": ["O", "wand\u00b7le", "mut\u00b7hig", "wei\u00b7ter", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dienst der Ewig-Reinen,", "tokens": ["Im", "Dienst", "der", "E\u00b7wig\u00b7Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df auch an dem S\u00fcndenort", "tokens": ["Und", "la\u00df", "auch", "an", "dem", "S\u00fcn\u00b7de\u00b7nort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die deutschen Ehren scheinen.", "tokens": ["Die", "deut\u00b7schen", "Eh\u00b7ren", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die deutsche Keuschheit, deutsche Scham,", "tokens": ["Die", "deut\u00b7sche", "Keuschheit", ",", "deut\u00b7sche", "Scham", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Scheu vor allen Ketten,", "tokens": ["Die", "Scheu", "vor", "al\u00b7len", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lust an Freiheit, welche kam", "tokens": ["Die", "Lust", "an", "Frei\u00b7heit", ",", "wel\u00b7che", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst Feindesland zu retten.", "tokens": ["Selbst", "Fein\u00b7des\u00b7land", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zur Heimat wende dich, mein Kind,", "tokens": ["Zur", "Hei\u00b7mat", "wen\u00b7de", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Kummer dich erf\u00fcllet,", "tokens": ["Wenn", "Kum\u00b7mer", "dich", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Aufgang weht ein frischer Wind,", "tokens": ["Vom", "Auf\u00b7gang", "weht", "ein", "fri\u00b7scher", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ha\u00df und Schmerzen stillet.", "tokens": ["Der", "Ha\u00df", "und", "Schmer\u00b7zen", "stil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O bleibe fremd, o bleibe fern", "tokens": ["O", "blei\u00b7be", "fremd", ",", "o", "blei\u00b7be", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "$,", "FM", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den S\u00fcndern und den Blinden,", "tokens": ["Den", "S\u00fcn\u00b7dern", "und", "den", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird der Deutschen Ehr' und Stern", "tokens": ["Dann", "wird", "der", "Deut\u00b7schen", "Ehr'", "und", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir nimmermehr verschwinden.", "tokens": ["Dir", "nim\u00b7mer\u00b7mehr", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der V\u00e4ter Segen ruht auf dir,", "tokens": ["Der", "V\u00e4\u00b7ter", "Se\u00b7gen", "ruht", "auf", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat dich treu geleitet,", "tokens": ["Er", "hat", "dich", "treu", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dir in sch\u00f6ner Heimat hier", "tokens": ["Und", "dir", "in", "sch\u00f6\u00b7ner", "Hei\u00b7mat", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar holden Lohn bereitet.", "tokens": ["Gar", "hol\u00b7den", "Lohn", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein deutsches M\u00e4dchen will als Braut", "tokens": ["Ein", "deut\u00b7sches", "M\u00e4d\u00b7chen", "will", "als", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den deutschen Helden gr\u00fc\u00dfen;", "tokens": ["Den", "deut\u00b7schen", "Hel\u00b7den", "gr\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sie j\u00fcngst ein Myrthenkraut", "tokens": ["Ich", "sah", "sie", "j\u00fcngst", "ein", "Myr\u00b7then\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im K\u00e4mmerlein begie\u00dfen.", "tokens": ["Im", "K\u00e4m\u00b7mer\u00b7lein", "be\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}