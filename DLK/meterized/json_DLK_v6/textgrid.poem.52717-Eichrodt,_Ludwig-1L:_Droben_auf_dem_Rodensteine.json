{"textgrid.poem.52717": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Droben auf dem Rodensteine", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droben auf dem Rodensteine", "tokens": ["Dro\u00b7ben", "auf", "dem", "Ro\u00b7dens\u00b7tei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut sich auf ein Felsenspalt,", "tokens": ["Thut", "sich", "auf", "ein", "Fel\u00b7sen\u00b7spalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sturm erwacht \u2013 im Sternenscheine", "tokens": ["Sturm", "er\u00b7wacht", "\u2013", "im", "Ster\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$(", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt der bleiche Odenwald.", "tokens": ["Liegt", "der", "blei\u00b7che", "O\u00b7den\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schatten, seltsam, unbest\u00e4ndig,", "tokens": ["Schat\u00b7ten", ",", "selt\u00b7sam", ",", "un\u00b7be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gaukeln um den Schnellertsberg,", "tokens": ["Gau\u00b7keln", "um", "den", "Schnel\u00b7lerts\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Burg wird es lebendig,", "tokens": ["Auf", "der", "Burg", "wird", "es", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Laut im alten Mauerwerk.", "tokens": ["Laut", "im", "al\u00b7ten", "Mau\u00b7er\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Waffen blitzen durch die Fenster,", "tokens": ["Waf\u00b7fen", "blit\u00b7zen", "durch", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosse tummeln auf dem Wall,", "tokens": ["Ros\u00b7se", "tum\u00b7meln", "auf", "dem", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Wirrwarr der Gespenster", "tokens": ["In", "den", "Wirr\u00b7warr", "der", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brauset jetzt Drommetenschall.", "tokens": ["Brau\u00b7set", "jetzt", "Drom\u00b7me\u00b7ten\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Schlachtruf t\u00f6net, Hunde kl\u00e4ffen,", "tokens": ["Schlach\u00b7truf", "t\u00f6\u00b7net", ",", "Hun\u00b7de", "kl\u00e4f\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und herab vom Geisterschlo\u00df,", "tokens": ["Und", "her\u00b7ab", "vom", "Geis\u00b7ter\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wuchtig auf den Feind zu treffen,", "tokens": ["Wuch\u00b7tig", "auf", "den", "Feind", "zu", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4lzt sich ein verwegner Tro\u00df.", "tokens": ["W\u00e4lzt", "sich", "ein", "ver\u00b7weg\u00b7ner", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt das echotolle Lachen!", "tokens": ["H\u00f6rt", "das", "ech\u00b7o\u00b7tol\u00b7le", "La\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei, die Gegner brechen los!", "tokens": ["Hei", ",", "die", "Geg\u00b7ner", "bre\u00b7chen", "los", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie vom Donner W\u00e4lder krachen", "tokens": ["Wie", "vom", "Don\u00b7ner", "W\u00e4l\u00b7der", "kra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fesselt sie der Lanzensto\u00df.", "tokens": ["Fes\u00b7selt", "sie", "der", "Lan\u00b7zen\u00b7sto\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sie springen von den Thieren,", "tokens": ["Und", "sie", "sprin\u00b7gen", "von", "den", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es entbrennt der Schwerterstreit;", "tokens": ["Es", "ent\u00b7brennt", "der", "Schwer\u00b7ter\u00b7streit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie grimme Fehde f\u00fchren,", "tokens": ["Wie", "sie", "grim\u00b7me", "Feh\u00b7de", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es toset meilenweit!", "tokens": ["Da\u00df", "es", "to\u00b7set", "mei\u00b7len\u00b7weit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und so tost es \u2013 bis die Sterne", "tokens": ["Und", "so", "tost", "es", "\u2013", "bis", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleichen, bis verbla\u00dft der Tro\u00df,", "tokens": ["Blei\u00b7chen", ",", "bis", "ver\u00b7bla\u00dft", "der", "Tro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis in stille Nebelferne", "tokens": ["Bis", "in", "stil\u00b7le", "Ne\u00b7bel\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwindet Reitersmann und Ro\u00df.", "tokens": ["Schwin\u00b7det", "Rei\u00b7ters\u00b7mann", "und", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}