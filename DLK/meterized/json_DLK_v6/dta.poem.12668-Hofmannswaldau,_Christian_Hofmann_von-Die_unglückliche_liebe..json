{"dta.poem.12668": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die ungl\u00fcckliche liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine liebe gleicht den bl\u00e4ttern,", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "gleicht", "den", "bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den rosen-stock geziert,", "tokens": ["Die", "den", "ro\u00b7sen\u00b7stock", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von vielen ungl\u00fccks-wettern", "tokens": ["Und", "von", "vie\u00b7len", "un\u00b7gl\u00fccks\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun demselben sind entf\u00fchrt,", "tokens": ["Nun", "dem\u00b7sel\u00b7ben", "sind", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die, von st\u00fcrmen gantz zerrissen,", "tokens": ["Die", ",", "von", "st\u00fcr\u00b7men", "gantz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun den staub der erde k\u00fcssen.", "tokens": ["Nun", "den", "staub", "der", "er\u00b7de", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor war alles zum vergn\u00fcgen", "tokens": ["Vor", "war", "al\u00b7les", "zum", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PIS", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zur wohllust angelegt,", "tokens": ["Und", "zur", "wohl\u00b7lust", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da es nun sehr hoch gestiegen,", "tokens": ["Da", "es", "nun", "sehr", "hoch", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Was in mir das gl\u00fcck erregt,", "tokens": ["Was", "in", "mir", "das", "gl\u00fcck", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00e4llt der bau, und mein vergn\u00fcgen", "tokens": ["F\u00e4llt", "der", "bau", ",", "und", "mein", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df durch dessen fall erliegen.", "tokens": ["Mu\u00df", "durch", "des\u00b7sen", "fall", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dornen bleiben mir zum besten,", "tokens": ["Dor\u00b7nen", "blei\u00b7ben", "mir", "zum", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ged\u00e4chtnis jener lust", "tokens": ["Das", "ge\u00b7d\u00e4cht\u00b7nis", "je\u00b7ner", "lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lagert sich mit andern g\u00e4sten", "tokens": ["La\u00b7gert", "sich", "mit", "an\u00b7dern", "g\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener noth in meine brust,", "tokens": ["Je\u00b7ner", "noth", "in", "mei\u00b7ne", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles wird nach jener liebe", "tokens": ["Al\u00b7les", "wird", "nach", "je\u00b7ner", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun bey mir verwirrt und tr\u00fcbe.", "tokens": ["Nun", "bey", "mir", "ver\u00b7wirrt", "und", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch ich will es gerne leiden,", "tokens": ["Doch", "ich", "will", "es", "ger\u00b7ne", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das schicksal mir bestimmt,", "tokens": ["Was", "das", "schick\u00b7sal", "mir", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mesolan an statt der seiden,", "tokens": ["Me\u00b7so\u00b7lan", "an", "statt", "der", "sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauch wo vor die gluth geglimmt,", "tokens": ["Rauch", "wo", "vor", "die", "gluth", "ge\u00b7glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die wie weyrauch flammen spreite,", "tokens": ["Die", "wie", "wey\u00b7rauch", "flam\u00b7men", "sprei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die man den hertzen weihte.", "tokens": ["Und", "die", "man", "den", "hert\u00b7zen", "weih\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Tr\u00f6ste dich demnach, mein hertze!", "tokens": ["Tr\u00f6s\u00b7te", "dich", "dem\u00b7nach", ",", "mein", "hert\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun auf dieser dornen-bahn,", "tokens": ["Nun", "auf", "die\u00b7ser", "dor\u00b7nen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dencke da\u00df es nach dem schmertze", "tokens": ["Den\u00b7cke", "da\u00df", "es", "nach", "dem", "schmert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder besser werden kan;", "tokens": ["Wie\u00b7der", "bes\u00b7ser", "wer\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wechseln herrscht ohndem in lieben.", "tokens": ["Wech\u00b7seln", "herrscht", "ohn\u00b7dem", "in", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noth auf lust, lust auf betr\u00fcben.", "tokens": ["Noth", "auf", "lust", ",", "lust", "auf", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mag ein seuffzer mich begleiten,", "tokens": ["Mag", "ein", "seuff\u00b7zer", "mich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich sterb an deiner seiten", "tokens": ["Wenn", "ich", "sterb", "an", "dei\u00b7ner", "sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mir auch im grabe wohl;", "tokens": ["Ist", "mir", "auch", "im", "gra\u00b7be", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mir deinen mund versprechen,", "tokens": ["La\u00df", "mir", "dei\u00b7nen", "mund", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die matten augen brechen,", "tokens": ["Wenn", "die", "mat\u00b7ten", "au\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich dein auch sterben soll.", "tokens": ["Da\u00df", "ich", "dein", "auch", "ster\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00df auf deinen weichen klippen,", "tokens": ["La\u00df", "auf", "dei\u00b7nen", "wei\u00b7chen", "klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den strande deiner lippen", "tokens": ["An", "den", "stran\u00b7de", "dei\u00b7ner", "lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht mein schiff zu grunde gehn;", "tokens": ["Nicht", "mein", "schiff", "zu", "grun\u00b7de", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich mit verliebtem winde", "tokens": ["La\u00df", "mich", "mit", "ver\u00b7lieb\u00b7tem", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Segeln, so kan ich geschwinde", "tokens": ["Se\u00b7geln", ",", "so", "kan", "ich", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "ADJA"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Den verlangten hafen sehn.", "tokens": ["Den", "ver\u00b7lang\u00b7ten", "ha\u00b7fen", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Will dein hertze mich verlassen,", "tokens": ["Will", "dein", "hert\u00b7ze", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So will ich mit lust erblassen.", "tokens": ["So", "will", "ich", "mit", "lust", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und verschmachten in der brunst;", "tokens": ["Und", "ver\u00b7schmach\u00b7ten", "in", "der", "brunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Deinen mund einmal zu k\u00fcssen,", "tokens": ["Dei\u00b7nen", "mund", "ein\u00b7mal", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll mir meinen tod vers\u00fcssen,", "tokens": ["Soll", "mir", "mei\u00b7nen", "tod", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterb ich nur in deiner gunst!", "tokens": ["Sterb", "ich", "nur", "in", "dei\u00b7ner", "gunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}