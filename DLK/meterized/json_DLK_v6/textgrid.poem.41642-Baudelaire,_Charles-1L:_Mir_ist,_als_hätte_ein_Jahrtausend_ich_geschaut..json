{"textgrid.poem.41642": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist, als h\u00e4tte ein Jahrtausend ich geschaut.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist, als h\u00e4tte ein Jahrtausend ich geschaut.", "tokens": ["Mir", "ist", ",", "als", "h\u00e4t\u00b7te", "ein", "Jahr\u00b7tau\u00b7send", "ich", "ge\u00b7schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie barg ein Schrank, darin der Akten Flut gestaut,", "tokens": ["Nie", "barg", "ein", "Schrank", ",", "da\u00b7rin", "der", "Ak\u00b7ten", "Flut", "ge\u00b7staut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Liebesbriefe sich, Urkunden, Bl\u00e4tter schichten,", "tokens": ["Wo", "Lie\u00b7bes\u00b7brie\u00b7fe", "sich", ",", "Ur\u00b7kun\u00b7den", ",", "Bl\u00e4t\u00b7ter", "schich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit Haaren, die verpackt in Scheine, mit Gedichten,", "tokens": ["Mit", "Haa\u00b7ren", ",", "die", "ver\u00b7packt", "in", "Schei\u00b7ne", ",", "mit", "Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mehr Heimlichkeiten, als mein Hirn, mein m\u00fcdes, kennt.", "tokens": ["Mehr", "Heim\u00b7lich\u00b7kei\u00b7ten", ",", "als", "mein", "Hirn", ",", "mein", "m\u00fc\u00b7des", ",", "kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ein K\u00f6nigsgrab, ein Riesenmonument;", "tokens": ["Es", "ist", "ein", "K\u00f6\u00b7nigs\u00b7grab", ",", "ein", "Rie\u00b7sen\u00b7mo\u00b7nu\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht eine Massengruft bedeckt so viele Leichen.", "tokens": ["Nicht", "ei\u00b7ne", "Mas\u00b7sen\u00b7gruft", "be\u00b7deckt", "so", "vie\u00b7le", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bin ein Kirchhof, der geflohn vom Mond, dem bleichen,", "tokens": ["Ich", "bin", "ein", "Kirch\u00b7hof", ",", "der", "ge\u00b7flohn", "vom", "Mond", ",", "dem", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch den die W\u00fcrmer ziehn wie scharfer Reue Pein", "tokens": ["Durch", "den", "die", "W\u00fcr\u00b7mer", "ziehn", "wie", "schar\u00b7fer", "Reu\u00b7e", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und meinen Teuersten zernagen das Gebein.", "tokens": ["Und", "mei\u00b7nen", "Teu\u00b7ers\u00b7ten", "zer\u00b7na\u00b7gen", "das", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich bin ein alt Gemach, wo welke Rosen sterben,", "tokens": ["Ich", "bin", "ein", "alt", "Ge\u00b7mach", ",", "wo", "wel\u00b7ke", "Ro\u00b7sen", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,", "PWAV", "PWAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo in der Jahre Rauch Gewande sich verf\u00e4rben,", "tokens": ["Wo", "in", "der", "Jah\u00b7re", "Rauch", "Ge\u00b7wan\u00b7de", "sich", "ver\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Pastelle wehmutvoll und Bouchers, wie getaucht", "tokens": ["Pas\u00b7tel\u00b7le", "weh\u00b7mut\u00b7voll", "und", "Bou\u00b7chers", ",", "wie", "ge\u00b7taucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "KON", "NN", "$,", "PWAV", "VVPP"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "In fahle D\u00fcfte, die ein offen Fl\u00e4schchen haucht.", "tokens": ["In", "fah\u00b7le", "D\u00fcf\u00b7te", ",", "die", "ein", "of\u00b7fen", "Fl\u00e4\u00b7schchen", "haucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nichts w\u00e4hrt so lange wie der lahmen Tage Stocken,", "tokens": ["Nichts", "w\u00e4hrt", "so", "lan\u00b7ge", "wie", "der", "lah\u00b7men", "Ta\u00b7ge", "Sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "KOKOM", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann vor der schneegen Zeit rastlosen schweren Flocken", "tokens": ["Wann", "vor", "der", "schnee\u00b7gen", "Zeit", "rast\u00b7lo\u00b7sen", "schwe\u00b7ren", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Langeweile, die aus tr\u00fcber Stumpfheit kam,", "tokens": ["Die", "Lan\u00b7ge\u00b7wei\u00b7le", ",", "die", "aus", "tr\u00fc\u00b7ber", "Stumpf\u00b7heit", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die schreckliche Gestalt der Ewigkeiten nahm.", "tokens": ["Die", "schreck\u00b7li\u00b7che", "Ge\u00b7stalt", "der", "E\u00b7wig\u00b7kei\u00b7ten", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun bist, belebter Staub, allein und unbeachtet,", "tokens": ["Nun", "bist", ",", "be\u00b7leb\u00b7ter", "Staub", ",", "al\u00b7lein", "und", "un\u00b7be\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADJA", "NN", "$,", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du ein Granit, um den ein dumpf Entsetzen nachtet,", "tokens": ["Du", "ein", "Gra\u00b7nit", ",", "um", "den", "ein", "dumpf", "Ent\u00b7set\u00b7zen", "nach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KOUI", "ART", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Entschlummert wie im Dunst der W\u00fcsten Afrikas,", "tokens": ["Ent\u00b7schlum\u00b7mert", "wie", "im", "Dunst", "der", "W\u00fcs\u00b7ten", "Af\u00b7ri\u00b7kas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich einer Sphinx, die l\u00e4ngst der nichtge Mensch verga\u00df,", "tokens": ["Gleich", "ei\u00b7ner", "Sphinx", ",", "die", "l\u00e4ngst", "der", "nicht\u00b7ge", "Mensch", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die keine Karte nennt, und die vom Gram umsponnen", "tokens": ["Die", "kei\u00b7ne", "Kar\u00b7te", "nennt", ",", "und", "die", "vom", "Gram", "um\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KON", "ART", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr grimmes Lied nur singt im Strahl der Abendsonnen.", "tokens": ["Ihr", "grim\u00b7mes", "Lied", "nur", "singt", "im", "Strahl", "der", "A\u00b7bend\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}