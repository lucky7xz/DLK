{"textgrid.poem.32720": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Untreue", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwing dich auf, mein Geist, und freue", "tokens": ["Schwing", "dich", "auf", ",", "mein", "Geist", ",", "und", "freu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder deines Lebens dich!", "tokens": ["Wie\u00b7der", "dei\u00b7nes", "Le\u00b7bens", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, die Sch\u00f6pfung lacht aufs neue;", "tokens": ["Sieh", ",", "die", "Sch\u00f6p\u00b7fung", "lacht", "aufs", "neu\u00b7e", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Berg und Thal versch\u00f6nen sich.", "tokens": ["Berg", "und", "Thal", "ver\u00b7sch\u00f6\u00b7nen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle V\u00f6gel singen wieder,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "sin\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da die Winternacht entflohn.", "tokens": ["Da", "die", "Win\u00b7ter\u00b7nacht", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf, und misch in ihre Lieder", "tokens": ["Auf", ",", "und", "misch", "in", "ih\u00b7re", "Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Lobges\u00e4nge Ton! \u2013", "tokens": ["Dei\u00b7ner", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", "Ton", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber ach, um mich ist's tr\u00fcbe;", "tokens": ["A\u00b7ber", "ach", ",", "um", "mich", "ist's", "tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "KOUI", "PRF", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winter trauret um mich her.", "tokens": ["Win\u00b7ter", "trau\u00b7ret", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Qual, o falsche Liebe,", "tokens": ["Dei\u00b7ne", "Qual", ",", "o", "fal\u00b7sche", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6nnt mir keine Freuden mehr.", "tokens": ["G\u00f6nnt", "mir", "kei\u00b7ne", "Freu\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle raubte mir Themire,", "tokens": ["Al\u00b7le", "raub\u00b7te", "mir", "The\u00b7mi\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich, ach! so sehr geliebt;", "tokens": ["Die", "ich", ",", "ach", "!", "so", "sehr", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ITJ", "$.", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nun, trotz der h\u00f6chsten Schw\u00fcre,", "tokens": ["Die", "nun", ",", "trotz", "der", "h\u00f6chs\u00b7ten", "Schw\u00fc\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Hand und Herz Dam\u00f6ten giebt.", "tokens": ["Hand", "und", "Herz", "Da\u00b7m\u00f6\u00b7ten", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wandl', o Falsche, zum Altare!", "tokens": ["Wandl'", ",", "o", "Fal\u00b7sche", ",", "zum", "Al\u00b7ta\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen bl\u00fchn zu Kr\u00e4nzen dir.", "tokens": ["Blu\u00b7men", "bl\u00fchn", "zu", "Kr\u00e4n\u00b7zen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir erwachsen sie zur Bahre;", "tokens": ["Mir", "er\u00b7wach\u00b7sen", "sie", "zur", "Bah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Verzweiflung pfl\u00fcckt sie mir.", "tokens": ["Und", "Ver\u00b7zwei\u00b7flung", "pfl\u00fcckt", "sie", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}