{"dta.poem.10394": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein Fl\u00f6ten und Geigen,", "tokens": ["Das", "ist", "ein", "Fl\u00f6\u00b7ten", "und", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Trompeten schmettern drein;", "tokens": ["Trom\u00b7pe\u00b7ten", "schmet\u00b7tern", "drein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da tanzt den Hochzeitreigen", "tokens": ["Da", "tanzt", "den", "Hoch\u00b7zei\u00b7trei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Herzallerliebste mein.", "tokens": ["Die", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist ein Klingen und Dr\u00f6hnen", "tokens": ["Das", "ist", "ein", "Klin\u00b7gen", "und", "Dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Pauken und Schallmei'n;", "tokens": ["Von", "Pau\u00b7ken", "und", "Schall\u00b7mei'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dazwischen schluchzen und st\u00f6hnen", "tokens": ["Da\u00b7zwi\u00b7schen", "schluch\u00b7zen", "und", "st\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die guten Engelein.", "tokens": ["Die", "gu\u00b7ten", "En\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}