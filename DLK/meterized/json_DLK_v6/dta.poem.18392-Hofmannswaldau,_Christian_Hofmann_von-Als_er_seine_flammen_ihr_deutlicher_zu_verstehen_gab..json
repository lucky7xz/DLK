{"dta.poem.18392": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er seine flammen ihr deutlicher  \n zu verstehen gab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein engel/ scheu dich nicht di\u00df blat hier zu ber\u00fchren/", "tokens": ["Mein", "en\u00b7gel", "/", "scheu", "dich", "nicht", "di\u00df", "blat", "hier", "zu", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "PTKNEG", "PDS", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nichts als feur und glut in seinen zeilen tr\u00e4gt/", "tokens": ["Das", "nichts", "als", "feur", "und", "glut", "in", "sei\u00b7nen", "zei\u00b7len", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "KOKOM", "ADJD", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du wirst den heissen brand im ersten angriff sp\u00fchren/", "tokens": ["Du", "wirst", "den", "heis\u00b7sen", "brand", "im", "ers\u00b7ten", "an\u00b7griff", "sp\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der jetzt mein mattes hertz mit tausend funcken regt.", "tokens": ["Der", "jetzt", "mein", "mat\u00b7tes", "hertz", "mit", "tau\u00b7send", "fun\u00b7cken", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jedwede zeile raucht noch von erhitzten flammen/", "tokens": ["Jed\u00b7we\u00b7de", "zei\u00b7le", "raucht", "noch", "von", "er\u00b7hitz\u00b7ten", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja selbst die dinte ist mein schwartz gebrandtes blut/", "tokens": ["Ja", "selbst", "die", "din\u00b7te", "ist", "mein", "schwartz", "ge\u00b7brand\u00b7tes", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es will der himmel mich zu einem feur verdammen/", "tokens": ["Es", "will", "der", "him\u00b7mel", "mich", "zu", "ei\u00b7nem", "feur", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ohne deine gunst verzehret geist und blut.", "tokens": ["Das", "oh\u00b7ne", "dei\u00b7ne", "gunst", "ver\u00b7zeh\u00b7ret", "geist", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht frage was mich brennt/ dein aug\u2019 hat mich enttz\u00fcndet/", "tokens": ["Nicht", "fra\u00b7ge", "was", "mich", "brennt", "/", "dein", "aug'", "hat", "mich", "entt\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PWS", "PPER", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein heisser strahl hat mich fast au\u00df mir selbst gesetzt.", "tokens": ["Dein", "heis\u00b7ser", "strahl", "hat", "mich", "fast", "au\u00df", "mir", "selbst", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn glut und feuer sich mit deinem glantz verbindet/", "tokens": ["Wenn", "glut", "und", "feu\u00b7er", "sich", "mit", "dei\u00b7nem", "glantz", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie kan es anders seyn? die seele wird verletzt.", "tokens": ["Wie", "kan", "es", "an\u00b7ders", "seyn", "?", "die", "see\u00b7le", "wird", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich sahe bein gesicht wie tausend sonnen blitzen/", "tokens": ["Ich", "sa\u00b7he", "bein", "ge\u00b7sicht", "wie", "tau\u00b7send", "son\u00b7nen", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "KOKOM", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als ich mein schwaches aug auf deinen himmel wandt/", "tokens": ["Als", "ich", "mein", "schwa\u00b7ches", "aug", "auf", "dei\u00b7nen", "him\u00b7mel", "wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach aber auch ein blick kont meine brust erhitzen/", "tokens": ["Ach", "a\u00b7ber", "auch", "ein", "blick", "kont", "mei\u00b7ne", "brust", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den du mir ohngefehr von weiten zugesand.", "tokens": ["Den", "du", "mir", "ohn\u00b7ge\u00b7fehr", "von", "wei\u00b7ten", "zu\u00b7ge\u00b7sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer konte dazumahl des himmels macht entkommen?", "tokens": ["Wer", "kon\u00b7te", "da\u00b7zu\u00b7mahl", "des", "him\u00b7mels", "macht", "ent\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn/ als ich nur vermeint ein irrdisch aug zu schaun/", "tokens": ["Denn", "/", "als", "ich", "nur", "ver\u00b7meint", "ein", "irr\u00b7disch", "aug", "zu", "schaun", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJD", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da bin ich ohnversehns von sonnen selbst entglommen/", "tokens": ["Da", "bin", "ich", "ohn\u00b7ver\u00b7sehns", "von", "son\u00b7nen", "selbst", "ent\u00b7glom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ietzt ihr sieges-hau\u00df in meiner seelen baun.", "tokens": ["Die", "ietzt", "ihr", "sie\u00b7ges\u00b7hau\u00df", "in", "mei\u00b7ner", "see\u00b7len", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie sehr ich auch gesucht di\u00df feuer zu verdecken/", "tokens": ["Wie", "sehr", "ich", "auch", "ge\u00b7sucht", "di\u00df", "feu\u00b7er", "zu", "ver\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVPP", "PDS", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So bricht es endlich doch mit vollen flammen aus/", "tokens": ["So", "bricht", "es", "end\u00b7lich", "doch", "mit", "vol\u00b7len", "flam\u00b7men", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es l\u00e4st die liebe nicht so leichtlich sich verstecken/", "tokens": ["Es", "l\u00e4st", "die", "lie\u00b7be", "nicht", "so", "leicht\u00b7lich", "sich", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "PTKNEG", "ADV", "ADJD", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie wirfft die funcken auch bi\u00df zu der sternen hau\u00df.", "tokens": ["Sie", "wirfft", "die", "fun\u00b7cken", "auch", "bi\u00df", "zu", "der", "ster\u00b7nen", "hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ich bin kein Aetna nicht/ der seine glut verschliesset/", "tokens": ["Ich", "bin", "kein", "A\u00b7et\u00b7na", "nicht", "/", "der", "sei\u00b7ne", "glut", "ver\u00b7schlies\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Noch kein Vesuvius der sie mit schnee bestreut/", "tokens": ["Noch", "kein", "Ve\u00b7su\u00b7vius", "der", "sie", "mit", "schnee", "be\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "Ich bin kein Heela nicht der sie mit ey\u00df begiesset/", "tokens": ["Ich", "bin", "kein", "Hee\u00b7la", "nicht", "der", "sie", "mit", "ey\u00df", "be\u00b7gies\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "ART", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mein zunder-reiches blut ist selbst zum feur geweyht.", "tokens": ["Mein", "zun\u00b7der\u00b7rei\u00b7ches", "blut", "ist", "selbst", "zum", "feur", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die liebe l\u00e4\u00dft mich nicht bey dieser zeit verfrieren/", "tokens": ["Die", "lie\u00b7be", "l\u00e4\u00dft", "mich", "nicht", "bey", "die\u00b7ser", "zeit", "ver\u00b7frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich mercke wie sie stets legt frische kohlen an/", "tokens": ["Ich", "mer\u00b7cke", "wie", "sie", "stets", "legt", "fri\u00b7sche", "koh\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "ADV", "VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und darff\u2019 ich einen blick von deinen sonnen sp\u00fcren/", "tokens": ["Und", "darff'", "ich", "ei\u00b7nen", "blick", "von", "dei\u00b7nen", "son\u00b7nen", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So f\u00fchl ich/ was ihr brand in meiner seelen kan.", "tokens": ["So", "f\u00fchl", "ich", "/", "was", "ihr", "brand", "in", "mei\u00b7ner", "see\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Verzeihe/ sch\u00f6nstes licht/ was meine feder schreibet/", "tokens": ["Ver\u00b7zei\u00b7he", "/", "sch\u00f6ns\u00b7tes", "licht", "/", "was", "mei\u00b7ne", "fe\u00b7der", "schrei\u00b7bet", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die flamme/ die mich qu\u00e4lt/ leid kein verstellen nicht.", "tokens": ["Die", "flam\u00b7me", "/", "die", "mich", "qu\u00e4lt", "/", "leid", "kein", "ver\u00b7stel\u00b7len", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PRELS", "PPER", "VVFIN", "$(", "ADJD", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die liebe ists allein/ die meine geister treibet", "tokens": ["Die", "lie\u00b7be", "ists", "al\u00b7lein", "/", "die", "mei\u00b7ne", "geis\u00b7ter", "trei\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$(", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und m\u00e4cht/ da\u00df diese glut aus meiner seelen bricht.", "tokens": ["Und", "m\u00e4cht", "/", "da\u00df", "die\u00b7se", "glut", "aus", "mei\u00b7ner", "see\u00b7len", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich sag es \u00f6ffentlich: dein aug hat mich verletzet/", "tokens": ["Ich", "sag", "es", "\u00f6f\u00b7fent\u00b7lich", ":", "dein", "aug", "hat", "mich", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.38": {"text": "Es hat mir eingefl\u00f6st den s\u00fcssen liebes-gifft/", "tokens": ["Es", "hat", "mir", "ein\u00b7ge\u00b7fl\u00f6st", "den", "s\u00fcs\u00b7sen", "lie\u00b7bes\u00b7gifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der meinen matten geist in heisse funcken setzet/", "tokens": ["Der", "mei\u00b7nen", "mat\u00b7ten", "geist", "in", "heis\u00b7se", "fun\u00b7cken", "set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ja durch und durch mein blut in allen adern trifft.", "tokens": ["Ja", "durch", "und", "durch", "mein", "blut", "in", "al\u00b7len", "a\u00b7dern", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "KON", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wann glut und flammen nun des feuers ursprung zeigen/", "tokens": ["Wann", "glut", "und", "flam\u00b7men", "nun", "des", "feu\u00b7ers", "ur\u00b7sprung", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So lenckt man sein geschrey auff h\u00fclff und rettung hin/", "tokens": ["So", "lenckt", "man", "sein", "ge\u00b7schrey", "auff", "h\u00fclff", "und", "ret\u00b7tung", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie kan ich armer dann dir meine noth verschweigen/", "tokens": ["Wie", "kan", "ich", "ar\u00b7mer", "dann", "dir", "mei\u00b7ne", "noth", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die ich zu hemmen selbst f\u00fcrwahr nicht m\u00e4chtig bin.", "tokens": ["Die", "ich", "zu", "hem\u00b7men", "selbst", "f\u00fcr\u00b7wahr", "nicht", "m\u00e4ch\u00b7tig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVFIN", "ADV", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Aus deinen augen quillt mein todt und auch mein leben/", "tokens": ["Aus", "dei\u00b7nen", "au\u00b7gen", "quillt", "mein", "todt", "und", "auch", "mein", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJD", "KON", "ADV", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Du hast es beydes nun/ mein licht/ in deiner macht/", "tokens": ["Du", "hast", "es", "bey\u00b7des", "nun", "/", "mein", "licht", "/", "in", "dei\u00b7ner", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dein auge st\u00fcrtzet mich/ es kan mich auch erheben/", "tokens": ["Dein", "au\u00b7ge", "st\u00fcrt\u00b7zet", "mich", "/", "es", "kan", "mich", "auch", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Es gibt mir freuden-schein und d\u00fcstre schmertzens-nacht.", "tokens": ["Es", "gibt", "mir", "freu\u00b7den\u00b7schein", "und", "d\u00fcst\u00b7re", "schmert\u00b7zens\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ergreiffe was du wilt/ ich nehm es an vor liebe/", "tokens": ["Er\u00b7greif\u00b7fe", "was", "du", "wilt", "/", "ich", "nehm", "es", "an", "vor", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VMFIN", "$(", "PPER", "VVFIN", "PPER", "APPR", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Erhalt ich deine gunst/ so bin ich h\u00f6chst vergn\u00fcgt/", "tokens": ["Er\u00b7halt", "ich", "dei\u00b7ne", "gunst", "/", "so", "bin", "ich", "h\u00f6chst", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "R\u00fchrt aber auch mein todt aus deines hertzens triebe/", "tokens": ["R\u00fchrt", "a\u00b7ber", "auch", "mein", "todt", "aus", "dei\u00b7nes", "hert\u00b7zens", "trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So hastu doch im grab auch \u00fcber mich gesiegt.", "tokens": ["So", "has\u00b7tu", "doch", "im", "grab", "auch", "\u00fc\u00b7ber", "mich", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}