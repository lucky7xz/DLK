{"textgrid.poem.24558": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen vornehmen Cavalier \u00fcber die Gebuhrt eines Sohnes", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn unser Leib entschl\u00e4fst/ soll unsre Seele wachen.", "tokens": ["Wenn", "un\u00b7ser", "Leib", "ent\u00b7schl\u00e4fst", "/", "soll", "uns\u00b7re", "See\u00b7le", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn die Begierden ruhn/ ist offt der Geist bem\u00fcht.", "tokens": ["Wenn", "die", "Be\u00b7gier\u00b7den", "ruhn", "/", "ist", "offt", "der", "Geist", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$(", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum f\u00fchrte mich ein Traum auf sonderbahre Sachen.", "tokens": ["Drum", "f\u00fchr\u00b7te", "mich", "ein", "Traum", "auf", "son\u00b7der\u00b7bah\u00b7re", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "[wie man in Tr\u00e4umen doch ein Bild des k\u00fcnftgen sieht!]", "tokens": ["wie", "man", "in", "Tr\u00e4u\u00b7men", "doch", "ein", "Bild", "des", "k\u00fcnft\u00b7gen", "sieht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "NN", "ADV", "ART", "NN", "ART", "ADJA", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Es brachte mich ein Zug/ der von den Sternen kommen/", "tokens": ["Es", "brach\u00b7te", "mich", "ein", "Zug", "/", "der", "von", "den", "Ster\u00b7nen", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An einen Meeres-Strand/ und gab der Seelen ein:", "tokens": ["An", "ei\u00b7nen", "Mee\u00b7res\u00b7Strand", "/", "und", "gab", "der", "See\u00b7len", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df Liebe von dem Meer viel gleiches angenommen/", "tokens": ["Da\u00df", "Lie\u00b7be", "von", "dem", "Meer", "viel", "glei\u00b7ches", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PIAT", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da\u00df mehr Sch\u00e4tze da als wie auf Erden seyn.", "tokens": ["Und", "da\u00df", "mehr", "Sch\u00e4t\u00b7ze", "da", "als", "wie", "auf", "Er\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ADV", "KOUS", "KOKOM", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So gleich erblickte sie der Muscheln Menge liegen.", "tokens": ["So", "gleich", "er\u00b7blick\u00b7te", "sie", "der", "Mu\u00b7scheln", "Men\u00b7ge", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie rief: ihr Schalen schlie\u00dft was herrliches in Euch.", "tokens": ["Sie", "rief", ":", "ihr", "Scha\u00b7len", "schlie\u00dft", "was", "herr\u00b7li\u00b7ches", "in", "Euch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PIS", "ADJA", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr seyd des Himmels-Schoo\u00df/ und seiner Kinder Wiegen.", "tokens": ["Ihr", "seyd", "des", "Him\u00b7mels\u00b7Schoo\u00df", "/", "und", "sei\u00b7ner", "Kin\u00b7der", "Wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was komt euch Perlen doch auf dieser Erden gleich?", "tokens": ["Was", "komt", "euch", "Per\u00b7len", "doch", "auf", "die\u00b7ser", "Er\u00b7den", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der edlen Liebe Bild ist eine Perl zu nennen/", "tokens": ["Der", "ed\u00b7len", "Lie\u00b7be", "Bild", "ist", "ei\u00b7ne", "Perl", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die weil des Himmels Gunst sie beyde rein gezeugt.", "tokens": ["Die", "weil", "des", "Him\u00b7mels", "Gunst", "sie", "bey\u00b7de", "rein", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "NN", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vor sch\u00f6n mu\u00df man die Perl/ dieweil sie rein erkennen.", "tokens": ["Vor", "sch\u00f6n", "mu\u00df", "man", "die", "Perl", "/", "die\u00b7weil", "sie", "rein", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VMFIN", "PIS", "ART", "NN", "$(", "KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nur reine Lieb' ist sch\u00f6n/ unreine die betreugt.", "tokens": ["Nur", "rei\u00b7ne", "Lieb'", "ist", "sch\u00f6n", "/", "un\u00b7rei\u00b7ne", "die", "be\u00b7treugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADJD", "$(", "ADJA", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drauf kam/ so wie mich deucht ein Englisch Frauenzimmer/", "tokens": ["Drauf", "kam", "/", "so", "wie", "mich", "deucht", "ein", "Eng\u00b7lisch", "Frau\u00b7en\u00b7zim\u00b7mer", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "ADV", "KOKOM", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hub eine Muschel auf/ und hielt sie an den Brand/", "tokens": ["Hub", "ei\u00b7ne", "Mu\u00b7schel", "auf", "/", "und", "hielt", "sie", "an", "den", "Brand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "$(", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der von der Sonnen kommt: Es \u00f6ffnet durch den Schimmer/", "tokens": ["Der", "von", "der", "Son\u00b7nen", "kommt", ":", "Es", "\u00f6ff\u00b7net", "durch", "den", "Schim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Durch ihren heissen Strahl/ sich ja di\u00df Himmels-Pfand.", "tokens": ["Durch", "ih\u00b7ren", "heis\u00b7sen", "Strahl", "/", "sich", "ja", "di\u00df", "Him\u00b7mels\u00b7Pfand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "PRF", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du Muschel pflegest dich nicht eher aufzuschliessen/", "tokens": ["Du", "Mu\u00b7schel", "pfle\u00b7gest", "dich", "nicht", "e\u00b7her", "auf\u00b7zu\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bi\u00df da\u00df des Himmels Strahl auf dich entz\u00fcndet schaut.", "tokens": ["Bi\u00df", "da\u00df", "des", "Him\u00b7mels", "Strahl", "auf", "dich", "ent\u00b7z\u00fcn\u00b7det", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mich Perle/ sprach sie fort/ kan also nur geniessen", "tokens": ["Mich", "Per\u00b7le", "/", "sprach", "sie", "fort", "/", "kan", "al\u00b7so", "nur", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$(", "VVFIN", "PPER", "PTKVZ", "$(", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Sonne/ welcher mich der Himmel hat vertraut.", "tokens": ["Die", "Son\u00b7ne", "/", "wel\u00b7cher", "mich", "der", "Him\u00b7mel", "hat", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PPER", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wie nur eine Perl die Muschel kan geb\u00e4hren:", "tokens": ["Und", "wie", "nur", "ei\u00b7ne", "Perl", "die", "Mu\u00b7schel", "kan", "ge\u00b7b\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn mehr empf\u00e4nget sie von Himmel niemahls nicht:", "tokens": ["Denn", "mehr", "emp\u00b7f\u00e4n\u00b7get", "sie", "von", "Him\u00b7mel", "nie\u00b7mahls", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Kan ich dem Liebsten auch ein Kleinod nur gew\u00e4hren/", "tokens": ["Kan", "ich", "dem", "Liebs\u00b7ten", "auch", "ein", "Klei\u00b7nod", "nur", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "In Liebe/ die ihm Wehrt/ und mir viel Lust verspricht.", "tokens": ["In", "Lie\u00b7be", "/", "die", "ihm", "Wehrt", "/", "und", "mir", "viel", "Lust", "ver\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PRELS", "PPER", "NN", "$(", "KON", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Sch\u00f6ne hielte noch die Muschel in den H\u00e4nden/", "tokens": ["Die", "Sch\u00f6\u00b7ne", "hiel\u00b7te", "noch", "die", "Mu\u00b7schel", "in", "den", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die l\u00e4ngst des Himmels Huld mit Morgen-Thau erqvickt/", "tokens": ["Die", "l\u00e4ngst", "des", "Him\u00b7mels", "Huld", "mit", "Mor\u00b7gen\u00b7\u00b7T\u00b7hau", "er\u00b7qvickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Als/ da die Sonn' anfieng/ mehr Strahlen drauf zu senden/", "tokens": ["Als", "/", "da", "die", "Sonn'", "an\u00b7fi\u00b7eng", "/", "mehr", "Strah\u00b7len", "drauf", "zu", "sen\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "PIAT", "NN", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Sie eine Perl daraus/ die wundersch\u00f6n begl\u00fcckt.", "tokens": ["Sie", "ei\u00b7ne", "Perl", "da\u00b7raus", "/", "die", "wun\u00b7der\u00b7sch\u00f6n", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PAV", "$(", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich kan die Sch\u00e4tzbarkeit mit Worten nicht beschreiben.", "tokens": ["Ich", "kan", "die", "Sch\u00e4tz\u00b7bar\u00b7keit", "mit", "Wor\u00b7ten", "nicht", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Himmel hat darzu so wie mich deucht gelacht/", "tokens": ["Der", "Him\u00b7mel", "hat", "dar\u00b7zu", "so", "wie", "mich", "deucht", "ge\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ADV", "KOKOM", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und hie\u00df die Welt damit ein gro\u00df Ergetzen treiben.", "tokens": ["Und", "hie\u00df", "die", "Welt", "da\u00b7mit", "ein", "gro\u00df", "Er\u00b7get\u00b7zen", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vor Freuden bin ich selbst dar\u00fcber aufgewacht.", "tokens": ["Vor", "Freu\u00b7den", "bin", "ich", "selbst", "da\u00b7r\u00fc\u00b7ber", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Fast war ich m\u00fc\u00dfvergn\u00fcgt/ da\u00df dieser Traum verschwunden/", "tokens": ["Fast", "war", "ich", "m\u00fc\u00df\u00b7ver\u00b7gn\u00fcgt", "/", "da\u00df", "die\u00b7ser", "Traum", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VMFIN", "$(", "KOUS", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da der begl\u00fcckte Tag mir die Erkl\u00e4hrung beut.", "tokens": ["Da", "der", "be\u00b7gl\u00fcck\u00b7te", "Tag", "mir", "die", "Er\u00b7kl\u00e4h\u00b7rung", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ach Himmel! war mein Traum ein Bild so froher Stunden/", "tokens": ["Ach", "Him\u00b7mel", "!", "war", "mein", "Traum", "ein", "Bild", "so", "fro\u00b7her", "Stun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da di\u00df Hochad