{"textgrid.poem.48312": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Prinz Louis Ferdinand", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechs Fu\u00df hoch aufgeschossen,", "tokens": ["Sechs", "Fu\u00df", "hoch", "auf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Kriegsgott anzuschaun,", "tokens": ["Ein", "Kriegs\u00b7gott", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebling der Genossen,", "tokens": ["Der", "Lieb\u00b7ling", "der", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Abgott sch\u00f6ner Fraun,", "tokens": ["Der", "Ab\u00b7gott", "sch\u00f6\u00b7ner", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Blau\u00e4ugig, blond, verwegen", "tokens": ["Blau\u00b7\u00e4u\u00b7gig", ",", "blond", ",", "ver\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und in der jungen Hand", "tokens": ["Und", "in", "der", "jun\u00b7gen", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den alten Preu\u00dfendegen \u2013", "tokens": ["Den", "al\u00b7ten", "Preu\u00b7\u00dfen\u00b7de\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Prinz Louis Ferdinand.", "tokens": ["Prinz", "Lou\u00b7is", "Fer\u00b7di\u00b7nand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Generalit\u00e4ten", "tokens": ["Die", "Ge\u00b7ne\u00b7ra\u00b7li\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kopfsch\u00fctteln fr\u00fch und sp\u00e4t,", "tokens": ["Kopf\u00b7sch\u00fct\u00b7teln", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie r\u00e4uspern sich und treten", "tokens": ["Sie", "r\u00e4us\u00b7pern", "sich", "und", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Seine Majest\u00e4t,", "tokens": ["Vor", "Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sprechen: \u00bbNicht zu dulden", "tokens": ["Sie", "spre\u00b7chen", ":", "\u00bb", "Nicht", "zu", "dul\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist dieser Lebenslauf,", "tokens": ["Ist", "die\u00b7ser", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die M\u00e4dchen und die Schulden", "tokens": ["Die", "M\u00e4d\u00b7chen", "und", "die", "Schul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zehren den Prinzen auf.\u00ab", "tokens": ["Zeh\u00b7ren", "den", "Prin\u00b7zen", "auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig drauf mit Lachen:", "tokens": ["Der", "K\u00f6\u00b7nig", "drauf", "mit", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdank' sch\u00f6n, ich wu\u00dft' es schon;", "tokens": ["\u00bb", "dank'", "sch\u00f6n", ",", "ich", "wu\u00dft'", "es", "schon", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gilt ihn kirr zu machen,", "tokens": ["Es", "gilt", "ihn", "kirr", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum: Festungsgarnison;", "tokens": ["Drum", ":", "Fes\u00b7tungs\u00b7gar\u00b7ni\u00b7son", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PAV", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er mu\u00df in die Provinzen", "tokens": ["Er", "mu\u00df", "in", "die", "Pro\u00b7vin\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nicht l\u00e4nger hier verziehn,", "tokens": ["Und", "nicht", "l\u00e4n\u00b7ger", "hier", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach Magdeburg mit dem Prinzen \u2013", "tokens": ["Nach", "Mag\u00b7de\u00b7burg", "mit", "dem", "Prin\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Der Prinz vernimmt die M\u00e4re,", "tokens": ["Der", "Prinz", "ver\u00b7nimmt", "die", "M\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df eben bei seinem Schatz:", "tokens": ["Sa\u00df", "e\u00b7ben", "bei", "sei\u00b7nem", "Schatz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbnach Magdeburg, auf Ehre,", "tokens": ["\u00bb", "nach", "Mag\u00b7de\u00b7burg", ",", "auf", "Eh\u00b7re", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist ein schlimmer Platz!\u00ab", "tokens": ["Das", "ist", "ein", "schlim\u00b7mer", "Platz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er meldet sich am Orte,", "tokens": ["Er", "mel\u00b7det", "sich", "am", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und es spricht der General:", "tokens": ["Und", "es", "spricht", "der", "Ge\u00b7ne\u00b7ral", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbt\u00e4glich elf Uhr zum Rapporte", "tokens": ["\u00bb", "t\u00e4g\u00b7lich", "elf", "Uhr", "zum", "Rap\u00b7por\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "CARD", "NN", "APPRART", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ein f\u00fcr allemal!\u00ab", "tokens": ["Ein", "f\u00fcr", "al\u00b7le\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O Prinz, das will nicht munden,", "tokens": ["O", "Prinz", ",", "das", "will", "nicht", "mun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PDS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch denkt er: \u203aSei gescheit,", "tokens": ["Doch", "denkt", "er", ":", "\u203a", "Sei", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "VAIMP", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Volle vierundzwanzig Stunden", "tokens": ["Vol\u00b7le", "vie\u00b7rund\u00b7zwan\u00b7zig", "Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind eine h\u00fcbsche Zeit.", "tokens": ["Sind", "ei\u00b7ne", "h\u00fcb\u00b7sche", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Relais, viermal verschnaufen,", "tokens": ["Re\u00b7lais", ",", "vier\u00b7mal", "ver\u00b7schnau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Auf dem Sattel Nachtquartier,", "tokens": ["Auf", "dem", "Sat\u00b7tel", "Nacht\u00b7quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und kann's ", "tokens": ["Und", "kann's"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "So laufen's ihrer vier.\u2039", "tokens": ["So", "lau\u00b7fen's", "ih\u00b7rer", "vier", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "CARD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hin fliegt er wie die Schwalben,", "tokens": ["Hin", "fliegt", "er", "wie", "die", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcnf Meilen ist Station,", "tokens": ["F\u00fcnf", "Mei\u00b7len", "ist", "Sta\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vom Braunen auf den Falben,", "tokens": ["Vom", "Brau\u00b7nen", "auf", "den", "Fal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist die Havel schon,", "tokens": ["Das", "ist", "die", "Ha\u00b7vel", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Rappen auf den Schimmel,", "tokens": ["Vom", "Rap\u00b7pen", "auf", "den", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun fa\u00dft die Sehnsucht ihn,", "tokens": ["Nun", "fa\u00dft", "die", "Sehn\u00b7sucht", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drei Meilen noch \u2013 hilf Himmel,", "tokens": ["Drei", "Mei\u00b7len", "noch", "\u2013", "hilf", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$(", "VVIMP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Prinz Louis in Berlin.", "tokens": ["Prinz", "Lou\u00b7is", "in", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Gegeben und genommen", "tokens": ["Ge\u00b7ge\u00b7ben", "und", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird einer Stunde Gl\u00fcck,", "tokens": ["Wird", "ei\u00b7ner", "Stun\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann, flugs wie er gekommen,", "tokens": ["Dann", ",", "flugs", "wie", "er", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Fluge geht's zur\u00fcck,", "tokens": ["Im", "Flu\u00b7ge", "geht's", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Elf Uhr am andern Tage", "tokens": ["Elf", "Uhr", "am", "an\u00b7dern", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00e4lt er am alten Ort,", "tokens": ["H\u00e4lt", "er", "am", "al\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mit dem Glockenschlage", "tokens": ["Und", "mit", "dem", "Glo\u00b7cken\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Da steht er zum Rapport. \u2013", "tokens": ["Da", "steht", "er", "zum", "Rap\u00b7port", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Das war nur blo\u00dfes Reiten,", "tokens": ["Das", "war", "nur", "blo\u00b7\u00dfes", "Rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch wer so reiten kann,", "tokens": ["Doch", "wer", "so", "rei\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ist in rechten Zeiten", "tokens": ["Der", "ist", "in", "rech\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch wohl der rechte Mann;", "tokens": ["Auch", "wohl", "der", "rech\u00b7te", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon \u00fcber Tal und H\u00fcgel", "tokens": ["Schon", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "St\u00fcrmt ostw\u00e4rts der Kolo\u00df \u2013", "tokens": ["St\u00fcrmt", "ost\u00b7w\u00e4rts", "der", "Ko\u00b7lo\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Prinz Louis sitzt am Fl\u00fcgel", "tokens": ["Prinz", "Lou\u00b7is", "sitzt", "am", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Rudolst\u00e4dter Schlo\u00df.", "tokens": ["Im", "Ru\u00b7dol\u00b7st\u00e4d\u00b7ter", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es blitzt der Saal von Kerzen,", "tokens": ["Es", "blitzt", "der", "Saal", "von", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zw\u00f6lf Lichter um ihn stehn,", "tokens": ["Zw\u00f6lf", "Lich\u00b7ter", "um", "ihn", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nacht ist's in seinem Herzen,", "tokens": ["Nacht", "ist's", "in", "sei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Nacht nur kann er sehn,", "tokens": ["Und", "Nacht", "nur", "kann", "er", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die T\u00f6ne schwellen, rauschen,", "tokens": ["Die", "T\u00f6\u00b7ne", "schwel\u00b7len", ",", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es klingt wie Lieb' und Ha\u00df,", "tokens": ["Es", "klingt", "wie", "Lieb'", "und", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Damen stehn und lauschen,", "tokens": ["Die", "Da\u00b7men", "stehn", "und", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und was er spielt, ist ", "tokens": ["Und", "was", "er", "spielt", ",", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "\u203azu sp\u00e4t zu Kampf und Beten,", "tokens": ["\u203a", "zu", "sp\u00e4t", "zu", "Kampf", "und", "Be\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKA", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Feinde Rosses-Huf", "tokens": ["Der", "Fein\u00b7de", "Ros\u00b7ses\u00b7Huf"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird \u00fcber Nacht zertreten,", "tokens": ["Wird", "\u00fc\u00b7ber", "Nacht", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ein Jahrhundert schuf,", "tokens": ["Was", "ein", "Jahr\u00b7hun\u00b7dert", "schuf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich seh' es fallen, enden,", "tokens": ["Ich", "seh'", "es", "fal\u00b7len", ",", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wie alles zusammenbricht \u2013", "tokens": ["Und", "wie", "al\u00b7les", "zu\u00b7sam\u00b7men\u00b7bricht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVPP", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ich kann den Tag nicht wenden,", "tokens": ["Ich", "kann", "den", "Tag", "nicht", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aber ", "tokens": ["A\u00b7ber"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Und als das Wort verklungen,", "tokens": ["Und", "als", "das", "Wort", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rollt Donner schon der Schlacht,", "tokens": ["Rollt", "Don\u00b7ner", "schon", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat sich aufgeschwungen,", "tokens": ["Er", "hat", "sich", "auf\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sein Herze noch einmal lacht,", "tokens": ["Und", "sein", "Her\u00b7ze", "noch", "ein\u00b7mal", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Vorauf den andern allen", "tokens": ["Vor\u00b7auf", "den", "an\u00b7dern", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er stolz zusammenbrach,", "tokens": ["Er", "stolz", "zu\u00b7sam\u00b7men\u00b7brach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Prinz Louis war gefallen,", "tokens": ["Prinz", "Lou\u00b7is", "war", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Preu\u00dfen fiel \u2013 ihm nach.", "tokens": ["Und", "Preu\u00b7\u00dfen", "fiel", "\u2013", "ihm", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$(", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}