{"dta.poem.20605": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Verg\u00e4nglichkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Schwetz lisli Aetti, bis mer \u00fcber d\u2019 Bruck", "tokens": ["Schwetz", "lis\u00b7li", "A\u00b7et\u00b7ti", ",", "bis", "mer", "\u00fc\u00b7ber", "d'", "Bruck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$,", "KOUS", "ADV", "APPR", "NE", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "do sin, und do an Berg und Wald verbey!", "tokens": ["do", "sin", ",", "und", "do", "an", "Berg", "und", "Wald", "ver\u00b7bey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00f6rt obe iagt e wilde J\u00e4ger, weisch?", "tokens": ["D\u00f6rt", "o\u00b7be", "i\u00b7agt", "e", "wil\u00b7de", "J\u00e4\u00b7ger", ",", "weisch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und lueg, do niden in de H\u00fcrste seig", "tokens": ["Und", "lu\u00b7eg", ",", "do", "ni\u00b7den", "in", "de", "H\u00fcrs\u00b7te", "seig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "gwi\u00df \u2019s Eyer-Meidli g\u2019lege, halber ful,", "tokens": ["gwi\u00df", "'s", "E\u00b7yer\u00b7Meid\u00b7li", "g'\u00b7le\u00b7ge", ",", "hal\u00b7ber", "ful", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVFIN", "$,", "FM.la", "FM.la", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "\u2019s isch Johr und Tag. H\u00f6rsch, wie der", "tokens": ["'s", "isch", "Johr", "und", "Tag", ".", "H\u00f6rsch", ",", "wie", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "NE", "KON", "NN", "$.", "NN", "$,", "PWAV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Laubi schnuft?", "tokens": ["Lau\u00b7bi", "schnuft", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}