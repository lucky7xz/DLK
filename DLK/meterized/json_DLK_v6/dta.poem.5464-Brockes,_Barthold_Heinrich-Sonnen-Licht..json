{"dta.poem.5464": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnen-Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Herbst, bey einer mehrentheils bedeckten und be-\nw\u00f6lckten Luft,", "tokens": ["Jm", "Herbst", ",", "bey", "ei\u00b7ner", "meh\u00b7ren\u00b7theils", "be\u00b7deck\u00b7ten", "und", "be", "w\u00f6lck\u00b7ten", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADV", "ADJA", "KON", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Stand ich an einem glatten Wasser, das Rohr und schwan-", "tokens": ["Stand", "ich", "an", "ei\u00b7nem", "glat\u00b7ten", "Was\u00b7ser", ",", "das", "Rohr", "und", "schwan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In einer angenehmen Landschaft. Das still\u2019 und klare", "tokens": ["In", "ei\u00b7ner", "an\u00b7ge\u00b7neh\u00b7men", "Land\u00b7schaft", ".", "Das", "still'", "und", "kla\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "KON", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch nur in schwach-und grauem Lichte. Ein sanfter zwar,", "tokens": ["Doch", "nur", "in", "schwach\u00b7\u00b7und", "grau\u00b7em", "Lich\u00b7te", ".", "Ein", "sanf\u00b7ter", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJD", "ADJA", "NN", "$.", "ART", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Der nicht allein die Luft erf\u00fcllte, der auch die B\u00e4um\u2019 und", "tokens": ["Der", "nicht", "al\u00b7lein", "die", "Luft", "er\u00b7f\u00fcll\u00b7te", ",", "der", "auch", "die", "B\u00e4um'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "KON"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und, wo nicht gantz, doch guten Theils, der Landschaft", "tokens": ["Und", ",", "wo", "nicht", "gantz", ",", "doch", "gu\u00b7ten", "Theils", ",", "der", "Land\u00b7schaft"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAV", "PTKNEG", "ADV", "$,", "ADV", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "War allenthalben ausgespannt. Was man noch sah, war", "tokens": ["War", "al\u00b7len\u00b7thal\u00b7ben", "aus\u00b7ge\u00b7spannt", ".", "Was", "man", "noch", "sah", ",", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "PWS", "PIS", "ADV", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Es wirckte die sonst helle Gegend dem, der sie jetzt voll", "tokens": ["Es", "wirck\u00b7te", "die", "sonst", "hel\u00b7le", "Ge\u00b7gend", "dem", ",", "der", "sie", "jetzt", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN", "ART", "$,", "PRELS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Mit einer schleichenden Gewalt,", "tokens": ["Mit", "ei\u00b7ner", "schlei\u00b7chen\u00b7den", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Fast eine D\u00e4mmrung im Gem\u00fcth.", "tokens": ["Fast", "ei\u00b7ne", "D\u00e4mm\u00b7rung", "im", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich schlug demnach von ungefehr, betr\u00fcbt, die Augen vor", "tokens": ["Ich", "schlug", "dem\u00b7nach", "von", "un\u00b7ge\u00b7fehr", ",", "be\u00b7tr\u00fcbt", ",", "die", "Au\u00b7gen", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJD", "$,", "VVPP", "$,", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Jedoch nicht zwo Minuten lang. Darauf erhub ich ihre", "tokens": ["Je\u00b7doch", "nicht", "zwo", "Mi\u00b7nu\u00b7ten", "lang", ".", "Da\u00b7rauf", "er\u00b7hub", "ich", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "CARD", "NN", "ADJD", "$.", "PAV", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Geschwinde wieder in die H\u00f6h\u2019. Allein wie sehr entsetzt\u2019", "tokens": ["Ge\u00b7schwin\u00b7de", "wie\u00b7der", "in", "die", "H\u00f6h'", ".", "Al\u00b7lein", "wie", "sehr", "ent\u00b7setzt'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$.", "ADV", "KOKOM", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Als ich, mit fast geblendeten und gantz f\u00fcr Lust erstaunten", "tokens": ["Als", "ich", ",", "mit", "fast", "ge\u00b7blen\u00b7de\u00b7ten", "und", "gantz", "f\u00fcr", "Lust", "er\u00b7staun\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADV", "ADJA", "KON", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Der Landschaft gantzen Stand ver\u00e4ndert, erleuchtet, ja", "tokens": ["Der", "Land\u00b7schaft", "gant\u00b7zen", "Stand", "ver\u00b7\u00e4n\u00b7dert", ",", "er\u00b7leuch\u00b7tet", ",", "ja"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Erheitert und verkl\u00e4hret sah! Es nahm ein angenehm", "tokens": ["Er\u00b7hei\u00b7tert", "und", "ver\u00b7kl\u00e4h\u00b7ret", "sah", "!", "Es", "nahm", "ein", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Mein Hertz, mein gantzes Wesen ein.", "tokens": ["Mein", "Hertz", ",", "mein", "gant\u00b7zes", "We\u00b7sen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lie\u00df Feld, Wiese, Berg und Thal mit Anmuth nicht", "tokens": ["Es", "lie\u00df", "Feld", ",", "Wie\u00b7se", ",", "Berg", "und", "Thal", "mit", "An\u00b7muth", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem hellen Meer von Glantz nicht nur umgeben", "tokens": ["Von", "ei\u00b7nem", "hel\u00b7len", "Meer", "von", "Glantz", "nicht", "nur", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schien, in einem bunten Lichte, die Welt ein irdisch", "tokens": ["Es", "schien", ",", "in", "ei\u00b7nem", "bun\u00b7ten", "Lich\u00b7te", ",", "die", "Welt", "ein", "ir\u00b7disch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was ich, vor einem Augenblick, noch kalt gesehen und", "tokens": ["Was", "ich", ",", "vor", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", ",", "noch", "kalt", "ge\u00b7se\u00b7hen", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "$,", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "VVPP", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Sah\u2019 ich nunmehr erheitert, warm, und in gef\u00e4rbtem", "tokens": ["Sah'", "ich", "nun\u00b7mehr", "er\u00b7hei\u00b7tert", ",", "warm", ",", "und", "in", "ge\u00b7f\u00e4rb\u00b7tem"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$,", "ADJD", "$,", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des schwancken Schilffs polirte Bl\u00e4tter, der glatten Bin-", "tokens": ["Des", "schwan\u00b7cken", "Schilffs", "po\u00b7lir\u00b7te", "Bl\u00e4t\u00b7ter", ",", "der", "glat\u00b7ten", "Bin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Sah man, da es der Sonnen Glantz, mit seinem heitern", "tokens": ["Sah", "man", ",", "da", "es", "der", "Son\u00b7nen", "Glantz", ",", "mit", "sei\u00b7nem", "hei\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht minder die belaubten B\u00e4ume, zusammt den Blu-", "tokens": ["Nicht", "min\u00b7der", "die", "be\u00b7laub\u00b7ten", "B\u00e4u\u00b7me", ",", "zu\u00b7sammt", "den", "Blu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die Sch\u00f6nheit in der Fluht verdoppeln, und sich im kla-", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "in", "der", "Fluht", "ver\u00b7dop\u00b7peln", ",", "und", "sich", "im", "kla"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,", "KON", "PRF", "APPRART", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Kurtz eine warm\u2019 und laue Klarheit und eine licht\u2019 und", "tokens": ["Kurtz", "ei\u00b7ne", "warm'", "und", "lau\u00b7e", "Klar\u00b7heit", "und", "ei\u00b7ne", "licht'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJD", "KON", "ADJA", "NN", "KON", "ART", "ADJD", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Bedeckt\u2019, umgab, beflo\u00df, durchstrahlte die gantze Gegend,", "tokens": ["Be\u00b7deckt'", ",", "um\u00b7gab", ",", "be\u00b7flo\u00df", ",", "durch\u00b7strahl\u00b7te", "die", "gant\u00b7ze", "Ge\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich stutzt und freute mich von Hertzen. Was werden", "tokens": ["Ich", "stutzt", "und", "freu\u00b7te", "mich", "von", "Hert\u00b7zen", ".", "Was", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "NN", "$.", "PWS", "VAFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "F\u00fcr Wunder nicht bey uns gewircket! rief ich, f\u00fcr Freu-", "tokens": ["F\u00fcr", "Wun\u00b7der", "nicht", "bey", "uns", "ge\u00b7wir\u00b7cket", "!", "rief", "ich", ",", "f\u00fcr", "Freu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "APPR", "PPER", "VVPP", "$.", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie wird, durch sie, die gantze Welt belebt, erqvickt,", "tokens": ["Wie", "wird", ",", "durch", "sie", ",", "die", "gant\u00b7ze", "Welt", "be\u00b7lebt", ",", "er\u00b7qvickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, fuhr ich fort bey mir zu dencken: die\u00df wircket eine", "tokens": ["Und", ",", "fuhr", "ich", "fort", "bey", "mir", "zu", "den\u00b7cken", ":", "die\u00df", "wir\u00b7cket", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "PTKZU", "VVINF", "$.", "PDS", "VVFIN", "ART"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "Was mu\u00df vor Anmuth, Pracht und Licht und Herrlichkeit", "tokens": ["Was", "mu\u00df", "vor", "An\u00b7muth", ",", "Pracht", "und", "Licht", "und", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn dort in jenen Himmels-H\u00f6h\u2019n, nach diesem Leben,", "tokens": ["Wenn", "dort", "in", "je\u00b7nen", "Him\u00b7mels\u00b7H\u00f6h'n", ",", "nach", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Stande sich befinden werden, von Sonnen, welche", "tokens": ["Jm", "Stan\u00b7de", "sich", "be\u00b7fin\u00b7den", "wer\u00b7den", ",", "von", "Son\u00b7nen", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PRF", "VVPP", "VAINF", "$,", "APPR", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Wirckungen zu sehn, zu f\u00fchlen? Mich nimmt ein", "tokens": ["Die", "Wir\u00b7ckun\u00b7gen", "zu", "sehn", ",", "zu", "f\u00fch\u00b7len", "?", "Mich", "nimmt", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wenn ich an solchen Glantz gedencke. Dies Dencken mehrt", "tokens": ["Wenn", "ich", "an", "sol\u00b7chen", "Glantz", "ge\u00b7den\u00b7cke", ".", "Dies", "Den\u00b7cken", "mehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$.", "PDS", "VVINF", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und ist mir, als ob ich dar\u00fcber schon in Gedancken see-", "tokens": ["Und", "ist", "mir", ",", "als", "ob", "ich", "da\u00b7r\u00fc\u00b7ber", "schon", "in", "Ge\u00b7dan\u00b7cken", "see"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "PAV", "ADV", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}}}}}