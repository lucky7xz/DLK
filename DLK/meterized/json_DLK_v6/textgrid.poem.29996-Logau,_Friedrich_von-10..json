{"textgrid.poem.29996": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werther Freund, du lieber Alter, alt von alten Bidersinnen,", "tokens": ["Wert\u00b7her", "Freund", ",", "du", "lie\u00b7ber", "Al\u00b7ter", ",", "alt", "von", "al\u00b7ten", "Bi\u00b7der\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Alt von Jahren, Witz und Ehren, wir sind hier; du bist von hinnen,", "tokens": ["Alt", "von", "Jah\u00b7ren", ",", "Witz", "und", "Eh\u00b7ren", ",", "wir", "sind", "hier", ";", "du", "bist", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "ADV", "$.", "PPER", "VAFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Einzunemen Ehr und Gut, das durch Alt-seyn nicht vergeht,", "tokens": ["Ein\u00b7zu\u00b7ne\u00b7men", "Ehr", "und", "Gut", ",", "das", "durch", "Al\u00b7tseyn", "nicht", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "$,", "PRELS", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Sondern mit der Ewigkeit immer in die Wette steht.", "tokens": ["Son\u00b7dern", "mit", "der", "E\u00b7wig\u00b7keit", "im\u00b7mer", "in", "die", "Wet\u00b7te", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Alt von Jahren, frisch von Lastern ist die Welt bey unsren Tagen,", "tokens": ["Alt", "von", "Jah\u00b7ren", ",", "frisch", "von", "Las\u00b7tern", "ist", "die", "Welt", "bey", "un\u00b7sren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Pflegt das Alter zu begehren, Alten aber Hohn zu sagen.", "tokens": ["Pflegt", "das", "Al\u00b7ter", "zu", "be\u00b7geh\u00b7ren", ",", "Al\u00b7ten", "a\u00b7ber", "Hohn", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKZU", "VVINF", "$,", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Aber wann der reine Schnee alter H\u00e4upter so zerfleust,", "tokens": ["A\u00b7ber", "wann", "der", "rei\u00b7ne", "Schnee", "al\u00b7ter", "H\u00e4up\u00b7ter", "so", "zer\u00b7fleust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Siht man, da\u00df in gantze L\u00e4nder tr\u00fcbes Wasser sich ergeust.", "tokens": ["Siht", "man", ",", "da\u00df", "in", "gant\u00b7ze", "L\u00e4n\u00b7der", "tr\u00fc\u00b7bes", "Was\u00b7ser", "sich", "er\u00b7geust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "APPR", "ADJA", "NN", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}