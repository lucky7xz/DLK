{"textgrid.poem.24200": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur, mein Freund, ist immer sittlich.", "tokens": ["Na\u00b7tur", ",", "mein", "Freund", ",", "ist", "im\u00b7mer", "sitt\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Staatsanwalt freilich ist unerbittlich.", "tokens": ["Der", "Staats\u00b7an\u00b7walt", "frei\u00b7lich", "ist", "un\u00b7er\u00b7bitt\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "J\u00fcngst hat er ein Andachtsbuch konfisziert,", "tokens": ["J\u00fcngst", "hat", "er", "ein", "An\u00b7dachts\u00b7buch", "kon\u00b7fis\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Weil sich zwei Fliegen drauf kopuliert.", "tokens": ["Weil", "sich", "zwei", "Flie\u00b7gen", "drauf", "ko\u00b7pu\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "CARD", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}}}}