{"textgrid.poem.49390": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Legende", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor einem hellen Marienbild", "tokens": ["Vor", "ei\u00b7nem", "hel\u00b7len", "Ma\u00b7ri\u00b7en\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Spielte ein Bettler die Geige.", "tokens": ["Spiel\u00b7te", "ein", "Bett\u00b7ler", "die", "Gei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die V\u00f6gel sangen im Herbstgefild,", "tokens": ["Die", "V\u00f6\u00b7gel", "san\u00b7gen", "im", "Herbst\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Tag ging schon zur Neige.", "tokens": ["Der", "Tag", "ging", "schon", "zur", "Nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er spielte der Reben s\u00fc\u00dfe Last,", "tokens": ["Er", "spiel\u00b7te", "der", "Re\u00b7ben", "s\u00fc\u00b7\u00dfe", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hingen ihm bis zur Stirne,", "tokens": ["Die", "hin\u00b7gen", "ihm", "bis", "zur", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er spielte den reifen Apfelast", "tokens": ["Er", "spiel\u00b7te", "den", "rei\u00b7fen", "Ap\u00b7fe\u00b7last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der Berge schneeige Firne.", "tokens": ["Und", "der", "Ber\u00b7ge", "schne\u00b7ei\u00b7ge", "Fir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er spielte der blauen Seen Licht,", "tokens": ["Er", "spiel\u00b7te", "der", "blau\u00b7en", "Seen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die leuchteten ihm aus den Augen.", "tokens": ["Die", "leuch\u00b7te\u00b7ten", "ihm", "aus", "den", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sang zu der Geige und immer noch nicht", "tokens": ["Er", "sang", "zu", "der", "Gei\u00b7ge", "und", "im\u00b7mer", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ADV", "ADV", "PTKNEG"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wollte das Lied ihm taugen.", "tokens": ["Woll\u00b7te", "das", "Lied", "ihm", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Da sang er den Mond und die Sterne dazu", "tokens": ["Da", "sang", "er", "den", "Mond", "und", "die", "Ster\u00b7ne", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "PAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die konnte er alle verschenken", "tokens": ["Die", "konn\u00b7te", "er", "al\u00b7le", "ver\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und weinte des Waldes einsame Ruh,", "tokens": ["Und", "wein\u00b7te", "des", "Wal\u00b7des", "ein\u00b7sa\u00b7me", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die t\u00e4t seine Geige tr\u00e4nken.", "tokens": ["Die", "t\u00e4t", "sei\u00b7ne", "Gei\u00b7ge", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Er spielte und sang und merkte kaum", "tokens": ["Er", "spiel\u00b7te", "und", "sang", "und", "merk\u00b7te", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Maria sich leise bewegte", "tokens": ["Wie", "Ma\u00b7ria", "sich", "lei\u00b7se", "be\u00b7weg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "PRF", "ADJD", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ihm beim Spiel ihrer H\u00e4nde Schaum", "tokens": ["Und", "ihm", "beim", "Spiel", "ih\u00b7rer", "H\u00e4n\u00b7de", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "PPOSAT", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf die wehenden Locken legte.", "tokens": ["Auf", "die", "we\u00b7hen\u00b7den", "Lo\u00b7cken", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Er drehte beim Spiele sich hin und her,", "tokens": ["Er", "dreh\u00b7te", "beim", "Spie\u00b7le", "sich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das t\u00f6nende Holz unterm Kinne.", "tokens": ["Das", "t\u00f6\u00b7nen\u00b7de", "Holz", "un\u00b7term", "Kin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er wollte, da\u00df seine s\u00fc\u00dfe M\u00e4r", "tokens": ["Er", "woll\u00b7te", ",", "da\u00df", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "M\u00e4r"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In alle vier Winde zerrinne.", "tokens": ["In", "al\u00b7le", "vier", "Win\u00b7de", "zer\u00b7rin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Da stieg die Madonna vom Sockel herab", "tokens": ["Da", "stieg", "die", "Ma\u00b7don\u00b7na", "vom", "So\u00b7ckel", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und folgte ihm auf seine Wege.", "tokens": ["Und", "folg\u00b7te", "ihm", "auf", "sei\u00b7ne", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gingen bergauf und gingen bergab", "tokens": ["Die", "gin\u00b7gen", "ber\u00b7gauf", "und", "gin\u00b7gen", "berg\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch Gestr\u00fcpp und Dornengehege.", "tokens": ["Durch", "Ge\u00b7str\u00fcpp", "und", "Dor\u00b7nen\u00b7ge\u00b7he\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Er spielte noch, als schon der Hahn gekr\u00e4ht", "tokens": ["Er", "spiel\u00b7te", "noch", ",", "als", "schon", "der", "Hahn", "ge\u00b7kr\u00e4ht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und manche Saite zersprungen.", "tokens": ["Und", "man\u00b7che", "Sai\u00b7te", "zer\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Auf Dreien spielt er die Trinit\u00e4t", "tokens": ["Auf", "Drei\u00b7en", "spielt", "er", "die", "Tri\u00b7ni\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf zweien die Engelszungen.", "tokens": ["Auf", "zwei\u00b7en", "die", "En\u00b7gels\u00b7zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Zuletzt war es nur noch das heimliche Lied", "tokens": ["Zu\u00b7letzt", "war", "es", "nur", "noch", "das", "heim\u00b7li\u00b7che", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom eingeborenen Sohne.", "tokens": ["Vom", "ein\u00b7ge\u00b7bo\u00b7re\u00b7nen", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Maria deckte den Mantel auf ihn", "tokens": ["Ma\u00b7ria", "deck\u00b7te", "den", "Man\u00b7tel", "auf", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "+-+--+-++", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Darin schl\u00e4ft er zum ewigen Lohne.", "tokens": ["Da\u00b7rin", "schl\u00e4ft", "er", "zum", "e\u00b7wi\u00b7gen", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}}}}}