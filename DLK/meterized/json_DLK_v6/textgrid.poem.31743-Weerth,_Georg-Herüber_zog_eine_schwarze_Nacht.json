{"textgrid.poem.31743": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Her\u00fcber zog eine schwarze Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Her\u00fcber zog eine schwarze Nacht.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "zog", "ei\u00b7ne", "schwar\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die F\u00f6hren rauschten im Sturme;", "tokens": ["Die", "F\u00f6h\u00b7ren", "rauschten", "im", "Stur\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hat das Wetter wild zerkracht", "tokens": ["Es", "hat", "das", "Wet\u00b7ter", "wild", "zer\u00b7kracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kirche mit ihrem Turme.", "tokens": ["Die", "Kir\u00b7che", "mit", "ih\u00b7rem", "Tur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zerschmettert das Kreuz, zerdr\u00fcckt der Altar,", "tokens": ["Zer\u00b7schmet\u00b7tert", "das", "Kreuz", ",", "zer\u00b7dr\u00fcckt", "der", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zermalmt das Gebein in den S\u00e4rgen \u2013", "tokens": ["Zer\u00b7malmt", "das", "Ge\u00b7bein", "in", "den", "S\u00e4r\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die gotischen B\u00f6gen w\u00e4lzen sich", "tokens": ["Die", "go\u00b7ti\u00b7schen", "B\u00f6\u00b7gen", "w\u00e4l\u00b7zen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Donnernd hinab von den Bergen.", "tokens": ["Don\u00b7nernd", "hin\u00b7ab", "von", "den", "Ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Zum Dorfe st\u00fcrzt sich Turm und Chor", "tokens": ["Zum", "Dor\u00b7fe", "st\u00fcrzt", "sich", "Turm", "und", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie zu einem Grabe \u2013", "tokens": ["Als", "wie", "zu", "ei\u00b7nem", "Gra\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da f\u00e4hrt entsetzt vom Lager empor", "tokens": ["Da", "f\u00e4hrt", "ent\u00b7setzt", "vom", "La\u00b7ger", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und spricht zur Mutter der Knabe:", "tokens": ["Und", "spricht", "zur", "Mut\u00b7ter", "der", "Kna\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbach Mutter, mir tr\u00e4umte ein Traum so schwer,", "tokens": ["\u00bb", "ach", "Mut\u00b7ter", ",", "mir", "tr\u00e4um\u00b7te", "ein", "Traum", "so", "schwer", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das hat den Schlaf mir verdorben.", "tokens": ["Das", "hat", "den", "Schlaf", "mir", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach Mutter, mir tr\u00e4umte, soeben w\u00e4r", "tokens": ["Ach", "Mut\u00b7ter", ",", "mir", "tr\u00e4um\u00b7te", ",", "soe\u00b7ben", "w\u00e4r"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "$,", "ADV", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der liebe Herrgott gestorben.\u00ab", "tokens": ["Der", "lie\u00b7be", "Herr\u00b7gott", "ge\u00b7stor\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}