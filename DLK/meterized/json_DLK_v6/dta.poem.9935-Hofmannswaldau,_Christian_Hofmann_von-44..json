{"dta.poem.9935": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungl\u00fcck das verfolget mich/ wenn ich an zu leben fange/", "tokens": ["Un\u00b7gl\u00fcck", "das", "ver\u00b7fol\u00b7get", "mich", "/", "wenn", "ich", "an", "zu", "le\u00b7ben", "fan\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich mu\u00df/ wie ein andrer will/ leben/ nicht wie ich verlange:", "tokens": ["Ich", "mu\u00df", "/", "wie", "ein", "an\u00b7drer", "will", "/", "le\u00b7ben", "/", "nicht", "wie", "ich", "ver\u00b7lan\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOKOM", "ART", "ADJA", "VMFIN", "$(", "VVINF", "$(", "PTKNEG", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Bald verdam\u0303t man mich zum feuer/ bald soll ich im wasser seyn/", "tokens": ["Bald", "ver\u00b7dam\u0303t", "man", "mich", "zum", "feu\u00b7er", "/", "bald", "soll", "ich", "im", "was\u00b7ser", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$(", "ADV", "VMFIN", "PPER", "APPRART", "ADJA", "VAINF", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "K\u00e4lte machet da\u00df ich brenne/ hitze giebt mir weissen schein.", "tokens": ["K\u00e4l\u00b7te", "ma\u00b7chet", "da\u00df", "ich", "bren\u00b7ne", "/", "hit\u00b7ze", "giebt", "mir", "weis\u00b7sen", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "PPER", "VVFIN", "$(", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}