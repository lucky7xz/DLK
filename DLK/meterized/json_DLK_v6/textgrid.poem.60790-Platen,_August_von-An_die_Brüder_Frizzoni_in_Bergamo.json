{"textgrid.poem.60790": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An die Br\u00fcder Frizzoni in Bergamo", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchen Vorwurf mu\u00dft ich ertragen von euch,", "tokens": ["Man\u00b7chen", "Vor\u00b7wurf", "mu\u00dft", "ich", "er\u00b7tra\u00b7gen", "von", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "APPR", "PPER", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weil so lang Pausilipos Ufer den Freund festhalten, indes", "tokens": ["Weil", "so", "lang", "Pau\u00b7si\u00b7li\u00b7pos", "U\u00b7fer", "den", "Freund", "fest\u00b7hal\u00b7ten", ",", "in\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ADJD", "NE", "NN", "ART", "NN", "VVINF", "$,", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Zwischen Alpen und Po sich ausdehnt, welche Flur!", "tokens": ["Zwi\u00b7schen", "Al\u00b7pen", "und", "Po", "sich", "aus\u00b7dehnt", ",", "wel\u00b7che", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "PRF", "VVPP", "$,", "PWAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Weinbekr\u00e4nzt, voll klarer Seen, volkreich und geschm\u00fcckt", "tokens": ["Wein\u00b7be\u00b7kr\u00e4nzt", ",", "voll", "kla\u00b7rer", "Seen", ",", "vol\u00b7kreich", "und", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "ADJA", "NN", "$,", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Durch der ehmals m\u00e4chtigen St\u00e4dte Gemeinsinn,", "tokens": ["Durch", "der", "eh\u00b7mals", "m\u00e4ch\u00b7ti\u00b7gen", "St\u00e4d\u00b7te", "Ge\u00b7mein\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der herbeirief edle Kunst,", "tokens": ["Der", "her\u00b7bei\u00b7rief", "ed\u00b7le", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Anschauliche Form zu verleihn bildloser Wahrheit sch\u00f6pferisch.", "tokens": ["An\u00b7schau\u00b7li\u00b7che", "Form", "zu", "ver\u00b7leihn", "bild\u00b7lo\u00b7ser", "Wahr\u00b7heit", "sch\u00f6p\u00b7fe\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Nicht verschm\u00e4ht mein festlicher Sang, in des Lobs", "tokens": ["Nicht", "ver\u00b7schm\u00e4ht", "mein", "fest\u00b7li\u00b7cher", "Sang", ",", "in", "des", "Lobs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "S\u00fc\u00dfen Born eintauchend der Fittige weithinschattiges Paar,", "tokens": ["S\u00fc\u00b7\u00dfen", "Born", "ein\u00b7tau\u00b7chend", "der", "Fit\u00b7ti\u00b7ge", "weit\u00b7hin\u00b7schat\u00b7ti\u00b7ges", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Euch lombardischer Heimatflur Preislied zu weihn.", "tokens": ["Euch", "lom\u00b7bar\u00b7di\u00b7scher", "Hei\u00b7mat\u00b7flur", "Preis\u00b7lied", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als in d\u00e4mmrungsgrauer Vorzeit Alboin einst", "tokens": ["Als", "in", "d\u00e4mm\u00b7rungs\u00b7grau\u00b7er", "Vor\u00b7zeit", "Al\u00b7bo\u00b7in", "einst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NE", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aus dem Nord herf\u00fchrte gepanzerte Heerschar,", "tokens": ["Aus", "dem", "Nord", "her\u00b7f\u00fchr\u00b7te", "ge\u00b7pan\u00b7zer\u00b7te", "Heer\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sah der F\u00fcrst, der auf des Bergs", "tokens": ["Sah", "der", "F\u00fcrst", ",", "der", "auf", "des", "Bergs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schneegipfel erobernden Blick lie\u00df schweifen, solch fruchtreich Gefild", "tokens": ["Schnee\u00b7gip\u00b7fel", "er\u00b7o\u00b7bern\u00b7den", "Blick", "lie\u00df", "schwei\u00b7fen", ",", "solch", "fruch\u00b7treich", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN", "VVINF", "$,", "PIAT", "ADJD", "NN"], "meter": "+---+--+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Hocherstaunt, klomm fr\u00f6hlich herab und erwarb's.", "tokens": ["Ho\u00b7cher\u00b7staunt", ",", "klomm", "fr\u00f6h\u00b7lich", "her\u00b7ab", "und", "er\u00b7wa\u00b7rb'", "s."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "$,", "VVFIN", "ADJD", "ADV", "KON", "VVFIN", "NE"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Widerstand nicht h\u00e4tte vermocht zu entziehn ihm gr\u00f6\u00dferes Ziel,", "tokens": ["Wi\u00b7der\u00b7stand", "nicht", "h\u00e4t\u00b7te", "ver\u00b7mocht", "zu", "ent\u00b7ziehn", "ihm", "gr\u00f6\u00b7\u00dfe\u00b7res", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VAFIN", "VVPP", "PTKZU", "VVINF", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "W\u00e4r's das leuchtende Rom sogar; bald st\u00f6rt jedoch", "tokens": ["W\u00e4r's", "das", "leuch\u00b7ten\u00b7de", "Rom", "so\u00b7gar", ";", "bald", "st\u00f6rt", "je\u00b7doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NE", "ADV", "$.", "ADV", "VVFIN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Seines Muts siegswerten Plan ihm h\u00e4usliches Weh,", "tokens": ["Sei\u00b7nes", "Muts", "siegs\u00b7wer\u00b7ten", "Plan", "ihm", "h\u00e4us\u00b7li\u00b7ches", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.12": {"text": "Welches ihm Ro\u00dfmunda bereitete, die ihm", "tokens": ["Wel\u00b7ches", "ihm", "Ro\u00df\u00b7mun\u00b7da", "be\u00b7rei\u00b7te\u00b7te", ",", "die", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "NE", "VVFIN", "$,", "PRELS", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Durch Gewalt ward anverm\u00e4hlt,", "tokens": ["Durch", "Ge\u00b7walt", "ward", "an\u00b7ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Unwilligen Sinns, im Gem\u00fct ausbr\u00fctend Rachsucht grenzenlos!", "tokens": ["Un\u00b7wil\u00b7li\u00b7gen", "Sinns", ",", "im", "Ge\u00b7m\u00fct", "aus\u00b7br\u00fc\u00b7tend", "Rach\u00b7sucht", "gren\u00b7zen\u00b7los", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Denn es fiel ihr Vater voreinst in dem Kampf", "tokens": ["Denn", "es", "fiel", "ihr", "Va\u00b7ter", "vor\u00b7e\u00b7inst", "in", "dem", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Durch den Beilschlag dessen, an den in des Ehbunds schn\u00f6de Gewalt", "tokens": ["Durch", "den", "Beil\u00b7schlag", "des\u00b7sen", ",", "an", "den", "in", "des", "Eh\u00b7bunds", "schn\u00f6\u00b7de", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDS", "$,", "APPR", "ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.3": {"text": "Nun das Los sie gekn\u00fcpft. Der Sieg zeugt \u00dcbermut:", "tokens": ["Nun", "das", "Los", "sie", "ge\u00b7kn\u00fcpft", ".", "Der", "Sieg", "zeugt", "\u00dc\u00b7ber\u00b7mut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVPP", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch die Burg scholl Jubel, laut auftobte das Fest,", "tokens": ["Durch", "die", "Burg", "scholl", "Ju\u00b7bel", ",", "laut", "auf\u00b7tob\u00b7te", "das", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Als Pokal rings kreiste der Sch\u00e4del des Feindes;", "tokens": ["Als", "Po\u00b7kal", "rings", "kreis\u00b7te", "der", "Sch\u00e4\u00b7del", "des", "Fein\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Diesen hob F\u00fcrst Alboin", "tokens": ["Die\u00b7sen", "hob", "F\u00fcrst", "Al\u00b7bo\u00b7in"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trotzvoll, in berauschter Bet\u00f6rtheit, auf und sprach: Ro\u00dfmunda, trink!", "tokens": ["Trotz\u00b7voll", ",", "in", "be\u00b7rauschter", "Be\u00b7t\u00f6r\u00b7theit", ",", "auf", "und", "sprach", ":", "Ro\u00df\u00b7mun\u00b7da", ",", "trink", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "KON", "VVFIN", "$.", "NE", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Jene trank; Stolz hemmte den Z\u00e4hrenergu\u00df,", "tokens": ["Je\u00b7ne", "trank", ";", "Stolz", "hemm\u00b7te", "den", "Z\u00e4h\u00b7re\u00b7ner\u00b7gu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Als sie wog schmerzvoll in der Hand des geliebt ehrw\u00fcrdigen Haupts", "tokens": ["Als", "sie", "wog", "schmerz\u00b7voll", "in", "der", "Hand", "des", "ge\u00b7liebt", "ehr\u00b7w\u00fcr\u00b7di\u00b7gen", "Haupts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "--+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Teure Last, und Vergeltung schwur stillschweigend ihr", "tokens": ["Teu\u00b7re", "Last", ",", "und", "Ver\u00b7gel\u00b7tung", "schwur", "still\u00b7schwei\u00b7gend", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "VVFIN", "ADJD", "PPOSAT"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Blick, und tief tr\u00fcbt ihn der Ohnmacht Jammergef\u00fchl.", "tokens": ["Blick", ",", "und", "tief", "tr\u00fcbt", "ihn", "der", "Ohn\u00b7macht", "Jam\u00b7mer\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Gegen Kraft hilft List nur allein und des Goldes", "tokens": ["Ge\u00b7gen", "Kraft", "hilft", "List", "nur", "al\u00b7lein", "und", "des", "Gol\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NE", "ADV", "ADV", "KON", "ART", "NN"], "meter": "+-+-+--++-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Allgewalt; Sch\u00f6nheit erreicht", "tokens": ["All\u00b7ge\u00b7walt", ";", "Sch\u00f6n\u00b7heit", "er\u00b7reicht"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Durch \u00fcppige K\u00fcnste so manch Wunschziel und durch Liebkosungen.", "tokens": ["Durch", "\u00fcp\u00b7pi\u00b7ge", "K\u00fcns\u00b7te", "so", "manch", "Wunschziel", "und", "durch", "Lieb\u00b7ko\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PIAT", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Alboins Freund fiel in die Netze des Weibs,", "tokens": ["Al\u00b7bo\u00b7ins", "Freund", "fiel", "in", "die", "Net\u00b7ze", "des", "Weibs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Helmiches; Schmach sinnt er dem K\u00f6nige, sinnt Blutd\u00fcrstigeres.", "tokens": ["Hel\u00b7mi\u00b7ches", ";", "Schmach", "sinnt", "er", "dem", "K\u00f6\u00b7ni\u00b7ge", ",", "sinnt", "Blut\u00b7d\u00fcrs\u00b7ti\u00b7ge\u00b7res", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "NN", "$."], "meter": "---+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Nacht umh\u00fcllte Veronas Burg, kampfm\u00fcder Schlaf:", "tokens": ["Nacht", "um\u00b7h\u00fcll\u00b7te", "Ve\u00b7ro\u00b7nas", "Burg", ",", "kampf\u00b7m\u00fc\u00b7der", "Schlaf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Sieh, da schlich, Mordlust im Sinn, Ro\u00dfmunda gemach,", "tokens": ["Sieh", ",", "da", "schlich", ",", "Mord\u00b7lust", "im", "Sinn", ",", "Ro\u00df\u00b7mun\u00b7da", "ge\u00b7mach", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADJD", "$,", "NN", "APPRART", "NN", "$,", "NE", "ADV", "$,"], "meter": "---+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wo der Held ausatmete ruhigen Schlummer;", "tokens": ["Wo", "der", "Held", "aus\u00b7at\u00b7me\u00b7te", "ru\u00b7hi\u00b7gen", "Schlum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Aber da\u00df wehrlos er sei,", "tokens": ["A\u00b7ber", "da\u00df", "wehr\u00b7los", "er", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Tr\u00e4gt weit von dem Lager sie weg Streitaxt und Schwert, Welschlands Ruin;", "tokens": ["Tr\u00e4gt", "weit", "von", "dem", "La\u00b7ger", "sie", "weg", "Strei\u00b7taxt", "und", "Schwert", ",", "Wel\u00b7schlands", "Ru\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PPER", "ADV", "NN", "KON", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+--+--+-+-+-+-+", "measure": 