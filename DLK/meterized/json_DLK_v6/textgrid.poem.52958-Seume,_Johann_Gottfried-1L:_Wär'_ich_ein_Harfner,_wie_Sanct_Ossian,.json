{"textgrid.poem.52958": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r' ich ein Harfner, wie Sanct Ossian,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich ein Harfner, wie Sanct Ossian,", "tokens": ["W\u00e4r'", "ich", "ein", "Harf\u00b7ner", ",", "wie", "Sanct", "Os\u00b7si\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der alten und der neuen Harfner Meister,", "tokens": ["Der", "al\u00b7ten", "und", "der", "neu\u00b7en", "Harf\u00b7ner", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich s\u00e4nge, wie allein der Mann der Felsenbahn", "tokens": ["Ich", "s\u00e4n\u00b7ge", ",", "wie", "al\u00b7lein", "der", "Mann", "der", "Fel\u00b7sen\u00b7bahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Lied auf goldnen Saiten singen kann,", "tokens": ["Ein", "Lied", "auf", "gold\u00b7nen", "Sai\u00b7ten", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und r\u00fchrend, wie die Stimme seiner Geister.", "tokens": ["Und", "r\u00fch\u00b7rend", ",", "wie", "die", "Stim\u00b7me", "sei\u00b7ner", "Geis\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Ton ist rauh, und ungelehrt die Hand,", "tokens": ["Mein", "Ton", "ist", "rauh", ",", "und", "un\u00b7ge\u00b7lehrt", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meinem Busen str\u00f6mt kein G\u00f6tterfeuer;", "tokens": ["In", "mei\u00b7nem", "Bu\u00b7sen", "str\u00f6mt", "kein", "G\u00f6t\u00b7ter\u00b7feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kommt mir auch ein Strahl aus seinem Geisterland,", "tokens": ["Und", "kommt", "mir", "auch", "ein", "Strahl", "aus", "sei\u00b7nem", "Geis\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hallt, was schnell wie Blitz die Brust empfand,", "tokens": ["So", "hallt", ",", "was", "schnell", "wie", "Blitz", "die", "Brust", "emp\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADJD", "KOKOM", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur schwach zur\u00fcck von der verstimmten Leyer.", "tokens": ["Nur", "schwach", "zu\u00b7r\u00fcck", "von", "der", "ver\u00b7stimm\u00b7ten", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Freund, nimm mich hin, so bieder, fest und schlicht,", "tokens": ["Freund", ",", "nimm", "mich", "hin", ",", "so", "bie\u00b7der", ",", "fest", "und", "schlicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie du mich schon vor langen Jahren kanntest;", "tokens": ["Wie", "du", "mich", "schon", "vor", "lan\u00b7gen", "Jah\u00b7ren", "kann\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hintergeht dich je mein ehrliches Gesicht,", "tokens": ["Und", "hin\u00b7ter\u00b7geht", "dich", "je", "mein", "ehr\u00b7li\u00b7ches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verklage mich einst vor dem Weltgericht,", "tokens": ["Ver\u00b7kla\u00b7ge", "mich", "einst", "vor", "dem", "Welt\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und spotte de\u00df, den du sonst redlich nanntest.", "tokens": ["Und", "spot\u00b7te", "de\u00df", ",", "den", "du", "sonst", "red\u00b7lich", "nann\u00b7test", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der gro\u00dfe Harfner, der die Sph\u00e4ren stimmt,", "tokens": ["Der", "gro\u00b7\u00dfe", "Harf\u00b7ner", ",", "der", "die", "Sph\u00e4\u00b7ren", "stimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Halleluja seine Geister gl\u00fchen,", "tokens": ["Wenn", "Hal\u00b7le\u00b7lu\u00b7ja", "sei\u00b7ne", "Geis\u00b7ter", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor dessen Flammenthron die Welt der Sonnen glimmt,", "tokens": ["Vor", "des\u00b7sen", "Flam\u00b7men\u00b7thron", "die", "Welt", "der", "Son\u00b7nen", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschenke noch, eh dich die Parze nimmt,", "tokens": ["Be\u00b7schen\u00b7ke", "noch", ",", "eh", "dich", "die", "Par\u00b7ze", "nimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dich, lieber Freund, mit sch\u00f6nen Harmonien.", "tokens": ["Dich", ",", "lie\u00b7ber", "Freund", ",", "mit", "sch\u00f6\u00b7nen", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}