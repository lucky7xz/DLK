{"textgrid.poem.36280": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die hohen Augen werden mich verzehren.", "tokens": ["Die", "ho\u00b7hen", "Au\u00b7gen", "wer\u00b7den", "mich", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Maria, gro\u00dfe Mutter, ach verschone!", "tokens": ["Ma\u00b7ria", ",", "gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "ach", "ver\u00b7scho\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ITJ", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Verbirg das lichte Haupt, die Strahlenkrone;", "tokens": ["Ver\u00b7birg", "das", "lich\u00b7te", "Haupt", ",", "die", "Strah\u00b7len\u00b7kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie soll ich sonst dem irren Wahnsinn wehren?", "tokens": ["Wie", "soll", "ich", "sonst", "dem", "ir\u00b7ren", "Wahn\u00b7sinn", "weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du selber, Heil'ge, mu\u00dft mich Sanftmut lehren,", "tokens": ["Du", "sel\u00b7ber", ",", "Heil'\u00b7ge", ",", "mu\u00dft", "mich", "Sanft\u00b7mut", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sch\u00f6ner Tod, geweiht vom ew'gen Sohne,", "tokens": ["Da\u00df", "sch\u00f6\u00b7ner", "Tod", ",", "ge\u00b7weiht", "vom", "ew'\u00b7gen", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Kreuz der Liebe meine Sehnsucht lohne.", "tokens": ["Am", "Kreuz", "der", "Lie\u00b7be", "mei\u00b7ne", "Sehn\u00b7sucht", "loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ich beginne, mu\u00df die Glut vermehren.", "tokens": ["Was", "ich", "be\u00b7gin\u00b7ne", ",", "mu\u00df", "die", "Glut", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So blicke wieder und dann la\u00df mich sterben! \u2013", "tokens": ["So", "bli\u00b7cke", "wie\u00b7der", "und", "dann", "la\u00df", "mich", "ster\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "VVIMP", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eilt' ich schnell durch dieser Erde Schw\u00e4chen!", "tokens": ["Wie", "eilt'", "ich", "schnell", "durch", "die\u00b7ser", "Er\u00b7de", "Schw\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fr\u00fch oder sp\u00e4t mu\u00df alles so verderben.", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4t", "mu\u00df", "al\u00b7les", "so", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VMFIN", "PIS", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Aus Liebe einzig flo\u00df, was ich verschuldet;", "tokens": ["Aus", "Lie\u00b7be", "ein\u00b7zig", "flo\u00df", ",", "was", "ich", "ver\u00b7schul\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Liebe will das Herz, Madonna, brechen,", "tokens": ["In", "Lie\u00b7be", "will", "das", "Herz", ",", "Ma\u00b7don\u00b7na", ",", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "$,", "NE", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des irre Liebe gn\u00e4dig du geduldet.", "tokens": ["Des", "ir\u00b7re", "Lie\u00b7be", "gn\u00e4\u00b7dig", "du", "ge\u00b7dul\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}