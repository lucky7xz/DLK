{"textgrid.poem.23878": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einem jungen Rosenblatt", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem jungen Rosenblatt", "tokens": ["Auf", "ei\u00b7nem", "jun\u00b7gen", "Ro\u00b7sen\u00b7blatt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Liebster mir geblasen hat", "tokens": ["Mein", "Liebs\u00b7ter", "mir", "ge\u00b7bla\u00b7sen", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl eine Melodei.", "tokens": ["Wohl", "ei\u00b7ne", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es gab mir viele Dinge kund", "tokens": ["Es", "gab", "mir", "vie\u00b7le", "Din\u00b7ge", "kund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Rosenblatt am roten Mund", "tokens": ["Das", "Ro\u00b7sen\u00b7blatt", "am", "ro\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und war kein Wort dabei.", "tokens": ["Und", "war", "kein", "Wort", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als das Blatt zerblasen war,", "tokens": ["Und", "als", "das", "Blatt", "zer\u00b7bla\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da gab ich meinen Mund ihm dar", "tokens": ["Da", "gab", "ich", "mei\u00b7nen", "Mund", "ihm", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00fc\u00dft an ihm mich satt.", "tokens": ["Und", "k\u00fc\u00dft", "an", "ihm", "mich", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und viel mehr Dinge that noch kund", "tokens": ["Und", "viel", "mehr", "Din\u00b7ge", "that", "noch", "kund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der rote Mund am roten Mund,", "tokens": ["Der", "ro\u00b7te", "Mund", "am", "ro\u00b7ten", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst als das Rosenblatt.", "tokens": ["Selbst", "als", "das", "Ro\u00b7sen\u00b7blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}