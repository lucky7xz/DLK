{"textgrid.poem.42534": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn letzter Donner fern verrollt", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn letzter Donner fern verrollt", "tokens": ["Wenn", "letz\u00b7ter", "Don\u00b7ner", "fern", "ver\u00b7rollt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dunkler Sommerstunde:", "tokens": ["Nach", "dunk\u00b7ler", "Som\u00b7mer\u00b7stun\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon winkt ein erstes Wolkengold", "tokens": ["Schon", "winkt", "ein", "ers\u00b7tes", "Wol\u00b7ken\u00b7gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem regensatten Grunde:", "tokens": ["Dem", "re\u00b7gen\u00b7sat\u00b7ten", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne k\u00fc\u00dft die Gr\u00e4ser wach,", "tokens": ["Die", "Son\u00b7ne", "k\u00fc\u00dft", "die", "Gr\u00e4\u00b7ser", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieben Lerchen singen,", "tokens": ["Die", "lie\u00b7ben", "Ler\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es tr\u00e4gt der Wind den blauen Tag", "tokens": ["Es", "tr\u00e4gt", "der", "Wind", "den", "blau\u00b7en", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Empor auf k\u00fchlen Schwingen:", "tokens": ["Em\u00b7por", "auf", "k\u00fch\u00b7len", "Schwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In solcher Stunde senkt mich ein,", "tokens": ["In", "sol\u00b7cher", "Stun\u00b7de", "senkt", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel M\u00fch ist nicht vonn\u00f6ten,", "tokens": ["Viel", "M\u00fch", "ist", "nicht", "von\u00b7n\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es wird die Erde hinterdrein", "tokens": ["Es", "wird", "die", "Er\u00b7de", "hin\u00b7ter\u00b7drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir rasch den Sarg verl\u00f6ten.", "tokens": ["Mir", "rasch", "den", "Sarg", "ver\u00b7l\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Streut Rosen, Rosen in das Grab,", "tokens": ["Streut", "Ro\u00b7sen", ",", "Ro\u00b7sen", "in", "das", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spielt Trompetenst\u00fccke;", "tokens": ["Und", "spielt", "Trom\u00b7pe\u00b7ten\u00b7st\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann brecht mir meinen Wanderstab", "tokens": ["Dann", "brecht", "mir", "mei\u00b7nen", "Wan\u00b7der\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit fester Hand in St\u00fccke!", "tokens": ["Mit", "fes\u00b7ter", "Hand", "in", "St\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es fiel ein Blatt vom Baum, es fiel", "tokens": ["Es", "fiel", "ein", "Blatt", "vom", "Baum", ",", "es", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch fruchtbeschwerte \u00c4ste.", "tokens": ["Durch", "frucht\u00b7be\u00b7schwer\u00b7te", "\u00c4s\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nun geht zu euerm eignen Ziel,", "tokens": ["Nun", "geht", "zu", "eu\u00b7erm", "eig\u00b7nen", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr meine letzten G\u00e4ste!", "tokens": ["Ihr", "mei\u00b7ne", "letz\u00b7ten", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zum eignen Ziel geht spielbereit,", "tokens": ["Zum", "eig\u00b7nen", "Ziel", "geht", "spiel\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwenkt hoch die Trauerfahnen,", "tokens": ["Schwenkt", "hoch", "die", "Trau\u00b7er\u00b7fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Froh, da\u00df ihr noch auf Erden seid", "tokens": ["Froh", ",", "da\u00df", "ihr", "noch", "auf", "Er\u00b7den", "seid"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht bei euern Ahnen!", "tokens": ["Und", "nicht", "bei", "eu\u00b7ern", "Ah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}