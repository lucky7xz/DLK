{"textgrid.poem.30172": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfern haben hertzlich gerne, da\u00df man sie bedien und ehre;", "tokens": ["Jung\u00b7fern", "ha\u00b7ben", "hertz\u00b7lich", "ger\u00b7ne", ",", "da\u00df", "man", "sie", "be\u00b7di\u00b7en", "und", "eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ADV", "$,", "KOUS", "PIS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Jungfern haben hertzlich gerne, da\u00df ihr Schmuck sich t\u00e4glich mehre;", "tokens": ["Jung\u00b7fern", "ha\u00b7ben", "hertz\u00b7lich", "ger\u00b7ne", ",", "da\u00df", "ihr", "Schmuck", "sich", "t\u00e4g\u00b7lich", "meh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jungfern haben gerne Geld; Jungfern leben gerne gut;", "tokens": ["Jung\u00b7fern", "ha\u00b7ben", "ger\u00b7ne", "Geld", ";", "Jung\u00b7fern", "le\u00b7ben", "ger\u00b7ne", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "$.", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+---+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Jungfern haben gerne Ruh; Jungfern haben gerne Muth.", "tokens": ["Jung\u00b7fern", "ha\u00b7ben", "ger\u00b7ne", "Ruh", ";", "Jung\u00b7fern", "ha\u00b7ben", "ger\u00b7ne", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "$.", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "K\u00fcmt nun denn ein alter Buler, der di\u00df alles kunte leisten,", "tokens": ["K\u00fcmt", "nun", "denn", "ein", "al\u00b7ter", "Bu\u00b7ler", ",", "der", "di\u00df", "al\u00b7les", "kun\u00b7te", "leis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PDS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Sah man, wie so viel geliebtes sie sam ihm bey Seite weisten,", "tokens": ["Sah", "man", ",", "wie", "so", "viel", "ge\u00b7lieb\u00b7tes", "sie", "sam", "ihm", "bey", "Sei\u00b7te", "weis\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "ADV", "PIAT", "ADJA", "PPER", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Nahmen einen Jungen an, wie es gleich um ihn bewand;", "tokens": ["Nah\u00b7men", "ei\u00b7nen", "Jun\u00b7gen", "an", ",", "wie", "es", "gleich", "um", "ihn", "be\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ursach ist am Tage nicht, ist vielleicht der Nacht bekant.", "tokens": ["Ur\u00b7sach", "ist", "am", "Ta\u00b7ge", "nicht", ",", "ist", "viel\u00b7leicht", "der", "Nacht", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "PTKNEG", "$,", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}