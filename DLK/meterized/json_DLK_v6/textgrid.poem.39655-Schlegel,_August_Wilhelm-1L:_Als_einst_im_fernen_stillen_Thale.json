{"textgrid.poem.39655": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als einst im fernen stillen Thale", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst im fernen stillen Thale", "tokens": ["Als", "einst", "im", "fer\u00b7nen", "stil\u00b7len", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Cythere Myrrhas Sohn erblickt,", "tokens": ["Cy\u00b7the\u00b7re", "Myrr\u00b7has", "Sohn", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da ward' ihr Herz zum erstenmale", "tokens": ["Da", "ward'", "ihr", "Herz", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch sterbliche Gestalt entz\u00fcckt.", "tokens": ["Durch", "sterb\u00b7li\u00b7che", "Ge\u00b7stalt", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachl\u00e4\u00dfig kam sie hergegangen,", "tokens": ["Nach\u00b7l\u00e4\u00b7\u00dfig", "kam", "sie", "her\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprach zu ihm mit s\u00fc\u00dfem Ton.", "tokens": ["Und", "sprach", "zu", "ihm", "mit", "s\u00fc\u00b7\u00dfem", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein wunderliebliches Verlangen", "tokens": ["Ein", "wun\u00b7der\u00b7lieb\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ergriff den sch\u00fcchternen Adon.", "tokens": ["Er\u00b7griff", "den", "sch\u00fcch\u00b7ter\u00b7nen", "A\u00b7don", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "G\u00f6ttinnen fa\u00dfen k\u00fchne Schl\u00fc\u00dfe,", "tokens": ["G\u00f6t\u00b7tin\u00b7nen", "fa\u00b7\u00dfen", "k\u00fch\u00b7ne", "Schl\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn Lieb' in ihrem Innern gl\u00fcht;", "tokens": ["Wenn", "Lieb'", "in", "ih\u00b7rem", "In\u00b7nern", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sorgen, da\u00df die Zeit der K\u00fcsse", "tokens": ["Sie", "sor\u00b7gen", ",", "da\u00df", "die", "Zeit", "der", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Zierereien nicht entflieht.", "tokens": ["Bei", "Zie\u00b7re\u00b7rei\u00b7en", "nicht", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schwebt, sobald mit mildem Fluge", "tokens": ["Sie", "schwebt", ",", "so\u00b7bald", "mit", "mil\u00b7dem", "Flu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tag in Thetis' Schoo\u00df entflohn,", "tokens": ["Der", "Tag", "in", "The\u00b7tis'", "Schoo\u00df", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herab mit ihrem Taubenzuge,", "tokens": ["Her\u00b7ab", "mit", "ih\u00b7rem", "Tau\u00b7ben\u00b7zu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und raubt den schlummernden Adon.", "tokens": ["Und", "raubt", "den", "schlum\u00b7mern\u00b7den", "A\u00b7don", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Tauben flattern durch die L\u00fcfte", "tokens": ["Die", "Tau\u00b7ben", "flat\u00b7tern", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und langen an in Paphos' Hain.", "tokens": ["Und", "lan\u00b7gen", "an", "in", "Pa\u00b7phos'", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPR", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier laden frische Balsamd\u00fcfte", "tokens": ["Hier", "la\u00b7den", "fri\u00b7sche", "Bal\u00b7sam\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie unter Rosenlauben ein.", "tokens": ["Sie", "un\u00b7ter", "Ro\u00b7sen\u00b7lau\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum scheucht der Glanz der Morgenstunde", "tokens": ["Kaum", "scheucht", "der", "Glanz", "der", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Gott des Schlafs, bekr\u00e4nzt mit Mohn,", "tokens": ["Den", "Gott", "des", "Schlafs", ",", "be\u00b7kr\u00e4nzt", "mit", "Mohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So weckt ein Ku\u00df von Venus Munde", "tokens": ["So", "weckt", "ein", "Ku\u00df", "von", "Ve\u00b7nus", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den frohbezauberten Adon.", "tokens": ["Den", "froh\u00b7be\u00b7zau\u00b7ber\u00b7ten", "A\u00b7don", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie liegen Arm um Arm geschlungen", "tokens": ["Sie", "lie\u00b7gen", "Arm", "um", "Arm", "ge\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aufgel\u00f6s't in Liebesglut.", "tokens": ["Und", "auf\u00b7ge\u00b7l\u00f6s't", "in", "Lie\u00b7bes\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie st\u00e4rkt durch matte Weigerungen", "tokens": ["Sie", "st\u00e4rkt", "durch", "mat\u00b7te", "Wei\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des unerfahrnen Lieblings Muth.", "tokens": ["Des", "un\u00b7er\u00b7fahr\u00b7nen", "Lieb\u00b7lings", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der G\u00fcrtel ist in's Gras gefallen,", "tokens": ["Der", "G\u00fcr\u00b7tel", "ist", "in's", "Gras", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Brautbett ist ein Rasenthron;", "tokens": ["Ihr", "Braut\u00b7bett", "ist", "ein", "Ra\u00b7sent\u00b7hron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Brautchor singen Nachtigallen", "tokens": ["Den", "Brautc\u00b7hor", "sin\u00b7gen", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem hochbeseligten Adon.", "tokens": ["Dem", "hoch\u00b7be\u00b7se\u00b7lig\u00b7ten", "A\u00b7don", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-----+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Bei'm Hauch des West's auf Rosen liegen,", "tokens": ["Bei'm", "Hauch", "des", "West's", "auf", "Ro\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und an der wollusthei\u00dfen Brust", "tokens": ["Und", "an", "der", "wol\u00b7lust\u00b7hei\u00b7\u00dfen", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sch\u00f6nsten G\u00f6ttin sich zu wiegen,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "G\u00f6t\u00b7tin", "sich", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mehr als Elys\u00e4er-Lust.", "tokens": ["Ist", "mehr", "als", "E\u00b7ly\u00b7s\u00e4\u00b7er\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dich kann diese Lust erm\u00fcden?", "tokens": ["Und", "dich", "kann", "die\u00b7se", "Lust", "er\u00b7m\u00fc\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach wenig Tagen fliehst du schon", "tokens": ["Nach", "we\u00b7nig", "Ta\u00b7gen", "fliehst", "du", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Lieb' und ihren weichen Frieden?", "tokens": ["Die", "Lieb'", "und", "ih\u00b7ren", "wei\u00b7chen", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! unbesonnener Adon!", "tokens": ["Ach", "!", "un\u00b7be\u00b7son\u00b7ne\u00b7ner", "A\u00b7don", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mag Venus noch so z\u00e4rtlich hadern,", "tokens": ["Mag", "Ve\u00b7nus", "noch", "so", "z\u00e4rt\u00b7lich", "ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie ihr Sch\u00e4fer treulos flieht;", "tokens": ["Da\u00df", "sie", "ihr", "Sch\u00e4\u00b7fer", "treu\u00b7los", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst! so lang' in seinen Adern", "tokens": ["Um\u00b7sonst", "!", "so", "lang'", "in", "sei\u00b7nen", "A\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ungest\u00fcme Streitlust gl\u00fcht.", "tokens": ["Die", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Streit\u00b7lust", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er folgt der Spur der W\u00f6lf' und Tiger,", "tokens": ["Er", "folgt", "der", "Spur", "der", "W\u00f6l\u00b7f'", "und", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gewaffnet wie Latonens Sohn,", "tokens": ["Ge\u00b7waff\u00b7net", "wie", "La\u00b7to\u00b7nens", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wer erkennt im raschen Krieger", "tokens": ["Und", "wer", "er\u00b7kennt", "im", "ra\u00b7schen", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch jenen l\u00e4chelnden Adon?", "tokens": ["Noch", "je\u00b7nen", "l\u00e4\u00b7cheln\u00b7den", "A\u00b7don", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr Nymphen! seid ihm nah mit Schutze!", "tokens": ["Ihr", "Nym\u00b7phen", "!", "seid", "ihm", "nah", "mit", "Schut\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Pfeil trifft keine feige Brut;", "tokens": ["Sein", "Pfeil", "trifft", "kei\u00b7ne", "fei\u00b7ge", "Brut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er scherzt, aus edlem M\u00e4nnertrutze,", "tokens": ["Er", "scherzt", ",", "aus", "ed\u00b7lem", "M\u00e4n\u00b7ner\u00b7trut\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit wilder Ungeheuer Wuth.", "tokens": ["Mit", "wil\u00b7der", "Un\u00b7ge\u00b7heu\u00b7er", "Wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, seht mit aufgestr\u00e4ubten Borsten", "tokens": ["O", ",", "seht", "mit", "auf\u00b7ge\u00b7str\u00e4ub\u00b7ten", "Bors\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm seitw\u00e4rts jenen Eber drohn!", "tokens": ["Ihm", "seit\u00b7w\u00e4rts", "je\u00b7nen", "E\u00b7ber", "drohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00f6rt nah und fern es durch die Forsten", "tokens": ["H\u00f6rt", "nah", "und", "fern", "es", "durch", "die", "Fors\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erschallen: blutend liegt Adon!", "tokens": ["Er\u00b7schal\u00b7len", ":", "blu\u00b7tend", "liegt", "A\u00b7don", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanz