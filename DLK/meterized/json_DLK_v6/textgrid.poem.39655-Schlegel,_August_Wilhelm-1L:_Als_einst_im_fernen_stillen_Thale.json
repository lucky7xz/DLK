["Die", "Hin\u00b7din", "aus", "der", "La\u00b7ger\u00b7st\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sucht, und ruft Adon! Adon!", "tokens": ["Und", "sucht", ",", "und", "ruft", "A\u00b7don", "!", "A\u00b7don", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dem aber quillt sein Blut am H\u00fcgel,", "tokens": ["Dem", "a\u00b7ber", "quillt", "sein", "Blut", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit dem Blut entquillt sein Schmerz,", "tokens": ["Und", "mit", "dem", "Blut", "ent\u00b7quillt", "sein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00e4lter weht des Todes Fl\u00fcgel,", "tokens": ["Und", "k\u00e4l\u00b7ter", "weht", "des", "To\u00b7des", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon an sein \u00e4ngstlich athmend Herz.", "tokens": ["Schon", "an", "sein", "\u00e4ngst\u00b7lich", "ath\u00b7mend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5"