{"dta.poem.999": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.57", "sv:0.42"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Da kaum noch sand im stundenglase l&#228;uft               ", "tokens": ["Da", "kaum", "noch", "sand", "im", "stun\u00b7den\u00b7gla\u00b7se", "l", "&#228;", "uft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$(", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So zieh ihm nach dem wandrer tau-betr&#228;uft               ", "tokens": ["So", "zieh", "ihm", "nach", "dem", "wand\u00b7rer", "tau\u00b7be\u00b7tr", "&#228;", "uft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$(", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die heisse luft verwehte ihn geschwind               ", "tokens": ["Die", "heis\u00b7se", "luft", "ver\u00b7weh\u00b7te", "ihn", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den freund der blumen und der sterne kind             ", "tokens": ["Den", "freund", "der", "blu\u00b7men", "und", "der", "ster\u00b7ne", "kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der eines morgens vor dem schnitt der saat               ", "tokens": ["Der", "ei\u00b7nes", "mor\u00b7gens", "vor", "dem", "schnitt", "der", "saat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "APPR", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die h&#228;nde traurig vor die stirne that               ", "tokens": ["Die", "h", "&#228;", "nde", "trau\u00b7rig", "vor", "die", "stir\u00b7ne", "that"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und durch wer weiss welch fr&#252;hen fluch gemahnt               ", "tokens": ["Und", "durch", "wer", "weiss", "welch", "fr", "&#252;", "hen", "fluch", "ge\u00b7mahnt"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KON", "APPR", "PWS", "VVFIN", "PWAT", "ADJA", "$(", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im heut den lezten jugendtag geahnt             ", "tokens": ["Im", "heut", "den", "lez\u00b7ten", "ju\u00b7gend\u00b7tag", "ge\u00b7ahnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der durch kein sonnenschmeicheln mehr erweicht               ", "tokens": ["Der", "durch", "kein", "son\u00b7nen\u00b7schmei\u00b7cheln", "mehr", "er\u00b7weicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Solang er sch\u00f6n war ohne klage leicht               ", "tokens": ["So\u00b7lang", "er", "sch\u00f6n", "war", "oh\u00b7ne", "kla\u00b7ge", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPR", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einem sommervogel &#252;berm ried               ", "tokens": ["Gleich", "ei\u00b7nem", "som\u00b7mer\u00b7vo\u00b7gel", "&#252;", "berm", "ried"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["ADV", "ART", "NN", "$(", "ADJD", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "An jenem tag aus unsren kreisen schied.             ", "tokens": ["An", "je\u00b7nem", "tag", "aus", "un\u00b7sren", "krei\u00b7sen", "schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}