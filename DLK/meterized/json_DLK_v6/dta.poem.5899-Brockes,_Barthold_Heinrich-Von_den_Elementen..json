{"dta.poem.5899": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierdurch vermeinte nun der Weise", "tokens": ["Hier\u00b7durch", "ver\u00b7mein\u00b7te", "nun", "der", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wahre Ordnung uns zu zeigen,", "tokens": ["Die", "wah\u00b7re", "Ord\u00b7nung", "uns", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Materie, die \u00fcberall bald f\u00e4llt,", "tokens": ["Und", "die", "Ma\u00b7te\u00b7rie", ",", "die", "\u00fc\u00b7be\u00b7rall", "bald", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Bald anf\u00e4ngt wiederum zu steigen.", "tokens": ["Bald", "an\u00b7f\u00e4ngt", "wie\u00b7de\u00b7rum", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Macht die ", "tokens": ["Macht", "die"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Von unten auf, von oben ab, kan nennen.", "tokens": ["Von", "un\u00b7ten", "auf", ",", "von", "o\u00b7ben", "ab", ",", "kan", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "APPR", "ADV", "PTKVZ", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Aendrungen, so man dadurch entstehen sieht,", "tokens": ["Die", "A\u00b7en\u00b7drun\u00b7gen", ",", "so", "man", "da\u00b7durch", "ent\u00b7ste\u00b7hen", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "PAV", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die Kr\u00e4ffte, die sich bald vereinen, bald sich trennen;", "tokens": ["Die", "Kr\u00e4ff\u00b7te", ",", "die", "sich", "bald", "ver\u00b7ei\u00b7nen", ",", "bald", "sich", "tren\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVINF", "$,", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erregen unaufh\u00f6rlich Streit,", "tokens": ["Er\u00b7re\u00b7gen", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "Streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und wieder Einigkeit,", "tokens": ["Und", "wie\u00b7der", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Jm Wasser, in der Lufft, und auf der Erden,", "tokens": ["Jm", "Was\u00b7ser", ",", "in", "der", "Lufft", ",", "und", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Den Krey\u00df der stets ver\u00e4nderten Figur;", "tokens": ["Den", "Krey\u00df", "der", "stets", "ver\u00b7\u00e4n\u00b7der\u00b7ten", "Fi\u00b7gur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.13": {"text": "Wodurch die Dinge der Natur", "tokens": ["Wo\u00b7durch", "die", "Din\u00b7ge", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So sterben, als gebohren werden.", "tokens": ["So", "ster\u00b7ben", ",", "als", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}