{"textgrid.poem.62386": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Wanderung am Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nonnen-Eiland in dem Flusse,", "tokens": ["Non\u00b7nen\u00b7Ei\u00b7land", "in", "dem", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rolandseck auf steiler H\u00f6h',", "tokens": ["Ro\u00b7lan\u00b7dseck", "auf", "stei\u00b7ler", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Seid gegr\u00fc\u00dft mit gutem Gru\u00dfe,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", "mit", "gu\u00b7tem", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich hier vor\u00fcber geh'.", "tokens": ["Weil", "ich", "hier", "vor\u00b7\u00fc\u00b7ber", "geh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mu\u00df ich stets vor\u00fcber ziehen,", "tokens": ["Mu\u00df", "ich", "stets", "vor\u00b7\u00fc\u00b7ber", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brech' ich keine Fr\u00fcchte ab,", "tokens": ["Brech'", "ich", "kei\u00b7ne", "Fr\u00fcch\u00b7te", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll mir keine Laube bl\u00fchen,", "tokens": ["Soll", "mir", "kei\u00b7ne", "Lau\u00b7be", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflanz' ich nie den Wanderstab?", "tokens": ["Pflanz'", "ich", "nie", "den", "Wan\u00b7der\u00b7stab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fl\u00fcchtet, fl\u00fcchtet, ihr Gedanken,", "tokens": ["Fl\u00fcch\u00b7tet", ",", "fl\u00fcch\u00b7tet", ",", "ihr", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der s\u00fc\u00dfen Ruhestatt,", "tokens": ["Nach", "der", "s\u00fc\u00b7\u00dfen", "Ru\u00b7he\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mit holden Zauberschranken", "tokens": ["Die", "mit", "hol\u00b7den", "Zau\u00b7ber\u00b7schran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euren Flug gefangen hat.", "tokens": ["Eu\u00b7ren", "Flug", "ge\u00b7fan\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "R\u00fcckw\u00e4rts, r\u00fcckw\u00e4rts, meine Blicke,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", ",", "r\u00fcck\u00b7w\u00e4rts", ",", "mei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem fernen stillen Haus,", "tokens": ["Nach", "dem", "fer\u00b7nen", "stil\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Baut euch muthig eine Br\u00fccke", "tokens": ["Baut", "euch", "mut\u00b7hig", "ei\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Berg und Thal hinaus.", "tokens": ["Ue\u00b7ber", "Berg", "und", "Thal", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gr\u00fc\u00dfet mir die frommen Kinder,", "tokens": ["Gr\u00fc\u00b7\u00dfet", "mir", "die", "from\u00b7men", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem frommen, lieben Fest,", "tokens": ["Bei", "dem", "from\u00b7men", ",", "lie\u00b7ben", "Fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Sehnsucht milder, linder", "tokens": ["Da\u00df", "die", "Sehn\u00b7sucht", "mil\u00b7der", ",", "lin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich im Lied vernehmen l\u00e4\u00dft.", "tokens": ["Sich", "im", "Lied", "ver\u00b7neh\u00b7men", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alle Freuden, allen Segen,", "tokens": ["Al\u00b7le", "Freu\u00b7den", ",", "al\u00b7len", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelsfrieden, s\u00fc\u00dfe Ruh,", "tokens": ["Him\u00b7mels\u00b7frie\u00b7den", ",", "s\u00fc\u00b7\u00dfe", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihr findet auf den Wegen", "tokens": ["Was", "ihr", "fin\u00b7det", "auf", "den", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchret meiner Freundin zu.", "tokens": ["F\u00fch\u00b7ret", "mei\u00b7ner", "Freun\u00b7din", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}