{"dta.poem.5771": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will ein entflammter Geist sich aus der Welt erh\u00f6hen;", "tokens": ["Will", "ein", "ent\u00b7flamm\u00b7ter", "Geist", "sich", "aus", "der", "Welt", "er\u00b7h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird er alsobald in einer tieffen See,", "tokens": ["So", "wird", "er", "al\u00b7so\u00b7bald", "in", "ei\u00b7ner", "tief\u00b7fen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Woselbst kein Grund, kein Strand, verwirret untergehen:", "tokens": ["Wo\u00b7selbst", "kein", "Grund", ",", "kein", "Strand", ",", "ver\u00b7wir\u00b7ret", "un\u00b7ter\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Unm\u00f6glich findet er ein Ziel in dieser H\u00f6h\u2019.", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "fin\u00b7det", "er", "ein", "Ziel", "in", "die\u00b7ser", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$."