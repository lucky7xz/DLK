{"textgrid.poem.26081": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir erkannten die Str\u00e4ucher noch nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir erkannten die Str\u00e4ucher noch nicht,", "tokens": ["Wir", "er\u00b7kann\u00b7ten", "die", "Str\u00e4u\u00b7cher", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Es hatte noch keiner sein Bl\u00e4ttergesicht.", "tokens": ["Es", "hat\u00b7te", "noch", "kei\u00b7ner", "sein", "Bl\u00e4t\u00b7ter\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nur der Rosenbusch zeigte zartzackige Spitzen,", "tokens": ["Nur", "der", "Ro\u00b7sen\u00b7busch", "zeig\u00b7te", "zart\u00b7za\u00b7cki\u00b7ge", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Und der Schlehdorn beperlt tat voll Augen sitzen.", "tokens": ["Und", "der", "Schleh\u00b7dorn", "be\u00b7perlt", "tat", "voll", "Au\u00b7gen", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADJD", "NN", "VVINF", "$."], "meter": "--++-++-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Sonst Gr\u00fcngras und Gr\u00fcnklee stunden bescheiden,", "tokens": ["Sonst", "Gr\u00fcn\u00b7gras", "und", "Gr\u00fcn\u00b7klee", "stun\u00b7den", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADJA", "ADJA", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gr\u00fcn ohne Blume durfte den Acker nur kleiden,", "tokens": ["Gr\u00fcn", "oh\u00b7ne", "Blu\u00b7me", "durf\u00b7te", "den", "A\u00b7cker", "nur", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und Wolken mu\u00dften im Grau noch weiden.", "tokens": ["Und", "Wol\u00b7ken", "mu\u00df\u00b7ten", "im", "Grau", "noch", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Uns gingen die Augen in Umschau sehns\u00fcchtig umher", "tokens": ["Uns", "gin\u00b7gen", "die", "Au\u00b7gen", "in", "Um\u00b7schau", "sehn\u00b7s\u00fcch\u00b7tig", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "PTKVZ"], "meter": "-+--+---+-+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und bepflanzten mit Herzw\u00fcnschen den Weg, der noch blumenleer.", "tokens": ["Und", "be\u00b7pflanz\u00b7ten", "mit", "Her\u00b7zw\u00fcn\u00b7schen", "den", "Weg", ",", "der", "noch", "blu\u00b7men\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "--+--++--+--+-+", "measure": "anapaest.di.plus"}}}}}