ter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Am heiligsten Altar und ehre Dich", "tokens": ["Am", "hei\u00b7ligs\u00b7ten", "Al\u00b7tar", "und", "eh\u00b7re", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "VVFIN", "PPER"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und pfleg in Dir Dein g\u00f6ttlich Selbst, ", "tokens": ["Und", "pfleg", "in", "Dir", "Dein", "g\u00f6tt\u00b7lich", "Selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die h\u00e4\u00dfliche Gestalt, die schaudernd Du", "tokens": ["Die", "h\u00e4\u00df\u00b7li\u00b7che", "Ge\u00b7stalt", ",", "die", "schau\u00b7dernd", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Spiegel Deines Lebens an Dir siehst,", "tokens": ["Im", "Spie\u00b7gel", "Dei\u00b7nes", "Le\u00b7bens", "an", "Dir", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Furie, die Dich zu Neid und Ha\u00df", "tokens": ["Die", "Fu\u00b7rie", ",", "die", "Dich", "zu", "Neid", "und", "Ha\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Eitelkeit anregte, sie, die Dich", "tokens": ["Und", "Ei\u00b7tel\u00b7keit", "an\u00b7reg\u00b7te", ",", "sie", ",", "die", "Dich"], "token_info": 