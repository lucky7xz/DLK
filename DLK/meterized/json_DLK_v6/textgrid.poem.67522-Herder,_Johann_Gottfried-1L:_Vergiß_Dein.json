{"textgrid.poem.67522": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergi\u00df Dein ", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergi\u00df Dein ", "tokens": ["Ver\u00b7gi\u00df", "Dein"], "token_info": ["word", "word"], "pos": ["VVIMP", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nichts Gr\u00f6\u00dfres konnt' aus ihrem Herzen Dir", "tokens": ["Nichts", "Gr\u00f6\u00df\u00b7res", "konnt'", "aus", "ih\u00b7rem", "Her\u00b7zen", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die reiche Gottheit geben als ", "tokens": ["Die", "rei\u00b7che", "Got\u00b7theit", "ge\u00b7ben", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was an der Mutter Brust, was an der Brust", "tokens": ["Was", "an", "der", "Mut\u00b7ter", "Brust", ",", "was", "an", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der gro\u00dfen Mutter, der belebenden", "tokens": ["Der", "gro\u00b7\u00dfen", "Mut\u00b7ter", ",", "der", "be\u00b7le\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Natur, von Elementen in Dich flo\u00df,", "tokens": ["Na\u00b7tur", ",", "von", "E\u00b7le\u00b7men\u00b7ten", "in", "Dich", "flo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Luft, Aether, Speis' und Trank und Regung, Bild,", "tokens": ["Luft", ",", "A\u00b7e\u00b7ther", ",", "Speis'", "und", "Trank", "und", "Re\u00b7gung", ",", "Bild", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Gedank' und Phantasei, bist Du nicht ", "tokens": ["Ge\u00b7dank'", "und", "Phan\u00b7ta\u00b7sei", ",", "bist", "Du", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du selbst bist, was aus Allem Du Dir schufst", "tokens": ["Du", "selbst", "bist", ",", "was", "aus", "Al\u00b7lem", "Du", "Dir", "schufst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "$,", "PRELS", "APPR", "PIS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bildetest und wardst und jetzo bist,", "tokens": ["Und", "bil\u00b7de\u00b7test", "und", "wardst", "und", "jet\u00b7zo", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht was Du siehest (auch das Thier bemerkt),", "tokens": ["Nicht", "was", "Du", "sie\u00b7hest", "(", "auch", "das", "Thier", "be\u00b7merkt", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "VVFIN", "$(", "ADV", "ART", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht was Du h\u00f6rest (auch das Thier vernimmt),", "tokens": ["Nicht", "was", "Du", "h\u00f6\u00b7rest", "(", "auch", "das", "Thier", "ver\u00b7nimmt", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "VVFIN", "$(", "ADV", "ART", "NN", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht was Du lernest (auch der Rabe lernt),", "tokens": ["Nicht", "was", "Du", "ler\u00b7nest", "(", "auch", "der", "Ra\u00b7be", "lernt", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "VVFIN", "$(", "ADV", "ART", "NN", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was Du verstehest und begreifst, die Macht,", "tokens": ["Was", "Du", "ver\u00b7ste\u00b7hest", "und", "be\u00b7greifst", ",", "die", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die in Dir wirkt, die innre Seherin,", "tokens": ["Die", "in", "Dir", "wirkt", ",", "die", "inn\u00b7re", "Se\u00b7he\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die aus der Vorwelt sich die Nachwelt schafft,", "tokens": ["Die", "aus", "der", "Vor\u00b7welt", "sich", "die", "Nach\u00b7welt", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Ordnerin, die aus Verwirrungen", "tokens": ["Die", "Ord\u00b7ne\u00b7rin", ",", "die", "aus", "Ver\u00b7wir\u00b7run\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Entwirrend webt den Kn\u00e4uel der Natur", "tokens": ["Ent\u00b7wir\u00b7rend", "webt", "den", "Kn\u00e4u\u00b7el", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zum sch\u00f6nen Teppich in und au\u00dfer Dir:", "tokens": ["Zum", "sch\u00f6\u00b7nen", "Tep\u00b7pich", "in", "und", "au\u00b7\u00dfer", "Dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "KON", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das bist Du ", "tokens": ["Das", "bist", "Du"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "\u00bbdie Gottheit?\u00ab Ja! denn denke, denke Dir", "tokens": ["\u00bb", "die", "Got\u00b7theit", "?", "\u00ab", "Ja", "!", "denn", "den\u00b7ke", ",", "den\u00b7ke", "Dir"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$.", "$(", "PTKANT", "$.", "KON", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wesen Chaos ohne Sinn und Geist,", "tokens": ["Der", "We\u00b7sen", "Chaos", "oh\u00b7ne", "Sinn", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ohn' einen Allerf\u00fcllenden, der sich", "tokens": ["Ohn'", "ei\u00b7nen", "Al\u00b7ler\u00b7f\u00fcl\u00b7len\u00b7den", ",", "der", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Allem Regel ist; gedenke Dir", "tokens": ["Und", "Al\u00b7lem", "Re\u00b7gel", "ist", ";", "ge\u00b7den\u00b7ke", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den gro\u00dfen Unsinn der sinnreichesten", "tokens": ["Den", "gro\u00b7\u00dfen", "Un\u00b7sinn", "der", "sinn\u00b7rei\u00b7ches\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Natur und st\u00fcrz unsinnig Dich hinab", "tokens": ["Na\u00b7tur", "und", "st\u00fcrz", "un\u00b7sin\u00b7nig", "Dich", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "ADJD", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ins \u00f6de Chaos, das sich selbst nicht kennt:", "tokens": ["Ins", "\u00f6\u00b7de", "Chaos", ",", "das", "sich", "selbst", "nicht", "kennt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn w\u00e4rest Du, wenn's nirgend ist, ein ", "tokens": ["Denn", "w\u00e4\u00b7rest", "Du", ",", "wenn's", "nir\u00b7gend", "ist", ",", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUS", "ADV", "VAFIN", "$,", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Zur\u00fcck in Dich! In Deinem innersten", "tokens": ["Zu\u00b7r\u00fcck", "in", "Dich", "!", "In", "Dei\u00b7nem", "in\u00b7ners\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$.", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bewu\u00dftsein lebt ein sprechender Beweis", "tokens": ["Be\u00b7wu\u00df\u00b7tsein", "lebt", "ein", "spre\u00b7chen\u00b7der", "Be\u00b7weis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom h\u00f6chsten ", "tokens": ["Vom", "h\u00f6chs\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Verliere Dich: und wunderst Dich, o Thor,", "tokens": ["Ver\u00b7lie\u00b7re", "Dich", ":", "und", "wun\u00b7derst", "Dich", ",", "o", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KON", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Du die Gottheit mit Dir selbst verlorst?", "tokens": ["Da\u00df", "Du", "die", "Got\u00b7theit", "mit", "Dir", "selbst", "ver\u00b7lorst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbder Wesen Harmonie!\u00ab Ein leeres Wort", "tokens": ["\u00bb", "der", "We\u00b7sen", "Har\u00b7mo\u00b7nie", "!", "\u00ab", "Ein", "lee\u00b7res", "Wort"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ohn' einen H\u00f6rer. H\u00f6re Du sie tief", "tokens": ["Ohn'", "ei\u00b7nen", "H\u00f6\u00b7rer", ".", "H\u00f6\u00b7re", "Du", "sie", "tief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Deinem Herzen, und es nennt Dein Herz", "tokens": ["In", "Dei\u00b7nem", "Her\u00b7zen", ",", "und", "es", "nennt", "Dein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In tiefster Stille mit dem vollen Chor", "tokens": ["In", "tiefs\u00b7ter", "Stil\u00b7le", "mit", "dem", "vol\u00b7len", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Welten Ihn, das ", "tokens": ["Der", "Wel\u00b7ten", "Ihn", ",", "das"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPER", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und Geist, das Wesen aller Wesen, Gott.", "tokens": ["Und", "Geist", ",", "das", "We\u00b7sen", "al\u00b7ler", "We\u00b7sen", ",", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"