{"textgrid.poem.56884": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du die B\u00e4ume im Windsto\u00df zischen?", "tokens": ["H\u00f6rst", "du", "die", "B\u00e4u\u00b7me", "im", "Wind\u00b7sto\u00df", "zi\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Siehst du, wie sie sich drehen und winden", "tokens": ["Siehst", "du", ",", "wie", "sie", "sich", "dre\u00b7hen", "und", "win\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVINF", "KON", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "unter des Regens tausendstr\u00e4hniger Gei\u00dfel?", "tokens": ["un\u00b7ter", "des", "Re\u00b7gens", "tau\u00b7send\u00b7str\u00e4h\u00b7ni\u00b7ger", "Gei\u00b7\u00dfel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gekr\u00fcmmten R\u00fcckens, erstarrten Blutes,", "tokens": ["Ge\u00b7kr\u00fcmm\u00b7ten", "R\u00fc\u00b7ckens", ",", "er\u00b7starr\u00b7ten", "Blu\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "fl\u00fcstern sie unaufh\u00f6rlich heisere Fl\u00fcche", "tokens": ["fl\u00fcs\u00b7tern", "sie", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "hei\u00b7se\u00b7re", "Fl\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "in den kalten, grausamen Herbst hinaus.", "tokens": ["in", "den", "kal\u00b7ten", ",", "grau\u00b7sa\u00b7men", "Herbst", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bl\u00fchten sie nicht in dankender Sch\u00f6nheit", "tokens": ["Bl\u00fch\u00b7ten", "sie", "nicht", "in", "dan\u00b7ken\u00b7der", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "G\u00f6ttern und Menschen auf?", "tokens": ["G\u00f6t\u00b7tern", "und", "Men\u00b7schen", "auf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Bargen der V\u00f6glein s\u00fc\u00dfes Geschw\u00e4tz nicht treu?", "tokens": ["Bar\u00b7gen", "der", "V\u00f6\u00b7glein", "s\u00fc\u00b7\u00dfes", "Ge\u00b7schw\u00e4tz", "nicht", "treu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Schildeten nicht vor Schlo\u00dfen das zarte Beet?", "tokens": ["Schil\u00b7de\u00b7ten", "nicht", "vor", "Schlo\u00b7\u00dfen", "das", "zar\u00b7te", "Beet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und der Sonne furchtbare Feuer \u2013", "tokens": ["Und", "der", "Son\u00b7ne", "furcht\u00b7ba\u00b7re", "Feu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "wer empfing sie, sich lautlos opfernd? ...", "tokens": ["wer", "emp\u00b7fing", "sie", ",", "sich", "laut\u00b7los", "op\u00b7fernd", "?", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PRF", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Sieh, wie die Armen im Sturm erschauern \u2013:", "tokens": ["Sieh", ",", "wie", "die", "Ar\u00b7men", "im", "Sturm", "er\u00b7schau\u00b7ern", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVINF", "$(", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Wie langzottige frierende Hunde,", "tokens": ["Wie", "lang\u00b7zot\u00b7ti\u00b7ge", "frie\u00b7ren\u00b7de", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "denen das nasse gestr\u00e4ubte Fell", "tokens": ["de\u00b7nen", "das", "nas\u00b7se", "ge\u00b7str\u00e4ub\u00b7te", "Fell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "\u00fcberwirbelt nach vorne weht,", "tokens": ["\u00fc\u00b7berw\u00b7ir\u00b7belt", "nach", "vor\u00b7ne", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "trotzen gestr\u00e4ubt die trostberaubten,", "tokens": ["trot\u00b7zen", "ge\u00b7str\u00e4ubt", "die", "trost\u00b7be\u00b7raub\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "und ihr herzzerbrechendes Seufzen", "tokens": ["und", "ihr", "herz\u00b7zer\u00b7bre\u00b7chen\u00b7des", "Seuf\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "rauscht umsonst", "tokens": ["rauscht", "um\u00b7sonst"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "ans graue Gew\u00f6lb der Wolken.", "tokens": ["ans", "grau\u00b7e", "Ge\u00b7w\u00f6lb", "der", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}