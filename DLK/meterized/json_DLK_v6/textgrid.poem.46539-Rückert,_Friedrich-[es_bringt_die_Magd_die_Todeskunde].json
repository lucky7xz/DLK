{"textgrid.poem.46539": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es bringt die Magd die Todeskunde]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bringt die Magd die Todeskunde", "tokens": ["Es", "bringt", "die", "Magd", "die", "To\u00b7des\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Schwesterchen der Knabenschaar;", "tokens": ["Vom", "Schwes\u00b7ter\u00b7chen", "der", "Kna\u00b7ben\u00b7schaar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da rufen sie mit Einem Munde:", "tokens": ["Da", "ru\u00b7fen", "sie", "mit", "Ei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist nicht todt, es ist nicht wahr.", "tokens": ["Sie", "ist", "nicht", "todt", ",", "es", "ist", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sehen sie mit blassem Munde,", "tokens": ["Sie", "se\u00b7hen", "sie", "mit", "blas\u00b7sem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wei\u00dfer Wang' im dunklen Haar,", "tokens": ["Mit", "wei\u00b7\u00dfer", "Wang'", "im", "dunk\u00b7len", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fl\u00fcstern leiser in die Runde:", "tokens": ["Und", "fl\u00fcs\u00b7tern", "lei\u00b7ser", "in", "die", "Run\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist nicht todt, es ist nicht wahr.", "tokens": ["Sie", "ist", "nicht", "todt", ",", "es", "ist", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Vater weint aus Herzenswunde,", "tokens": ["Der", "Va\u00b7ter", "weint", "aus", "Her\u00b7zens\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter weint, sie nehmen's wahr", "tokens": ["Die", "Mut\u00b7ter", "weint", ",", "sie", "neh\u00b7men's", "wahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleiben doch bei ihrem Grunde:", "tokens": ["Und", "blei\u00b7ben", "doch", "bei", "ih\u00b7rem", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist nicht todt, es ist nicht wahr.", "tokens": ["Sie", "ist", "nicht", "todt", ",", "es", "ist", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als gekommen war die Stunde,", "tokens": ["Und", "als", "ge\u00b7kom\u00b7men", "war", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man legt sie auf die Todtenbahr,", "tokens": ["Man", "legt", "sie", "auf", "die", "Tod\u00b7ten\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man senkt sie ein im k\u00fchlen Grunde:", "tokens": ["Man", "senkt", "sie", "ein", "im", "k\u00fch\u00b7len", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist nicht todt, es ist nicht wahr.", "tokens": ["Sie", "ist", "nicht", "todt", ",", "es", "ist", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So bleibe sie mit euch im Bunde", "tokens": ["So", "blei\u00b7be", "sie", "mit", "euch", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und werde sch\u00f6ner jedes Jahr", "tokens": ["Und", "wer\u00b7de", "sch\u00f6\u00b7ner", "je\u00b7des", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und werd' euch lieber jede Stunde!", "tokens": ["Und", "werd'", "euch", "lie\u00b7ber", "je\u00b7de", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist nicht todt, es ist nicht wahr.", "tokens": ["Sie", "ist", "nicht", "todt", ",", "es", "ist", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}