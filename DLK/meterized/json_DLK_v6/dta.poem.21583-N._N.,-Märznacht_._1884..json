{"dta.poem.21583": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rznacht .  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht, in Deinem Mutterscho\u00dfe", "tokens": ["Nacht", ",", "in", "Dei\u00b7nem", "Mut\u00b7ter\u00b7scho\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht der Lenz, ein stilles Kind,", "tokens": ["Ruht", "der", "Lenz", ",", "ein", "stil\u00b7les", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df noch nicht, wie herrlich gro\u00dfe", "tokens": ["Wei\u00df", "noch", "nicht", ",", "wie", "herr\u00b7lich", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wonnen ihm beschieden sind.", "tokens": ["Won\u00b7nen", "ihm", "be\u00b7schie\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seine Augen blicken staunend", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "bli\u00b7cken", "stau\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Erde, auf die Braut,", "tokens": ["Auf", "die", "Er\u00b7de", ",", "auf", "die", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seinen Lippen raunend", "tokens": ["Und", "von", "sei\u00b7nen", "Lip\u00b7pen", "rau\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klingt der erste Liebeslaut.", "tokens": ["Klingt", "der", "ers\u00b7te", "Lie\u00b7bes\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Erde h\u00f6rt ihn klingen,", "tokens": ["Und", "die", "Er\u00b7de", "h\u00f6rt", "ihn", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breitet weit die Arme aus,", "tokens": ["Brei\u00b7tet", "weit", "die", "Ar\u00b7me", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnsuchtsvolle Gr\u00fc\u00dfe dringen", "tokens": ["Sehn\u00b7suchts\u00b7vol\u00b7le", "Gr\u00fc\u00b7\u00dfe", "drin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich in die Nacht hinaus.", "tokens": ["Heim\u00b7lich", "in", "die", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch das Herz geht ihr ein Beben,", "tokens": ["Durch", "das", "Herz", "geht", "ihr", "ein", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umend neigt sie ihr Gesicht,", "tokens": ["Tr\u00e4u\u00b7mend", "neigt", "sie", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Luft beginnt\u2019s zu weben,", "tokens": ["In", "der", "Luft", "be\u00b7ginnt's", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Silbern rinnt des Mondes Licht.", "tokens": ["Sil\u00b7bern", "rinnt", "des", "Mon\u00b7des", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die noch schlafen, aus den W\u00e4ldern", "tokens": ["Die", "noch", "schla\u00b7fen", ",", "aus", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht\u2019s wie leiser Vogelsang,", "tokens": ["Rauscht's", "wie", "lei\u00b7ser", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die noch keimen, von den Feldern", "tokens": ["Die", "noch", "kei\u00b7men", ",", "von", "den", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht\u2019s wie Duft das Thal entlang.", "tokens": ["Bl\u00fcht's", "wie", "Duft", "das", "Thal", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Flammen leuchten durch die Ferne,", "tokens": ["Flam\u00b7men", "leuch\u00b7ten", "durch", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unh\u00f6rbare Winde weh\u2019n", "tokens": ["Un\u00b7h\u00f6r\u00b7ba\u00b7re", "Win\u00b7de", "weh'n"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und das Aug\u2019 von Stern zu Sterne", "tokens": ["Und", "das", "Aug'", "von", "Stern", "zu", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann den Himmel offen seh\u2019n.", "tokens": ["Kann", "den", "Him\u00b7mel", "of\u00b7fen", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Liebste, siehst Du rings es glimmen,", "tokens": ["Liebs\u00b7te", ",", "siehst", "Du", "rings", "es", "glim\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst Du rings den goldnen Schein,", "tokens": ["Siehst", "Du", "rings", "den", "gold\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst Du rings die tausend Stimmen?", "tokens": ["H\u00f6rst", "Du", "rings", "die", "tau\u00b7send", "Stim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erde saugt den Himmel ein.", "tokens": ["Er\u00b7de", "saugt", "den", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebste, la\u00df in Dir die Schauer", "tokens": ["Liebs\u00b7te", ",", "la\u00df", "in", "Dir", "die", "Schau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weben dieser heil\u2019gen Nacht,", "tokens": ["We\u00b7ben", "die\u00b7ser", "heil'\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keines Winters d\u00fcst\u2019re Trauer", "tokens": ["Kei\u00b7nes", "Win\u00b7ters", "d\u00fcst'\u00b7re", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat nun f\u00fcrder ob uns Macht.", "tokens": ["Hat", "nun", "f\u00fcr\u00b7der", "ob", "uns", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "PPER", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Und wie diese Nacht, so pr\u00e4chtig,", "tokens": ["Und", "wie", "die\u00b7se", "Nacht", ",", "so", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wird ob unserm Leben stehn,", "tokens": ["Wird", "ob", "un\u00b7serm", "Le\u00b7ben", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Liebe, lenzesm\u00e4chtig", "tokens": ["Uns\u00b7re", "Lie\u00b7be", ",", "len\u00b7zes\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sie durch die Seele wehn.", "tokens": ["Wird", "sie", "durch", "die", "See\u00b7le", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Tausend Bl\u00fcthen wird sie reifen,", "tokens": ["Tau\u00b7send", "Bl\u00fc\u00b7then", "wird", "sie", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns mit tausend Kr\u00e4nzen zier\u2019n,", "tokens": ["Uns", "mit", "tau\u00b7send", "Kr\u00e4n\u00b7zen", "zier'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird mit lauen Winden streifen", "tokens": ["Wird", "mit", "lau\u00b7en", "Win\u00b7den", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen Staub von unsrer Stirn.", "tokens": ["Al\u00b7len", "Staub", "von", "uns\u00b7rer", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nach den Tagen hei\u00df vom Ringen", "tokens": ["Nach", "den", "Ta\u00b7gen", "hei\u00df", "vom", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird sie mondesglanzgeweiht,", "tokens": ["Wird", "sie", "mon\u00b7des\u00b7glanz\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns mit heimlich s\u00fc\u00dfem Klingen", "tokens": ["Uns", "mit", "heim\u00b7lich", "s\u00fc\u00b7\u00dfem", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiegen in Traumseligkeit.", "tokens": ["Wie\u00b7gen", "in", "Traum\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Nacht des M\u00e4rzen, Nacht der Liebe,", "tokens": ["Nacht", "des", "M\u00e4r\u00b7zen", ",", "Nacht", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Scho\u00df gebiert das Licht,", "tokens": ["Eu\u00b7er", "Scho\u00df", "ge\u00b7biert", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr heiliget die Triebe,", "tokens": ["Die", "ihr", "hei\u00b7li\u00b7get", "die", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Flammen l\u00f6schen nicht.", "tokens": ["Eu\u00b7re", "Flam\u00b7men", "l\u00f6\u00b7schen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Lenze keimen und vergehen", "tokens": ["Len\u00b7ze", "kei\u00b7men", "und", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Erde Bau zerf\u00e4llt,", "tokens": ["Und", "der", "Er\u00b7de", "Bau", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch aus euch wird auferstehen", "tokens": ["Doch", "aus", "euch", "wird", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ewig neu die goldne Welt.", "tokens": ["E\u00b7wig", "neu", "die", "gold\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}