{"dta.poem.18795": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Lass deinen s\u00fcssen Rubinenmund", "tokens": ["Lass", "dei\u00b7nen", "s\u00fcs\u00b7sen", "Ru\u00b7bi\u00b7nen\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zudringlichkeiten nicht verfluchen,", "tokens": ["Zu\u00b7dring\u00b7lich\u00b7kei\u00b7ten", "nicht", "ver\u00b7flu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was hat Liebesschmerz andern Grund", "tokens": ["Was", "hat", "Lie\u00b7bes\u00b7schmerz", "an\u00b7dern", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "NN", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Als seine Heilung zu suchen.", "tokens": ["Als", "sei\u00b7ne", "Hei\u00b7lung", "zu", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bist du von deiner Geliebten getrennt", "tokens": ["Bist", "du", "von", "dei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "ge\u00b7trennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Orient vom Occident,", "tokens": ["Wie", "O\u00b7rient", "vom", "Oc\u00b7ci\u00b7dent", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Herz durch alle W\u00fcste rennt,", "tokens": ["Das", "Herz", "durch", "al\u00b7le", "W\u00fcs\u00b7te", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es giebt sich \u00fcberall selbst das Geleit,", "tokens": ["Es", "giebt", "sich", "\u00fc\u00b7be\u00b7rall", "selbst", "das", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "F\u00fcr Liebende ist Bagdad nicht weit.", "tokens": ["F\u00fcr", "Lie\u00b7ben\u00b7de", "ist", "Bag\u00b7dad", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NE", "PTKNEG", "ADJD", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}}}}