{"dta.poem.12566": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Jakob.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte Jakob starb heut Nacht \u2014", "tokens": ["Der", "al\u00b7te", "Ja\u00b7kob", "starb", "heut", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da haben sie am fr\u00fchen Morgen", "tokens": ["Da", "ha\u00b7ben", "sie", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sechs Brettchen ihm zurecht gemacht", "tokens": ["Sechs", "Brett\u00b7chen", "ihm", "zu\u00b7recht", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "PPER", "PTKVZ", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und drin den Schatz geborgen.", "tokens": ["Und", "drin", "den", "Schatz", "ge\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein schmucklos Haus! Man giebt in's Grab", "tokens": ["Ein", "schmuck\u00b7los", "Haus", "!", "Man", "giebt", "in's", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Feldherrn doch den Feldherrndegen \u2014", "tokens": ["Dem", "Feld\u00b7herrn", "doch", "den", "Feld\u00b7herrn\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warum nicht auch den Bettelstab", "tokens": ["Wa\u00b7rum", "nicht", "auch", "den", "Bet\u00b7tel\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf diese Bahre legen?", "tokens": ["Auf", "die\u00b7se", "Bah\u00b7re", "le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Degen, den er treu gef\u00fchrt,", "tokens": ["Den", "De\u00b7gen", ",", "den", "er", "treu", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in die Scheide nie gekommen,", "tokens": ["Der", "in", "die", "Schei\u00b7de", "nie", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis ihn der letzte Schlag ger\u00fchrt", "tokens": ["Bis", "ihn", "der", "letz\u00b7te", "Schlag", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Welt genommen.", "tokens": ["Und", "von", "der", "Welt", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er war der Welt, sie seiner satt \u2014", "tokens": ["Er", "war", "der", "Welt", ",", "sie", "sei\u00b7ner", "satt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Zw\u00f6lfen in der engen Stube! \u2014", "tokens": ["Zu", "Zw\u00f6l\u00b7fen", "in", "der", "en\u00b7gen", "Stu\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weh' ihm ein \u00fcberfl\u00fcssig Blatt,", "tokens": ["Weh'", "ihm", "ein", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Lenz, in seine Grube!", "tokens": ["O", "Lenz", ",", "in", "sei\u00b7ne", "Gru\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als h\u00e4tt' er Gro\u00dfes nie gethan,", "tokens": ["Als", "h\u00e4tt'", "er", "Gro\u00b7\u00dfes", "nie", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist rasch der Gl\u00fcckliche vergessen,", "tokens": ["Ist", "rasch", "der", "Gl\u00fcck\u00b7li\u00b7che", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Dichter stimmt ihm Psalmen an,", "tokens": ["Kein", "Dich\u00b7ter", "stimmt", "ihm", "Psal\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Pfaffe liest ihm Messen.", "tokens": ["Kein", "Pfaf\u00b7fe", "liest", "ihm", "Mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Heller, die man in den Sand", "tokens": ["Die", "Hel\u00b7ler", ",", "die", "man", "in", "den", "Sand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm warf aus schimmernden Karossen,", "tokens": ["Ihm", "warf", "aus", "schim\u00b7mern\u00b7den", "Ka\u00b7ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind Alles, was vom Vaterland", "tokens": ["Sind", "Al\u00b7les", ",", "was", "vom", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der arme Mann genossen.", "tokens": ["Der", "ar\u00b7me", "Mann", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Just die vom Himmel ihm geprahlt,", "tokens": ["Just", "die", "vom", "Him\u00b7mel", "ihm", "ge\u00b7prahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah'n diese Erde zwiefach gerne:", "tokens": ["Sah'n", "die\u00b7se", "Er\u00b7de", "zwie\u00b7fach", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wird die Schuld an's Volk bezahlt", "tokens": ["So", "wird", "die", "Schuld", "an's", "Volk", "be\u00b7zahlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wechseln auf die Sterne.", "tokens": ["Mit", "Wech\u00b7seln", "auf", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und kaum ist uns genug am Joch", "tokens": ["Und", "kaum", "ist", "uns", "ge\u00b7nug", "am", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Armuth auf gekr\u00fcmmten R\u00fccken:", "tokens": ["Der", "Ar\u00b7muth", "auf", "ge\u00b7kr\u00fcmm\u00b7ten", "R\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man will der Knechtschaft Stempel noch", "tokens": ["Man", "will", "der", "Knecht\u00b7schaft", "Stem\u00b7pel", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr auf die Stirne dr\u00fccken!", "tokens": ["Ihr", "auf", "die", "Stir\u00b7ne", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schlaf wohl in deinem Sarkophag,", "tokens": ["Schlaf", "wohl", "in", "dei\u00b7nem", "Sar\u00b7ko\u00b7phag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin sie dich ohne Hemd begraben:", "tokens": ["Drin", "sie", "dich", "oh\u00b7ne", "Hemd", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird kein F\u00fcrst am j\u00fcngsten Tag", "tokens": ["Es", "wird", "kein", "F\u00fcrst", "am", "j\u00fcng\u00b7sten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch reine W\u00e4sche haben!", "tokens": ["Noch", "rei\u00b7ne", "W\u00e4\u00b7sche", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}