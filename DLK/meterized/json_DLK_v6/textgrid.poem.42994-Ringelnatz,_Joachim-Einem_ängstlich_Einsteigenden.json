{"textgrid.poem.42994": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Einem \u00e4ngstlich Einsteigenden", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieg zu, Insasse!", "tokens": ["Flieg", "zu", ",", "In\u00b7sas\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und lasse,", "tokens": ["Und", "las\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Lasse dich", "tokens": ["Las\u00b7se", "dich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Nur \u00e4u\u00dferlich", "tokens": ["Nur", "\u00e4u\u00b7\u00dfer\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von andern lenken.", "tokens": ["Von", "an\u00b7dern", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft denken:", "tokens": ["Du", "mu\u00dft", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Deine Linie geht", "tokens": ["Dei\u00b7ne", "Li\u00b7nie", "geht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nach deinem Willen", "tokens": ["Nach", "dei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und im stillen", "tokens": ["Und", "im", "stil\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie ein arglos Gebet.", "tokens": ["Wie", "ein", "arg\u00b7los", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Selbstverst\u00e4ndlich interessiere dich", "tokens": ["Selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", "in\u00b7ter\u00b7es\u00b7sie\u00b7re", "dich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sehr f\u00fcr Wetter, H\u00f6henmesser,", "tokens": ["Sehr", "f\u00fcr", "Wet\u00b7ter", ",", "H\u00f6\u00b7hen\u00b7mes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Richtung, Zeit etcetera. Jedoch:", "tokens": ["Rich\u00b7tung", ",", "Zeit", "et\u00b7ce\u00b7te\u00b7ra", ".", "Je\u00b7doch", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weite Gedanken tragen dich", "tokens": ["Wei\u00b7te", "Ge\u00b7dan\u00b7ken", "tra\u00b7gen", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Noch h\u00f6her und noch besser", "tokens": ["Noch", "h\u00f6\u00b7her", "und", "noch", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als es deine Maschine tut,", "tokens": ["Als", "es", "dei\u00b7ne", "Ma\u00b7schi\u00b7ne", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Fliege gut!", "tokens": ["Flie\u00b7ge", "gut", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}