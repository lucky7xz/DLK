{"textgrid.poem.62309": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich m\u00f6chte auf einem Bilde dich seh'n,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte auf einem Bilde dich seh'n,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "auf", "ei\u00b7nem", "Bil\u00b7de", "dich", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umringt von gl\u00fccklichen Tieren,", "tokens": ["Um\u00b7ringt", "von", "gl\u00fcck\u00b7li\u00b7chen", "Tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die heute hochjubelnd vor dir steh'n", "tokens": ["Die", "heu\u00b7te", "hoch\u00b7ju\u00b7belnd", "vor", "dir", "steh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dankbar dir gratulieren.", "tokens": ["Und", "dank\u00b7bar", "dir", "gra\u00b7tu\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Einen, die V\u00f6gel nach ihrer Art", "tokens": ["Die", "Ei\u00b7nen", ",", "die", "V\u00f6\u00b7gel", "nach", "ih\u00b7rer", "Art"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit sinnigen Liedern, so weich und zart,", "tokens": ["Mit", "sin\u00b7ni\u00b7gen", "Lie\u00b7dern", ",", "so", "weich", "und", "zart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Hunde, die treuesten Seelen,", "tokens": ["Die", "Hun\u00b7de", ",", "die", "treu\u00b7es\u00b7ten", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die H\u00e4nde zum Kusse dir stehlen.", "tokens": ["Die", "H\u00e4n\u00b7de", "zum", "Kus\u00b7se", "dir", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}