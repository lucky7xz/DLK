{"textgrid.poem.46891": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du kannst mich auch nicht tr\u00f6sten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kannst mich auch nicht tr\u00f6sten,", "tokens": ["Du", "kannst", "mich", "auch", "nicht", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Natur, wie ich geglaubt;", "tokens": ["Na\u00b7tur", ",", "wie", "ich", "ge\u00b7glaubt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir ist von Gluth und Fr\u00f6sten", "tokens": ["Dir", "ist", "von", "Gluth", "und", "Fr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Selbst aller Trost geraubt.", "tokens": ["Selbst", "al\u00b7ler", "Trost", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich mu\u00df an andrer Quelle", "tokens": ["Ich", "mu\u00df", "an", "an\u00b7drer", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erst sch\u00f6pfen andres Heil,", "tokens": ["Erst", "sch\u00f6p\u00b7fen", "and\u00b7res", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh mir in deiner Zelle", "tokens": ["Eh", "mir", "in", "dei\u00b7ner", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird wieder Ruh zu Theil.", "tokens": ["Wird", "wie\u00b7der", "Ruh", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich selber mu\u00df verk\u00fcnden", "tokens": ["Ich", "sel\u00b7ber", "mu\u00df", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir deiner R\u00e4thsel Wort,", "tokens": ["Dir", "dei\u00b7ner", "R\u00e4th\u00b7sel", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darob in dunkeln Gr\u00fcnden", "tokens": ["Da\u00b7rob", "in", "dun\u00b7keln", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du sinnest fort und fort.", "tokens": ["Du", "sin\u00b7nest", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann jubeln wir und klagen", "tokens": ["Dann", "ju\u00b7beln", "wir", "und", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um unser gleich Geschick,", "tokens": ["Um", "un\u00b7ser", "gleich", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6nnen still es tragen,", "tokens": ["Und", "k\u00f6n\u00b7nen", "still", "es", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verkl\u00e4rt von Gottes Blick.", "tokens": ["Ver\u00b7kl\u00e4rt", "von", "Got\u00b7tes", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}