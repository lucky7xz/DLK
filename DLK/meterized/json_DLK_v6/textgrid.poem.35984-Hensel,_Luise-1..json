{"textgrid.poem.35984": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie rinnt sie, die Welle,", "tokens": ["Wie", "rinnt", "sie", ",", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Gr\u00fcnen so helle,", "tokens": ["Im", "Gr\u00fc\u00b7nen", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die blauen Vergi\u00dfmeinnicht schauen hinein.", "tokens": ["Die", "blau\u00b7en", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "schau\u00b7en", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drin leben und weben", "tokens": ["Drin", "le\u00b7ben", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Fischlein, sie schweben", "tokens": ["Die", "Fisc\u00b7hlein", ",", "sie", "schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und spielen im freundlichen Sonnenschein.", "tokens": ["Und", "spie\u00b7len", "im", "freund\u00b7li\u00b7chen", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ihr Blumen, ihr lieben,", "tokens": ["Ihr", "Blu\u00b7men", ",", "ihr", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was schaut ihr nach dr\u00fcben,", "tokens": ["Was", "schaut", "ihr", "nach", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Als w\u00e4re die Sonne dort immer zu sehn?", "tokens": ["Als", "w\u00e4\u00b7re", "die", "Son\u00b7ne", "dort", "im\u00b7mer", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Winde, geschwinde,", "tokens": ["Die", "Win\u00b7de", ",", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie spielen hier linde,", "tokens": ["Sie", "spie\u00b7len", "hier", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Hier scheint ja die Sonne ebenso sch\u00f6n.", "tokens": ["Hier", "scheint", "ja", "die", "Son\u00b7ne", "e\u00b7ben\u00b7so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}