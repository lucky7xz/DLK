{"textgrid.poem.57083": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Traumwald", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Vogels Aug verschleiert sich;", "tokens": ["Des", "Vo\u00b7gels", "Aug", "ver\u00b7schlei\u00b7ert", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er sinkt in Schlaf auf seinem Baum.", "tokens": ["er", "sinkt", "in", "Schlaf", "auf", "sei\u00b7nem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wald verwandelt sich in Traum", "tokens": ["Der", "Wald", "ver\u00b7wan\u00b7delt", "sich", "in", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wird so tief und feierlich.", "tokens": ["und", "wird", "so", "tief", "und", "fei\u00b7er\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mond, der stille, steigt empor:", "tokens": ["Der", "Mond", ",", "der", "stil\u00b7le", ",", "steigt", "em\u00b7por", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kleine Kehle zwitschert matt.", "tokens": ["Die", "klei\u00b7ne", "Keh\u00b7le", "zwit\u00b7schert", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im ganzen Walde schwingt kein Blatt.", "tokens": ["Im", "gan\u00b7zen", "Wal\u00b7de", "schwingt", "kein", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fern l\u00e4utet, fern, der Sterne Chor.", "tokens": ["Fern", "l\u00e4u\u00b7tet", ",", "fern", ",", "der", "Ster\u00b7ne", "Chor", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}