{"dta.poem.8628": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.71", "af:0.28"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcbe wird's, die Wolken jagen,", "tokens": ["Tr\u00fc\u00b7be", "wird's", ",", "die", "Wol\u00b7ken", "ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Regen niederbricht,", "tokens": ["Und", "der", "Re\u00b7gen", "nie\u00b7der\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die lauten Winde klagen:", "tokens": ["Und", "die", "lau\u00b7ten", "Win\u00b7de", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eteich, wo ist dein Sternenlicht?\u201c", "tokens": ["\u201e", "teich", ",", "wo", "ist", "dein", "Ster\u00b7nen\u00b7licht", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Suchen den erloschnen Schimmer", "tokens": ["Su\u00b7chen", "den", "er\u00b7lo\u00b7schnen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief im aufgew\u00fchlten See.", "tokens": ["Tief", "im", "auf\u00b7ge\u00b7w\u00fchl\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Liebe l\u00e4chelt nimmer", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "l\u00e4\u00b7chelt", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder in mein tiefes Weh!", "tokens": ["Nie\u00b7der", "in", "mein", "tie\u00b7fes", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}