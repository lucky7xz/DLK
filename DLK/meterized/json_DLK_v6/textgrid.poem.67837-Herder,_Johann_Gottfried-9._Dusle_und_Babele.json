{"textgrid.poem.67837": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9. Dusle und Babele", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es h\u00e4tt' e' Buur e' T\u00f6chterli,", "tokens": ["Es", "h\u00e4tt'", "e'", "Buur", "e'", "T\u00f6ch\u00b7ter\u00b7li", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Name hie\u00df es Babeli,", "tokens": ["Mit", "Na\u00b7me", "hie\u00df", "es", "Ba\u00b7be\u00b7li", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00e4tt' e' paar Z\u00f6pfle, sie sind wie Gold,", "tokens": ["Es", "h\u00e4tt'", "e'", "paar", "Z\u00f6pf\u00b7le", ",", "sie", "sind", "wie", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,", "PPER", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Drum ist ihm auch der Dusle hold.", "tokens": ["Drum", "ist", "ihm", "auch", "der", "Dus\u00b7le", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Dusle lief dem Vater na':", "tokens": ["Der", "Dus\u00b7le", "lief", "dem", "Va\u00b7ter", "na'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo Vater, wollt ihr mir 's Babele lahn?\u00ab", "tokens": ["\u00bb", "o", "Va\u00b7ter", ",", "wollt", "ihr", "mir", "'s", "Ba\u00b7be\u00b7le", "lahn", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "VMFIN", "PPER", "PPER", "PPER", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "\u00bbdas Babele ist noch viel zu klein;", "tokens": ["\u00bb", "das", "Ba\u00b7be\u00b7le", "ist", "noch", "viel", "zu", "klein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es schl\u00e4ft dies Jahr noch wohl allein.\u00ab", "tokens": ["Es", "schl\u00e4ft", "dies", "Jahr", "noch", "wohl", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Dusle lief in einer Stund',", "tokens": ["Der", "Dus\u00b7le", "lief", "in", "ei\u00b7ner", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lief abe bis gen Solothurn,", "tokens": ["Lief", "a\u00b7be", "bis", "gen", "So\u00b7lo\u00b7thurn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lief die Stadt wohl uf und ab,", "tokens": ["Er", "lief", "die", "Stadt", "wohl", "uf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er zum \u00f6bersten Hauptmann kam:", "tokens": ["Bis", "er", "zum", "\u00f6\u00b7bers\u00b7ten", "Haupt\u00b7mann", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbo Hauptmann, lieber Hauptmann mi',", "tokens": ["\u00bb", "o", "Haupt\u00b7mann", ",", "lie\u00b7ber", "Haupt\u00b7mann", "mi'", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "I' will mi' dingen in Flandern 'ni'!\u00ab", "tokens": ["I'", "will", "mi'", "din\u00b7gen", "in", "Flan\u00b7dern", "'", "ni", "'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "VMFIN", "PIAT", "NN", "APPR", "NE", "$(", "NE", "$(", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Hauptmann zog die Seckelschnur,", "tokens": ["Der", "Haupt\u00b7mann", "zog", "die", "Se\u00b7ckel\u00b7schnur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gab dem Dusle drey Thaler drus.", "tokens": ["Gab", "dem", "Dus\u00b7le", "drey", "Tha\u00b7ler", "drus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "CARD", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Der Dusle lief wohl wieder heim,", "tokens": ["Der", "Dus\u00b7le", "lief", "wohl", "wie\u00b7der", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heim zu s'in'm liebe Babelein:", "tokens": ["Heim", "zu", "s'\u00b7in'm", "lie\u00b7be", "Ba\u00b7be\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbo Babele, liebes Babele mi',", "tokens": ["\u00bb", "o", "Ba\u00b7be\u00b7le", ",", "lie\u00b7bes", "Ba\u00b7be\u00b7le", "mi'", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jezt hab i' mi' dungen in Flandern 'ni'!\u00ab", "tokens": ["Jezt", "hab", "i'", "mi'", "dun\u00b7gen", "in", "Flan\u00b7dern", "'", "ni", "'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "FM.nl", "FM.nl", "FM.nl", "APPR", "NE", "$(", "NE", "$(", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Das Babele lief wohl hinters Huus,", "tokens": ["Das", "Ba\u00b7be\u00b7le", "lief", "wohl", "hin\u00b7ters", "Huus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es grient ihm schier sin' Aeugele uus:", "tokens": ["Es", "grient", "ihm", "schier", "sin'", "A\u00b7e\u00b7u\u00b7ge\u00b7le", "u\u00b7us", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbo Babele, thu doch nit so sehr,", "tokens": ["\u00bb", "o", "Ba\u00b7be\u00b7le", ",", "thu", "doch", "nit", "so", "sehr", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "I' will ja wieder kommen zu dir!", "tokens": ["I'", "will", "ja", "wie\u00b7der", "kom\u00b7men", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Und komm i' \u00fcbers Jahr nit heim,", "tokens": ["Und", "komm", "i'", "\u00fc\u00b7bers", "Jahr", "nit", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So will i' dir schreiben e' Briefelein,", "tokens": ["So", "will", "i'", "dir", "schrei\u00b7ben", "e'", "Brie\u00b7fel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Darinnen soll geschrieben stahn:", "tokens": ["Da\u00b7rin\u00b7nen", "soll", "ge\u00b7schrie\u00b7ben", "stahn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "I' will min Babele nit verlahn!\u00ab", "tokens": ["I'", "will", "min", "Ba\u00b7be\u00b7le", "nit", "ver\u00b7lahn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}