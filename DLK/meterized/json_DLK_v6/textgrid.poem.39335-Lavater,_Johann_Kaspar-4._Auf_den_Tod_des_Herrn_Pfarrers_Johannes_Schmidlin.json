{"textgrid.poem.39335": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf den Tod des Herrn Pfarrers Johannes Schmidlin", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! Ihn hat Gott uns weggenommen!", "tokens": ["Ach", "!", "Ihn", "hat", "Gott", "uns", "weg\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Schmidlin folgt des Todes Ruf!", "tokens": ["Auch", "Schmid\u00b7lin", "folgt", "des", "To\u00b7des", "Ruf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Weynt ihn, den Redlichen und Frommen,", "tokens": ["Weynt", "ihn", ",", "den", "Red\u00b7li\u00b7chen", "und", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der tausend Freuden uns erschuff!", "tokens": ["Der", "tau\u00b7send", "Freu\u00b7den", "uns", "er\u00b7schuff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! Wie sein Leben schnell verbl\u00fchte!", "tokens": ["Ach", "!", "Wie", "sein", "Le\u00b7ben", "schnell", "ver\u00b7bl\u00fch\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Aug voll Heiterkeit und G\u00fcte,", "tokens": ["Sein", "Aug", "voll", "Hei\u00b7ter\u00b7keit", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie schnell es jedem Aug sich schlo\u00df,", "tokens": ["Wie", "schnell", "es", "je\u00b7dem", "Aug", "sich", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das reine Thr\u00e4nen ihm vergo\u00df!", "tokens": ["Das", "rei\u00b7ne", "Thr\u00e4\u00b7nen", "ihm", "ver\u00b7go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf sanfter Harmonieen Fl\u00fcgel", "tokens": ["Auf", "sanf\u00b7ter", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Flog er empfindungsvoll uns vor,", "tokens": ["Flog", "er", "emp\u00b7fin\u00b7dungs\u00b7voll", "uns", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ri\u00df sanft uns mit sich! \u00dcber H\u00fcgel", "tokens": ["Ri\u00df", "sanft", "uns", "mit", "sich", "!", "\u00dc\u00b7ber", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PRF", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde trug er uns empor!", "tokens": ["Der", "Er\u00b7de", "trug", "er", "uns", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er str\u00f6mte Lust um sich, beseelte", "tokens": ["Er", "str\u00f6m\u00b7te", "Lust", "um", "sich", ",", "be\u00b7seel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PRF", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Dichters Lied, dem Leben fehlte!", "tokens": ["Des", "Dich\u00b7ters", "Lied", ",", "dem", "Le\u00b7ben", "fehl\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie gab's dem hei\u00dfen Wandrer k\u00fchl", "tokens": ["Wie", "gab's", "dem", "hei\u00b7\u00dfen", "Wand\u00b7rer", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Auferstehung Vorgef\u00fchl!", "tokens": ["Der", "Auf\u00b7er\u00b7ste\u00b7hung", "Vor\u00b7ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie viele tausend stille Freuden", "tokens": ["Wie", "vie\u00b7le", "tau\u00b7send", "stil\u00b7le", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbreitet' er stets um sich her!", "tokens": ["Ver\u00b7brei\u00b7tet'", "er", "stets", "um", "sich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie manchen Trost in dunkeln Leiden,", "tokens": ["Wie", "man\u00b7chen", "Trost", "in", "dun\u00b7keln", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie manche s\u00fc\u00dfe Thr\u00e4nen er!", "tokens": ["Wie", "man\u00b7che", "s\u00fc\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum lie\u00df uns den frommen S\u00e4nger,", "tokens": ["Wa\u00b7rum", "lie\u00df", "uns", "den", "from\u00b7men", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Gott, Deine Vaterhuld nicht l\u00e4nger!", "tokens": ["Gott", ",", "Dei\u00b7ne", "Va\u00b7ter\u00b7huld", "nicht", "l\u00e4n\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du, Dem nur seine Sayte klang,", "tokens": ["Du", ",", "Dem", "nur", "sei\u00b7ne", "Say\u00b7te", "klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "De\u00df Leben wie sein Lied Dich sang!", "tokens": ["De\u00df", "Le\u00b7ben", "wie", "sein", "Lied", "Dich", "sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch war es vielleicht seines Strebens", "tokens": ["Doch", "war", "es", "viel\u00b7leicht", "sei\u00b7nes", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Vater, l\u00e4ngst bekanntes Ziel!", "tokens": ["Dir", ",", "Va\u00b7ter", ",", "l\u00e4ngst", "be\u00b7kann\u00b7tes", "Ziel", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du fandest eines bessern Lebens", "tokens": ["Du", "fan\u00b7dest", "ei\u00b7nes", "bes\u00b7sern", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn werth, den Mann, der Dir gefiel!", "tokens": ["Ihn", "werth", ",", "den", "Mann", ",", "der", "Dir", "ge\u00b7fiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist gerecht! Doch unsre Klagen", "tokens": ["Du", "bist", "ge\u00b7recht", "!", "Doch", "uns\u00b7re", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kann auch Dein Vaterohr vertragen!", "tokens": ["Kann", "auch", "Dein", "Va\u00b7te\u00b7rohr", "ver\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist er nicht mancher Thr\u00e4ne werth,", "tokens": ["Ist", "er", "nicht", "man\u00b7cher", "Thr\u00e4\u00b7ne", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Der Mann, der Dich uns singen lehrt?", "tokens": ["Der", "Mann", ",", "der", "Dich", "uns", "sin\u00b7gen", "lehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Mann voll Redlichkeit, voll heller", "tokens": ["Der", "Mann", "voll", "Red\u00b7lich\u00b7keit", ",", "voll", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zufriedenheit und edler Ruh'!", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "und", "ed\u00b7ler", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer war zum Dienst des Bruders schneller?", "tokens": ["Wer", "war", "zum", "Dienst", "des", "Bru\u00b7ders", "schnel\u00b7ler", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sah' ihm ohne Liebe zu?", "tokens": ["Wer", "sah'", "ihm", "oh\u00b7ne", "Lie\u00b7be", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sanfte lehrt er milde Sitten,", "tokens": ["Wie", "sanf\u00b7te", "lehrt", "er", "mil\u00b7de", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und pflanzt in H\u00e4usern und in H\u00fctten", "tokens": ["Und", "pflanzt", "in", "H\u00e4u\u00b7sern", "und", "in", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Zweytracht und Unwissenheit", "tokens": ["Der", "Zweyt\u00b7racht", "und", "Un\u00b7wis\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-++-++-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Erkenntnis und Vertragsamkeit!", "tokens": ["Er\u00b7kennt\u00b7nis", "und", "Ver\u00b7tra\u00b7gsam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie warst du seine Freude, Jugend!", "tokens": ["Wie", "warst", "du", "sei\u00b7ne", "Freu\u00b7de", ",", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie laut schlug ihm sein Herz f\u00fcr dich!", "tokens": ["Wie", "laut", "schlug", "ihm", "sein", "Herz", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sanfte f\u00fchrt er dich zur Tugend,", "tokens": ["Wie", "sanf\u00b7te", "f\u00fchrt", "er", "dich", "zur", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie reizend und wie v\u00e4terlich!", "tokens": ["Wie", "rei\u00b7zend", "und", "wie", "v\u00e4\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nie mit der tr\u00fcben Schwehrmuth Stimme!", "tokens": ["Nie", "mit", "der", "tr\u00fc\u00b7ben", "Schwehr\u00b7muth", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Bitterkeit nie, nie im Grimme!", "tokens": ["Mit", "Bit\u00b7ter\u00b7keit", "nie", ",", "nie", "im", "Grim\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du mu\u00dftest dich der Tugend freu'n!", "tokens": ["Du", "mu\u00df\u00b7test", "dich", "der", "Tu\u00b7gend", "freu'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er sang sie dir in's Herz hinein!", "tokens": ["Er", "sang", "sie", "dir", "in's", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie edel war bey jedem Streite,", "tokens": ["Wie", "e\u00b7del", "war", "bey", "je\u00b7dem", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie fest sein Herz! Wie sanft sein Ton!", "tokens": ["Wie", "fest", "sein", "Herz", "!", "Wie", "sanft", "sein", "Ton", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stand er nicht stets auf deiner Seite,", "tokens": ["Stand", "er", "nicht", "stets", "auf", "dei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "O Wahrheit und Religion?", "tokens": ["O", "Wahr\u00b7heit", "und", "Re\u00b7li\u00b7gi\u00b7on", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die gute Sache rechnet' immer", "tokens": ["Die", "gu\u00b7te", "Sa\u00b7che", "rech\u00b7net'", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuerst auf ihn, betrog sich nimmer;", "tokens": ["Zu\u00b7erst", "auf", "ihn", ",", "be\u00b7trog", "sich", "nim\