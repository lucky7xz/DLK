 ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wahrheit troff von seinem Munde,", "tokens": ["Die", "Wahr\u00b7heit", "troff", "von", "sei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Balsam auf die offne Wunde", "tokens": ["Und", "Bal\u00b7sam", "auf", "die", "off\u00b7ne", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Reue, die den Stolz bezwang", "tokens": ["Der", "Reu\u00b7e", ",", "die", "den", "Stolz", "be\u00b7zwang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und d\u00fcrstend nur nach Gnade rang!", "tokens": ["Und", "d\u00fcrs\u00b7tend", "nur", "nach", "Gna\u00b7de", "rang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach! Noch, da ihm die Krankheit drohte,", "tokens": ["Ach", "!", "Noch", ",", "da", "ihm", "die", "Krank\u00b7heit", "droh\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch trat er auf die Kanzel hin \u2013", "tokens": ["Noch", "trat", "er", "auf", "die", "Kan\u00b7zel", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmein Leben\u00ab, rief, ach, der nun Todte,", "tokens": ["\u00bb", "mein", "Le\u00b7ben", "\u00ab", ",", "rief", ",", "ach", ",", "der", "nun", "Tod\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "$,", "VVFIN", "$,", "ITJ", "$,", "PRELS", "ADV", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00bbist Christus! Sterben mein Gewinn\u00ab!", "tokens": ["\u00bb", "ist", "Chris\u00b7tus", "!", "Ster\u00b7ben", "mein", "Ge\u00b7winn", "\u00ab", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "NE", "$.", "NN", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sprach's, stieg von der Kanzel nieder", "tokens": ["Er", "sprach's", ",", "stieg", "von", "der", "Kan\u00b7zel", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Todesahndung, Todeslieder", "tokens": ["Voll", "To\u00b7de\u00b7sahn\u00b7dung", ",", "To\u00b7des\u00b7lie\u00b7der"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch waren's, die er singen hie\u00df,", "tokens": ["Noch", "wa\u00b7ren's", ",", "die", "er", "sin\u00b7gen", "hie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Eh' er der Schaafe Schaar verlie\u00df.", "tokens": ["Eh'", "er", "der", "Schaa\u00b7fe", "Schaar", "ver\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Schon lag er an des Todes Pforte,", "tokens": ["Schon", "lag", "er", "an", "des", "To\u00b7des", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon zitterte sein kaltes Knie,", "tokens": ["Schon", "zit\u00b7ter\u00b7te", "sein", "kal\u00b7tes", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesang noch waren seine Worte,", "tokens": ["Ge\u00b7sang", "noch", "wa\u00b7ren", "sei\u00b7ne", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Gebehrden Harmonie.", "tokens": ["Und", "die", "Ge\u00b7behr\u00b7den", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, nun! Nun schwieg des S\u00e4ngers Ehre!", "tokens": ["Und", ",", "nun", "!", "Nun", "schwieg", "des", "S\u00e4n\u00b7gers", "Eh\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er schlummerte in jene Ch\u00f6re", "tokens": ["Er", "schlum\u00b7mer\u00b7te", "in", "je\u00b7ne", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Lichts hin\u00fcber. Jubel war", "tokens": ["Des", "Lichts", "hin\u00b7\u00fc\u00b7ber", ".", "Ju\u00b7bel", "war"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Geist bey der Verkl\u00e4rten Schaar!", "tokens": ["Der", "Geist", "bey", "der", "Ver\u00b7kl\u00e4r\u00b7ten", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Er starb, und Thr\u00e4nenstr\u00f6me flossen", "tokens": ["Er", "starb", ",", "und", "Thr\u00e4\u00b7nen\u00b7str\u00f6\u00b7me", "flos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem nun verstummten S\u00e4nger nach!", "tokens": ["Dem", "nun", "ver\u00b7stumm\u00b7ten", "S\u00e4n\u00b7ger", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer war vom Kleinen bis zum Gro\u00dfen", "tokens": ["Wer", "war", "vom", "Klei\u00b7nen", "bis", "zum", "Gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df Thr\u00e4ne nicht vom Todten sprach?", "tokens": ["De\u00df", "Thr\u00e4\u00b7ne", "nicht", "vom", "Tod\u00b7ten", "sprach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Greise weynen, Kinder sammeln,", "tokens": ["Die", "Grei\u00b7se", "wey\u00b7nen", ",", "Kin\u00b7der", "sam\u00b7meln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Trauerlieder nachzustammeln,", "tokens": ["Die", "Trau\u00b7er\u00b7lie\u00b7der", "nach\u00b7zu\u00b7stam\u00b7meln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um Greise sich und M\u00e4nner her ...", "tokens": ["Um", "Grei\u00b7se", "sich", "und", "M\u00e4n\u00b7ner", "her", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PRF", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jeder singt: \u00bbEr ist nicht mehr\u00ab!", "tokens": ["Und", "je\u00b7der", "singt", ":", "\u00bb", "Er", "ist", "nicht", "mehr", "\u00ab", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PTKNEG", "ADV", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ach! Nicht mehr, unser Vater, Lehrer,", "tokens": ["Ach", "!", "Nicht", "mehr", ",", "un\u00b7ser", "Va\u00b7ter", ",", "Leh\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PTKNEG", "ADV", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns empor vom Staube singt!", "tokens": ["Der", "uns", "em\u00b7por", "vom", "Stau\u00b7be", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gottheit kindlicher Verehrer,", "tokens": ["Der", "Got\u00b7theit", "kind\u00b7li\u00b7cher", "Ver\u00b7eh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den keine Klage wiederbringt!", "tokens": ["Den", "kei\u00b7ne", "Kla\u00b7ge", "wie\u00b7der\u00b7bringt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O! K\u00f6nnten wir Dich nun erblicken,", "tokens": ["O", "!", "K\u00f6nn\u00b7ten", "wir", "Dich", "nun", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich einmahl h\u00f6hren! In Entz\u00fccken", "tokens": ["Dich", "ein\u00b7mahl", "h\u00f6h\u00b7ren", "!", "In", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00e4m' uns auch nur ein Laut von Dir,", "tokens": ["K\u00e4m'", "uns", "auch", "nur", "ein", "Laut", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Wonne, wie zerfl\u00f6ssen wir!", "tokens": ["In", "Won\u00b7ne", ",", "wie", "zer\u00b7fl\u00f6s\u00b7sen", "wir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}