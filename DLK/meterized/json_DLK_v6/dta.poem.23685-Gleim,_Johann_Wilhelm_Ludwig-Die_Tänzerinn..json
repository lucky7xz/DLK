{"dta.poem.23685": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die T\u00e4nzerinn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "M&#228;dchen mit dem sch\u00f6nen Busen,", "tokens": ["M", "&#228;", "dchen", "mit", "dem", "sch\u00f6\u00b7nen", "Bu\u00b7sen", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitze nicht so fromm im Winkel!", "tokens": ["Sit\u00b7ze", "nicht", "so", "fromm", "im", "Win\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh die fr\u00f6lichen Brunetten", "tokens": ["Sieh", "die", "fr\u00f6\u00b7li\u00b7chen", "Bru\u00b7net\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Tanzen mit vergn\u00fcgten M\u00e4nnern!", "tokens": ["Tan\u00b7zen", "mit", "ver\u00b7gn\u00fcg\u00b7ten", "M\u00e4n\u00b7nern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh die T\u00e4nzer, sieh die F\u00fcsse", "tokens": ["Sieh", "die", "T\u00e4n\u00b7zer", ",", "sieh", "die", "F\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fliehen, wie die schnellen T\u00f6ne!", "tokens": ["Flie\u00b7hen", ",", "wie", "die", "schnel\u00b7len", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh wie fr\u00f6lich tanzt der Haufe!", "tokens": ["Sieh", "wie", "fr\u00f6\u00b7lich", "tanzt", "der", "Hau\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh, es trennet sich der Haufe!", "tokens": ["Sieh", ",", "es", "tren\u00b7net", "sich", "der", "Hau\u00b7fe", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sieh, das M\u00e4dchen tanzt ein Solo!", "tokens": ["Sieh", ",", "das", "M\u00e4d\u00b7chen", "tanzt", "ein", "So\u00b7lo", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Sieh, wie reitzend wirfts die F\u00fcsse!", "tokens": ["Sieh", ",", "wie", "reit\u00b7zend", "wirfts", "die", "F\u00fcs\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sieh, wie schnell kann es sie drehen!", "tokens": ["Sieh", ",", "wie", "schnell", "kann", "es", "sie", "dre\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sieh, sieh, sieh wie hoch es springet!", "tokens": ["Sieh", ",", "sieh", ",", "sieh", "wie", "hoch", "es", "sprin\u00b7get", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "KOKOM", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sieh, nun schleicht es mit den T\u00f6nen!", "tokens": ["Sieh", ",", "nun", "schleicht", "es", "mit", "den", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sieh, nun h\u00fcpft es mit den Saiten!", "tokens": ["Sieh", ",", "nun", "h\u00fcpft", "es", "mit", "den", "Sai\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sieh, nun dreht es sich im Zirkel,", "tokens": ["Sieh", ",", "nun", "dreht", "es", "sich", "im", "Zir\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sieh, wie langsam machts die R\u00fcnde!", "tokens": ["Sieh", ",", "wie", "lang\u00b7sam", "machts", "die", "R\u00fcn\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sieh, nun fliegt es aus dem Zirkel,", "tokens": ["Sieh", ",", "nun", "fliegt", "es", "aus", "dem", "Zir\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sieh, nun dreht sichs wieder langsam,", "tokens": ["Sieh", ",", "nun", "dreht", "sichs", "wie\u00b7der", "lang\u00b7sam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Als wenn ", "tokens": ["Als", "wenn"], "token_info": ["word", "word"], "pos": ["KOUS", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Leib und F\u00fc\u00df und H\u00e4nde z\u00f6ge.", "tokens": ["Leib", "und", "F\u00fc\u00df", "und", "H\u00e4n\u00b7de", "z\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sieh, nun schleicht, nun springt es wieder!", "tokens": ["Sieh", ",", "nun", "schleicht", ",", "nun", "springt", "es", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sahst du, wie es, unterm Springen,", "tokens": ["Sahst", "du", ",", "wie", "es", ",", "un\u00b7term", "Sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Schwebend mit den F\u00fcssen spielte?", "tokens": ["Schwe\u00b7bend", "mit", "den", "F\u00fcs\u00b7sen", "spiel\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Sieh, ietzt eben spielt es wieder!", "tokens": ["Sieh", ",", "ietzt", "e\u00b7ben", "spielt", "es", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "T\u00e4nzerinn, h\u00f6r auf zu tanzen;", "tokens": ["T\u00e4n\u00b7ze\u00b7rinn", ",", "h\u00f6r", "auf", "zu", "tan\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Denn indem ich deine F\u00fcsse", "tokens": ["Denn", "in\u00b7dem", "ich", "dei\u00b7ne", "F\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Springen, schleichen, spielen sehe,", "tokens": ["Sprin\u00b7gen", ",", "schlei\u00b7chen", ",", "spie\u00b7len", "se\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Seh ich keinen sch\u00f6nen Busen.", "tokens": ["Seh", "ich", "kei\u00b7nen", "sch\u00f6\u00b7nen", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}