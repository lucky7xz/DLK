{"dta.poem.9986": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alles wei\u00df ich ohne farb/ ohne pinsel/ vorzustellen/", "tokens": ["Al\u00b7les", "wei\u00df", "ich", "oh\u00b7ne", "farb", "/", "oh\u00b7ne", "pin\u00b7sel", "/", "vor\u00b7zu\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "$(", "APPR", "NN", "$(", "VVIZU", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Von der sch\u00f6n und he\u00dfligkeit urtheil/ das gerecht/ zu f\u00e4llen.", "tokens": ["Von", "der", "sch\u00f6n", "und", "he\u00df\u00b7lig\u00b7keit", "ur\u00b7theil", "/", "das", "ge\u00b7recht", "/", "zu", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "KON", "NN", "NE", "$(", "ART", "ADJD", "$(", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die natur hat nichts gemachet/ menschen list hat nichts erdacht/", "tokens": ["Die", "na\u00b7tur", "hat", "nichts", "ge\u00b7ma\u00b7chet", "/", "men\u00b7schen", "list", "hat", "nichts", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$(", "ADJA", "NN", "VAFIN", "PIS", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Was aus mir nicht wird lebendig und sehr k\u00fcnstlich vorge-", "tokens": ["Was", "aus", "mir", "nicht", "wird", "le\u00b7ben\u00b7dig", "und", "sehr", "k\u00fcnst\u00b7lich", "vor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "PTKNEG", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "bracht.", "tokens": ["bracht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}