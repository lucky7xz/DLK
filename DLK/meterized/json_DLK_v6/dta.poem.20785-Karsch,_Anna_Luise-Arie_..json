{"dta.poem.20785": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Arie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcgte Einsamkeit! du bist die Ruhe,               ", "tokens": ["Ver\u00b7gn\u00fcg\u00b7te", "Ein\u00b7sam\u00b7keit", "!", "du", "bist", "die", "Ru\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So meine stille Brust sich l\u00e4ngst erw\u00e4hlet,", "tokens": ["So", "mei\u00b7ne", "stil\u00b7le", "Brust", "sich", "l\u00e4ngst", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich hier unternehm, gedenk ", "tokens": ["Was", "ich", "hier", "un\u00b7ter\u00b7nehm", ",", "ge\u00b7denk"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "ADV", "ADJD", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wird der Weltcensur nicht aufgestellt;", "tokens": ["Das", "wird", "der", "Welt\u00b7cen\u00b7sur", "nicht", "auf\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bin ich gleich stets allein und ganz verborgen,", "tokens": ["Bin", "ich", "gleich", "stets", "al\u00b7lein", "und", "ganz", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So bleibt mein freier Sinn doch ungekr\u00e4nkt:", "tokens": ["So", "bleibt", "mein", "frei\u00b7er", "Sinn", "doch", "un\u00b7ge\u00b7kr\u00e4nkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich lebe h\u00f6chst content und ohne Sorgen,", "tokens": ["Ich", "le\u00b7be", "h\u00f6chst", "con\u00b7tent", "und", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil mir die Einsamkeit Vergn\u00fcgen schenkt.", "tokens": ["Weil", "mir", "die", "Ein\u00b7sam\u00b7keit", "Ver\u00b7gn\u00fc\u00b7gen", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es giebt verschiedene Art von Lustbarkeiten,", "tokens": ["Es", "giebt", "ver\u00b7schie\u00b7de\u00b7ne", "Art", "von", "Lust\u00b7bar\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So die galante Welt h\u00f6chst sch\u00e4tzbar preist;", "tokens": ["So", "die", "ga\u00b7lan\u00b7te", "Welt", "h\u00f6chst", "sch\u00e4tz\u00b7bar", "preist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn mans \u00fcberlegt sinds Eitelkeiten,", "tokens": ["Doch", "wenn", "mans", "\u00fc\u00b7ber\u00b7legt", "sinds", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVPP", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum sag ich noch einmal: mein freier Geist", "tokens": ["Drum", "sag", "ich", "noch", "ein\u00b7mal", ":", "mein", "frei\u00b7er", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ehrt mit gela\u00dfnem Muth die stillen Stunden,", "tokens": ["Ehrt", "mit", "ge\u00b7la\u00df\u00b7nem", "Muth", "die", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So das Verh\u00e4ngni\u00df mir hier zugez\u00e4hlt,", "tokens": ["So", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "mir", "hier", "zu\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es wird auch in der That sonst nichts gefunden,", "tokens": ["Es", "wird", "auch", "in", "der", "That", "sonst", "nichts", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das mehr Vergn\u00fcgen giebt und mir gef\u00e4llt.", "tokens": ["Das", "mehr", "Ver\u00b7gn\u00fc\u00b7gen", "giebt", "und", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So magst du denn o Welt, das Eitle loben,", "tokens": ["So", "magst", "du", "denn", "o", "Welt", ",", "das", "Eit\u00b7le", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "FM", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geh mache dir Pl\u00e4sir wie dirs beliebt,", "tokens": ["Geh", "ma\u00b7che", "dir", "Pl\u00e4\u00b7sir", "wie", "dirs", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "KOKOM", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist die gr\u00f6\u00dfte Lust noch aufgehoben,", "tokens": ["Mir", "ist", "die", "gr\u00f6\u00df\u00b7te", "Lust", "noch", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dort das h\u00f6chste Gut den Seelen giebt.", "tokens": ["Die", "dort", "das", "h\u00f6chs\u00b7te", "Gut", "den", "See\u00b7len", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach ich verlache nur das Weltget\u00fcmmel,", "tokens": ["Ach", "ich", "ver\u00b7la\u00b7che", "nur", "das", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Indem mein Herze sich die Losung setzt:", "tokens": ["In\u00b7dem", "mein", "Her\u00b7ze", "sich", "die", "Lo\u00b7sung", "setzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mein bester Theil mein Schatz ist noch im Himmel,", "tokens": ["Mein", "bes\u00b7ter", "Theil", "mein", "Schatz", "ist", "noch", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und hier ist Einsamkeit was mich erg\u00f6tzt.", "tokens": ["Und", "hier", "ist", "Ein\u00b7sam\u00b7keit", "was", "mich", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}