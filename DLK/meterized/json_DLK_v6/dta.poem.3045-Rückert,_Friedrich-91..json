{"dta.poem.3045": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Botaniker zugleich wer ist und Astronom,", "tokens": ["Bo\u00b7ta\u00b7ni\u00b7ker", "zu\u00b7gleich", "wer", "ist", "und", "Ast\u00b7ro\u00b7nom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PWS", "VAFIN", "KON", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Betrachtet wechselweis Erdflur und Himmelsdom.", "tokens": ["Be\u00b7trach\u00b7tet", "wech\u00b7sel\u00b7weis", "Erd\u00b7flur", "und", "Him\u00b7mels\u00b7dom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und eines w\u00fcrd' er oft beim andern gar vers\u00e4umen,", "tokens": ["Und", "ei\u00b7nes", "w\u00fcrd'", "er", "oft", "beim", "an\u00b7dern", "gar", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00fchte zu gleicher Zeit sein Flor in beiden R\u00e4umen.", "tokens": ["Bl\u00fch\u00b7te", "zu", "glei\u00b7cher", "Zeit", "sein", "Flor", "in", "bei\u00b7den", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Doch ihm zum Gl\u00fccke gehn die Stern' auf in der Nacht,", "tokens": ["Doch", "ihm", "zum", "Gl\u00fc\u00b7cke", "gehn", "die", "Stern'", "auf", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zu am Morgen, wann der Blumen Aug' erwacht.", "tokens": ["Und", "zu", "am", "Mor\u00b7gen", ",", "wann", "der", "Blu\u00b7men", "Aug'", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mir ist es nicht wie ihm geworden ganz so gut,", "tokens": ["Mir", "ist", "es", "nicht", "wie", "ihm", "ge\u00b7wor\u00b7den", "ganz", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "PWAV", "PPER", "VAPP", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wol mein Doppelkram einander Eintrag thut:", "tokens": ["Da", "wol", "mein", "Dop\u00b7pel\u00b7kram", "ein\u00b7an\u00b7der", "Ein\u00b7trag", "thut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Poetische Blumenles' und hohes Spekuliren,", "tokens": ["Poe\u00b7ti\u00b7sche", "Blu\u00b7men\u00b7les'", "und", "ho\u00b7hes", "Spe\u00b7ku\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von einem mu\u00df ich mich zum andern hin verlieren.", "tokens": ["Von", "ei\u00b7nem", "mu\u00df", "ich", "mich", "zum", "an\u00b7dern", "hin", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "PRF", "APPRART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das eine w\u00fcrd' ich denn verlieren \u00fcberm andern,", "tokens": ["Das", "ei\u00b7ne", "w\u00fcrd'", "ich", "denn", "ver\u00b7lie\u00b7ren", "\u00fc\u00b7berm", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "ADV", "VVINF", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich von diesem weit zu jenem m\u00fc\u00dfte wandern.", "tokens": ["Wenn", "ich", "von", "die\u00b7sem", "weit", "zu", "je\u00b7nem", "m\u00fc\u00df\u00b7te", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJD", "APPR", "PDAT", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Auskunft traf ich drum hier beides zu vereinen,", "tokens": ["Die", "Aus\u00b7kunft", "traf", "ich", "drum", "hier", "bei\u00b7des", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Stern' und Blumen durch einander bl\u00fchn im Kleinen.", "tokens": ["Wo", "Stern'", "und", "Blu\u00b7men", "durch", "ein\u00b7an\u00b7der", "bl\u00fchn", "im", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}