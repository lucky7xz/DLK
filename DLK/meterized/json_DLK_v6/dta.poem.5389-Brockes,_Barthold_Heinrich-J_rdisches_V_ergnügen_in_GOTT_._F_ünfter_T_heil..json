{"dta.poem.5389": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie w\u00fcnscht und hofft zugleich, sie werde,", "tokens": ["Sie", "w\u00fcnscht", "und", "hofft", "zu\u00b7gleich", ",", "sie", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschm\u00fcckt durch seiner Wercke Schein,", "tokens": ["Ge\u00b7schm\u00fcckt", "durch", "sei\u00b7ner", "Wer\u00b7cke", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Sch\u00f6pfer Himmels und der Erde", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "Him\u00b7mels", "und", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein nicht mi\u00dff\u00e4llig Opfer seyn!", "tokens": ["Ein", "nicht", "mi\u00df\u00b7f\u00e4l\u00b7lig", "Op\u00b7fer", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}