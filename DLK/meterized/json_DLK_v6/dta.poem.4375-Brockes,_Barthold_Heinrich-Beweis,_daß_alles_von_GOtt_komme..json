{"dta.poem.4375": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beweis, da\u00df alles von GOtt komme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df alle Theile unsers C\u00f6rpers nicht unsrer Seelen", "tokens": ["Da\u00df", "al\u00b7le", "Thei\u00b7le", "un\u00b7sers", "C\u00f6r\u00b7pers", "nicht", "uns\u00b7rer", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "unterthan,", "tokens": ["un\u00b7ter\u00b7than", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da sie so wenig Herz als Magen, die Adern, Lunge, Le-", "tokens": ["Da", "sie", "so", "we\u00b7nig", "Herz", "als", "Ma\u00b7gen", ",", "die", "A\u00b7dern", ",", "Lun\u00b7ge", ",", "Le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "KOUS", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "ber, Nieren,", "tokens": ["ber", ",", "Nie\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die Milz und noch viel andre St\u00fccke, durch Wollen und", "tokens": ["Die", "Milz", "und", "noch", "viel", "and\u00b7re", "St\u00fc\u00b7cke", ",", "durch", "Wol\u00b7len", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "PIAT", "ADJA", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Verstand, regieren,", "tokens": ["Ver\u00b7stand", ",", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Sie lenken, \u00e4ndern, bessern, f\u00fcgen, versehn und unter-", "tokens": ["Sie", "len\u00b7ken", ",", "\u00e4n\u00b7dern", ",", "bes\u00b7sern", ",", "f\u00fc\u00b7gen", ",", "ver\u00b7sehn", "und", "un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "halten kann,", "tokens": ["hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Zeigt unumst\u00f6\u00dflich diese Wahrheit dem, der es wohl er-", "tokens": ["Zeigt", "un\u00b7um\u00b7st\u00f6\u00df\u00b7lich", "die\u00b7se", "Wahr\u00b7heit", "dem", ",", "der", "es", "wohl", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PDAT", "NN", "ART", "$,", "PRELS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "weget, an:", "tokens": ["we\u00b7get", ",", "an", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Da\u00df wir von einem andern Wesen erkennen m\u00fcssen abzu-", "tokens": ["Da\u00df", "wir", "von", "ei\u00b7nem", "an\u00b7dern", "We\u00b7sen", "er\u00b7ken\u00b7nen", "m\u00fcs\u00b7sen", "ab\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "VMFIN", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "hangen;", "tokens": ["han\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Da\u00df unsre Seele sich den C\u00f6rper nicht selbst formirt, nicht", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "sich", "den", "C\u00f6r\u00b7per", "nicht", "selbst", "for\u00b7mirt", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "selbst gegeben,", "tokens": ["selbst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Da\u00df wir durch eines h\u00f6hern Wesens Macht, Weisheit,", "tokens": ["Da\u00df", "wir", "durch", "ei\u00b7nes", "h\u00f6\u00b7hern", "We\u00b7sens", "Macht", ",", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Lieb' und Willen leben,", "tokens": ["Lieb'", "und", "Wil\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Und da\u00df wir alles, was wir haben und seyn, allein von", "tokens": ["Und", "da\u00df", "wir", "al\u00b7les", ",", "was", "wir", "ha\u00b7ben", "und", "seyn", ",", "al\u00b7lein", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIS", "$,", "PRELS", "PPER", "VAFIN", "KON", "VAINF", "$,", "ADV", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Gott empfangen.", "tokens": ["Gott", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}