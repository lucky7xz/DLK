{"textgrid.poem.58796": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "15. Sei dankbar!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk, Seele: Was hat Gott getan,", "tokens": ["Denk", ",", "See\u00b7le", ":", "Was", "hat", "Gott", "ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWS", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er gab sein Allerliebstes dran!", "tokens": ["Er", "gab", "sein", "Al\u00b7ler\u00b7liebs\u00b7tes", "dran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du ", "tokens": ["Willst", "du"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "--", "measure": "unknown.measure.zero"}, "line.4": {"text": "So mag man wohl von Undank sagen.", "tokens": ["So", "mag", "man", "wohl", "von", "Un\u00b7dank", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}