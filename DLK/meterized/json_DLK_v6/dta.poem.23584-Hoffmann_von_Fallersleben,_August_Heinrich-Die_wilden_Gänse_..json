{"dta.poem.23584": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die wilden G\u00e4nse .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr wilden G\u00e4nse habt es gut,", "tokens": ["Ihr", "wil\u00b7den", "G\u00e4n\u00b7se", "habt", "es", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr ziehet frei und wohlgemuth", "tokens": ["Ihr", "zie\u00b7het", "frei", "und", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Strand zum andern Strand", "tokens": ["Von", "ei\u00b7nem", "Strand", "zum", "an\u00b7dern", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch's ganze liebe deutsche Land.", "tokens": ["Durch's", "gan\u00b7ze", "lie\u00b7be", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Uns zahmen Menschen geht's nicht so,", "tokens": ["Uns", "zah\u00b7men", "Men\u00b7schen", "geht's", "nicht", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir reisten gern' auch frei und froh", "tokens": ["Wir", "reis\u00b7ten", "gern'", "auch", "frei", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ununtersucht und unbekannt", "tokens": ["Un\u00b7un\u00b7ter\u00b7sucht", "und", "un\u00b7be\u00b7kannt"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch's ganze liebe deutsche Land.", "tokens": ["Durch's", "gan\u00b7ze", "lie\u00b7be", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaum sind wir aber fort von Haus,", "tokens": ["Kaum", "sind", "wir", "a\u00b7ber", "fort", "von", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mu\u00df auch schon der Pass heraus.", "tokens": ["So", "mu\u00df", "auch", "schon", "der", "Pass", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir werden niemals sorgenfrei", "tokens": ["Wir", "wer\u00b7den", "nie\u00b7mals", "sor\u00b7gen\u00b7frei"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor lauter Mauth und Polizei.", "tokens": ["Vor", "lau\u00b7ter", "Mauth", "und", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O da\u00df doch einer es erdenkt,", "tokens": ["O", "da\u00df", "doch", "ei\u00b7ner", "es", "er\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man den Luftball sicher lenkt!", "tokens": ["Wie", "man", "den", "Luft\u00b7ball", "si\u00b7cher", "lenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nur in den L\u00fcften sind wir frei.", "tokens": ["Nur", "in", "den", "L\u00fcf\u00b7ten", "sind", "wir", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}