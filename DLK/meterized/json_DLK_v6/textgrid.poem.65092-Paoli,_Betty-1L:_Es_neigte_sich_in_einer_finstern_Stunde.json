{"textgrid.poem.65092": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es neigte sich in einer finstern Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es neigte sich in einer finstern Stunde", "tokens": ["Es", "neig\u00b7te", "sich", "in", "ei\u00b7ner", "fins\u00b7tern", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Geist zu mir mit Worten des Propheten:", "tokens": ["Dein", "Geist", "zu", "mir", "mit", "Wor\u00b7ten", "des", "Pro\u00b7phe\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdie zu zerst\u00f6rt dich w\u00e4hnest und zertreten,", "tokens": ["\u00bb", "die", "zu", "zer\u00b7st\u00f6rt", "dich", "w\u00e4h\u00b7nest", "und", "zer\u00b7tre\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PTKZU", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sage dir: Erheb' dich und gesunde!", "tokens": ["Ich", "sa\u00b7ge", "dir", ":", "Er\u00b7heb'", "dich", "und", "ge\u00b7sun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das nied're Unkraut dir im Seelengrunde", "tokens": ["Das", "nie\u00b7d'\u00b7re", "Un\u00b7kraut", "dir", "im", "See\u00b7len\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wild aufgeschossen, trachte auszuj\u00e4ten!", "tokens": ["Wild", "auf\u00b7ge\u00b7schos\u00b7sen", ",", "trach\u00b7te", "aus\u00b7zu\u00b7j\u00e4\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Thaten lerne zu der Gottheit beten \u2013", "tokens": ["Durch", "Tha\u00b7ten", "ler\u00b7ne", "zu", "der", "Got\u00b7theit", "be\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Wirken liegt der Balsam jeder Wunde.\u00ab", "tokens": ["Im", "Wir\u00b7ken", "liegt", "der", "Bal\u00b7sam", "je\u00b7der", "Wun\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als ob das Schicksal mir durch dich geb\u00f6the,", "tokens": ["Als", "ob", "das", "Schick\u00b7sal", "mir", "durch", "dich", "ge\u00b7b\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gab willenlos dem Wort ich mich gefangen,", "tokens": ["Gab", "wil\u00b7len\u00b7los", "dem", "Wort", "ich", "mich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das \u00fcber Wunsch und Schmerzen mich erh\u00f6h'te.", "tokens": ["Das", "\u00fc\u00b7ber", "Wunsch", "und", "Schmer\u00b7zen", "mich", "er\u00b7h\u00f6h'\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Roth der Scham entglomm auf meinen Wangen,", "tokens": ["Und", "Roth", "der", "Scham", "ent\u00b7glomm", "auf", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gelobt sei Gott! es war die Morgenr\u00f6the,", "tokens": ["Ge\u00b7lobt", "sei", "Gott", "!", "es", "war", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die neuem Lebenstag vorangegangen!", "tokens": ["Die", "neu\u00b7em", "Le\u00b7bens\u00b7tag", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}