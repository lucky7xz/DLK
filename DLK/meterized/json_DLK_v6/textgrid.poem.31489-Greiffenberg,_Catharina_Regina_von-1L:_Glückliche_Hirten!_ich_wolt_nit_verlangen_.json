{"textgrid.poem.31489": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fcckliche Hirten! ich wolt nit verlangen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckliche Hirten! ich wolt nit verlangen /", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", "Hir\u00b7ten", "!", "ich", "wolt", "nit", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "K\u00f6nig' und F\u00fcrste stat euer zu seyn?", "tokens": ["K\u00f6\u00b7nig'", "und", "F\u00fcrs\u00b7te", "stat", "eu\u00b7er", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Tausendmal lieblicher f\u00fcnkert der schein /", "tokens": ["Tau\u00b7send\u00b7mal", "lieb\u00b7li\u00b7cher", "f\u00fcn\u00b7kert", "der", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "welcher von Engeln auf euch ist gegangen /", "tokens": ["wel\u00b7cher", "von", "En\u00b7geln", "auf", "euch", "ist", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "sch\u00f6ner / als Kronen / da K\u00f6nig mit prangen.", "tokens": ["sch\u00f6\u00b7ner", "/", "als", "Kro\u00b7nen", "/", "da", "K\u00f6\u00b7nig", "mit", "pran\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "KOUS", "NN", "$(", "KOUS", "NN", "APPR", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Herrlich verk\u00fcndigt die Himmels-Gemein /", "tokens": ["Herr\u00b7lich", "ver\u00b7k\u00fcn\u00b7digt", "die", "Him\u00b7mels\u00b7Ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "singen das Freuden-Lied kl\u00e4rlich und rein /", "tokens": ["sin\u00b7gen", "das", "Freu\u00b7den\u00b7Lied", "kl\u00e4r\u00b7lich", "und", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.8": {"text": "was ihr vor einen Christ-Helden empfangen.", "tokens": ["was", "ihr", "vor", "ei\u00b7nen", "Christ\u00b7Hel\u00b7den", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Engel sind froh /", "tokens": ["Die", "En\u00b7gel", "sind", "froh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "verlassen den Himmel /", "tokens": ["ver\u00b7las\u00b7sen", "den", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "und nehmen die Cimbel /", "tokens": ["und", "neh\u00b7men", "die", "Cim\u00b7bel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "sie gehen zum Stroh.", "tokens": ["sie", "ge\u00b7hen", "zum", "Stroh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Verlasset die Herden / seht Wunder mit Freuden:", "tokens": ["Ver\u00b7las\u00b7set", "die", "Her\u00b7den", "/", "seht", "Wun\u00b7der", "mit", "Freu\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "hinf\u00fcro wird Gottes Lamm selber euch weiden.", "tokens": ["hin\u00b7f\u00fc\u00b7ro", "wird", "Got\u00b7tes", "Lamm", "sel\u00b7ber", "euch", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "+---+-+---+-", "measure": "dactylic.init"}}}}}