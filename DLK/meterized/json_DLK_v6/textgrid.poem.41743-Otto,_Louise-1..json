{"textgrid.poem.41743": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still ist's im Dorf \u2013 der letzte Erntewagen", "tokens": ["Still", "ist's", "im", "Dorf", "\u2013", "der", "letz\u00b7te", "Ern\u00b7te\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er schwankte eben voll und schwer herein;", "tokens": ["Er", "schwank\u00b7te", "e\u00b7ben", "voll", "und", "schwer", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Abendglocken haben ausgeschlagen,", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "ha\u00b7ben", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne sank mit sanftem Purpurschein.", "tokens": ["Die", "Son\u00b7ne", "sank", "mit", "sanf\u00b7tem", "Pur\u00b7pur\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist ein Abend, recht wie ein Idyll,", "tokens": ["Es", "ist", "ein", "A\u00b7bend", ",", "recht", "wie", "ein", "I\u00b7dyll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wo in der weiten Runde Alles still,", "tokens": ["Wo", "in", "der", "wei\u00b7ten", "Run\u00b7de", "Al\u00b7les", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nur der Heimchen alte Fl\u00fcsterweisen", "tokens": ["Und", "nur", "der", "Heim\u00b7chen", "al\u00b7te", "Fl\u00fcs\u00b7ter\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Tag, der nun vollendet, selig preisen.", "tokens": ["Den", "Tag", ",", "der", "nun", "voll\u00b7en\u00b7det", ",", "se\u00b7lig", "prei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Mondessichel h\u00e4ngt am Firmamente,", "tokens": ["Die", "Mon\u00b7des\u00b7si\u00b7chel", "h\u00e4ngt", "am", "Fir\u00b7ma\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sterne wandeln den gewohnten Gang,", "tokens": ["Die", "Ster\u00b7ne", "wan\u00b7deln", "den", "ge\u00b7wohn\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie da man sehnend dorthin hob die H\u00e4nde,", "tokens": ["Wie", "da", "man", "seh\u00b7nend", "dor\u00b7thin", "hob", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und noch vom Mondschein blasse Lieder sang.", "tokens": ["Und", "noch", "vom", "Mond\u00b7schein", "blas\u00b7se", "Lie\u00b7der", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So steh ich einsam in des Gartens Ruh,", "tokens": ["So", "steh", "ich", "ein\u00b7sam", "in", "des", "Gar\u00b7tens", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Seh ruhig nur den bunten Blumen zu \u2013", "tokens": ["Seh", "ru\u00b7hig", "nur", "den", "bun\u00b7ten", "Blu\u00b7men", "zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Erinn'rung f\u00fchrt zu weit vergangnen Tagen,", "tokens": ["Er\u00b7inn'\u00b7rung", "f\u00fchrt", "zu", "weit", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Kindheit Buch liegt vor mir aufgeschlagen.", "tokens": ["Der", "Kind\u00b7heit", "Buch", "liegt", "vor", "mir", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Still ist's im Dorf \u2013 doch pl\u00f6tzlich welch Bewegen", "tokens": ["Still", "ist's", "im", "Dorf", "\u2013", "doch", "pl\u00f6tz\u00b7lich", "welch", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "$(", "ADV", "ADJD", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht durch die Luft, die still zu stehen schien?", "tokens": ["Geht", "durch", "die", "Luft", ",", "die", "still", "zu", "ste\u00b7hen", "schien", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will das Haupt dicht an die Erde legen,", "tokens": ["Ich", "will", "das", "Haupt", "dicht", "an", "die", "Er\u00b7de", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df in das Ohr des Schalles Wellen ziehn.", "tokens": ["Da\u00df", "in", "das", "Ohr", "des", "Schal\u00b7les", "Wel\u00b7len", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es klang wie Jagdruf und wie B\u00fcchsenknall,", "tokens": ["Es", "klang", "wie", "Jag\u00b7druf", "und", "wie", "B\u00fcch\u00b7sen\u00b7knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie tausendfacher Menschenstimmen Schall \u2013 \u2013", "tokens": ["Wie", "tau\u00b7send\u00b7fa\u00b7cher", "Men\u00b7schen\u00b7stim\u00b7men", "Schall", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht m\u00f6glich! \u2013 nein \u2013 ein Wahn hat mich beth\u00f6rt,", "tokens": ["Nicht", "m\u00f6g\u00b7lich", "!", "\u2013", "nein", "\u2013", "ein", "Wahn", "hat", "mich", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$.", "$(", "PTKANT", "$(", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie w\u00fcrde hier und jetzt ein Schu\u00df geh\u00f6rt?! \u2013", "tokens": ["Wie", "w\u00fcr\u00b7de", "hier", "und", "jetzt", "ein", "Schu\u00df", "ge\u00b7h\u00f6rt", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}