{"textgrid.poem.36134": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonn' ist hin \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn' ist hin \u2013", "tokens": ["Die", "Sonn'", "ist", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Welch' D\u00fcsterni\u00df umschwebt mir den verst\u00f6rten Sinn!", "tokens": ["Welch'", "D\u00fcs\u00b7ter\u00b7ni\u00df", "um\u00b7schwebt", "mir", "den", "ver\u00b7st\u00f6r\u00b7ten", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die kalte Flur umsitzt die Nacht,", "tokens": ["Die", "kal\u00b7te", "Flur", "um\u00b7sitzt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch in meiner Brust hat's ausgetagt.", "tokens": ["Und", "auch", "in", "mei\u00b7ner", "Brust", "hat's", "aus\u00b7ge\u00b7tagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Freund ist hin \u2013", "tokens": ["Mein", "Freund", "ist", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Welch' D\u00fcsterni\u00df umschwebt mir den verst\u00f6rten Sinn!", "tokens": ["Welch'", "D\u00fcs\u00b7ter\u00b7ni\u00df", "um\u00b7schwebt", "mir", "den", "ver\u00b7st\u00f6r\u00b7ten", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Jonathan ist fort,", "tokens": ["Mein", "Jo\u00b7na\u00b7than", "ist", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mit ihm floh die Lust aus meinem Herzen fort \u2013", "tokens": ["Und", "mit", "ihm", "floh", "die", "Lust", "aus", "mei\u00b7nem", "Her\u00b7zen", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schweren, dumpfen, hohlen Klangs", "tokens": ["Schwe\u00b7ren", ",", "dum\u00b7pfen", ",", "hoh\u00b7len", "Klangs"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hallete des Abschieds d\u00fcstre Stunde,", "tokens": ["Hal\u00b7le\u00b7te", "des", "Ab\u00b7schieds", "d\u00fcst\u00b7re", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie aus heis'rer Unken Munde", "tokens": ["Wie", "aus", "heis'\u00b7rer", "Un\u00b7ken", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Stimme des Wehegesangs.", "tokens": ["Die", "Stim\u00b7me", "des", "We\u00b7he\u00b7ge\u00b7sangs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Weh! \u2013 Weh!", "tokens": ["Weh", "!", "\u2013", "Weh", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Sie hallet, der Stunden d\u00fcsterste.", "tokens": ["Sie", "hal\u00b7let", ",", "der", "Stun\u00b7den", "d\u00fcs\u00b7ters\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun kommt er, langsam, finster, beklemmt,", "tokens": ["Nun", "kommt", "er", ",", "lang\u00b7sam", ",", "fins\u00b7ter", ",", "be\u00b7klemmt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie Hagelgew\u00f6lk im Herbste k\u00f6mmt.", "tokens": ["Wie", "Ha\u00b7gel\u00b7ge\u00b7w\u00f6lk", "im", "Herbs\u00b7te", "k\u00f6mmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und es wird feierlich stille,", "tokens": ["Und", "es", "wird", "fei\u00b7er\u00b7lich", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "So wie die Sch\u00f6pfung vor kommendem Donner wird \u2013", "tokens": ["So", "wie", "die", "Sch\u00f6p\u00b7fung", "vor", "kom\u00b7men\u00b7dem", "Don\u00b7ner", "wird", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Schweigens finstre H\u00fclle", "tokens": ["Des", "Schwei\u00b7gens", "finst\u00b7re", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umgiebet ihn. Sein dunkles Auge irrt.", "tokens": ["Um\u00b7gie\u00b7bet", "ihn", ".", "Sein", "dunk\u00b7les", "Au\u00b7ge", "irrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dem starren Mund enthallte", "tokens": ["Dem", "star\u00b7ren", "Mund", "ent\u00b7hall\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Lebewohl!", "tokens": ["Kein", "Le\u00b7be\u00b7wohl", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem starren Aug' entwallte", "tokens": ["Dem", "star\u00b7ren", "Aug'", "ent\u00b7wall\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Thr\u00e4nchen \u2013 Schwermuthvoll", "tokens": ["Kein", "Thr\u00e4n\u00b7chen", "\u2013", "Schwer\u00b7muth\u00b7voll"], "token_info": ["word", "word", "punct", "word"], "pos": ["PIAT", "NN", "$(", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schweigend sah' auf ihn der Freunde Antlitz nieder \u2013", "tokens": ["Und", "schwei\u00b7gend", "sah'", "auf", "ihn", "der", "Freun\u00b7de", "Ant\u00b7litz", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er naht sich \u2013 bebet und \u2013 geht,", "tokens": ["Er", "naht", "sich", "\u2013", "be\u00b7bet", "und", "\u2013", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "VVFIN", "KON", "$(", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und \u2013 bebt noch mehr, und \u2013 kehret wieder,", "tokens": ["Und", "\u2013", "bebt", "noch", "mehr", ",", "und", "\u2013", "keh\u00b7ret", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ADV", "ADV", "$,", "KON", "$(", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u2013 strecket die Arme \u2013 und geht. \u2013 \u2013 \u2013", "tokens": ["Und", "\u2013", "stre\u00b7cket", "die", "Ar\u00b7me", "\u2013", "und", "geht", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "$(", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "$.", "$(", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Weh! \u2013 Weh!", "tokens": ["Weh", "!", "\u2013", "Weh", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Er geht, der Gute, der Redliche! \u2013", "tokens": ["Er", "geht", ",", "der", "Gu\u00b7te", ",", "der", "Red\u00b7li\u00b7che", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da quoll die Z\u00e4hre der Edlen", "tokens": ["Da", "quoll", "die", "Z\u00e4h\u00b7re", "der", "Ed\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Auge des lieblichen M\u00e4dchens", "tokens": ["Dem", "Au\u00b7ge", "des", "lieb\u00b7li\u00b7chen", "M\u00e4d\u00b7chens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Entquoll der Z\u00e4hren K\u00f6stlichste.", "tokens": ["Ent\u00b7quoll", "der", "Z\u00e4h\u00b7ren", "K\u00f6st\u00b7lichs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$."], "meter": "---+-+--", "measure": "unknown.measure.di"}}, "stanza.9": {"line.1": {"text": "Ein Seraph fa\u00dfte die Z\u00e4hre,", "tokens": ["Ein", "Se\u00b7raph", "fa\u00df\u00b7te", "die", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wandelte sie in Rubin,", "tokens": ["Wan\u00b7del\u00b7te", "sie", "in", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Um zu des M\u00e4dchens Ehre", "tokens": ["Um", "zu", "des", "M\u00e4d\u00b7chens", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00fcnftig in ihrem Diadem zu gl\u00fchn.", "tokens": ["K\u00fcnf\u00b7tig", "in", "ih\u00b7rem", "Di\u00b7a\u00b7dem", "zu", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Und fl\u00f6ss' um mich des M\u00e4dchens Z\u00e4hre,", "tokens": ["Und", "fl\u00f6ss'", "um", "mich", "des", "M\u00e4d\u00b7chens", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weinte sie um mich ihr helles Auge roth \u2013", "tokens": ["Wein\u00b7te", "sie", "um", "mich", "ihr", "hel\u00b7les", "Au\u00b7ge", "roth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ich st\u00fcrzte durch Eisgebirge, durch uferlose Meere,", "tokens": ["Ich", "st\u00fcrz\u00b7te", "durch", "Eis\u00b7ge\u00b7bir\u00b7ge", ",", "durch", "u\u00b7fer\u00b7lo\u00b7se", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Durch Schwert und Flammen in deine kalten Arme, Tod!!", "tokens": ["Durch", "Schwert", "und", "Flam\u00b7men", "in", "dei\u00b7ne", "kal\u00b7ten", "Ar\u00b7me", ",", "Tod", "!!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}