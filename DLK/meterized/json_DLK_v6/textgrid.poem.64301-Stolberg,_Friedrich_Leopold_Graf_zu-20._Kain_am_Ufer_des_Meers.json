{"textgrid.poem.64301": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "20. Kain am Ufer des Meers", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehe, wehe mir! Wohin", "tokens": ["We\u00b7he", ",", "we\u00b7he", "mir", "!", "Wo\u00b7hin"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$.", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt mich mein geschlagner Sinn?", "tokens": ["Treibt", "mich", "mein", "ge\u00b7schlag\u00b7ner", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Str\u00f6me brausen her!", "tokens": ["Got\u00b7tes", "Str\u00f6\u00b7me", "brau\u00b7sen", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abels Blut! es ist das Meer!", "tokens": ["A\u00b7bels", "Blut", "!", "es", "ist", "das", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bis zur Erde letztem Rand", "tokens": ["Bis", "zur", "Er\u00b7de", "letz\u00b7tem", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Rache mich gebannt!", "tokens": ["Hat", "die", "Ra\u00b7che", "mich", "ge\u00b7bannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo kein Jammer noch geklagt,", "tokens": ["Wo", "kein", "Jam\u00b7mer", "noch", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mich Abels Blut gejagt!", "tokens": ["Hat", "mich", "A\u00b7bels", "Blut", "ge\u00b7jagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wehe mir! des Bruders Blut", "tokens": ["We\u00b7he", "mir", "!", "des", "Bru\u00b7ders", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnert in der wilden Flut!", "tokens": ["Don\u00b7nert", "in", "der", "wil\u00b7den", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Felsenufers Schall!", "tokens": ["In", "des", "Fel\u00b7se\u00b7nu\u00b7fers", "Schall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Grotten Wiederhall!", "tokens": ["In", "der", "Grot\u00b7ten", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie den Stein das Meer umfleu\u00dft,", "tokens": ["Wie", "den", "Stein", "das", "Meer", "um\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So umstr\u00f6men meinen Geist", "tokens": ["So", "um\u00b7str\u00f6\u00b7men", "mei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seelenangst und Qual und Wut,", "tokens": ["See\u00b7len\u00b7angst", "und", "Qual", "und", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Schrecken! Abels Blut!", "tokens": ["Got\u00b7tes", "Schre\u00b7cken", "!", "A\u00b7bels", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00d6ffnet, Wogen, euren Schlund,", "tokens": ["\u00d6ff\u00b7net", ",", "Wo\u00b7gen", ",", "eu\u00b7ren", "Schlund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! der Muttererde Mund", "tokens": ["Ach", "!", "der", "Mut\u00b7ter\u00b7er\u00b7de", "Mund"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trank sein Blut, da ich ihn schlug,", "tokens": ["Trank", "sein", "Blut", ",", "da", "ich", "ihn", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vernahm des R\u00e4chers Fluch!", "tokens": ["Und", "ver\u00b7nahm", "des", "R\u00e4\u00b7chers", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00d6ffnet, Wogen, euren Schlund,", "tokens": ["\u00d6ff\u00b7net", ",", "Wo\u00b7gen", ",", "eu\u00b7ren", "Schlund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und enth\u00fcllet euren Grund!", "tokens": ["Und", "ent\u00b7h\u00fcl\u00b7let", "eu\u00b7ren", "Grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach umsonst! Die Rache wacht", "tokens": ["Ach", "um\u00b7sonst", "!", "Die", "Ra\u00b7che", "wacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch im Scho\u00df der alten Nacht!", "tokens": ["Auch", "im", "Scho\u00df", "der", "al\u00b7ten", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In der tiefsten Tiefe Graun", "tokens": ["In", "der", "tiefs\u00b7ten", "Tie\u00b7fe", "Graun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "W\u00fcrd' ich Abels Schatten schaun!", "tokens": ["W\u00fcrd'", "ich", "A\u00b7bels", "Schat\u00b7ten", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd' ihn schauen, ob ich fl\u00f6h'", "tokens": ["W\u00fcrd'", "ihn", "schau\u00b7en", ",", "ob", "ich", "fl\u00f6h'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des h\u00f6chsten Berges H\u00f6h!", "tokens": ["Auf", "des", "h\u00f6chs\u00b7ten", "Ber\u00b7ges", "H\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "W\u00fcrde dieses Leibes Staub", "tokens": ["W\u00fcr\u00b7de", "die\u00b7ses", "Lei\u00b7bes", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Wirbelst\u00fcrme Raub,", "tokens": ["Al\u00b7ler", "Wir\u00b7bel\u00b7st\u00fcr\u00b7me", "Raub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so schaute Kain doch", "tokens": ["O", "so", "schau\u00b7te", "Kain", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gottes Feuereifer noch!", "tokens": ["Got\u00b7tes", "Feu\u00b7e\u00b7rei\u00b7fer", "noch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ohne Ma\u00df und ohne Zahl", "tokens": ["Oh\u00b7ne", "Ma\u00df", "und", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fctet meiner Seele Qual,", "tokens": ["W\u00fc\u00b7tet", "mei\u00b7ner", "See\u00b7le", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Grenzen ferner Zeit,", "tokens": ["Oh\u00b7ne", "Gren\u00b7zen", "fer\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4hrt in alle Ewigkeit!", "tokens": ["W\u00e4hrt", "in", "al\u00b7le", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn mich traf des R\u00e4chers Fluch,", "tokens": ["Denn", "mich", "traf", "des", "R\u00e4\u00b7chers", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ich meinen Bruder schlug!", "tokens": ["Da", "ich", "mei\u00b7nen", "Bru\u00b7der", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehe! wehe! wehe mir!", "tokens": ["We\u00b7he", "!", "we\u00b7he", "!", "we\u00b7he", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abels Schatten folget mir!", "tokens": ["A\u00b7bels", "Schat\u00b7ten", "fol\u00b7get", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}