{"dta.poem.3027": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6tter nahen gern dem Menschenaufenthalt,", "tokens": ["Die", "G\u00f6t\u00b7ter", "na\u00b7hen", "gern", "dem", "Men\u00b7schen\u00b7au\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stellen uns sich dar in menschlicher Gestalt.", "tokens": ["Und", "stel\u00b7len", "uns", "sich", "dar", "in", "menschli\u00b7cher", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Doch k\u00f6nnen sie so ganz den Menschen niemals gleichen,", "tokens": ["Doch", "k\u00f6n\u00b7nen", "sie", "so", "ganz", "den", "Men\u00b7schen", "nie\u00b7mals", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht von G\u00f6ttlichkeit an ihnen blieb' ein Zeichen.", "tokens": ["Da\u00df", "nicht", "von", "G\u00f6tt\u00b7lich\u00b7keit", "an", "ih\u00b7nen", "blieb'", "ein", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie tragen eine Spur von g\u00f6ttlicher Natur,", "tokens": ["Sie", "tra\u00b7gen", "ei\u00b7ne", "Spur", "von", "g\u00f6tt\u00b7li\u00b7cher", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch dem geweihten Aug' erkennbar ist sie nur.", "tokens": ["Doch", "dem", "ge\u00b7weih\u00b7ten", "Aug'", "er\u00b7kenn\u00b7bar", "ist", "sie", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn nicht sichtbar beim Erscheinen auch ihr Zeichen", "tokens": ["Und", "wenn", "nicht", "sicht\u00b7bar", "beim", "Er\u00b7schei\u00b7nen", "auch", "ihr", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ADJD", "APPRART", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Auge ward, so wird es sichtbar beim Entweichen.", "tokens": ["Dem", "Au\u00b7ge", "ward", ",", "so", "wird", "es", "sicht\u00b7bar", "beim", "Ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wer ihr Zeichen selbst nicht sp\u00fcrt mit dumpfem Sinne,", "tokens": ["Und", "wer", "ihr", "Zei\u00b7chen", "selbst", "nicht", "sp\u00fcrt", "mit", "dum\u00b7pfem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird doch die G\u00f6ttern\u00e4h' an einem Schauder inne.", "tokens": ["Wird", "doch", "die", "G\u00f6t\u00b7ter\u00b7n\u00e4h'", "an", "ei\u00b7nem", "Schau\u00b7der", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}