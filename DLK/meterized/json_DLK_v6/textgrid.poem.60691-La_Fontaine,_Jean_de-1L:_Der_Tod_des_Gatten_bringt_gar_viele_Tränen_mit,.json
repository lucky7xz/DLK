{"textgrid.poem.60691": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Tod des Gatten bringt gar viele Tr\u00e4nen mit,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod des Gatten bringt gar viele Tr\u00e4nen mit,", "tokens": ["Der", "Tod", "des", "Gat\u00b7ten", "bringt", "gar", "vie\u00b7le", "Tr\u00e4\u00b7nen", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man macht ein gro\u00df Geschrei und tr\u00f6stet sich am Ende,", "tokens": ["Man", "macht", "ein", "gro\u00df", "Ge\u00b7schrei", "und", "tr\u00f6s\u00b7tet", "sich", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJD", "NN", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und mit der Zeit vergi\u00dft", "tokens": ["Und", "mit", "der", "Zeit", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Man alles, was man litt,", "tokens": ["Man", "al\u00b7les", ",", "was", "man", "litt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und reicht der Zukunft froh die H\u00e4nde.", "tokens": ["Und", "reicht", "der", "Zu\u00b7kunft", "froh", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Witwe, die ein Jahr lang Witwe ist,", "tokens": ["Die", "Wit\u00b7we", ",", "die", "ein", "Jahr", "lang", "Wit\u00b7we", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jene, die erst Witwe einen Tag,", "tokens": ["Und", "je\u00b7ne", ",", "die", "erst", "Wit\u00b7we", "ei\u00b7nen", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sind so verschieden, da\u00df man zweifeln mag,", "tokens": ["Sind", "so", "ver\u00b7schie\u00b7den", ",", "da\u00df", "man", "zwei\u00b7feln", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ob beides denn dieselbe Frau.", "tokens": ["Ob", "bei\u00b7des", "denn", "die\u00b7sel\u00b7be", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die trieb die M\u00e4nner fort, und diese lockt sie schlau;", "tokens": ["Die", "trieb", "die", "M\u00e4n\u00b7ner", "fort", ",", "und", "die\u00b7se", "lockt", "sie", "schlau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur Seufzer wu\u00dfte jene auszuschicken,", "tokens": ["Nur", "Seuf\u00b7zer", "wu\u00df\u00b7te", "je\u00b7ne", "aus\u00b7zu\u00b7schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PDS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die andere versteht es, jeden zu ber\u00fccken.", "tokens": ["Die", "an\u00b7de\u00b7re", "ver\u00b7steht", "es", ",", "je\u00b7den", "zu", "be\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja, immer ist die Sache einerlei:", "tokens": ["Ja", ",", "im\u00b7mer", "ist", "die", "Sa\u00b7che", "ei\u00b7ner\u00b7lei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Man sagt, da\u00df man untr\u00f6stlich sei \u2013", "tokens": ["Man", "sagt", ",", "da\u00df", "man", "un\u00b7tr\u00f6st\u00b7lich", "sei", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Man sagt's, jedoch man ist es nicht.", "tokens": ["Man", "sagt's", ",", "je\u00b7doch", "man", "ist", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "PIS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das zeigt uns folgender Bericht,", "tokens": ["Das", "zeigt", "uns", "fol\u00b7gen\u00b7der", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der ganz der Wirklichkeit entspricht.", "tokens": ["Der", "ganz", "der", "Wirk\u00b7lich\u00b7keit", "ent\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Gatte einer jungen Sch\u00f6nen", "tokens": ["Der", "Gat\u00b7te", "ei\u00b7ner", "jun\u00b7gen", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schied hin von dieser Welt; in herzbewegten T\u00f6nen", "tokens": ["Schied", "hin", "von", "die\u00b7ser", "Welt", ";", "in", "herz\u00b7be\u00b7weg\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Rief seine Frau: \u00bbIch werde mit dir ziehen,", "tokens": ["Rief", "sei\u00b7ne", "Frau", ":", "\u00bb", "Ich", "wer\u00b7de", "mit", "dir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch meine Seele will von hinnen fliehen,", "tokens": ["Auch", "mei\u00b7ne", "See\u00b7le", "will", "von", "hin\u00b7nen", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um dir zu folgen; Liebster, warte mein!\u00ab", "tokens": ["Um", "dir", "zu", "fol\u00b7gen", ";", "Liebs\u00b7ter", ",", "war\u00b7te", "mein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$.", "NN", "$,", "VVFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Gatte tat die Reise doch allein.", "tokens": ["Der", "Gat\u00b7te", "tat", "die", "Rei\u00b7se", "doch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Sch\u00f6nen Vater war ein kluger Mann,", "tokens": ["Der", "Sch\u00f6\u00b7nen", "Va\u00b7ter", "war", "ein", "klu\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er sah zun\u00e4chst den Sturm mit an,", "tokens": ["Er", "sah", "zu\u00b7n\u00e4chst", "den", "Sturm", "mit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Worauf er tr\u00f6stend dann begann:", "tokens": ["Wo\u00b7rauf", "er", "tr\u00f6s\u00b7tend", "dann", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbmein Kind, was sollen deine Tr\u00e4nen?", "tokens": ["\u00bb", "mein", "Kind", ",", "was", "sol\u00b7len", "dei\u00b7ne", "Tr\u00e4\u00b7nen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Kannst du den Toten so tyrannisch w\u00e4hnen,", "tokens": ["Kannst", "du", "den", "To\u00b7ten", "so", "ty\u00b7ran\u00b7nisch", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df ihn danach verlangt, da\u00df deine Reize schwinden?", "tokens": ["Da\u00df", "ihn", "da\u00b7nach", "ver\u00b7langt", ",", "da\u00df", "dei\u00b7ne", "Rei\u00b7ze", "schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Noch gibt es Lebende, la\u00df tot den Toten sein!", "tokens": ["Noch", "gibt", "es", "Le\u00b7ben\u00b7de", ",", "la\u00df", "tot", "den", "To\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "VVIMP", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Du k\u00f6nntest deine gro\u00dfe Pein", "tokens": ["Du", "k\u00f6nn\u00b7test", "dei\u00b7ne", "gro\u00b7\u00dfe", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit mehr Beherrschung leicht verwinden;", "tokens": ["Mit", "mehr", "Be\u00b7herr\u00b7schung", "leicht", "ver\u00b7win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Denn unschwer lie\u00dfe sich ein Gatte finden,", "tokens": ["Denn", "un\u00b7schwer", "lie\u00b7\u00dfe", "sich", "ein", "Gat\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der jung und stattlich anzuschauen \u2013\u00ab", "tokens": ["Der", "jung", "und", "statt\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVIZU", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbach la\u00df!\u00ab versetzte sie gequ\u00e4lt,", "tokens": ["\u00bb", "ach", "la\u00df", "!", "\u00ab", "ver\u00b7setz\u00b7te", "sie", "ge\u00b7qu\u00e4lt", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "XY", "$.", "$(", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbdas Kloster ist der Gatte, der mir fehlt.\u00ab", "tokens": ["\u00bb", "das", "Klos\u00b7ter", "ist", "der", "Gat\u00b7te", ",", "der", "mir", "fehlt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der Vater lie\u00df sie ihren Schmerz verdauen.", "tokens": ["Der", "Va\u00b7ter", "lie\u00df", "sie", "ih\u00b7ren", "Schmerz", "ver\u00b7dau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Mond geht hin. Im n\u00e4chsten schon gew\u00f6hnt man sie,", "tokens": ["Ein", "Mond", "geht", "hin", ".", "Im", "n\u00e4chs\u00b7ten", "schon", "ge\u00b7w\u00f6hnt", "man", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "APPRART", "ADJA", "ADV", "VVPP", "PIS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das Kleid, die W\u00e4sche, die Frisur zu \u00e4ndern.", "tokens": ["Das", "Kleid", ",", "die", "W\u00e4\u00b7sche", ",", "die", "Fri\u00b7sur", "zu", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Trauerkleid mit Schleifen und mit B\u00e4ndern", "tokens": ["Das", "Trau\u00b7er\u00b7kleid", "mit", "Schlei\u00b7fen", "und", "mit", "B\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat eine eigne zarte Poesie.", "tokens": ["Hat", "ei\u00b7ne", "eig\u00b7ne", "zar\u00b7te", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Waffen der Koketterie,", "tokens": ["Und", "al\u00b7le", "Waf\u00b7fen", "der", "Ko\u00b7ket\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Augenspiel, das L\u00e4cheln, selbst den Tanz", "tokens": ["Das", "Au\u00b7gen\u00b7spiel", ",", "das", "L\u00e4\u00b7cheln", ",", "selbst", "den", "Tanz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nimmt sie zur Hand und badet sich im Perlenglanz,", "tokens": ["Nimmt", "sie", "zur", "Hand", "und", "ba\u00b7det", "sich", "im", "Per\u00b7len\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den ihr des Jungbrunns Flut ins Antlitz spr\u00fcht.", "tokens": ["Den", "ihr", "des", "Jung\u00b7brunns", "Flut", "ins", "Ant\u00b7litz", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Vater sieht nicht l\u00e4nger sich bem\u00fcht;", "tokens": ["Der", "Va\u00b7ter", "sieht", "nicht", "l\u00e4n\u00b7ger", "sich", "be\u00b7m\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch da er unsrer Sch\u00f6nen nichts mehr sagt,", "tokens": ["Doch", "da", "er", "uns\u00b7rer", "Sch\u00f6\u00b7nen", "nichts", "mehr", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "