{"textgrid.poem.66899": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: In ihrem Blut rollt ein geheimer Tropfen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ihrem Blut rollt ein geheimer Tropfen", "tokens": ["In", "ih\u00b7rem", "Blut", "rollt", "ein", "ge\u00b7hei\u00b7mer", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstaunlicher, durch nichts gest\u00f6rter Kraft.", "tokens": ["Er\u00b7staun\u00b7li\u00b7cher", ",", "durch", "nichts", "ge\u00b7st\u00f6r\u00b7ter", "Kraft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Halb scheint vom Satan, halb von Gott der Saft,", "tokens": ["Halb", "scheint", "vom", "Sa\u00b7tan", ",", "halb", "von", "Gott", "der", "Saft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "$,", "ADJD", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gemischt, um jede Wunde zu verstopfen.", "tokens": ["Ge\u00b7mischt", ",", "um", "je\u00b7de", "Wun\u00b7de", "zu", "ver\u00b7stop\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo hundertmal verloren Malz und Hopfen", "tokens": ["Wo", "hun\u00b7dert\u00b7mal", "ver\u00b7lo\u00b7ren", "Malz", "und", "Hop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVPP", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei andern K\u00e4mpfern, wenn der Abgrund klafft,", "tokens": ["Bei", "an\u00b7dern", "K\u00e4mp\u00b7fern", ",", "wenn", "der", "Ab\u00b7grund", "klafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der dunkelg\u00e4hnend seine Opfer rafft,", "tokens": ["Der", "dun\u00b7kel\u00b7g\u00e4h\u00b7nend", "sei\u00b7ne", "Op\u00b7fer", "rafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie setzen \u00fcber. Siegerpulse klopfen.", "tokens": ["Sie", "set\u00b7zen", "\u00fc\u00b7ber", ".", "Sie\u00b7ger\u00b7pul\u00b7se", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In ihrem Blick loht ein geheimes Feuer,", "tokens": ["In", "ih\u00b7rem", "Blick", "loht", "ein", "ge\u00b7hei\u00b7mes", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Halb scheint von Gott, vom Satan halb die Glut,", "tokens": ["Halb", "scheint", "von", "Gott", ",", "vom", "Sa\u00b7tan", "halb", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$,", "APPRART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So b\u00f6s wie gut \u2013 dem Schwachen nicht geheuer.", "tokens": ["So", "b\u00f6s", "wie", "gut", "\u2013", "dem", "Schwa\u00b7chen", "nicht", "ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$(", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vor ihrem Schritt verreckt die Otternbrut.", "tokens": ["Vor", "ih\u00b7rem", "Schritt", "ver\u00b7reckt", "die", "Ot\u00b7tern\u00b7brut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das giftgeschw\u00e4ngert zischende Gewimmel", "tokens": ["Das", "gift\u00b7ge\u00b7schw\u00e4n\u00b7gert", "zi\u00b7schen\u00b7de", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sinkt in den Sumpf. Sie rettet H\u00f6ll' und Himmel.", "tokens": ["Sinkt", "in", "den", "Sumpf", ".", "Sie", "ret\u00b7tet", "H\u00f6ll'", "und", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}