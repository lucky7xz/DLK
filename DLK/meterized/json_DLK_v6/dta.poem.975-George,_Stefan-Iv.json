{"dta.poem.975": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Iv", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Erwachen aus dem tiefsten traumesschoosse                 ", "tokens": ["Er\u00b7wa\u00b7chen", "aus", "dem", "tiefs\u00b7ten", "trau\u00b7mes\u00b7schoos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich von langer spiegelung betroffen                 ", "tokens": ["Als", "ich", "von", "lan\u00b7ger", "spie\u00b7ge\u00b7lung", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich neigte auf die lippen die erblichen               ", "tokens": ["Mich", "neig\u00b7te", "auf", "die", "lip\u00b7pen", "die", "er\u00b7bli\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ertragen sollet ihr nur mitleidgrosse!                 ", "tokens": ["Er\u00b7tra\u00b7gen", "sol\u00b7let", "ihr", "nur", "mit\u00b7leid\u00b7gros\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seid nur aus dank den euch geweihten offen:                 ", "tokens": ["Seid", "nur", "aus", "dank", "den", "euch", "ge\u00b7weih\u00b7ten", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "APPR", "APPR", "ART", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die ber&#252;hrten dann in solchen gluten               ", "tokens": ["Und", "die", "ber", "&#252;", "hr\u00b7ten", "dann", "in", "sol\u00b7chen", "glu\u00b7ten"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "$(", "VVFIN", "ADV", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die antwort gaben wider h\u00f6chstes hoffen                 ", "tokens": ["Die", "ant\u00b7wort", "ga\u00b7ben", "wi\u00b7der", "h\u00f6chs\u00b7tes", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass dem noch zweifelnden die sinne wichen                 ", "tokens": ["Dass", "dem", "noch", "zwei\u00b7feln\u00b7den", "die", "sin\u00b7ne", "wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O rinnen der gl&#252;ckseligen minuten!               ", "tokens": ["O", "rin\u00b7nen", "der", "gl", "&#252;", "ck\u00b7se\u00b7li\u00b7gen", "mi\u00b7nu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "$(", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}