{"textgrid.poem.35581": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im Park", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder wandelnd im alten Park,", "tokens": ["Wie\u00b7der", "wan\u00b7delnd", "im", "al\u00b7ten", "Park", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "O! Stille gelb und roter Blumen.", "tokens": ["O", "!", "Stil\u00b7le", "gelb", "und", "ro\u00b7ter", "Blu\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr auch trauert, ihr sanften G\u00f6tter,", "tokens": ["Ihr", "auch", "trau\u00b7ert", ",", "ihr", "sanf\u00b7ten", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und das herbstliche Gold der Ulme.", "tokens": ["Und", "das", "herbst\u00b7li\u00b7che", "Gold", "der", "Ul\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Reglos ragt am bl\u00e4ulichen Weiher", "tokens": ["Reg\u00b7los", "ragt", "am", "bl\u00e4u\u00b7li\u00b7chen", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Das Rohr, verstummt am Abend die Drossel.", "tokens": ["Das", "Rohr", ",", "ver\u00b7stummt", "am", "A\u00b7bend", "die", "Dros\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O! dann neige auch du die Stirne", "tokens": ["O", "!", "dann", "nei\u00b7ge", "auch", "du", "die", "Stir\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADV", "PPER", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Vor der Ahnen verfallenem Marmor.", "tokens": ["Vor", "der", "Ah\u00b7nen", "ver\u00b7fal\u00b7le\u00b7nem", "Mar\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}