{"dta.poem.10942": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Luft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du irrest dich. Wenn ich nicht w\u00e4r,", "tokens": ["Du", "ir\u00b7rest", "dich", ".", "Wenn", "ich", "nicht", "w\u00e4r", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KOUS", "PPER", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00e4rst du selbst von allem leer.", "tokens": ["So", "w\u00e4rst", "du", "selbst", "von", "al\u00b7lem", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schenkt ich dir nicht mein fruchtbar Na\u00df:", "tokens": ["Schenkt", "ich", "dir", "nicht", "mein", "frucht\u00b7bar", "Na\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PPOSAT", "ADJD", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "So w\u00fcchse nicht ein Spierchen Gras.", "tokens": ["So", "w\u00fcch\u00b7se", "nicht", "ein", "Spier\u00b7chen", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch minder k\u00f6nntest du mit Zweigen", "tokens": ["Noch", "min\u00b7der", "k\u00f6nn\u00b7test", "du", "mit", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sch\u00f6n gekr\u00f6nte St\u00e4mme zeugen.", "tokens": ["So", "sch\u00f6n", "ge\u00b7kr\u00f6n\u00b7te", "St\u00e4m\u00b7me", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}