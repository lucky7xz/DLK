{"textgrid.poem.42361": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht war mondhell, doch die Wolken flohn.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht war mondhell, doch die Wolken flohn.", "tokens": ["Die", "Nacht", "war", "mond\u00b7hell", ",", "doch", "die", "Wol\u00b7ken", "flohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du kamst im Traum und bist im Traum entschwunden.", "tokens": ["Du", "kamst", "im", "Traum", "und", "bist", "im", "Traum", "ent\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als m\u00fcsstest du erdulden Schmach und Hohn,", "tokens": ["Als", "m\u00fcss\u00b7test", "du", "er\u00b7dul\u00b7den", "Schmach", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Trugst du die Spur von Herzensnot und Wunden.", "tokens": ["Trugst", "du", "die", "Spur", "von", "Her\u00b7zens\u00b7not", "und", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Mund war schmerzlich, deine Blicke fern", "tokens": ["Dein", "Mund", "war", "schmerz\u00b7lich", ",", "dei\u00b7ne", "Bli\u00b7cke", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und so verzagt, als wollt' kein Gott dir gnaden \u2013", "tokens": ["Und", "so", "ver\u00b7zagt", ",", "als", "wollt'", "kein", "Gott", "dir", "gna\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "KOUS", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00e4tt' um dich, um deinen Frieden gern", "tokens": ["Ich", "h\u00e4tt'", "um", "dich", ",", "um", "dei\u00b7nen", "Frie\u00b7den", "gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "KOUI", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Joch der Niedrigkeit auf mich geladen.", "tokens": ["Ein", "Joch", "der", "Nied\u00b7rig\u00b7keit", "auf", "mich", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein eilendes Gew\u00f6lk trieb schwarz und wild", "tokens": ["Ein", "ei\u00b7len\u00b7des", "Ge\u00b7w\u00f6lk", "trieb", "schwarz", "und", "wild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An uns vorbei wie Todesschattenwehen,", "tokens": ["An", "uns", "vor\u00b7bei", "wie", "To\u00b7des\u00b7schat\u00b7ten\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich klammerte mich an dein stummes Bild", "tokens": ["Ich", "klam\u00b7mer\u00b7te", "mich", "an", "dein", "stum\u00b7mes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sank mit dir in Nebel und Vergehen.", "tokens": ["Und", "sank", "mit", "dir", "in", "Ne\u00b7bel", "und", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}