{"textgrid.poem.65731": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieselben Stunden, die mit sanftem Kreisen", "tokens": ["Die\u00b7sel\u00b7ben", "Stun\u00b7den", ",", "die", "mit", "sanf\u00b7tem", "Krei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den s\u00fc\u00dfen Blick geformt, wonach uns so verlangt,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Blick", "ge\u00b7formt", ",", "wo\u00b7nach", "uns", "so", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie werden ihm tyrannisch sich erweisen", "tokens": ["Sie", "wer\u00b7den", "ihm", "ty\u00b7ran\u00b7nisch", "sich", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und das entstellen, was so herrlich prangt.", "tokens": ["Und", "das", "ent\u00b7stel\u00b7len", ",", "was", "so", "herr\u00b7lich", "prangt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn Zeit, nie rastend, f\u00fchrt den Sommer fort", "tokens": ["Denn", "Zeit", ",", "nie", "ras\u00b7tend", ",", "f\u00fchrt", "den", "Som\u00b7mer", "fort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum finstern Winter, und verdirbt ihn da.", "tokens": ["Zum", "fins\u00b7tern", "Win\u00b7ter", ",", "und", "ver\u00b7dirbt", "ihn", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es stocken S\u00e4fte, Blatt auf Blatt verdorrt,", "tokens": ["Es", "sto\u00b7cken", "S\u00e4f\u00b7te", ",", "Blatt", "auf", "Blatt", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verschneit liegt Sch\u00f6nheit, W\u00fcste fern und nah.", "tokens": ["Ver\u00b7schneit", "liegt", "Sch\u00f6n\u00b7heit", ",", "W\u00fcs\u00b7te", "fern", "und", "nah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Blieb dann nicht Sommers abgezogner Sinn,", "tokens": ["Blieb", "dann", "nicht", "Som\u00b7mers", "ab\u00b7ge\u00b7zog\u00b7ner", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der fl\u00fcssige Gefangn' in Glases Mauern,", "tokens": ["Der", "fl\u00fcs\u00b7si\u00b7ge", "Ge\u00b7fangn'", "in", "Gla\u00b7ses", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "W\u00e4r mit dem Sch\u00f6nen Sch\u00f6nheitsfrucht dahin,", "tokens": ["W\u00e4r", "mit", "dem", "Sch\u00f6\u00b7nen", "Sch\u00f6n\u00b7heits\u00b7frucht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nicht selbst, noch im Ged\u00e4chtnis fortzudauern.", "tokens": ["Nicht", "selbst", ",", "noch", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "fort\u00b7zu\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch abgezogne Blumen, ob auch Winter", "tokens": ["Doch", "ab\u00b7ge\u00b7zog\u00b7ne", "Blu\u00b7men", ",", "ob", "auch", "Win\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie bleicht, ihr Wesen duftet drum nicht minder.", "tokens": ["Sie", "bleicht", ",", "ihr", "We\u00b7sen", "duf\u00b7tet", "drum", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PAV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}