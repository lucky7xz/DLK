{"textgrid.poem.65876": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "150", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welche Macht kann dir die Allmacht leihen,", "tokens": ["O", "wel\u00b7che", "Macht", "kann", "dir", "die", "All\u00b7macht", "lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz zu b\u00e4ndigen durch D\u00fcrftigkeit,", "tokens": ["Mein", "Herz", "zu", "b\u00e4n\u00b7di\u00b7gen", "durch", "D\u00fcrf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich der L\u00fcge mu\u00df die offnen Augen zeihen", "tokens": ["Da\u00df", "ich", "der", "L\u00fc\u00b7ge", "mu\u00df", "die", "off\u00b7nen", "Au\u00b7gen", "zei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schw\u00f6ren, hellen Tag entstelle Helligkeit?", "tokens": ["Und", "schw\u00f6\u00b7ren", ",", "hel\u00b7len", "Tag", "ent\u00b7stel\u00b7le", "Hel\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von woher kommt dir dieser Reiz des B\u00f6sen,", "tokens": ["Von", "wo\u00b7her", "kommt", "dir", "die\u00b7ser", "Reiz", "des", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "VVFIN", "PPER", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df, wenn ich w\u00e4hlen sollte, selbst dein Gift,", "tokens": ["Da\u00df", ",", "wenn", "ich", "w\u00e4h\u00b7len", "soll\u00b7te", ",", "selbst", "dein", "Gift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein Abschaum durch sein freies, sichres Wesen", "tokens": ["Dein", "Ab\u00b7schaum", "durch", "sein", "frei\u00b7es", ",", "sich\u00b7res", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Edelste der andern \u00fcbertrifft?", "tokens": ["Das", "E\u00b7dels\u00b7te", "der", "an\u00b7dern", "\u00fc\u00b7bert\u00b7rifft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.9": {"text": "Wer lehrte dich mehr Lieb' in mir entz\u00fcnden,", "tokens": ["Wer", "lehr\u00b7te", "dich", "mehr", "Lieb'", "in", "mir", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Je mehr ich Hassensgr\u00fcnde h\u00f6r' und seh'? \u2013", "tokens": ["Je", "mehr", "ich", "Has\u00b7sens\u00b7gr\u00fcn\u00b7de", "h\u00f6r'", "und", "seh'", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "O, lieb' ich gleich was andre schm\u00e4hlich finden,", "tokens": ["O", ",", "lieb'", "ich", "gleich", "was", "and\u00b7re", "schm\u00e4h\u00b7lich", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "PWS", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit andern solltest du nicht schm\u00e4hn mein Weh!", "tokens": ["Mit", "an\u00b7dern", "soll\u00b7test", "du", "nicht", "schm\u00e4hn", "mein", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn du durch Unwert mich zum Lieben triebst,", "tokens": ["Wenn", "du", "durch", "Un\u00b7wert", "mich", "zum", "Lie\u00b7ben", "triebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bin ich nur w\u00fcrdiger, da\u00df du mich liebst.", "tokens": ["Bin", "ich", "nur", "w\u00fcr\u00b7di\u00b7ger", ",", "da\u00df", "du", "mich", "liebst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}