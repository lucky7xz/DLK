{"textgrid.poem.40418": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pfarrherr, du k\u00fchler, \u00f6ffne dein Tor,", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pfarrherr, du k\u00fchler, \u00f6ffne dein Tor,", "tokens": ["Pfar\u00b7rherr", ",", "du", "k\u00fch\u00b7ler", ",", "\u00f6ff\u00b7ne", "dein", "Tor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fahrende Sch\u00fcler stehen davor.", "tokens": ["Fah\u00b7ren\u00b7de", "Sch\u00fc\u00b7ler", "ste\u00b7hen", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Fahrende Sch\u00fcler, unstete Kind,", "tokens": ["Fah\u00b7ren\u00b7de", "Sch\u00fc\u00b7ler", ",", "un\u00b7ste\u00b7te", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Singer und Spieler, wirbliger Wind.", "tokens": ["Sin\u00b7ger", "und", "Spie\u00b7ler", ",", "wirb\u00b7li\u00b7ger", "Wind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Eisern die Kehlen, M\u00e4gen von Erz,", "tokens": ["Ei\u00b7sern", "die", "Keh\u00b7len", ",", "M\u00e4\u00b7gen", "von", "Erz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Goldklare Seelen ... doch keiner begehrt's.", "tokens": ["Gold\u00b7kla\u00b7re", "See\u00b7len", "...", "doch", "kei\u00b7ner", "be\u00b7gehrt'", "s."], "token_info": ["word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "$(", "ADV", "PIS", "VVFIN", "NE"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Kleidung ist d\u00fcnne, Spreitung ist roh,", "tokens": ["Klei\u00b7dung", "ist", "d\u00fcn\u00b7ne", ",", "Sprei\u00b7tung", "ist", "roh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ach und die Minne?... im Heu und auf Stroh.", "tokens": ["Ach", "und", "die", "Min\u00b7ne", "?", "...", "im", "Heu", "und", "auf", "Stroh", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "ART", "NN", "$.", "$(", "APPRART", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Pfarrrherrr, du k\u00fchler, \u00f6ffne dein Tor,", "tokens": ["Pfarr\u00b7rher\u00b7rr", ",", "du", "k\u00fch\u00b7ler", ",", "\u00f6ff\u00b7ne", "dein", "Tor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Fahrende Sch\u00fcler stehen davor.", "tokens": ["Fah\u00b7ren\u00b7de", "Sch\u00fc\u00b7ler", "ste\u00b7hen", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Franken und Schwaben kennen uns gut,", "tokens": ["Fran\u00b7ken", "und", "Schwa\u00b7ben", "ken\u00b7nen", "uns", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "L\u00fcftige Knaben, fr\u00e4\u00dfige Brut.", "tokens": ["L\u00fcf\u00b7ti\u00b7ge", "Kna\u00b7ben", ",", "fr\u00e4\u00b7\u00dfi\u00b7ge", "Brut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "M\u00fcssen uns n\u00e4hren, Gotteserbarm,", "tokens": ["M\u00fcs\u00b7sen", "uns", "n\u00e4h\u00b7ren", ",", "Got\u00b7te\u00b7ser\u00b7barm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gleich dem verheerenden Heuschreckenschwarm.", "tokens": ["Gleich", "dem", "ver\u00b7hee\u00b7ren\u00b7den", "Heu\u00b7schre\u00b7cken\u00b7schwarm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Was wir durchstrichen, Bergflur und Tal,", "tokens": ["Was", "wir", "durch\u00b7stri\u00b7chen", ",", "Berg\u00b7flur", "und", "Tal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Alles verblichen, abgegrast, kahl.", "tokens": ["Al\u00b7les", "ver\u00b7bli\u00b7chen", ",", "ab\u00b7ge\u00b7grast", ",", "kahl", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Pfarrrrherrrr, du k\u00fchler, \u00f6ffne dein Tor,", "tokens": ["Pfarrrr\u00b7herrrr", ",", "du", "k\u00fch\u00b7ler", ",", "\u00f6ff\u00b7ne", "dein", "Tor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Fahrende Sch\u00fcler stehen davor!", "tokens": ["Fah\u00b7ren\u00b7de", "Sch\u00fc\u00b7ler", "ste\u00b7hen", "da\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sparst du den Habersack, knaus'riger Kropf,", "tokens": ["Sparst", "du", "den", "Ha\u00b7ber\u00b7sack", ",", "knaus'\u00b7ri\u00b7ger", "Kropf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Packen zum Schabernack wir dich am Kopf,", "tokens": ["Pa\u00b7cken", "zum", "Scha\u00b7ber\u00b7nack", "wir", "dich", "am", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ziehen die Hosen, den Kuttrock dir aus,", "tokens": ["Zie\u00b7hen", "die", "Ho\u00b7sen", ",", "den", "Kut\u00b7trock", "dir", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "H\u00e4ngen die losen vors Fenster als Strau\u00df.", "tokens": ["H\u00e4n\u00b7gen", "die", "lo\u00b7sen", "vors", "Fens\u00b7ter", "als", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "KOKOM", "NE", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Wer um den s\u00fc\u00dfen Labtrunk uns klemmt,", "tokens": ["Wer", "um", "den", "s\u00fc\u00b7\u00dfen", "Lab\u00b7trunk", "uns", "klemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der mu\u00df uns b\u00fc\u00dfen in Str\u00fcmpfen und Hemd.", "tokens": ["Der", "mu\u00df", "uns", "b\u00fc\u00b7\u00dfen", "in", "Str\u00fcmp\u00b7fen", "und", "Hemd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Pfarrherr, du k\u00fchler, \u00f6ffne den Turm,", "tokens": ["Pfar\u00b7rherr", ",", "du", "k\u00fch\u00b7ler", ",", "\u00f6ff\u00b7ne", "den", "Turm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fahrende Sch\u00fcler r\u00fcsten zum Sturm!", "tokens": ["Fah\u00b7ren\u00b7de", "Sch\u00fc\u00b7ler", "r\u00fcs\u00b7ten", "zum", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ho, ho, heiadihoh!", "tokens": ["Ho", ",", "ho", ",", "hei\u00b7a\u00b7di\u00b7hoh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Avoy, avoy, alez avanz!", "tokens": ["A\u00b7voy", ",", "a\u00b7voy", ",", "a\u00b7lez", "a\u00b7vanz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM.fr", "FM.fr", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Als\u00fbs als\u00f4, als\u00fbs als\u00f4!", "tokens": ["Als\u00fbs", "als\u00f4", ",", "als\u00fbs", "als\u00f4", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "FM.la", "FM.la", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ho, ho, heiadihoh, hoh, ho, ho!", "tokens": ["Ho", ",", "ho", ",", "hei\u00b7a\u00b7di\u00b7hoh", ",", "hoh", ",", "ho", ",", "ho", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ADJD", "$,", "ADJD", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}}}}}