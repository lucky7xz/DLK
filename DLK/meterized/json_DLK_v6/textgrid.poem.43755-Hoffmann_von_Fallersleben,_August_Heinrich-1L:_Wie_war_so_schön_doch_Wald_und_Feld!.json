{"textgrid.poem.43755": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie war so sch\u00f6n doch Wald und Feld!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie war so sch\u00f6n doch Wald und Feld!", "tokens": ["Wie", "war", "so", "sch\u00f6n", "doch", "Wald", "und", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie traurig ist anjetzt die Welt!", "tokens": ["Wie", "trau\u00b7rig", "ist", "an\u00b7jetzt", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hin ist die sch\u00f6ne Sommerzeit", "tokens": ["Hin", "ist", "die", "sch\u00f6\u00b7ne", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach der Freude kam das Leid.", "tokens": ["Und", "nach", "der", "Freu\u00b7de", "kam", "das", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir wu\u00dften nichts von Ungemach,", "tokens": ["Wir", "wu\u00df\u00b7ten", "nichts", "von", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sa\u00dfen unterm Laubesdach", "tokens": ["Wir", "sa\u00b7\u00dfen", "un\u00b7term", "Lau\u00b7bes\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergn\u00fcgt und froh im Sonnenschein", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "froh", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sangen in die Welt hinein.", "tokens": ["Und", "san\u00b7gen", "in", "die", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir armen V\u00f6gel trauern sehr:", "tokens": ["Wir", "ar\u00b7men", "V\u00f6\u00b7gel", "trau\u00b7ern", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben keine Heimath mehr,", "tokens": ["Wir", "ha\u00b7ben", "kei\u00b7ne", "Hei\u00b7math", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir m\u00fcssen jetzt von hinnen flieh'n", "tokens": ["Wir", "m\u00fcs\u00b7sen", "jetzt", "von", "hin\u00b7nen", "flieh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in die weite Fremde zieh'n.", "tokens": ["Und", "in", "die", "wei\u00b7te", "Frem\u00b7de", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}