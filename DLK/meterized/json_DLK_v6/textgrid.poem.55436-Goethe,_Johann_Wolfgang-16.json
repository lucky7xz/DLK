{"textgrid.poem.55436": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "16", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die K\u00f6rner wollte z\u00e4hlen,", "tokens": ["Wer", "die", "K\u00f6r\u00b7ner", "woll\u00b7te", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dem Stundenglas entrinnen,", "tokens": ["Die", "dem", "Stun\u00b7den\u00b7glas", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrde Zeit und Ziel verfehlen,", "tokens": ["W\u00fcr\u00b7de", "Zeit", "und", "Ziel", "ver\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solchem Strome nachzusinnen.", "tokens": ["Sol\u00b7chem", "Stro\u00b7me", "nach\u00b7zu\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch vergehn uns die Gedanken,", "tokens": ["Auch", "ver\u00b7gehn", "uns", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir in dein Leben schauen,", "tokens": ["Wenn", "wir", "in", "dein", "Le\u00b7ben", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freien Geist in Erdeschranken,", "tokens": ["Frei\u00b7en", "Geist", "in", "Er\u00b7de\u00b7schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Festes Handeln und Vertrauen.", "tokens": ["Fes\u00b7tes", "Han\u00b7deln", "und", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So entrinnen jeder Stunde", "tokens": ["So", "ent\u00b7rin\u00b7nen", "je\u00b7der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcgsam gl\u00fcckliche Gesch\u00e4fte.", "tokens": ["F\u00fcg\u00b7sam", "gl\u00fcck\u00b7li\u00b7che", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Segen dir von Mund zu Munde!", "tokens": ["Se\u00b7gen", "dir", "von", "Mund", "zu", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neuen Mut und frische Kr\u00e4fte!", "tokens": ["Neu\u00b7en", "Mut", "und", "fri\u00b7sche", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}