{"textgrid.poem.30304": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutschland hat f\u00fcr l\u00e4ngst geherrscht als ein Haupt der Christenheit;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland hat f\u00fcr l\u00e4ngst geherrscht als ein Haupt der Christenheit;", "tokens": ["Deutschland", "hat", "f\u00fcr", "l\u00e4ngst", "ge\u00b7herrscht", "als", "ein", "Haupt", "der", "Chris\u00b7ten\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ADV", "VVPP", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber deutscher Sprache wehrt lag in tieffer Dienstbarkeit.", "tokens": ["A\u00b7ber", "deut\u00b7scher", "Spra\u00b7che", "wehrt", "lag", "in", "tief\u00b7fer", "Dienst\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Da\u00df nun auch die Sprache herrscht, h\u00f6chlich gilt und lieblich schillt,", "tokens": ["Da\u00df", "nun", "auch", "die", "Spra\u00b7che", "herrscht", ",", "h\u00f6ch\u00b7lich", "gilt", "und", "lieb\u00b7lich", "schillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dieses macht der theure Held, welchen altes Anhalt hilt,", "tokens": ["Die\u00b7ses", "macht", "der", "theu\u00b7re", "Held", ",", "wel\u00b7chen", "al\u00b7tes", "An\u00b7halt", "hilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ludewig, der weise F\u00fcrst. Deutschland, Deutschland, wie mich d\u00fcnckt,", "tokens": ["Lu\u00b7de\u00b7wig", ",", "der", "wei\u00b7se", "F\u00fcrst", ".", "Deutschland", ",", "Deutschland", ",", "wie", "mich", "d\u00fcnckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$.", "NE", "$,", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "Ist dein Mund gar viel zu schwach, da\u00df sein Ruhm durch dich erklingt!", "tokens": ["Ist", "dein", "Mund", "gar", "viel", "zu", "schwach", ",", "da\u00df", "sein", "Ruhm", "durch", "dich", "er\u00b7klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKA", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Singe, was du weist und kanst; sage, was du kanst und weist:", "tokens": ["Sin\u00b7ge", ",", "was", "du", "weist", "und", "kanst", ";", "sa\u00b7ge", ",", "was", "du", "kanst", "und", "weist", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "KON", "VMFIN", "$.", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Du wirst nimmer recht geschickt; Er wird nie genug gepreist.", "tokens": ["Du", "wirst", "nim\u00b7mer", "recht", "ge\u00b7schickt", ";", "Er", "wird", "nie", "ge\u00b7nug", "ge\u00b7preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}