{"textgrid.poem.42026": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Bl\u00fcmlein schl\u00e4ft, die Sonne sank", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Bl\u00fcmlein schl\u00e4ft, die Sonne sank", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "schl\u00e4ft", ",", "die", "Son\u00b7ne", "sank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Zwielicht ringen Nacht und Helle;", "tokens": ["Im", "Zwie\u00b7licht", "rin\u00b7gen", "Nacht", "und", "Hel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Still wandl' ich stets den Bach entlang", "tokens": ["Still", "wandl'", "ich", "stets", "den", "Bach", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seh hinab zur dunkeln Welle.", "tokens": ["Und", "seh", "hin\u00b7ab", "zur", "dun\u00b7keln", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da schwimmt ein leises, liebes Bild", "tokens": ["Da", "schwimmt", "ein", "lei\u00b7ses", ",", "lie\u00b7bes", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erzitternd in der blauen Tiefe", "tokens": ["Er\u00b7zit\u00b7ternd", "in", "der", "blau\u00b7en", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lacht so hold und winkt so mild,", "tokens": ["Und", "lacht", "so", "hold", "und", "winkt", "so", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob es mich zur Heimath riefe.", "tokens": ["Als", "ob", "es", "mich", "zur", "Hei\u00b7math", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist der Liebe goldner Stern,", "tokens": ["Das", "ist", "der", "Lie\u00b7be", "gold\u00b7ner", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich im B\u00e4chlein leuchten sehe:", "tokens": ["Den", "ich", "im", "B\u00e4ch\u00b7lein", "leuch\u00b7ten", "se\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibt er selbst mir ewig fern,", "tokens": ["Doch", "bleibt", "er", "selbst", "mir", "e\u00b7wig", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Bild nur l\u00e4chelt in der N\u00e4he.", "tokens": ["Sein", "Bild", "nur", "l\u00e4\u00b7chelt", "in", "der", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O wollte doch die Fr\u00fchlingsluft", "tokens": ["O", "woll\u00b7te", "doch", "die", "Fr\u00fch\u00b7lings\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empor auf ihrem Wehn mich wiegen!", "tokens": ["Em\u00b7por", "auf", "ih\u00b7rem", "Wehn", "mich", "wie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O wenn doch Glanz und Bl\u00fcthenduft", "tokens": ["O", "wenn", "doch", "Glanz", "und", "Bl\u00fc\u00b7then\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum holden Licht mich aufw\u00e4rts tr\u00fcgen!", "tokens": ["Zum", "hol\u00b7den", "Licht", "mich", "auf\u00b7w\u00e4rts", "tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So mu\u00df ich still und einsam gehn", "tokens": ["So", "mu\u00df", "ich", "still", "und", "ein\u00b7sam", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und niederschaun zum klaren Sterne;", "tokens": ["Und", "nie\u00b7der\u00b7schaun", "zum", "kla\u00b7ren", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Stern, wie l\u00e4chelst du so sch\u00f6n!", "tokens": ["O", "Stern", ",", "wie", "l\u00e4\u00b7chelst", "du", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich lieb' ich auch in weiter Ferne!", "tokens": ["Dich", "lieb'", "ich", "auch", "in", "wei\u00b7ter", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}