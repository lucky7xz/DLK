{"textgrid.poem.66119": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Wir zwei", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben oft beim Wein gesessen", "tokens": ["Wir", "ha\u00b7ben", "oft", "beim", "Wein", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00f6fter beim Grog.", "tokens": ["Und", "\u00f6f\u00b7ter", "beim", "Grog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Beim Pfandverleiher lag indessen", "tokens": ["Beim", "Pfand\u00b7ver\u00b7lei\u00b7her", "lag", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sonntagsrock.", "tokens": ["Der", "Sonn\u00b7tags\u00b7rock", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wir haben die lustigsten M\u00e4delgeschichten", "tokens": ["Wir", "ha\u00b7ben", "die", "lus\u00b7tigs\u00b7ten", "M\u00e4\u00b7del\u00b7ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ausgetauscht,", "tokens": ["Aus\u00b7ge\u00b7tauscht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "An Abenteuer und an Gedichten", "tokens": ["An", "A\u00b7bent\u00b7eu\u00b7er", "und", "an", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns weidlich berauscht.", "tokens": ["Uns", "weid\u00b7lich", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wir haben, o je, von unsern Schulden", "tokens": ["Wir", "ha\u00b7ben", ",", "o", "je", ",", "von", "un\u00b7sern", "Schul\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Uns vorgeklagt,", "tokens": ["Uns", "vor\u00b7ge\u00b7klagt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vertranken dabei den letzten Gulden:", "tokens": ["Ver\u00b7tran\u00b7ken", "da\u00b7bei", "den", "letz\u00b7ten", "Gul\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nur nicht verzagt!", "tokens": ["Nur", "nicht", "ver\u00b7zagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wir haben uns immer zusammengefunden,", "tokens": ["Wir", "ha\u00b7ben", "uns", "im\u00b7mer", "zu\u00b7sam\u00b7men\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wars Wetter schlecht;", "tokens": ["Wars", "Wet\u00b7ter", "schlecht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und waren die greulichen Wolken verschwunden,", "tokens": ["Und", "wa\u00b7ren", "die", "greu\u00b7li\u00b7chen", "Wol\u00b7ken", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann erst recht.", "tokens": ["Dann", "erst", "recht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wir sind zwei Kirschen an einem Stengel,", "tokens": ["Wir", "sind", "zwei", "Kir\u00b7schen", "an", "ei\u00b7nem", "Sten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Zwiegesang,", "tokens": ["Ein", "Zwie\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Kanon, wie er von Bach bis Klengel", "tokens": ["Ein", "Ka\u00b7non", ",", "wie", "er", "von", "Bach", "bis", "Klen\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPR", "NE", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Noch keinem gelang.", "tokens": ["Noch", "kei\u00b7nem", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wir sind zwei Schelme. Wenn sie uns fangen,", "tokens": ["Wir", "sind", "zwei", "Schel\u00b7me", ".", "Wenn", "sie", "uns", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Philistergericht,", "tokens": ["Phi\u00b7lis\u00b7ter\u00b7ge\u00b7richt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir m\u00fcssen an einem Galgen hangen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "an", "ei\u00b7nem", "Gal\u00b7gen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonst tun wirs nicht.", "tokens": ["Sonst", "tun", "wirs", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}