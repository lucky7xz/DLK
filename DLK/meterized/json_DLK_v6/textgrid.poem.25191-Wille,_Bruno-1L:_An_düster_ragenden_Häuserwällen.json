{"textgrid.poem.25191": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: An d\u00fcster ragenden H\u00e4userw\u00e4llen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An d\u00fcster ragenden H\u00e4userw\u00e4llen", "tokens": ["An", "d\u00fcs\u00b7ter", "ra\u00b7gen\u00b7den", "H\u00e4u\u00b7ser\u00b7w\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch flammenbes\u00e4te steinerne Schlucht", "tokens": ["Durch", "flam\u00b7men\u00b7be\u00b7s\u00e4\u00b7te", "stei\u00b7ner\u00b7ne", "Schlucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Branden die rasselnden Wagen, die Menschen \u2013", "tokens": ["Bran\u00b7den", "die", "ras\u00b7seln\u00b7den", "Wa\u00b7gen", ",", "die", "Men\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wie Wellen in klippiger Meeresbucht \u2013", "tokens": ["Wie", "Wel\u00b7len", "in", "klip\u00b7pi\u00b7ger", "Mee\u00b7res\u00b7bucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der rote Vollmond taucht empor.", "tokens": ["Der", "ro\u00b7te", "Voll\u00b7mond", "taucht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Menge w\u00fchlt und dr\u00e4ngt und st\u00f6\u00dft;", "tokens": ["Die", "Men\u00b7ge", "w\u00fchlt", "und", "dr\u00e4ngt", "und", "st\u00f6\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedweden k\u00fcmmert nur ", "tokens": ["Jed\u00b7we\u00b7den", "k\u00fcm\u00b7mert", "nur"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie auf dem Deck des lecken Schiffes,", "tokens": ["Wie", "auf", "dem", "Deck", "des", "le\u00b7cken", "Schif\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das in den Tod zu sinken droht \u2013", "tokens": ["Das", "in", "den", "Tod", "zu", "sin\u00b7ken", "droht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der rote Mond schaut d\u00fcster drein.", "tokens": ["Der", "ro\u00b7te", "Mond", "schaut", "d\u00fcs\u00b7ter", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf glattem B\u00fcrgersteige kauert \u2013", "tokens": ["Auf", "glat\u00b7tem", "B\u00fcr\u00b7ger\u00b7stei\u00b7ge", "kau\u00b7ert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleichwie am Felsenriff das Wrack \u2013", "tokens": ["Gleich\u00b7wie", "am", "Fel\u00b7sen\u00b7riff", "das", "Wrack", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Mann mit vorgesunknem Kopfe,", "tokens": ["Ein", "Mann", "mit", "vor\u00b7ge\u00b7sun\u00b7knem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Seite einen Lumpensack \u2013", "tokens": ["Zur", "Sei\u00b7te", "ei\u00b7nen", "Lum\u00b7pen\u00b7sack", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vollmond blickt mit d\u00fcstrer Glut.", "tokens": ["Der", "Voll\u00b7mond", "blickt", "mit", "d\u00fcst\u00b7rer", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Leute auf dem B\u00fcrgersteige", "tokens": ["Die", "Leu\u00b7te", "auf", "dem", "B\u00fcr\u00b7ger\u00b7stei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Treiben vorbei und blicken kalt;", "tokens": ["Trei\u00b7ben", "vor\u00b7bei", "und", "bli\u00b7cken", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Pferdebahn beglotzt im Rollen", "tokens": ["Die", "Pfer\u00b7de\u00b7bahn", "be\u00b7glotzt", "im", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gr\u00fcnem Auge die Gestalt \u2013", "tokens": ["Mit", "gr\u00fc\u00b7nem", "Au\u00b7ge", "die", "Ge\u00b7stalt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der rote Mond schaut d\u00fcster drein.", "tokens": ["Der", "ro\u00b7te", "Mond", "schaut", "d\u00fcs\u00b7ter", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort dr\u00fcben lockt die blutige Flamme", "tokens": ["Dort", "dr\u00fc\u00b7ben", "lockt", "die", "blu\u00b7ti\u00b7ge", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Schnapswirt manchen Gast ins Haus;", "tokens": ["Dem", "Schnaps\u00b7wirt", "man\u00b7chen", "Gast", "ins", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00f6ffnet sich die dunstige Schenke,", "tokens": ["Und", "\u00f6ff\u00b7net", "sich", "die", "duns\u00b7ti\u00b7ge", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dringt Schelten und Gejohl heraus \u2013", "tokens": ["Dringt", "Schel\u00b7ten", "und", "Ge\u00b7johl", "he\u00b7raus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vollmond blickt mit d\u00fcstrer Glut.", "tokens": ["Der", "Voll\u00b7mond", "blickt", "mit", "d\u00fcst\u00b7rer", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Des Handelshauses Fensterreihe", "tokens": ["Des", "Han\u00b7dels\u00b7hau\u00b7ses", "Fens\u00b7ter\u00b7rei\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist noch vom Gaslicht grell erhellt;", "tokens": ["Ist", "noch", "vom", "Gas\u00b7licht", "grell", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Papier und Pult und blasse Schreiber;", "tokens": ["Pa\u00b7pier", "und", "Pult", "und", "blas\u00b7se", "Schrei\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Chef durchz\u00e4hlt des Tages Geld \u2013", "tokens": ["Der", "Chef", "durch\u00b7z\u00e4hlt", "des", "Ta\u00b7ges", "Geld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vollmond blickt mit d\u00fcstrer Glut.", "tokens": ["Der", "Voll\u00b7mond", "blickt", "mit", "d\u00fcst\u00b7rer", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun heult vom Hofe die Maschine", "tokens": ["Nun", "heult", "vom", "Ho\u00b7fe", "die", "Ma\u00b7schi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Vesper; da entl\u00e4\u00dft das Thor", "tokens": ["Zur", "Ves\u00b7per", ";", "da", "ent\u00b7l\u00e4\u00dft", "das", "Thor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel arbeitsmatte Blusenm\u00e4nner;", "tokens": ["Viel", "ar\u00b7beits\u00b7mat\u00b7te", "Blu\u00b7sen\u00b7m\u00e4n\u00b7ner", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur der Fabrikschlot st\u00f6\u00dft empor", "tokens": ["Nur", "der", "Fab\u00b7rik\u00b7schlot", "st\u00f6\u00dft", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zum roten Monde schwarzen Rauch.", "tokens": ["Zum", "ro\u00b7ten", "Mon\u00b7de", "schwar\u00b7zen", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein w\u00fcrdiger B\u00fcrger kommt geschritten,", "tokens": ["Ein", "w\u00fcr\u00b7di\u00b7ger", "B\u00fcr\u00b7ger", "kommt", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Lump am Steige trifft sein Blick;", "tokens": ["Den", "Lump", "am", "Stei\u00b7ge", "trifft", "sein", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entr\u00fcstet mit dem Kopfe sch\u00fcttelnd", "tokens": ["Ent\u00b7r\u00fcs\u00b7tet", "mit", "dem", "Kop\u00b7fe", "sch\u00fct\u00b7telnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht er zu Bier und Politik \u2013", "tokens": ["Geht", "er", "zu", "Bier", "und", "Po\u00b7li\u00b7tik", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zornrot gl\u00fcht der volle Mond.", "tokens": ["Und", "zorn\u00b7rot", "gl\u00fcht", "der", "vol\u00b7le", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}