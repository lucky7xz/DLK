{"textgrid.poem.26429": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweigen sei heute in allen Feldlagern.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigen sei heute in allen Feldlagern.", "tokens": ["Schwei\u00b7gen", "sei", "heu\u00b7te", "in", "al\u00b7len", "Feld\u00b7la\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Legt Trauerflore um eure schlachterprobten Waffen,", "tokens": ["Legt", "Trau\u00b7er\u00b7flo\u00b7re", "um", "eu\u00b7re", "schlach\u00b7ter\u00b7prob\u00b7ten", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Soldaten!", "tokens": ["Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Jeder halte Einkehr und feiere still", "tokens": ["Je\u00b7der", "hal\u00b7te", "Ein\u00b7kehr", "und", "fei\u00b7e\u00b7re", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Unseres geliebten F\u00fchrers Tod.", "tokens": ["Un\u00b7se\u00b7res", "ge\u00b7lieb\u00b7ten", "F\u00fch\u00b7rers", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "F\u00fcr ihn keine St\u00fcrme des Lebens mehr, nicht Sieg noch Niederlagen; keine dunkeln Schicksalsschl\u00e4ge,", "tokens": ["F\u00fcr", "ihn", "kei\u00b7ne", "St\u00fcr\u00b7me", "des", "Le\u00b7bens", "mehr", ",", "nicht", "Sieg", "noch", "Nie\u00b7der\u00b7la\u00b7gen", ";", "kei\u00b7ne", "dun\u00b7keln", "Schick\u00b7sals\u00b7schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "ART", "NN", "ADV", "$,", "PTKNEG", "NN", "ADV", "NN", "$.", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Die endlos wie Wolken am Himmel dahinjagen.", "tokens": ["Die", "end\u00b7los", "wie", "Wol\u00b7ken", "am", "Him\u00b7mel", "da\u00b7hin\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch singe du, Dichter, in unserm Namen", "tokens": ["Doch", "sin\u00b7ge", "du", ",", "Dich\u00b7ter", ",", "in", "un\u00b7serm", "Na\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von unserer Liebe zu ihm.", "tokens": ["Von", "un\u00b7se\u00b7rer", "Lie\u00b7be", "zu", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Denn du, Bewohner der Zelte,", "tokens": ["Denn", "du", ",", "Be\u00b7woh\u00b7ner", "der", "Zel\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du kennst sie!", "tokens": ["Du", "kennst", "sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Wenn sie den Sarg in die Gruft niedersenken", "tokens": ["Wenn", "sie", "den", "Sarg", "in", "die", "Gruft", "nie\u00b7der\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und die Erdentore sich hinter ihm schlie\u00dfen,", "tokens": ["Und", "die", "Er\u00b7den\u00b7to\u00b7re", "sich", "hin\u00b7ter", "ihm", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Singe du einen Vers,", "tokens": ["Sin\u00b7ge", "du", "ei\u00b7nen", "Vers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "F\u00fcr das trauernde Herz der Soldaten.", "tokens": ["F\u00fcr", "das", "trau\u00b7ern\u00b7de", "Herz", "der", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}