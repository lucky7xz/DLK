{"textgrid.poem.47281": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Feld der K\u00f6nig Salomon", "tokens": ["Im", "Feld", "der", "K\u00f6\u00b7nig", "Sa\u00b7lo\u00b7mon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt unterm Himmel auf den Thron;", "tokens": ["Schl\u00e4gt", "un\u00b7term", "Him\u00b7mel", "auf", "den", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieht er einen S\u00e4mann schreiten,", "tokens": ["Da", "sieht", "er", "ei\u00b7nen", "S\u00e4\u00b7mann", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00f6rner wirft nach allen Seiten.", "tokens": ["Der", "K\u00f6r\u00b7ner", "wirft", "nach", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwas machst du da?\u00ab der K\u00f6nig spricht;", "tokens": ["\u00bb", "was", "machst", "du", "da", "?", "\u00ab", "der", "K\u00f6\u00b7nig", "spricht", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbder Boden hier tr\u00e4gt Ernte nicht.", "tokens": ["\u00bb", "der", "Bo\u00b7den", "hier", "tr\u00e4gt", "Ern\u00b7te", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df ab vom th\u00f6richten Beginnen;", "tokens": ["La\u00df", "ab", "vom", "th\u00f6\u00b7rich\u00b7ten", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du wirst die Aussaat nicht gewinnen.\u00ab", "tokens": ["Du", "wirst", "die", "Aus\u00b7saat", "nicht", "ge\u00b7win\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der S\u00e4mann, seinen Arm gesenkt,", "tokens": ["Der", "S\u00e4\u00b7mann", ",", "sei\u00b7nen", "Arm", "ge\u00b7senkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unschl\u00fcssig steht er still und denkt;", "tokens": ["Un\u00b7schl\u00fcs\u00b7sig", "steht", "er", "still", "und", "denkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann f\u00e4hrt er fort, ihn r\u00fcstig hebend,", "tokens": ["Dann", "f\u00e4hrt", "er", "fort", ",", "ihn", "r\u00fcs\u00b7tig", "he\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem weisen K\u00f6nig Antwort gebend:", "tokens": ["Dem", "wei\u00b7sen", "K\u00f6\u00b7nig", "Ant\u00b7wort", "ge\u00b7bend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbich habe nichts als dieses Feld,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "nichts", "als", "die\u00b7ses", "Feld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geackert hab' ich's und bestellt;", "tokens": ["Ge\u00b7a\u00b7ckert", "hab'", "ich's", "und", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll ich weitre Rechnung pflegen?", "tokens": ["Was", "soll", "ich", "weit\u00b7re", "Rech\u00b7nung", "pfle\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Korn von mir, von Gott der Segen!\u00ab", "tokens": ["Das", "Korn", "von", "mir", ",", "von", "Gott", "der", "Se\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "APPR", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}