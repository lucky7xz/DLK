{"textgrid.poem.55176": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Entschuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du verklagest das Weib, sie schwanke von einem zum andern!", "tokens": ["Du", "ver\u00b7kla\u00b7gest", "das", "Weib", ",", "sie", "schwan\u00b7ke", "von", "ei\u00b7nem", "zum", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "APPRART", "ADJA", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tadle sie nicht; sie sucht einen best\u00e4ndigen Mann.", "tokens": ["Tad\u00b7le", "sie", "nicht", ";", "sie", "sucht", "ei\u00b7nen", "be\u00b7st\u00e4n\u00b7di\u00b7gen", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}