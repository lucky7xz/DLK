{"textgrid.poem.63380": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Zwiegespr\u00e4ch", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt es, M\u00e4dchen,", "tokens": ["Wie", "kommt", "es", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df du deine zarten, wei\u00dfen Schuhe", "tokens": ["Da\u00df", "du", "dei\u00b7ne", "zar\u00b7ten", ",", "wei\u00b7\u00dfen", "Schu\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Beim Tanzen nie beschmutzest? \u2013", "tokens": ["Beim", "Tan\u00b7zen", "nie", "be\u00b7schmut\u00b7zest", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich auf zarten, roten Herzen tanze.", "tokens": ["Weil", "ich", "auf", "zar\u00b7ten", ",", "ro\u00b7ten", "Her\u00b7zen", "tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}