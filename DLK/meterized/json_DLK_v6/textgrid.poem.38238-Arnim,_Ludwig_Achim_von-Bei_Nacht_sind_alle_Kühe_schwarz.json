{"textgrid.poem.38238": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Bei Nacht sind alle K\u00fche schwarz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei der Nacht ist so finster im Weg,", "tokens": ["Bei", "der", "Nacht", "ist", "so", "fins\u00b7ter", "im", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Man sieht weder Br\u00fccke noch Steg,", "tokens": ["Man", "sieht", "we\u00b7der", "Br\u00fc\u00b7cke", "noch", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weder Stock noch Stein,", "tokens": ["We\u00b7der", "Stock", "noch", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Man st\u00f6\u00dft sich ans Bein,", "tokens": ["Man", "st\u00f6\u00dft", "sich", "ans", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Drum geh ich nicht gern allein.", "tokens": ["Drum", "geh", "ich", "nicht", "gern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bei der Nacht ist meine Frau auch so sch\u00f6n,", "tokens": ["Bei", "der", "Nacht", "ist", "mei\u00b7ne", "Frau", "auch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Bei Tag mag ich nicht mit ihr gehn.", "tokens": ["Bei", "Tag", "mag", "ich", "nicht", "mit", "ihr", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei der Nacht so sch\u00f6n!", "tokens": ["Bei", "der", "Nacht", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kanns gar nicht verstehn,", "tokens": ["Kanns", "gar", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mag halters nicht mit ihr gegehn.", "tokens": ["Mag", "hal\u00b7ters", "nicht", "mit", "ihr", "ge\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wann ich wieder heurathen thu,", "tokens": ["Und", "wann", "ich", "wie\u00b7der", "heu\u00b7ra\u00b7then", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So nehm ichs Laternel dazu;", "tokens": ["So", "nehm", "ichs", "La\u00b7ter\u00b7nel", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieht man beim Licht,", "tokens": ["Da", "sieht", "man", "beim", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch was einer kriegt,", "tokens": ["Doch", "was", "ei\u00b7ner", "kriegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Eine W\u00fcste, die mag ich mehr nicht.", "tokens": ["Ei\u00b7ne", "W\u00fcs\u00b7te", ",", "die", "mag", "ich", "mehr", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VMFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn ich ein Kindelein krieg,", "tokens": ["Und", "wenn", "ich", "ein", "Kin\u00b7de\u00b7lein", "krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So mu\u00df es so sch\u00f6n seyn als ich,", "tokens": ["So", "mu\u00df", "es", "so", "sch\u00f6n", "seyn", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "KOKOM", "PPER", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sonst g'h\u00f6rt es nicht mein,", "tokens": ["Sonst", "g'\u00b7h\u00f6rt", "es", "nicht", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich gehs halt nicht ein,", "tokens": ["Ich", "gehs", "halt", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es mu\u00df wie ich so sch\u00f6n seyn.", "tokens": ["Es", "mu\u00df", "wie", "ich", "so", "sch\u00f6n", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bei der Nacht hat mich oft was gefreut,", "tokens": ["Bei", "der", "Nacht", "hat", "mich", "oft", "was", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ich denk halt, 's giebt noch mehr so Leut,", "tokens": ["Ich", "denk", "halt", ",", "'s", "giebt", "noch", "mehr", "so", "Leut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADV", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da schl\u00e4ft man in Ruh,", "tokens": ["Da", "schl\u00e4ft", "man", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und deckt sich brav zu,", "tokens": ["Und", "deckt", "sich", "brav", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Es geht, ich wei\u00df selber nicht wuh!", "tokens": ["Es", "geht", ",", "ich", "wei\u00df", "sel\u00b7ber", "nicht", "wuh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}