{"dta.poem.15905": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "110.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wieweit die Kr\u00e4fte, die dir Gott gab, sich erstrecken,", "tokens": ["Wie\u00b7weit", "die", "Kr\u00e4f\u00b7te", ",", "die", "dir", "Gott", "gab", ",", "sich", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das kanst du nur, indem du sie gebrauchst, entdecken.", "tokens": ["Das", "kanst", "du", "nur", ",", "in\u00b7dem", "du", "sie", "ge\u00b7brauchst", ",", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch auch dem st\u00e4rksten Trieb des Baumes ist gesteckt", "tokens": ["Doch", "auch", "dem", "st\u00e4rks\u00b7ten", "Trieb", "des", "Bau\u00b7mes", "ist", "ge\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ziel, dar\u00fcber sich sein Wachsthum nicht erstreckt.", "tokens": ["Ein", "Ziel", ",", "da\u00b7r\u00fc\u00b7ber", "sich", "sein", "Wach\u00b7sthum", "nicht", "er\u00b7streckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PRF", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und besser manches Reis, das unentwickelt bleibt,", "tokens": ["Und", "bes\u00b7ser", "man\u00b7ches", "Reis", ",", "das", "un\u00b7ent\u00b7wi\u00b7ckelt", "bleibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sch\u00f6pfertrieb, der sich ersch\u00f6pfend \u00fcbertreibt.", "tokens": ["Als", "Sch\u00f6p\u00b7fer\u00b7trieb", ",", "der", "sich", "er\u00b7sch\u00f6p\u00b7fend", "\u00fc\u00b7bert\u00b7reibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}