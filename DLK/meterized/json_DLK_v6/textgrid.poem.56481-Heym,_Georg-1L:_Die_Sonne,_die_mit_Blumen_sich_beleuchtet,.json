{"textgrid.poem.56481": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne, die mit Blumen sich beleuchtet,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, die mit Blumen sich beleuchtet,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "mit", "Blu\u00b7men", "sich", "be\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00f6\u00dft wie ein Aar zu deinen H\u00e4upten weit,", "tokens": ["St\u00f6\u00dft", "wie", "ein", "Aar", "zu", "dei\u00b7nen", "H\u00e4up\u00b7ten", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ihrer Purpurlippen Traum befeuchtet", "tokens": ["Und", "ih\u00b7rer", "Pur\u00b7pur\u00b7lip\u00b7pen", "Traum", "be\u00b7feuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Tr\u00e4nentau dein wei\u00dfes Totenkleid.", "tokens": ["Mit", "Tr\u00e4\u00b7nen\u00b7tau", "dein", "wei\u00b7\u00dfes", "To\u00b7ten\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann nimmst dein Herz du aus den wei\u00dfen Br\u00fcsten", "tokens": ["Dann", "nimmst", "dein", "Herz", "du", "aus", "den", "wei\u00b7\u00dfen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zeigst es rings dem stillen Heiligtum.", "tokens": ["Und", "zeigst", "es", "rings", "dem", "stil\u00b7len", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und deine stolze Flamme r\u00fchrt die K\u00fcsten", "tokens": ["Und", "dei\u00b7ne", "stol\u00b7ze", "Flam\u00b7me", "r\u00fchrt", "die", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Himmels an, die werfen deinen Ruhm", "tokens": ["Des", "Him\u00b7mels", "an", ",", "die", "wer\u00b7fen", "dei\u00b7nen", "Ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ins Meer der Toten aus wie starke Wellen.", "tokens": ["Ins", "Meer", "der", "To\u00b7ten", "aus", "wie", "star\u00b7ke", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gro\u00dfen Schiffe schwimmen um dich her,", "tokens": ["Die", "gro\u00b7\u00dfen", "Schif\u00b7fe", "schwim\u00b7men", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um deinen Turm, und ihre Lieder schwellen", "tokens": ["Um", "dei\u00b7nen", "Turm", ",", "und", "ih\u00b7re", "Lie\u00b7der", "schwel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Abendwolken sanft vom gro\u00dfen Meer.", "tokens": ["Wie", "Ab\u00b7end\u00b7wol\u00b7ken", "sanft", "vom", "gro\u00b7\u00dfen", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und was ich dir in meinen Tr\u00e4umen sage,", "tokens": ["Und", "was", "ich", "dir", "in", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das schrein die Priester aus mit Tuba-Ton.", "tokens": ["Das", "schrein", "die", "Pries\u00b7ter", "aus", "mit", "Tu\u00b7ba\u00b7Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Meere dunkle Buchten f\u00fcllt die Klage", "tokens": ["Der", "Mee\u00b7re", "dunk\u00b7le", "Buch\u00b7ten", "f\u00fcllt", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um dich wie Schilfrohr sanft und schwarzer Mohn.", "tokens": ["Um", "dich", "wie", "Schilf\u00b7rohr", "sanft", "und", "schwar\u00b7zer", "Mohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "KOKOM", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}