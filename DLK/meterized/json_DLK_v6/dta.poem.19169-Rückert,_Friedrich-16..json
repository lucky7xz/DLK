{"dta.poem.19169": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schwalbe die ins Haus, und die am Hause baut,", "tokens": ["Die", "Schwal\u00b7be", "die", "ins", "Haus", ",", "und", "die", "am", "Hau\u00b7se", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "$,", "KON", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind in verschiednem Grad dem Menschen lieb und traut.", "tokens": ["Sind", "in", "ver\u00b7schied\u00b7nem", "Grad", "dem", "Men\u00b7schen", "lieb", "und", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die eine bietet sich zu n\u00e4chstem Nachbarsmann,", "tokens": ["Die", "ei\u00b7ne", "bie\u00b7tet", "sich", "zu", "n\u00e4chs\u00b7tem", "Nach\u00b7bars\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andere sich dir zum Hausgenossen an.", "tokens": ["Die", "an\u00b7de\u00b7re", "sich", "dir", "zum", "Haus\u00b7ge\u00b7nos\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O h\u00e4tt' ich immer, w\u00e4rs vom Himmel mir beschlossen,", "tokens": ["O", "h\u00e4tt'", "ich", "im\u00b7mer", ",", "w\u00e4rs", "vom", "Him\u00b7mel", "mir", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So treue Nachbarn und so fromme Hausgenossen!", "tokens": ["So", "treu\u00b7e", "Nach\u00b7barn", "und", "so", "from\u00b7me", "Haus\u00b7ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}