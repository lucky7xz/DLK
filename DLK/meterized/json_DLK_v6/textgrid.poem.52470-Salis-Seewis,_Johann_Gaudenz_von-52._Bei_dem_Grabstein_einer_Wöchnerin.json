{"textgrid.poem.52470": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "52. Bei dem Grabstein einer W\u00f6chnerin", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, Wandrer, so entkeimt des dunkeln Grabes Nacht", "tokens": ["Sieh", ",", "Wand\u00b7rer", ",", "so", "ent\u00b7keimt", "des", "dun\u00b7keln", "Gra\u00b7bes", "Nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einst Gottes Samenkorn, versenkt im Erdenthale!", "tokens": ["Einst", "Got\u00b7tes", "Sa\u00b7men\u00b7korn", ",", "ver\u00b7senkt", "im", "Er\u00b7dent\u00b7ha\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sprengt den Leichenstein der ew'gen Liebe Macht!", "tokens": ["So", "sprengt", "den", "Lei\u00b7chen\u00b7stein", "der", "ew'\u00b7gen", "Lie\u00b7be", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sprie\u00dft einst Gottes Saat und bricht des Sarges Schale!", "tokens": ["So", "sprie\u00dft", "einst", "Got\u00b7tes", "Saat", "und", "bricht", "des", "Sar\u00b7ges", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So steigt die Lilie aus k\u00fchler Erde Scho\u00df", "tokens": ["So", "steigt", "die", "Li\u00b7lie", "aus", "k\u00fch\u00b7ler", "Er\u00b7de", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und l\u00e4\u00dft ihr Knospenblatt, das Leichentuch, entfallen.", "tokens": ["Und", "l\u00e4\u00dft", "ihr", "Knos\u00b7pen\u00b7blatt", ",", "das", "Lei\u00b7chen\u00b7tuch", ",", "ent\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Engel windet sich vom Grabesschleier los", "tokens": ["Der", "En\u00b7gel", "win\u00b7det", "sich", "vom", "Gra\u00b7bes\u00b7schlei\u00b7er", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und f\u00fchlt das Sterbgewand \u00e4therisch ihn umwallen.", "tokens": ["Und", "f\u00fchlt", "das", "Sterb\u00b7ge\u00b7wand", "\u00e4t\u00b7he\u00b7risch", "ihn", "um\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch du, verwelkter Kelch, gebrochnes Menschenherz,", "tokens": ["Auch", "du", ",", "ver\u00b7welk\u00b7ter", "Kelch", ",", "ge\u00b7broch\u00b7nes", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerknicktes zartes Rohr, zerqu\u00e4lte K\u00f6rperh\u00fclle,", "tokens": ["Zer\u00b7knick\u00b7tes", "zar\u00b7tes", "Rohr", ",", "zer\u00b7qu\u00e4l\u00b7te", "K\u00f6r\u00b7per\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einst hebt ihr euch und bl\u00fcht, wo weder Angst noch Schmerz,", "tokens": ["Einst", "hebt", "ihr", "euch", "und", "bl\u00fcht", ",", "wo", "we\u00b7der", "Angst", "noch", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "$,", "PWAV", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch Tod euch mehr erreicht, in ew'ger Himmelsstille.", "tokens": ["Noch", "Tod", "euch", "mehr", "er\u00b7reicht", ",", "in", "ew'\u00b7ger", "Him\u00b7mels\u00b7stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Knospe, schon zerst\u00f6rt, eh' sie sich uns enth\u00fcllt,", "tokens": ["Die", "Knos\u00b7pe", ",", "schon", "zer\u00b7st\u00f6rt", ",", "eh'", "sie", "sich", "uns", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,", "KOUS", "PPER", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der S\u00e4ugling strebet nicht umsonst mit schwachen H\u00e4nden;", "tokens": ["Der", "S\u00e4ug\u00b7ling", "stre\u00b7bet", "nicht", "um\u00b7sonst", "mit", "schwa\u00b7chen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch seines Schicksals Kreis wird endlich einst erf\u00fcllt,", "tokens": ["Auch", "sei\u00b7nes", "Schick\u00b7sals", "Kreis", "wird", "end\u00b7lich", "einst", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seines Mondes Ring wird hell sich einst vollenden!", "tokens": ["Und", "sei\u00b7nes", "Mon\u00b7des", "Ring", "wird", "hell", "sich", "einst", "voll\u00b7en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "ADJD", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O Wandrer, den das Bild der Auferstehung r\u00fchrt,", "tokens": ["O", "Wand\u00b7rer", ",", "den", "das", "Bild", "der", "Auf\u00b7er\u00b7ste\u00b7hung", "r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du dankst dem K\u00fcnstler, der in diesen Stein es pr\u00e4gte;", "tokens": ["Du", "dankst", "dem", "K\u00fcnst\u00b7ler", ",", "der", "in", "die\u00b7sen", "Stein", "es", "pr\u00e4g\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erheb auch deinen Blick \u2013 zu dem, dem Preis geb\u00fchrt,", "tokens": ["Er\u00b7heb", "auch", "dei\u00b7nen", "Blick", "\u2013", "zu", "dem", ",", "dem", "Preis", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$(", "APPR", "ART", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er die Hoffnung tief in unsre Seele legte!", "tokens": ["Da\u00df", "er", "die", "Hoff\u00b7nung", "tief", "in", "uns\u00b7re", "See\u00b7le", "leg\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}