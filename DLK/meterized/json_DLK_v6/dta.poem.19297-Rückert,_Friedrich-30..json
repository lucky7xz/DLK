{"dta.poem.19297": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie oft geschieht's, da\u00df ich ein Dunkles mir erkl\u00e4re", "tokens": ["Wie", "oft", "ge\u00b7schieht's", ",", "da\u00df", "ich", "ein", "Dunk\u00b7les", "mir", "er\u00b7kl\u00e4\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NE", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch etwas andres, das an sich noch dunkler w\u00e4re.", "tokens": ["Durch", "et\u00b7was", "and\u00b7res", ",", "das", "an", "sich", "noch", "dunk\u00b7ler", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "PRELS", "APPR", "PRF", "ADV", "ADJA", "VAFIN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Doch weil der Forschung Blick ruht auf der dunklen Stelle,", "tokens": ["Doch", "weil", "der", "For\u00b7schung", "Blick", "ruht", "auf", "der", "dunk\u00b7len", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint im Gegensatz ihm jede andre helle.", "tokens": ["Er\u00b7scheint", "im", "Ge\u00b7gen\u00b7satz", "ihm", "je\u00b7de", "and\u00b7re", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PIAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gelang' ich dorthin nun, so ist das R\u00e4thsel dort,", "tokens": ["Ge\u00b7lang'", "ich", "dor\u00b7thin", "nun", ",", "so", "ist", "das", "R\u00e4th\u00b7sel", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Unerkl\u00e4rliche r\u00fcckt mit der Forschung fort.", "tokens": ["Das", "Un\u00b7er\u00b7kl\u00e4r\u00b7li\u00b7che", "r\u00fcckt", "mit", "der", "For\u00b7schung", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und unversehns mach' ich dis neue Dunkel klar", "tokens": ["Und", "un\u00b7ver\u00b7sehns", "mach'", "ich", "dis", "neu\u00b7e", "Dun\u00b7kel", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PDS", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch jenes alte, das erst zu erkl\u00e4ren war.", "tokens": ["Durch", "je\u00b7nes", "al\u00b7te", ",", "das", "erst", "zu", "er\u00b7kl\u00e4\u00b7ren", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es scheint, kein Ausgang ist aus diesem Zauberkreise,", "tokens": ["Es", "scheint", ",", "kein", "Aus\u00b7gang", "ist", "aus", "die\u00b7sem", "Zau\u00b7ber\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIAT", "NN", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald der Geist sich will einlassen auf Beweise.", "tokens": ["So\u00b7bald", "der", "Geist", "sich", "will", "ein\u00b7las\u00b7sen", "auf", "Be\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}