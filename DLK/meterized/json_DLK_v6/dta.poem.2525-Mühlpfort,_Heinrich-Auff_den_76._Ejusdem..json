{"dta.poem.2525": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auff den  76.  Ejusdem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kuhmw\u00fcrdigster Regent/ u&#241; Nestor unsrer Stadt/", "tokens": ["Kuhm\u00b7w\u00fcr\u00b7digs\u00b7ter", "Re\u00b7gent", "/", "u", "&#241;", "Nes\u00b7tor", "uns\u00b7rer", "Stadt", "/"], "token_info": ["word", "word", "punct", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NE", "$(", "NN", "PPOSAT", "NN", "$("], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den GOtt im Alter wil auff Adlers-Fl\u00fcgeln tragen/", "tokens": ["Den", "Gott", "im", "Al\u00b7ter", "wil", "auff", "Ad\u00b7ler\u00b7sF\u00b7l\u00fc\u00b7geln", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df auch die Nach-Welt kan von seinen Wundern sagen/", "tokens": ["Da\u00df", "auch", "die", "Nach\u00b7Welt", "kan", "von", "sei\u00b7nen", "Wun\u00b7dern", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es heist mich Schuld und Pflicht ein ewig Lorber-Blat", "tokens": ["Es", "heist", "mich", "Schuld", "und", "Pflicht", "ein", "e\u00b7wig", "Lor\u00b7ber\u00b7Blat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu opffern diesem Fest/ weil Bre\u00dflau niemals hat", "tokens": ["Zu", "opf\u00b7fern", "die\u00b7sem", "Fest", "/", "weil", "Bre\u00df\u00b7lau", "nie\u00b7mals", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PDAT", "NN", "$(", "KOUS", "NE", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dergleichen graues Haupt bey so verlebten Tagen", "tokens": ["Derg\u00b7lei\u00b7chen", "grau\u00b7es", "Haupt", "bey", "so", "ver\u00b7leb\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und solcher Jahre Zahl auff seinem Ehren-Wagen", "tokens": ["Und", "sol\u00b7cher", "Jah\u00b7re", "Zahl", "auff", "sei\u00b7nem", "Eh\u00b7ren\u00b7Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Freuden angeblickt ", "tokens": ["In", "Freu\u00b7den", "an\u00b7ge\u00b7blickt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Alleine Geist und Muth l\u00e4hmt mein beharrlich krancken/", "tokens": ["Al\u00b7lei\u00b7ne", "Geist", "und", "Muth", "l\u00e4hmt", "mein", "be\u00b7harr\u00b7lich", "kran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "De\u00df Ruhmes Treffligkeit fa\u00dft nicht mein schwacher Kiel/", "tokens": ["De\u00df", "Ruh\u00b7mes", "Tref\u00b7flig\u00b7keit", "fa\u00dft", "nicht", "mein", "schwa\u00b7cher", "Kiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und der Verdienste Prei\u00df/ so ich erzehlen wil/", "tokens": ["Und", "der", "Ver\u00b7diens\u00b7te", "Prei\u00df", "/", "so", "ich", "er\u00b7zeh\u00b7len", "wil", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ADV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Schleust auch die Ewigkeit in ihre graue Schrancken.", "tokens": ["Schleust", "auch", "die", "E\u00b7wig\u00b7keit", "in", "ih\u00b7re", "grau\u00b7e", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Selbst die Verwunderung legt mir Stillschweigen bey/", "tokens": ["Selbst", "die", "Ver\u00b7wun\u00b7de\u00b7rung", "legt", "mir", "Still\u00b7schwei\u00b7gen", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "NN", "APPR", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Und sagt: Da\u00df dieses auch der Ehrfurcht Abri\u00df sey.", "tokens": ["Und", "sagt", ":", "Da\u00df", "die\u00b7ses", "auch", "der", "Ehr\u00b7furcht", "Ab\u00b7ri\u00df", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PDAT", "ADV", "ART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn jetzt der Sonnen Liecht die gantze Welt durch-\nfahren/", "tokens": ["Wenn", "jetzt", "der", "Son\u00b7nen", "Liecht", "die", "gant\u00b7ze", "Welt", "durch", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "TRUNC", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erden Scho\u00df erw\u00e4rmt/ der Berge Haupt erhitzt/", "tokens": ["Der", "Er\u00b7den", "Scho\u00df", "er\u00b7w\u00e4rmt", "/", "der", "Ber\u00b7ge", "Haupt", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch allgemeines Liecht der Menschen Thun gen\u00fctzt/", "tokens": ["Durch", "all\u00b7ge\u00b7mei\u00b7nes", "Liecht", "der", "Men\u00b7schen", "Thun", "ge\u00b7n\u00fctzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sieht man/ wie sie sich mit ihren Purpur-Haaren", "tokens": ["So", "sieht", "man", "/", "wie", "sie", "sich", "mit", "ih\u00b7ren", "Pur\u00b7pur\u00b7Haa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$(", "PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "St\u00fcrtzt in die blaue See: de\u00df Morgenlandes Wahren", "tokens": ["St\u00fcrtzt", "in", "die", "blau\u00b7e", "See", ":", "de\u00df", "Mor\u00b7gen\u00b7lan\u00b7des", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sind nichts f\u00fcr ihrem Glantz; wie hell der Demant blitzt/", "tokens": ["Sind", "nichts", "f\u00fcr", "ih\u00b7rem", "Glantz", ";", "wie", "hell", "der", "De\u00b7mant", "blitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie feurig der Rubin auch seine Funcken spritzt/", "tokens": ["Wie", "feu\u00b7rig", "der", "Ru\u00b7bin", "auch", "sei\u00b7ne", "Fun\u00b7cken", "spritzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NE", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "So k\u00f6nnen sie doch nicht mit ihrer Pracht sich paaren/", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "doch", "nicht", "mit", "ih\u00b7rer", "Pracht", "sich", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie scheint am herrlichsten wenn sie zu Golde geht/", "tokens": ["Sie", "scheint", "am", "herr\u00b7lichs\u00b7ten", "wenn", "sie", "zu", "Gol\u00b7de", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und gr\u00fcst die tr\u00fcbe Welt noch durch der Stralen Blicke.", "tokens": ["Und", "gr\u00fcst", "die", "tr\u00fc\u00b7be", "Welt", "noch", "durch", "der", "Stra\u00b7len", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O Sonne dieser Stadt/ so ist Er auch erh\u00f6ht/", "tokens": ["O", "Son\u00b7ne", "die\u00b7ser", "Stadt", "/", "so", "ist", "Er", "auch", "er\u00b7h\u00f6ht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PDAT", "NN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "De\u00df Lebens Abend gl\u00e4ntzt von Tugend/ Ehr und Gl\u00fccke/", "tokens": ["De\u00df", "Le\u00b7bens", "A\u00b7bend", "gl\u00e4ntzt", "von", "Tu\u00b7gend", "/", "Ehr", "und", "Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Seiu Ehren-Purpur wird in reinster Hoheit stralen", "tokens": ["Sei\u00b7u", "Eh\u00b7ren\u00b7Pur\u00b7pur", "wird", "in", "reins\u00b7ter", "Ho\u00b7heit", "stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "So lange Ph\u00f6bus wird das Rund der Welt bemahlen.", "tokens": ["So", "lan\u00b7ge", "Ph\u00f6\u00b7bus", "wird", "das", "Rund", "der", "Welt", "be\u00b7mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Ph\u00f6nix/ den man sonst der Sonnen Vogel heist/", "tokens": ["Der", "Ph\u00f6\u00b7nix", "/", "den", "man", "sonst", "der", "Son\u00b7nen", "Vo\u00b7gel", "heist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIS", "ADV", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er von Alter schwach/ von Kr\u00e4fften abgezehret/", "tokens": ["Wenn", "er", "von", "Al\u00b7ter", "schwach", "/", "von", "Kr\u00e4ff\u00b7ten", "ab\u00b7ge\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nichts mehr als sein Grab/ den Port der Ruh/ be-", "tokens": ["Und", "nichts", "mehr", "als", "sein", "Grab", "/", "den", "Port", "der", "Ruh", "/", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIS", "PIS", "KOKOM", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$(", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sucht/ wo das Morgenland von W\u00fcrtz und Balsam fleust/", "tokens": ["Sucht", "/", "wo", "das", "Mor\u00b7gen\u00b7land", "von", "W\u00fcrtz", "und", "Bal\u00b7sam", "fleust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhm R\u00e4uchwerck zu der Grufft; der V\u00f6gel Volck das preist", "tokens": ["Jhm", "R\u00e4uch\u00b7werck", "zu", "der", "Grufft", ";", "der", "V\u00f6\u00b7gel", "Volck", "das", "preist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$.", "ART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Hochbegl\u00fcckten Tod/ bald wird sein Wunsch gewehret/", "tokens": ["Den", "Hoch\u00b7be\u00b7gl\u00fcck\u00b7ten", "Tod", "/", "bald", "wird", "sein", "Wunsch", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In dem der Sonnen Glut sein gantzes Nest verheeret/", "tokens": ["In", "dem", "der", "Son\u00b7nen", "Glut", "sein", "gant\u00b7zes", "Nest", "ver\u00b7hee\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und au\u00df der Aschen sich ein junger Ph\u00f6nix weist.", "tokens": ["Und", "au\u00df", "der", "A\u00b7schen", "sich", "ein", "jun\u00b7ger", "Ph\u00f6\u00b7nix", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gewi\u00df/ ", "tokens": ["Ge\u00b7wi\u00df", "/"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$("], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Und Kronen/ so die Treu der B\u00fcrgerschafft ihm flicht/", "tokens": ["Und", "Kro\u00b7nen", "/", "so", "die", "Treu", "der", "B\u00fcr\u00b7ger\u00b7schafft", "ihm", "flicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er Ehr- und Lebensvoll sich wird zum Grabe neigen/", "tokens": ["Er", "Ehr", "und", "Le\u00b7bens\u00b7voll", "sich", "wird", "zum", "Gra\u00b7be", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "ADJD", "PRF", "VAFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sol doch nicht untergehn das angebohrne Licht/", "tokens": ["Sol", "doch", "nicht", "un\u00b7ter\u00b7gehn", "das", "an\u00b7ge\u00b7bohr\u00b7ne", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "VVINF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein Grab und Asche wird noch dermaleins bekr\u00f6nen", "tokens": ["Sein", "Grab", "und", "A\u00b7sche", "wird", "noch", "der\u00b7ma\u00b7leins", "be\u00b7kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}}}}