{"dta.poem.11722": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den ew'gen Faden zieht die Spinn' aus ihrem Leibe;", "tokens": ["Den", "ew'\u00b7gen", "Fa\u00b7den", "zieht", "die", "Spinn'", "aus", "ih\u00b7rem", "Lei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sammlerbiene f\u00fcllt mit fremdem Seim die Scheibe.", "tokens": ["Die", "Samm\u00b7ler\u00b7bie\u00b7ne", "f\u00fcllt", "mit", "frem\u00b7dem", "Seim", "die", "Schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Spinnweb' ist Fliegengrab und keines Lebens Labe,", "tokens": ["Spinn\u00b7web'", "ist", "Flie\u00b7gen\u00b7grab", "und", "kei\u00b7nes", "Le\u00b7bens", "La\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die S\u00fc\u00dfigkeit der Welt ist in der Honigwabe.", "tokens": ["Die", "S\u00fc\u00b7\u00dfig\u00b7keit", "der", "Welt", "ist", "in", "der", "Ho\u00b7nig\u00b7wa\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fleug, s\u00fc\u00dfe Poesie, auf Bienenraub von hinnen,", "tokens": ["Fleug", ",", "s\u00fc\u00b7\u00dfe", "Poe\u00b7sie", ",", "auf", "Bie\u00b7nen\u00b7raub", "von", "hin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "NN", "APPR", "ADV", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und la\u00df Philosophie im grauen Netz der Spinnen.", "tokens": ["Und", "la\u00df", "Phi\u00b7lo\u00b7so\u00b7phie", "im", "grau\u00b7en", "Netz", "der", "Spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob die Philosophie die Spinn' im Netze sei,", "tokens": ["Ob", "die", "Phi\u00b7lo\u00b7so\u00b7phie", "die", "Spinn'", "im", "Net\u00b7ze", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob selbst die Fliege drin, das ist nur einerlei.", "tokens": ["Ob", "selbst", "die", "Flie\u00b7ge", "drin", ",", "das", "ist", "nur", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In keinem Falle wird sie fett bei diesem Schmaus,", "tokens": ["In", "kei\u00b7nem", "Fal\u00b7le", "wird", "sie", "fett", "bei", "die\u00b7sem", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ausgesogne Flieg', ob Fliegen saugend aus.", "tokens": ["Ob", "aus\u00b7ge\u00b7sog\u00b7ne", "Flieg'", ",", "ob", "Flie\u00b7gen", "sau\u00b7gend", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "KOUS", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}