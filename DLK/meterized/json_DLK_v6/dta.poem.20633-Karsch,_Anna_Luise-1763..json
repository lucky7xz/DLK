{"dta.poem.20633": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "1763.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Apoll! nimm deine Leyer wieder,                   ", "tokens": ["A\u00b7poll", "!", "nimm", "dei\u00b7ne", "Le\u00b7yer", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPOSAT", "NN", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des Flakkus T\u00f6ne fehlen ihr,", "tokens": ["Des", "Flak\u00b7kus", "T\u00f6\u00b7ne", "feh\u00b7len", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sang im dunklen Walde Lieder", "tokens": ["Er", "sang", "im", "dunk\u00b7len", "Wal\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und vor ihm staunete das Thier.", "tokens": ["Und", "vor", "ihm", "stau\u00b7ne\u00b7te", "das", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die W\u00f6lfinn ging f\u00fcr ihre Jungen", "tokens": ["Die", "W\u00f6l\u00b7finn", "ging", "f\u00fcr", "ih\u00b7re", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Nahrung, und verga\u00df den Raub,", "tokens": ["Nach", "Nah\u00b7rung", ",", "und", "ver\u00b7ga\u00df", "den", "Raub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und horchte was Horaz gesungen,", "tokens": ["Und", "horch\u00b7te", "was", "Ho\u00b7raz", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und nagte, gleich der Ziege, Laub.", "tokens": ["Und", "nag\u00b7te", ",", "gleich", "der", "Zie\u00b7ge", ",", "Laub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Tiger und der L\u00f6we lie\u00dfen", "tokens": ["Der", "Ti\u00b7ger", "und", "der", "L\u00f6\u00b7we", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lange Zeit verfolgtes Reh,", "tokens": ["Ein", "lan\u00b7ge", "Zeit", "ver\u00b7folg\u00b7tes", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00f6rten den gesungnen, s\u00fc\u00dfen,", "tokens": ["Und", "h\u00f6r\u00b7ten", "den", "ge\u00b7sung\u00b7nen", ",", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reizvollen Namen: Lalage.", "tokens": ["Reiz\u00b7vol\u00b7len", "Na\u00b7men", ":", "La\u00b7la\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ich aber kann durch diese Leyer", "tokens": ["Ich", "a\u00b7ber", "kann", "durch", "die\u00b7se", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nicht \u00f6ffnen deines Friedrichs Ohr;", "tokens": ["Nicht", "\u00f6ff\u00b7nen", "dei\u00b7nes", "Fried\u00b7richs", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir stellt der Traum oft Ungeheuer", "tokens": ["Mir", "stellt", "der", "Traum", "oft", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner dunklen Kammer vor.", "tokens": ["In", "mei\u00b7ner", "dunk\u00b7len", "Kam\u00b7mer", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In ihr seufz\u2019 ich oft mittern\u00e4chtlich", "tokens": ["In", "ihr", "seuf\u00b7z'", "ich", "oft", "mit\u00b7ter\u00b7n\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVIMP", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herauf zum nachbarlichen Mond,", "tokens": ["Her\u00b7auf", "zum", "nach\u00b7bar\u00b7li\u00b7chen", "Mond", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df ich dem P\u00f6bel bin ver\u00e4chtlich,", "tokens": ["Da\u00df", "ich", "dem", "P\u00f6\u00b7bel", "bin", "ver\u00b7\u00e4cht\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gold besitzt und besser wohnt.", "tokens": ["Der", "Gold", "be\u00b7sitzt", "und", "bes\u00b7ser", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mich in dem Winkel unterm Dache", "tokens": ["Mich", "in", "dem", "Win\u00b7kel", "un\u00b7term", "Da\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nennt er ein schlechtgebornes Weib;", "tokens": ["Nennt", "er", "ein", "schlecht\u00b7ge\u00b7bor\u00b7nes", "Weib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fordert, da\u00df er vornehm lache,", "tokens": ["Und", "for\u00b7dert", ",", "da\u00df", "er", "vor\u00b7nehm", "la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von mir ein Lied zum Zeitvertreib.", "tokens": ["Von", "mir", "ein", "Lied", "zum", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O helfender Apoll! gesch\u00e4ndet", "tokens": ["O", "hel\u00b7fen\u00b7der", "A\u00b7poll", "!", "ge\u00b7sch\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$.", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wirst du, wenn deine Vaterhand", "tokens": ["Wirst", "du", ",", "wenn", "dei\u00b7ne", "Va\u00b7ter\u00b7hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir nicht die goldnen Saiten sendet,", "tokens": ["Mir", "nicht", "die", "gold\u00b7nen", "Sai\u00b7ten", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die der Sabiner aufgespannt,", "tokens": ["Die", "der", "Sa\u00b7bi\u00b7ner", "auf\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn mich des dritten C\u00e4sars Rechte", "tokens": ["Wenn", "mich", "des", "drit\u00b7ten", "C\u00e4\u00b7sars", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht \u00fcber Gl\u00fcck und P\u00f6bel hebt,", "tokens": ["Nicht", "\u00fc\u00b7ber", "Gl\u00fcck", "und", "P\u00f6\u00b7bel", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weit unter dem Bezirk der N\u00e4chte", "tokens": ["Weit", "un\u00b7ter", "dem", "Be\u00b7zirk", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch, wie der Tiberschwan geschwebt.", "tokens": ["Hoch", ",", "wie", "der", "Ti\u00b7ber\u00b7schwan", "ge\u00b7schwebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}