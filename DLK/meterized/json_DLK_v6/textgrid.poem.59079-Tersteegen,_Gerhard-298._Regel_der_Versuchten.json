{"textgrid.poem.59079": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "298. Regel der Versuchten", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wirst du versucht, bleib abgeschieden", "tokens": ["Wirst", "du", "ver\u00b7sucht", ",", "bleib", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberla\u00df dich Gott mit Frieden!", "tokens": ["Und", "\u00fc\u00b7berl\u00b7a\u00df", "dich", "Gott", "mit", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bist du selbst schuld, meinst du's zu sein,", "tokens": ["Bist", "du", "selbst", "schuld", ",", "meinst", "du's", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "PIS", "PTKZU", "VAINF", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sink bald und still ins Nichts hinein!", "tokens": ["Sink", "bald", "und", "still", "ins", "Nichts", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "APPRART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}