{"textgrid.poem.63030": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, der den S\u00f6hnen und den T\u00f6chtern", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, der den S\u00f6hnen und den T\u00f6chtern", "tokens": ["Freund", ",", "der", "den", "S\u00f6h\u00b7nen", "und", "den", "T\u00f6ch\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der horchenden Germania", "tokens": ["Der", "hor\u00b7chen\u00b7den", "Ger\u00b7ma\u00b7nia"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und allen kommenden Geschlechtern,", "tokens": ["Und", "al\u00b7len", "kom\u00b7men\u00b7den", "Ge\u00b7schlech\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hackbrett und Harmonika", "tokens": ["Die", "Hack\u00b7brett", "und", "Har\u00b7mo\u00b7ni\u00b7ka"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit ihren Ohren nicht vermengen,", "tokens": ["Mit", "ih\u00b7ren", "Oh\u00b7ren", "nicht", "ver\u00b7men\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom schlauen Sohn der Zypria,", "tokens": ["Vom", "schlau\u00b7en", "Sohn", "der", "Zyp\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In hundert reizenden Ges\u00e4ngen,", "tokens": ["In", "hun\u00b7dert", "rei\u00b7zen\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So manchen Schwank erzehlet hat:", "tokens": ["So", "man\u00b7chen", "Schwank", "er\u00b7zeh\u00b7let", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Darf ich an deinen Arm mich h\u00e4ngen,", "tokens": ["Darf", "ich", "an", "dei\u00b7nen", "Arm", "mich", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und dir auf einem Epheublatt", "tokens": ["Und", "dir", "auf", "ei\u00b7nem", "E\u00b7pheu\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von ihm ein rauhes Liedchen blasen?", "tokens": ["Von", "ihm", "ein", "rau\u00b7hes", "Lied\u00b7chen", "bla\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ich borg es aus der goldnen Zeit,", "tokens": ["Ich", "borg", "es", "aus", "der", "gold\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da Lamm und Tieger, Hund und Hasen,", "tokens": ["Da", "Lamm", "und", "Tie\u00b7ger", ",", "Hund", "und", "Ha\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In friedlicher Vertraulichkeit", "tokens": ["In", "fried\u00b7li\u00b7cher", "Ver\u00b7trau\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Aus Tellus gro\u00dfer Sch\u00fcssel a\u00dfen,", "tokens": ["Aus", "Tel\u00b7lus", "gro\u00b7\u00dfer", "Sch\u00fcs\u00b7sel", "a\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und Einfalt und Zufriedenheit", "tokens": ["Und", "Ein\u00b7falt", "und", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die reine Brust des jungen Hirten", "tokens": ["Die", "rei\u00b7ne", "Brust", "des", "jun\u00b7gen", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und seiner schmucken Dirne zierten.", "tokens": ["Und", "sei\u00b7ner", "schmu\u00b7cken", "Dir\u00b7ne", "zier\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "In jener sch\u00f6nen goldnen Zeit", "tokens": ["In", "je\u00b7ner", "sch\u00f6\u00b7nen", "gold\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "War Amor noch ein biedrer Junge;", "tokens": ["War", "A\u00b7mor", "noch", "ein", "bie\u00b7drer", "Jun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Er schnellte freundlich sein Gescho\u00df", "tokens": ["Er", "schnell\u00b7te", "freund\u00b7lich", "sein", "Ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nur auf verwandte Herzen los,", "tokens": ["Nur", "auf", "ver\u00b7wand\u00b7te", "Her\u00b7zen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und jeder Blick und jede Zunge", "tokens": ["Und", "je\u00b7der", "Blick", "und", "je\u00b7de", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Pries seine s\u00fc\u00dfe Zaubermacht:", "tokens": ["Pries", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Zau\u00b7ber\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Denn seine Falkenaugen dr\u00fcckte", "tokens": ["Denn", "sei\u00b7ne", "Fal\u00b7ken\u00b7au\u00b7gen", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Noch nicht der Flor der Mitternacht,", "tokens": ["Noch", "nicht", "der", "Flor", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und seine weissen Schultern schm\u00fcckte", "tokens": ["Und", "sei\u00b7ne", "weis\u00b7sen", "Schul\u00b7tern", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Noch nicht der Fl\u00fcgel falsche Pracht.", "tokens": ["Noch", "nicht", "der", "Fl\u00fc\u00b7gel", "fal\u00b7sche", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Auch trennte nur der Tod die Herzen,", "tokens": ["Auch", "trenn\u00b7te", "nur", "der", "Tod", "die", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die sein geweihtes Band umgab,", "tokens": ["Die", "sein", "ge\u00b7weih\u00b7tes", "Band", "um\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und mancher Greis fuhr unter Scherzen", "tokens": ["Und", "man\u00b7cher", "Greis", "fuhr", "un\u00b7ter", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mit seinem M\u00fctterchen ins Grab.", "tokens": ["Mit", "sei\u00b7nem", "M\u00fct\u00b7ter\u00b7chen", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Denn auch im Bild der sch\u00f6nsten Tage", "tokens": ["Denn", "auch", "im", "Bild", "der", "sch\u00f6ns\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Der Menschheit stehen Sarkophage,", "tokens": ["Der", "Menschheit", "ste\u00b7hen", "Sar\u00b7ko\u00b7pha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und leider oft im Vordergrund.", "tokens": ["Und", "lei\u00b7der", "oft", "im", "Vor\u00b7der\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So fiel in ihrem Lenz Charite,", "tokens": ["So", "fiel", "in", "ih\u00b7rem", "Lenz", "Cha\u00b7ri\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ein M\u00e4dchen, gleich der Aphrodite,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "gleich", "der", "A\u00b7phro\u00b7di\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Wie sie vor Priams Sohne stund.", "tokens": ["Wie", "sie", "vor", "Pri\u00b7ams", "Soh\u00b7ne", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Umsonst drang Thirsis, von den Ketten", "tokens": ["Um\u00b7sonst", "drang", "Thir\u00b7sis", ",", "von", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Der Libitina sie zu retten,", "tokens": ["Der", "Li\u00b7bi\u00b7ti\u00b7na", "sie", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.41": {"text": "Wie Orpheus in den H\u00f6llenschlund.", "tokens": ["Wie", "Or\u00b7pheus", "in", "den", "H\u00f6l\u00b7len\u00b7schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Umsonst war selbst des Amors Z\u00e4hre,", "tokens": ["Um\u00b7sonst", "war", "selbst", "des", "A\u00b7mors", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Die auf der Mutter Busen flo\u00df.", "tokens": ["Die", "auf", "der", "Mut\u00b7ter", "Bu\u00b7sen", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Mein Kind, sprach Venus, hat die Scheere", "tokens": ["Mein", "Kind", ",", "sprach", "Ve\u00b7nus", ",", "hat", "die", "Schee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "NE", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Der f\u00fcrchterlichen Atropos", "tokens": ["Der", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "A\u00b7tro\u00b7pos"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Einmal den Faden abgeschnitten,", "tokens": ["Ein\u00b7mal", "den", "Fa\u00b7den", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.47": {"text": "So kn\u00fcpft kein Gott ihn wieder an. \u2013", "tokens": ["So", "kn\u00fcpft", "kein", "Gott", "ihn", "wie\u00b7der", "an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Nun, sagt er, kann ich nichts erbitten,", "tokens": ["Nun", ",", "sagt", "er", ",", "kann", "ich", "nichts", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "So r\u00e4ch ich mich! \u2013 Gesagt, gethan.", "tokens": ["So", "r\u00e4ch", "ich", "mich", "!", "\u2013", "Ge\u00b7sagt", ",", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "$(", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Er eilet in die dunkeln Zonen,", "tokens": ["Er", "ei\u00b7let", "in", "die", "dun\u00b7keln", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Wo die drey Spinnerinnen thronen,", "tokens": ["Wo", "die", "drey", "Spin\u00b7ne\u00b7rin\u00b7nen", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Die des Geschickes M\u00e4gde sind.", "tokens": ["Die", "des", "Ge\u00b7schi\u00b7ckes", "M\u00e4g\u00b7de", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Nach vielen Knicksen, vielen Gr\u00fc\u00dfen", "tokens": ["Nach", "vie\u00b7len", "Knick\u00b7sen", ",", "vie\u00b7len", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Von der Mama, schleicht sich das Kind", "tokens": ["Von", "der", "Ma\u00b7ma", ",", "schleicht", "sich", "das", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.55": {"text": "Zur Atropos. Mit hundert K\u00fcssen", "tokens": ["Zur", "A\u00b7tro\u00b7pos", ".", "Mit", "hun\u00b7dert", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Macht er die graue Vettel zahm,", "tokens": ["Macht", "er", "die", "grau\u00b7e", "Vet\u00b7tel", "zahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.57": {"text": "Und als sie eine Prise nahm,", "tokens": ["Und", "als", "sie", "ei\u00b7ne", "Pri\u00b7se", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Erwischt er schnell die blanke Scheere", "tokens": ["Er\u00b7wischt", "er", "schnell", "die", "blan\u00b7ke", "Schee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.59": {"text": "Und wirft sie in den Erebus.", "tokens": ["Und", "wirft", "sie", "in", "den", "E\u00b7re\u00b7bus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Er flieht. Doch schnell erschien die M\u00e4hre", "tokens": ["Er", "flieht", ".", "Doch", "schnell", "er\u00b7schien", "die", "M\u00e4h\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "In dem Olymp. Saturnius", "tokens": ["In", "dem", "O\u00b7lymp", ".", "Sa\u00b7tur\u00b7nius"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Berief die himmlischen Magnaten,", "tokens": ["Be\u00b7rief", "die", "himm\u00b7li\u00b7schen", "Mag\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.63": {"text": "Um sich mit ihnen zu berathen,", "tokens": ["Um", "sich", "mit", "ih\u00b7nen", "zu", "be\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Und alle schrieen voll Verdru\u00df:", "tokens": ["Und", "al\u00b7le", "schri\u00b7een", "voll", "Ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Erh\u00f6hen wir die stillen Freuden", "tokens": ["Er\u00b7h\u00f6\u00b7hen", "wir", "die", "stil\u00b7len", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Des Menschen durch Unsterblichkeit,", "tokens": ["Des", "Men\u00b7schen", "durch", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.67": {"text": "So w\u00fcrden seine Seligkeit", "tokens": ["So", "w\u00fcr\u00b7den", "sei\u00b7ne", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Die G\u00f6tter selbst mit Recht beneiden! \u2013", "tokens": ["Die", "G\u00f6t\u00b7ter", "selbst", "mit", "Recht", "be\u00b7nei\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "Nur Atropos schwieg zu dem Streit,", "tokens": ["Nur", "A\u00b7tro\u00b7pos", "schwieg", "zu", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.70": {"text": "Mit ihren Ferien zufrieden,", "tokens": ["Mit", "ih\u00b7ren", "Fe\u00b7ri\u00b7en", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.71": {"text": "Und Zevs war auch noch unentschieden,", "tokens": ["Und", "Zevs", "war", "auch", "noch", "un\u00b7ent\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.72": {"text": "Als Charon, eisgrau, wie die Zeit,", "tokens": ["Als", "Cha\u00b7ron", ",", "eis\u00b7grau", ",", "wie", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Und mit Alektos Wuth im Blicke,", "tokens": ["Und", "mit", "A\u00b7lek\u00b7tos", "Wuth", "im", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "APPRART", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.74": {"text": "In die lazurne Halle drang.", "tokens": ["In", "die", "la\u00b7zur\u00b7ne", "Hal\u00b7le", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.75": {"text": "Wie lang, o Vater der Geschicke,", "tokens": ["Wie", "lang", ",", "o", "Va\u00b7ter", "der", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.76": {"text": "Rief er aus heiserm Hals, wie lang", "tokens": ["Rief", "er", "aus", "hei\u00b7serm", "Hals", ",", "wie", "lang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.77": {"text": "Schweigst du zu Amors Bubenst\u00fccke?", "tokens": ["Schweigst", "du", "zu", "A\u00b7mors", "Bu\u00b7ben\u00b7st\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.78": {"text": "Schon sieben Tage bringt der Tod", "tokens": ["Schon", "sie\u00b7ben", "Ta\u00b7ge", "bringt", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.79": {"text": "Mir keine Schatten aus dem Lande", "tokens": ["Mir", "kei\u00b7ne", "Schat\u00b7ten", "aus", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.80": {"text": "Der Sterblichkeit. Mein leckes Boot", "tokens": ["Der", "Sterb\u00b7lich\u00b7keit", ".", "Mein", "le\u00b7ckes", "Boot"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.81": {"text": "Liegt m\u00fc\u00dfig an dem \u00f6den Strande;", "tokens": ["Liegt", "m\u00fc\u00b7\u00dfig", "an", "dem", "\u00f6\u00b7den", "Stran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.82": {"text": "Und ich, Herr Zevs, bin ohne Brod.", "tokens": ["Und", "ich", ",", "Herr", "Zevs", ",", "bin", "oh\u00b7ne", "Brod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "NE", "$,", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.83": {"text": "Auf einmal braust es wie ein Wetter", "tokens": ["Auf", "ein\u00b7mal", "braust", "es", "wie", "ein", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.84": {"text": "Durch den Olymp. Das Chor der G\u00f6tter", "tokens": ["Durch", "den", "O\u00b7lymp", ".", "Das", "Chor", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.85": {"text": "Kreischt des Matrosen Klage nach;", "tokens": ["Kreischt", "des", "Mat\u00b7ro\u00b7sen", "Kla\u00b7ge", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.86": {"text": "Und Zevs, statt ihn zum Stubenheitzer,", "tokens": ["Und", "Zevs", ",", "statt", "ihn", "zum", "Stu\u00b7ben\u00b7heit\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "KOUI", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.87": {"text": "Zum Bratenwender, oder Schweizer", "tokens": ["Zum", "Bra\u00b7ten\u00b7wen\u00b7der", ",", "o\u00b7der", "Schwei\u00b7zer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "NN"], "meter": "-+-+-+