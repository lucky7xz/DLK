{"textgrid.poem.33094": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leise wie ein Traumgesicht", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise wie ein Traumgesicht", "tokens": ["Lei\u00b7se", "wie", "ein", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt Erinn'rung mich umfangen,", "tokens": ["H\u00e4lt", "Er\u00b7inn'\u00b7rung", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise, wie die Morgenluft", "tokens": ["Lei\u00b7se", ",", "wie", "die", "Mor\u00b7gen\u00b7luft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir umspielet Stirn und Wangen.", "tokens": ["Mir", "um\u00b7spie\u00b7let", "Stirn", "und", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der klare, blaue See", "tokens": ["Und", "der", "kla\u00b7re", ",", "blau\u00b7e", "See"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt mich an wie Menschenaugen,", "tokens": ["Blickt", "mich", "an", "wie", "Men\u00b7schen\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich m\u00f6chte tief hinab", "tokens": ["Da\u00df", "ich", "m\u00f6ch\u00b7te", "tief", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich in seine Fluthen tauchen.", "tokens": ["Mich", "in", "sei\u00b7ne", "Flut\u00b7hen", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Alpenspitzen Glanz", "tokens": ["Und", "der", "Al\u00b7pen\u00b7spit\u00b7zen", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt mich an wie Menschenherzen,", "tokens": ["Blickt", "mich", "an", "wie", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so schroff und eisig kalt", "tokens": ["Die", "so", "schroff", "und", "ei\u00b7sig", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lohnen dem mit tausend Schmerzen,", "tokens": ["Loh\u00b7nen", "dem", "mit", "tau\u00b7send", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der sich ihnen froh genaht,", "tokens": ["Der", "sich", "ih\u00b7nen", "froh", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da im ros'gen Alpengl\u00fchen", "tokens": ["Da", "im", "ros'\u00b7gen", "Al\u00b7pen\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, von fremdem Licht umstrahlt,", "tokens": ["Sie", ",", "von", "frem\u00b7dem", "Licht", "um\u00b7strahlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schienen lebenswarm zu bl\u00fchen.", "tokens": ["Schie\u00b7nen", "le\u00b7bens\u00b7warm", "zu", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, Erinn'rung! flieh' hinweg", "tokens": ["O", ",", "Er\u00b7inn'\u00b7rung", "!", "flieh'", "hin\u00b7weg"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den falschen Alpenh\u00f6hen,", "tokens": ["Von", "den", "fal\u00b7schen", "Al\u00b7pen\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasche in der blauen Fluth", "tokens": ["Wa\u00b7sche", "in", "der", "blau\u00b7en", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich gesund von allen Wehen!", "tokens": ["Dich", "ge\u00b7sund", "von", "al\u00b7len", "We\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Such' in ihrem feuchten Glanz", "tokens": ["Such'", "in", "ih\u00b7rem", "feuch\u00b7ten", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Augen treue Klarheit,", "tokens": ["Je\u00b7ner", "Au\u00b7gen", "treu\u00b7e", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die du frei noch lieben kannst,", "tokens": ["Die", "du", "frei", "noch", "lie\u00b7ben", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die stets dir blickten Wahrheit!", "tokens": ["Und", "die", "stets", "dir", "blick\u00b7ten", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}