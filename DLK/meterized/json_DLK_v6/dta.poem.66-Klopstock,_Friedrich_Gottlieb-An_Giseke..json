{"dta.poem.66": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "An Giseke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Geh! ich reisse mich los, obgleich die m\u00e4nnliche", "tokens": ["Geh", "!", "ich", "reis\u00b7se", "mich", "los", ",", "ob\u00b7gleich", "die", "m\u00e4nn\u00b7li\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nicht die Thr\u00e4ne verbeut,", "tokens": ["Nicht", "die", "Thr\u00e4\u00b7ne", "ver\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Geh! ich weine nicht, Freund. Ich m\u00fc\u00dfte mein Le-", "tokens": ["Geh", "!", "ich", "wei\u00b7ne", "nicht", ",", "Freund", ".", "Ich", "m\u00fc\u00df\u00b7te", "mein", "Le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "TRUNC"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Weint\u2019 ich dir, Giseke, nach!", "tokens": ["Weint'", "ich", "dir", ",", "Gi\u00b7se\u00b7ke", ",", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NE", "$,", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn so werden sie alle dahingehn, jeder den andern", "tokens": ["Denn", "so", "wer\u00b7den", "sie", "al\u00b7le", "da\u00b7hin\u00b7gehn", ",", "je\u00b7der", "den", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,", "PIS", "ART", "ADJA"], "meter": "--+--+-+-+++-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Traurend verlassen, und fliehn.", "tokens": ["Trau\u00b7rend", "ver\u00b7las\u00b7sen", ",", "und", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Also trennet der Tod gew\u00e4hlte Gatten! der Mann", "tokens": ["Al\u00b7so", "tren\u00b7net", "der", "Tod", "ge\u00b7w\u00e4hl\u00b7te", "Gat\u00b7ten", "!", "der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "ART", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Seufzend im Ocean um,", "tokens": ["Seuf\u00b7zend", "im", "O\u00b7cean", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Sie am Gestad, wo von Todtengeripp, und Schei-", "tokens": ["Sie", "am", "Ge\u00b7stad", ",", "wo", "von", "Tod\u00b7ten\u00b7ge\u00b7ripp", ",", "und", "Schei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPRART", "NN", "$,", "PWAV", "APPR", "NE", "$,", "KON", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "St\u00fcrme das Grab ihr erh\u00f6hn.", "tokens": ["St\u00fcr\u00b7me", "das", "Grab", "ihr", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "So liegt Miltons Gebein von Homers Gebeine ge-", "tokens": ["So", "liegt", "Mil\u00b7tons", "Ge\u00b7bein", "von", "Ho\u00b7mers", "Ge\u00b7bei\u00b7ne", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN", "APPR", "NE", "NN", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und der Cypresse verweht", "tokens": ["Und", "der", "Cyp\u00b7res\u00b7se", "ver\u00b7weht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ihre Klag\u2019 am Grabe des Einen, und kommt nicht", "tokens": ["Ih\u00b7re", "Klag'", "am", "Gra\u00b7be", "des", "Ei\u00b7nen", ",", "und", "kommt", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PTKNEG"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Nach des Anderen Gruft.", "tokens": ["Nach", "des", "An\u00b7de\u00b7ren", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "So schrieb unser aller Verh\u00e4ngni\u00df auf eherne Ta-", "tokens": ["So", "schrieb", "un\u00b7ser", "al\u00b7ler", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "auf", "e\u00b7her\u00b7ne", "Ta"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Der im Himmel, und schwieg.", "tokens": ["Der", "im", "Him\u00b7mel", ",", "und", "schwieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Was der Hocherhabene schrieb, verehr\u2019 ich im Staube,", "tokens": ["Was", "der", "Ho\u00b7cher\u00b7ha\u00b7be\u00b7ne", "schrieb", ",", "ver\u00b7ehr'", "ich", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Weine gen Himmel nicht auf.", "tokens": ["Wei\u00b7ne", "gen", "Him\u00b7mel", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Geh, mein Theurer! Es letzen vielleicht sich unsere", "tokens": ["Geh", ",", "mein", "Theu\u00b7rer", "!", "Es", "let\u00b7zen", "viel\u00b7leicht", "sich", "un\u00b7se\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "PRF", "PPOSAT"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Auch ohne Thr\u00e4nen mit dir;", "tokens": ["Auch", "oh\u00b7ne", "Thr\u00e4\u00b7nen", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Wenn nicht Thr\u00e4nen die Seele vergie\u00dft, unweinbar", "tokens": ["Wenn", "nicht", "Thr\u00e4\u00b7nen", "die", "See\u00b7le", "ver\u00b7gie\u00dft", ",", "un\u00b7wein\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PTKNEG", "NN", "ART", "NN", "VVFIN", "$,", "ADJD"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Sanften edlen Gef\u00fchls.", "tokens": ["Sanf\u00b7ten", "ed\u00b7len", "Ge\u00b7f\u00fchls", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Eile zu Hagedorn hin, und hast du genung ihn um", "tokens": ["Ei\u00b7le", "zu", "Ha\u00b7ge\u00b7dorn", "hin", ",", "und", "hast", "du", "ge\u00b7nung", "ihn", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "PTKVZ", "$,", "KON", "VAFIN", "PPER", "ADV", "PPER", "APPR"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Ist euch die erste Begier,", "tokens": ["Ist", "euch", "die", "ers\u00b7te", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Euch zu sehen, gestillt, sind alle Thr\u00e4nen der Freu", "tokens": ["Euch", "zu", "se\u00b7hen", ",", "ge\u00b7stillt", ",", "sind", "al\u00b7le", "Thr\u00e4\u00b7nen", "der", "Freu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVPP", "$,", "VAFIN", "PIAT", "NN", "ART", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Weggel\u00e4chelt entflohn,", "tokens": ["Weg\u00b7ge\u00b7l\u00e4\u00b7chelt", "ent\u00b7flohn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Giseke, sag\u2019 ihm alsdann, nach drey genossenen T", "tokens": ["Gi\u00b7se\u00b7ke", ",", "sag'", "ihm", "als\u00b7dann", ",", "nach", "drey", "ge\u00b7nos\u00b7se\u00b7nen", "T"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "$,", "APPR", "CARD", "ADJA", "NE"], "meter": "+----+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Da\u00df ich ihn liebe, wie du!", "tokens": ["Da\u00df", "ich", "ihn", "lie\u00b7be", ",", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}