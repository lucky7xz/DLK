{"textgrid.poem.63291": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Emsig dreht sich meine Spule,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Emsig dreht sich meine Spule,", "tokens": ["Em\u00b7sig", "dreht", "sich", "mei\u00b7ne", "Spu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer zur Musik bereit,", "tokens": ["Im\u00b7mer", "zur", "Mu\u00b7sik", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Denn ich bin die Harfenjule,", "tokens": ["Denn", "ich", "bin", "die", "Har\u00b7fen\u00b7ju\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon seit meiner Kinderzeit.", "tokens": ["Schon", "seit", "mei\u00b7ner", "Kin\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Niemand schl\u00e4gt wie ich die Saiten,", "tokens": ["Nie\u00b7mand", "schl\u00e4gt", "wie", "ich", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand hat wie ich Gewalt.", "tokens": ["Nie\u00b7mand", "hat", "wie", "ich", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst die wilden Tiere schreiten", "tokens": ["Selbst", "die", "wil\u00b7den", "Tie\u00b7re", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft wie L\u00e4mmer durch den Wald.", "tokens": ["Sanft", "wie", "L\u00e4m\u00b7mer", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich schlage meine Harfe,", "tokens": ["Und", "ich", "schla\u00b7ge", "mei\u00b7ne", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo und wie es immer sei,", "tokens": ["Wo", "und", "wie", "es", "im\u00b7mer", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zum Familienbedarfe,", "tokens": ["Zum", "Fa\u00b7mi\u00b7li\u00b7en\u00b7be\u00b7dar\u00b7fe", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kindstauf' oder Rauferei.", "tokens": ["Kind\u00b7stauf'", "o\u00b7der", "Rau\u00b7fe\u00b7rei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Reich mir einer eine Halbe", "tokens": ["Reich", "mir", "ei\u00b7ner", "ei\u00b7ne", "Hal\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder einen Groschen nur,", "tokens": ["O\u00b7der", "ei\u00b7nen", "Gro\u00b7schen", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als des Sommers letzte Schwalbe", "tokens": ["Als", "des", "Som\u00b7mers", "letz\u00b7te", "Schwal\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebe ich durch die Natur.", "tokens": ["Schwe\u00b7be", "ich", "durch", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so dreht sich meine Spule,", "tokens": ["Und", "so", "dreht", "sich", "mei\u00b7ne", "Spu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief vom Innersten bewegt,", "tokens": ["Tief", "vom", "In\u00b7ners\u00b7ten", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Bis die alte Harfenjule", "tokens": ["Bis", "die", "al\u00b7te", "Har\u00b7fen\u00b7ju\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst im Himmel Harfe schl\u00e4gt.", "tokens": ["Einst", "im", "Him\u00b7mel", "Har\u00b7fe", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}