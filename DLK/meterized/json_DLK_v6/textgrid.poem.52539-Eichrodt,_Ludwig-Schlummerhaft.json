{"textgrid.poem.52539": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schlummerhaft", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder an dem sp\u00e4ten Abend", "tokens": ["Wie\u00b7der", "an", "dem", "sp\u00e4\u00b7ten", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieg ich auf dem Kanape,", "tokens": ["Lieg", "ich", "auf", "dem", "Ka\u00b7na\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Welt ist schlafengangen,", "tokens": ["Al\u00b7le", "Welt", "ist", "schla\u00b7fen\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab ein Lied mir angefangen,", "tokens": ["Hab", "ein", "Lied", "mir", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4ume von der hohen See.", "tokens": ["Tr\u00e4u\u00b7me", "von", "der", "ho\u00b7hen", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo auf heimgewandtem Schiffe", "tokens": ["Wo", "auf", "heim\u00b7ge\u00b7wand\u00b7tem", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein verliebter Dichter sitzt,", "tokens": ["Ein", "ver\u00b7lieb\u00b7ter", "Dich\u00b7ter", "sitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die stummen Wolken jagen,", "tokens": ["Wo", "die", "stum\u00b7men", "Wol\u00b7ken", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die sanften Wellen schagen,", "tokens": ["Wo", "die", "sanf\u00b7ten", "Wel\u00b7len", "scha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wos am Himmel ferne blitzt.", "tokens": ["Wos", "am", "Him\u00b7mel", "fer\u00b7ne", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo der Mond am Meeressaume", "tokens": ["Wo", "der", "Mond", "am", "Mee\u00b7res\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die gelbe Rose bl\u00fcht \u2013", "tokens": ["Wie", "die", "gel\u00b7be", "Ro\u00b7se", "bl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles in bewegter Stille,", "tokens": ["Al\u00b7les", "in", "be\u00b7weg\u00b7ter", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Seele sonder Wille", "tokens": ["Wo", "die", "See\u00b7le", "son\u00b7der", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich erschlie\u00dfet zu dem Lied \u2013", "tokens": ["Sich", "er\u00b7schlie\u00b7\u00dfet", "zu", "dem", "Lied", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ich in enger Stube", "tokens": ["A\u00b7ber", "ich", "in", "en\u00b7ger", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicke beim Gedanken ein,", "tokens": ["Ni\u00b7cke", "beim", "Ge\u00b7dan\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er sich zum Traum verflechte,", "tokens": ["Da\u00df", "er", "sich", "zum", "Traum", "ver\u00b7flech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die schwarze Zeit der N\u00e4chte", "tokens": ["Und", "die", "schwar\u00b7ze", "Zeit", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fclle mit belebtem Schein.", "tokens": ["F\u00fcl\u00b7le", "mit", "be\u00b7leb\u00b7tem", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}