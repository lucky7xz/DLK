{"textgrid.poem.52927": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frei ist die Kunst, allein, f\u00fcrwahr! nicht frei wie ein Nomadenzelt,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frei ist die Kunst, allein, f\u00fcrwahr! nicht frei wie ein Nomadenzelt,", "tokens": ["Frei", "ist", "die", "Kunst", ",", "al\u00b7lein", ",", "f\u00fcr\u00b7wahr", "!", "nicht", "frei", "wie", "ein", "No\u00b7ma\u00b7den\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "$,", "ADV", "$.", "PTKNEG", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "++-+-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Das man vom Isarstrande flugs hier an der Spree Gestaden stellt;", "tokens": ["Das", "man", "vom", "Is\u00b7ar\u00b7stran\u00b7de", "flugs", "hier", "an", "der", "Spree", "Ge\u00b7sta\u00b7den", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPRART", "NN", "ADV", "ADV", "APPR", "ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Sie schie\u00dft, ein stolzes Prachtgew\u00e4chs, aus jeder Scholle nicht hervor", "tokens": ["Sie", "schie\u00dft", ",", "ein", "stol\u00b7zes", "Pracht\u00b7ge\u00b7w\u00e4chs", ",", "aus", "je\u00b7der", "Schol\u00b7le", "nicht", "her\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und ist so gut wie K\u00f6nigtum eine Von-Gottes-Gnaden-Welt.", "tokens": ["Und", "ist", "so", "gut", "wie", "K\u00f6\u00b7nig\u00b7tum", "ei\u00b7ne", "Von\u00b7Got\u00b7tes\u00b7Gna\u00b7den\u00b7Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Soldaten lassen sich zur Not erziehen und Geheime R\u00e4t',", "tokens": ["Sol\u00b7da\u00b7ten", "las\u00b7sen", "sich", "zur", "Not", "er\u00b7zie\u00b7hen", "und", "Ge\u00b7hei\u00b7me", "R\u00e4t'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Wenn nur die rechte Meisterhand den goldnen Puppenfaden h\u00e4lt.", "tokens": ["Wenn", "nur", "die", "rech\u00b7te", "Meis\u00b7ter\u00b7hand", "den", "gold\u00b7nen", "Pup\u00b7pen\u00b7fa\u00b7den", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Doch Dichter wuchern nicht empor, wo man verlegnen Samen streut,", "tokens": ["Doch", "Dich\u00b7ter", "wu\u00b7chern", "nicht", "em\u00b7por", ",", "wo", "man", "ver\u00b7leg\u00b7nen", "Sa\u00b7men", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PWAV", "PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Zumal wenn er in Sand herab und steinig-harten Pfaden f\u00e4llt;", "tokens": ["Zu\u00b7mal", "wenn", "er", "in", "Sand", "her\u00b7ab", "und", "stei\u00b7nig\u00b7har\u00b7ten", "Pfa\u00b7den", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "ADV", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Und Maler lieben nicht zu gehn, wo ihnen rings auf jedem Schritt", "tokens": ["Und", "Ma\u00b7ler", "lie\u00b7ben", "nicht", "zu", "gehn", ",", "wo", "ih\u00b7nen", "rings", "auf", "je\u00b7dem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Ein kritisch-lautes K\u00f6terlein heimt\u00fcckisch um die Waden bellt;", "tokens": ["Ein", "kri\u00b7tischlau\u00b7tes", "K\u00f6\u00b7ter\u00b7lein", "heim\u00b7t\u00fc\u00b7ckisch", "um", "die", "Wa\u00b7den", "bellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und Philosophen denken nicht, wenn das profane Marktgeschrei", "tokens": ["Und", "Phi\u00b7lo\u00b7so\u00b7phen", "den\u00b7ken", "nicht", ",", "wenn", "das", "pro\u00b7fa\u00b7ne", "Markt\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Allt\u00e4glich um ihr stilles Haus und die verschlo\u00dfnen Laden gellt;", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "um", "ihr", "stil\u00b7les", "Haus", "und", "die", "ver\u00b7schlo\u00df\u00b7nen", "La\u00b7den", "gellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Spielleute endlich schweigen bald, wenn P\u00f6belwahn und Fr\u00f6mmelei", "tokens": ["Spiel\u00b7leu\u00b7te", "end\u00b7lich", "schwei\u00b7gen", "bald", ",", "wenn", "P\u00f6\u00b7bel\u00b7wahn", "und", "Fr\u00f6m\u00b7me\u00b7lei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "ADV", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.14": {"text": "Auf ihre liederreiche Brust schwer wie ein gift'ger Schwaden f\u00e4llt.", "tokens": ["Auf", "ih\u00b7re", "lie\u00b7der\u00b7rei\u00b7che", "Brust", "schwer", "wie", "ein", "gift'\u00b7ger", "Schwa\u00b7den", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.15": {"text": "Nein, R\u00fcben und Kadetten zieht, Kartoffeln und Magister gro\u00df,", "tokens": ["Nein", ",", "R\u00fc\u00b7ben", "und", "Ka\u00b7det\u00b7ten", "zieht", ",", "Kar\u00b7tof\u00b7feln", "und", "Ma\u00b7gis\u00b7ter", "gro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "VVFIN", "$,", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Da\u00df schadlos solche Ernte Euch f\u00fcr Eurer K\u00fcnste Schaden h\u00e4lt!", "tokens": ["Da\u00df", "schad\u00b7los", "sol\u00b7che", "Ern\u00b7te", "Euch", "f\u00fcr", "Eu\u00b7rer", "K\u00fcns\u00b7te", "Scha\u00b7den", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}