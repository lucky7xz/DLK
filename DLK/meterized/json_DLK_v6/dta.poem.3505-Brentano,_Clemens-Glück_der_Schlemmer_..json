{"dta.poem.3505": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck der Schlemmer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Baum in Oesterreich,               ", "tokens": ["Es", "steht", "ein", "Baum", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der tr\u00e4gt Muskaten Blumen;", "tokens": ["Der", "tr\u00e4gt", "Mus\u00b7ka\u00b7ten", "Blu\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die erste Blume, die er trug,", "tokens": ["Die", "ers\u00b7te", "Blu\u00b7me", ",", "die", "er", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die brach ein's K\u00f6nigs Tochter.", "tokens": ["Die", "brach", "ein's", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}