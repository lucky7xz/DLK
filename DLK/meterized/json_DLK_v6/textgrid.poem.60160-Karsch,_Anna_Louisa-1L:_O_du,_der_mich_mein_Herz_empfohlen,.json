{"textgrid.poem.60160": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, der mich mein Herz empfohlen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der mich mein Herz empfohlen,", "tokens": ["O", "du", ",", "der", "mich", "mein", "Herz", "emp\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich dir sagen wer ich bin?", "tokens": ["Soll", "ich", "dir", "sa\u00b7gen", "wer", "ich", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Weib, die niemahls sich erstohlen", "tokens": ["Ein", "Weib", ",", "die", "nie\u00b7mahls", "sich", "er\u00b7stoh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Schmeicheley, Gunst und Gewinn.", "tokens": ["Durch", "Schmei\u00b7che\u00b7ley", ",", "Gunst", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "W\u00fcst ich mit goldner M\u00fcnzen Haufen,", "tokens": ["W\u00fcst", "ich", "mit", "gold\u00b7ner", "M\u00fcn\u00b7zen", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein F\u00fcrstenthum und seine Pracht", "tokens": ["Ein", "F\u00fcrs\u00b7ten\u00b7thum", "und", "sei\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch eine L\u00fcge zu erkaufen;", "tokens": ["Durch", "ei\u00b7ne", "L\u00fc\u00b7ge", "zu", "er\u00b7kau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00e4be keiner auf mich acht", "tokens": ["Und", "g\u00e4\u00b7be", "kei\u00b7ner", "auf", "mich", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPER", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als nur mein Herz mit Richterblicken:", "tokens": ["Als", "nur", "mein", "Herz", "mit", "Rich\u00b7ter\u00b7bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So tr\u00fcg ich f\u00fcr dem Herzen Scheu:", "tokens": ["So", "tr\u00fcg", "ich", "f\u00fcr", "dem", "Her\u00b7zen", "Scheu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich liesse mich den Mangel dr\u00fccken,", "tokens": ["Ich", "lies\u00b7se", "mich", "den", "Man\u00b7gel", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bliebe ganz der Wahrheit treu.", "tokens": ["Und", "blie\u00b7be", "ganz", "der", "Wahr\u00b7heit", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Wahrheit spricht im Sonnenlichte:", "tokens": ["Die", "Wahr\u00b7heit", "spricht", "im", "Son\u00b7nen\u00b7lich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist ein Gott der alles sieht;", "tokens": ["Es", "ist", "ein", "Gott", "der", "al\u00b7les", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der vor sein allgemein Gerichte", "tokens": ["Der", "vor", "sein", "all\u00b7ge\u00b7mein", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch unbekannte L\u00fcgner zieht.", "tokens": ["Auch", "un\u00b7be\u00b7kann\u00b7te", "L\u00fcg\u00b7ner", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn der Gott in seinem Grimme", "tokens": ["Und", "wenn", "der", "Gott", "in", "sei\u00b7nem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch nicht der Falschheit R\u00e4cher w\u00e4r;", "tokens": ["Auch", "nicht", "der", "Falschheit", "R\u00e4\u00b7cher", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So fiele mir doch schon die Stimme", "tokens": ["So", "fie\u00b7le", "mir", "doch", "schon", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tugend, in der Seele schwer.", "tokens": ["Der", "Tu\u00b7gend", ",", "in", "der", "See\u00b7le", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Wahrheit Stimme will ich brauchen,", "tokens": ["Der", "Wahr\u00b7heit", "Stim\u00b7me", "will", "ich", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und solt ich meinen Bissen Brodt,", "tokens": ["Und", "solt", "ich", "mei\u00b7nen", "Bis\u00b7sen", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Salz bestreut in E\u00dfig tauchen,", "tokens": ["Mit", "Salz", "be\u00b7streut", "in", "E\u00b7\u00dfig", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bliebe sie mein gr\u00f6\u00dft Geboth.", "tokens": ["So", "blie\u00b7be", "sie", "mein", "gr\u00f6\u00dft", "Ge\u00b7both", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie hie\u00df mir Friedrichs Siege singen;", "tokens": ["Sie", "hie\u00df", "mir", "Fried\u00b7richs", "Sie\u00b7ge", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollten seine Feinde mich", "tokens": ["Und", "woll\u00b7ten", "sei\u00b7ne", "Fein\u00b7de", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu andern Th\u00f6nen grausam zwingen,", "tokens": ["Zu", "an\u00b7dern", "Th\u00f6\u00b7nen", "grau\u00b7sam", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch s\u00e4ng ich sterbend Friederich.", "tokens": ["Doch", "s\u00e4ng", "ich", "ster\u00b7bend", "Frie\u00b7de\u00b7rich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}