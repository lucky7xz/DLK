{"textgrid.poem.47371": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zu Lessings Denkmal", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder Deutsche, wenn er Lessing nennen h\u00f6ret, f\u00fchle Stolz;", "tokens": ["Je\u00b7der", "Deut\u00b7sche", ",", "wenn", "er", "Les\u00b7sing", "nen\u00b7nen", "h\u00f6\u00b7ret", ",", "f\u00fch\u00b7le", "Stolz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "NN", "VVINF", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der, der Bildung Baum zu pflanzen, ausgereutet faules Holz.", "tokens": ["Der", ",", "der", "Bil\u00b7dung", "Baum", "zu", "pflan\u00b7zen", ",", "aus\u00b7ge\u00b7reu\u00b7tet", "fau\u00b7les", "Holz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "NE", "PTKZU", "VVINF", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Deutschen Geistes spr\u00f6des Erz mit m\u00e4nnlicher Begeist'rung schmolz,", "tokens": ["Deut\u00b7schen", "Geis\u00b7tes", "spr\u00f6\u00b7des", "Erz", "mit", "m\u00e4nn\u00b7li\u00b7cher", "Be\u00b7geist'\u00b7rung", "schmolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und wohin er immer zielte, stets ins Schwarze scho\u00df den Bolz.", "tokens": ["Und", "wo\u00b7hin", "er", "im\u00b7mer", "ziel\u00b7te", ",", "stets", "ins", "Schwar\u00b7ze", "scho\u00df", "den", "Bolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ihm ein Denkmal zu errichten braucht es nicht, Er hat's gethan;", "tokens": ["Ihm", "ein", "Denk\u00b7mal", "zu", "er\u00b7rich\u00b7ten", "braucht", "es", "nicht", ",", "Er", "hat's", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber wie wir ihm verpflichtet uns erkennen, zeig' es an:", "tokens": ["A\u00b7ber", "wie", "wir", "ihm", "ver\u00b7pflich\u00b7tet", "uns", "er\u00b7ken\u00b7nen", ",", "zeig'", "es", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Er hat eingeschlagen, die wir wollen gehn, der Forschung Bahn,", "tokens": ["Er", "hat", "ein\u00b7ge\u00b7schla\u00b7gen", ",", "die", "wir", "wol\u00b7len", "gehn", ",", "der", "For\u00b7schung", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und zum Ziel der Wahrheit, das wir suchen, ging er uns voran.", "tokens": ["Und", "zum", "Ziel", "der", "Wahr\u00b7heit", ",", "das", "wir", "su\u00b7chen", ",", "ging", "er", "uns", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Er zuerst hat unser Wesen fremder Fessel frei gemacht", "tokens": ["Er", "zu\u00b7erst", "hat", "un\u00b7ser", "We\u00b7sen", "frem\u00b7der", "Fes\u00b7sel", "frei", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und zu Ehren vor Europas Augen unser Volk gebracht:", "tokens": ["Und", "zu", "Eh\u00b7ren", "vor", "Eu\u00b7ro\u00b7pas", "Au\u00b7gen", "un\u00b7ser", "Volk", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NE", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Drum, solang in uns Gef\u00fchl der Ehre, Mut der Freiheit wacht,", "tokens": ["Drum", ",", "so\u00b7lang", "in", "uns", "Ge\u00b7f\u00fchl", "der", "Eh\u00b7re", ",", "Mut", "der", "Frei\u00b7heit", "wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VMFIN", "APPR", "PPER", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Als Befreiers, Ehrenw\u00e4chters, sei, o Lessing, dein gedacht.", "tokens": ["Als", "Be\u00b7frei\u00b7ers", ",", "Eh\u00b7ren\u00b7w\u00e4ch\u00b7ters", ",", "sei", ",", "o", "Les\u00b7sing", ",", "dein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "VAFIN", "$,", "FM", "NE", "$,", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}