{"dta.poem.2367": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Hertzeleid!", "tokens": ["Ach", "Hert\u00b7ze\u00b7leid", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dieweil die Zeit", "tokens": ["Die\u00b7weil", "die", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nicht einmal wil verg\u00f6nnen/", "tokens": ["Nicht", "ein\u00b7mal", "wil", "ver\u00b7g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00dfich bey dir/", "tokens": ["Da\u00b7\u00dfich", "bey", "dir", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein\u2019 \u00e4dle ", "tokens": ["Mein'", "\u00e4d\u00b7le"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Mag eine Gunst gewinnen.", "tokens": ["Mag", "ei\u00b7ne", "Gunst", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00df ich meine Herde Vieh", "tokens": ["So", "la\u00df", "ich", "mei\u00b7ne", "Her\u00b7de", "Vieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Wald\u2019 alleine weiden/", "tokens": ["Jm", "Wald'", "al\u00b7lei\u00b7ne", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und bin betr\u00fcbet je und je", "tokens": ["Und", "bin", "be\u00b7tr\u00fc\u00b7bet", "je", "und", "je"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich leb\u2019 in stetem ", "tokens": ["Ich", "leb'", "in", "ste\u00b7tem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Es ist mir lauter Hertzverdru\u00df", "tokens": ["Es", "ist", "mir", "lau\u00b7ter", "Hertz\u00b7ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df ich die Sonne schauen mu\u00df.", "tokens": ["Da\u00df", "ich", "die", "Son\u00b7ne", "schau\u00b7en", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der sch\u00f6nste Wald", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Wald"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist ungestalt/", "tokens": ["Ist", "un\u00b7ge\u00b7stalt", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ich d\u00e4nk\u2019 allzeit", "tokens": ["Ich", "d\u00e4nk'", "all\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Bald setz\u2019 ich mich danieder/", "tokens": ["Bald", "setz'", "ich", "mich", "da\u00b7nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und klage meine Liebesnoht/", "tokens": ["Und", "kla\u00b7ge", "mei\u00b7ne", "Lie\u00b7bes\u00b7noht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit bittren heissen ", "tokens": ["Mit", "bit\u00b7tren", "heis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "VVINF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Bald w\u00fcnsch\u2019 ich mir den herben ", "tokens": ["Bald", "w\u00fcn\u00b7sch'", "ich", "mir", "den", "her\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit einem tieffen ", "tokens": ["Mit", "ei\u00b7nem", "tief\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ich geh\u2019 ich sitz\u2019/ ich schlaff\u2019 und wach", "tokens": ["Ich", "geh'", "ich", "sitz'", "/", "ich", "schlaff'", "und", "wach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich lebe doch in Weh und Ach.", "tokens": ["Ich", "le\u00b7be", "doch", "in", "Weh", "und", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die\u00df geschicht/", "tokens": ["Und", "die\u00df", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein liebstes ", "tokens": ["Mein", "liebs\u00b7tes"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Weil ich dir nicht kan sagen", "tokens": ["Weil", "ich", "dir", "nicht", "kan", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was grossen Schmertz/", "tokens": ["Was", "gros\u00b7sen", "Schmertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein treues Hertz/", "tokens": ["Mein", "treu\u00b7es", "Hertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "bi\u00dfhero hat getragen", "tokens": ["bi\u00df\u00b7he\u00b7ro", "hat", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Weil ich betr\u00fcbt-verliebter Mann/", "tokens": ["Weil", "ich", "be\u00b7tr\u00fcb\u00b7tver\u00b7lieb\u00b7ter", "Mann", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir sch\u00f6nsten ", "tokens": ["Dir", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Mein Hertze nicht entdekken kan/", "tokens": ["Mein", "Hert\u00b7ze", "nicht", "ent\u00b7dek\u00b7ken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und meinen treuen Willen/", "tokens": ["Und", "mei\u00b7nen", "treu\u00b7en", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die\u00df die\u00df macht mir bey dieser ", "tokens": ["Die\u00df", "die\u00df", "macht", "mir", "bey", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PDS", "VVFIN", "PPER", "APPR", "PDAT"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ein immerwerend Hertzeleid.", "tokens": ["Ein", "im\u00b7mer\u00b7we\u00b7rend", "Hert\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ich dich seh\u2019", "tokens": ["Wenn", "ich", "dich", "seh'"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jm gr\u00fcnen ", "tokens": ["Jm", "gr\u00fc\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dein Wollen viehchen treiben", "tokens": ["Dein", "Wol\u00b7len", "vieh\u00b7chen", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So kan ich kaum/", "tokens": ["So", "kan", "ich", "kaum", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Jm weiten Raum\u2019/", "tokens": ["Jm", "wei\u00b7ten", "Raum'", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auf meinen Wiesen bleiben/", "tokens": ["Auf", "mei\u00b7nen", "Wie\u00b7sen", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich jage meine Schafe fort", "tokens": ["Ich", "ja\u00b7ge", "mei\u00b7ne", "Scha\u00b7fe", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu deinen fetten Gr\u00fcnden/", "tokens": ["Zu", "dei\u00b7nen", "fet\u00b7ten", "Gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Erdenk' im treiben manches Wort", "tokens": ["Er\u00b7denk'", "im", "trei\u00b7ben", "man\u00b7ches", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Welchs deinen Sinn soll binden.", "tokens": ["Welchs", "dei\u00b7nen", "Sinn", "soll", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So bald ich aber bey dir bin/", "tokens": ["So", "bald", "ich", "a\u00b7ber", "bey", "dir", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ist alle Rednerkunst dahin.", "tokens": ["Ist", "al\u00b7le", "Red\u00b7ner\u00b7kunst", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich werde taub", "tokens": ["Ich", "wer\u00b7de", "taub"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zittr' als ein Laub/", "tokens": ["Zit\u00b7tr'", "als", "ein", "Laub", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Rede wil nicht fliessen/", "tokens": ["Die", "Re\u00b7de", "wil", "nicht", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es m\u00f6chte sich", "tokens": ["Es", "m\u00f6ch\u00b7te", "sich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Bald mildiglich", "tokens": ["Bald", "mil\u00b7dig\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Augenstrohm ergiessen;", "tokens": ["Der", "Au\u00b7gen\u00b7strohm", "er\u00b7gies\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Reden sch\u00f6nste Sch\u00e4ferinn", "tokens": ["Dein", "Re\u00b7den", "sch\u00f6ns\u00b7te", "Sch\u00e4\u00b7fe\u00b7rinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dein so s\u00fcsses Lachen/\nDie\n\nPoetisch- und Musikalisches Lust-", "tokens": ["Und", "dein", "so", "s\u00fcs\u00b7ses", "La\u00b7chen", "/", "Die", "Po\u00b7e\u00b7tisch", "und", "Mu\u00b7si\u00b7ka\u00b7li\u00b7sches", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN", "$(", "ART", "TRUNC", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+---+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Die k\u00f6nnen ", "tokens": ["Die", "k\u00f6n\u00b7nen"], "token_info": ["word", "word"], "pos": ["PDS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Bald fesselfeste machen.", "tokens": ["Bald", "fes\u00b7sel\u00b7fes\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Doch treuer Liebe Hertzenskrafft/", "tokens": ["Doch", "treu\u00b7er", "Lie\u00b7be", "Hert\u00b7zen\u00b7skrafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hat allzeit solch ein\u2019 ", "tokens": ["Hat", "all\u00b7zeit", "solch", "ein'"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ach wenn doch mir", "tokens": ["Ach", "wenn", "doch", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das wiederf\u00fchr", "tokens": ["Das", "wie\u00b7der\u00b7f\u00fchr"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein\u2019 H\u00e4ndchen nur zu k\u00fcssen", "tokens": ["Dein'", "H\u00e4nd\u00b7chen", "nur", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wolt\u2019 ich das", "tokens": ["So", "wolt'", "ich", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit Thr\u00e4nen na\u00df", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "na\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Schon zu befeuchten wissen.", "tokens": ["Schon", "zu", "be\u00b7feuch\u00b7ten", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Dann wolt' ich gerne stille seyn", "tokens": ["Dann", "wolt'", "ich", "ger\u00b7ne", "stil\u00b7le", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mich nicht so beklagen/", "tokens": ["Und", "mich", "nicht", "so", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Weil dir ein Seuftzer meine Pein", "tokens": ["Weil", "dir", "ein", "Seuft\u00b7zer", "mei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+",