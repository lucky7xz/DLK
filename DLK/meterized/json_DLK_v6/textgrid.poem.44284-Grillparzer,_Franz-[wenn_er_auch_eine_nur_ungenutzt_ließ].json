{"textgrid.poem.44284": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn er auch eine nur ungenutzt lie\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1804, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn er auch eine nur ungenutzt lie\u00df,", "tokens": ["Wenn", "er", "auch", "ei\u00b7ne", "nur", "un\u00b7ge\u00b7nutzt", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ihr nicht Spenden wie andern entri\u00df,", "tokens": ["Und", "ihr", "nicht", "Spen\u00b7den", "wie", "an\u00b7dern", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "NN", "KOKOM", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach zu sp\u00e4t, ach zu sp\u00e4t!", "tokens": ["Ach", "zu", "sp\u00e4t", ",", "ach", "zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKA", "ADJD", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Alles was sch\u00f6n ist und edel und gut,", "tokens": ["Al\u00b7les", "was", "sch\u00f6n", "ist", "und", "e\u00b7del", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "ADJD", "VAFIN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Zeuget nur rascher und m\u00e4nnlicher Mut,", "tokens": ["Zeu\u00b7get", "nur", "ra\u00b7scher", "und", "m\u00e4nn\u00b7li\u00b7cher", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und die Zeit, und die Zeit!", "tokens": ["Und", "die", "Zeit", ",", "und", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Freunde, drum haltet die Fliehenden auf,", "tokens": ["Freun\u00b7de", ",", "drum", "hal\u00b7tet", "die", "Flie\u00b7hen\u00b7den", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Flei\u00df nur und Tugend bez\u00e4hmt ihren Lauf,", "tokens": ["Flei\u00df", "nur", "und", "Tu\u00b7gend", "be\u00b7z\u00e4hmt", "ih\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Haltet sie, haltet sie!", "tokens": ["Hal\u00b7tet", "sie", ",", "hal\u00b7tet", "sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}