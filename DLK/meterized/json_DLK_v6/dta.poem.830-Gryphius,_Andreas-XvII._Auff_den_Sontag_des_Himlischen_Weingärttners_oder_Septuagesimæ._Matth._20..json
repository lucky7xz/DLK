{"dta.poem.830": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  Auff den Sontag des Himlischen  \n Weing\u00e4rttners/ oder  Septuagesim\u00e6.  \n Matth. 20.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der H\u00f6chste rufft vns von dem Marckt der Welt/ ", "tokens": ["Der", "H\u00f6chs\u00b7te", "rufft", "vns", "von", "dem", "Marckt", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In den Weinberg den sein Sohn hat mit Schwei\u00df vnd", "tokens": ["In", "den", "Wein\u00b7berg", "den", "sein", "Sohn", "hat", "mit", "Schwei\u00df", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Blut genetzet/ ", "tokens": ["Blut", "ge\u00b7net\u00b7zet", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Den Er vnabl\u00f6\u00dflich bawt/ der so werth vor jhm gesch\u00e4tzet", "tokens": ["Den", "Er", "vnab\u00b7l\u00f6\u00df\u00b7lich", "bawt", "/", "der", "so", "werth", "vor", "jhm", "ge\u00b7sch\u00e4t\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$(", "ART", "ADV", "ADJD", "APPR", "PPER", "VVPP"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Da\u00df Er sich selbst darumb in Todt gestelt", "tokens": ["Da\u00df", "Er", "sich", "selbst", "da\u00b7rumb", "in", "Todt", "ge\u00b7stelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PAV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wir sind die/ den m\u00fcssig-gehn gef\u00e4lt", "tokens": ["Doch", "wir", "sind", "die", "/", "den", "m\u00fcs\u00b7sig\u00b7gehn", "ge\u00b7f\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "$(", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vnsers ersten Morgenslicht ward mit nichts-thun hing\u1ebd-", "tokens": ["Vn\u00b7sers", "ers\u00b7ten", "Mor\u00b7gen\u00b7slicht", "ward", "mit", "nichts\u00b7thun", "hin\u00b7g\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "setzet: ", "tokens": ["set\u00b7zet", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Jtzt nun vn\u00df der Mittag druckt/ hat vns Hitz\u2019 vnd last ver-", "tokens": ["Jtzt", "nun", "vn\u00df", "der", "Mit\u00b7tag", "druckt", "/", "hat", "vns", "Hitz'", "vnd", "last", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$(", "VAFIN", "PPER", "NE", "KON", "VVFIN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "letzet ", "tokens": ["let\u00b7zet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Vnd was noch mehr von fleissig-seyn abh\u00e4lt.", "tokens": ["Vnd", "was", "noch", "mehr", "von", "fleis\u00b7sig\u00b7seyn", "ab\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auff Menschen auff! gebt acht auff ewre sachen", "tokens": ["Auff", "Men\u00b7schen", "auff", "!", "gebt", "acht", "auff", "ew\u00b7re", "sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$.", "VVFIN", "CARD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht bricht an/ der Todt wil abend machen.", "tokens": ["Die", "Nacht", "bricht", "an", "/", "der", "Todt", "wil", "a\u00b7bend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denckt wie werden wir bestehen/ wenn GOtt selbst wird", "tokens": ["Denckt", "wie", "wer\u00b7den", "wir", "be\u00b7ste\u00b7hen", "/", "wenn", "Gott", "selbst", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "VAFIN", "PPER", "VVFIN", "$(", "KOUS", "NN", "ADV", "VAFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Rechnung hegen? ", "tokens": ["Rech\u00b7nung", "he\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Er siht zwar den vnd mehr denn gn\u00e4dig an/", "tokens": ["Er", "siht", "zwar", "den", "vnd", "mehr", "denn", "gn\u00e4\u00b7dig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "KON", "ADV", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der eine stund jhm fleissig dienen kan", "tokens": ["Der", "ei\u00b7ne", "stund", "jhm", "fleis\u00b7sig", "die\u00b7nen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch Er heist auch von Jhm gehn/ die/ die seinen grim\u2019erreg\u1ebd.", "tokens": ["Doch", "Er", "heist", "auch", "von", "Jhm", "gehn", "/", "die", "/", "die", "sei\u00b7nen", "grim'\u00b7er\u00b7reg\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$(", "ART", "$(", "ART", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}