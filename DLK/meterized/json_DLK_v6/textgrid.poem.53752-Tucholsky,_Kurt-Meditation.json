{"textgrid.poem.53752": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Meditation", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chtest du, mein Goldkind, einen Knaben?", "tokens": ["M\u00f6ch\u00b7test", "du", ",", "mein", "Gold\u00b7kind", ",", "ei\u00b7nen", "Kna\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier im Buche steht, da\u00df man bestimmt", "tokens": ["Hier", "im", "Bu\u00b7che", "steht", ",", "da\u00df", "man", "be\u00b7stimmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "sie so kriegen kann, wie man sie haben", "tokens": ["sie", "so", "krie\u00b7gen", "kann", ",", "wie", "man", "sie", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$,", "PWAV", "PIS", "PPER", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "will, wenn mans methodisch unternimmt.", "tokens": ["will", ",", "wenn", "mans", "me\u00b7tho\u00b7disch", "un\u00b7ter\u00b7nimmt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Gott, ein Junge hat ja viel auf Erden.", "tokens": ["Gott", ",", "ein", "Jun\u00b7ge", "hat", "ja", "viel", "auf", "Er\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Er wird mannbar und Regierungsrat.", "tokens": ["Er", "wird", "mann\u00b7bar", "und", "Re\u00b7gie\u00b7rungs\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Geht es schief, dann kann er Richter werden", "tokens": ["Geht", "es", "schief", ",", "dann", "kann", "er", "Rich\u00b7ter", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "NN", "VAINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "oder gutgesinnter Demokrat.", "tokens": ["o\u00b7der", "gut\u00b7ge\u00b7sinn\u00b7ter", "De\u00b7mo\u00b7krat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Halt! Tus nicht! Du nimmst da eine N\u00e4hrpflicht", "tokens": ["Halt", "!", "Tus", "nicht", "!", "Du", "nimmst", "da", "ei\u00b7ne", "N\u00e4hr\u00b7pflicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "NE", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "auf dich ohne jeden Hoffnungsstrahl.", "tokens": ["auf", "dich", "oh\u00b7ne", "je\u00b7den", "Hoff\u00b7nungs\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bald hat Deutschland seine alte Wehrpflicht,", "tokens": ["Bald", "hat", "Deutschland", "sei\u00b7ne", "al\u00b7te", "Wehr\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und dann wird er Menschenmaterial.", "tokens": ["und", "dann", "wird", "er", "Men\u00b7schen\u00b7ma\u00b7te\u00b7ri\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Oder m\u00f6chtest du eine Knabine \u2013?", "tokens": ["O\u00b7der", "m\u00f6ch\u00b7test", "du", "ei\u00b7ne", "Kna\u00b7bi\u00b7ne", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "$(", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Abdenitten? Immer so dewest?", "tokens": ["Ab\u00b7de\u00b7nit\u00b7ten", "?", "Im\u00b7mer", "so", "de\u00b7west", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sucht ein Bankdirektor nach Titine,", "tokens": ["Sucht", "ein", "Bank\u00b7di\u00b7rek\u00b7tor", "nach", "Ti\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "erntest du doch, was du heute s\u00e4est.", "tokens": ["ern\u00b7test", "du", "doch", ",", "was", "du", "heu\u00b7te", "s\u00e4\u00b7est", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Halt! Tus nicht! Mit f\u00fcnfundf\u00fcnfzig Jahren", "tokens": ["Halt", "!", "Tus", "nicht", "!", "Mit", "f\u00fcn\u00b7fund\u00b7f\u00fcnf\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "NE", "PTKNEG", "$.", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "da verknallt sie sich in den Chauffeur.", "tokens": ["da", "ver\u00b7knallt", "sie", "sich", "in", "den", "Chauf\u00b7feur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Scheidung, Krach, Trag\u00f6die . . . wir ersparen", "tokens": ["Schei\u00b7dung", ",", "Krach", ",", "Tra\u00b7g\u00f6\u00b7die", ".", ".", ".", "wir", "er\u00b7spa\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$.", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ihr und ihm wohl lieber das Malheur.", "tokens": ["ihr", "und", "ihm", "wohl", "lie\u00b7ber", "das", "Mal\u00b7heur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ja, was nun? Ich bleibe gern im Ruhstand.", "tokens": ["Ja", ",", "was", "nun", "?", "Ich", "blei\u00b7be", "gern", "im", "Ruh\u00b7stand", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kriege keine! La\u00df sie lieber da!", "tokens": ["Krie\u00b7ge", "kei\u00b7ne", "!", "La\u00df", "sie", "lie\u00b7ber", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$.", "VVIMP", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df es ruhig bei dem alten Zustand!", "tokens": ["La\u00df", "es", "ru\u00b7hig", "bei", "dem", "al\u00b7ten", "Zu\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und bleib kinderlos!", "tokens": ["Und", "bleib", "kin\u00b7der\u00b7los", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie dein Papa!", "tokens": ["Wie", "dein", "Pa\u00b7pa", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}