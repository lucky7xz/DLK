{"textgrid.poem.41626": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem Schauspielhaus voll abgeschmackter Pracht,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Schauspielhaus voll abgeschmackter Pracht,", "tokens": ["In", "ei\u00b7nem", "Schau\u00b7spiel\u00b7haus", "voll", "ab\u00b7ge\u00b7schmack\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das der L\u00e4rm des Orchesters durchgellte,", "tokens": ["Das", "der", "L\u00e4rm", "des", "Or\u00b7ches\u00b7ters", "durch\u00b7gell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sah ich, wie eine Fee aus tiefer H\u00f6llennacht", "tokens": ["Sah", "ich", ",", "wie", "ei\u00b7ne", "Fee", "aus", "tie\u00b7fer", "H\u00f6l\u00b7len\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein wundersam Fr\u00fchrot erhellte;", "tokens": ["Ein", "wun\u00b7der\u00b7sam", "Fr\u00fch\u00b7rot", "er\u00b7hell\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "In einem Schauspielhaus voll abgeschmackter Pracht", "tokens": ["In", "ei\u00b7nem", "Schau\u00b7spiel\u00b7haus", "voll", "ab\u00b7ge\u00b7schmack\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sah ich, wie ein Gesch\u00f6pf, das Licht war, Gold und Gaze,", "tokens": ["Sah", "ich", ",", "wie", "ein", "Ge\u00b7sch\u00f6pf", ",", "das", "Licht", "war", ",", "Gold", "und", "Ga\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den riesigen Satan bezwang;", "tokens": ["Den", "rie\u00b7si\u00b7gen", "Sa\u00b7tan", "be\u00b7zwang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch mein Herz, das nie gel\u00f6st wird in Ekstase,", "tokens": ["Je\u00b7doch", "mein", "Herz", ",", "das", "nie", "ge\u00b7l\u00f6st", "wird", "in", "Eks\u00b7ta\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist ein Theater, das endlos lang", "tokens": ["Ist", "ein", "The\u00b7a\u00b7ter", ",", "das", "end\u00b7los", "lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf das Gesch\u00f6pf harrt mit dem Fl\u00fcgelpaar von Gaze.", "tokens": ["Auf", "das", "Ge\u00b7sch\u00f6pf", "harrt", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7paar", "von", "Ga\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}