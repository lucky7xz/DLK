{"textgrid.poem.58641": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lauf hin und her, o fromme Seel', in vielem \u00e4u\u00dfern \u00dcben;", "tokens": ["Lauf", "hin", "und", "her", ",", "o", "from\u00b7me", "Seel'", ",", "in", "vie\u00b7lem", "\u00e4u\u00b7\u00dfern", "\u00dc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,", "APPR", "PIS", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "So sind die besten Kinder nicht, die unser Gott will lieben!", "tokens": ["So", "sind", "die", "bes\u00b7ten", "Kin\u00b7der", "nicht", ",", "die", "un\u00b7ser", "Gott", "will", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Du lebst zerstreut in dies und das, gest\u00f6ret immerdar,", "tokens": ["Du", "lebst", "zer\u00b7streut", "in", "dies", "und", "das", ",", "ge\u00b7st\u00f6\u00b7ret", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "PDS", "KON", "PDS", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Drum wirst du Gott und Gottes Werk nicht in dir selbst gewahr.", "tokens": ["Drum", "wirst", "du", "Gott", "und", "Got\u00b7tes", "Werk", "nicht", "in", "dir", "selbst", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "KON", "NN", "NN", "PTKNEG", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Die guten Kinder bleiben gern daheim bei Gott im Herzen,", "tokens": ["Die", "gu\u00b7ten", "Kin\u00b7der", "blei\u00b7ben", "gern", "da\u00b7heim", "bei", "Gott", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und wenn sie laufen hin und her, so kriegen sie nur Schmerzen.", "tokens": ["Und", "wenn", "sie", "lau\u00b7fen", "hin", "und", "her", ",", "so", "krie\u00b7gen", "sie", "nur", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Mein Herr und Vater, hol mich heim und halt mich Nacht und Tag,", "tokens": ["Mein", "Herr", "und", "Va\u00b7ter", ",", "hol", "mich", "heim", "und", "halt", "mich", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Da\u00df ich noch als dein liebes Kind dir stets gefallen mag!", "tokens": ["Da\u00df", "ich", "noch", "als", "dein", "lie\u00b7bes", "Kind", "dir", "stets", "ge\u00b7fal\u00b7len", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}