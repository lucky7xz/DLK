{"textgrid.poem.28443": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der zu dem Todten sagt: Steh auff und lebe wieder!", "tokens": ["Der", "zu", "dem", "Tod\u00b7ten", "sagt", ":", "Steh", "auff", "und", "le\u00b7be", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der kan auch sagen dem, der lebt: Geh, leg dich nieder!", "tokens": ["Der", "kan", "auch", "sa\u00b7gen", "dem", ",", "der", "lebt", ":", "Geh", ",", "leg", "dich", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVFIN", "ART", "$,", "PRELS", "VVFIN", "$.", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was trotzet dann ein Mensch, der sterblich ist wie wir?", "tokens": ["Was", "trot\u00b7zet", "dann", "ein", "Mensch", ",", "der", "sterb\u00b7lich", "ist", "wie", "wir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist nur um ein Wort, so ist er mehr nicht hier.", "tokens": ["Es", "ist", "nur", "um", "ein", "Wort", ",", "so", "ist", "er", "mehr", "nicht", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}