{"textgrid.poem.40649": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob dir die Brust unst\u00e4t und st\u00fcrmisch schwelle", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob dir die Brust unst\u00e4t und st\u00fcrmisch schwelle", "tokens": ["Ob", "dir", "die", "Brust", "un\u00b7st\u00e4t", "und", "st\u00fcr\u00b7misch", "schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich jenem Meer im rauhen Nordensunde,", "tokens": ["Gleich", "je\u00b7nem", "Meer", "im", "rau\u00b7hen", "Nor\u00b7den\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl kommt dir einst solch seltne gute Stunde,", "tokens": ["Wohl", "kommt", "dir", "einst", "solch", "selt\u00b7ne", "gu\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohl bl\u00fcht auch dir noch jene heil'ge Stelle.", "tokens": ["Wohl", "bl\u00fcht", "auch", "dir", "noch", "je\u00b7ne", "heil'\u00b7ge", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verbrausen la\u00df' der Leidenschaften Welle,", "tokens": ["Ver\u00b7brau\u00b7sen", "la\u00df'", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sie verdeckt, wird dir zu neuem Funde;", "tokens": ["Was", "sie", "ver\u00b7deckt", ",", "wird", "dir", "zu", "neu\u00b7em", "Fun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein mild Vergessen schlie\u00dfe deine Wunde,", "tokens": ["Ein", "mild", "Ver\u00b7ges\u00b7sen", "schlie\u00b7\u00dfe", "dei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe dein Umw\u00f6lktes dir erhelle.", "tokens": ["Die", "Lie\u00b7be", "dein", "Um\u00b7w\u00f6lk\u00b7tes", "dir", "er\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und still in dir, so still und klar soll's werden,", "tokens": ["Und", "still", "in", "dir", ",", "so", "still", "und", "klar", "soll's", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df bis zum Grund der Seele du kannst sehen,", "tokens": ["Da\u00df", "bis", "zum", "Grund", "der", "See\u00b7le", "du", "kannst", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "APPRART", "NN", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann senke dich in deiner Brust Verlie\u00dfe!", "tokens": ["Dann", "sen\u00b7ke", "dich", "in", "dei\u00b7ner", "Brust", "Ver\u00b7lie\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist kein Herz so krank und arm auf Erden,", "tokens": ["Es", "ist", "kein", "Herz", "so", "krank", "und", "arm", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem dort nicht Palmen noch des Friedens stehen", "tokens": ["Dem", "dort", "nicht", "Pal\u00b7men", "noch", "des", "Frie\u00b7dens", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PTKNEG", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und St\u00fccke bl\u00fchn versunkner Paradiese.", "tokens": ["Und", "St\u00fc\u00b7cke", "bl\u00fchn", "ver\u00b7sun\u00b7kner", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}