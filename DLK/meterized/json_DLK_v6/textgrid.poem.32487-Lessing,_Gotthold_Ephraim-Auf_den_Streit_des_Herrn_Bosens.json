{"textgrid.poem.32487": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Streit des Herrn Bosens", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hat den Pabst gelobt, und wir, zu Luthers Ehre,", "tokens": ["Er", "hat", "den", "Pabst", "ge\u00b7lobt", ",", "und", "wir", ",", "zu", "Lu\u00b7thers", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "KON", "PPER", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sollten ihn nicht schelten?", "tokens": ["Wir", "soll\u00b7ten", "ihn", "nicht", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Pabst, den Pabst gelobt? Wenns noch der Teufel w\u00e4re", "tokens": ["Den", "Pabst", ",", "den", "Pabst", "ge\u00b7lobt", "?", "Wenns", "noch", "der", "Teu\u00b7fel", "w\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$.", "KOUS", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lie\u00dfen wir es gelten.", "tokens": ["So", "lie\u00b7\u00dfen", "wir", "es", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}