{"textgrid.poem.48096": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Ohne Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Fu\u00df zertrat den Veilchenstrau\u00df,", "tokens": ["Dein", "Fu\u00df", "zer\u00b7trat", "den", "Veil\u00b7chen\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den eine Kindeshand gewunden,", "tokens": ["den", "ei\u00b7ne", "Kin\u00b7des\u00b7hand", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ins Leben st\u00fcrmtest du hinaus;", "tokens": ["ins", "Le\u00b7ben", "st\u00fcrm\u00b7test", "du", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "hoch stieg dein Stern, \u2013 im eignen Haus", "tokens": ["hoch", "stieg", "dein", "Stern", ",", "\u2013", "im", "eig\u00b7nen", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "$(", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "nur hast du nie das Gl\u00fcck gefunden.", "tokens": ["nur", "hast", "du", "nie", "das", "Gl\u00fcck", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nun dein Stolz in Scherben bricht,", "tokens": ["Und", "nun", "dein", "Stolz", "in", "Scher\u00b7ben", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 was liegt daran, ob selbst verschuldet, \u2013", "tokens": ["\u2013", "was", "liegt", "da\u00b7ran", ",", "ob", "selbst", "ver\u00b7schul\u00b7det", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PAV", "$,", "KOUS", "ADV", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "nun tr\u00f6stet dich kein traut Gesicht:", "tokens": ["nun", "tr\u00f6s\u00b7tet", "dich", "kein", "traut", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du nahmst ins Haus die Liebe nicht,", "tokens": ["du", "nahmst", "ins", "Haus", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die alles hofft und tr\u00e4gt und duldet.", "tokens": ["die", "al\u00b7les", "hofft", "und", "tr\u00e4gt", "und", "dul\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O, w\u00e4r der Weg nicht meilenweit,", "tokens": ["O", ",", "w\u00e4r", "der", "Weg", "nicht", "mei\u00b7len\u00b7weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nicht alle Br\u00fccken abgebrochen,", "tokens": ["nicht", "al\u00b7le", "Br\u00fc\u00b7cken", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich st\u00e4nd noch heute dir zur Seit.", "tokens": ["ich", "st\u00e4nd", "noch", "heu\u00b7te", "dir", "zur", "Seit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit einem Gru\u00df der Jugendzeit", "tokens": ["mit", "ei\u00b7nem", "Gru\u00df", "der", "Ju\u00b7gend\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "an deine trotzge Brust zu pochen.", "tokens": ["an", "dei\u00b7ne", "trotz\u00b7ge", "Brust", "zu", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "doch unausf\u00fcllbar g\u00e4hnt die Kluft,", "tokens": ["doch", "un\u00b7aus\u00b7f\u00fcll\u00b7bar", "g\u00e4hnt", "die", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mag noch so bang das Herz erbeben,", "tokens": ["mag", "noch", "so", "bang", "das", "Herz", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 verweht ist lang der Veilchenduft,", "tokens": ["\u2013", "ver\u00b7weht", "ist", "lang", "der", "Veil\u00b7chen\u00b7duft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und keines Gottes Stimme ruft", "tokens": ["und", "kei\u00b7nes", "Got\u00b7tes", "Stim\u00b7me", "ruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die toten Bl\u00fcten mehr ins Leben.", "tokens": ["die", "to\u00b7ten", "Bl\u00fc\u00b7ten", "mehr", "ins", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}