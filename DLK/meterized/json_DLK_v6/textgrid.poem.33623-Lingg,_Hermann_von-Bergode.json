{"textgrid.poem.33623": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Bergode", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille Nacht herrscht feierlich ernst ringsum noch,", "tokens": ["Stil\u00b7le", "Nacht", "herrscht", "fei\u00b7er\u00b7lich", "ernst", "ring\u00b7sum", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADJD", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nur der Waldbach braust im Gekl\u00fcft hin sch\u00e4umend,", "tokens": ["Nur", "der", "Wald\u00b7bach", "braust", "im", "Ge\u00b7kl\u00fcft", "hin", "sch\u00e4u\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur der Fr\u00fchwind weht und verhei\u00dft des Morgens", "tokens": ["Nur", "der", "Fr\u00fch\u00b7wind", "weht", "und", "ver\u00b7hei\u00dft", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Grauende D\u00e4mmrung.", "tokens": ["Grau\u00b7en\u00b7de", "D\u00e4mm\u00b7rung", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Tief im Grund ruhn schattige T\u00e4ler, dunkler", "tokens": ["Tief", "im", "Grund", "ruhn", "schat\u00b7ti\u00b7ge", "T\u00e4\u00b7ler", ",", "dunk\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ragt der Tannwald, \u00fcber den Wipfeln funkelt", "tokens": ["Ragt", "der", "Tann\u00b7wald", ",", "\u00fc\u00b7ber", "den", "Wip\u00b7feln", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Noch ein Stern. Du bist es, der Stern der Liebe,", "tokens": ["Noch", "ein", "Stern", ".", "Du", "bist", "es", ",", "der", "Stern", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PPER", "VAFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Strahlende Venus!", "tokens": ["Strah\u00b7len\u00b7de", "Ve\u00b7nus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Stern des Morgens! wie von den H\u00f6h'n dein Lichtglanz", "tokens": ["Stern", "des", "Mor\u00b7gens", "!", "wie", "von", "den", "H\u00f6h'n", "dein", "Licht\u00b7glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADV", "$.", "PWAV", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Durch die D\u00e4mmrung blinkt und im Fr\u00fchrot zittert,", "tokens": ["Durch", "die", "D\u00e4mm\u00b7rung", "blinkt", "und", "im", "Fr\u00fch\u00b7rot", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Zart und hell, wie perlender Tau vom Kelchgrund", "tokens": ["Zart", "und", "hell", ",", "wie", "per\u00b7len\u00b7der", "Tau", "vom", "Kelch\u00b7grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bl\u00fchender Rosen.", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "O wohl magst du lieblich ergl\u00e4nzen, taureich!", "tokens": ["O", "wohl", "magst", "du", "lieb\u00b7lich", "er\u00b7gl\u00e4n\u00b7zen", ",", "tau\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,", "ADJD", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Von zu fr\u00fch Verblichener stillem Grabkreuz", "tokens": ["Von", "zu", "fr\u00fch", "Ver\u00b7bli\u00b7che\u00b7ner", "stil\u00b7lem", "Grab\u00b7kreuz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PTKA", "ADJD", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "K\u00fc\u00dft dein L\u00e4cheln weg an den Kr\u00e4nzen laut're", "tokens": ["K\u00fc\u00dft", "dein", "L\u00e4\u00b7cheln", "weg", "an", "den", "Kr\u00e4n\u00b7zen", "laut'\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Tr\u00e4nen der Wehmut.", "tokens": ["Tr\u00e4\u00b7nen", "der", "Weh\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und im Himmel bist du der Saiten eine,", "tokens": ["Und", "im", "Him\u00b7mel", "bist", "du", "der", "Sai\u00b7ten", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "ART", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die, von Engelsharfen erklungen, jubelnd", "tokens": ["Die", ",", "von", "En\u00b7gels\u00b7har\u00b7fen", "er\u00b7klun\u00b7gen", ",", "ju\u00b7belnd"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "NN", "VVINF", "$,", "VVPP"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch das Weltall t\u00f6nen im ewig neuen", "tokens": ["Durch", "das", "Wel\u00b7tall", "t\u00f6\u00b7nen", "im", "e\u00b7wig", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "ADJD", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Liede der Sch\u00f6pfung. \u2013", "tokens": ["Lie\u00b7de", "der", "Sch\u00f6p\u00b7fung", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf jetzt! Gl\u00fchrot leuchtet der Gletscher Haupt schon,", "tokens": ["Auf", "jetzt", "!", "Gl\u00fch\u00b7rot", "leuch\u00b7tet", "der", "Glet\u00b7scher", "Haupt", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Sonne! Dein Titanengeschlecht begr\u00fc\u00dft dich,", "tokens": ["Son\u00b7ne", "!", "Dein", "Ti\u00b7ta\u00b7nen\u00b7ge\u00b7schlecht", "be\u00b7gr\u00fc\u00dft", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Deiner Urzeit Kinder. Zu euch jetzt, taghell", "tokens": ["Dei\u00b7ner", "Ur\u00b7zeit", "Kin\u00b7der", ".", "Zu", "euch", "jetzt", ",", "ta\u00b7ghell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "NN", "$.", "APPR", "PPER", "ADV", "$,", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Flammende Gipfel!", "tokens": ["Flam\u00b7men\u00b7de", "Gip\u00b7fel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00dcber Gras und Bl\u00fcmchen, im Taulicht zitternd,", "tokens": ["\u00dc\u00b7ber", "Gras", "und", "Bl\u00fcm\u00b7chen", ",", "im", "Tau\u00b7licht", "zit\u00b7ternd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klimmt des Wandrers Schritt, und im Moos darunter", "tokens": ["Klimmt", "des", "Wand\u00b7rers", "Schritt", ",", "und", "im", "Moos", "da\u00b7run\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "KON", "APPRART", "NN", "PAV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Schl\u00fcpfen hastig Spinnen und emsig summen", "tokens": ["Schl\u00fcp\u00b7fen", "has\u00b7tig", "Spin\u00b7nen", "und", "em\u00b7sig", "sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "KON", "ADJD", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bienen im Goldklee.", "tokens": ["Bie\u00b7nen", "im", "Gold\u00b7klee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "R\u00fcckw\u00e4rts sinkt in Nebel zur\u00fcck die Talschlucht,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "sinkt", "in", "Ne\u00b7bel", "zu\u00b7r\u00fcck", "die", "Tal\u00b7schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Vor dem sehnsuchttrunkenen Blick erschlie\u00dft sich", "tokens": ["Vor", "dem", "sehn\u00b7sucht\u00b7trun\u00b7ke\u00b7nen", "Blick", "er\u00b7schlie\u00dft", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bis zum Seegel\u00e4nde hinab die ganze", "tokens": ["Bis", "zum", "See\u00b7ge\u00b7l\u00e4n\u00b7de", "hin\u00b7ab", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ADV", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Liebliche Fernsicht.", "tokens": ["Lieb\u00b7li\u00b7che", "Fern\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}