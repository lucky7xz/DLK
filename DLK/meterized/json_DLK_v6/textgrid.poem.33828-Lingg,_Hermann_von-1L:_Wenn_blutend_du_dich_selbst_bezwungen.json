{"textgrid.poem.33828": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn blutend du dich selbst bezwungen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn blutend du dich selbst bezwungen", "tokens": ["Wenn", "blu\u00b7tend", "du", "dich", "selbst", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit dem Besten deiner Kraft", "tokens": ["Und", "mit", "dem", "Bes\u00b7ten", "dei\u00b7ner", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sieg hast \u00fcber dich errungen", "tokens": ["Den", "Sieg", "hast", "\u00fc\u00b7ber", "dich", "er\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcber Qual und Leidenschaft,", "tokens": ["Und", "\u00fc\u00b7ber", "Qual", "und", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wirst du wohl auch das ertragen,", "tokens": ["Dann", "wirst", "du", "wohl", "auch", "das", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df heimlich Wichte nach dir schlagen.", "tokens": ["Da\u00df", "heim\u00b7lich", "Wich\u00b7te", "nach", "dir", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcck fest die Hand auf deine Wunde,", "tokens": ["Dr\u00fcck", "fest", "die", "Hand", "auf", "dei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie von Tugend und Moral", "tokens": ["Wenn", "sie", "von", "Tu\u00b7gend", "und", "Mo\u00b7ral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir pred'gen mit verlognem Munde,", "tokens": ["Dir", "pre\u00b7d'\u00b7gen", "mit", "ver\u00b7log\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dann schweige, zucke nicht einmal,", "tokens": ["Dann", "schwei\u00b7ge", ",", "zu\u00b7cke", "nicht", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und leichter wirst du dann verschmerzen", "tokens": ["Und", "leich\u00b7ter", "wirst", "du", "dann", "ver\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Bitterkeit in deinem Herzen.", "tokens": ["Die", "Bit\u00b7ter\u00b7keit", "in", "dei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf hohen Bergen ist, verlassen", "tokens": ["Auf", "ho\u00b7hen", "Ber\u00b7gen", "ist", ",", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einsam sein, nur eine Lust,", "tokens": ["Und", "ein\u00b7sam", "sein", ",", "nur", "ei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tief unter dir das niedre Hassen:", "tokens": ["Tief", "un\u00b7ter", "dir", "das", "nied\u00b7re", "Has\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird es deinem Stolz bewu\u00dft,", "tokens": ["So", "wird", "es", "dei\u00b7nem", "Stolz", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df \u00fcber die gemeinen Dinge", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "die", "ge\u00b7mei\u00b7nen", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns hoch erhebt die Leidensschwinge.", "tokens": ["Uns", "hoch", "er\u00b7hebt", "die", "Lei\u00b7dens\u00b7schwin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}