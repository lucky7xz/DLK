{"dta.poem.18156": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist doch woll der Mensch, wenn", "tokens": ["Was", "ist", "doch", "woll", "der", "Mensch", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "er sich selbst beschaut?", "tokens": ["er", "sich", "selbst", "be\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ein herrlich Meisterst\u00fck von ", "tokens": ["Ein", "herr\u00b7lich", "Meis\u00b7ter\u00b7st\u00fck", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht erbaut", "tokens": ["Macht", "er\u00b7baut"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Aus einen Geist und Leib, ein leben-", "tokens": ["Aus", "ei\u00b7nen", "Geist", "und", "Leib", ",", "ein", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "des Geh\u00e4use,", "tokens": ["des", "Ge\u00b7h\u00e4u\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Das k\u00fcnstlichste Gesch\u00f6pf im weiten Erden Kreise;", "tokens": ["Das", "k\u00fcnst\u00b7lichs\u00b7te", "Ge\u00b7sch\u00f6pf", "im", "wei\u00b7ten", "Er\u00b7den", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Seele wohnt im Leib und f\u00fchrt das Regiment,", "tokens": ["Die", "See\u00b7le", "wohnt", "im", "Leib", "und", "f\u00fchrt", "das", "Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die macht da\u00df er sich selbst und andre Dinge kennt,", "tokens": ["Die", "macht", "da\u00df", "er", "sich", "selbst", "und", "and\u00b7re", "Din\u00b7ge", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOUS", "PPER", "PRF", "ADV", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die macht da\u00df er sich kan an seines Sch\u00f6pfers Gaben", "tokens": ["Die", "macht", "da\u00df", "er", "sich", "kan", "an", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOUS", "PPER", "PRF", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die er ihm f\u00fcrgesezt, mit allen Sinnen laben.", "tokens": ["Die", "er", "ihm", "f\u00fcr\u00b7ge\u00b7sezt", ",", "mit", "al\u00b7len", "Sin\u00b7nen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Begl\u00fckte Kreatur! du bist des H\u00f6chsten Bild,", "tokens": ["Be\u00b7gl\u00fck\u00b7te", "Kre\u00b7a\u00b7tur", "!", "du", "bist", "des", "H\u00f6chs\u00b7ten", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das er mit Weisheits Glanz und Heiligkeit erf\u00fcllt,", "tokens": ["Das", "er", "mit", "Weis\u00b7heits", "Glanz", "und", "Hei\u00b7lig\u00b7keit", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das er zum Ober Herrn der ganzen Welt erw\u00e4hlet,", "tokens": ["Das", "er", "zum", "O\u00b7ber", "Herrn", "der", "gan\u00b7zen", "Welt", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und mit der Seligkeit des Ewigen verm\u00e4hlet.", "tokens": ["Und", "mit", "der", "Se\u00b7lig\u00b7keit", "des", "E\u00b7wi\u00b7gen", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Gefallnes Jammer Bild! wo ist die Herrligkeit,", "tokens": ["Ge\u00b7fall\u00b7nes", "Jam\u00b7mer", "Bild", "!", "wo", "ist", "die", "Herr\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Des edlen Unschuld Stands, der vormahls g\u00fcldnen Zeit", "tokens": ["Des", "ed\u00b7len", "Un\u00b7schuld", "Stands", ",", "der", "vor\u00b7mahls", "g\u00fcld\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die ist schon l\u00e4ngst dahin, denn bei der Schuld der", "tokens": ["Die", "ist", "schon", "l\u00e4ngst", "da\u00b7hin", ",", "denn", "bei", "der", "Schuld", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PAV", "$,", "KON", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "S\u00fcnden,", "tokens": ["S\u00fcn\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Die Leib und Geist verdirbt, ist sie nicht mehr zu", "tokens": ["Die", "Leib", "und", "Geist", "ver\u00b7dirbt", ",", "ist", "sie", "nicht", "mehr", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "PTKZU"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "finden", "tokens": ["fin\u00b7den"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Statt da\u00df der Mensch vorhin des H\u00f6chsten Bildnis", "tokens": ["Statt", "da\u00df", "der", "Mensch", "vor\u00b7hin", "des", "H\u00f6chs\u00b7ten", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "war,", "tokens": ["war", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "So stellt er ganz verstellt des Satans Larve dar;", "tokens": ["So", "stellt", "er", "ganz", "ver\u00b7stellt", "des", "Sa\u00b7tans", "Lar\u00b7ve", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Anstat der Weisheit Licht, das ihn vorhin gezieret,", "tokens": ["An\u00b7stat", "der", "Weis\u00b7heit", "Licht", ",", "das", "ihn", "vor\u00b7hin", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und auf den rechten Weg des wahren Gl\u00fcks gef\u00fchret", "tokens": ["Und", "auf", "den", "rech\u00b7ten", "Weg", "des", "wah\u00b7ren", "Gl\u00fcks", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist der Verstand verwirrt, dem man ein Blend Licht", "tokens": ["Ist", "der", "Ver\u00b7stand", "ver\u00b7wirrt", ",", "dem", "man", "ein", "Blend", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "PRELS", "PIS", "ART", "NN", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.28": {"text": "nennt,", "tokens": ["nennt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Des flatterhafter Schein nunmehr so dunkel brennt,", "tokens": ["Des", "flat\u00b7ter\u00b7haf\u00b7ter", "Schein", "nun\u00b7mehr", "so", "dun\u00b7kel", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df keiner dabei wird die Th\u00fcr des Himmels sehen,", "tokens": ["Da\u00df", "kei\u00b7ner", "da\u00b7bei", "wird", "die", "Th\u00fcr", "des", "Him\u00b7mels", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vielweniger den Weg drauf wir zu solcher gehen.", "tokens": ["Viel\u00b7we\u00b7ni\u00b7ger", "den", "Weg", "drauf", "wir", "zu", "sol\u00b7cher", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "PPER", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Jhr Weisen der Vernunft komt her und sagt mir an,", "tokens": ["Ihr", "Wei\u00b7sen", "der", "Ver\u00b7nunft", "komt", "her", "und", "sagt", "mir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was hat der arme Mensch der so verderbt gethan?", "tokens": ["Was", "hat", "der", "ar\u00b7me", "Mensch", "der", "so", "ver\u00b7derbt", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "ART", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn GOttes weise Hand die ihn zuerst gebildet,", "tokens": ["Denn", "Got\u00b7tes", "wei\u00b7se", "Hand", "die", "ihn", "zu\u00b7erst", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Hat ihn so nimmermehr, wie er jetzt ist, geschildet;", "tokens": ["Hat", "ihn", "so", "nim\u00b7mer\u00b7mehr", ",", "wie", "er", "jetzt", "ist", ",", "ge\u00b7schil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Komt her und zeiget mir, da\u00df eur Verstand ein Licht,", "tokens": ["Komt", "her", "und", "zei\u00b7get", "mir", ",", "da\u00df", "eur", "Ver\u00b7stand", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der alle Finsternis mit seinen Strahl durchbricht,", "tokens": ["Der", "al\u00b7le", "Fins\u00b7ter\u00b7nis", "mit", "sei\u00b7nen", "Strahl", "durch\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Kann euer scharfer Witz den Ursprung von dem B\u00f6sen,", "tokens": ["Kann", "eu\u00b7er", "schar\u00b7fer", "Witz", "den", "Ur\u00b7sprung", "von", "dem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das R\u00e4tzel aller Zeit ohn Wiederspruch aufl\u00f6sen?", "tokens": ["Das", "R\u00e4t\u00b7zel", "al\u00b7ler", "Zeit", "ohn", "Wie\u00b7der\u00b7spruch", "auf\u00b7l\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Komt her und lehret mich, wie wirds nach dieser Zeit,", "tokens": ["Komt", "her", "und", "leh\u00b7ret", "mich", ",", "wie", "wirds", "nach", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PPER", "$,", "PWAV", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mit einer andern Welt vollkomner Seligkeit,", "tokens": ["Mit", "ei\u00b7ner", "an\u00b7dern", "Welt", "voll\u00b7kom\u00b7ner", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wornach die Sehnsucht doch der Seelen sich bestrebet,", "tokens": ["Wor\u00b7nach", "die", "Sehn\u00b7sucht", "doch", "der", "See\u00b7len", "sich", "be\u00b7stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die durch geheimen Trieb sich immer mehr erhebet,", "tokens": ["Die", "durch", "ge\u00b7hei\u00b7men", "Trieb", "sich", "im\u00b7mer", "mehr", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Denn dieser starke Zug der von dem Sch\u00f6pfer stammt,", "tokens": ["Denn", "die\u00b7ser", "star\u00b7ke", "Zug", "der", "von", "dem", "Sch\u00f6p\u00b7fer", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ist auch von ewgen ", "tokens": ["Ist", "auch", "von", "ew\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Wie komt man an den Ort den unser Wille suchet", "tokens": ["Wie", "komt", "man", "an", "den", "Ort", "den", "un\u00b7ser", "Wil\u00b7le", "su\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "APPR", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Da uns des Richters Spruch in unsern Busen fluchet", "tokens": ["Da", "uns", "des", "Rich\u00b7ters", "Spruch", "in", "un\u00b7sern", "Bu\u00b7sen", "flu\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hie bleibet ihr verstummt, und sagt ihr mir was vor,", "tokens": ["Hie", "blei\u00b7bet", "ihr", "ver\u00b7stummt", ",", "und", "sagt", "ihr", "mir", "was", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "So m\u00fcst ihr selbst gestehn ein Weiser werd ein Thor,", "tokens": ["So", "m\u00fcst", "ihr", "selbst", "ge\u00b7stehn", "ein", "Wei\u00b7ser", "werd", "ein", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wenn er durch die Vernunft die Dinge will ergr\u00fcnden", "tokens": ["Wenn", "er", "durch", "die", "Ver\u00b7nunft", "die", "Din\u00b7ge", "will", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Die kein gesch\u00e4rfter Witz verm\u00f6gend zu erfinden.", "tokens": ["Die", "kein", "ge\u00b7sch\u00e4rf\u00b7ter", "Witz", "ver\u00b7m\u00f6\u00b7gend", "zu", "er\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Was ist also ein Mensch? Er ist ein Erdenklos,", "tokens": ["Was", "ist", "al\u00b7so", "ein", "Mensch", "?", "Er", "ist", "ein", "Er\u00b7denk\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Von einer Seite klein, von einer Seite gros", "tokens": ["Von", "ei\u00b7ner", "Sei\u00b7te", "klein", ",", "von", "ei\u00b7ner", "Sei\u00b7te", "gros"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Er ist die Kreatur die GOttes Macht beseelet", "tokens": ["Er", "ist", "die", "Kre\u00b7a\u00b7tur", "die", "Got\u00b7tes", "Macht", "be\u00b7see\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Aus einen Geist und Leib vereinigt und verm\u00e4hlet,", "tokens": ["Aus", "ei\u00b7nen", "Geist", "und", "Leib", "ver\u00b7ei\u00b7nigt", "und", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Der sich aus eigner Schuld zum Elends-Stand ge-", "tokens": ["Der", "sich", "aus", "eig\u00b7ner", "Schuld", "zum", "E\u00b7lends\u00b7Stand", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "bracht,", "tokens": ["bracht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.58": {"text": "Da ihn des Sch\u00f6pfers Gunst gl\u00fckselig gnug gemacht", "tokens": ["Da", "ihn", "des", "Sch\u00f6p\u00b7fers", "Gunst", "gl\u00fck\u00b7se\u00b7lig", "gnug", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Indem er Freiheit w\u00fcnscht, liebt er den Sclaven Orden", "tokens": ["In\u00b7dem", "er", "Frei\u00b7heit", "w\u00fcnscht", ",", "liebt", "er", "den", "Scla\u00b7ven", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.60": {"text": "Denn g\u00e4nzlich frei zu sein ist er ein Knecht geworden", "tokens": ["Denn", "g\u00e4nz\u00b7lich", "frei", "zu", "sein", "ist", "er", "ein", "Knecht", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "PTKZU", "VAINF", "VAFIN", "PPER", "ART", "NN", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Der untern Joche liegt, das er sich aufgelegt", "tokens": ["Der", "un\u00b7tern", "Jo\u00b7che", "liegt", ",", "das", "er", "sich", "auf\u00b7ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und mit verfluchter Lust bald gern bald ungern tr\u00e4gt", "tok