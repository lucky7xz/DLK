{"textgrid.poem.25849": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[greise sinnen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Greise sinnen.", "tokens": ["Grei\u00b7se", "sin\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Eine gro\u00dfe graue Spinne.", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "grau\u00b7e", "Spin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Netze schleiern,", "tokens": ["Net\u00b7ze", "schlei\u00b7ern", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "F\u00e4den rinnen.", "tokens": ["F\u00e4\u00b7den", "rin\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die jahrhundert grauen W\u00e4lder", "tokens": ["Die", "jahr\u00b7hun\u00b7dert", "grau\u00b7en", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tragen ernst den alten Himmel,", "tokens": ["Tra\u00b7gen", "ernst", "den", "al\u00b7ten", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verdorrte, alte Lippen", "tokens": ["Und", "ver\u00b7dorr\u00b7te", ",", "al\u00b7te", "Lip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nippen an dem kalten Horte", "tokens": ["Nip\u00b7pen", "an", "dem", "kal\u00b7ten", "Hor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4ngstvergl\u00fchter alter Worte.", "tokens": ["L\u00e4ngst\u00b7ver\u00b7gl\u00fch\u00b7ter", "al\u00b7ter", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}