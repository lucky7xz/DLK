{"textgrid.poem.55496": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "76", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kommt, Gebildetes allhier zu schauen,", "tokens": ["Ihr", "kommt", ",", "Ge\u00b7bil\u00b7de\u00b7tes", "all\u00b7hier", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebildet scheinbar, doch ein lebend Bild;", "tokens": ["Ge\u00b7bil\u00b7det", "schein\u00b7bar", ",", "doch", "ein", "le\u00b7bend", "Bild", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "$,", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wei\u00df die Kunst vielf\u00e4ltig anzubauen", "tokens": ["So", "wei\u00df", "die", "Kunst", "viel\u00b7f\u00e4l\u00b7tig", "an\u00b7zu\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fabel, der Geschichte reich Gefild.", "tokens": ["Der", "Fa\u00b7bel", ",", "der", "Ge\u00b7schich\u00b7te", "reich", "Ge\u00b7fild", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr sehet t\u00fccht'ge M\u00e4nner, wackre Frauen,", "tokens": ["Ihr", "se\u00b7het", "t\u00fccht'\u00b7ge", "M\u00e4n\u00b7ner", ",", "wack\u00b7re", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Taten m\u00e4chtig wie zur H\u00fclfe mild,", "tokens": ["Zu", "Ta\u00b7ten", "m\u00e4ch\u00b7tig", "wie", "zur", "H\u00fcl\u00b7fe", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KOKOM", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und so entgegnen wir euch, starr erscheinend,", "tokens": ["Und", "so", "ent\u00b7geg\u00b7nen", "wir", "euch", ",", "starr", "er\u00b7schei\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lebendig, uns zu eurer Lust vereinend.", "tokens": ["Le\u00b7ben\u00b7dig", ",", "uns", "zu", "eu\u00b7rer", "Lust", "ver\u00b7ei\u00b7nend", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}