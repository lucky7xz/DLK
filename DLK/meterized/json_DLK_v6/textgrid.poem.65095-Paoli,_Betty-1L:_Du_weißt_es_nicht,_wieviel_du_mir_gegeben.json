{"textgrid.poem.65095": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wei\u00dft es nicht, wieviel du mir gegeben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft es nicht, wieviel du mir gegeben", "tokens": ["Du", "wei\u00dft", "es", "nicht", ",", "wie\u00b7viel", "du", "mir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einer finstern Zeit, da meinem Leben", "tokens": ["In", "ei\u00b7ner", "fins\u00b7tern", "Zeit", ",", "da", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Hoffens gold'ner Schein entschwand!", "tokens": ["Des", "Hof\u00b7fens", "gold'\u00b7ner", "Schein", "ent\u00b7schwand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kennst es nicht, das tiefe bitt're Elend,", "tokens": ["Du", "kennst", "es", "nicht", ",", "das", "tie\u00b7fe", "bitt'\u00b7re", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In welches deine Worte, neu beseelend,", "tokens": ["In", "wel\u00b7ches", "dei\u00b7ne", "Wor\u00b7te", ",", "neu", "be\u00b7see\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Himmelsboten ", "tokens": ["Den", "Him\u00b7mels\u00b7bo\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du siehst sie nicht die stolz verh\u00fcllten Wunden,", "tokens": ["Du", "siehst", "sie", "nicht", "die", "stolz", "ver\u00b7h\u00fcll\u00b7ten", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf die in dunkeln mittern\u00e4cht'gen Stunden", "tokens": ["Auf", "die", "in", "dun\u00b7keln", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sanfte K\u00fchlung niederweh'st! \u2013", "tokens": ["Du", "sanf\u00b7te", "K\u00fch\u00b7lung", "nie\u00b7der\u00b7weh'st", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du ahnest nichts von meines Innern Streite,", "tokens": ["Du", "ah\u00b7nest", "nichts", "von", "mei\u00b7nes", "In\u00b7nern", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch ahnest du, da\u00df du an meiner Seite", "tokens": ["Noch", "ah\u00b7nest", "du", ",", "da\u00df", "du", "an", "mei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Am Siechbett' eines Herzens stehst. \u2013", "tokens": ["Am", "Siech\u00b7bett'", "ei\u00b7nes", "Her\u00b7zens", "stehst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn ich dich betrachte, m\u00f6cht' ich fragen:", "tokens": ["Und", "wenn", "ich", "dich", "be\u00b7trach\u00b7te", ",", "m\u00f6cht'", "ich", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bist du ein Strahl, der mir von bessern Tagen", "tokens": ["Bist", "du", "ein", "Strahl", ",", "der", "mir", "von", "bes\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voll freudiger Verhei\u00dfung spricht?", "tokens": ["Voll", "freu\u00b7di\u00b7ger", "Ver\u00b7hei\u00b7\u00dfung", "spricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie? Oder bist du nur ein letzter Schimmer,", "tokens": ["Wie", "?", "O\u00b7der", "bist", "du", "nur", "ein", "letz\u00b7ter", "Schim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NE", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mir nachgesandt zum Trost eh' ich auf immer", "tokens": ["Mir", "nach\u00b7ge\u00b7sandt", "zum", "Trost", "eh'", "ich", "auf", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOUS", "PPER", "APPR", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vom Leben scheide und vom Licht? \u2013", "tokens": ["Vom", "Le\u00b7ben", "schei\u00b7de", "und", "vom", "Licht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie dem auch sei, und wie es sich mag wenden,", "tokens": ["Wie", "dem", "auch", "sei", ",", "und", "wie", "es", "sich", "mag", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "VAFIN", "$,", "KON", "PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nimm meinen Dank f\u00fcr deiner Seele Spenden!", "tokens": ["Nimm", "mei\u00b7nen", "Dank", "f\u00fcr", "dei\u00b7ner", "See\u00b7le", "Spen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Segen sei ob deinem Haupt!", "tokens": ["Mein", "Se\u00b7gen", "sei", "ob", "dei\u00b7nem", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn eine St\u00fctze bist du mir gewesen", "tokens": ["Denn", "ei\u00b7ne", "St\u00fct\u00b7ze", "bist", "du", "mir", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "PPER", "VAPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In b\u00f6sen Stunden, wo von allen Wesen", "tokens": ["In", "b\u00f6\u00b7sen", "Stun\u00b7den", ",", "wo", "von", "al\u00b7len", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Herz nur mehr an dich geglaubt.", "tokens": ["Mein", "Herz", "nur", "mehr", "an", "dich", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wird Heilung mir noch in des Lebens Thalen,", "tokens": ["Wird", "Hei\u00b7lung", "mir", "noch", "in", "des", "Le\u00b7bens", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit welchem Wucher will ich dir bezahlen", "tokens": ["Mit", "wel\u00b7chem", "Wu\u00b7cher", "will", "ich", "dir", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schuld, in die du mich verstrickt!", "tokens": ["Die", "Schuld", ",", "in", "die", "du", "mich", "ver\u00b7strickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und soll mir nur der Tod Genesung bringen,", "tokens": ["Und", "soll", "mir", "nur", "der", "Tod", "Ge\u00b7ne\u00b7sung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird es dein Gem\u00fcth wie Duft durchdringen,", "tokens": ["Dann", "wird", "es", "dein", "Ge\u00b7m\u00fcth", "wie", "Duft", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df du die Sterbende erquickt. \u2013", "tokens": ["Da\u00df", "du", "die", "Ster\u00b7ben\u00b7de", "er\u00b7quickt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann denke mein! In stillen Mittern\u00e4chten,", "tokens": ["Dann", "den\u00b7ke", "mein", "!", "In", "stil\u00b7len", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich aus Sternen Friedenskr\u00e4nze flechten,", "tokens": ["Die", "sich", "aus", "Ster\u00b7nen", "Frie\u00b7dens\u00b7kr\u00e4n\u00b7ze", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedenke, o gedenke mein! \u2013", "tokens": ["Ge\u00b7den\u00b7ke", ",", "o", "ge\u00b7den\u00b7ke", "mein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn laue Weste deine Stirn' umf\u00e4cheln,", "tokens": ["Wenn", "lau\u00b7e", "Wes\u00b7te", "dei\u00b7ne", "Stirn'", "um\u00b7f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann sage dir mit wehmuthsvollem L\u00e4cheln:", "tokens": ["Dann", "sa\u00b7ge", "dir", "mit", "weh\u00b7muths\u00b7vol\u00b7lem", "L\u00e4\u00b7cheln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es mag ein Gru\u00df der Todten sein!", "tokens": ["Es", "mag", "ein", "Gru\u00df", "der", "Tod\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}