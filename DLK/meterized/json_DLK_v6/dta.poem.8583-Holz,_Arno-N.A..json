{"dta.poem.8583": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grossmutter im Lehnstuhl ist eingeschlafen.", "tokens": ["Gross\u00b7mut\u00b7ter", "im", "Lehn\u00b7stuhl", "ist", "ein\u00b7ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Vom Fenster,", "tokens": ["Vom", "Fens\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "durch die weissen Zwirngardinen,", "tokens": ["durch", "die", "weis\u00b7sen", "Zwirn\u00b7gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "leuchten die Hyazintent\u00fcten.", "tokens": ["leuch\u00b7ten", "die", "Hy\u00b7a\u00b7zin\u00b7ten\u00b7t\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Im Tarlatankleidchen, am alten Klavier,", "tokens": ["Im", "Tar\u00b7la\u00b7tan\u00b7kleid\u00b7chen", ",", "am", "al\u00b7ten", "Kla\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "sitzt noch immer artig das kleine Linchen und spielt.", "tokens": ["sitzt", "noch", "im\u00b7mer", "ar\u00b7tig", "das", "klei\u00b7ne", "Lin\u00b7chen", "und", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine Musik aus Sonnenst\u00e4ubchen!", "tokens": ["Ei\u00b7ne", "Mu\u00b7sik", "aus", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}