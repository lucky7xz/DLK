{"dta.poem.8147": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Abseits.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Im warmen Mittagssonnenstrahle,", "tokens": ["Im", "war\u00b7men", "Mit\u00b7tags\u00b7son\u00b7nen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein rosenrother Schimmer fliegt", "tokens": ["Ein", "ro\u00b7sen\u00b7ro\u00b7ther", "Schim\u00b7mer", "fliegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um ihre alten Gr\u00e4bermale;", "tokens": ["Um", "ih\u00b7re", "al\u00b7ten", "Gr\u00e4\u00b7ber\u00b7ma\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kr\u00e4uter bl\u00fchn; der Haideduft", "tokens": ["Die", "Kr\u00e4u\u00b7ter", "bl\u00fchn", ";", "der", "Hai\u00b7de\u00b7duft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt in die blaue Sommerluft.", "tokens": ["Steigt", "in", "die", "blau\u00b7e", "Som\u00b7mer\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Laufk\u00e4fer hasten durch's Gestr\u00e4uch", "tokens": ["Lauf\u00b7k\u00e4\u00b7fer", "has\u00b7ten", "durch's", "Ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In ihren gold'nen Panzerr\u00f6ckchen,", "tokens": ["In", "ih\u00b7ren", "gold'\u00b7nen", "Pan\u00b7zer\u00b7r\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bienen h\u00e4ngen Zweig um Zweig", "tokens": ["Die", "Bie\u00b7nen", "h\u00e4n\u00b7gen", "Zweig", "um", "Zweig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich an der Edelhaide Gl\u00f6ckchen;", "tokens": ["Sich", "an", "der", "E\u00b7del\u00b7hai\u00b7de", "Gl\u00f6ck\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6gel schwirren aus dem Kraut \u2014", "tokens": ["Die", "V\u00f6\u00b7gel", "schwir\u00b7ren", "aus", "dem", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Luft ist voller Lerchenlaut.", "tokens": ["Die", "Luft", "ist", "vol\u00b7ler", "Ler\u00b7chen\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein halbverfallen Schindelhaus", "tokens": ["Ein", "halb\u00b7ver\u00b7fal\u00b7len", "Schin\u00b7del\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht einsam hier und sonnbeschienen;", "tokens": ["Steht", "ein\u00b7sam", "hier", "und", "sonn\u00b7be\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00e4thner lehnt zur Th\u00fcr hinaus,", "tokens": ["Der", "K\u00e4\u00b7th\u00b7ner", "lehnt", "zur", "Th\u00fcr", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Behaglich blinzelnd nach den Bienen;", "tokens": ["Be\u00b7hag\u00b7lich", "blin\u00b7zelnd", "nach", "den", "Bie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Junge auf dem Stein davor", "tokens": ["Sein", "Jun\u00b7ge", "auf", "dem", "Stein", "da\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schnitzt Pfeifen sich aus K\u00e4lberrohr.", "tokens": ["Schnitzt", "Pfei\u00b7fen", "sich", "aus", "K\u00e4l\u00b7ber\u00b7rohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaum zittert durch die Mittagsruh", "tokens": ["Kaum", "zit\u00b7tert", "durch", "die", "Mit\u00b7tags\u00b7ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schlag der Dorfuhr, der entfernten;", "tokens": ["Ein", "Schlag", "der", "Dor\u00b7fuhr", ",", "der", "ent\u00b7fern\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Dem Alten f\u00e4llt die Wimper zu,", "tokens": ["Dem", "Al\u00b7ten", "f\u00e4llt", "die", "Wim\u00b7per", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er tr\u00e4umt von seinen Honigerndten.", "tokens": ["Er", "tr\u00e4umt", "von", "sei\u00b7nen", "Ho\u00b7ni\u00b7gernd\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u2014 Kein Klang der aufgeregten Zeit", "tokens": ["Kein", "Klang", "der", "auf\u00b7ge\u00b7reg\u00b7ten", "Zeit"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Drang noch in diese Einsamkeit.", "tokens": ["Drang", "noch", "in", "die\u00b7se", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}