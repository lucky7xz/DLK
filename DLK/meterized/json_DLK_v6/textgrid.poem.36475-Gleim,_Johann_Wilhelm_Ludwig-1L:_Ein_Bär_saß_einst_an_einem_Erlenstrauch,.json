{"textgrid.poem.36475": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein B\u00e4r sa\u00df einst an einem Erlenstrauch,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein B\u00e4r sa\u00df einst an einem Erlenstrauch,", "tokens": ["Ein", "B\u00e4r", "sa\u00df", "einst", "an", "ei\u00b7nem", "Er\u00b7len\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leckte sich an seiner Tatze;", "tokens": ["Und", "leck\u00b7te", "sich", "an", "sei\u00b7ner", "Tat\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kater sah's, und eine Katze;", "tokens": ["Ein", "Ka\u00b7ter", "sah's", ",", "und", "ei\u00b7ne", "Kat\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das, sagte Hinz, das kann ich auch!", "tokens": ["Das", ",", "sag\u00b7te", "Hinz", ",", "das", "kann", "ich", "auch", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "NE", "$,", "PDS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Wolf erschien! der Kater schlich", "tokens": ["Ein", "Wolf", "er\u00b7schien", "!", "der", "Ka\u00b7ter", "schlich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$.", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einen Baum, die Katze setzte sich", "tokens": ["Auf", "ei\u00b7nen", "Baum", ",", "die", "Kat\u00b7ze", "setz\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Still neben ihn, und beide, nun", "tokens": ["Still", "ne\u00b7ben", "ihn", ",", "und", "bei\u00b7de", ",", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "PPER", "$,", "KON", "PIS", "$,", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Sicherheit, sahn Heldenthaten thun!", "tokens": ["In", "Si\u00b7cher\u00b7heit", ",", "sahn", "Hel\u00b7den\u00b7tha\u00b7ten", "thun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn Wolf und B\u00e4r bekamen Krieg,", "tokens": ["Denn", "Wolf", "und", "B\u00e4r", "be\u00b7ka\u00b7men", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Ritter B\u00e4r erk\u00e4mpfte hohen Sieg!", "tokens": ["Und", "Rit\u00b7ter", "B\u00e4r", "er\u00b7k\u00e4mpf\u00b7te", "ho\u00b7hen", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da machte sich die Katze rauch,", "tokens": ["Da", "mach\u00b7te", "sich", "die", "Kat\u00b7ze", "rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fragte: Hinz! kannst du das auch?", "tokens": ["Und", "frag\u00b7te", ":", "Hinz", "!", "kannst", "du", "das", "auch", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "$.", "VMFIN", "PPER", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}