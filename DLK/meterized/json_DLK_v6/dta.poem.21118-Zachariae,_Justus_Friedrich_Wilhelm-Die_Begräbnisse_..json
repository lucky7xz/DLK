{"dta.poem.21118": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Begr\u00e4bnisse .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steige hinab, o eremitische Seele,", "tokens": ["Stei\u00b7ge", "hin\u00b7ab", ",", "o", "e\u00b7re\u00b7mi\u00b7ti\u00b7sche", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Unter den Staub des dich erwartenden Grabes.", "tokens": ["Un\u00b7ter", "den", "Staub", "des", "dich", "er\u00b7war\u00b7ten\u00b7den", "Gra\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Scheue du nicht den schwarzen entsetzlichen Anblick", "tokens": ["Scheu\u00b7e", "du", "nicht", "den", "schwar\u00b7zen", "ent\u00b7setz\u00b7li\u00b7chen", "An\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Jm dunklem Schattenreich.", "tokens": ["Jm", "dunk\u00b7lem", "Schat\u00b7ten\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seyd mir gegr\u00fc\u00dft, ihr Monumente des Schre-", "tokens": ["Seyd", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "Mo\u00b7nu\u00b7men\u00b7te", "des", "Schre"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PPOSAT", "NN", "ART", "NN"], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Vor euch erbebt nur die unm\u00e4nnliche Seele,", "tokens": ["Vor", "euch", "er\u00b7bebt", "nur", "die", "un\u00b7m\u00e4nn\u00b7li\u00b7che", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Welche, noch nie dem Gegenw\u00e4rtgen entrissen,", "tokens": ["Wel\u00b7che", ",", "noch", "nie", "dem", "Ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Stets an dem Staube klebt.", "tokens": ["Stets", "an", "dem", "Stau\u00b7be", "klebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schauernd steh ich \u2014 Tief in die traurende Stille", "tokens": ["Schau\u00b7ernd", "steh", "ich", "Tief", "in", "die", "trau\u00b7ren\u00b7de", "Stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$(", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sind sie verh\u00fcllt, des Todes \u00f6de Gefilde!", "tokens": ["Sind", "sie", "ver\u00b7h\u00fcllt", ",", "des", "To\u00b7des", "\u00f6\u00b7de", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf das Gebein, vor seiner Zerst\u00f6rung gef\u00fcrchtet,", "tokens": ["Auf", "das", "Ge\u00b7bein", ",", "vor", "sei\u00b7ner", "Zer\u00b7st\u00f6\u00b7rung", "ge\u00b7f\u00fcrch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Tritt des Geringern Fu\u00df.", "tokens": ["Tritt", "des", "Ge\u00b7rin\u00b7gern", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Siehe! wie prahlt in der betr\u00fcgrischen Innschrift", "tokens": ["Sie\u00b7he", "!", "wie", "prahlt", "in", "der", "be\u00b7tr\u00fcg\u00b7ri\u00b7schen", "I\u00b7nnschrift"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vornehmer Grab. Jm stillen Schatten des Ahorns", "tokens": ["Vor\u00b7neh\u00b7mer", "Grab", ".", "Jm", "stil\u00b7len", "Schat\u00b7ten", "des", "A\u00b7horns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ruht, unger\u00fchmt vom panegyrischen Marmor,", "tokens": ["Ruht", ",", "un\u00b7ge\u00b7r\u00fchmt", "vom", "pa\u00b7ne\u00b7gy\u00b7ri\u00b7schen", "Mar\u00b7mor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Weisen Aschenkrug.", "tokens": ["Des", "Wei\u00b7sen", "A\u00b7schen\u00b7krug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mich auch empf\u00e4ngt einst eine der schauernden", "tokens": ["Mich", "auch", "emp\u00b7f\u00e4ngt", "einst", "ei\u00b7ne", "der", "schau\u00b7ern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn sich mein Haupt, gleich einer sterbenden Rose,", "tokens": ["Wenn", "sich", "mein", "Haupt", ",", "gleich", "ei\u00b7ner", "ster\u00b7ben\u00b7den", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Welcher der Nordwind Unschuld und Purpur geraubet,", "tokens": ["Wel\u00b7cher", "der", "Nord\u00b7wind", "Un\u00b7schuld", "und", "Pur\u00b7pur", "ge\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "In dunkle Schatten neigt.", "tokens": ["In", "dunk\u00b7le", "Schat\u00b7ten", "neigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hier oder da wird mein Gebeine dann schlafen.", "tokens": ["Hier", "o\u00b7der", "da", "wird", "mein", "Ge\u00b7bei\u00b7ne", "dann", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gl\u00fccklich, wenn noch in Thr\u00e4nen die z\u00e4rtliche Freund-", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wenn", "noch", "in", "Thr\u00e4\u00b7nen", "die", "z\u00e4rt\u00b7li\u00b7che", "Freun\u00b7d"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ADV", "APPR", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Um mich sich h\u00e4rmt, und meine verla\u00dfne Geliebte", "tokens": ["Um", "mich", "sich", "h\u00e4rmt", ",", "und", "mei\u00b7ne", "ver\u00b7la\u00df\u00b7ne", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PRF", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Um mich geklaget hat.", "tokens": ["Um", "mich", "ge\u00b7kla\u00b7get", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ruhet dann sanft, o ihr entschlafnen Gebeine!", "tokens": ["Ru\u00b7het", "dann", "sanft", ",", "o", "ihr", "ent\u00b7schlaf\u00b7nen", "Ge\u00b7bei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "FM", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Moder und Staub wird euch nur herrlicher machen.", "tokens": ["Mo\u00b7der", "und", "Staub", "wird", "euch", "nur", "herr\u00b7li\u00b7cher", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ADJA", "VVINF", "$."], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Herrlicher noch sollt ihr die z\u00e4rtlichen Freunde,", "tokens": ["Herr\u00b7li\u00b7cher", "noch", "sollt", "ihr", "die", "z\u00e4rt\u00b7li\u00b7chen", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und die Geliebte sehn!", "tokens": ["Und", "die", "Ge\u00b7lieb\u00b7te", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}