{"dta.poem.7259": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "In seiner Buchenhalle sa\u00df ein Greis auf gr\u00fcner Bank,", "tokens": ["In", "sei\u00b7ner", "Bu\u00b7chen\u00b7hal\u00b7le", "sa\u00df", "ein", "Greis", "auf", "gr\u00fc\u00b7ner", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Vor ihm, in gr\u00fcnlichem Pokal, der Rebe Feuertrank;", "tokens": ["Vor", "ihm", ",", "in", "gr\u00fcn\u00b7li\u00b7chem", "Po\u00b7kal", ",", "der", "Re\u00b7be", "Feu\u00b7er\u00b7trank", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Zur Seite seiner Jugend Spro\u00df, sich lehnend an den Zweigen,", "tokens": ["Zur", "Sei\u00b7te", "sei\u00b7ner", "Ju\u00b7gend", "Spro\u00df", ",", "sich", "leh\u00b7nend", "an", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ein ernster Vierziger, vernahm des Alten Wort in Schweigen.", "tokens": ["Ein", "erns\u00b7ter", "Vier\u00b7zi\u00b7ger", ",", "ver\u00b7nahm", "des", "Al\u00b7ten", "Wort", "in", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "\u201esohn\u201c, sprach der Patriarch, es klang die Stimme schier", "tokens": ["\u201e", "sohn", "\u201c", ",", "sprach", "der", "Pat\u00b7ri\u00b7arch", ",", "es", "klang", "die", "Stim\u00b7me", "schier"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bewegt:", "tokens": ["be\u00b7wegt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u201edas Kissen f\u00fcr mein Sterbebett du hast es weich gelegt;", "tokens": ["\u201e", "das", "Kis\u00b7sen", "f\u00fcr", "mein", "Ster\u00b7be\u00b7bett", "du", "hast", "es", "weich", "ge\u00b7legt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ich wei\u00df es, eine Thr\u00e4ne wird das Leichentuch mir netzen,", "tokens": ["Ich", "wei\u00df", "es", ",", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "wird", "das", "Lei\u00b7chen\u00b7tuch", "mir", "net\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "In meinen Sessel wird dereinst ein Ehrenmann sich setzen.", "tokens": ["In", "mei\u00b7nen", "Ses\u00b7sel", "wird", "de\u00b7reinst", "ein", "Eh\u00b7ren\u00b7mann", "sich", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Zu Gottes Ehr' und deiner Pflicht, und nach der Vordern", "tokens": ["Zu", "Got\u00b7tes", "Ehr'", "und", "dei\u00b7ner", "Pflicht", ",", "und", "nach", "der", "Vor\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Art,", "tokens": ["Art", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zog ich in aller Treue dich, als schon dein Kinn behaart.", "tokens": ["Zog", "ich", "in", "al\u00b7ler", "Treu\u00b7e", "dich", ",", "als", "schon", "dein", "Kinn", "be\u00b7haart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Nicht will die neue Weise mir zum alten Haupte gehen,", "tokens": ["Nicht", "will", "die", "neu\u00b7e", "Wei\u00b7se", "mir", "zum", "al\u00b7ten", "Haup\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "ADJA", "NN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Ein Sohn hat seinen Herrn, so lang zwei Augen offen stehen.", "tokens": ["Ein", "Sohn", "hat", "sei\u00b7nen", "Herrn", ",", "so", "lang", "zwei", "Au\u00b7gen", "of\u00b7fen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "CARD", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Mein Vater, \u2014 tr\u00f6st ihn Gott, er fiel in einem guten", "tokens": ["Mein", "Va\u00b7ter", ",", "tr\u00f6st", "ihn", "Gott", ",", "er", "fiel", "in", "ei\u00b7nem", "gu\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "$(", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Straus! \u2014", "tokens": ["Straus", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "War Diener seinem F\u00fcrsten und ein K\u00f6nig seinem Haus,", "tokens": ["War", "Die\u00b7ner", "sei\u00b7nem", "F\u00fcrs\u00b7ten", "und", "ein", "K\u00f6\u00b7nig", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sein treues Auge wu\u00dfte wohl der Kinder Heil zu wahren\u201a", "tokens": ["Sein", "treu\u00b7es", "Au\u00b7ge", "wu\u00df\u00b7te", "wohl", "der", "Kin\u00b7der", "Heil", "zu", "wah\u00b7ren", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Den letzten Schlag von seiner Hand f\u00fchlt ich mit zwanzig", "tokens": ["Den", "letz\u00b7ten", "Schlag", "von", "sei\u00b7ner", "Hand", "f\u00fchlt", "ich", "mit", "zwan\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "CARD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jahren.", "tokens": ["Jah\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "So macht' er mich zum Mann, wie du, mein Sohn, zum", "tokens": ["So", "macht'", "er", "mich", "zum", "Mann", ",", "wie", "du", ",", "mein", "Sohn", ",", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "PWAV", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "frohen Greis,", "tokens": ["fro\u00b7hen", "Greis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zum Mann der tragen kann und sich im Gl\u00fcck zu fassen wei\u00df,", "tokens": ["Zum", "Mann", "der", "tra\u00b7gen", "kann", "und", "sich", "im", "Gl\u00fcck", "zu", "fas\u00b7sen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "VVINF", "VMFIN", "KON", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wie mag, wer seiner Launen Knecht, ein Herrenamt be-", "tokens": ["Wie", "mag", ",", "wer", "sei\u00b7ner", "Lau\u00b7nen", "Knecht", ",", "ein", "Her\u00b7ren\u00b7amt", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "$,", "PWS", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "zwingen?", "tokens": ["zwin\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Wer seiner Knospe Kraft verpra\u00dft, wie m\u00f6cht er Fr\u00fcchte", "tokens": ["Wer", "sei\u00b7ner", "Knos\u00b7pe", "Kraft", "ver\u00b7pra\u00dft", ",", "wie", "m\u00f6cht", "er", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVPP", "$,", "PWAV", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "bringen?", "tokens": ["brin\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Nur von der Pike dient sich's recht zum braven General.", "tokens": ["Nur", "von", "der", "Pi\u00b7ke", "dient", "sich's", "recht", "zum", "bra\u00b7ven", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PIS", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Gesegnet sey die Hand die mir erspart der Thorheit Wahl!", "tokens": ["Ge\u00b7seg\u00b7net", "sey", "die", "Hand", "die", "mir", "er\u00b7spart", "der", "Thor\u00b7heit", "Wahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Mit tausend Thr\u00e4nen hab' ich sie in unsre Gruft getragen,", "tokens": ["Mit", "tau\u00b7send", "Thr\u00e4\u00b7nen", "hab'", "ich", "sie", "in", "uns\u00b7re", "Gruft", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Denn eines Vaters heilge Hand hat nie zu hart geschlagen.", "tokens": ["Denn", "ei\u00b7nes", "Va\u00b7ters", "heil\u00b7ge", "Hand", "hat", "nie", "zu", "hart", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Mein Haar ist grau, mein bl\u00f6des Aug' hat deinen Spro\u00df", "tokens": ["Mein", "Haar", "ist", "grau", ",", "mein", "bl\u00f6\u00b7des", "Aug'", "hat", "dei\u00b7nen", "Spro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gesehn,", "tokens": ["ge\u00b7sehn", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Bald f\u00fcllst du meinen Sitz, und er wird horchend vor dir", "tokens": ["Bald", "f\u00fcllst", "du", "mei\u00b7nen", "Sitz", ",", "und", "er", "wird", "hor\u00b7chend", "vor", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "stehn.", "tokens": ["stehn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Gedenk der Rechenschaft, mein Sohn, lehr deinen Blick ihn", "tokens": ["Ge\u00b7denk", "der", "Re\u00b7chen\u00b7schaft", ",", "mein", "Sohn", ",", "lehr", "dei\u00b7nen", "Blick", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "lesen,", "tokens": ["le\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Gehorsam sey er dir, wie du gehorsam mir gewesen!\u201c", "tokens": ["Ge\u00b7hor\u00b7sam", "sey", "er", "dir", ",", "wie", "du", "ge\u00b7hor\u00b7sam", "mir", "ge\u00b7we\u00b7sen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "VVFIN", "PPER", "VAPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "So sprach der Patriarch, und schritt entlang die Buchenhall',", "tokens": ["So", "sprach", "der", "Pat\u00b7ri\u00b7arch", ",", "und", "schritt", "ent\u00b7lang", "die", "Bu\u00b7chen\u00b7hall'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ehrf\u00fcrchtig folgte ihm der Sohn, wie F\u00fcrsten der Vasall,", "tokens": ["Ehr\u00b7f\u00fcrch\u00b7tig", "folg\u00b7te", "ihm", "der", "Sohn", ",", "wie", "F\u00fcrs\u00b7ten", "der", "Va\u00b7sall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und seinen Knaben winkt er sacht herbei vom Bl\u00fcthenhagen,", "tokens": ["Und", "sei\u00b7nen", "Kna\u00b7ben", "winkt", "er", "sacht", "her\u00b7bei", "vom", "Bl\u00fc\u00b7then\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Lie\u00df k\u00fcssen ihn des Alten Hand, und seinen Stab ihn tragen.", "tokens": ["Lie\u00df", "k\u00fcs\u00b7sen", "ihn", "des", "Al\u00b7ten", "Hand", ",", "und", "sei\u00b7nen", "Stab", "ihn", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}