{"textgrid.poem.39800": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An die deutschen Frauen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun uns Freiheit ist erstritten", "tokens": ["Nun", "uns", "Frei\u00b7heit", "ist", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Gedanken, That und Wort,", "tokens": ["In", "Ge\u00b7dan\u00b7ken", ",", "That", "und", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffet denn die fremden Sitten", "tokens": ["Schaf\u00b7fet", "denn", "die", "frem\u00b7den", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr auch, deutsche Frauen, fort!", "tokens": ["Ihr", "auch", ",", "deut\u00b7sche", "Frau\u00b7en", ",", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist nur erst aus euern Herzen", "tokens": ["Ist", "nur", "erst", "aus", "eu\u00b7ern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller welsche Sinn verbannt,", "tokens": ["Al\u00b7ler", "wel\u00b7sche", "Sinn", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenes Locken, Buhlen, Scherzen \u2013", "tokens": ["Je\u00b7nes", "Lo\u00b7cken", ",", "Buh\u00b7len", ",", "Scher\u00b7zen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weicht von selbst der andre Tand.", "tokens": ["Weicht", "von", "selbst", "der", "and\u00b7re", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00fchmt nicht lange hier und dorten", "tokens": ["R\u00fchmt", "nicht", "lan\u00b7ge", "hier", "und", "dor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vaterl\u00e4ndischen Verein;", "tokens": ["Va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7schen", "Ver\u00b7ein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All' auf einmal aller Orten", "tokens": ["All'", "auf", "ein\u00b7mal", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen deutsche Weiber sein.", "tokens": ["Sol\u00b7len", "deut\u00b7sche", "Wei\u00b7ber", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht in hohen Saales Mitte,", "tokens": ["Nicht", "in", "ho\u00b7hen", "Saa\u00b7les", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der alten Eitelkeit,", "tokens": ["In", "der", "al\u00b7ten", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der \u00fcpp'gen Frankensitte,", "tokens": ["Nach", "der", "\u00fcpp'\u00b7gen", "Fran\u00b7ken\u00b7sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die deutsche Tracht geweiht!", "tokens": ["Wird", "die", "deut\u00b7sche", "Tracht", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fromm und mild soll sie gedeihen", "tokens": ["Fromm", "und", "mild", "soll", "sie", "ge\u00b7dei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still in jedem K\u00e4mmerlein,", "tokens": ["Still", "in", "je\u00b7dem", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandeln soll sie dann im Freien,", "tokens": ["Wan\u00b7deln", "soll", "sie", "dann", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingeweiht vom Sonnenschein.", "tokens": ["Ein\u00b7ge\u00b7weiht", "vom", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht in stolzem Haargeflechte,", "tokens": ["Nicht", "in", "stol\u00b7zem", "Haar\u00b7ge\u00b7flech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht in Sammt- und Seide-Glanz", "tokens": ["Nicht", "in", "Samm\u00b7t", "und", "Sei\u00b7de\u00b7Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "TRUNC", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sollt ihr suchen euch das Rechte,", "tokens": ["Sollt", "ihr", "su\u00b7chen", "euch", "das", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kinder unsres deutschen Lands.", "tokens": ["Kin\u00b7der", "uns\u00b7res", "deut\u00b7schen", "Lands", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kr\u00e4nze schweben von den Eichen", "tokens": ["Kr\u00e4n\u00b7ze", "schwe\u00b7ben", "von", "den", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch von selbst ins schlichte Haar,", "tokens": ["Euch", "von", "selbst", "ins", "schlich\u00b7te", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frische Waldesbl\u00fcten reichen", "tokens": ["Fri\u00b7sche", "Wal\u00b7des\u00b7bl\u00fc\u00b7ten", "rei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eurer Brust zum Schmuck sich dar.", "tokens": ["Eu\u00b7rer", "Brust", "zum", "Schmuck", "sich", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eure lieben Angesichter", "tokens": ["Eu\u00b7re", "lie\u00b7ben", "An\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind noch aus der alten Zeit,", "tokens": ["Sind", "noch", "aus", "der", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie sie Maler uns und Dichter", "tokens": ["Wie", "sie", "Ma\u00b7ler", "uns", "und", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "PPER", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst getreulich konterfeit.", "tokens": ["Einst", "ge\u00b7treu\u00b7lich", "kon\u00b7ter\u00b7feit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jene herrlichen Geschlechter", "tokens": ["Je\u00b7ne", "herr\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Sch\u00f6nstes auch gekannt,", "tokens": ["Ha\u00b7ben", "Sch\u00f6ns\u00b7tes", "auch", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hlet ihrer hohen T\u00f6chter", "tokens": ["W\u00e4h\u00b7let", "ih\u00b7rer", "ho\u00b7hen", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Edel zierliches Gewand.", "tokens": ["E\u00b7del", "zier\u00b7li\u00b7ches", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Vieles wirkten sie und woben", "tokens": ["Vie\u00b7les", "wirk\u00b7ten", "sie", "und", "wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem stillen Frauenkreis,", "tokens": ["In", "dem", "stil\u00b7len", "Frau\u00b7en\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also m\u00f6get ihr erproben", "tokens": ["Al\u00b7so", "m\u00f6\u00b7get", "ihr", "er\u00b7pro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu den anererbten Flei\u00df.", "tokens": ["Neu", "den", "a\u00b7ner\u00b7erb\u00b7ten", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie sich's Einmal dann gestaltet,", "tokens": ["Wie", "sich's", "Ein\u00b7mal", "dann", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Biet' es stets dem Fremden Trutz,", "tokens": ["Biet'", "es", "stets", "dem", "Frem\u00b7den", "Trutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn was wechselt und veraltet,", "tokens": ["Denn", "was", "wech\u00b7selt", "und", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nur eitler, welscher Putz.", "tokens": ["Ist", "nur", "eit\u00b7ler", ",", "wel\u00b7scher", "Putz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ganz in seine strenge H\u00fclle", "tokens": ["Ganz", "in", "sei\u00b7ne", "stren\u00b7ge", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00df' es rings die Glieder ein,", "tokens": ["Schlie\u00df'", "es", "rings", "die", "Glie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unerforscht, wie euer Wille,", "tokens": ["Un\u00b7er\u00b7forscht", ",", "wie", "eu\u00b7er", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen eure Reize sein.", "tokens": ["Sol\u00b7len", "eu\u00b7re", "Rei\u00b7ze", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Mag der Fremde l\u00fcstern sp\u00e4hen:", "tokens": ["Mag", "der", "Frem\u00b7de", "l\u00fcs\u00b7tern", "sp\u00e4\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigend, eingeh\u00fcllt in Zucht", "tokens": ["Schwei\u00b7gend", ",", "ein\u00b7ge\u00b7h\u00fcllt", "in", "Zucht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird die deutsche Jungfrau stehen,", "tokens": ["Wird", "die", "deut\u00b7sche", "Jung\u00b7frau", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unzug\u00e4nglich, unversucht.", "tokens": ["Un\u00b7zu\u00b7g\u00e4ng\u00b7lich", ",", "un\u00b7ver\u00b7sucht", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Von des Landes treuen S\u00f6hnen", "tokens": ["Von", "des", "Lan\u00b7des", "treu\u00b7en", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehrerbietig angeschaut,", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "an\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reine W\u00fcnsche nur zu kr\u00f6nen,", "tokens": ["Rei\u00b7ne", "W\u00fcn\u00b7sche", "nur", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4chst sie still heran zur Braut.", "tokens": ["W\u00e4chst", "sie", "still", "he\u00b7ran", "zur", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Solches singen wir in Liedern", "tokens": ["Sol\u00b7ches", "sin\u00b7gen", "wir", "in", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dichtend euch, ihr Frauen rein!", "tokens": ["Dich\u00b7tend", "euch", ",", "ihr", "Frau\u00b7en", "rein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt es mit der That erwiedern,", "tokens": ["Wollt", "es", "mit", "der", "That", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsch an Leib und Seele sein!", "tokens": ["Deutsch", "an", "Leib", "und", "See\u00b7le", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR