{"textgrid.poem.67930": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "26. Der Wassermann", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bb O Mutter, guten Rath mir leiht,", "tokens": ["\u00bb", "O", "Mut\u00b7ter", ",", "gu\u00b7ten", "Rath", "mir", "leiht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$,", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie soll ich bekommen das sch\u00f6ne Maid?\u00ab", "tokens": ["Wie", "soll", "ich", "be\u00b7kom\u00b7men", "das", "sch\u00f6\u00b7ne", "Maid", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie baut ihm ein Pferd von Wasser klar,", "tokens": ["Sie", "baut", "ihm", "ein", "Pferd", "von", "Was\u00b7ser", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Zaum und Sattel von Sande gar.", "tokens": ["Und", "Zaum", "und", "Sat\u00b7tel", "von", "San\u00b7de", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie kleidet ihn an zum Ritter fein,", "tokens": ["Sie", "klei\u00b7det", "ihn", "an", "zum", "Rit\u00b7ter", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So ritt er Marienkirchhof hinein.", "tokens": ["So", "ritt", "er", "Ma\u00b7ri\u00b7en\u00b7kirch\u00b7hof", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er band sein Pferd an die Kirchenth\u00fcr,", "tokens": ["Er", "band", "sein", "Pferd", "an", "die", "Kir\u00b7chent\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er ging um die Kirch dreimal und vier.", "tokens": ["Er", "ging", "um", "die", "Kirch", "drei\u00b7mal", "und", "vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "KON", "CARD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Wassermann in die Kirch ging ein,", "tokens": ["Der", "Was\u00b7ser\u00b7mann", "in", "die", "Kirch", "ging", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie kamen um ihn, gro\u00df und klein.", "tokens": ["Sie", "ka\u00b7men", "um", "ihn", ",", "gro\u00df", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Priester eben stand vorm Altar:", "tokens": ["Der", "Pries\u00b7ter", "e\u00b7ben", "stand", "vorm", "Al\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbwas kommt f\u00fcr ein blanker Ritter dar.\u00ab", "tokens": ["\u00bb", "was", "kommt", "f\u00fcr", "ein", "blan\u00b7ker", "Rit\u00b7ter", "dar", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Das sch\u00f6ne M\u00e4dchen lacht in sich:", "tokens": ["Das", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "lacht", "in", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo w\u00e4r der blanke Ritter f\u00fcr mich!\u00ab", "tokens": ["\u00bb", "o", "w\u00e4r", "der", "blan\u00b7ke", "Rit\u00b7ter", "f\u00fcr", "mich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Er trat \u00fcber einen Stuhl und zwei:", "tokens": ["Er", "trat", "\u00fc\u00b7ber", "ei\u00b7nen", "Stuhl", "und", "zwei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "CARD", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "\u00bbo M\u00e4dchen gib mir Wort und Treu.\u00ab", "tokens": ["\u00bb", "o", "M\u00e4d\u00b7chen", "gib", "mir", "Wort", "und", "Treu", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "VVIMP", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er trat \u00fcber St\u00fchle drei und vier:", "tokens": ["Er", "trat", "\u00fc\u00b7ber", "St\u00fch\u00b7le", "drei", "und", "vier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "CARD", "KON", "CARD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbo sch\u00f6nes M\u00e4dchen zieh mit mir.\u00ab", "tokens": ["\u00bb", "o", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "zieh", "mit", "mir", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Das sch\u00f6ne M\u00e4dchen die Hand ihm reicht:", "tokens": ["Das", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "die", "Hand", "ihm", "reicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbhier hast meine Treu, ich folg dir leicht.\u00ab", "tokens": ["\u00bb", "hier", "hast", "mei\u00b7ne", "Treu", ",", "ich", "folg", "dir", "leicht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Sie gingen hinaus mit Hochzeitschaar,", "tokens": ["Sie", "gin\u00b7gen", "hin\u00b7aus", "mit", "Hoch\u00b7zeit\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie tanzten freudig und ohn Gefahr.", "tokens": ["Sie", "tanz\u00b7ten", "freu\u00b7dig", "und", "ohn", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Sie tanzten nieder bis an den Strand,", "tokens": ["Sie", "tanz\u00b7ten", "nie\u00b7der", "bis", "an", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie waren allein jetzt Hand in Hand.", "tokens": ["Sie", "wa\u00b7ren", "al\u00b7lein", "jetzt", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbhalt, sch\u00f6nes M\u00e4dchen, das Ro\u00df mir hier!", "tokens": ["\u00bb", "halt", ",", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", ",", "das", "Ro\u00df", "mir", "hier", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das niedlichste Schiffchen bring ich dir.\u00ab", "tokens": ["Das", "nied\u00b7lichs\u00b7te", "Schiff\u00b7chen", "bring", "ich", "dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Und als sie kamen auf 'n weissen Sand,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "auf", "'n", "weis\u00b7sen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da kehrten sich alle Schiffe zu Land.", "tokens": ["Da", "kehr\u00b7ten", "sich", "al\u00b7le", "Schif\u00b7fe", "zu", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Und als sie kamen auf den Sund,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "auf", "den", "Sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sch\u00f6ne M\u00e4dchen sank zu Grund.", "tokens": ["Das", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "sank", "zu", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Noch lange h\u00f6rten am Lande sie,", "tokens": ["Noch", "lan\u00b7ge", "h\u00f6r\u00b7ten", "am", "Lan\u00b7de", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie das sch\u00f6ne M\u00e4dchen im Wasser schrie.", "tokens": ["Wie", "das", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "im", "Was\u00b7ser", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.17": {"line.1": {"text": "Ich rath euch, Jungfern, was ich kann:", "tokens": ["Ich", "rath", "euch", ",", "Jung\u00b7fern", ",", "was", "ich", "kann", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht nicht in Tanz mit dem Wassermann.", "tokens": ["Geht", "nicht", "in", "Tanz", "mit", "dem", "Was\u00b7ser\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}