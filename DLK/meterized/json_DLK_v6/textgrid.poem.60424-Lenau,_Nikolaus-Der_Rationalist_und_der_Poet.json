{"textgrid.poem.60424": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Rationalist und der Poet", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbfreund, du sitzest hier auf weichem Moose,", "tokens": ["\u00bb", "freund", ",", "du", "sit\u00b7zest", "hier", "auf", "wei\u00b7chem", "Moo\u00b7se", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ins Geruchzeug duftet dir die Rose,", "tokens": ["Ins", "Ge\u00b7ruch\u00b7zeug", "duf\u00b7tet", "dir", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um dein Antlitz Fr\u00fchlingswinde wallen,", "tokens": ["Um", "dein", "Ant\u00b7litz", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und da dr\u00fcben l\u00e4rmen Nachtigallen.", "tokens": ["Und", "da", "dr\u00fc\u00b7ben", "l\u00e4r\u00b7men", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Darum singst du hier ein Lied vers\u00f6hnend,", "tokens": ["Da\u00b7rum", "singst", "du", "hier", "ein", "Lied", "ver\u00b7s\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weich und duftig, lind und z\u00e4rtlich t\u00f6nend.", "tokens": ["Weich", "und", "duf\u00b7tig", ",", "lind", "und", "z\u00e4rt\u00b7lich", "t\u00f6\u00b7nend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "S\u00e4\u00dfest du auf einem harten Stumpfe,", "tokens": ["S\u00e4\u00b7\u00dfest", "du", "auf", "ei\u00b7nem", "har\u00b7ten", "Stump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "K\u00e4me dir der Duft von einem Sumpfe,", "tokens": ["K\u00e4\u00b7me", "dir", "der", "Duft", "von", "ei\u00b7nem", "Sump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sp\u00fcrtest du den Herbstwind frostig wehen,", "tokens": ["Sp\u00fcr\u00b7test", "du", "den", "Herbst\u00b7wind", "fros\u00b7tig", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "W\u00e4rst du hier umkr\u00e4chzt von rauhen Kr\u00e4hen:", "tokens": ["W\u00e4rst", "du", "hier", "um\u00b7kr\u00e4chzt", "von", "rau\u00b7hen", "Kr\u00e4\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ha! ich wette, hart und widrig kl\u00e4nge,", "tokens": ["Ha", "!", "ich", "wet\u00b7te", ",", "hart", "und", "wid\u00b7rig", "kl\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "K\u00fchl und rauh, was deine Muse s\u00e4nge.", "tokens": ["K\u00fchl", "und", "rauh", ",", "was", "dei\u00b7ne", "Mu\u00b7se", "s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "W\u00e4re dort die Wolke losgebrochen,", "tokens": ["W\u00e4\u00b7re", "dort", "die", "Wol\u00b7ke", "los\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "H\u00e4ttest du dich ohne Lied verkrochen.", "tokens": ["H\u00e4t\u00b7test", "du", "dich", "oh\u00b7ne", "Lied", "ver\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Hundert Dinge st\u00f6ren dirs Gehege,", "tokens": ["Hun\u00b7dert", "Din\u00b7ge", "st\u00f6\u00b7ren", "dirs", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Weisen deiner Phantasie die Wege,", "tokens": ["Wei\u00b7sen", "dei\u00b7ner", "Phan\u00b7ta\u00b7sie", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Hundert Mitarbeitern bist du pflichtig;", "tokens": ["Hun\u00b7dert", "Mit\u00b7ar\u00b7bei\u00b7tern", "bist", "du", "pflich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "All dein Dichtertreiben find ich nichtig.\u00ab", "tokens": ["All", "dein", "Dich\u00b7ter\u00b7trei\u00b7ben", "find", "ich", "nich\u00b7tig", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Also spricht der Rationaliste,", "tokens": ["Al\u00b7so", "spricht", "der", "Ra\u00b7ti\u00b7o\u00b7na\u00b7lis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Der den Dichter heimlich hat belauert,", "tokens": ["Der", "den", "Dich\u00b7ter", "heim\u00b7lich", "hat", "be\u00b7lau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Stolzer Hahn auf dem Verstandesmiste,", "tokens": ["Stol\u00b7zer", "Hahn", "auf", "dem", "Ver\u00b7stan\u00b7des\u00b7mis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Da\u00df dem Dichter vor dem Wichte schauert.", "tokens": ["Da\u00df", "dem", "Dich\u00b7ter", "vor", "dem", "Wich\u00b7te", "schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Dichter spricht: \u00bbWenn V\u00f6gel, Blumen, Winde", "tokens": ["Dich\u00b7ter", "spricht", ":", "\u00bb", "Wenn", "V\u00f6\u00b7gel", ",", "Blu\u00b7men", ",", "Win\u00b7de"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$.", "$(", "KOUS", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Und das ganze liebe Lenzgesinde", "tokens": ["Und", "das", "gan\u00b7ze", "lie\u00b7be", "Lenz\u00b7ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Meinem Liede helfen, wirds ihm frommen,", "tokens": ["Mei\u00b7nem", "Lie\u00b7de", "hel\u00b7fen", ",", "wirds", "ihm", "from\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VAFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Und es wird der Welt zu Herzen kommen.", "tokens": ["Und", "es", "wird", "der", "Welt", "zu", "Her\u00b7zen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "H\u00e4tt ich rauhen Felsensitz erklettert,", "tokens": ["H\u00e4tt", "ich", "rau\u00b7hen", "Fel\u00b7sen\u00b7sitz", "er\u00b7klet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Schw\u00fcl bedr\u00fcckt von einer Sumpfeswolke,", "tokens": ["Schw\u00fcl", "be\u00b7dr\u00fcckt", "von", "ei\u00b7ner", "Sump\u00b7fes\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Rauh umkr\u00e4chzt von einem Rabenvolke,", "tokens": ["Rauh", "um\u00b7kr\u00e4chzt", "von", "ei\u00b7nem", "Ra\u00b7ben\u00b7vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Oder auch von Hagelschlag umwettert:", "tokens": ["O\u00b7der", "auch", "von", "Ha\u00b7gel\u00b7schlag", "um\u00b7wet\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "S\u00e4ng ich! und in meinem Liede schalten", "tokens": ["S\u00e4ng", "ich", "!", "und", "in", "mei\u00b7nem", "Lie\u00b7de", "schal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Lie\u00df' ich gern auch die Naturgewalten.", "tokens": ["Lie\u00df'", "ich", "gern", "auch", "die", "Na\u00b7tur\u00b7ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Aber gleich entfl\u00fcchten Lust und Schmerzen,", "tokens": ["A\u00b7ber", "gleich", "ent\u00b7fl\u00fcch\u00b7ten", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Dringt heran mir ein Gesicht wie deines,", "tokens": ["Dringt", "he\u00b7ran", "mir", "ein", "Ge\u00b7sicht", "wie", "dei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "ART", "NN", "KOKOM", "PPOSAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Kalt genug, mir trotz des Maienscheines", "tokens": ["Kalt", "ge\u00b7nug", ",", "mir", "trotz", "des", "Mai\u00b7en\u00b7schei\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Aus der Welt die Poesie zu merzen.\u00ab", "tokens": ["Aus", "der", "Welt", "die", "Poe\u00b7sie", "zu", "mer\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}