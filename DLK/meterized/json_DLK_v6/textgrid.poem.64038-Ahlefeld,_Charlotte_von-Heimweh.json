{"textgrid.poem.64038": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zieht uns aus der Fremde bunten Kreisen", "tokens": ["Was", "zieht", "uns", "aus", "der", "Frem\u00b7de", "bun\u00b7ten", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Heimath hin mit st\u00fcrmischer Gewalt,", "tokens": ["Zur", "Hei\u00b7math", "hin", "mit", "st\u00fcr\u00b7mi\u00b7scher", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ginge ewig dort in sanften Gleisen", "tokens": ["Als", "gin\u00b7ge", "e\u00b7wig", "dort", "in", "sanf\u00b7ten", "Glei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADJD", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Strom des Gl\u00fccks, der sonst uns nirgends wallt?", "tokens": ["Der", "Strom", "des", "Gl\u00fccks", ",", "der", "sonst", "uns", "nir\u00b7gends", "wallt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was l\u00f6schet in verborgnen, heissen Thr\u00e4nen", "tokens": ["Was", "l\u00f6\u00b7schet", "in", "ver\u00b7borg\u00b7nen", ",", "heis\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Glanz, der d\u00fcster uns verbleicht,", "tokens": ["Des", "Le\u00b7bens", "Glanz", ",", "der", "d\u00fcs\u00b7ter", "uns", "ver\u00b7bleicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn still herbei gef\u00fchrt von zartem Sehnen", "tokens": ["Wenn", "still", "her\u00b7bei", "ge\u00b7f\u00fchrt", "von", "zar\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Bild des Vaterlandes uns beschleicht? \u2013", "tokens": ["Das", "Bild", "des", "Va\u00b7ter\u00b7lan\u00b7des", "uns", "be\u00b7schleicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist das ", "tokens": ["Es", "ist", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In fremden L\u00e4ndern schauernd uns ergreift,", "tokens": ["In", "frem\u00b7den", "L\u00e4n\u00b7dern", "schau\u00b7ernd", "uns", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das nach und nach von dem erkrankten Herzen", "tokens": ["Das", "nach", "und", "nach", "von", "dem", "er\u00b7krank\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "KON", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Bl\u00fcthenschimmer jedes Frohsinns streift.", "tokens": ["Den", "Bl\u00fc\u00b7then\u00b7schim\u00b7mer", "je\u00b7des", "Froh\u00b7sinns", "streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zum Vaterland mit m\u00e4chtiger Gewalt,", "tokens": ["Zum", "Va\u00b7ter\u00b7land", "mit", "m\u00e4ch\u00b7ti\u00b7ger", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tauscht den Trieb nach Neuheit und nach Reisen", "tokens": ["Und", "tauscht", "den", "Trieb", "nach", "Neu\u00b7heit", "und", "nach", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In W\u00fcnsche um nach stetem Aufenthalt.", "tokens": ["In", "W\u00fcn\u00b7sche", "um", "nach", "ste\u00b7tem", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}