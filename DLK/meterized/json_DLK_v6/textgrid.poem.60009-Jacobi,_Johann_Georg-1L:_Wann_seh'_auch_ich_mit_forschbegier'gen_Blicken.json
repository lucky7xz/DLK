{"textgrid.poem.60009": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann seh' auch ich mit forschbegier'gen Blicken", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann seh' auch ich mit forschbegier'gen Blicken", "tokens": ["Wann", "seh'", "auch", "ich", "mit", "forschbe\u00b7gier'\u00b7gen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den jungen wunderbaren Mann,", "tokens": ["Den", "jun\u00b7gen", "wun\u00b7der\u00b7ba\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lieder singt, den Musen zum Entz\u00fccken;", "tokens": ["Der", "Lie\u00b7der", "singt", ",", "den", "Mu\u00b7sen", "zum", "Ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der dich bezaubern kann?", "tokens": ["Der", "dich", "be\u00b7zau\u00b7bern", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So ganz bezaubern, da\u00df du von Vergn\u00fcgen", "tokens": ["So", "ganz", "be\u00b7zau\u00b7bern", ",", "da\u00df", "du", "von", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Berauschet bist, und mich j\u00fcngsthin", "tokens": ["Be\u00b7rau\u00b7schet", "bist", ",", "und", "mich", "j\u00fcng\u00b7sthin"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "KON", "PPER", "ADV"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Vergessen hast, und lange mir geschwiegen,", "tokens": ["Ver\u00b7ges\u00b7sen", "hast", ",", "und", "lan\u00b7ge", "mir", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir, deiner Sch\u00e4ferin!", "tokens": ["Mir", ",", "dei\u00b7ner", "Sch\u00e4\u00b7fe\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Suada mu\u00df ihn auferzogen haben;", "tokens": ["Die", "Sua\u00b7da", "mu\u00df", "ihn", "auf\u00b7er\u00b7zo\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach! reden mu\u00df er, wie Merkur,", "tokens": ["Ach", "!", "re\u00b7den", "mu\u00df", "er", ",", "wie", "Mer\u00b7kur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVINF", "VMFIN", "PPER", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ehedem, gleich einem Sch\u00e4ferknaben,", "tokens": ["Der", "e\u00b7he\u00b7dem", ",", "gleich", "ei\u00b7nem", "Sch\u00e4\u00b7fer\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dem Olympus fuhr,", "tokens": ["Von", "dem", "O\u00b7lym\u00b7pus", "fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und vor dem immer wachenden Bemerker", "tokens": ["Und", "vor", "dem", "im\u00b7mer", "wa\u00b7chen\u00b7den", "Be\u00b7mer\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der armen Inachide, s\u00fc\u00df", "tokens": ["Der", "ar\u00b7men", "I\u00b7na\u00b7chi\u00b7de", ",", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kl\u00e4glich sch\u00f6n, und stark, und immer st\u00e4rker", "tokens": ["Und", "kl\u00e4g\u00b7lich", "sch\u00f6n", ",", "und", "stark", ",", "und", "im\u00b7mer", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Fl\u00f6te t\u00f6nen lie\u00df,", "tokens": ["Die", "Fl\u00f6\u00b7te", "t\u00f6\u00b7nen", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und nach dem Spiel ihn mit Geschw\u00e4tz erg\u00f6tzte,", "tokens": ["Und", "nach", "dem", "Spiel", "ihn", "mit", "Ge\u00b7schw\u00e4tz", "er\u00b7g\u00f6tz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das lieblich von den Lippen flo\u00df,", "tokens": ["Das", "lieb\u00b7lich", "von", "den", "Lip\u00b7pen", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis im Entz\u00fcckungsschlummer sich das letzte", "tokens": ["Bis", "im", "Ent\u00b7z\u00fc\u00b7ckungs\u00b7schlum\u00b7mer", "sich", "das", "letz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der hundert Augen schlo\u00df!", "tokens": ["Der", "hun\u00b7dert", "Au\u00b7gen", "schlo\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}