{"dta.poem.8735": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n  Der Pilgrim vor St. Just.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Hispan'sche M\u00f6nche, schlie\u00dft mir auf die Th\u00fcr!", "tokens": ["Hi\u00b7span'\u00b7sche", "M\u00f6n\u00b7che", ",", "schlie\u00dft", "mir", "auf", "die", "Th\u00fcr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00dft hier mich ruh'n, bis Glockenton mich weckt,", "tokens": ["La\u00dft", "hier", "mich", "ruh'n", ",", "bis", "Glo\u00b7cken\u00b7ton", "mich", "weckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVFIN", "$,", "APPR", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der zum Gebet mich in die Kirche schreckt!", "tokens": ["Der", "zum", "Ge\u00b7bet", "mich", "in", "die", "Kir\u00b7che", "schreckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Bereitet mir was euer Haus vermag,", "tokens": ["Be\u00b7rei\u00b7tet", "mir", "was", "eu\u00b7er", "Haus", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ordenskleid und einen Sarkophag!", "tokens": ["Ein", "Or\u00b7dens\u00b7kleid", "und", "ei\u00b7nen", "Sar\u00b7ko\u00b7phag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "G\u00f6nnt mir die kleine Zelle, weiht mich ein,", "tokens": ["G\u00f6nnt", "mir", "die", "klei\u00b7ne", "Zel\u00b7le", ",", "weiht", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mehr als die H\u00e4lfte dieser Welt war mein.", "tokens": ["Mehr", "als", "die", "H\u00e4lf\u00b7te", "die\u00b7ser", "Welt", "war", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "PDAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Haupt, das nun der Scheere sich bequemt,", "tokens": ["Das", "Haupt", ",", "das", "nun", "der", "Schee\u00b7re", "sich", "be\u00b7quemt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit mancher Krone ward's bediademt.", "tokens": ["Mit", "man\u00b7cher", "Kro\u00b7ne", "ward's", "be\u00b7di\u00b7a\u00b7demt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Schulter, die der Kutte nun sich b\u00fcckt,", "tokens": ["Die", "Schul\u00b7ter", ",", "die", "der", "Kut\u00b7te", "nun", "sich", "b\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat kaiserlicher Hermelin geschm\u00fcckt.", "tokens": ["Hat", "kai\u00b7ser\u00b7li\u00b7cher", "Her\u00b7me\u00b7lin", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun bin ich vor dem Tod den Todten gleich,", "tokens": ["Nun", "bin", "ich", "vor", "dem", "Tod", "den", "Tod\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und fall' in Tr\u00fcmmer, wie das alte Reich.", "tokens": ["Und", "fall'", "in", "Tr\u00fcm\u00b7mer", ",", "wie", "das", "al\u00b7te", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}