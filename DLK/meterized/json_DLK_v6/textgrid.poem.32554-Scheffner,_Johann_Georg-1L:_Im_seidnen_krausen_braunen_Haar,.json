{"textgrid.poem.32554": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im seidnen krausen braunen Haar,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im seidnen krausen braunen Haar,", "tokens": ["Im", "seid\u00b7nen", "krau\u00b7sen", "brau\u00b7nen", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df j\u00fcngst der Liebesgott,", "tokens": ["Sa\u00df", "j\u00fcngst", "der", "Lie\u00b7bes\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und trieb da \u00fcbers Scheitelhaar,", "tokens": ["Und", "trieb", "da", "\u00fc\u00b7bers", "Schei\u00b7tel\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den freventlichsten Spott.", "tokens": ["Den", "fre\u00b7vent\u00b7lichs\u00b7ten", "Spott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er lachte \u00fcberlaut und schrien:", "tokens": ["Er", "lach\u00b7te", "\u00fc\u00b7berl\u00b7aut", "und", "schri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbseht diese Locken an,", "tokens": ["\u00bb", "seht", "die\u00b7se", "Lo\u00b7cken", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch macht man krau\u00df mit vieler M\u00fch,", "tokens": ["Euch", "macht", "man", "krau\u00df", "mit", "vie\u00b7ler", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier hats Natur gethan.", "tokens": ["Hier", "hats", "Na\u00b7tur", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Euch salbt man mit Pomaden ein", "tokens": ["Euch", "salbt", "man", "mit", "Po\u00b7ma\u00b7den", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00e4mmt, pudert euch erst sch\u00f6n,", "tokens": ["K\u00e4mmt", ",", "pu\u00b7dert", "euch", "erst", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies parf\u00fcmirt sich ganz allein,", "tokens": ["Dies", "par\u00b7f\u00fc\u00b7mirt", "sich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und riecht zehnmal so sch\u00f6n.\u00ab", "tokens": ["Und", "riecht", "zehn\u00b7mal", "so", "sch\u00f6n", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das weiche braune Scheitelhaar", "tokens": ["Das", "wei\u00b7che", "brau\u00b7ne", "Schei\u00b7tel\u00b7haar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lang gnung sanftm\u00fcthig, sprach:", "tokens": ["Lang", "gnung", "sanft\u00b7m\u00fct\u00b7hig", ",", "sprach", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "\u00bbprahl doch nicht so mit diesem Haar,", "tokens": ["\u00bb", "prahl", "doch", "nicht", "so", "mit", "die\u00b7sem", "Haar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und setz' so sehr uns nach.", "tokens": ["Und", "setz'", "so", "sehr", "uns", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Eh' Damon jens gesehn, ber\u00fchrt", "tokens": ["Eh'", "Da\u00b7mon", "jens", "ge\u00b7sehn", ",", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hatt' er uns l\u00e4ngst gek\u00fc\u00dft,", "tokens": ["Hatt'", "er", "uns", "l\u00e4ngst", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer weis wenn wir ihn nicht gef\u00fchrt,", "tokens": ["Wer", "weis", "wenn", "wir", "ihn", "nicht", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob er noch von dem w\u00fc\u00dft.", "tokens": ["Ob", "er", "noch", "von", "dem", "w\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ihn reizte unsrer Locken Pracht,", "tokens": ["Ihn", "reiz\u00b7te", "uns\u00b7rer", "Lo\u00b7cken", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbaut von ", "tokens": ["Er\u00b7baut", "von"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Von uns erst dreust und warm gemacht", "tokens": ["Von", "uns", "erst", "dreust", "und", "warm", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Traf sichs, da\u00df er jens fand.", "tokens": ["Traf", "sichs", ",", "da\u00df", "er", "jens", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und kurz das Haar, das wie man glaubt", "tokens": ["Und", "kurz", "das", "Haar", ",", "das", "wie", "man", "glaubt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PRELS", "KOKOM", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Sternenhimmel steht,", "tokens": ["Am", "Ster\u00b7nen\u00b7him\u00b7mel", "steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War von der Berenice Haupt", "tokens": ["War", "von", "der", "Be\u00b7re\u00b7ni\u00b7ce", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Nicht sonst wo abgem\u00e4ht \u2013 \u2013 \u00ab", "tokens": ["Nicht", "sonst", "wo", "ab\u00b7ge\u00b7m\u00e4ht", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKNEG", "ADV", "PWAV", "VVPP", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzt ward das kurze Haar auch laut,", "tokens": ["Jetzt", "ward", "das", "kur\u00b7ze", "Haar", "auch", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rief: \u00bbIch mu\u00df gestehn", "tokens": ["Und", "rief", ":", "\u00bb", "Ich", "mu\u00df", "ge\u00b7stehn"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So findt euch jeder sch\u00f6n.", "tokens": ["So", "findt", "euch", "je\u00b7der", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr schm\u00fcckt Ihr bl\u00fchendes Gesicht,", "tokens": ["Ihr", "schm\u00fcckt", "Ihr", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebt der Stirne Wei\u00df,", "tokens": ["Er\u00b7hebt", "der", "Stir\u00b7ne", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenns ", "tokens": ["Doch", "wenns"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Machts doch das Blut nicht hei\u00df.", "tokens": ["Machts", "doch", "das", "Blut", "nicht", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nur dann, wenn Kunst euch gar nicht zwingt,", "tokens": ["Nur", "dann", ",", "wenn", "Kunst", "euch", "gar", "nicht", "zwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "NN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn ihr sch\u00f6n verwirrt", "tokens": ["Und", "wenn", "ihr", "sch\u00f6n", "ver\u00b7wirrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um Doris Hals und Stirn euch schlingt,", "tokens": ["Um", "Do\u00b7ris", "Hals", "und", "Stirn", "euch", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und um den Busen irrt;", "tokens": ["Und", "um", "den", "Bu\u00b7sen", "irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dann sieht der J\u00fcngling im Tapon", "tokens": ["Dann", "sieht", "der", "J\u00fcng\u00b7ling", "im", "Ta\u00b7pon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein reitzend Ebenbild:", "tokens": ["Mein", "reit\u00b7zend", "E\u00b7ben\u00b7bild", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denkt an den weichen Wollustthron", "tokens": ["Denkt", "an", "den", "wei\u00b7chen", "Wol\u00b7lust\u00b7thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der bey ihm alles gilt.", "tokens": ["Der", "bey", "ihm", "al\u00b7les", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "K\u00fc\u00dft euch denn z\u00e4rtlich, nennt euch sch\u00f6n;", "tokens": ["K\u00fc\u00dft", "euch", "denn", "z\u00e4rt\u00b7lich", ",", "nennt", "euch", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denkt aber mich dabey,", "tokens": ["Denkt", "a\u00b7ber", "mich", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wird, so bald er mich gesehn", "tokens": ["Und", "wird", ",", "so", "bald", "er", "mich", "ge\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "$,", "ADV", "ADV", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewis euch ungtreu \u2013 \u2013 \u00ab", "tokens": ["Ge\u00b7wis", "euch", "ung\u00b7treu", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPER", "ADJD", "$(", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Als Richter sprach drauf Venus Sohn!", "tokens": ["Als", "Rich\u00b7ter", "sprach", "drauf", "Ve\u00b7nus", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PAV", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbschweigt Z\u00e4nker und h\u00f6rt mich:", "tokens": ["\u00bb", "schweigt", "Z\u00e4n\u00b7ker", "und", "h\u00f6rt", "mich", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du kleines Haar, schm\u00fcckst Venus Thron,", "tokens": ["Du", "klei\u00b7nes", "Haar", ",", "schm\u00fcckst", "Ve\u00b7nus", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorz\u00fcglich liebt sie dich.", "tokens": ["Vor\u00b7z\u00fcg\u00b7lich", "liebt", "sie", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ihr Scheitelhaare seyd mein Netz", "tokens": ["Ihr", "Schei\u00b7tel\u00b7haa\u00b7re", "seyd", "mein", "Netz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dem sich mancher f\u00e4ngt,", "tokens": ["In", "dem", "sich", "man\u00b7cher", "f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der th\u00f6richt \u00fcber mein Gesetz", "tokens": ["Der", "th\u00f6\u00b7richt", "\u00fc\u00b7ber", "mein", "Ge\u00b7setz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich l\u00e4ngst erhaben denkt.", "tokens": ["Sich", "l\u00e4ngst", "er\u00b7ha\u00b7ben", "denkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "In euren Schlingen f\u00fchr ich ihn", "tokens": ["In", "eu\u00b7ren", "Schlin\u00b7gen", "f\u00fchr", "ich", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann hin zu Venus Thron,", "tokens": ["Dann", "hin", "zu", "Ve\u00b7nus", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df das Grottchen ihn beziehn;", "tokens": ["Und", "la\u00df", "das", "Grott\u00b7chen", "ihn", "be\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich bei Psychen wohn.", "tokens": ["Wo", "ich", "bei", "Psy\u00b7chen", "wohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}