{"textgrid.poem.52067": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als man dich wandern hie\u00df, Mein Freund, des Glaubens wegen;", "tokens": ["Als", "man", "dich", "wan\u00b7dern", "hie\u00df", ",", "Mein", "Freund", ",", "des", "Glau\u00b7bens", "we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fieng sich der Glauben an in deiner Brust zu regen:", "tokens": ["Fi\u00b7eng", "sich", "der", "Glau\u00b7ben", "an", "in", "dei\u00b7ner", "Brust", "zu", "re\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Du giengst, u. nahmst dir Geld auff deinen Glauben aus,", "tokens": ["Du", "giengst", ",", "u.", "nahmst", "dir", "Geld", "auff", "dei\u00b7nen", "Glau\u00b7ben", "aus", ","], "token_info": ["word", "word", "punct", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Verschriebst voll Glauben drauff dem Nechsten Hoff u. Haus.", "tokens": ["Ver\u00b7schriebst", "voll", "Glau\u00b7ben", "drauff", "dem", "Nechs\u00b7ten", "Hoff", "u.", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Itzt, weil die Straff und Pein des Glaubens wil vergehen,", "tokens": ["Itzt", ",", "weil", "die", "Straff", "und", "Pein", "des", "Glau\u00b7bens", "wil", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "KON", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor den Glauben Hau\u00df und Hoff ohn das nicht stehen:", "tokens": ["Und", "vor", "den", "Glau\u00b7ben", "Hau\u00df", "und", "Hoff", "ohn", "das", "nicht", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KON", "VVFIN", "APPR", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verlischt des Glaubens Krafft: Denn du giebst weder Geld", "tokens": ["Ver\u00b7lischt", "des", "Glau\u00b7bens", "Krafft", ":", "Denn", "du", "giebst", "we\u00b7der", "Geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$.", "KON", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch Hau\u00df dem Glaubiger, der dir zu Fu\u00dfe f\u00e4llt.", "tokens": ["Noch", "Hau\u00df", "dem", "Glau\u00b7bi\u00b7ger", ",", "der", "dir", "zu", "Fu\u00b7\u00dfe", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das hei\u00dft auff Glauben Gott nicht Haus noch Hoff befohlen,", "tokens": ["Das", "hei\u00dft", "auff", "Glau\u00b7ben", "Gott", "nicht", "Haus", "noch", "Hoff", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "NN", "PTKNEG", "NN", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es hei\u00dft dem Nechsten Geld auff Glauben abgestohlen:", "tokens": ["Es", "hei\u00dft", "dem", "Nechs\u00b7ten", "Geld", "auff", "Glau\u00b7ben", "ab\u00b7ge\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nihm nur den Glauben an, dein Glauben nimmt dir schon,", "tokens": ["Nihm", "nur", "den", "Glau\u00b7ben", "an", ",", "dein", "Glau\u00b7ben", "nimmt", "dir", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil du nicht Glauben h\u00e4lst, des wahren Glaubens Lohn.", "tokens": ["Weil", "du", "nicht", "Glau\u00b7ben", "h\u00e4lst", ",", "des", "wah\u00b7ren", "Glau\u00b7bens", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}