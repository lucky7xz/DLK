{"dta.poem.8219": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Herrgottskinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr d\u00fcrft inde\u00df der Ruhe pflegen;", "tokens": ["Ihr", "d\u00fcrft", "in\u00b7de\u00df", "der", "Ru\u00b7he", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er giebt der Arbeit das Gedeihn", "tokens": ["Er", "giebt", "der", "Ar\u00b7beit", "das", "Ge\u00b7deihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4uft herab den Himmelssegen.", "tokens": ["Und", "tr\u00e4uft", "her\u00b7ab", "den", "Him\u00b7mels\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn dann in Bl\u00fcthe die Saaten stehn,", "tokens": ["Und", "wenn", "dann", "in", "Bl\u00fc\u00b7the", "die", "Saa\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "So l\u00e4\u00dft er die L\u00fcftlein dar\u00fcber gehn,", "tokens": ["So", "l\u00e4\u00dft", "er", "die", "L\u00fcft\u00b7lein", "da\u00b7r\u00fc\u00b7ber", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Auf da\u00df sich die Halme zusammenbeugen", "tokens": ["Auf", "da\u00df", "sich", "die", "Hal\u00b7me", "zu\u00b7sam\u00b7men\u00b7beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.7": {"text": "Und frisch aus der Bl\u00fcthe das Korn erzeugen;", "tokens": ["Und", "frisch", "aus", "der", "Bl\u00fc\u00b7the", "das", "Korn", "er\u00b7zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und h\u00e4lt am Himmel hoch die Sonne,", "tokens": ["Und", "h\u00e4lt", "am", "Him\u00b7mel", "hoch", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Alles reife in ihrer Wonne.", "tokens": ["Da\u00df", "Al\u00b7les", "rei\u00b7fe", "in", "ih\u00b7rer", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da st\u00fcnd' es den Bauern wohl pr\u00e4chtig an,", "tokens": ["Da", "st\u00fcnd'", "es", "den", "Bau\u00b7ern", "wohl", "pr\u00e4ch\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Das Alles in ihre Scheuern zu laden!", "tokens": ["Das", "Al\u00b7les", "in", "ih\u00b7re", "Scheu\u00b7ern", "zu", "la\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Gott Vater hat auch seinen Theil daran;", "tokens": ["Gott", "Va\u00b7ter", "hat", "auch", "sei\u00b7nen", "Theil", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Den will er vergaben nach seiner Gnaden.", "tokens": ["Den", "will", "er", "ver\u00b7ga\u00b7ben", "nach", "sei\u00b7ner", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Da ruft er die j\u00fcngsten Kinder sein;", "tokens": ["Da", "ruft", "er", "die", "j\u00fcng\u00b7sten", "Kin\u00b7der", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die n\u00e4hrt er selbst aus seiner Hand,", "tokens": ["Die", "n\u00e4hrt", "er", "selbst", "aus", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Rehlein, die H\u00e4slein, die W\u00fcrmlein klein", "tokens": ["Die", "Reh\u00b7lein", ",", "die", "H\u00e4s\u00b7lein", ",", "die", "W\u00fcrm\u00b7lein", "klein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Und alles Gethier in Luft und Land;", "tokens": ["Und", "al\u00b7les", "Ge\u00b7thier", "in", "Luft", "und", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Das flattert herbei und kreucht und springt,", "tokens": ["Das", "flat\u00b7tert", "her\u00b7bei", "und", "kreucht", "und", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Ist fr\u00f6hlich all zu Gottes Ehr'", "tokens": ["Ist", "fr\u00f6h\u00b7lich", "all", "zu", "Got\u00b7tes", "Ehr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PIAT", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und all gen\u00fcgsam, was er bringt.", "tokens": ["Und", "all", "ge\u00b7n\u00fcg\u00b7sam", ",", "was", "er", "bringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "De\u00df freut sich der Herrgott m\u00e4chtig sehr,", "tokens": ["De\u00df", "freut", "sich", "der", "Herr\u00b7gott", "m\u00e4ch\u00b7tig", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Er breitet weit die Arme aus", "tokens": ["Er", "brei\u00b7tet", "weit", "die", "Ar\u00b7me", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und spricht in Liebe \u00fcberaus:", "tokens": ["Und", "spricht", "in", "Lie\u00b7be", "\u00fc\u00b7be\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "All, was da lebet, soll sich freun,", "tokens": ["All", ",", "was", "da", "le\u00b7bet", ",", "soll", "sich", "freun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Seid Alle von den Kindern mein;", "tokens": ["Seid", "Al\u00b7le", "von", "den", "Kin\u00b7dern", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIS", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und will euch drum doch nicht vergessen,", "tokens": ["Und", "will", "euch", "drum", "doch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PAV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Da\u00df ihr nichts k\u00f6nnt als springen und fressen.", "tokens": ["Da\u00df", "ihr", "nichts", "k\u00f6nnt", "als", "sprin\u00b7gen", "und", "fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "KOKOM", "VVFIN", "KON", "VVINF", "$."], "meter": "--++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Hat jedes seinen eignen Ton!", "tokens": ["Hat", "je\u00b7des", "sei\u00b7nen", "eig\u00b7nen", "Ton", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ihr sollt euch tummeln frisch im Gr\u00fcnen;", "tokens": ["Ihr", "sollt", "euch", "tum\u00b7meln", "frisch", "im", "Gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Doch m\u00fcndig ist der Mensch, mein Sohn;", "tokens": ["Doch", "m\u00fcn\u00b7dig", "ist", "der", "Mensch", ",", "mein", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Drum mag er selbst sein Brod verdienen!", "tokens": ["Drum", "mag", "er", "selbst", "sein", "Brod", "ver\u00b7die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}