{"textgrid.poem.56980": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Problem", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zw\u00f6lf-Elf kam auf sein Problem", "tokens": ["Der", "Zw\u00f6l\u00b7fElf", "kam", "auf", "sein", "Prob\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und sprach: Ich hei\u00dfe unbequem.", "tokens": ["und", "sprach", ":", "Ich", "hei\u00b7\u00dfe", "un\u00b7be\u00b7quem", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als hie\u00df ich etwa Drei-Vier", "tokens": ["Als", "hie\u00df", "ich", "et\u00b7wa", "Drei\u00b7Vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "CARD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "statt Sieben \u2013 Gott verzeih mir!", "tokens": ["statt", "Sie\u00b7ben", "\u2013", "Gott", "ver\u00b7zeih", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "$(", "NN", "VVIMP", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und siehe da, der Zw\u00f6lf-Elf nannt sich", "tokens": ["Und", "sie\u00b7he", "da", ",", "der", "Zw\u00f6l\u00b7fElf", "nannt", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$,", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von jenem Tag ab Dreiundzwanzig.", "tokens": ["von", "je\u00b7nem", "Tag", "ab", "Drei\u00b7und\u00b7zwan\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}