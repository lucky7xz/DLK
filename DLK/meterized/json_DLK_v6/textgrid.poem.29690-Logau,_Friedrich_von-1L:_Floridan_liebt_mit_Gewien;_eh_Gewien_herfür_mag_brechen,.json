{"textgrid.poem.29690": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Floridan liebt mit Gewien; eh Gewien herf\u00fcr mag brechen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Floridan liebt mit Gewien; eh Gewien herf\u00fcr mag brechen,", "tokens": ["Flo\u00b7ri\u00b7dan", "liebt", "mit", "Ge\u00b7wi\u00b7en", ";", "eh", "Ge\u00b7wi\u00b7en", "her\u00b7f\u00fcr", "mag", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$.", "KOUS", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+---+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sagt man, da\u00df er seinen Gaul woll um einen Lauff besprechen.", "tokens": ["Sagt", "man", ",", "da\u00df", "er", "sei\u00b7nen", "Gaul", "woll", "um", "ei\u00b7nen", "Lauff", "be\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "M\u00e4urer pflegens so zu thun, da\u00df sie nach de\u00df Bogens schliessen", "tokens": ["M\u00e4u\u00b7rer", "pfle\u00b7gens", "so", "zu", "thun", ",", "da\u00df", "sie", "nach", "de\u00df", "Bo\u00b7gens", "schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+--++--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ihre B\u00f6ck und andren Zeug ab- und weg zu reumen wissen.", "tokens": ["Ih