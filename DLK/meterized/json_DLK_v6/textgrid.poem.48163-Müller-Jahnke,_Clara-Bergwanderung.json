{"textgrid.poem.48163": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Bergwanderung", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder bl\u00fcht das Heidekraut", "tokens": ["Wie\u00b7der", "bl\u00fcht", "das", "Hei\u00b7de\u00b7kraut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "auf den spinnwebgrauen Wegen;", "tokens": ["auf", "den", "spinn\u00b7web\u00b7grau\u00b7en", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcber glatten F\u00f6hrenboden", "tokens": ["\u00fc\u00b7ber", "glat\u00b7ten", "F\u00f6h\u00b7ren\u00b7bo\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gleiten lautlos unsre Schritte", "tokens": ["glei\u00b7ten", "laut\u00b7los", "uns\u00b7re", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "unserm Wanderziel entgegen:", "tokens": ["un\u00b7serm", "Wan\u00b7der\u00b7ziel", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "droben, wo der Bergwald blaut.", "tokens": ["dro\u00b7ben", ",", "wo", "der", "Berg\u00b7wald", "blaut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einmal schon zur Fr\u00fchherbstzeit", "tokens": ["Ein\u00b7mal", "schon", "zur", "Fr\u00fch\u00b7herbst\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bin ich diesen Weg gegangen,", "tokens": ["bin", "ich", "die\u00b7sen", "Weg", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6hensehnsucht in der Seele,", "tokens": ["H\u00f6\u00b7hen\u00b7sehn\u00b7sucht", "in", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bl\u00fchnde Heide mir zu F\u00fc\u00dfen,", "tokens": ["bl\u00fchn\u00b7de", "Hei\u00b7de", "mir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "fliegend Rot auf Stirn und Wangen", "tokens": ["flie\u00b7gend", "Rot", "auf", "Stirn", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und das Ziel noch meilenweit.", "tokens": ["und", "das", "Ziel", "noch", "mei\u00b7len\u00b7weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Droben, wo der Bergwald blaut,", "tokens": ["Dro\u00b7ben", ",", "wo", "der", "Berg\u00b7wald", "blaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sa\u00df die Fee auf felsgen Zinnen;", "tokens": ["sa\u00df", "die", "Fee", "auf", "fels\u00b7gen", "Zin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ihre wei\u00dfen H\u00e4nde winkten,", "tokens": ["ih\u00b7re", "wei\u00b7\u00dfen", "H\u00e4n\u00b7de", "wink\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihre seidnen Schleier flogen", "tokens": ["ih\u00b7re", "seid\u00b7nen", "Schlei\u00b7er", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wie ein zart Geweb der Spinnen", "tokens": ["wie", "ein", "zart", "Ge\u00b7web", "der", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00fcber Stein und Heidekraut.", "tokens": ["\u00fc\u00b7ber", "Stein", "und", "Hei\u00b7de\u00b7kraut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einmal schon zur Fr\u00fchherbstzeit", "tokens": ["Ein\u00b7mal", "schon", "zur", "Fr\u00fch\u00b7herbst\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ging ich fehl im M\u00e4rchenwalde.", "tokens": ["ging", "ich", "fehl", "im", "M\u00e4r\u00b7chen\u00b7wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sturmwind bog die Tannenwipfel.", "tokens": ["Sturm\u00b7wind", "bog", "die", "Tan\u00b7nen\u00b7wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fahl verschwammen alle Gipfel,", "tokens": ["Fahl", "ver\u00b7schwam\u00b7men", "al\u00b7le", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und der Schnee fiel auf die Halde \u2013", "tokens": ["und", "der", "Schnee", "fiel", "auf", "die", "Hal\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "du, wie liegt der Tag so weit!", "tokens": ["du", ",", "wie", "liegt", "der", "Tag", "so", "weit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimm den Strau\u00df von Erika \u2013", "tokens": ["Nimm", "den", "Strau\u00df", "von", "E\u00b7ri\u00b7ka", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "APPR", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00f6rst du fern die H\u00e4her rufen?", "tokens": ["h\u00f6rst", "du", "fern", "die", "H\u00e4\u00b7her", "ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor der Bergwelt Heimlichkeiten", "tokens": ["Vor", "der", "Berg\u00b7welt", "Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sind die Schleier all zerrissen \u2013", "tokens": ["sind", "die", "Schlei\u00b7er", "all", "zer\u00b7ris\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIAT", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00fcber grauen Felsenstufen", "tokens": ["\u00fc\u00b7ber", "grau\u00b7en", "Fel\u00b7sen\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sehn wir schon den Gipfel nah!", "tokens": ["sehn", "wir", "schon", "den", "Gip\u00b7fel", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}