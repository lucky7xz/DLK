{"textgrid.poem.48912": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Der gel\u00e4hmte Kranich", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbst entlaubte schon den bunten Hayn,", "tokens": ["Der", "Herbst", "ent\u00b7laub\u00b7te", "schon", "den", "bun\u00b7ten", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und streut aus kalter Luft Reif auf die Flur,", "tokens": ["Und", "streut", "aus", "kal\u00b7ter", "Luft", "Reif", "auf", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als am Gestad ein Heer von Kranichen", "tokens": ["Als", "am", "Ge\u00b7stad", "ein", "Heer", "von", "Kra\u00b7ni\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Zusammen kam, um in ein wirthbar Land,", "tokens": ["Zu\u00b7sam\u00b7men", "kam", ",", "um", "in", "ein", "wirth\u00b7bar", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUI", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jenseit des Meers, zu ziehn. Ein Kranich, den", "tokens": ["Jen\u00b7seit", "des", "Meers", ",", "zu", "ziehn", ".", "Ein", "Kra\u00b7nich", ",", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$.", "ART", "NN", "$,", "ART"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Des J\u00e4gers Pfeil am Fu\u00df getroffen, sa\u00df", "tokens": ["Des", "J\u00e4\u00b7gers", "Pfeil", "am", "Fu\u00df", "ge\u00b7trof\u00b7fen", ",", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein, betr\u00fcbt und stumm, und mehrte nicht", "tokens": ["Al\u00b7lein", ",", "be\u00b7tr\u00fcbt", "und", "stumm", ",", "und", "mehr\u00b7te", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVPP", "KON", "ADJD", "$,", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das wilde Lustgeschrey der Schw\u00e4rmenden,", "tokens": ["Das", "wil\u00b7de", "Lust\u00b7ge\u00b7schrey", "der", "Schw\u00e4r\u00b7men\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Und war der laute Spott der frohen Schaar.", "tokens": ["Und", "war", "der", "lau\u00b7te", "Spott", "der", "fro\u00b7hen", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin durch meine Schuld nicht lahm, dacht er", "tokens": ["Ich", "bin", "durch", "mei\u00b7ne", "Schuld", "nicht", "lahm", ",", "dacht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In sich gekehrt, ich half so viel als ihr,", "tokens": ["In", "sich", "ge\u00b7kehrt", ",", "ich", "half", "so", "viel", "als", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,", "PPER", "VVFIN", "ADV", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Wohl von unserm Staat. Mich trift mit Recht", "tokens": ["Zum", "Wohl", "von", "un\u00b7serm", "Staat", ".", "Mich", "trift", "mit", "Recht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Spott und Verachtung nicht. Nur ach! wie wirds", "tokens": ["Spott", "und", "Ver\u00b7ach\u00b7tung", "nicht", ".", "Nur", "ach", "!", "wie", "wirds"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "PTKNEG", "$.", "ADV", "ADV", "$.", "PWAV", "VAFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Mir auf der Reis ergehn! Mir, dem der Schmertz", "tokens": ["Mir", "auf", "der", "Reis", "er\u00b7gehn", "!", "Mir", ",", "dem", "der", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$.", "NE", "$,", "PRELS", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Muth und Verm\u00f6gen raubt zum weiten Flug!", "tokens": ["Muth", "und", "Ver\u00b7m\u00f6\u00b7gen", "raubt", "zum", "wei\u00b7ten", "Flug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich Ungl\u00fcckseeliger, das Wa\u00dfer wird", "tokens": ["Ich", "Un\u00b7gl\u00fcck\u00b7see\u00b7li\u00b7ger", ",", "das", "Wa\u00b7\u00dfer", "wird"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bald mein gewi\u00dfes Grab! \u2013 Warum erscho\u00df", "tokens": ["Bald", "mein", "ge\u00b7wi\u00b7\u00dfes", "Grab", "!", "\u2013", "Wa\u00b7rum", "er\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "$(", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Grausame mich nicht! \u2013 Inde\u00dfen weht", "tokens": ["Der", "Grau\u00b7sa\u00b7me", "mich", "nicht", "!", "\u2013", "In\u00b7de\u00b7\u00dfen", "weht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKNEG", "$.", "$(", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gewogner Wind vom Land ins Meer. Die Schaar", "tokens": ["Ge\u00b7wog\u00b7ner", "Wind", "vom", "Land", "ins", "Meer", ".", "Die", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "APPRART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Beginnt, geordnet, jezt die Reis, und eilt", "tokens": ["Be\u00b7ginnt", ",", "ge\u00b7ord\u00b7net", ",", "jezt", "die", "Reis", ",", "und", "eilt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVPP", "$,", "ADV", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mit schnellen Fl\u00fcgeln fort und schreyt f\u00fcr Lust.", "tokens": ["Mit", "schnel\u00b7len", "Fl\u00fc\u00b7geln", "fort", "und", "schreyt", "f\u00fcr", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Kranke nur blieb weit zur\u00fcck und ruht", "tokens": ["Der", "Kran\u00b7ke", "nur", "blieb", "weit", "zu\u00b7r\u00fcck", "und", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auf Lotos Bl\u00e4ttern oft, womit die See", "tokens": ["Auf", "Lo\u00b7tos", "Bl\u00e4t\u00b7tern", "oft", ",", "wo\u00b7mit", "die", "See"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Bestreuet war, und seufzt f\u00fcr Gram und Schmerz. \u2013", "tokens": ["Be\u00b7streu\u00b7et", "war", ",", "und", "seufzt", "f\u00fcr", "Gram", "und", "Schmerz", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach vielen Ruhn, sah er das be\u00dfre Land,", "tokens": ["Nach", "vie\u00b7len", "Ruhn", ",", "sah", "er", "das", "be\u00df\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den g\u00fctgern Himmel, der ihn pl\u00f6tzlich heilt.", "tokens": ["Den", "g\u00fct\u00b7gern", "Him\u00b7mel", ",", "der", "ihn", "pl\u00f6tz\u00b7lich", "heilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die F\u00fcrsicht leitet ihn begl\u00fcckt dahin,", "tokens": ["Die", "F\u00fcr\u00b7sicht", "lei\u00b7tet", "ihn", "be\u00b7gl\u00fcckt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und vielen Sp\u00f6ttern ward die Fluth zum Grab.", "tokens": ["Und", "vie\u00b7len", "Sp\u00f6t\u00b7tern", "ward", "die", "Fluth", "zum", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr, die die schwere Hand des Ungl\u00fccks dr\u00fcckt,", "tokens": ["Ihr", ",", "die", "die", "schwe\u00b7re", "Hand", "des", "Un\u00b7gl\u00fccks", "dr\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Redlichen, die ihr mit Harm erf\u00fcllt,", "tokens": ["Ihr", "Red\u00b7li\u00b7chen", ",", "die", "ihr", "mit", "Harm", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben oft verw\u00fcnscht, verzaget nicht,", "tokens": ["Das", "Le\u00b7ben", "oft", "ver\u00b7w\u00fcnscht", ",", "ver\u00b7za\u00b7get", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wagt die Reise durch das Leben nur!", "tokens": ["Und", "wagt", "die", "Rei\u00b7se", "durch", "das", "Le\u00b7ben", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jenseit dem Ufer giebts ein be\u00dfer Land,", "tokens": ["Jen\u00b7seit", "dem", "U\u00b7fer", "giebts", "ein", "be\u00b7\u00dfer", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Gefilde voller Lust erwarten euch!", "tokens": ["Ge\u00b7fil\u00b7de", "vol\u00b7ler", "Lust", "er\u00b7war\u00b7ten", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}