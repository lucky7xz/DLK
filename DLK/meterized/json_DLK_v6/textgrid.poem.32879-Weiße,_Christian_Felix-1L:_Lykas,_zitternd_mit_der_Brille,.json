{"textgrid.poem.32879": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lykas, zitternd mit der Brille,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lykas, zitternd mit der Brille,", "tokens": ["Ly\u00b7kas", ",", "zit\u00b7ternd", "mit", "der", "Bril\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft Belinden, sie h\u00e4lt stille,", "tokens": ["K\u00fc\u00dft", "Be\u00b7lin\u00b7den", ",", "sie", "h\u00e4lt", "stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wird seines Goldes Prei\u00df:", "tokens": ["Und", "wird", "sei\u00b7nes", "Gol\u00b7des", "Prei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Da\u00df er sich durch Liebkosungen", "tokens": ["Da\u00df", "er", "sich", "durch", "Lieb\u00b7ko\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Hand, ihr Herz errungen,", "tokens": ["Ih\u00b7re", "Hand", ",", "ihr", "Herz", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, das macht er mir nicht wei\u00df.", "tokens": ["Nein", ",", "das", "macht", "er", "mir", "nicht", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gestern scherzt ich mit Nerinen,", "tokens": ["Ge\u00b7stern", "scherzt", "ich", "mit", "Ne\u00b7ri\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.2": {"text": "Trotz der alten Modeminen", "tokens": ["Trotz", "der", "al\u00b7ten", "Mo\u00b7de\u00b7mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sie ziemlich roth und wei\u00df:", "tokens": ["Sah", "sie", "ziem\u00b7lich", "roth", "und", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch da\u00df sie vom Schlaf erwachet,", "tokens": ["Doch", "da\u00df", "sie", "vom", "Schlaf", "er\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Gleich so fr\u00fchlingsm\u00e4\u00dfig lachet,", "tokens": ["Gleich", "so", "fr\u00fch\u00b7lings\u00b7m\u00e4\u00b7\u00dfig", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, das macht sie mir nicht wei\u00df.", "tokens": ["Nein", ",", "das", "macht", "sie", "mir", "nicht", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mops will keine Schw\u00e4ger n\u00e4hren,", "tokens": ["Mops", "will", "kei\u00b7ne", "Schw\u00e4\u00b7ger", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und macht seiner Frau, Ne\u00e4ren,", "tokens": ["Und", "macht", "sei\u00b7ner", "Frau", ",", "Ne\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Oft darum die H\u00f6lle hei\u00df:", "tokens": ["Oft", "da\u00b7rum", "die", "H\u00f6l\u00b7le", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er kauft ihr die fr\u00f6mmsten B\u00fccher,", "tokens": ["Er", "kauft", "ihr", "die", "fr\u00f6mms\u00b7ten", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schlie\u00dft sie ein, und glaubt sich sicher:", "tokens": ["Schlie\u00dft", "sie", "ein", ",", "und", "glaubt", "sich", "si\u00b7cher", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch mir macht er es nicht wei\u00df.", "tokens": ["Doch", "mir", "macht", "er", "es", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ueber den Verfall der Tugend", "tokens": ["Ue\u00b7ber", "den", "Ver\u00b7fall", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreyt Beatrix, da die Jugend,", "tokens": ["Schreyt", "Be\u00b7at\u00b7rix", ",", "da", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sonst M\u00fctter lernten, wei\u00df.", "tokens": ["Was", "sonst", "M\u00fct\u00b7ter", "lern\u00b7ten", ",", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df stets Singen, Beten, Lesen", "tokens": ["Da\u00df", "stets", "Sin\u00b7gen", ",", "Be\u00b7ten", ",", "Le\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einst ihr Zeitvertreib gewesen,", "tokens": ["Einst", "ihr", "Zeit\u00b7ver\u00b7treib", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, das macht sie mir nicht wei\u00df.", "tokens": ["Nein", ",", "das", "macht", "sie", "mir", "nicht", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Keinen von den Freyern allen", "tokens": ["Kei\u00b7nen", "von", "den", "Frey\u00b7ern", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "APPR", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft Themire sich gefallen,", "tokens": ["L\u00e4\u00dft", "The\u00b7mi\u00b7re", "sich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie kennt ihrer Sch\u00f6nheit Preis:", "tokens": ["Sie", "kennt", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Preis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eingebildet will sie sterben,", "tokens": ["Ein\u00b7ge\u00b7bil\u00b7det", "will", "sie", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn nicht Grafen um sie werben;", "tokens": ["Wenn", "nicht", "Gra\u00b7fen", "um", "sie", "wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir nur macht sie es nicht wei\u00df.", "tokens": ["Mir", "nur", "macht", "sie", "es", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Meine reitzende Selinde,", "tokens": ["Mei\u00b7ne", "reit\u00b7zen\u00b7de", "Se\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der ich mich ietzt verbinde,", "tokens": ["Mit", "der", "ich", "mich", "ietzt", "ver\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist f\u00fcr andre kalt wie Eis.", "tokens": ["Ist", "f\u00fcr", "and\u00b7re", "kalt", "wie", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie einzig f\u00fcr mich brennet,", "tokens": ["Da\u00df", "sie", "ein\u00b7zig", "f\u00fcr", "mich", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Au\u00dfer mir kein Gl\u00fccke kennet,", "tokens": ["Au\u00b7\u00dfer", "mir", "kein", "Gl\u00fc\u00b7cke", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies macht sie mir doch wohl wei\u00df.", "tokens": ["Dies", "macht", "sie", "mir", "doch", "wohl", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}