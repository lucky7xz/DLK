{"textgrid.poem.25732": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hu hu! wie kommt der Wind so kalt", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hu hu! wie kommt der Wind so kalt", "tokens": ["Hu", "hu", "!", "wie", "kommt", "der", "Wind", "so", "kalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon \u00fcber die Stoppel gelaufen!", "tokens": ["Schon", "\u00fc\u00b7ber", "die", "Stop\u00b7pel", "ge\u00b7lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie f\u00e4rbet sich so gelb der Wald,", "tokens": ["Wie", "f\u00e4r\u00b7bet", "sich", "so", "gelb", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie versammlen sich so bald", "tokens": ["Und", "wie", "ver\u00b7samm\u00b7len", "sich", "so", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schwalben zum Abzug' in Haufen!", "tokens": ["Die", "Schwal\u00b7ben", "zum", "Ab\u00b7zug'", "in", "Hau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Wiese dampft, der Brocken braut,", "tokens": ["Die", "Wie\u00b7se", "dampft", ",", "der", "Bro\u00b7cken", "braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sch\u00fcttelt, Schauer auf Schauer,", "tokens": ["Und", "sch\u00fct\u00b7telt", ",", "Schau\u00b7er", "auf", "Schau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den Regen ab; durch Nebel schaut", "tokens": ["Den", "Re\u00b7gen", "ab", ";", "durch", "Ne\u00b7bel", "schaut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Sonn' herab, wie eine Braut", "tokens": ["Die", "Sonn'", "her\u00b7ab", ",", "wie", "ei\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Geh\u00fcllet in d\u00fcstere Trauer.", "tokens": ["Ge\u00b7h\u00fcl\u00b7let", "in", "d\u00fcs\u00b7te\u00b7re", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Ein Heer von Dro\u00dfeln kommt vom Rhein',", "tokens": ["Ein", "Heer", "von", "Dro\u00b7\u00dfeln", "kommt", "vom", "Rhein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Im Schimmer des Morgens, gezogen;", "tokens": ["Im", "Schim\u00b7mer", "des", "Mor\u00b7gens", ",", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADV", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Doch manche wird bei Hespers Schein", "tokens": ["Doch", "man\u00b7che", "wird", "bei", "Hes\u00b7pers", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bereits des Amtmanns Speise seyn,", "tokens": ["Be\u00b7reits", "des", "Amt\u00b7manns", "Spei\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch r\u00f6thliche Beeren betrogen.", "tokens": ["Durch", "r\u00f6th\u00b7li\u00b7che", "Bee\u00b7ren", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Der Cantor sondert nun das Wachs", "tokens": ["Der", "Can\u00b7tor", "son\u00b7dert", "nun", "das", "Wachs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vom goldgelben Honig der Scheiben;", "tokens": ["Vom", "gold\u00b7gel\u00b7ben", "Ho\u00b7nig", "der", "Schei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Die Dirne sonnt den grauen Flachs,", "tokens": ["Die", "Dir\u00b7ne", "sonnt", "den", "grau\u00b7en", "Flachs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der J\u00e4ger gr\u00e4bt, um Fuchs und Dachs", "tokens": ["Der", "J\u00e4\u00b7ger", "gr\u00e4bt", ",", "um", "Fuchs", "und", "Dachs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Hervor aus dem Baue zu treiben.", "tokens": ["Her\u00b7vor", "aus", "dem", "Bau\u00b7e", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Wir suchen das Kamin nunmehr,", "tokens": ["Wir", "su\u00b7chen", "das", "Ka\u00b7min", "nun\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ohn' ewig aufs Wetter zu schm\u00e4len.", "tokens": ["Ohn'", "e\u00b7wig", "aufs", "Wet\u00b7ter", "zu", "schm\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Ist unser eigner Kopf zu leer,", "tokens": ["Ist", "un\u00b7ser", "eig\u00b7ner", "Kopf", "zu", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So soll Herr Mars von Land und Meer", "tokens": ["So", "soll", "Herr", "Mars", "von", "Land", "und", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Uns etwas neues erz\u00e4hlen.", "tokens": ["Uns", "et\u00b7was", "neu\u00b7es", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "S\u00fc\u00df mag es seyn, f\u00fcrs Vaterland", "tokens": ["S\u00fc\u00df", "mag", "es", "seyn", ",", "f\u00fcrs", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Als Held zu sterben mit Freuden;", "tokens": ["Als", "Held", "zu", "ster\u00b7ben", "mit", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Doch haben wir so viel Verstand,", "tokens": ["Doch", "ha\u00b7ben", "wir", "so", "viel", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Um F\u00fcrstengeitz und Vaterland", "tokens": ["Um", "F\u00fcrs\u00b7ten\u00b7geitz", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ein wenig zu unterscheiden.", "tokens": ["Ein", "we\u00b7nig", "zu", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKZU", "VVINF", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "La\u00dft uns, bei dieser Schaale Punsch,", "tokens": ["La\u00dft", "uns", ",", "bei", "die\u00b7ser", "Schaa\u00b7le", "Pun\u00b7sch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Dem Himmel danken, ihr Lieben!", "tokens": ["Dem", "Him\u00b7mel", "dan\u00b7ken", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Da\u00df wir, nach unsrer Jugend Wunsch,", "tokens": ["Da\u00df", "wir", ",", "nach", "uns\u00b7rer", "Ju\u00b7gend", "Wunsch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Nicht da sind, wo die Schaalen Punsch", "tokens": ["Nicht", "da", "sind", ",", "wo", "die", "Schaa\u00b7len", "Pun\u00b7sch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.35": {"text": "Von Kugeln in Scherben zerstieben.", "tokens": ["Von", "Ku\u00b7geln", "in", "Scher\u00b7ben", "zer\u00b7stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.36": {"text": "Von unsern Thaten am Kamin',", "tokens": ["Von", "un\u00b7sern", "Tha\u00b7ten", "am", "Ka\u00b7min'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Schweigt zwar der Fama Posaune,", "tokens": ["Schweigt", "zwar", "der", "Fa\u00b7ma", "Po\u00b7sau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.38": {"text": "Doch unser Schlaf wird nicht entfliehn", "tokens": ["Doch", "un\u00b7ser", "Schlaf", "wird", "nicht", "ent\u00b7fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Vor einem ", "tokens": ["Vor", "ei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Und Boreas t\u00fcckischer Laune.", "tokens": ["Und", "Bo\u00b7re\u00b7as", "t\u00fc\u00b7cki\u00b7scher", "Lau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Herr Boreas st\u00f6rt nicht den Klang", "tokens": ["Herr", "Bo\u00b7re\u00b7as", "st\u00f6rt", "nicht", "den", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Von unsern warmen Pokalen,", "tokens": ["Von", "un\u00b7sern", "war\u00b7men", "Po\u00b7ka\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Und nicht den lautern Lobgesang,", "tokens": ["Und", "nicht", "den", "lau\u00b7tern", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Den wir dem Herbst', aus altem Hang'", "tokens": ["Den", "wir", "dem", "Herbst'", ",", "aus", "al\u00b7tem", "Hang'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Zu reichen Weinlesen, bezahlen.", "tokens": ["Zu", "rei\u00b7chen", "Wein\u00b7le\u00b7sen", ",", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}