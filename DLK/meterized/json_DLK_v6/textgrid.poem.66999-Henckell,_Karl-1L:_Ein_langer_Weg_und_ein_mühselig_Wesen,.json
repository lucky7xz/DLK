{"textgrid.poem.66999": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein langer Weg und ein m\u00fchselig Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein langer Weg und ein m\u00fchselig Wesen,", "tokens": ["Ein", "lan\u00b7ger", "Weg", "und", "ein", "m\u00fch\u00b7se\u00b7lig", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis du die Kunst erwirbst, die dich erlesen,", "tokens": ["Bis", "du", "die", "Kunst", "er\u00b7wirbst", ",", "die", "dich", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Menge scheint sie leicht wie Kinderspiel.", "tokens": ["Der", "Men\u00b7ge", "scheint", "sie", "leicht", "wie", "Kin\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn du fragst: womit wird sie gewonnen?", "tokens": ["Und", "wenn", "du", "fragst", ":", "wo\u00b7mit", "wird", "sie", "ge\u00b7won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geduld und Liebe winken aus dem Bronnen;", "tokens": ["Ge\u00b7duld", "und", "Lie\u00b7be", "win\u00b7ken", "aus", "dem", "Bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn die zwei fehlen, n\u00fctzt Talent nicht viel.", "tokens": ["Wenn", "die", "zwei", "feh\u00b7len", ",", "n\u00fctzt", "Ta\u00b7lent", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "CARD", "VVFIN", "$,", "VVFIN", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}