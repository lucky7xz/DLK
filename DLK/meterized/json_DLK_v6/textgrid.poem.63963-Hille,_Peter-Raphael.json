{"textgrid.poem.63963": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Raphael", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O komm leise, leise komm,", "tokens": ["O", "komm", "lei\u00b7se", ",", "lei\u00b7se", "komm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df das Licht und sieh, wie fromm", "tokens": ["La\u00df", "das", "Licht", "und", "sieh", ",", "wie", "fromm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KON", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da liegt es, Atem steigt", "tokens": ["Da", "liegt", "es", ",", "A\u00b7tem", "steigt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als Gebet noch, Lippe schweigt", "tokens": ["Als", "Ge\u00b7bet", "noch", ",", "Lip\u00b7pe", "schweigt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "ADV", "$,", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schl\u00e4ft in lieber Heimlichkeit.", "tokens": ["Schl\u00e4ft", "in", "lie\u00b7ber", "Heim\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin nun weiht", "tokens": ["Hin", "nun", "weiht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dir sich hin sein ganzes Leben.", "tokens": ["Dir", "sich", "hin", "sein", "gan\u00b7zes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du h\u00f6rst die fromme Seele beben.", "tokens": ["Du", "h\u00f6rst", "die", "from\u00b7me", "See\u00b7le", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nun kannst du ihr den Segen geben", "tokens": ["Nun", "kannst", "du", "ihr", "den", "Se\u00b7gen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr die Nacht.", "tokens": ["F\u00fcr", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}