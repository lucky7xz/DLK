{"textgrid.poem.66484": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel Ungeheures wagt der Mensch. Um Eisen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Ungeheures wagt der Mensch. Um Eisen", "tokens": ["Viel", "Un\u00b7ge\u00b7heu\u00b7res", "wagt", "der", "Mensch", ".", "Um", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "VVFIN", "ART", "NN", "$.", "KOUI", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Gold im Erdenschoo\u00df w\u00fchlt er sich Bahn,", "tokens": ["Und", "Gold", "im", "Er\u00b7den\u00b7schoo\u00df", "w\u00fchlt", "er", "sich", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und droben, wo im Blau die Adler kreisen,", "tokens": ["Und", "dro\u00b7ben", ",", "wo", "im", "Blau", "die", "Ad\u00b7ler", "krei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sehn Eisesfirnen ihrem Thron ihn nahn.", "tokens": ["Sehn", "Ei\u00b7ses\u00b7fir\u00b7nen", "ih\u00b7rem", "Thron", "ihn", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das fernste Meer mit ersten Wellengleisen", "tokens": ["Das", "ferns\u00b7te", "Meer", "mit", "ers\u00b7ten", "Wel\u00b7len\u00b7glei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Macht er sich dienstbar, trotzend dem Orkan,", "tokens": ["Macht", "er", "sich", "dienst\u00b7bar", ",", "trot\u00b7zend", "dem", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+--++", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und was Jahrtausend' und Natur geschieden", "tokens": ["Und", "was", "Jahr\u00b7tau\u00b7send'", "und", "Na\u00b7tur", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "L\u00e4\u00dft f\u00fcr das Dampfro\u00df er zusammenschmieden.", "tokens": ["L\u00e4\u00dft", "f\u00fcr", "das", "Dam\u00b7pfro\u00df", "er", "zu\u00b7sam\u00b7men\u00b7schmie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was ist Gefahr ihm? Bis zum Meeresgrunde,", "tokens": ["Was", "ist", "Ge\u00b7fahr", "ihm", "?", "Bis", "zum", "Mee\u00b7res\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "PPER", "$.", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Perlenmuschel nach, wagt er sein Heil,", "tokens": ["Der", "Per\u00b7len\u00b7mu\u00b7schel", "nach", ",", "wagt", "er", "sein", "Heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und taucht zum Licht empor mit seinem Funde.", "tokens": ["Und", "taucht", "zum", "Licht", "em\u00b7por", "mit", "sei\u00b7nem", "Fun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wo des Nordens Felsenw\u00e4nde steil", "tokens": ["Und", "wo", "des", "Nor\u00b7dens", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", "steil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und pfadlos ragen an der Brandung Schlunde", "tokens": ["Und", "pfad\u00b7los", "ra\u00b7gen", "an", "der", "Bran\u00b7dung", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4\u00dft er hinab sich an dem schwanken Seil", "tokens": ["L\u00e4\u00dft", "er", "hin\u00b7ab", "sich", "an", "dem", "schwan\u00b7ken", "Seil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum Dunenneste wilder V\u00f6gelbruten,", "tokens": ["Zum", "Du\u00b7nen\u00b7nes\u00b7te", "wil\u00b7der", "V\u00f6\u00b7gel\u00b7bru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Umg\u00e4hnt, umdonnert von des Abgrunds Fluthen.", "tokens": ["Um\u00b7g\u00e4hnt", ",", "um\u00b7don\u00b7nert", "von", "des", "Ab\u00b7grunds", "Flut\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch das verwegenste von allem Wagen", "tokens": ["Doch", "das", "ver\u00b7we\u00b7gens\u00b7te", "von", "al\u00b7lem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist, da\u00df der Mensch sein Innerstes nicht scheut", "tokens": ["Ist", ",", "da\u00df", "der", "Mensch", "sein", "In\u00b7ners\u00b7tes", "nicht", "scheut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der tausendk\u00f6pfigen Menge vorzutragen,", "tokens": ["Der", "tau\u00b7send\u00b7k\u00f6p\u00b7fi\u00b7gen", "Men\u00b7ge", "vor\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df er sich selbst in seinem Werk ihr beut.", "tokens": ["Da\u00df", "er", "sich", "selbst", "in", "sei\u00b7nem", "Werk", "ihr", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn Wasser \u00fcber ihm zusammenschlagen,", "tokens": ["Wenn", "Was\u00b7ser", "\u00fc\u00b7ber", "ihm", "zu\u00b7sam\u00b7men\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Fels ihn hinrafft, wie der Sturz ihm dr\u00e4ut,", "tokens": ["Der", "Fels", "ihn", "hin\u00b7rafft", ",", "wie", "der", "Sturz", "ihm", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.7": {"text": "Kein Vorwurf wird des Muth'gen Tod erreichen,", "tokens": ["Kein", "Vor\u00b7wurf", "wird", "des", "Muth'\u00b7gen", "Tod", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch der Lebend'ge f\u00fcrchte Seinesgleichen!", "tokens": ["Doch", "der", "Le\u00b7ben\u00b7d'\u00b7ge", "f\u00fcrch\u00b7te", "Sei\u00b7nes\u00b7glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Vom Hohn gezeichnet ist der Ueberk\u00fchne,", "tokens": ["Vom", "Hohn", "ge\u00b7zeich\u00b7net", "ist", "der", "Ue\u00b7ber\u00b7k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der seiner Phantasie zu viel vertraut;", "tokens": ["Der", "sei\u00b7ner", "Phan\u00b7ta\u00b7sie", "zu", "viel", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verspottet, wenn sein Werk er auf der B\u00fchne", "tokens": ["Ver\u00b7spot\u00b7tet", ",", "wenn", "sein", "Werk", "er", "auf", "der", "B\u00fch\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu leicht f\u00fcr die entt\u00e4uschte Schaar gebaut;", "tokens": ["Zu", "leicht", "f\u00fcr", "die", "ent\u00b7t\u00e4uschte", "Schaar", "ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Grausam und unerbittlich wird die S\u00fchne", "tokens": ["Grau\u00b7sam", "und", "un\u00b7er\u00b7bitt\u00b7lich", "wird", "die", "S\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "F\u00fcr seines Sanges mi\u00dfgestimmten Laut.", "tokens": ["F\u00fcr", "sei\u00b7nes", "San\u00b7ges", "mi\u00df\u00b7ge\u00b7stimm\u00b7ten", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn viel des Guten schwindet im Ged\u00e4chtni\u00df,", "tokens": ["Wenn", "viel", "des", "Gu\u00b7ten", "schwin\u00b7det", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Fehltritt bleibt ein dauerndes Verm\u00e4chtni\u00df.", "tokens": ["Ein", "Fehl\u00b7tritt", "bleibt", "ein", "dau\u00b7ern\u00b7des", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was treibt den Schaffenden zu h\u00f6chstem Wagen?", "tokens": ["Was", "treibt", "den", "Schaf\u00b7fen\u00b7den", "zu", "h\u00f6chs\u00b7tem", "Wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er glaubt an der Nothwendigkeit Gebot.", "tokens": ["Er", "glaubt", "an", "der", "Noth\u00b7wen\u00b7dig\u00b7keit", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sein Recht ist Recht, und wei\u00df sein Werk zu sagen", "tokens": ["Sein", "Recht", "ist", "Recht", ",", "und", "wei\u00df", "sein", "Werk", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von h\u00f6chstem K\u00f6nnen, bleibt es unbedroht.", "tokens": ["Von", "h\u00f6chs\u00b7tem", "K\u00f6n\u00b7nen", ",", "bleibt", "es", "un\u00b7be\u00b7droht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vergessen will das Denken und Behagen", "tokens": ["Ver\u00b7ges\u00b7sen", "will", "das", "Den\u00b7ken", "und", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Werdens Ringen, des Gestaltens Noth.", "tokens": ["Des", "Wer\u00b7dens", "Rin\u00b7gen", ",", "des", "Ge\u00b7stal\u00b7tens", "Noth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Recht steht um Recht. F\u00fchlt sie sich \u00fcberlegen,", "tokens": ["Recht", "steht", "um", "Recht", ".", "F\u00fchlt", "sie", "sich", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$.", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Giebt dir die Menge f\u00fcrchterlichen Segen!", "tokens": ["Giebt", "dir", "die", "Men\u00b7ge", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Doch sei sie grimm und noch so scharf im H\u00f6hnen,", "tokens": ["Doch", "sei", "sie", "grimm", "und", "noch", "so", "scharf", "im", "H\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu fesseln leicht ist selbst ihr Widerstand.", "tokens": ["Zu", "fes\u00b7seln", "leicht", "ist", "selbst", "ihr", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es lebt ein Zug zum Edlen und zum Sch\u00f6nen,", "tokens": ["Es", "lebt", "ein", "Zug", "zum", "Ed\u00b7len", "und", "zum", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch da, wo Gro\u00dfes fremd und unbekannt.", "tokens": ["Auch", "da", ",", "wo", "Gro\u00b7\u00dfes", "fremd", "und", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ber\u00fchrst du den auch nur mit leisen T\u00f6nen,", "tokens": ["Be\u00b7r\u00fchrst", "du", "den", "auch", "nur", "mit", "lei\u00b7sen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist das Erwachen ganz dir zugewandt.", "tokens": ["Ist", "das", "Er\u00b7wa\u00b7chen", "ganz", "dir", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie wird dich deines Rechtes nicht berauben,", "tokens": ["Sie", "wird", "dich", "dei\u00b7nes", "Rech\u00b7tes", "nicht", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Giebst du ihr erst das Recht, an dich zu glauben.", "tokens": ["Giebst", "du", "ihr", "erst", "das", "Recht", ",", "an", "dich", "zu", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}