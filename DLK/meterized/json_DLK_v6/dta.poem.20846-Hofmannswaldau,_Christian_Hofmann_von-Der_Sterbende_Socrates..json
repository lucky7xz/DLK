{"dta.poem.20846": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Man glaubt vor langer Zeit/ da\u00df der entleibte Geist/", "tokens": ["Man", "glaubt", "vor", "lan\u00b7ger", "Zeit", "/", "da\u00df", "der", "ent\u00b7leib\u00b7te", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sonst die alte Welt nur ein Gespenste heist/", "tokens": ["Den", "sonst", "die", "al\u00b7te", "Welt", "nur", "ein", "Ge\u00b7spens\u00b7te", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann er die faule Haut des Leibes abgezogen/", "tokens": ["Wann", "er", "die", "fau\u00b7le", "Haut", "des", "Lei\u00b7bes", "ab\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in das schw\u00e4rtze Hau\u00df der H\u00f6llen ist geflogen/", "tokens": ["Und", "in", "das", "schw\u00e4rt\u00b7ze", "Hau\u00df", "der", "H\u00f6l\u00b7len", "ist", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch endlich/ weil die Todes-Bande", "tokens": ["Noch", "end\u00b7lich", "/", "weil", "die", "To\u00b7des\u00b7Ban\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhn nicht auf ewig hier bestrickt/", "tokens": ["Jhn", "nicht", "auf", "e\u00b7wig", "hier", "be\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch einen Wechsel wird erquickt/", "tokens": ["Durch", "ei\u00b7nen", "Wech\u00b7sel", "wird", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In dem er aus dem d\u00fcrren Lande", "tokens": ["In", "dem", "er", "aus", "dem", "d\u00fcr\u00b7ren", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit freien Fl\u00fcgeln wieder fleucht/", "tokens": ["Mit", "frei\u00b7en", "Fl\u00fc\u00b7geln", "wie\u00b7der", "fleucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und in den Mensch zur\u00fccke zeucht.", "tokens": ["Und", "in", "den", "Mensch", "zu\u00b7r\u00fc\u00b7cke", "zeucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}