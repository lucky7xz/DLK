{"textgrid.poem.33556": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sank ein Tag zur Ruhe nieder,", "tokens": ["Es", "sank", "ein", "Tag", "zur", "Ru\u00b7he", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tag, der uns gar hold gelacht,", "tokens": ["Ein", "Tag", ",", "der", "uns", "gar", "hold", "ge\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir fanden uns so innig wieder,", "tokens": ["Wir", "fan\u00b7den", "uns", "so", "in\u00b7nig", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie stets ich mir das h\u00f6chste Gl\u00fcck gedacht;", "tokens": ["Wie", "stets", "ich", "mir", "das", "h\u00f6chs\u00b7te", "Gl\u00fcck", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun schlie\u00dfe dir die Augenlider", "tokens": ["Nun", "schlie\u00b7\u00dfe", "dir", "die", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein s\u00fc\u00dfer Schlaf \u2013 mein Engelskind, gut' Nacht!", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Schlaf", "\u2013", "mein", "En\u00b7gels\u00b7kind", ",", "gut'", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sanft m\u00f6gen dich hin\u00fcberziehen", "tokens": ["Sanft", "m\u00f6\u00b7gen", "dich", "hin\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Reich des Traums mit ihrer Macht", "tokens": ["Ins", "Reich", "des", "Traums", "mit", "ih\u00b7rer", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beethoven's gro\u00dfe Melodieen,", "tokens": ["Beet\u00b7ho\u00b7ven's", "gro\u00b7\u00dfe", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und alles, was uns Leid und Schmerz gebracht,", "tokens": ["Und", "al\u00b7les", ",", "was", "uns", "Leid", "und", "Schmerz", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vergessen ist's, vers\u00f6hnt, verziehen \u2013", "tokens": ["Ver\u00b7ges\u00b7sen", "ist's", ",", "ver\u00b7s\u00f6hnt", ",", "ver\u00b7zie\u00b7hen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVPP", "$,", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schlaf' wohl! schlaf' wohl! Mein Engelskind, gut' Nacht!", "tokens": ["Schlaf'", "wohl", "!", "schlaf'", "wohl", "!", "Mein", "En\u00b7gels\u00b7kind", ",", "gut'", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VVFIN", "ADV", "$.", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}