{"textgrid.poem.56310": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Des Gesellen Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer klopft so stark? wer begehrt ins Haus?", "tokens": ["Wer", "klopft", "so", "stark", "?", "wer", "be\u00b7gehrt", "ins", "Haus", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich schlie\u00dfe nicht auf, mein Ehherr ist aus.", "tokens": ["Ich", "schlie\u00b7\u00dfe", "nicht", "auf", ",", "mein", "Eh\u00b7herr", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbund sag ich dir an, der klopft, ist dein Sohn,", "tokens": ["\u00bb", "und", "sag", "ich", "dir", "an", ",", "der", "klopft", ",", "ist", "dein", "Sohn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Mutter, o Mutter! so \u00f6ffnest du schon.\u00ab", "tokens": ["O", "Mut\u00b7ter", ",", "o", "Mut\u00b7ter", "!", "so", "\u00f6ff\u00b7nest", "du", "schon", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Was kehrtest du heim, mein Sohn, so geschwind,", "tokens": ["Was", "kehr\u00b7test", "du", "heim", ",", "mein", "Sohn", ",", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bevor noch die Jahre verstrichen sind?", "tokens": ["Be\u00b7vor", "noch", "die", "Jah\u00b7re", "ver\u00b7stri\u00b7chen", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbich kehrte heim \u2013 ich war wohl bet\u00f6rt \u2013", "tokens": ["\u00bb", "ich", "kehr\u00b7te", "heim", "\u2013", "ich", "war", "wohl", "be\u00b7t\u00f6rt", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hast, Mutter, du nie von Heimweh geh\u00f6rt?\u00ab", "tokens": ["Hast", ",", "Mut\u00b7ter", ",", "du", "nie", "von", "Heim\u00b7weh", "ge\u00b7h\u00f6rt", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "ADV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Mein Mann, bef\u00fcrcht ich, vernimmt's nicht gern; \u2013", "tokens": ["Mein", "Mann", ",", "be\u00b7f\u00fcrcht", "ich", ",", "ver\u00b7nimmt's", "nicht", "gern", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O weh, da\u00df ich freite den anderen Herrn!", "tokens": ["O", "weh", ",", "da\u00df", "ich", "frei\u00b7te", "den", "an\u00b7de\u00b7ren", "Herrn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbo weh, da\u00df dem zweiten du hin dich warfst,", "tokens": ["\u00bb", "o", "weh", ",", "da\u00df", "dem", "zwei\u00b7ten", "du", "hin", "dich", "warfst", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und nicht mit dem Sohne dich freuen mehr darfst!\u00ab", "tokens": ["Und", "nicht", "mit", "dem", "Soh\u00b7ne", "dich", "freu\u00b7en", "mehr", "darfst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "PPER", "VVFIN", "ADV", "VVFIN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Mein Sohn, o schone der Mutter dein,", "tokens": ["Mein", "Sohn", ",", "o", "scho\u00b7ne", "der", "Mut\u00b7ter", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "VVFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und la\u00df das Gericht nur Gottes sein!", "tokens": ["Und", "la\u00df", "das", "Ge\u00b7richt", "nur", "Got\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "NN", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbo meine Mutter! \u2013 doch, mache mir kund,", "tokens": ["\u00bb", "o", "mei\u00b7ne", "Mut\u00b7ter", "!", "\u2013", "doch", ",", "ma\u00b7che", "mir", "kund", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPOSAT", "NN", "$.", "$(", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo weilt die Christel zu dieser Stund?\u00ab", "tokens": ["Wo", "weilt", "die", "Chris\u00b7tel", "zu", "die\u00b7ser", "Stund", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Mein Mann ist streng, unfreundlich fast,", "tokens": ["Mein", "Mann", "ist", "streng", ",", "un\u00b7freund\u00b7lich", "fast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er trieb aus dem Haus den ihm l\u00e4stigen Gast.", "tokens": ["Er", "trieb", "aus", "dem", "Haus", "den", "ihm", "l\u00e4s\u00b7ti\u00b7gen", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbdes Sohnes Braut aus dem Hause gejagt! \u2013", "tokens": ["\u00bb", "des", "Soh\u00b7nes", "Braut", "aus", "dem", "Hau\u00b7se", "ge\u00b7jagt", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So auch den Sohn, sei Gott es geklagt!", "tokens": ["So", "auch", "den", "Sohn", ",", "sei", "Gott", "es", "ge\u00b7klagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Das Heimweh trieb, ich kam geeilt,", "tokens": ["Das", "Heim\u00b7weh", "trieb", ",", "ich", "kam", "ge\u00b7eilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heimat hat gar bald mich geheilt.", "tokens": ["Die", "Hei\u00b7mat", "hat", "gar", "bald", "mich", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Und falls Frau Mutter mich l\u00e4nger nicht h\u00e4lt,", "tokens": ["Und", "falls", "Frau", "Mut\u00b7ter", "mich", "l\u00e4n\u00b7ger", "nicht", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "NN", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00f6cht weiter ich ziehn in die weite Welt.", "tokens": ["M\u00f6cht", "wei\u00b7ter", "ich", "ziehn", "in", "die", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.13": {"line.1": {"text": "Wohin \u2013 wen k\u00fcmmert's? \u2013 auf gutes Gl\u00fcck,", "tokens": ["Wo\u00b7hin", "\u2013", "wen", "k\u00fcm\u00b7mert's", "?", "\u2013", "auf", "gu\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "PWS", "VVFIN", "$.", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und k\u00e4me vielleicht so bald nicht zur\u00fcck.", "tokens": ["Und", "k\u00e4\u00b7me", "viel\u00b7leicht", "so", "bald", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Ade! du gibst deinen Segen mir doch, \u2013", "tokens": ["A\u00b7de", "!", "du", "gibst", "dei\u00b7nen", "Se\u00b7gen", "mir", "doch", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$,", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und Gott, vielleicht, erbarmet sich noch!\u00ab", "tokens": ["Und", "Gott", ",", "viel\u00b7leicht", ",", "er\u00b7bar\u00b7met", "sich", "noch", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "$,", "ADV", "$,", "VVFIN", "PRF", "ADV", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.15": {"line.1": {"text": "So schied er, und wandte zu gehen sich um;", "tokens": ["So", "schied", "er", ",", "und", "wand\u00b7te", "zu", "ge\u00b7hen", "sich", "um", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKZU", "VVINF", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Mutter verharrte zitternd und stumm.", "tokens": ["Die", "Mut\u00b7ter", "ver\u00b7harr\u00b7te", "zit\u00b7ternd", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Und wie hinab er die Stra\u00dfe gewallt,", "tokens": ["Und", "wie", "hin\u00b7ab", "er", "die", "Stra\u00b7\u00dfe", "ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Tor, vor der Wache, da macht er Halt.", "tokens": ["Am", "Tor", ",", "vor", "der", "Wa\u00b7che", ",", "da", "macht", "er", "Halt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.17": {"line.1": {"text": "Stand Christel dort im Soldatenschwarm,", "tokens": ["Stand", "Chris\u00b7tel", "dort", "im", "Sol\u00b7da\u00b7ten\u00b7schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hing verbuhlt dem einen im Arm.", "tokens": ["Und", "hing", "ver\u00b7buhlt", "dem", "ei\u00b7nen", "im", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.18": {"line.1": {"text": "Wie aber sie erst den Gesellen erschaut,", "tokens": ["Wie", "a\u00b7ber", "sie", "erst", "den", "Ge\u00b7sel\u00b7len", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verh\u00fcllt' sie ihr Antlitz und weinte laut.", "tokens": ["Ver\u00b7h\u00fcllt'", "sie", "ihr", "Ant\u00b7litz", "und", "wein\u00b7te", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.19": {"line.1": {"text": "Da haben umher die Soldaten der Wacht", "tokens": ["Da", "ha\u00b7ben", "um\u00b7her", "die", "Sol\u00b7da\u00b7ten", "der", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit l\u00e4rmendem Jubel sie ausgelacht.", "tokens": ["Mit", "l\u00e4r\u00b7men\u00b7dem", "Ju\u00b7bel", "sie", "aus\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.20": {"line.1": {"text": "Er hat nicht gelacht, er hat nicht geweint,", "tokens": ["Er", "hat", "nicht", "ge\u00b7lacht", ",", "er", "hat", "nicht", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er starrte sie an und war wie versteint.", "tokens": ["Er", "starr\u00b7te", "sie", "an", "und", "war", "wie", "ver\u00b7steint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "KOKOM", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "Er raffte sich endlich, endlich auf,", "tokens": ["Er", "raff\u00b7te", "sich", "end\u00b7lich", ",", "end\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und st\u00fcrzte hinaus mit schnellerem Lauf.", "tokens": ["Und", "st\u00fcrz\u00b7te", "hin\u00b7aus", "mit", "schnel\u00b7le\u00b7rem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+----+--+", "measure": "iambic.tri.chol"}}, "stanza.22": {"line.1": {"text": "Wohin? wen k\u00fcmmert's? man wei\u00df es nicht,", "tokens": ["Wo\u00b7hin", "?", "wen", "k\u00fcm\u00b7mert's", "?", "man", "wei\u00df", "es", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWS", "VVFIN", "$.", "PIS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erz\u00e4hlt sich zur Kurzweil nur manche Geschicht.", "tokens": ["Er\u00b7z\u00e4hlt", "sich", "zur", "Kurz\u00b7weil", "nur", "man\u00b7che", "Ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word",