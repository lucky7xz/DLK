{"textgrid.poem.31080": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "194.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mancher, der nichts wei\u00df zu rathen, wei\u00df doch viel Verdacht zu machen;", "tokens": ["Man\u00b7cher", ",", "der", "nichts", "wei\u00df", "zu", "ra\u00b7then", ",", "wei\u00df", "doch", "viel", "Ver\u00b7dacht", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PIS", "VVFIN", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wei\u00df sonst, da\u00df er klug mag scheinen, nicht zu helffen seinen Sachen.", "tokens": ["Wei\u00df", "sonst", ",", "da\u00df", "er", "klug", "mag", "schei\u00b7nen", ",", "nicht", "zu", "helf\u00b7fen", "sei\u00b7nen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}