{"textgrid.poem.42568": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Versp\u00e4tetes Hochzeitlied", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Muse fehlt nicht selten,", "tokens": ["Die", "Mu\u00b7se", "fehlt", "nicht", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn man sie eben will;", "tokens": ["Wenn", "man", "sie", "e\u00b7ben", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schweift in fernen Welten,", "tokens": ["Sie", "schweift", "in", "fer\u00b7nen", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nirgends h\u00e4lt sie still.", "tokens": ["Und", "nir\u00b7gends", "h\u00e4lt", "sie", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Schw\u00e4rmerin vertr\u00e4umet", "tokens": ["Die", "Schw\u00e4r\u00b7me\u00b7rin", "ver\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gar oft den Glockenschlag,", "tokens": ["Gar", "oft", "den", "Glo\u00b7cken\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was sag ich? Sie vers\u00e4umet", "tokens": ["Was", "sag", "ich", "?", "Sie", "ver\u00b7s\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selbst einen Hochzeittag.", "tokens": ["Selbst", "ei\u00b7nen", "Hoch\u00b7zeit\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So auch zu eurem Feste", "tokens": ["So", "auch", "zu", "eu\u00b7rem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erscheinet sie zu sp\u00e4t", "tokens": ["Er\u00b7schei\u00b7net", "sie", "zu", "sp\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bittet nun aufs Beste", "tokens": ["Und", "bit\u00b7tet", "nun", "aufs", "Bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihr sie nicht verschm\u00e4ht.", "tokens": ["Da\u00df", "ihr", "sie", "nicht", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des sch\u00f6nsten Gl\u00fcckes Schimmer", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Gl\u00fc\u00b7ckes", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ergl\u00e4nzt euch eben dann,", "tokens": ["Er\u00b7gl\u00e4nzt", "euch", "e\u00b7ben", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn man euch jetzt und immer", "tokens": ["Wenn", "man", "euch", "jetzt", "und", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Brautlied singen kann.", "tokens": ["Ein", "Braut\u00b7lied", "sin\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}