{"textgrid.poem.61268": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die junge Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl schwillt mir hoch die Brust mit raschem Klopfen,", "tokens": ["Wohl", "schwillt", "mir", "hoch", "die", "Brust", "mit", "ra\u00b7schem", "Klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich, im Angesicht des Schwei\u00dfes Tropfen,", "tokens": ["Seh'", "ich", ",", "im", "An\u00b7ge\u00b7sicht", "des", "Schwei\u00b7\u00dfes", "Trop\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die junge Zeit, wie sie gewaltsam ringt,", "tokens": ["Die", "jun\u00b7ge", "Zeit", ",", "wie", "sie", "ge\u00b7walt\u00b7sam", "ringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie, zu stetem Werk gesch\u00fcrzt die Lenden,", "tokens": ["Wie", "sie", ",", "zu", "ste\u00b7tem", "Werk", "ge\u00b7sch\u00fcrzt", "die", "Len\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein neuer Herkules, mit Kinderh\u00e4nden", "tokens": ["Ein", "neu\u00b7er", "Her\u00b7ku\u00b7les", ",", "mit", "Kin\u00b7der\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das Ungeheure schon vollbringt.", "tokens": ["Das", "Un\u00b7ge\u00b7heu\u00b7re", "schon", "voll\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In tausend Schmieden bei der Essen Brande", "tokens": ["In", "tau\u00b7send", "Schmie\u00b7den", "bei", "der", "Es\u00b7sen", "Bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gie\u00dft sie das Erz und schwei\u00dft in Eisenbande", "tokens": ["Gie\u00dft", "sie", "das", "Erz", "und", "schwei\u00dft", "in", "Ei\u00b7sen\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die weiten L\u00e4nder, die ihr untertan;", "tokens": ["Die", "wei\u00b7ten", "L\u00e4n\u00b7der", ",", "die", "ihr", "un\u00b7ter\u00b7tan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom m\u00fcden Saumro\u00df, das sich wund getragen,", "tokens": ["Vom", "m\u00fc\u00b7den", "Saum\u00b7ro\u00df", ",", "das", "sich", "wund", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nimmt sie das Joch und schirrt vor ihrem Wagen", "tokens": ["Nimmt", "sie", "das", "Joch", "und", "schirrt", "vor", "ih\u00b7rem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Dampf, den wilden Riesen, an.", "tokens": ["Den", "Dampf", ",", "den", "wil\u00b7den", "Rie\u00b7sen", ",", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch Felsenschachte w\u00fchlt sie ihm die G\u00e4nge", "tokens": ["Durch", "Fel\u00b7sen\u00b7schach\u00b7te", "w\u00fchlt", "sie", "ihm", "die", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gew\u00f6lbt und fest, da\u00df in der d\u00fcstern Enge", "tokens": ["Ge\u00b7w\u00f6lbt", "und", "fest", ",", "da\u00df", "in", "der", "d\u00fcs\u00b7tern", "En\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "PTKVZ", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Schlotes Feuer rot wie Fackeln spr\u00fchn;", "tokens": ["Des", "Schlo\u00b7tes", "Feu\u00b7er", "rot", "wie", "Fa\u00b7ckeln", "spr\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie schl\u00e4gt ihm \u00fcbers Tal mit Strom und Weilern", "tokens": ["Sie", "schl\u00e4gt", "ihm", "\u00fc\u00b7bers", "Tal", "mit", "Strom", "und", "Wei\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie einen Aqu\u00e4dukt auf hundert Pfeilern", "tokens": ["Wie", "ei\u00b7nen", "A\u00b7qu\u00e4d\u00b7ukt", "auf", "hun\u00b7dert", "Pfei\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Berg zu Berg die Br\u00fccke k\u00fchn.", "tokens": ["Von", "Berg", "zu", "Berg", "die", "Br\u00fc\u00b7cke", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Schiff, das keck entgegen jedem Winde", "tokens": ["Im", "Schiff", ",", "das", "keck", "ent\u00b7ge\u00b7gen", "je\u00b7dem", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr D\u00e4mon treibt, durchfliegt sie pfeilgeschwinde", "tokens": ["Ihr", "D\u00e4\u00b7mon", "treibt", ",", "durch\u00b7fliegt", "sie", "pfeil\u00b7ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum fremden K\u00fcstenland die salz'ge Bahn;", "tokens": ["Zum", "frem\u00b7den", "K\u00fcs\u00b7ten\u00b7land", "die", "sa\u00b7lz'\u00b7ge", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Stolz flattert wie ein Busch von schwarzen Federn", "tokens": ["Stolz", "flat\u00b7tert", "wie", "ein", "Busch", "von", "schwar\u00b7zen", "Fe\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Rauch am Mast, und grollend in den R\u00e4dern", "tokens": ["Der", "Rauch", "am", "Mast", ",", "und", "grol\u00b7lend", "in", "den", "R\u00e4\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Knirscht der bezwungne Ozean.", "tokens": ["Knirscht", "der", "be\u00b7zwung\u00b7ne", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Des frost'gen Nords, des hei\u00dfen S\u00fcdens Sterne", "tokens": ["Des", "frost'\u00b7gen", "Nords", ",", "des", "hei\u00b7\u00dfen", "S\u00fc\u00b7dens", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlingt sie zum Kranz, schon gibt es keine Ferne;", "tokens": ["Schlingt", "sie", "zum", "Kranz", ",", "schon", "gibt", "es", "kei\u00b7ne", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vorm Hammerschlage ihrer m\u00e4cht'gen Hand,", "tokens": ["Vorm", "Ham\u00b7mer\u00b7schla\u00b7ge", "ih\u00b7rer", "m\u00e4cht'\u00b7gen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie einst vor Israels Posaunenschalle", "tokens": ["Wie", "einst", "vor", "Is\u00b7raels", "Po\u00b7sau\u00b7nen\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NE", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Mauern Jerichos, zerbarst im Falle", "tokens": ["Die", "Mau\u00b7ern", "Je\u00b7ri\u00b7chos", ",", "zer\u00b7barst", "im", "Fal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Raumes eh'rne Scheidewand.", "tokens": ["Des", "Rau\u00b7mes", "eh'r\u00b7ne", "Schei\u00b7de\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sieh, nun braust es her auf tausend Wegen,", "tokens": ["Und", "sieh", ",", "nun", "braust", "es", "her", "auf", "tau\u00b7send", "We\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was nie sich schaute, tritt sich keck entgegen,", "tokens": ["Was", "nie", "sich", "schau\u00b7te", ",", "tritt", "sich", "keck", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VVFIN", "$,", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bunt sind die Trachten, das Gedr\u00e4ng' ist dicht -", "tokens": ["Bunt", "sind", "die", "Trach\u00b7ten", ",", "das", "Ge\u00b7dr\u00e4ng'", "ist", "dicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der Bergsch\u00fctz gr\u00fc\u00dft den Reitersmann im Panzer,", "tokens": ["Der", "Berg\u00b7sch\u00fctz", "gr\u00fc\u00dft", "den", "Rei\u00b7ters\u00b7mann", "im", "Pan\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der deutsche Bauer schaut dem Steppenpflanzer", "tokens": ["Der", "deut\u00b7sche", "Bau\u00b7er", "schaut", "dem", "Step\u00b7pen\u00b7pflan\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ins tiefgebr\u00e4unte Angesicht.", "tokens": ["Ins", "tief\u00b7ge\u00b7br\u00e4un\u00b7te", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O welch ein endlos W\u00fchlen, welch ein Rauschen!", "tokens": ["O", "welch", "ein", "end\u00b7los", "W\u00fch\u00b7len", ",", "welch", "ein", "Rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "ADJD", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O welch ein Markt, welch Hinundwiedertauschen", "tokens": ["O", "welch", "ein", "Markt", ",", "welch", "Hi\u00b7nund\u00b7wie\u00b7der\u00b7tau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PIAT", "ART", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Sch\u00e4tzen, wie sie jede Zon' erzieht!", "tokens": ["Von", "Sch\u00e4t\u00b7zen", ",", "wie", "sie", "je\u00b7de", "Zon'", "er\u00b7zieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jeder ist Kaufmann, und mit ew'gem Schwanken", "tokens": ["Je\u00b7der", "ist", "Kauf\u00b7mann", ",", "und", "mit", "ew'\u00b7gem", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Von Mann zu Mann gehn Waren und Gedanken,", "tokens": ["Von", "Mann", "zu", "Mann", "gehn", "Wa\u00b7ren", "und", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Juden Gold, des S\u00e4ngers Lied.", "tokens": ["Des", "Ju\u00b7den", "Gold", ",", "des", "S\u00e4n\u00b7gers", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der tote Buchstab' weicht lebend'ger Rede,", "tokens": ["Der", "to\u00b7te", "Buch\u00b7stab'", "weicht", "le\u00b7ben\u00b7d'\u00b7ger", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gek\u00e4mpft wird Blick in Blick der Geister Fehde,", "tokens": ["Ge\u00b7k\u00e4mpft", "wird", "Blick", "in", "Blick", "der", "Geis\u00b7ter", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "APPR", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder schlie\u00dft sich Hand in Hand der Bund;", "tokens": ["Und", "wie\u00b7der", "schlie\u00dft", "sich", "Hand", "in", "Hand", "der", "Bund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Frohlockend sp\u00fcrt der Stamm im Bruderstamme", "tokens": ["Froh\u00b7lo\u00b7ckend", "sp\u00fcrt", "der", "Stamm", "im", "Bru\u00b7der\u00b7stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein eigen Blut, es schwebt wie eine Flamme", "tokens": ["Sein", "ei\u00b7gen", "Blut", ",", "es", "schwebt", "wie", "ei\u00b7ne", "Flam\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Freiheit Wort auf jedem Mund.", "tokens": ["Der", "Frei\u0