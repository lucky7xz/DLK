{"textgrid.poem.47629": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[untersuche meinen armen Kopf!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Untersuche meinen armen Kopf!", "tokens": ["Un\u00b7ter\u00b7su\u00b7che", "mei\u00b7nen", "ar\u00b7men", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Denn es scheint, als sei es hier nicht richtig.", "tokens": ["Denn", "es", "scheint", ",", "als", "sei", "es", "hier", "nicht", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Alles, Alles scheint mir ohne Werth;", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "scheint", "mir", "oh\u00b7ne", "Werth", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und nur eine liebe Locke wichtig.", "tokens": ["Und", "nur", "ei\u00b7ne", "lie\u00b7be", "Lo\u00b7cke", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}