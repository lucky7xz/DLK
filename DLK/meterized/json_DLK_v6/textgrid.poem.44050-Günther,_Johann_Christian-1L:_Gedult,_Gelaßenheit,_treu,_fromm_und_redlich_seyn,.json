{"textgrid.poem.44050": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gedult, Gela\u00dfenheit, treu, fromm und redlich seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedult, Gela\u00dfenheit, treu, fromm und redlich seyn,", "tokens": ["Ge\u00b7dult", ",", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", ",", "treu", ",", "fromm", "und", "red\u00b7lich", "seyn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie ihr Tugenden euch sonst noch alle nennet,", "tokens": ["Und", "wie", "ihr", "Tu\u00b7gen\u00b7den", "euch", "sonst", "noch", "al\u00b7le", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzeiht es, doch nicht mir, nein, sondern meiner Pein,", "tokens": ["Ver\u00b7zeiht", "es", ",", "doch", "nicht", "mir", ",", "nein", ",", "son\u00b7dern", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PTKNEG", "PPER", "$,", "PTKANT", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die unaufh\u00f6rlich tobt und bis zum Marcke brennet,", "tokens": ["Die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "tobt", "und", "bis", "zum", "Mar\u00b7cke", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich geb euch mit Vernunft und reifem Wohlbedacht,", "tokens": ["Ich", "geb", "euch", "mit", "Ver\u00b7nunft", "und", "rei\u00b7fem", "Wohl\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Merckt dieses Wort nur wohl, von nun an gute Nacht;", "tokens": ["Merckt", "die\u00b7ses", "Wort", "nur", "wohl", ",", "von", "nun", "an", "gu\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "ADV", "$,", "APPR", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df ich euch gedient, das nenn ich eine S\u00fcnde,", "tokens": ["Und", "da\u00df", "ich", "euch", "ge\u00b7dient", ",", "das", "nenn", "ich", "ei\u00b7ne", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ich mir selber kaum jemahls vergeben kan.", "tokens": ["Die", "ich", "mir", "sel\u00b7ber", "kaum", "je\u00b7mahls", "ver\u00b7ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Steckt k\u00fcnftig, wen ihr wollt, mit euren Strahlen an,", "tokens": ["Steckt", "k\u00fcnf\u00b7tig", ",", "wen", "ihr", "wollt", ",", "mit", "eu\u00b7ren", "Strah\u00b7len", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWS", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich schw\u00f6re, da\u00df ich mich von eurem Ruhm entbinde.", "tokens": ["Ich", "schw\u00f6\u00b7re", ",", "da\u00df", "ich", "mich", "von", "eu\u00b7rem", "Ruhm", "ent\u00b7bin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr L\u00fcgner, die ihr noch dem P\u00f6bel Nasen dreht,", "tokens": ["Ihr", "L\u00fcg\u00b7ner", ",", "die", "ihr", "noch", "dem", "P\u00f6\u00b7bel", "Na\u00b7sen", "dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von vieler Vorsicht schwazt, des H\u00f6chsten Gnad erhebet,", "tokens": ["Von", "vie\u00b7ler", "Vor\u00b7sicht", "schwazt", ",", "des", "H\u00f6chs\u00b7ten", "Gnad", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Armen Trost versprecht und, wenn ein S\u00fcnder fleht,", "tokens": ["Dem", "Ar\u00b7men", "Trost", "ver\u00b7sprecht", "und", ",", "wenn", "ein", "S\u00fcn\u00b7der", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihm Rettung, Rath und Kraft, ja, mit dem Maule gebet,", "tokens": ["Ihm", "Ret\u00b7tung", ",", "Rath", "und", "Kraft", ",", "ja", ",", "mit", "dem", "Mau\u00b7le", "ge\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "$,", "PTKANT", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo steckt denn nun der Gott, der helfen will und kan?", "tokens": ["Wo", "steckt", "denn", "nun", "der", "Gott", ",", "der", "hel\u00b7fen", "will", "und", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er nimmt ja, wie ihr sprecht, die gr\u00f6bsten S\u00fcnder an:", "tokens": ["Er", "nimmt", "ja", ",", "wie", "ihr", "sprecht", ",", "die", "gr\u00f6bs\u00b7ten", "S\u00fcn\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich will der gr\u00f6bste seyn, ich warthe, schrey und leide;", "tokens": ["Ich", "will", "der", "gr\u00f6bs\u00b7te", "seyn", ",", "ich", "wart\u00b7he", ",", "schrey", "und", "lei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "VAINF", "$,", "PPER", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo bleibt denn auch sein Sohn? Wo ist der Geist der Ruh?", "tokens": ["Wo", "bleibt", "denn", "auch", "sein", "Sohn", "?", "Wo", "ist", "der", "Geist", "der", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Langt jenes Unschuldskleid und dieses Kraft nicht zu,", "tokens": ["Langt", "je\u00b7nes", "Un\u00b7schulds\u00b7kleid", "und", "die\u00b7ses", "Kraft", "nicht", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "KON", "PDAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df beider Liebe mich vor Gottes Zorn bekleide?", "tokens": ["Da\u00df", "bei\u00b7der", "Lie\u00b7be", "mich", "vor", "Got\u00b7tes", "Zorn", "be\u00b7klei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ha, blindes Fabelwerck, ich seh dein Larvenspiel.", "tokens": ["Ha", ",", "blin\u00b7des", "Fa\u00b7bel\u00b7werck", ",", "ich", "seh", "dein", "Lar\u00b7ven\u00b7spiel", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies geb ich auch noch zu: es ist ein ewig Wesen,", "tokens": ["Dies", "geb", "ich", "auch", "noch", "zu", ":", "es", "ist", "ein", "e\u00b7wig", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das seine gr\u00f6ste Macht an mir nur zeigen will", "tokens": ["Das", "sei\u00b7ne", "gr\u00f6s\u00b7te", "Macht", "an", "mir", "nur", "zei\u00b7gen", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und das mich obenhin zur Marter auserlesen;", "tokens": ["Und", "das", "mich", "o\u00b7ben\u00b7hin", "zur", "Mar\u00b7ter", "au\u00b7ser\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es f\u00fchrt, es leitet mich, doch stets auf meinen Fall,", "tokens": ["Es", "f\u00fchrt", ",", "es", "lei\u00b7tet", "mich", ",", "doch", "stets", "auf", "mei\u00b7nen", "Fall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es giebt Gelegenheit, damit es \u00fcberall", "tokens": ["Es", "giebt", "Ge\u00b7le\u00b7gen\u00b7heit", ",", "da\u00b7mit", "es", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich r\u00fchmlich strafen kan und stets entschuldigt scheine.", "tokens": ["Mich", "r\u00fchm\u00b7lich", "stra\u00b7fen", "kan", "und", "stets", "ent\u00b7schul\u00b7digt", "schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "KON", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bisweilen zeigt es mir das Gl\u00fccke, recht zu gehn,", "tokens": ["Bis\u00b7wei\u00b7len", "zeigt", "es", "mir", "das", "Gl\u00fc\u00b7cke", ",", "recht", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald l\u00e4st es mich in mir dem Guten widerstehn,", "tokens": ["Bald", "l\u00e4st", "es", "mich", "in", "mir", "dem", "Gu\u00b7ten", "wi\u00b7der\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit die fr\u00f6mmste Welt das \u00c4rgste von mir meine.", "tokens": ["Da\u00b7mit", "die", "fr\u00f6mms\u00b7te", "Welt", "das", "\u00c4rgs\u00b7te", "von", "mir", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus dieser Quelle springt mein langes Ungemach:", "tokens": ["Aus", "die\u00b7ser", "Quel\u00b7le", "springt", "mein", "lan\u00b7ges", "Un\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Arbeit und kein Lohn als Kranckheit, Ha\u00df und Schande.", "tokens": ["Viel", "Ar\u00b7beit", "und", "kein", "Lohn", "als", "Kran\u00b7ck\u00b7heit", ",", "Ha\u00df", "und", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Sp\u00f6tter pfeifen mir mit Neid und L\u00fcgen nach,", "tokens": ["Die", "Sp\u00f6t\u00b7ter", "pfei\u00b7fen", "mir", "mit", "Neid", "und", "L\u00fc\u00b7gen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Armuth jagt den Fu\u00df aus dem und jenem Lande,", "tokens": ["Die", "Ar\u00b7muth", "jagt", "den", "Fu\u00df", "aus", "dem", "und", "je\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Eltern treiben mich den Feinden vor die Th\u00fcr", "tokens": ["Die", "El\u00b7tern", "trei\u00b7ben", "mich", "den", "Fein\u00b7den", "vor", "die", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sto\u00dfen mich \u2013 o Gott, gieb Acht, sie folgen dir \u2013", "tokens": ["Und", "sto\u00b7\u00dfen", "mich", "\u2013", "o", "Gott", ",", "gieb", "Acht", ",", "sie", "fol\u00b7gen", "dir", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "FM", "NN", "$,", "VVIMP", "CARD", "$,", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ohn Ursach in den Staub und ewig aus dem Herzen.", "tokens": ["Ohn", "Ur\u00b7sach", "in", "den", "Staub", "und", "e\u00b7wig", "aus", "dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Wi\u00dfen wird verlacht, mein ehrlich Herz erdr\u00fcckt,", "tokens": ["Mein", "Wi\u00b7\u00dfen", "wird", "ver\u00b7lacht", ",", "mein", "ehr\u00b7lich", "Herz", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Fehler, die ich hab, als Laster vorger\u00fcckt,", "tokens": ["Die", "Feh\u00b7ler", ",", "die", "ich", "hab", ",", "als", "Las\u00b7ter", "vor\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "p