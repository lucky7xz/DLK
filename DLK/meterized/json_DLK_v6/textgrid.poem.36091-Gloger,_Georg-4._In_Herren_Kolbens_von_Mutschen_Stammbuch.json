{"textgrid.poem.36091": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "4. In Herren Kolbens von Mutschen Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil nichts best\u00e4ndig ist, als Unbest\u00e4ndigkeit,", "tokens": ["Weil", "nichts", "be\u00b7st\u00e4n\u00b7dig", "ist", ",", "als", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kan man freilich wol nicht Haft und Anker finden,", "tokens": ["So", "kan", "man", "frei\u00b7lich", "wol", "nicht", "Haft", "und", "An\u00b7ker", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "PTKNEG", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wormit man sicherlich sich k\u00f6nnt' auf Frewde gr\u00fcnden.", "tokens": ["Wor\u00b7mit", "man", "si\u00b7cher\u00b7lich", "sich", "k\u00f6nnt'", "auf", "Frew\u00b7de", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "PRF", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das macht das stolze Gl\u00fcck und die verkehrte Zeit.", "tokens": ["Das", "macht", "das", "stol\u00b7ze", "Gl\u00fcck", "und", "die", "ver\u00b7kehr\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es geht nach Hofes Art, das zwar von Erden an", "tokens": ["Es", "geht", "nach", "Ho\u00b7fes", "Art", ",", "das", "zwar", "von", "Er\u00b7den", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Viel H\u00e4nd' und K\u00fcsse gibt und wenig trewe Herzen,", "tokens": ["Viel", "H\u00e4nd'", "und", "K\u00fcs\u00b7se", "gibt", "und", "we\u00b7nig", "tre\u00b7we", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das meist ist nur ein Schein und ein verbl\u00fcmtes Scherzen,", "tokens": ["Das", "meist", "ist", "nur", "ein", "Schein", "und", "ein", "ver\u00b7bl\u00fcm\u00b7tes", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ADV", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In welchem Meister hei\u00dft der es am besten kan.", "tokens": ["In", "wel\u00b7chem", "Meis\u00b7ter", "hei\u00dft", "der", "es", "am", "bes\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "ART", "PPER", "PTKA", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drumb traw und schawe wem, und richt' dich nach der Zeit,", "tokens": ["Drumb", "traw", "und", "scha\u00b7we", "wem", ",", "und", "richt'", "dich", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "VVFIN", "PIS", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Probier den hundertmal, dem du wilst einmal trawen,", "tokens": ["Pro\u00b7bier", "den", "hun\u00b7dert\u00b7mal", ",", "dem", "du", "wilst", "ein\u00b7mal", "tra\u00b7wen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "$,", "PRELS", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und deiner Trewe Grund auf seine Trewe bawen,", "tokens": ["Und", "dei\u00b7ner", "Tre\u00b7we", "Grund", "auf", "sei\u00b7ne", "Tre\u00b7we", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Weil nichts best\u00e4ndig ist als Unbest\u00e4ndigkeit.", "tokens": ["Weil", "nichts", "be\u00b7st\u00e4n\u00b7dig", "ist", "als", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}