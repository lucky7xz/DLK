{"dta.poem.2262": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfer B\u00fccherfreund besa\u00df, was alle Weisen,", "tokens": ["Ein", "gro\u00b7\u00dfer", "B\u00fc\u00b7cher\u00b7freund", "be\u00b7sa\u00df", ",", "was", "al\u00b7le", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tugend einzusehn, die Tugend anzupreisen,", "tokens": ["Die", "Tu\u00b7gend", "ein\u00b7zu\u00b7sehn", ",", "die", "Tu\u00b7gend", "an\u00b7zu\u00b7prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In alt- und neuer Zeit mit Sorgfalt ausgedacht,", "tokens": ["In", "al\u00b7t", "und", "neu\u00b7er", "Zeit", "mit", "Sorg\u00b7falt", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In alt- und neuer Zeit zur Be\u00dfrung kund gemacht.", "tokens": ["In", "al\u00b7t", "und", "neu\u00b7er", "Zeit", "zur", "Be\u00df\u00b7rung", "kund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Allein, war die\u00df gleich alles da,", "tokens": ["Al\u00b7lein", ",", "war", "die\u00df", "gleich", "al\u00b7les", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PDS", "ADV", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So blieb doch eins, ach eins! vergessen:", "tokens": ["So", "blieb", "doch", "eins", ",", "ach", "eins", "!", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$,", "XY", "CARD", "$.", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhm fehlte ja so viel, als er bisher besessen;", "tokens": ["Jhm", "fehl\u00b7te", "ja", "so", "viel", ",", "als", "er", "bis\u00b7her", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhm fehlte Pamela!", "tokens": ["Jhm", "fehl\u00b7te", "Pa\u00b7me\u00b7la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}