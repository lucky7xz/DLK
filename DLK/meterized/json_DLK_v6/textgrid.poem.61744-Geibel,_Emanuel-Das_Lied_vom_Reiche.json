{"textgrid.poem.61744": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Reiche", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf und unverdrossen,", "tokens": ["Frisch", "auf", "und", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie grimm die Welt auch tut!", "tokens": ["Wie", "grimm", "die", "Welt", "auch", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die zwei sind dir Genossen,", "tokens": ["Die", "zwei", "sind", "dir", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Gott und deutscher Mut.", "tokens": ["Dein", "Gott", "und", "deut\u00b7scher", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob's Herz schier bricht,", "tokens": ["Ob's", "Herz", "schier", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Verzage nicht,", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Z\u00e4hne bei\u00df zusammen!", "tokens": ["Die", "Z\u00e4h\u00b7ne", "bei\u00df", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es f\u00fcgt sich doch,", "tokens": ["Es", "f\u00fcgt", "sich", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wof\u00fcr so hoch", "tokens": ["Wo\u00b7f\u00fcr", "so", "hoch"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die besten Herzen flammen.", "tokens": ["Die", "bes\u00b7ten", "Her\u00b7zen", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht knechtisch Wohlbehagen,", "tokens": ["Nicht", "knech\u00b7tisch", "Wohl\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch blutig Gaukelspiel", "tokens": ["Noch", "blu\u00b7tig", "Gau\u00b7kel\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus welscher Gleichheit Tagen", "tokens": ["Aus", "wel\u00b7scher", "Gleich\u00b7heit", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist unsres Volkes Ziel.", "tokens": ["Ist", "uns\u00b7res", "Vol\u00b7kes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch birgt sein Herz", "tokens": ["Doch", "birgt", "sein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nicht mehr den Schmerz", "tokens": ["Nicht", "mehr", "den", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Um die zerborstne Eiche,", "tokens": ["Um", "die", "zer\u00b7borst\u00b7ne", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch w\u00e4chst das Wort", "tokens": ["Doch", "w\u00e4chst", "das", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Allm\u00e4chtig fort,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "fort", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das Wort vom deutschen Reiche.", "tokens": ["Das", "Wort", "vom", "deut\u00b7schen", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl h\u00e4lt der alte Drache", "tokens": ["Wohl", "h\u00e4lt", "der", "al\u00b7te", "Dra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielk\u00f6pf'ger Eifersucht", "tokens": ["Viel\u00b7k\u00f6pf'\u00b7ger", "Ei\u00b7fer\u00b7sucht"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Baum des Lebens Wache", "tokens": ["Am", "Baum", "des", "Le\u00b7bens", "Wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weigert uns die Frucht.", "tokens": ["Und", "wei\u00b7gert", "uns", "die", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch, wie er faucht", "tokens": ["Doch", ",", "wie", "er", "faucht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und Flammen haucht,", "tokens": ["Und", "Flam\u00b7men", "haucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "La\u00df dich nicht mit zerspalten!", "tokens": ["La\u00df", "dich", "nicht", "mit", "zer\u00b7spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Getrost im Graus,", "tokens": ["Ge\u00b7trost", "im", "Graus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mein Volk, halt aus!", "tokens": ["Mein", "Volk", ",", "halt", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Gott wird der Hoffnung walten.", "tokens": ["Gott", "wird", "der", "Hoff\u00b7nung", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Treue kann's nicht fehlen,", "tokens": ["Der", "Treu\u00b7e", "kann's", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beharren bringt Gedeihn;", "tokens": ["Be\u00b7har\u00b7ren", "bringt", "Ge\u00b7deihn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was reif ward in den Seelen,", "tokens": ["Was", "reif", "ward", "in", "den", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das schafft sich Fleisch und Bein.", "tokens": ["Das", "schafft", "sich", "Fleisch", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird die Not", "tokens": ["Es", "wird", "die", "Not"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ihr laut Gebot", "tokens": ["Ihr", "laut", "Ge\u00b7bot"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Im Schlachtendonner sprechen;", "tokens": ["Im", "Schlach\u00b7ten\u00b7don\u00b7ner", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kommt's nicht jetzt,", "tokens": ["Und", "kommt's", "nicht", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So kommt's zuletzt", "tokens": ["So", "kommt's", "zu\u00b7letzt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Mit Biegen oder Brechen.", "tokens": ["Mit", "Bie\u00b7gen", "o\u00b7der", "Bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das ist die einz'ge S\u00fchne", "tokens": ["Das", "ist", "die", "einz'\u00b7ge", "S\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist des Liedes Schlu\u00df,", "tokens": ["Das", "ist", "des", "Lie\u00b7des", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist der Lenz, der gr\u00fcne,", "tokens": ["Das", "ist", "der", "Lenz", ",", "der", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der endlich werden mu\u00df:", "tokens": ["Der", "end\u00b7lich", "wer\u00b7den", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Voll Macht und Ruhm", "tokens": ["Voll", "Macht", "und", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das Kaisertum,", "tokens": ["Das", "Kai\u00b7ser\u00b7tum", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dem freien Volk zum Frommen.", "tokens": ["Dem", "frei\u00b7en", "Volk", "zum", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Drum, wie's auch tost,", "tokens": ["Drum", ",", "wie's", "auch", "tost", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Herz, sei getrost!", "tokens": ["Herz", ",", "sei", "ge\u00b7trost", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das Reich wird dennoch kommen.", "tokens": ["Das", "Reich", "wird", "den\u00b7noch", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}