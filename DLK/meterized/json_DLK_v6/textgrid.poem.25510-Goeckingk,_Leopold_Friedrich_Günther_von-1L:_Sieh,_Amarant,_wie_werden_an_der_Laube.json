{"textgrid.poem.25510": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, Amarant, wie werden an der Laube", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, Amarant, wie werden an der Laube", "tokens": ["Sieh", ",", "A\u00b7ma\u00b7rant", ",", "wie", "wer\u00b7den", "an", "der", "Lau\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PWAV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00e4tter gelb und roth!", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "gelb", "und", "roth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Horch! wie da schon der Nord, zu seinem Raube", "tokens": ["Horch", "!", "wie", "da", "schon", "der", "Nord", ",", "zu", "sei\u00b7nem", "Rau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADV", "ADV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie abzuholen, droht!", "tokens": ["Sie", "ab\u00b7zu\u00b7ho\u00b7len", ",", "droht", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was wird uns nun im Vollmond' noch verstecken?", "tokens": ["Was", "wird", "uns", "nun", "im", "Voll\u00b7mond'", "noch", "ver\u00b7ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kalt sey die Nacht; f\u00fcr mich", "tokens": ["Kalt", "sey", "die", "Nacht", ";", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist's warm genug; doch wird kein Schnee entdecken,", "tokens": ["Ist's", "warm", "ge\u00b7nug", ";", "doch", "wird", "kein", "Schnee", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "$.", "ADV", "VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer durch das Pf\u00f6rtchen schlich?", "tokens": ["Wer", "durch", "das", "Pf\u00f6rt\u00b7chen", "schlich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wird nicht der G\u00e4nse Schnattern, nicht das Knarren", "tokens": ["Wird", "nicht", "der", "G\u00e4n\u00b7se", "Schnat\u00b7tern", ",", "nicht", "das", "Knar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Th\u00fcren, das Gebell", "tokens": ["Der", "Th\u00fc\u00b7ren", ",", "das", "Ge\u00b7bell"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der Hunde, dich verrathen? Welch ein Harren", "tokens": ["Der", "Hun\u00b7de", ",", "dich", "ver\u00b7ra\u00b7then", "?", "Welch", "ein", "Har\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVPP", "$.", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr mich, am Kammerschwell'?", "tokens": ["F\u00fcr", "mich", ",", "am", "Kam\u00b7mer\u00b7schwell'", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ein jeder Laut ruft da gewi\u00df dem bangen,", "tokens": ["Ein", "je\u00b7der", "Laut", "ruft", "da", "ge\u00b7wi\u00df", "dem", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Verzagten Herzen zu:", "tokens": ["Ver\u00b7zag\u00b7ten", "Her\u00b7zen", "zu", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Horch', Nante! deine Mutter kommt gegangen,", "tokens": ["Horch'", ",", "Nan\u00b7te", "!", "dei\u00b7ne", "Mut\u00b7ter", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und, was sie sucht, bist du!", "tokens": ["Und", ",", "was", "sie", "sucht", ",", "bist", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Doch, den sie finden wird, auf leisen Socken", "tokens": ["Doch", ",", "den", "sie", "fin\u00b7den", "wird", ",", "auf", "lei\u00b7sen", "So\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "PPER", "VVINF", "VAFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Einschleichend, wie ein Dieb,", "tokens": ["Ein\u00b7schlei\u00b7chend", ",", "wie", "ein", "Dieb", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Der, \u2013 ha! wie steht sie staunend und erschrocken! \u2013", "tokens": ["Der", ",", "\u2013", "ha", "!", "wie", "steht", "sie", "stau\u00b7nend", "und", "er\u00b7schro\u00b7cken", "!", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$,", "$(", "ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADJD", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "War heut' ihr noch so lieb!", "tokens": ["War", "heut'", "ihr", "noch", "so", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Nein! lieber Mann! wo willst du sonst mich sprechen?", "tokens": ["Nein", "!", "lie\u00b7ber", "Mann", "!", "wo", "willst", "du", "sonst", "mich", "spre\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "NN", "$.", "PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und finden sollst du mich!", "tokens": ["Und", "fin\u00b7den", "sollst", "du", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Nur solch ein sch\u00f6nes Mutterherz zu brechen \u2013", "tokens": ["Nur", "solch", "ein", "sch\u00f6\u00b7nes", "Mut\u00b7ter\u00b7herz", "zu", "bre\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ich liebe sie \u2013 wie dich.", "tokens": ["Ich", "lie\u00b7be", "sie", "\u2013", "wie", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}