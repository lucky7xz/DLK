{"dta.poem.21884": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Xl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Der verfluchte Hagels-Neid", "tokens": ["Der", "ver\u00b7fluch\u00b7te", "Ha\u00b7gels\u00b7Neid"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hat ein allzu scharff Gesicht", "tokens": ["hat", "ein", "all\u00b7zu", "scharff", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PTKA", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "drum hat Rosilis das Licht", "tokens": ["drum", "hat", "Ro\u00b7si\u00b7lis", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "NE", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "allzun\u00e4hrlich abgemeyt.", "tokens": ["all\u00b7zu\u00b7n\u00e4\u00b7hr\u00b7lich", "ab\u00b7ge\u00b7meyt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Weil der Neid nach Schwefel tacht/", "tokens": ["Weil", "der", "Neid", "nach", "Schwe\u00b7fel", "tacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Feuer-zeug und Zunder gehet/", "tokens": ["Feu\u00b7er\u00b7zeug", "und", "Zun\u00b7der", "ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "hat die Rosilis verbracht", "tokens": ["hat", "die", "Ro\u00b7si\u00b7lis", "ver\u00b7bracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NE", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "was ein ieder nicht verstehet.", "tokens": ["was", "ein", "ie\u00b7der", "nicht", "ver\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}