{"textgrid.poem.53857": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Herr Wendriner geht ins Theater", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsehn Se, es hat schon angefangen!\u00ab", "tokens": ["\u00bb", "sehn", "Se", ",", "es", "hat", "schon", "an\u00b7ge\u00b7fan\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbverr\u00e4terei wird nicht vererbt, mein F\u00fcrst,", "tokens": ["\u00bb", "ver\u00b7r\u00e4\u00b7te\u00b7rei", "wird", "nicht", "ver\u00b7erbt", ",", "mein", "F\u00fcrst", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcberk\u00e4men wir sie von Verwandten,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7k\u00e4\u00b7men", "wir", "sie", "von", "Ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was gehts mich an? Mein Vater \u00fcbte keine.\u00ab", "tokens": ["Was", "gehts", "mich", "an", "?", "Mein", "Va\u00b7ter", "\u00fcb\u00b7te", "kei\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "PIAT", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbo arme Rosalinde, wohin willst du?", "tokens": ["\u00bb", "o", "ar\u00b7me", "Ro\u00b7sa\u00b7lin\u00b7de", ",", "wo\u00b7hin", "willst", "du", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Willst du die V\u00e4ter tauschen? So nimm meinen!\u00ab", "tokens": ["Willst", "du", "die", "V\u00e4\u00b7ter", "tau\u00b7schen", "?", "So", "nimm", "mei\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "ADV", "VVIMP", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbk\u00f6nnt ich vom Gl\u00fcck nur diesen Lohn erwerben,", "tokens": ["\u00bb", "k\u00f6nnt", "ich", "vom", "Gl\u00fcck", "nur", "die\u00b7sen", "Lohn", "er\u00b7wer\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "ADV", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Schuldner meines Herrn und sanft zu sterben!\u00ab", "tokens": ["Nicht", "Schuld\u00b7ner", "mei\u00b7nes", "Herrn", "und", "sanft", "zu", "ster\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "NN", "PPOSAT", "NN", "KON", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}