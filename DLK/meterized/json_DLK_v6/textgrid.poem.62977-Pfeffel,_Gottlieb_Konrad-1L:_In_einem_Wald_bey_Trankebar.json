{"textgrid.poem.62977": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem Wald bey Trankebar", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Wald bey Trankebar", "tokens": ["In", "ei\u00b7nem", "Wald", "bey", "Tran\u00b7ke\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam eine L\u00f6win in die Wochen,", "tokens": ["Kam", "ei\u00b7ne", "L\u00f6\u00b7win", "in", "die", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, selbst aus Feindes Mund gesprochen,", "tokens": ["Die", ",", "selbst", "aus", "Fein\u00b7des", "Mund", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Muster jeder Tugend war.", "tokens": ["Ein", "Mus\u00b7ter", "je\u00b7der", "Tu\u00b7gend", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ach! schon in den ersten Tagen", "tokens": ["Doch", "ach", "!", "schon", "in", "den", "ers\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ward ihre Frucht zu Grab getragen.", "tokens": ["Ward", "ih\u00b7re", "Frucht", "zu", "Grab", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie lag betr\u00fcbt auf k\u00fchlem Grund,", "tokens": ["Sie", "lag", "be\u00b7tr\u00fcbt", "auf", "k\u00fch\u00b7lem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als ihr getreuer Freund, der Hund,", "tokens": ["Als", "ihr", "ge\u00b7treu\u00b7er", "Freund", ",", "der", "Hund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der stets an ihrer Seite wachte,", "tokens": ["Der", "stets", "an", "ih\u00b7rer", "Sei\u00b7te", "wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit einem Blick voll Zuversicht", "tokens": ["Mit", "ei\u00b7nem", "Blick", "voll", "Zu\u00b7ver\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihr einen jungen Tieger brachte.", "tokens": ["Ihr", "ei\u00b7nen", "jun\u00b7gen", "Tie\u00b7ger", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Was soll ich mit dem kleinen Wicht?", "tokens": ["Was", "soll", "ich", "mit", "dem", "klei\u00b7nen", "Wicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Fragt sie best\u00fcrzt. \u00bbEy nun, ich dachte,", "tokens": ["Fragt", "sie", "be\u00b7st\u00fcrzt", ".", "\u00bb", "Ey", "nun", ",", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "$(", "NN", "ADV", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Versetzt der Freund, du solltest ihn", "tokens": ["Ver\u00b7setzt", "der", "Freund", ",", "du", "soll\u00b7test", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Statt deines Kindes auferziehn.\u00ab", "tokens": ["Statt", "dei\u00b7nes", "Kin\u00b7des", "auf\u00b7er\u00b7ziehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ich? rief sie hastig; lieber sterben:", "tokens": ["Ich", "?", "rief", "sie", "has\u00b7tig", ";", "lie\u00b7ber", "ster\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$.", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ich einen Tieger! \u2013 \u00bbEitler Wahn!", "tokens": ["Ich", "ei\u00b7nen", "Tie\u00b7ger", "!", "\u2013", "\u00bb", "Eit\u00b7ler", "Wahn", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "$(", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sprach Philax; kann man Laster erben,", "tokens": ["Sprach", "Phi\u00b7lax", ";", "kann", "man", "Las\u00b7ter", "er\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So steckt wohl auch die Tugend an.\u00ab", "tokens": ["So", "steckt", "wohl", "auch", "die", "Tu\u00b7gend", "an", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}