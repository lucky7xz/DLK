{"textgrid.poem.54101": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Frau spricht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Mann schl\u00e4ft immer gleich ein . . . oder er raucht seine Zeitung", "tokens": ["Mein", "Mann", "schl\u00e4ft", "im\u00b7mer", "gleich", "ein", ".", ".", ".", "o\u00b7der", "er", "raucht", "sei\u00b7ne", "Zei\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$.", "$.", "$.", "KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und liest seine Zigarre", "tokens": ["und", "liest", "sei\u00b7ne", "Zi\u00b7gar\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": " . . . Ich bin so nerv\u00f6s . . . und w\u00e4hrend ich an die Decke starre,", "tokens": [".", ".", ".", "Ich", "bin", "so", "ner\u00b7v\u00f6s", ".", ".", ".", "und", "w\u00e4h\u00b7rend", "ich", "an", "die", "De\u00b7cke", "star\u00b7re", ","], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$.", "$.", "KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "denke ich mir mein Teil.", "tokens": ["den\u00b7ke", "ich", "mir", "mein", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man gibt ihnen so viel, wenigstens zu Beginn. Sie sind es nicht wert.", "tokens": ["Man", "gibt", "ih\u00b7nen", "so", "viel", ",", "we\u00b7nigs\u00b7tens", "zu", "Be\u00b7ginn", ".", "Sie", "sind", "es", "nicht", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "APPR", "NN", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.6": {"text": "Sie glauben immer, man m\u00fcsse hochgeehrt", "tokens": ["Sie", "glau\u00b7ben", "im\u00b7mer", ",", "man", "m\u00fcs\u00b7se", "hoch\u00b7geehrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PIS", "VMFIN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "sein, weil man sie liebt.", "tokens": ["sein", ",", "weil", "man", "sie", "liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAINF", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ob es das wohl gibt:", "tokens": ["Ob", "es", "das", "wohl", "gibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "ein Mann, der so nett bleibt, so aufmerksam", "tokens": ["ein", "Mann", ",", "der", "so", "nett", "bleibt", ",", "so", "auf\u00b7merk\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+--+--++-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "wie am ersten Tag, wo er einen nahm . . . ?", "tokens": ["wie", "am", "ers\u00b7ten", "Tag", ",", "wo", "er", "ei\u00b7nen", "nahm", ".", ".", ".", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$.", "$.", "$.", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Einer, der Freund ist und Mann und Liebhaber; der uns mal neckt,", "tokens": ["Ei\u00b7ner", ",", "der", "Freund", "ist", "und", "Mann", "und", "Lieb\u00b7ha\u00b7ber", ";", "der", "uns", "mal", "neckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ART", "NN", "VAFIN", "KON", "NN", "KON", "NN", "$.", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "mal bevatert, der immer neu ist, vor dem man Respekt", "tokens": ["mal", "be\u00b7va\u00b7tert", ",", "der", "im\u00b7mer", "neu", "ist", ",", "vor", "dem", "man", "Res\u00b7pekt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "APPR", "ART", "PIS", "NN"], "meter": "--+--+-+-+---+", "measure": "anapaest.di.plus"}, "line.13": {"text": "hat und der einen liebt . . . liebt . . . liebt . . .", "tokens": ["hat", "und", "der", "ei\u00b7nen", "liebt", ".", ".", ".", "liebt", ".", ".", ".", "liebt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "KON", "ART", "PIS", "VVFIN", "$.", "$.", "$.", "VVFIN", "$.", "$.", "$.", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "ob es das gibt?", "tokens": ["ob", "es", "das", "gibt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Manchmal denke ich: ja.", "tokens": ["Manch\u00b7mal", "den\u00b7ke", "ich", ":", "ja", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dann sehe ich: nein.", "tokens": ["Dann", "se\u00b7he", "ich", ":", "nein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PTKANT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Man f\u00e4llt immer wieder auf sie herein.", "tokens": ["Man", "f\u00e4llt", "im\u00b7mer", "wie\u00b7der", "auf", "sie", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und ich frage mich blo\u00df, wo diese Kerls ihre Nerven haben.", "tokens": ["Und", "ich", "fra\u00b7ge", "mich", "blo\u00df", ",", "wo", "die\u00b7se", "Kerls", "ih\u00b7re", "Ner\u00b7ven", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PDAT", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "--+--+-+-+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wahrscheinlich . . . na ja. Die diesbez\u00fcglichen Gaben", "tokens": ["Wahr\u00b7schein\u00b7lich", ".", ".", ".", "na", "ja", ".", "Die", "dies\u00b7be\u00b7z\u00fcg\u00b7li\u00b7chen", "Ga\u00b7ben"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "$.", "$.", "ITJ", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-------+--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "sind wohl ungleich verteilt. So richtig verstehen sie uns nie.", "tokens": ["sind", "wohl", "un\u00b7gleich", "ver\u00b7teilt", ".", "So", "rich\u00b7tig", "ver\u00b7ste\u00b7hen", "sie", "uns", "nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$.", "ADV", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Weil sie faul sind, murmeln sie was von Hysterie.", "tokens": ["Weil", "sie", "faul", "sind", ",", "mur\u00b7meln", "sie", "was", "von", "Hys\u00b7te\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PIS", "APPR", "NE", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ist aber keine. Und wollen wir Z\u00e4rtlichkeit,", "tokens": ["Ist", "a\u00b7ber", "kei\u00b7ne", ".", "Und", "wol\u00b7len", "wir", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "$.", "KON", "VMFIN", "PPER", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "dann haben die Herren meist keine Zeit.", "tokens": ["dann", "ha\u00b7ben", "die", "Her\u00b7ren", "meist", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Sie spielen: Symphonie mit dem Paukenschlag.", "tokens": ["Sie", "spie\u00b7len", ":", "Sym\u00b7pho\u00b7nie", "mit", "dem", "Pau\u00b7ken\u00b7schlag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Unsere Liebe aber verzittert, das ist nicht ihr Geschmack.", "tokens": ["Un\u00b7se\u00b7re", "Lie\u00b7be", "a\u00b7ber", "ver\u00b7zit\u00b7tert", ",", "das", "ist", "nicht", "ihr", "Ge\u00b7schmack", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,", "PDS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--++--+", "measure": "iambic.septa.invert"}, "line.12": {"text": "Hop-hop-hop \u2013 wie an der B\u00f6rse. Sie sind eigentlich nie mehr als erotische Statisterie.", "tokens": ["Hop\u00b7hop\u00b7hop", "\u2013", "wie", "an", "der", "B\u00f6r\u00b7se", ".", "Sie", "sind", "ei\u00b7gent\u00b7lich", "nie", "mehr", "als", "e\u00b7ro\u00b7ti\u00b7sche", "Sta\u00b7tis\u00b7te\u00b7rie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOKOM", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.13": {"text": "Die Hauptrolle spielen wir. Wir singen allein Duett,", "tokens": ["Die", "Haupt\u00b7rol\u00b7le", "spie\u00b7len", "wir", ".", "Wir", "sin\u00b7gen", "al\u00b7lein", "Du\u00b7ett", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "leer in der Seele, bei sonst gut besuchtem Bett.", "tokens": ["leer", "in", "der", "See\u00b7le", ",", "bei", "sonst", "gut", "be\u00b7such\u00b7tem", "Bett", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "APPR", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Mein Mann schl\u00e4ft immer gleich ein, oder er dreht sich um und raucht seine Zigarre.", "tokens": ["Mein", "Mann", "schl\u00e4ft", "im\u00b7mer", "gleich", "ein", ",", "o\u00b7der", "er", "dreht", "sich", "um", "und", "raucht", "sei\u00b7ne", "Zi\u00b7gar\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PRF", "APPR", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+---+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Warum? Weil . . .", "tokens": ["Wa\u00b7rum", "?", "Weil", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["PWAV", "$.", "KOUS", "$.", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und w\u00e4hrend ich an die Decke starre,", "tokens": ["Und", "w\u00e4h\u00b7rend", "ich", "an", "die", "De\u00b7cke", "star\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "denke ich mir mein Teil.", "tokens": ["den\u00b7ke", "ich", "mir", "mein", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}