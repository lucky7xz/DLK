{"textgrid.poem.25710": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, Freund! ich mag nicht vorgestellt", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, Freund! ich mag nicht vorgestellt", "tokens": ["Nein", ",", "Freund", "!", "ich", "mag", "nicht", "vor\u00b7ge\u00b7stellt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei deinem F\u00fcrsten seyn,", "tokens": ["Bei", "dei\u00b7nem", "F\u00fcrs\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil er's f\u00fcr gro\u00dfe Gnade h\u00e4lt,", "tokens": ["Weil", "er's", "f\u00fcr", "gro\u00b7\u00dfe", "Gna\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In hohen Augenschein,", "tokens": ["In", "ho\u00b7hen", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gleich einem Thier' der neuen Welt,", "tokens": ["Gleich", "ei\u00b7nem", "Thier'", "der", "neu\u00b7en", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von ihm genommen seyn.", "tokens": ["Von", "ihm", "ge\u00b7nom\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auch geh' ich morgen nicht zu Gast", "tokens": ["Auch", "geh'", "ich", "mor\u00b7gen", "nicht", "zu", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei deinem reichen Mann',", "tokens": ["Bei", "dei\u00b7nem", "rei\u00b7chen", "Mann'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der blo\u00df aus Eitelkeit verpra\u00dft,", "tokens": ["Der", "blo\u00df", "aus", "Ei\u00b7tel\u00b7keit", "ver\u00b7pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was er durch Trug gewann;", "tokens": ["Was", "er", "durch", "Trug", "ge\u00b7wann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Denn wer, wie ich, die Schurken ha\u00dft,", "tokens": ["Denn", "wer", ",", "wie", "ich", ",", "die", "Schur\u00b7ken", "ha\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "PWAV", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dem stinkt ihr Wein auch an.", "tokens": ["Dem", "stinkt", "ihr", "Wein", "auch", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Dein gro\u00dfer Autor bleibt f\u00fcr mich", "tokens": ["Dein", "gro\u00b7\u00dfer", "Au\u00b7tor", "bleibt", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wohl auch in guter Ruh'.", "tokens": ["Wohl", "auch", "in", "gu\u00b7ter", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Streust du ihm Weihrauch, h\u00e4lt er sich", "tokens": ["Streust", "du", "ihm", "Weih\u00b7rauch", ",", "h\u00e4lt", "er", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$,", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Nase h\u00f6hnisch zu;", "tokens": ["Die", "Na\u00b7se", "h\u00f6h\u00b7nisch", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Streust keinen? so besieht er dich", "tokens": ["Streust", "kei\u00b7nen", "?", "so", "be\u00b7sieht", "er", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "$.", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Vom Kopf' bis auf die Schuh'.", "tokens": ["Vom", "Kopf'", "bis", "auf", "die", "Schuh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Drum bitte, Freund, auf ein Gericht", "tokens": ["Drum", "bit\u00b7te", ",", "Freund", ",", "auf", "ein", "Ge\u00b7richt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKANT", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Von Tauben und Salat,", "tokens": ["Von", "Tau\u00b7ben", "und", "Sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Den Kantor her, der weiser spricht,", "tokens": ["Den", "Kan\u00b7tor", "her", ",", "der", "wei\u00b7ser", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Als dein Geheimerrath.", "tokens": ["Als", "dein", "Ge\u00b7hei\u00b7mer\u00b7rath", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Denn, siehst du, eitel bin ich nicht,", "tokens": ["Denn", ",", "siehst", "du", ",", "ei\u00b7tel", "bin", "ich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Doch stolz in hohem Grad'.", "tokens": ["Doch", "stolz", "in", "ho\u00b7hem", "Grad'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}