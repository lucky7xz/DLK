{"dta.poem.4323": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dank-Gedanken  \n wegen eines  \n zu Ritzeb\u00fcttel durch Gottes Gnade  \n abgewandten Einbruchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem, mit unerh\u00f6rter Wut,", "tokens": ["Nach\u00b7dem", ",", "mit", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Wut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nord-Nord-Westen-Wind gest\u00fcrmet,", "tokens": ["Der", "Nord\u00b7Nord\u00b7Wes\u00b7ten\u00b7Wind", "ge\u00b7st\u00fcr\u00b7met", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und er des Meeres wilde Fluht", "tokens": ["Und", "er", "des", "Mee\u00b7res", "wil\u00b7de", "Fluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sehr gepre\u00dft und aufgeth\u00fcrmet,", "tokens": ["So", "sehr", "ge\u00b7pre\u00dft", "und", "auf\u00b7ge\u00b7th\u00fcr\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sie mit unsers Landes Damm,", "tokens": ["Da\u00df", "sie", "mit", "un\u00b7sers", "Lan\u00b7des", "Damm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dessen sogenanntem Kamm,", "tokens": ["Und", "des\u00b7sen", "so\u00b7ge\u00b7nann\u00b7tem", "Kamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bereits in gleicher Lage flo\u00df,", "tokens": ["Be\u00b7reits", "in", "glei\u00b7cher", "La\u00b7ge", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ja gar (was schrecklich anzusehen)", "tokens": ["Ja", "gar", "(", "was", "schreck\u00b7lich", "an\u00b7zu\u00b7se\u00b7hen", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$(", "PWS", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von einigen gesenkten H\u00f6hen", "tokens": ["Von", "ei\u00b7ni\u00b7gen", "ge\u00b7senk\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Schon sch\u00e4umend sich her\u00fcber go\u00df,", "tokens": ["Schon", "sch\u00e4u\u00b7mend", "sich", "her\u00b7\u00fc\u00b7ber", "go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und sich, in weissen Wasser-F\u00e4llen,", "tokens": ["Und", "sich", ",", "in", "weis\u00b7sen", "Was\u00b7ser\u00b7F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Schon zeigt auf unterschiednen Stellen,", "tokens": ["Schon", "zeigt", "auf", "un\u00b7ter\u00b7schied\u00b7nen", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wodurch bereits das ganze Land", "tokens": ["Wo\u00b7durch", "be\u00b7reits", "das", "gan\u00b7ze", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Voll Jammer, Angst und Kummer stand,", "tokens": ["Voll", "Jam\u00b7mer", ",", "Angst", "und", "Kum\u00b7mer", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und die entsetzliche Gefahr,", "tokens": ["Und", "die", "ent\u00b7setz\u00b7li\u00b7che", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die ihm fast unvermeidlich war,", "tokens": ["Die", "ihm", "fast", "un\u00b7ver\u00b7meid\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit nass- und starren Augen sah;", "tokens": ["Mit", "nass", "und", "star\u00b7ren", "Au\u00b7gen", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "War GOtt, Der Wind und Wellen lenket,", "tokens": ["War", "Gott", ",", "Der", "Wind", "und", "Wel\u00b7len", "len\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mit Seiner H\u00fclf\u2019 und Gnade da.", "tokens": ["Mit", "Sei\u00b7ner", "H\u00fcl\u00b7f'", "und", "Gna\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Er wollt\u2019: und augenblicklich senket", "tokens": ["Er", "wollt'", ":", "und", "au\u00b7gen\u00b7blick\u00b7lich", "sen\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die wilde Last der Fluhten sich von unsrer D\u00e4mme H\u00f6h\u2019", "tokens": ["Die", "wil\u00b7de", "Last", "der", "Fluh\u00b7ten", "sich", "von", "uns\u00b7rer", "D\u00e4m\u00b7me", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "hernieder,", "tokens": ["her\u00b7nie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Zu ihren hohlen Tiefen wieder.", "tokens": ["Zu", "ih\u00b7ren", "hoh\u00b7len", "Tie\u00b7fen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man konnte, mit erfreutem Aug\u2019, aus blauer Fluht die", "tokens": ["Man", "konn\u00b7te", ",", "mit", "er\u00b7freu\u00b7tem", "Aug'", ",", "aus", "blau\u00b7er", "Fluht", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gr\u00fcnen R\u00fccken", "tokens": ["gr\u00fc\u00b7nen", "R\u00fc\u00b7cken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Des, GOtt sey Lob! noch g\u00e4nzen Damms, und unzer-", "tokens": ["Des", ",", "Gott", "sey", "Lob", "!", "noch", "g\u00e4n\u00b7zen", "Damms", ",", "und", "un\u00b7zer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "NN", "VAFIN", "NN", "$.", "ADV", "ADJA", "NN", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "brochnen Deichs, erblicken.", "tokens": ["broch\u00b7nen", "Deichs", ",", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "So sey demnach, Du ew\u2019ge Lieb\u2019 und ewig weise Macht,", "tokens": ["So", "sey", "dem\u00b7nach", ",", "Du", "ew'\u00b7ge", "Lieb'", "und", "e\u00b7wig", "wei\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "$,", "PPER", "ADJA", "NN", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "gepriesen,", "tokens": ["ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da\u00df Du, zu dieses Landes Heil, Dich Selbst fast sicht-", "tokens": ["Da\u00df", "Du", ",", "zu", "die\u00b7ses", "Lan\u00b7des", "Heil", ",", "Dich", "Selbst", "fast", "sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PDAT", "NN", "NN", "$,", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "barlich gewiesen!", "tokens": ["bar\u00b7lich", "ge\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Du zu der ergrimmten Fluht ein gn\u00e4dig Macht-", "tokens": ["Der", "Du", "zu", "der", "er\u00b7grimm\u00b7ten", "Fluht", "ein", "gn\u00e4\u00b7dig", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wort ausgesprochen:", "tokens": ["Wort", "aus\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bis hieher komm, und weiter nicht! Hier sey dein", "tokens": ["Bis", "hie\u00b7her", "komm", ",", "und", "wei\u00b7ter", "nicht", "!", "Hier", "sey", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PAV", "VVFIN", "$,", "KON", "ADV", "PTKNEG", "$.", "ADV", "VAFIN", "PPOSAT"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "strenger Drang gebrochen!", "tokens": ["stren\u00b7ger", "Drang", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Hier soll dein fre\u00dfiges Verschlingen, hier soll dein st\u00fcr-", "tokens": ["Hier", "soll", "dein", "fre\u00b7\u00dfi\u00b7ges", "Ver\u00b7schlin\u00b7gen", ",", "hier", "soll", "dein", "st\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "misches Bewegen,", "tokens": ["mi\u00b7sches", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Der regen Wellen schwehre Macht, und w\u00fctende Ge-", "tokens": ["Der", "re\u00b7gen", "Wel\u00b7len", "schweh\u00b7re", "Macht", ",", "und", "w\u00fc\u00b7ten\u00b7de", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "walt sich legen!", "tokens": ["walt", "sich", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Gieb, da\u00df wir die\u00df, da\u00df es ein Wunder, ein wirklich", "tokens": ["Gieb", ",", "da\u00df", "wir", "die\u00df", ",", "da\u00df", "es", "ein", "Wun\u00b7der", ",", "ein", "wirk\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PDS", "$,", "KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJD"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Wunder, voller Segen,", "tokens": ["Wun\u00b7der", ",", "vol\u00b7ler", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Wodurch Du alles Unsrige aufs neu uns schenkst, be-", "tokens": ["Wo\u00b7durch", "Du", "al\u00b7les", "Uns\u00b7ri\u00b7ge", "aufs", "neu", "uns", "schenkst", ",", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN", "APPRART", "ADJD", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "trachten m\u00f6gen!", "tokens": ["trach\u00b7ten", "m\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "La\u00df uns in selbem Deine Lieb\u2019 und m\u00e4cht\u2019ge Weisheit", "tokens": ["La\u00df", "uns", "in", "sel\u00b7bem", "Dei\u00b7ne", "Lieb'", "und", "m\u00e4cht'\u00b7ge", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "oft ermessen!", "tokens": ["oft", "er\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "La\u00df unsern Dank, den wir anitzt, da noch die Ruhte", "tokens": ["La\u00df", "un\u00b7sern", "Dank", ",", "den", "wir", "a\u00b7nitzt", ",", "da", "noch", "die", "Ruh\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "nahe, zeigen,", "tokens": ["na\u00b7he", ",", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Nicht mit der Fluht von hinnen rauschen, nicht wie der", "tokens": ["Nicht", "mit", "der", "Fluht", "von", "hin\u00b7nen", "rau\u00b7schen", ",", "nicht", "wie", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "APPR", "ADV", "VVINF", "$,", "PTKNEG", "KOKOM", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "schnelle Strohm verseigen!", "tokens": ["schnel\u00b7le", "Strohm", "ver\u00b7sei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Gieb, da\u00df wir die so lange Folge, von einem kurzen Au-", "tokens": ["Gieb", ",", "da\u00df", "wir", "die", "so", "lan\u00b7ge", "Fol\u00b7ge", ",", "von", "ei\u00b7nem", "kur\u00b7zen", "Au"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ART", "ADV", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "genblick,", "tokens": ["gen\u00b7blick", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Voll Elend, Armuht, Kummer, Sorgen, und ungez\u00e4hlten", "tokens": ["Voll", "E\u00b7lend", ",", "Ar\u00b7muht", ",", "Kum\u00b7mer", ",", "Sor\u00b7gen", ",", "und", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Ungel\u00fcck,", "tokens": ["Un\u00b7ge\u00b7l\u00fcck", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "So uns gewi\u00df betroffen h\u00