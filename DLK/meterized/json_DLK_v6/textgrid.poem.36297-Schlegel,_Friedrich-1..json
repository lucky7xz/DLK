{"textgrid.poem.36297": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Apollo, wirst du diese Glut noch lindern?", "tokens": ["A\u00b7pol\u00b7lo", ",", "wirst", "du", "die\u00b7se", "Glut", "noch", "lin\u00b7dern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich selber sprang hinunter in die Tiefen,", "tokens": ["Ich", "sel\u00b7ber", "sprang", "hin\u00b7un\u00b7ter", "in", "die", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo heil'ge Musen s\u00fc\u00df und wild mich riefen;", "tokens": ["Wo", "heil'\u00b7ge", "Mu\u00b7sen", "s\u00fc\u00df", "und", "wild", "mich", "rie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon fleh' ich dir, die Freudenflut zu mindern.", "tokens": ["Schon", "fleh'", "ich", "dir", ",", "die", "Freu\u00b7den\u00b7flut", "zu", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Pfeil brennt stark, und willst du ihn nicht hindern,", "tokens": ["Dein", "Pfeil", "brennt", "stark", ",", "und", "willst", "du", "ihn", "nicht", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mu\u00df der Stirn die letzte Kraft enttriefen;", "tokens": ["So", "mu\u00df", "der", "Stirn", "die", "letz\u00b7te", "Kraft", "ent\u00b7trie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lieder, die im Haupt mir lange schliefen,", "tokens": ["Die", "Lie\u00b7der", ",", "die", "im", "Haupt", "mir", "lan\u00b7ge", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versinken auch mit andern Chaoskindern.", "tokens": ["Ver\u00b7sin\u00b7ken", "auch", "mit", "an\u00b7dern", "Cha\u00b7os\u00b7kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gib Heil! die treuste Treue soll dir lohnen.", "tokens": ["Gib", "Heil", "!", "die", "treus\u00b7te", "Treu\u00b7e", "soll", "dir", "loh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fchn hab ich in der eignen Brust gerungen,", "tokens": ["K\u00fchn", "hab", "ich", "in", "der", "eig\u00b7nen", "Brust", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon str\u00f6mt mir alle Kraft zum Ziel zusammen.", "tokens": ["Schon", "str\u00f6mt", "mir", "al\u00b7le", "Kraft", "zum", "Ziel", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allstarker! wolle jetzt nur freundlich schonen,", "tokens": ["A\u00b7llstar\u00b7ker", "!", "wol\u00b7le", "jetzt", "nur", "freund\u00b7lich", "scho\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hab' ich, was du mir enth\u00fcllt, gesungen,", "tokens": ["Und", "hab'", "ich", ",", "was", "du", "mir", "ent\u00b7h\u00fcllt", ",", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich verzehren schnell von deinen Flammen!", "tokens": ["La\u00df", "mich", "ver\u00b7zeh\u00b7ren", "schnell", "von", "dei\u00b7nen", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}