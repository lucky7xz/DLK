{"dta.poem.11740": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm, Brahma's J\u00fcnger, was ich vom Araber nahm;", "tokens": ["Nimm", ",", "Brah\u00b7ma's", "J\u00fcn\u00b7ger", ",", "was", "ich", "vom", "A\u00b7ra\u00b7ber", "nahm", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh auf den Kern, und \u00fcbersieh den Wortspielkram!", "tokens": ["Sieh", "auf", "den", "Kern", ",", "und", "\u00fc\u00b7ber\u00b7sieh", "den", "Wort\u00b7spiel\u00b7kram", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Bruder, o mein Sohn, ist auch der Muselman;", "tokens": ["Dein", "Bru\u00b7der", ",", "o", "mein", "Sohn", ",", "ist", "auch", "der", "Mu\u00b7sel\u00b7man", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Von ihm auch lerne gern, was er dich lehren kan.", "tokens": ["Von", "ihm", "auch", "ler\u00b7ne", "gern", ",", "was", "er", "dich", "leh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ADV", "$,", "PWS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Arabersprichwort sagt: dir hilft in der Gefahr", "tokens": ["A\u00b7ra\u00b7ber\u00b7sprich\u00b7wort", "sagt", ":", "dir", "hilft", "in", "der", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bruder oft, den nicht die Mutter dir gebar.", "tokens": ["Ein", "Bru\u00b7der", "oft", ",", "den", "nicht", "die", "Mut\u00b7ter", "dir", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PTKNEG", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verwandtschaft kann, mein Sohn, der Liebe nicht mit Ehren,", "tokens": ["Ver\u00b7wandt\u00b7schaft", "kann", ",", "mein", "Sohn", ",", "der", "Lie\u00b7be", "nicht", "mit", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der Verwandtschaft kann die Liebe wohl entbehren.", "tokens": ["Doch", "der", "Ver\u00b7wandt\u00b7schaft", "kann", "die", "Lie\u00b7be", "wohl", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer f\u00fcr mein Bestes sich mit Rath und That verwandt,", "tokens": ["Wer", "f\u00fcr", "mein", "Bes\u00b7tes", "sich", "mit", "Rath", "und", "That", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur der Verwandte ist mir in der That verwandt.", "tokens": ["Nur", "der", "Ver\u00b7wand\u00b7te", "ist", "mir", "in", "der", "That", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer f\u00fcr mein Bestes selbst hat Gut und Blut verwandt,", "tokens": ["Wer", "f\u00fcr", "mein", "Bes\u00b7tes", "selbst", "hat", "Gut", "und", "Blut", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie fremd er sei, der ist mir wahrhaft blutverwandt.", "tokens": ["Wie", "fremd", "er", "sei", ",", "der", "ist", "mir", "wahr\u00b7haft", "blut\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "PRELS", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht der so lieber selbst sein letztes Blut verwendet,", "tokens": ["Nicht", "der", "so", "lie\u00b7ber", "selbst", "sein", "letz\u00b7tes", "Blut", "ver\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Blutverwandten er ihr letztes Gut entwendet.", "tokens": ["Da\u00df", "Blut\u00b7ver\u00b7wand\u00b7ten", "er", "ihr", "letz\u00b7tes", "Gut", "ent\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der ist alswie ein Wolf, der nicht kann Blut entdecken", "tokens": ["Der", "ist", "als\u00b7wie", "ein", "Wolf", ",", "der", "nicht", "kann", "Blut", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "KOKOM", "ART", "NE", "$,", "PRELS", "PTKNEG", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am wunden Bruder, ohn' es gierig selbst zu lecken.", "tokens": ["Am", "wun\u00b7den", "Bru\u00b7der", ",", "ohn'", "es", "gie\u00b7rig", "selbst", "zu", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUI", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wer besser sei zum Feind zu haben als zum Freunde?", "tokens": ["Wer", "bes\u00b7ser", "sei", "zum", "Feind", "zu", "ha\u00b7ben", "als", "zum", "Freun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "APPRART", "NN", "PTKZU", "VAINF", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, scheulos vor dem Freund, sich nur vorm Feinde scheu'nde.", "tokens": ["Der", ",", "scheu\u00b7los", "vor", "dem", "Freund", ",", "sich", "nur", "vorm", "Fein\u00b7de", "scheu'n\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ART", "NN", "$,", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der dem Gewognen in den Weg tritt als Verwegner,", "tokens": ["Der", "dem", "Ge\u00b7wog\u00b7nen", "in", "den", "Weg", "tritt", "als", "Ver\u00b7weg\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Weg, wo ihm entgegen tritt ein Gegner.", "tokens": ["Und", "aus", "dem", "Weg", ",", "wo", "ihm", "ent\u00b7ge\u00b7gen", "tritt", "ein", "Geg\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der k\u00fchn den L\u00f6wen spielt in seinem Jagdreviere,", "tokens": ["Der", "k\u00fchn", "den", "L\u00f6\u00b7wen", "spielt", "in", "sei\u00b7nem", "Jagd\u00b7re\u00b7vie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schmeichlerisch den Fuchs im Kreis vornehmer Thiere.", "tokens": ["Und", "schmeich\u00b7le\u00b7risch", "den", "Fuchs", "im", "Kreis", "vor\u00b7neh\u00b7mer", "Thie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NE", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der st\u00e4rkst' in gutem Rath, zu guter That der schw\u00e4chste,", "tokens": ["Der", "st\u00e4rkst'", "in", "gu\u00b7tem", "Rath", ",", "zu", "gu\u00b7ter", "That", "der", "schw\u00e4chs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, wenn sein Nachbar ruft, sagt: ich bin mir der n\u00e4chste.", "tokens": ["Der", ",", "wenn", "sein", "Nach\u00b7bar", "ruft", ",", "sagt", ":", "ich", "bin", "mir", "der", "n\u00e4chs\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Ruft er den Nachbar einst, vergelt' ihm der die List,", "tokens": ["Ruft", "er", "den", "Nach\u00b7bar", "einst", ",", "ver\u00b7gelt'", "ihm", "der", "die", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "VVFIN", "PPER", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sage: hilf dir selbst, weil du dein N\u00e4chster bist.", "tokens": ["Und", "sa\u00b7ge", ":", "hilf", "dir", "selbst", ",", "weil", "du", "dein", "N\u00e4chs\u00b7ter", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVIMP", "PPER", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}