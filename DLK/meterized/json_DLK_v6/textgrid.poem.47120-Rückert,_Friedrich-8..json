{"textgrid.poem.47120": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Th\u00f6richt, wer im Paradies kann wohnen", "tokens": ["Th\u00f6\u00b7richt", ",", "wer", "im", "Pa\u00b7ra\u00b7dies", "kann", "woh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und will reisen gehn in andre Zonen.", "tokens": ["Und", "will", "rei\u00b7sen", "gehn", "in", "and\u00b7re", "Zo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Also th\u00f6richt ging ich j\u00fcngst von dir.", "tokens": ["Al\u00b7so", "th\u00f6\u00b7richt", "ging", "ich", "j\u00fcngst", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wollte sehn, ob au\u00dfer deiner Sph\u00e4re", "tokens": ["Woll\u00b7te", "sehn", ",", "ob", "au\u00b7\u00dfer", "dei\u00b7ner", "Sph\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Noch ein Wohnplatz mir auf Erden w\u00e4re;", "tokens": ["Noch", "ein", "Wohn\u00b7platz", "mir", "auf", "Er\u00b7den", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Keinen fand ich und bin wieder hier.", "tokens": ["Kei\u00b7nen", "fand", "ich", "und", "bin", "wie\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "KON", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Warum soll ich in der Irre schweifen,", "tokens": ["Wa\u00b7rum", "soll", "ich", "in", "der", "Ir\u00b7re", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sehn, wie andern ihre Fr\u00fcchte reifen,", "tokens": ["Sehn", ",", "wie", "an\u00b7dern", "ih\u00b7re", "Fr\u00fcch\u00b7te", "rei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fern der Au, wo meine Saaten stehn?", "tokens": ["Fern", "der", "Au", ",", "wo", "mei\u00b7ne", "Saa\u00b7ten", "stehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nimm dahin in Fesseln die Gedanken,", "tokens": ["Nimm", "da\u00b7hin", "in", "Fes\u00b7seln", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "La\u00df mich ruhn in deiner Arme Schranken,", "tokens": ["La\u00df", "mich", "ruhn", "in", "dei\u00b7ner", "Ar\u00b7me", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meine Welt in deinen Augen sehn!", "tokens": ["Mei\u00b7ne", "Welt", "in", "dei\u00b7nen", "Au\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}