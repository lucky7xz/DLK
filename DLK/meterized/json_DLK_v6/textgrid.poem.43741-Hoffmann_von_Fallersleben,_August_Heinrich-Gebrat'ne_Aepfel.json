{"textgrid.poem.43741": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gebrat'ne Aepfel", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebrat'ne Aepfel, s\u00fc\u00dfes Wort,", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", ",", "s\u00fc\u00b7\u00dfes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "O Wort voll Trost und Leben!", "tokens": ["O", "Wort", "voll", "Trost", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O m\u00f6cht's f\u00fcr mich doch immerfort", "tokens": ["O", "m\u00f6cht's", "f\u00fcr", "mich", "doch", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebrat'ne Aepfel geben!", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Des Abends sp\u00e4t, da denk' ich noch,", "tokens": ["Des", "A\u00b7bends", "sp\u00e4t", ",", "da", "denk'", "ich", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bevor ich mu\u00df zu Bette:", "tokens": ["Be\u00b7vor", "ich", "mu\u00df", "zu", "Bet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich gebrat'ne Aepfel doch,", "tokens": ["Wenn", "ich", "ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach, wenn ich sie doch h\u00e4tte!", "tokens": ["Ach", ",", "wenn", "ich", "sie", "doch", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Nachts da schlaf' ich was ich kann,", "tokens": ["Des", "Nachts", "da", "schlaf'", "ich", "was", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch tr\u00e4um' ich auch mitunter:", "tokens": ["Doch", "tr\u00e4um'", "ich", "auch", "mi\u00b7tun\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebrat'ne Aepfel machen dann", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", "ma\u00b7chen", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich oftmals wieder munter.", "tokens": ["Mich", "oft\u00b7mals", "wie\u00b7der", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Morgens, wenn ich werde wach,", "tokens": ["Des", "Mor\u00b7gens", ",", "wenn", "ich", "wer\u00b7de", "wach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist mein erstes Denken:", "tokens": ["So", "ist", "mein", "ers\u00b7tes", "Den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer wird gebrat'ne Aepfel, ach!", "tokens": ["Wer", "wird", "ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den heut'gen Tag mir schenken?", "tokens": ["Den", "heut'\u00b7gen", "Tag", "mir", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gebrat'ne Aepfel sind allein", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", "sind", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Sinnen, Dichten, Trachten.", "tokens": ["Mein", "Sin\u00b7nen", ",", "Dich\u00b7ten", ",", "Trach\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebrat'ne Aepfel, stellt euch ein!", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", ",", "stellt", "euch", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "La\u00dft mich nicht l\u00e4nger schmachten!", "tokens": ["La\u00dft", "mich", "nicht", "l\u00e4n\u00b7ger", "schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gebrat'ne Aepfel, s\u00fc\u00dfes Wort,", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", ",", "s\u00fc\u00b7\u00dfes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "O Wort voll Trost und Leben!", "tokens": ["O", "Wort", "voll", "Trost", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O m\u00f6cht's f\u00fcr mich doch immerfort", "tokens": ["O", "m\u00f6cht's", "f\u00fcr", "mich", "doch", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebrat'ne Aepfel geben!", "tokens": ["Ge\u00b7brat'\u00b7ne", "A\u00b7e\u00b7pfel", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}