{"textgrid.poem.65905": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Au", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Apfelb\u00e4ume bl\u00fchen sanft wie Wangen:", "tokens": ["Die", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "bl\u00fc\u00b7hen", "sanft", "wie", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Winde lachen Knaben vom Ge\u00e4st;", "tokens": ["Im", "Win\u00b7de", "la\u00b7chen", "Kna\u00b7ben", "vom", "Ge\u00b7\u00e4st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die kleinen Nackten suchen laut ein Nest", "tokens": ["Die", "klei\u00b7nen", "Nack\u00b7ten", "su\u00b7chen", "laut", "ein", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und legen Eilein unters Bl\u00fctenprangen.", "tokens": ["Und", "le\u00b7gen", "Ei\u00b7lein", "un\u00b7ters", "Bl\u00fc\u00b7ten\u00b7pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Behutsam gleicht, ihr Zweige, dem Verlangen", "tokens": ["Be\u00b7hut\u00b7sam", "gleicht", ",", "ihr", "Zwei\u00b7ge", ",", "dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Bl\u00e4ttersamt ums Blumen-Seidenfest;", "tokens": ["Nach", "Bl\u00e4t\u00b7ter\u00b7samt", "ums", "Blu\u00b7men\u00b7Sei\u00b7den\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr winkt, an euch in zarter Pracht gepre\u00dft,", "tokens": ["Ihr", "winkt", ",", "an", "euch", "in", "zar\u00b7ter", "Pracht", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den B\u00e4umchenherzlein in ergr\u00fcntem Bangen.", "tokens": ["Den", "B\u00e4um\u00b7chen\u00b7herz\u00b7lein", "in", "er\u00b7gr\u00fcn\u00b7tem", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bei schweigender Zypresse kennt das Kommen", "tokens": ["Bei", "schwei\u00b7gen\u00b7der", "Zyp\u00b7res\u00b7se", "kennt", "das", "Kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Erhorchter Tochter Demeter und lauscht", "tokens": ["Er\u00b7horch\u00b7ter", "Toch\u00b7ter", "De\u00b7me\u00b7ter", "und", "lauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Pochen ihres Herzens froh-beklommen:", "tokens": ["Dem", "Po\u00b7chen", "ih\u00b7res", "Her\u00b7zens", "froh\u00b7be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Oh, wenn sich Laub aus tausend \u00c4sten bauscht!", "tokens": ["Oh", ",", "wenn", "sich", "Laub", "aus", "tau\u00b7send", "\u00c4s\u00b7ten", "bauscht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PRF", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das goldne Knospenklimmen bleib verglommen,", "tokens": ["Das", "gold\u00b7ne", "Knos\u00b7pen\u00b7klim\u00b7men", "bleib", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Blau hat sich die laue Au berauscht!", "tokens": ["Am", "Blau", "hat", "sich", "die", "lau\u00b7e", "Au", "be\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PRF", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}