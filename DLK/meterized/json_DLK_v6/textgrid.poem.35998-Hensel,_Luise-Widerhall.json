{"textgrid.poem.35998": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Widerhall", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist es hier so sch\u00f6n und lind,", "tokens": ["Wie", "ist", "es", "hier", "so", "sch\u00f6n", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So herbstlich bunt und k\u00fchl!", "tokens": ["So", "herbst\u00b7lich", "bunt", "und", "k\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier spielt ich einst, ein zartes Kind,", "tokens": ["Hier", "spielt", "ich", "einst", ",", "ein", "zar\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Manch liebes, s\u00fc\u00dfes Spiel.", "tokens": ["Manch", "lie\u00b7bes", ",", "s\u00fc\u00b7\u00dfes", "Spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da war ich harmlos noch und froh \u2013 oh!", "tokens": ["Da", "war", "ich", "harm\u00b7los", "noch", "und", "froh", "\u2013", "oh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "KON", "ADJD", "$(", "FM", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier war ein Garten, bunt und fein,", "tokens": ["Hier", "war", "ein", "Gar\u00b7ten", ",", "bunt", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier pflegt' ich R\u00f6slein rot,", "tokens": ["Hier", "pflegt'", "ich", "R\u00f6s\u00b7lein", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, sind denn all die Rosen mein", "tokens": ["Ach", ",", "sind", "denn", "all", "die", "Ro\u00b7sen", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "PIAT", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwelkt und bleich und tot?", "tokens": ["Ver\u00b7welkt", "und", "bleich", "und", "tot", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja, sag es mir nun, lieber Widerhall \u2013 all!", "tokens": ["Ja", ",", "sag", "es", "mir", "nun", ",", "lie\u00b7ber", "Wi\u00b7der\u00b7hall", "\u2013", "all", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADV", "NN", "$(", "PIAT", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Ja, meine Blumenlust ist hin,", "tokens": ["Ja", ",", "mei\u00b7ne", "Blu\u00b7men\u00b7lust", "ist", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Garten \u00f6d und leer:", "tokens": ["Mein", "Gar\u00b7ten", "\u00f6d", "und", "leer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie mu\u00dften all mit ihm verbl\u00fchn,", "tokens": ["Sie", "mu\u00df\u00b7ten", "all", "mit", "ihm", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist gar kein Bl\u00fcmlein mehr.", "tokens": ["Ist", "gar", "kein", "Bl\u00fcm\u00b7lein", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O sag, was mir hienieden blieben? \u2013 lieben!", "tokens": ["O", "sag", ",", "was", "mir", "hien\u00b7ie\u00b7den", "blie\u00b7ben", "?", "\u2013", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$.", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, wenn ich nicht den Lieben seh',", "tokens": ["Ach", ",", "wenn", "ich", "nicht", "den", "Lie\u00b7ben", "seh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Liebe doch nur Leid.", "tokens": ["Ist", "Lie\u00b7be", "doch", "nur", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz vergeht im Sehnsuchtweh,", "tokens": ["Mein", "Herz", "ver\u00b7geht", "im", "Sehn\u00b7sucht\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stirbt hin in Traurigkeit.", "tokens": ["Stirbt", "hin", "in", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O weh! \u2013 Was h\u00fcll' ich um dies wunde Herz? \u2013 Erz!", "tokens": ["O", "weh", "!", "\u2013", "Was", "h\u00fcll'", "ich", "um", "dies", "wun\u00b7de", "Herz", "?", "\u2013", "Erz", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "$(", "PWS", "VVFIN", "PPER", "APPR", "PDS", "ADJA", "NN", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Und liegt es schwer wie kalter Stahl", "tokens": ["Und", "liegt", "es", "schwer", "wie", "kal\u00b7ter", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um meine warme Brust,", "tokens": ["Um", "mei\u00b7ne", "war\u00b7me", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmt es nimmer Schmerz und Qual", "tokens": ["Und", "nimmt", "es", "nim\u00b7mer", "Schmerz", "und", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmer Freud und Lust,", "tokens": ["Und", "nim\u00b7mer", "Freud", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wen kann ich dann an meinen Busen ziehn? \u2013 Ihn!", "tokens": ["Wen", "kann", "ich", "dann", "an", "mei\u00b7nen", "Bu\u00b7sen", "ziehn", "?", "\u2013", "Ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$(", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O du mein stolzer, milder Held,", "tokens": ["O", "du", "mein", "stol\u00b7zer", ",", "mil\u00b7der", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Stern und Licht warst du!", "tokens": ["Mein", "Stern", "und", "Licht", "warst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun find ich auf der ganzen Welt", "tokens": ["Nun", "find", "ich", "auf", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nimmer Trost und Ruh.", "tokens": ["Doch", "nim\u00b7mer", "Trost", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohin nun wend' ich meine Sinnen? \u2013 Innen!", "tokens": ["Wo\u00b7hin", "nun", "wend'", "ich", "mei\u00b7ne", "Sin\u00b7nen", "?", "\u2013", "In\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wird nun Liebe Leben mein", "tokens": ["Wird", "nun", "Lie\u00b7be", "Le\u00b7ben", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im innersten Gem\u00fct,", "tokens": ["Im", "in\u00b7ners\u00b7ten", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und wird Musik mein tiefstes Sein,", "tokens": ["Und", "wird", "Mu\u00b7sik", "mein", "tiefs\u00b7tes", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leid ein stilles Lied,", "tokens": ["Mein", "Leid", "ein", "stil\u00b7les", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Greift nicht die Welt in meine Harmonie? \u2013 Nie!", "tokens": ["Greift", "nicht", "die", "Welt", "in", "mei\u00b7ne", "Har\u00b7mo\u00b7nie", "?", "\u2013", "Nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$(", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "Mein treues Lieben la\u00df ich nicht,", "tokens": ["Mein", "treu\u00b7es", "Lie\u00b7ben", "la\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "VVIMP", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leid, mein liebes Weh,", "tokens": ["Mein", "Leid", ",", "mein", "lie\u00b7bes", "Weh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn mein Herz in Tr\u00e4nen bricht,", "tokens": ["Und", "wenn", "mein", "Herz", "in", "Tr\u00e4\u00b7nen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und einsam ich vergeh. \u2013", "tokens": ["Und", "ein\u00b7sam", "ich", "ver\u00b7geh", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft ist mir noch, als w\u00fcrd' er wieder wach \u2013 Ach!", "tokens": ["Oft", "ist", "mir", "noch", ",", "als", "w\u00fcrd'", "er", "wie\u00b7der", "wach", "\u2013", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "ADJD", "$(", "ITJ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}