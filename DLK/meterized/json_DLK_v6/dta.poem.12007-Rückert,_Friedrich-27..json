{"dta.poem.12007": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O ew'ger Lebenshauch, durch den der Baum der Zeiten", "tokens": ["O", "ew'\u00b7ger", "Le\u00b7bens\u00b7hauch", ",", "durch", "den", "der", "Baum", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "ART", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Treibt Bl\u00fcten, Fr\u00fcchte tr\u00e4gt und falbes Laub l\u00e4\u00dft gleiten.", "tokens": ["Treibt", "Bl\u00fc\u00b7ten", ",", "Fr\u00fcch\u00b7te", "tr\u00e4gt", "und", "fal\u00b7bes", "Laub", "l\u00e4\u00dft", "glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was stockt und was sich regt, regt sich und stockt in dir;", "tokens": ["Was", "stockt", "und", "was", "sich", "regt", ",", "regt", "sich", "und", "stockt", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "PRF", "VVFIN", "$,", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedes Herz das schl\u00e4gt, schl\u00e4gt und frolockt in dir.", "tokens": ["Und", "je\u00b7des", "Herz", "das", "schl\u00e4gt", ",", "schl\u00e4gt", "und", "fro\u00b7lockt", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PDS", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du hebst den Menschengeist in deiner Lieb' empor,", "tokens": ["Du", "hebst", "den", "Men\u00b7schen\u00b7geist", "in", "dei\u00b7ner", "Lieb'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er f\u00fchlet sich in dir, und kommt so gro\u00df sich vor.", "tokens": ["Er", "f\u00fch\u00b7let", "sich", "in", "dir", ",", "und", "kommt", "so", "gro\u00df", "sich", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "KON", "VVFIN", "ADV", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann f\u00fchlt er sich so klein vor deiner Gr\u00f6\u00dfe wieder,", "tokens": ["Dann", "f\u00fchlt", "er", "sich", "so", "klein", "vor", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tiefe Demut beugt den k\u00fchnen Stolz danieder.", "tokens": ["Und", "tie\u00b7fe", "De\u00b7mut", "beugt", "den", "k\u00fch\u00b7nen", "Stolz", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du aber \u00f6ffnest dem gebeugten deinen Scho\u00df,", "tokens": ["Du", "a\u00b7ber", "\u00f6ff\u00b7nest", "dem", "ge\u00b7beug\u00b7ten", "dei\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhebst ihn wieder, und der kleine gilt dir gro\u00df.", "tokens": ["Er\u00b7hebst", "ihn", "wie\u00b7der", ",", "und", "der", "klei\u00b7ne", "gilt", "dir", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ART", "ADJA", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du kehrest in ihm ein mit dem Gef\u00fchl der Huld,", "tokens": ["Du", "keh\u00b7rest", "in", "ihm", "ein", "mit", "dem", "Ge\u00b7f\u00fchl", "der", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Sehnen stillest du und s\u00fchnest seine Schuld.", "tokens": ["Sein", "Seh\u00b7nen", "stil\u00b7lest", "du", "und", "s\u00fch\u00b7nest", "sei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit Zittern sieht er dich als Herren, der ihn schuf,", "tokens": ["Mit", "Zit\u00b7tern", "sieht", "er", "dich", "als", "Her\u00b7ren", ",", "der", "ihn", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "KOUS", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit Vertrauen h\u00f6rt er deinen Vaterruf.", "tokens": ["Und", "mit", "Ver\u00b7trau\u00b7en", "h\u00f6rt", "er", "dei\u00b7nen", "Va\u00b7ter\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}