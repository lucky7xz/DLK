{"textgrid.poem.57614": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "An die Ungezeugten", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch, welche noch zum Leben nicht", "tokens": ["Euch", ",", "wel\u00b7che", "noch", "zum", "Le\u00b7ben", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Samen hat geweckt,", "tokens": ["Der", "Sa\u00b7men", "hat", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch, die noch vor dem grellen Licht", "tokens": ["Euch", ",", "die", "noch", "vor", "dem", "grel\u00b7len", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mutterleib bedeckt,", "tokens": ["Der", "Mut\u00b7ter\u00b7leib", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch gilt unser Schreiben und Streiten,", "tokens": ["Euch", "gilt", "un\u00b7ser", "Schrei\u00b7ben", "und", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Wege euch vorzubereiten.", "tokens": ["Die", "We\u00b7ge", "euch", "vor\u00b7zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Wir schwingen mit Begeisterung", "tokens": ["Wir", "schwin\u00b7gen", "mit", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das scharfe Wahrheitsbeil", "tokens": ["Das", "schar\u00b7fe", "Wahr\u00b7heits\u00b7beil"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hacken, roden Stamm und Strunk", "tokens": ["Und", "ha\u00b7cken", ",", "ro\u00b7den", "Stamm", "und", "Strunk"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Urwald \u00bbVorurteil\u00ab,", "tokens": ["Vom", "Ur\u00b7wald", "\u00bb", "Vor\u00b7ur\u00b7teil", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Damit ihr freie Wege wandelt", "tokens": ["Da\u00b7mit", "ihr", "frei\u00b7e", "We\u00b7ge", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ohne Hindernis handelt.", "tokens": ["Und", "oh\u00b7ne", "Hin\u00b7der\u00b7nis", "han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir fasten und wir frieren gern", "tokens": ["Wir", "fas\u00b7ten", "und", "wir", "frie\u00b7ren", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Schmach und Schmutz gebannt,", "tokens": ["In", "Schmach", "und", "Schmutz", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns st\u00e4hlt ein heller Hoffnungsstern", "tokens": ["Uns", "st\u00e4hlt", "ein", "hel\u00b7ler", "Hoff\u00b7nungs\u00b7stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die arbeitsschwarze Hand,", "tokens": ["Die", "ar\u00b7beits\u00b7schwar\u00b7ze", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was liegt an uns Verlornen,", "tokens": ["Was", "liegt", "an", "uns", "Ver\u00b7lor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es leben die Ungebornen!", "tokens": ["Es", "le\u00b7ben", "die", "Un\u00b7ge\u00b7bor\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein freies, sch\u00f6nes Menschengeschlecht,", "tokens": ["Ein", "frei\u00b7es", ",", "sch\u00f6\u00b7nes", "Men\u00b7schen\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gott\u00e4hnlich sollt ihr sein,", "tokens": ["Got\u00b7t\u00e4hn\u00b7lich", "sollt", "ihr", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein einigstarkes Liebesrecht", "tokens": ["Ein", "ei\u00b7nigs\u00b7tar\u00b7kes", "Lie\u00b7bes\u00b7recht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird euer K\u00f6nig sein \u2013", "tokens": ["Wird", "eu\u00b7er", "K\u00f6\u00b7nig", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Amen und diesen Glauben", "tokens": ["A\u00b7men", "und", "die\u00b7sen", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "PDAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Soll uns kein Knechtmensch rauben.", "tokens": ["Soll", "uns", "kein", "Knecht\u00b7mensch", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}