{"textgrid.poem.58696": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "8. Endlicher Sieg", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trotz aller Feinde Wut, sie werden endlich m\u00fcssen", "tokens": ["Trotz", "al\u00b7ler", "Fein\u00b7de", "Wut", ",", "sie", "wer\u00b7den", "end\u00b7lich", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "PPER", "VAFIN", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich beugen auch in mir zu meines K\u00f6nigs F\u00fc\u00dfen!", "tokens": ["Sich", "beu\u00b7gen", "auch", "in", "mir", "zu", "mei\u00b7nes", "K\u00f6\u00b7nigs", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wartet auf dem Thron, doch lehrt sein Warten mir,", "tokens": ["Er", "war\u00b7tet", "auf", "dem", "Thron", ",", "doch", "lehrt", "sein", "War\u00b7ten", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Glauben und Geduld auch darauf warten hier.", "tokens": ["In", "Glau\u00b7ben", "und", "Ge\u00b7duld", "auch", "da\u00b7rauf", "war\u00b7ten", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PAV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}