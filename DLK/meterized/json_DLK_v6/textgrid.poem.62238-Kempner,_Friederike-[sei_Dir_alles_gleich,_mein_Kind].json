{"textgrid.poem.62238": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[sei Dir alles gleich, mein Kind]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei Dir alles gleich, mein Kind,", "tokens": ["Sei", "Dir", "al\u00b7les", "gleich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S' ist ja alles gleich;", "tokens": ["S'", "ist", "ja", "al\u00b7les", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jedes Ding vergeht geschwind", "tokens": ["Je\u00b7des", "Ding", "ver\u00b7geht", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem fl\u00fcchtigen Reich.", "tokens": ["In", "dem", "fl\u00fcch\u00b7ti\u00b7gen", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Freud' und Leiden, Schmerz und Gl\u00fcck,", "tokens": ["Freud'", "und", "Lei\u00b7den", ",", "Schmerz", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eis und Schnee und Sonnenblick,", "tokens": ["Eis", "und", "Schnee", "und", "Son\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles trifft ja ein Geschick,", "tokens": ["Al\u00b7les", "trifft", "ja", "ein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt auf eins zur\u00fcck. \u2013", "tokens": ["Kommt", "auf", "eins", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PIS", "PTKVZ", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Inn'res Gl\u00fcck nur wechselt nie,", "tokens": ["Inn'\u00b7res", "Gl\u00fcck", "nur", "wech\u00b7selt", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Bewu\u00dftsein bleibt,", "tokens": ["Das", "Be\u00b7wu\u00df\u00b7tsein", "bleibt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ewig gleich der Poesie", "tokens": ["E\u00b7wig", "gleich", "der", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Seine Bl\u00fcten treibt.", "tokens": ["Sei\u00b7ne", "Bl\u00fc\u00b7ten", "treibt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}