{"textgrid.poem.62115": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[o ist's denn ganz unm\u00f6glich]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ist's denn ganz unm\u00f6glich,", "tokens": ["O", "ist's", "denn", "ganz", "un\u00b7m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2013 Was doch nicht ganz uns\u00e4glich \u2013", "tokens": ["\u2013", "Was", "doch", "nicht", "ganz", "un\u00b7s\u00e4g\u00b7lich", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df alles gl\u00fccklich w\u00e4r'?", "tokens": ["Da\u00df", "al\u00b7les", "gl\u00fcck\u00b7lich", "w\u00e4r'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O, wenn's doch m\u00f6glich w\u00e4r'!", "tokens": ["O", ",", "wenn's", "doch", "m\u00f6g\u00b7lich", "w\u00e4r'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}