{"dta.poem.544": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das unbest\u00e4ndige Frauen-Zimmer.  \n Aus dem Jtali\u00e4nischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn schon der Himmel lacht/ und Nereus Fluhten schertzen/", "tokens": ["Wenn", "schon", "der", "Him\u00b7mel", "lacht", "/", "und", "Ne\u00b7reus", "Fluh\u00b7ten", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So st\u00fcrmmt es bald daselbst/ wo jetzt die Stille prangt:", "tokens": ["So", "st\u00fcrmmt", "es", "bald", "da\u00b7selbst", "/", "wo", "jetzt", "die", "Stil\u00b7le", "prangt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$(", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die Best\u00e4ndigkeit hat in der Weiber Hertzen/", "tokens": ["Und", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "in", "der", "Wei\u00b7ber", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Ort der Wanckelmuth noch keinen Sitz erlangt.", "tokens": ["Den", "Ort", "der", "Wan\u00b7ckel\u00b7muth", "noch", "kei\u00b7nen", "Sitz", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}