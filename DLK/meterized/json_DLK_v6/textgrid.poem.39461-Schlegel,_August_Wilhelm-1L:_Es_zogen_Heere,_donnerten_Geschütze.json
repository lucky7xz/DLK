{"textgrid.poem.39461": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es zogen Heere, donnerten Gesch\u00fctze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zogen Heere, donnerten Gesch\u00fctze", "tokens": ["Es", "zo\u00b7gen", "Hee\u00b7re", ",", "don\u00b7ner\u00b7ten", "Ge\u00b7sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fernab, die starke Veste zu best\u00fcrmen,", "tokens": ["Fern\u00b7ab", ",", "die", "star\u00b7ke", "Ves\u00b7te", "zu", "be\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich ihr Strom mit Leichen mu\u00dfte th\u00fcrmen;", "tokens": ["Da\u00df", "sich", "ihr", "Strom", "mit", "Lei\u00b7chen", "mu\u00df\u00b7te", "th\u00fcr\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sommernacht erhellten Kriegesblitze.", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "er\u00b7hell\u00b7ten", "Krie\u00b7ges\u00b7blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da wu\u00dft', im engen Thal, auf schatt'gem Sitze,", "tokens": ["Da", "wu\u00dft'", ",", "im", "en\u00b7gen", "Thal", ",", "auf", "schatt'\u00b7gem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor allen andern als der Freude St\u00fcrmen", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "als", "der", "Freu\u00b7de", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PIS", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich und die Liebste Liebe zu beschirmen,", "tokens": ["Mich", "und", "die", "Liebs\u00b7te", "Lie\u00b7be", "zu", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vereinigt wie die Reb' und ihre St\u00fctze.", "tokens": ["Ver\u00b7ei\u00b7nigt", "wie", "die", "Reb'", "und", "ih\u00b7re", "St\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die\u00df deutet frohe Zukunft unserm Bunde.", "tokens": ["Die\u00df", "deu\u00b7tet", "fro\u00b7he", "Zu\u00b7kunft", "un\u00b7serm", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sollt' er nicht, von freundlichen Gestirnen", "tokens": ["Wie", "sollt'", "er", "nicht", ",", "von", "freund\u00b7li\u00b7chen", "Ge\u00b7stir\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst im Gewitter angelacht, bestehen?", "tokens": ["Selbst", "im", "Ge\u00b7wit\u00b7ter", "an\u00b7ge\u00b7lacht", ",", "be\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch donnert, uns auch, der Verheerung Stunde,", "tokens": ["Doch", "don\u00b7nert", ",", "uns", "auch", ",", "der", "Ver\u00b7hee\u00b7rung", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So la\u00df uns, achtlos um der G\u00f6tter Z\u00fcrnen,", "tokens": ["So", "la\u00df", "uns", ",", "acht\u00b7los", "um", "der", "G\u00f6t\u00b7ter", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Schoo\u00df der Liebe selig untergehen.", "tokens": ["Im", "Schoo\u00df", "der", "Lie\u00b7be", "se\u00b7lig", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}