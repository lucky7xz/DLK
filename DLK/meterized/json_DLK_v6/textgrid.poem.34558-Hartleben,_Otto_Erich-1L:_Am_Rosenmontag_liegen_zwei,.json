{"textgrid.poem.34558": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Rosenmontag liegen zwei,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Rosenmontag liegen zwei,", "tokens": ["Am", "Ro\u00b7sen\u00b7mon\u00b7tag", "lie\u00b7gen", "zwei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die kalten H\u00e4nde noch verschlungen \u2013", "tokens": ["die", "kal\u00b7ten", "H\u00e4n\u00b7de", "noch", "ver\u00b7schlun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "das Leben str\u00f6mte rauh vorbei,", "tokens": ["das", "Le\u00b7ben", "str\u00f6m\u00b7te", "rauh", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die beiden haben's nicht bezwungen.", "tokens": ["die", "bei\u00b7den", "ha\u00b7ben's", "nicht", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als \u00fcberwunden gr\u00fcssen sie", "tokens": ["Als", "\u00fc\u00b7berw\u00b7un\u00b7den", "gr\u00fcs\u00b7sen", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den Sieger, dem das Gl\u00fcck begegnet \u2013", "tokens": ["den", "Sie\u00b7ger", ",", "dem", "das", "Gl\u00fcck", "be\u00b7geg\u00b7net", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "im Tod verbunden, segnen sie", "tokens": ["im", "Tod", "ver\u00b7bun\u00b7den", ",", "seg\u00b7nen", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "all jene, die das Leben segnet.", "tokens": ["all", "je\u00b7ne", ",", "die", "das", "Le\u00b7ben", "seg\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}