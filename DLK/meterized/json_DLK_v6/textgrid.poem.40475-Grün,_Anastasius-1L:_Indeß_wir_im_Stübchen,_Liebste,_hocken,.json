{"textgrid.poem.40475": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Inde\u00df wir im St\u00fcbchen, Liebste, hocken,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Inde\u00df wir im St\u00fcbchen, Liebste, hocken,", "tokens": ["In\u00b7de\u00df", "wir", "im", "St\u00fcb\u00b7chen", ",", "Liebs\u00b7te", ",", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und vor den windger\u00fcttelten Scheiben", "tokens": ["Und", "vor", "den", "wind\u00b7ge\u00b7r\u00fct\u00b7tel\u00b7ten", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Winters wei\u00dfe, schwere Flocken,", "tokens": ["Des", "Win\u00b7ters", "wei\u00b7\u00dfe", ",", "schwe\u00b7re", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sturme wirbelnd, vor\u00fcbertreiben:", "tokens": ["Im", "Stur\u00b7me", "wir\u00b7belnd", ",", "vor\u00b7\u00fc\u00b7bert\u00b7rei\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wird jenes Wanderv\u00f6glein, das freie,", "tokens": ["Wird", "je\u00b7nes", "Wan\u00b7der\u00b7v\u00f6\u00b7glein", ",", "das", "frei\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das du im Sommer gepflegt mit Kosen,", "tokens": ["Das", "du", "im", "Som\u00b7mer", "ge\u00b7pflegt", "mit", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sich sonnen in S\u00fcdens Himmelsbl\u00e4ue", "tokens": ["Sich", "son\u00b7nen", "in", "S\u00fc\u00b7dens", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wiegen sich \u00fcber S\u00fcdens Rosen.", "tokens": ["Und", "wie\u00b7gen", "sich", "\u00fc\u00b7ber", "S\u00fc\u00b7dens", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "NE", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf gr\u00fcnende Myrten wird sich's schwingen,", "tokens": ["Auf", "gr\u00fc\u00b7nen\u00b7de", "Myr\u00b7ten", "wird", "sich's", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Abends vom Zweig im Mondenscheine", "tokens": ["Und", "A\u00b7bends", "vom", "Zweig", "im", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Lieder von seinen Fahrten singen", "tokens": ["Die", "Lie\u00b7der", "von", "sei\u00b7nen", "Fahr\u00b7ten", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der horchenden fremden Schwestergemeine.", "tokens": ["Der", "hor\u00b7chen\u00b7den", "frem\u00b7den", "Schwes\u00b7ter\u00b7ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbweit \u00fcber dem Meer, am Donaustrande,", "tokens": ["\u00bb", "weit", "\u00fc\u00b7ber", "dem", "Meer", ",", "am", "Do\u00b7naus\u00b7tran\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dort steht ein H\u00e4uschen, ein niedliches, blankes,", "tokens": ["Dort", "steht", "ein", "H\u00e4usc\u00b7hen", ",", "ein", "nied\u00b7li\u00b7ches", ",", "blan\u00b7kes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und aus dem H\u00e4uschen, am Fensterrande,", "tokens": ["Und", "aus", "dem", "H\u00e4usc\u00b7hen", ",", "am", "Fens\u00b7ter\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Winkt mir ein M\u00e4dchen, ein liebliches, schlankes.", "tokens": ["Winkt", "mir", "ein", "M\u00e4d\u00b7chen", ",", "ein", "lieb\u00b7li\u00b7ches", ",", "schlan\u00b7kes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn auf ihren Arm ich dann fliege,", "tokens": ["Und", "wenn", "auf", "ih\u00b7ren", "Arm", "ich", "dann", "flie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will fast mich des Nordens Schnee erschrecken,", "tokens": ["Will", "fast", "mich", "des", "Nor\u00b7dens", "Schnee", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als ob auf silbernem Baum ich mich wiege,", "tokens": ["Als", "ob", "auf", "sil\u00b7ber\u00b7nem", "Baum", "ich", "mich", "wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Draus f\u00fcnf der silbernen Zweige sich strecken.", "tokens": ["Draus", "f\u00fcnf", "der", "sil\u00b7ber\u00b7nen", "Zwei\u00b7ge", "sich", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf ihren Schultern am Lockenbuge,", "tokens": ["Auf", "ih\u00b7ren", "Schul\u00b7tern", "am", "Lo\u00b7cken\u00b7bu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da fehlte nicht viel, da\u00df Stolz mich ber\u00fcckte,", "tokens": ["Da", "fehl\u00b7te", "nicht", "viel", ",", "da\u00df", "Stolz", "mich", "be\u00b7r\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da meint' ich der Adler zu sein, der im Fluge", "tokens": ["Da", "meint'", "ich", "der", "Ad\u00b7ler", "zu", "sein", ",", "der", "im", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VAINF", "$,", "PRELS", "APPRART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im Sonnenstrahlennetz sich verstrickte!", "tokens": ["Im", "Son\u00b7nen\u00b7strah\u00b7len\u00b7netz", "sich", "ver\u00b7strick\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und wenn aus der hohlen Hand zum Mahle", "tokens": ["Und", "wenn", "aus", "der", "hoh\u00b7len", "Hand", "zum", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der frische kristallene Born mir quillet,", "tokens": ["Der", "fri\u00b7sche", "kris\u00b7tal\u00b7le\u00b7ne", "Born", "mir", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schl\u00fcrf' ich aus alabasterner Schale,", "tokens": ["Da", "schl\u00fcr\u00b7f'", "ich", "aus", "a\u00b7la\u00b7bas\u00b7ter\u00b7ner", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie sie dem Sultan der Sklave f\u00fcllet.", "tokens": ["Wie", "sie", "dem", "Sul\u00b7tan", "der", "Skla\u00b7ve", "f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Und wenn das K\u00f6rnlein in ihren Lippen,", "tokens": ["Und", "wenn", "das", "K\u00f6rn\u00b7lein", "in", "ih\u00b7ren", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein t\u00e4glich Brod, mir entgegen blickte,", "tokens": ["Mein", "t\u00e4g\u00b7lich", "Brod", ",", "mir", "ent\u00b7ge\u00b7gen", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da meint' ich Purpurkirschen zu nippen,", "tokens": ["Da", "meint'", "ich", "Pur\u00b7pur\u00b7kir\u00b7schen", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als ich den k\u00f6stlichen Kern daraus pickte.", "tokens": ["Als", "ich", "den", "k\u00f6st\u00b7li\u00b7chen", "Kern", "da\u00b7raus", "pick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und Solches ist wohl in jenen Landen", "tokens": ["Und", "Sol\u00b7ches", "ist", "wohl", "in", "je\u00b7nen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die s\u00fc\u00dfeste Speise, das Mahl der Freude;", "tokens": ["Die", "s\u00fc\u00b7\u00dfes\u00b7te", "Spei\u00b7se", ",", "das", "Mahl", "der", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Denn Einer, der oft daneben gestanden,", "tokens": ["Denn", "Ei\u00b7ner", ",", "der", "oft", "da\u00b7ne\u00b7ben", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sah mein Picken immer mit Neide.\u00ab", "tokens": ["Der", "sah", "mein", "Pi\u00b7cken", "im\u00b7mer", "mit", "Nei\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "So wird dein Preis jetzt im S\u00fcden klingen!", "tokens": ["So", "wird", "dein", "Preis", "jetzt", "im", "S\u00fc\u00b7den", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heil mir, dem solche Liebste zu eigen,", "tokens": ["Heil", "mir", ",", "dem", "sol\u00b7che", "Liebs\u00b7te", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von der die V\u00f6gel in Afrika singen", "tokens": ["Von", "der", "die", "V\u00f6\u00b7gel", "in", "Af\u00b7ri\u00b7ka", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "NE", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und in Europa die Nachbarn schweigen!", "tokens": ["Und", "in", "Eu\u00b7ro\u00b7pa", "die", "Nach\u00b7barn", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}