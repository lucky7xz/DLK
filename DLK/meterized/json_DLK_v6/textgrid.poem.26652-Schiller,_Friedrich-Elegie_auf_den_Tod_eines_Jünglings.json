{"textgrid.poem.26652": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Elegie auf den Tod eines J\u00fcnglings", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Banges St\u00f6hnen, wie vorm nahen Sturme,", "tokens": ["Ban\u00b7ges", "St\u00f6h\u00b7nen", ",", "wie", "vorm", "na\u00b7hen", "Stur\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hallet her vom \u00f6den Trauerhaus,", "tokens": ["Hal\u00b7let", "her", "vom", "\u00f6\u00b7den", "Trau\u00b7er\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Totent\u00f6ne fallen von des M\u00fcnsters Turme,", "tokens": ["To\u00b7ten\u00b7t\u00f6\u00b7ne", "fal\u00b7len", "von", "des", "M\u00fcns\u00b7ters", "Tur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Einen J\u00fcngling tr\u00e4gt man hier heraus:", "tokens": ["Ei\u00b7nen", "J\u00fcng\u00b7ling", "tr\u00e4gt", "man", "hier", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Einen J\u00fcngling \u2013 noch nicht reif zum Sarge,", "tokens": ["Ei\u00b7nen", "J\u00fcng\u00b7ling", "\u2013", "noch", "nicht", "reif", "zum", "Sar\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In des Lebens Mai gepfl\u00fcckt,", "tokens": ["In", "des", "Le\u00b7bens", "Mai", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Pochend mit der Jugend Nervenmarke,", "tokens": ["Po\u00b7chend", "mit", "der", "Ju\u00b7gend", "Ner\u00b7ven\u00b7mar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mit der Flamme, die im Auge z\u00fcckt;", "tokens": ["Mit", "der", "Flam\u00b7me", ",", "die", "im", "Au\u00b7ge", "z\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Einen Sohn, die Wonne seiner Mutter", "tokens": ["Ei\u00b7nen", "Sohn", ",", "die", "Won\u00b7ne", "sei\u00b7ner", "Mut\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "(o das lehrt ihr jammernd Ach),", "tokens": ["(", "o", "das", "lehrt", "ihr", "jam\u00b7mernd", "Ach", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PDS", "VVFIN", "PPER", "ADJD", "NN", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Busenfreund, ach! meinen Bruder \u2013", "tokens": ["Mei\u00b7nen", "Bu\u00b7sen\u00b7freund", ",", "ach", "!", "mei\u00b7nen", "Bru\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$.", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf! was Mensch hei\u00dft, folge nach!", "tokens": ["Auf", "!", "was", "Mensch", "hei\u00dft", ",", "fol\u00b7ge", "nach", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "PWS", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Prahlt ihr Fichten, die ihr hoch veraltet", "tokens": ["Prahlt", "ihr", "Fich\u00b7ten", ",", "die", "ihr", "hoch", "ver\u00b7al\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "St\u00fcrmen stehet und den Donner neckt?", "tokens": ["St\u00fcr\u00b7men", "ste\u00b7het", "und", "den", "Don\u00b7ner", "neckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ihr Berge, die ihr Himmel haltet,", "tokens": ["Und", "ihr", "Ber\u00b7ge", ",", "die", "ihr", "Him\u00b7mel", "hal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ihr Himmel, die ihr Sonnen hegt?", "tokens": ["Und", "ihr", "Him\u00b7mel", ",", "die", "ihr", "Son\u00b7nen", "hegt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Prahlt der Greis noch, der auf stolzen Werken", "tokens": ["Prahlt", "der", "Greis", "noch", ",", "der", "auf", "stol\u00b7zen", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie auf Wogen zur Vollendung steigt?", "tokens": ["Wie", "auf", "Wo\u00b7gen", "zur", "Vol\u00b7len\u00b7dung", "steigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Prahlt der Held noch, der auf aufgew\u00e4lzten Tatenbergen", "tokens": ["Prahlt", "der", "Held", "noch", ",", "der", "auf", "auf\u00b7ge\u00b7w\u00e4lz\u00b7ten", "Ta\u00b7ten\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "In des Nachruhms Sonnentempel fleugt?", "tokens": ["In", "des", "Nach\u00b7ruhms", "Son\u00b7nen\u00b7tem\u00b7pel", "fleugt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wenn der Wurm schon naget in den Bl\u00fcten:", "tokens": ["Wenn", "der", "Wurm", "schon", "na\u00b7get", "in", "den", "Bl\u00fc\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wer ist Tor, zu w\u00e4hnen, da\u00df er nie verdirbt?", "tokens": ["Wer", "ist", "Tor", ",", "zu", "w\u00e4h\u00b7nen", ",", "da\u00df", "er", "nie", "ver\u00b7dirbt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "$,", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Wer dort oben hofft noch und hienieden", "tokens": ["Wer", "dort", "o\u00b7ben", "hofft", "noch", "und", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Auszudauren \u2013 wenn der J\u00fcngling stirbt?", "tokens": ["Aus\u00b7zu\u00b7dau\u00b7ren", "\u2013", "wenn", "der", "J\u00fcng\u00b7ling", "stirbt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Lieblich h\u00fcpften, voll der Jugendfreude,", "tokens": ["Lieb\u00b7lich", "h\u00fcpf\u00b7ten", ",", "voll", "der", "Ju\u00b7gend\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seine Tage hin im Rosenkleide,", "tokens": ["Sei\u00b7ne", "Ta\u00b7ge", "hin", "im", "Ro\u00b7sen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Welt, die Welt war ihm so s\u00fc\u00df \u2013", "tokens": ["Und", "die", "Welt", ",", "die", "Welt", "war", "ihm", "so", "s\u00fc\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und so freundlich, so bezaubernd winkte", "tokens": ["Und", "so", "freund\u00b7lich", ",", "so", "be\u00b7zau\u00b7bernd", "wink\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihm die Zukunft, und so golden blinkte", "tokens": ["Ihm", "die", "Zu\u00b7kunft", ",", "und", "so", "gol\u00b7den", "blink\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihm des Lebens Paradies;", "tokens": ["Ihm", "des", "Le\u00b7bens", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch, als schon das Mutterauge tr\u00e4nte,", "tokens": ["Noch", ",", "als", "schon", "das", "Mut\u00b7ter\u00b7au\u00b7ge", "tr\u00e4n\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unter ihm das Totenreich schon g\u00e4hnte,", "tokens": ["Un\u00b7ter", "ihm", "das", "To\u00b7ten\u00b7reich", "schon", "g\u00e4hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "\u00dcber ihm der Parzen Faden ri\u00df,", "tokens": ["\u00dc\u00b7ber", "ihm", "der", "Par\u00b7zen", "Fa\u00b7den", "ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Erd und Himmel seinem Blick entsanken,", "tokens": ["Erd", "und", "Him\u00b7mel", "sei\u00b7nem", "Blick", "ent\u00b7san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Floh er \u00e4ngstlich vor dem Grabgedanken \u2013", "tokens": ["Floh", "er", "\u00e4ngst\u00b7lich", "vor", "dem", "Grab\u00b7ge\u00b7dan\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ach, die Welt ist Sterbenden so s\u00fc\u00df.", "tokens": ["Ach", ",", "die", "Welt", "ist", "Ster\u00b7ben\u00b7den", "so", "s\u00fc\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Stumm und taub ists in dem engen Hause,", "tokens": ["Stumm", "und", "taub", "ists", "in", "dem", "en\u00b7gen", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tief der Schlummer der Begrabenen;", "tokens": ["Tief", "der", "Schlum\u00b7mer", "der", "Be\u00b7gra\u00b7be\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Bruder! ach, in ewig tiefer Pause", "tokens": ["Bru\u00b7der", "!", "ach", ",", "in", "e\u00b7wig", "tie\u00b7fer", "Pau\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "XY", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Feiern alle deine Hoffnungen;", "tokens": ["Fei\u00b7ern", "al\u00b7le", "dei\u00b7ne", "Hoff\u00b7nun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Oft erw\u00e4rmt die Sonne deinen H\u00fcgel,", "tokens": ["Oft", "er\u00b7w\u00e4rmt", "die", "Son\u00b7ne", "dei\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihre Glut empfindest du nicht mehr;", "tokens": ["Ih\u00b7re", "Glut", "emp\u00b7fin\u00b7dest", "du", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Seine Blumen wiegt des Westwinds Fl\u00fcgel,", "tokens": ["Sei\u00b7ne", "Blu\u00b7men", "wiegt", "des", "West\u00b7winds", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sein Gelispel h\u00f6rest du nicht mehr;", "tokens": ["Sein", "Ge\u00b7lis\u00b7pel", "h\u00f6\u00b7rest", "du", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Liebe wird dein Auge nie vergolden,", "tokens": ["Lie\u00b7be", "wird", "dein", "Au\u00b7ge", "nie", "ver\u00b7gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Nie umhalsen deine Braut wirst du,", "tokens": ["Nie", "um\u00b7hal\u00b7sen", "dei\u00b7ne", "Braut", "wirst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Nie, wenn unsre Tr\u00e4nen stromweis rollten, \u2013", "tokens": ["Nie", ",", "wenn", "uns\u00b7re", "Tr\u00e4\u00b7nen", "strom\u00b7weis", "roll\u00b7ten", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ewig, ewig sinkt dein Auge zu.", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "sinkt", "dein", "Au\u00b7ge", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber wohl dir! \u2013 k\u00f6stlich ist dein Schlummer,", "tokens": ["A\u00b7ber", "wohl", "dir", "!", "\u2013", "k\u00f6st\u00b7lich", "ist", "dein", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$.", "$(", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ru