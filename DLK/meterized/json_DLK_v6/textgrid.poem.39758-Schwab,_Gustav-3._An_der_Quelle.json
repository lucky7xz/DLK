{"textgrid.poem.39758": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3. An der Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich werfe nieder mich am Bach,", "tokens": ["Ich", "wer\u00b7fe", "nie\u00b7der", "mich", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wird so jung zu Sinne.", "tokens": ["Mir", "wird", "so", "jung", "zu", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In seine Wellen schau' ich \u2013 ach!", "tokens": ["In", "sei\u00b7ne", "Wel\u00b7len", "schau'", "ich", "\u2013", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$(", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was werd' ich Armer inne?", "tokens": ["Was", "werd'", "ich", "Ar\u00b7mer", "in\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es blickt mir statt dem Lockenkopf", "tokens": ["Es", "blickt", "mir", "statt", "dem", "Lo\u00b7cken\u00b7kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entgegen ein fast grauer Schopf,", "tokens": ["Ent\u00b7ge\u00b7gen", "ein", "fast", "grau\u00b7er", "Schopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Augen \u00fcberbauen", "tokens": ["Die", "Au\u00b7gen", "\u00fc\u00b7berb\u00b7au\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir wei\u00dfbebuschte Brauen.", "tokens": ["Mir", "wei\u00df\u00b7be\u00b7buschte", "Brau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sink' immerhin veraltet ein,", "tokens": ["Sink'", "im\u00b7mer\u00b7hin", "ver\u00b7al\u00b7tet", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du halb schon trockne H\u00fclle!", "tokens": ["Du", "halb", "schon", "trock\u00b7ne", "H\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kann nur mein Geist noch J\u00fcngling sein,", "tokens": ["Kann", "nur", "mein", "Geist", "noch", "J\u00fcng\u00b7ling", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er nur Saft und F\u00fclle!", "tokens": ["Hat", "er", "nur", "Saft", "und", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es wandeln viel, gelockt und glatt,", "tokens": ["Es", "wan\u00b7deln", "viel", ",", "ge\u00b7lockt", "und", "glatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um mich herum, und sind schon matt", "tokens": ["Um", "mich", "he\u00b7rum", ",", "und", "sind", "schon", "matt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APZR", "$,", "KON", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meinen halben Jahren,", "tokens": ["Mit", "mei\u00b7nen", "hal\u00b7ben", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind Greise trotz den Haaren.", "tokens": ["Sind", "Grei\u00b7se", "trotz", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Werd' mir nicht m\u00fcrrisch, alt Gesicht!", "tokens": ["Werd'", "mir", "nicht", "m\u00fcr\u00b7risch", ",", "alt", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht wolkicht, kahle Stirne!", "tokens": ["Nicht", "wol\u00b7kicht", ",", "kah\u00b7le", "Stir\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die einz'ge Jugend nicht,", "tokens": ["Das", "ist", "die", "einz'\u00b7ge", "Ju\u00b7gend", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach welcher schielt die Dirne.", "tokens": ["Nach", "wel\u00b7cher", "schielt", "die", "Dir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jung bleibt, wem in der argen Welt", "tokens": ["Jung", "bleibt", ",", "wem", "in", "der", "ar\u00b7gen", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gemeines nie den Mut verg\u00e4llt,", "tokens": ["Ge\u00b7mei\u00b7nes", "nie", "den", "Mut", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer noch f\u00fcr's H\u00f6chste Sehnen,", "tokens": ["Wer", "noch", "f\u00fcr's", "H\u00f6chs\u00b7te", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr edles Leid hat Thr\u00e4nen.", "tokens": ["F\u00fcr", "ed\u00b7les", "Leid", "hat", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch schwillt, du halbgeschlossner Mund,", "tokens": ["Noch", "schwillt", ",", "du", "halb\u00b7ge\u00b7schloss\u00b7ner", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lied auf deinen Lippen,", "tokens": ["Das", "Lied", "auf", "dei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch leerst du Becher noch zu Grund", "tokens": ["Auch", "leerst", "du", "Be\u00b7cher", "noch", "zu", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00dfest nichts vom Nippen.", "tokens": ["Und", "wei\u00b7\u00dfest", "nichts", "vom", "Nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du, Brust, auch bist noch weit und warm", "tokens": ["Du", ",", "Brust", ",", "auch", "bist", "noch", "weit", "und", "warm"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "ADV", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du selbst bist nicht welk, mein Arm!", "tokens": ["Und", "du", "selbst", "bist", "nicht", "welk", ",", "mein", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin ein Mann und strebe,", "tokens": ["Ich", "bin", "ein", "Mann", "und", "stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich f\u00fchl's mit Lust: ich lebe!", "tokens": ["Ich", "f\u00fchl's", "mit", "Lust", ":", "ich", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn die bessre Zeit noch tagt,", "tokens": ["Und", "wenn", "die", "bess\u00b7re", "Zeit", "noch", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang ich wandl' auf Erden,", "tokens": ["So", "lang", "ich", "wandl'", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeit, von der man singt und sagt,", "tokens": ["Die", "Zeit", ",", "von", "der", "man", "singt", "und", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Angst- und Lustgeberden:", "tokens": ["Mit", "Angst", "und", "Lust\u00b7ge\u00b7ber\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie findet mich im Silberhaar,", "tokens": ["Sie", "fin\u00b7det", "mich", "im", "Sil\u00b7ber\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nicht der Dichterjugend baar;", "tokens": ["Doch", "nicht", "der", "Dich\u00b7ter\u00b7ju\u00b7gend", "baar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wird mein Sang verk\u00fcnden,", "tokens": ["Dann", "wird", "mein", "Sang", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was J\u00fcngste soll entz\u00fcnden.", "tokens": ["Was", "J\u00fcngs\u00b7te", "soll", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Pl\u00e4tscherbach! verspotte nicht", "tokens": ["O", "Pl\u00e4t\u00b7scher\u00b7bach", "!", "ver\u00b7spot\u00b7te", "nicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich und mein Lied verwegen;", "tokens": ["Mich", "und", "mein", "Lied", "ver\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O frischer Rasen, gr\u00fcnes Licht!", "tokens": ["O", "fri\u00b7scher", "Ra\u00b7sen", ",", "gr\u00fc\u00b7nes", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schiel' mir nicht so entgegen.", "tokens": ["Schiel'", "mir", "nicht", "so", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ach freilich, wenn der goldne Tag", "tokens": ["Ach", "frei\u00b7lich", ",", "wenn", "der", "gold\u00b7ne", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Anbricht nach Sturm und Donnerschlag,", "tokens": ["An\u00b7bricht", "nach", "Sturm", "und", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist diese S\u00e4ngerkehle", "tokens": ["Ist", "die\u00b7se", "S\u00e4n\u00b7ger\u00b7keh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zerst\u00e4ubt, und fern die Seele.", "tokens": ["Zer\u00b7st\u00e4ubt", ",", "und", "fern", "die", "See\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}