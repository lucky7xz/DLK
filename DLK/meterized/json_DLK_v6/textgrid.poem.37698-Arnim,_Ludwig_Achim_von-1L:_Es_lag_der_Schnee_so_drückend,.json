{"textgrid.poem.37698": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lag der Schnee so dr\u00fcckend,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lag der Schnee so dr\u00fcckend,", "tokens": ["Es", "lag", "der", "Schnee", "so", "dr\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df meine Laube brach,", "tokens": ["Da\u00df", "mei\u00b7ne", "Lau\u00b7be", "brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Fr\u00fchling kam entz\u00fcckend,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kam", "ent\u00b7z\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht ihr ein buntes Dach;", "tokens": ["Macht", "ihr", "ein", "bun\u00b7tes", "Dach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles noch werden.", "tokens": ["Es", "wird", "mir", "al\u00b7les", "noch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Schwalbe kam geflogen", "tokens": ["Die", "Schwal\u00b7be", "kam", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und flog in meine Hand,", "tokens": ["Und", "flog", "in", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O sprich, wer mir gewogen,", "tokens": ["O", "sprich", ",", "wer", "mir", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer dich mir zugesandt;", "tokens": ["Wer", "dich", "mir", "zu\u00b7ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles noch werden.", "tokens": ["Es", "wird", "mir", "al\u00b7les", "noch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und darfst du es nicht sagen,", "tokens": ["Und", "darfst", "du", "es", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich la\u00df dich dennoch frei,", "tokens": ["Ich", "la\u00df", "dich", "den\u00b7noch", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du magst der Jungfrau klagen,", "tokens": ["Du", "magst", "der", "Jung\u00b7frau", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich nicht bei ihr sei;", "tokens": ["Da\u00df", "ich", "nicht", "bei", "ihr", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles noch werden.", "tokens": ["Es", "wird", "mir", "al\u00b7les", "noch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und sag' ihr, wie ich pflanze", "tokens": ["Und", "sag'", "ihr", ",", "wie", "ich", "pflan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wie die Saat hier gr\u00fcnt,", "tokens": ["Und", "wie", "die", "Saat", "hier", "gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df ich wohl zum Tanze", "tokens": ["Und", "da\u00df", "ich", "wohl", "zum", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir ihre Hand verdient;", "tokens": ["Mir", "ih\u00b7re", "Hand", "ver\u00b7dient", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles noch werden.", "tokens": ["Es", "wird", "mir", "al\u00b7les", "noch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und bring' die Blumen alle", "tokens": ["Und", "bring'", "die", "Blu\u00b7men", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu ihrer freien Wahl,", "tokens": ["Zu", "ih\u00b7rer", "frei\u00b7en", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich ihr gefalle,", "tokens": ["Und", "wenn", "ich", "ihr", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So tr\u00e4gt sie die im Saal;", "tokens": ["So", "tr\u00e4gt", "sie", "die", "im", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles noch werden.", "tokens": ["Es", "wird", "mir", "al\u00b7les", "noch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und tr\u00e4gt sie die am Herzen,", "tokens": ["Und", "tr\u00e4gt", "sie", "die", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So soll's ein Zeichen sein,", "tokens": ["So", "soll's", "ein", "Zei\u00b7chen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nach Tanz und Scherzen", "tokens": ["Da\u00df", "ich", "nach", "Tanz", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihr darf gehen ein;", "tokens": ["Zu", "ihr", "darf", "ge\u00b7hen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles noch werden.", "tokens": ["Es", "wird", "mir", "al\u00b7les", "noch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Bienlein summen Lehren", "tokens": ["Die", "Bien\u00b7lein", "sum\u00b7men", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und warnen uns davor,", "tokens": ["Und", "war\u00b7nen", "uns", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schw\u00f6r's, du bleibst bei Ehren,", "tokens": ["Ich", "schw\u00f6r's", ",", "du", "bleibst", "bei", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin ein frommer Thor;", "tokens": ["Ich", "bin", "ein", "from\u00b7mer", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird mir alles ", "tokens": ["Es", "wird", "mir", "al\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}}}}}