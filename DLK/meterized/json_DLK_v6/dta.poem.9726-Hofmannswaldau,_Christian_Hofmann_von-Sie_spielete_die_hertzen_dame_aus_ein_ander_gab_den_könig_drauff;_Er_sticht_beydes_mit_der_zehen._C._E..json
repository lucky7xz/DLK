{"dta.poem.9726": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie spielete die hertzen dame aus/ ein ander  \n gab den k\u00f6nig drauff; Er sticht beydes  \n mit der zehen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Spiel auff dein eines hertz; es soll mir nicht entrinnen/", "tokens": ["Spiel", "auff", "dein", "ei\u00b7nes", "hertz", ";", "es", "soll", "mir", "nicht", "ent\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schl\u00fcg ein ander gleich den k\u00f6nig selber drein:", "tokens": ["Und", "schl\u00fcg", "ein", "an\u00b7der", "gleich", "den", "k\u00f6\u00b7nig", "sel\u00b7ber", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein hertz ist nur zu schwach; zehn hertzen m\u00fcssens seyn/", "tokens": ["Ein", "hertz", "ist", "nur", "zu", "schwach", ";", "zehn", "hert\u00b7zen", "m\u00fcs\u00b7sens", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$.", "CARD", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wofern man anders will dein eines hertz gewinnen.", "tokens": ["Wo\u00b7fern", "man", "an\u00b7ders", "will", "dein", "ei\u00b7nes", "hertz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "PPOSAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}