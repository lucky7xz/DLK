{"dta.poem.20770": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Recept zur St\u00e4rkungschokolade .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Provisor mengt geschwinde                 ", "tokens": ["Der", "Pro\u00b7vi\u00b7sor", "mengt", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch zwo Theilchen Teufelsmist,", "tokens": ["Euch", "zwo", "Theil\u00b7chen", "Teu\u00b7fels\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Theilchen Chinarinde,", "tokens": ["Und", "ein", "Theil\u00b7chen", "Chi\u00b7na\u00b7rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fein wie dies Gemengsel ist,", "tokens": ["Fein", "wie", "dies", "Ge\u00b7men\u00b7gsel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PDS", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier im P\u00fclverchen zu schauen;", "tokens": ["Hier", "im", "P\u00fcl\u00b7ver\u00b7chen", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fchmenswerth ist seine Kraft,", "tokens": ["R\u00fch\u00b7mens\u00b7werth", "ist", "sei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weils den M\u00e4nnern und den Frauen", "tokens": ["Weils", "den", "M\u00e4n\u00b7nern", "und", "den", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder junge Kr\u00e4fte schafft.", "tokens": ["Wie\u00b7der", "jun\u00b7ge", "Kr\u00e4f\u00b7te", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In ein Sch\u00e4lchen Chokolade", "tokens": ["In", "ein", "Sch\u00e4l\u00b7chen", "Cho\u00b7ko\u00b7la\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehmt ihr einer Erbse gro\u00df,", "tokens": ["Nehmt", "ihr", "ei\u00b7ner", "Erb\u00b7se", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es dem Geschmack nicht schade.", "tokens": ["Da\u00df", "es", "dem", "Ge\u00b7schmack", "nicht", "scha\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Delicat und tadello\u00df", "tokens": ["De\u00b7li\u00b7cat", "und", "ta\u00b7de\u00b7llo\u00df"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmeckt euch diese Chokolade,", "tokens": ["Schmeckt", "euch", "die\u00b7se", "Cho\u00b7ko\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00e4rkt euch Nerven und Gebein,", "tokens": ["St\u00e4rkt", "euch", "Ner\u00b7ven", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ihr schon am Styxgestade", "tokens": ["Wenn", "ihr", "schon", "am", "Styx\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schatten w\u00e4hnt zu seyn.", "tokens": ["Schat\u00b7ten", "w\u00e4hnt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}