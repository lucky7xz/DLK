{"textgrid.poem.41673": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herbstende, Winter ihr, Fr\u00fchlinge reich an Regen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbstende, Winter ihr, Fr\u00fchlinge reich an Regen,", "tokens": ["Herbs\u00b7ten\u00b7de", ",", "Win\u00b7ter", "ihr", ",", "Fr\u00fch\u00b7lin\u00b7ge", "reich", "an", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "$,", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--++-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Euch Schlummerzeiten sehnt die Seele sich entgegen,", "tokens": ["Euch", "Schlum\u00b7mer\u00b7zei\u00b7ten", "sehnt", "die", "See\u00b7le", "sich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wie ein weites Grab ihr Herz und Hirn umgebt", "tokens": ["Die", "wie", "ein", "wei\u00b7tes", "Grab", "ihr", "Herz", "und", "Hirn", "um\u00b7gebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Nebel, der mich wie ein Leichentuch umwebt.", "tokens": ["Im", "Ne\u00b7bel", ",", "der", "mich", "wie", "ein", "Lei\u00b7chen\u00b7tuch", "um\u00b7webt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In weiter Ebne, die die kalten Winde fegen,", "tokens": ["In", "wei\u00b7ter", "Eb\u00b7ne", ",", "die", "die", "kal\u00b7ten", "Win\u00b7de", "fe\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Wetterfahnen in der Nacht sich kreischend regen,", "tokens": ["Wo", "Wet\u00b7ter\u00b7fah\u00b7nen", "in", "der", "Nacht", "sich", "krei\u00b7schend", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Spannt meine Seele, die kein warmer Lenz belebt,", "tokens": ["Spannt", "mei\u00b7ne", "See\u00b7le", ",", "die", "kein", "war\u00b7mer", "Lenz", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Den Rabenfittich, der sie d\u00fcstren Flugs erhebt.", "tokens": ["Den", "Ra\u00b7ben\u00b7fit\u00b7tich", ",", "der", "sie", "d\u00fcst\u00b7ren", "Flugs", "er\u00b7hebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nichts kann so s\u00fc\u00df sein f\u00fcr ein Herz, das gramzerrissen,", "tokens": ["Nichts", "kann", "so", "s\u00fc\u00df", "sein", "f\u00fcr", "ein", "Herz", ",", "das", "gram\u00b7zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VAINF", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf das seit langem schon der Frost herniederf\u00e4llt,", "tokens": ["Auf", "das", "seit", "lan\u00b7gem", "schon", "der", "Frost", "her\u00b7nie\u00b7der\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADJA", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O bleiche Himmel, ihr Gebieter unsrer Welt,", "tokens": ["O", "blei\u00b7che", "Him\u00b7mel", ",", "ihr", "Ge\u00b7bie\u00b7ter", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als stets zu sehn das Graun von fahlen Finsternissen,", "tokens": ["Als", "stets", "zu", "sehn", "das", "Graun", "von", "fah\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKZU", "VVINF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nicht den bittren Schmerz auf unsrem Bett wir sacht", "tokens": ["Wenn", "nicht", "den", "bit\u00b7tren", "Schmerz", "auf", "uns\u00b7rem", "Bett", "wir", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einschl\u00e4fern Brust an Brust in mondesleerer Nacht.", "tokens": ["Ein\u00b7schl\u00e4\u00b7fern", "Brust", "an", "Brust", "in", "mon\u00b7des\u00b7lee\u00b7rer", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}