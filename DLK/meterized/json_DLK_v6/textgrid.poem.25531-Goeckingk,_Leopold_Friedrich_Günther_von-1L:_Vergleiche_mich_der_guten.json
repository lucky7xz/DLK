{"textgrid.poem.25531": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergleiche mich der guten ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergleiche mich der guten ", "tokens": ["Ver\u00b7glei\u00b7che", "mich", "der", "gu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie singt aus Noth, ich aber sing' aus Liebe,", "tokens": ["Sie", "singt", "aus", "Noth", ",", "ich", "a\u00b7ber", "sing'", "aus", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Singt, bis die Welt von ihr bewundernd spricht,", "tokens": ["Singt", ",", "bis", "die", "Welt", "von", "ihr", "be\u00b7wun\u00b7dernd", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da ich der Welt so gern verborgen bliebe.", "tokens": ["Da", "ich", "der", "Welt", "so", "gern", "ver\u00b7bor\u00b7gen", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie dr\u00e4nget sich zu kargen F\u00fcrsten hin,", "tokens": ["Sie", "dr\u00e4n\u00b7get", "sich", "zu", "kar\u00b7gen", "F\u00fcrs\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vergi\u00dft den Stolz, der gro\u00dfen Seelen ziemet,", "tokens": ["Ver\u00b7gi\u00dft", "den", "Stolz", ",", "der", "gro\u00b7\u00dfen", "See\u00b7len", "zie\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Inde\u00df ich klein, so eigensinnig bin,", "tokens": ["In\u00b7de\u00df", "ich", "klein", ",", "so", "ei\u00b7gen\u00b7sin\u00b7nig", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df mein Gesang, nur dich, nicht F\u00fcrsten r\u00fchmet.", "tokens": ["Da\u00df", "mein", "Ge\u00b7sang", ",", "nur", "dich", ",", "nicht", "F\u00fcrs\u00b7ten", "r\u00fch\u00b7met", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "PPER", "$,", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vergleiche mich der ", "tokens": ["Ver\u00b7glei\u00b7che", "mich", "der"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Zwar neid' ich ihr die z\u00e4rtlichen Ges\u00e4nge,", "tokens": ["Zwar", "neid'", "ich", "ihr", "die", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Doch wenn der Mund von Sch\u00e4ferliebe spricht,", "tokens": ["Doch", "wenn", "der", "Mund", "von", "Sch\u00e4\u00b7fer\u00b7lie\u00b7be", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Was f\u00fchlt das Herz im gro\u00dfen Weltgedr\u00e4nge?", "tokens": ["Was", "f\u00fchlt", "das", "Herz", "im", "gro\u00b7\u00dfen", "Welt\u00b7ge\u00b7dr\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Vergleiche mich der gro\u00dfen ", "tokens": ["Ver\u00b7glei\u00b7che", "mich", "der", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sie sang, wie ich, zwar Liebesmelodien,", "tokens": ["Sie", "sang", ",", "wie", "ich", ",", "zwar", "Lie\u00b7bes\u00b7me\u00b7lo\u00b7di\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch von dem Kranz', der ihre Stirn' umflicht,", "tokens": ["Doch", "von", "dem", "Kranz'", ",", "der", "ih\u00b7re", "Stirn'", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verdient kein Rei\u00df in ", "tokens": ["Ver\u00b7di\u00b7ent", "kein", "Rei\u00df", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PIAT", "NN", "APPR"], "meter": "+---+-", "measure": "dactylic.init"}, "line.17": {"text": "Doch, w\u00fcrdest du so hart wie ", "tokens": ["Doch", ",", "w\u00fcr\u00b7dest", "du", "so", "hart", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "ADJD", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und wolltest scheu vor meinem Blick entweichen,", "tokens": ["Und", "woll\u00b7test", "scheu", "vor", "mei\u00b7nem", "Blick", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Dann kannst du mich der armen ", "tokens": ["Dann", "kannst", "du", "mich", "der", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Doch, Amarant! im Schicksal' nur vergleichen.", "tokens": ["Doch", ",", "A\u00b7ma\u00b7rant", "!", "im", "Schick\u00b7sal'", "nur", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$.", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}