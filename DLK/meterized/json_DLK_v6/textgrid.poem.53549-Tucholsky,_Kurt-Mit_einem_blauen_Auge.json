{"textgrid.poem.53549": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Mit einem blauen Auge", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alten Kegelbr\u00fcder seh ich wieder.", "tokens": ["Die", "al\u00b7ten", "Ke\u00b7gel\u00b7br\u00fc\u00b7der", "seh", "ich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie \u00fcberlebten selbst des Krieges Lauf.", "tokens": ["Sie", "\u00fc\u00b7ber\u00b7leb\u00b7ten", "selbst", "des", "Krie\u00b7ges", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schicken revolution\u00e4re Lieder", "tokens": ["Sie", "schi\u00b7cken", "re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7n\u00e4\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "gleich Taubenschw\u00e4rmen in das Blau hinauf.", "tokens": ["gleich", "Tau\u00b7ben\u00b7schw\u00e4r\u00b7men", "in", "das", "Blau", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und locken sie zur\u00fcck:", "tokens": ["Und", "lo\u00b7cken", "sie", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "nun h\u00e4ngt ein Wenn und Aber im Gefieder", "tokens": ["nun", "h\u00e4ngt", "ein", "Wenn", "und", "A\u00b7ber", "im", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "KOUS", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u2013 ein Gl\u00fcck! Ein Gl\u00fcck!", "tokens": ["\u2013", "ein", "Gl\u00fcck", "!", "Ein", "Gl\u00fcck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das Land im Elend. Wer ist schuld am ganzen?", "tokens": ["Das", "Land", "im", "E\u00b7lend", ".", "Wer", "ist", "schuld", "am", "gan\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "PWS", "VAFIN", "ADJD", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Krieg, und da\u00df man ihn so schwer verlor?", "tokens": ["Am", "Krieg", ",", "und", "da\u00df", "man", "ihn", "so", "schwer", "ver\u00b7lor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "KOUS", "PIS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sieht die Wackern zierlich eiertanzen.", "tokens": ["Man", "sieht", "die", "Wa\u00b7ckern", "zier\u00b7lich", "ei\u00b7er\u00b7tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie schreiten voller Schwung drei Schritte vor", "tokens": ["Sie", "schrei\u00b7ten", "vol\u00b7ler", "Schwung", "drei", "Schrit\u00b7te", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "CARD", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und drei zur\u00fcck.", "tokens": ["und", "drei", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Man braucht ja doch die blanken S\u00f6ldnerlanzen", "tokens": ["Man", "braucht", "ja", "doch", "die", "blan\u00b7ken", "S\u00f6ld\u00b7ner\u00b7lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u2013 welch Gl\u00fcck! Welch Gl\u00fcck!", "tokens": ["\u2013", "welch", "Gl\u00fcck", "!", "Welch", "Gl\u00fcck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Domestik liebt seine Offiziere.", "tokens": ["Der", "Do\u00b7mes\u00b7tik", "liebt", "sei\u00b7ne", "Of\u00b7fi\u00b7zie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gute, brave, liebe Ludendorff!", "tokens": ["Der", "gu\u00b7te", ",", "bra\u00b7ve", ",", "lie\u00b7be", "Lu\u00b7den\u00b7dorff", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das w\u00fctete vier Jahre wie die Stiere.", "tokens": ["Das", "w\u00fc\u00b7te\u00b7te", "vier", "Jah\u00b7re", "wie", "die", "Stie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "CARD", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rei\u00dft einer auf den alten Wundenschorf?", "tokens": ["Rei\u00dft", "ei\u00b7ner", "auf", "den", "al\u00b7ten", "Wun\u00b7den\u00b7schorf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sanft holt man ihn zur\u00fcck \u2013", "tokens": ["Sanft", "holt", "man", "ihn", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und bleibt h\u00fcbsch ma\u00dfvoll bei dem Stammtischbiere", "tokens": ["und", "bleibt", "h\u00fcbsch", "ma\u00df\u00b7voll", "bei", "dem", "Stamm\u00b7tischbie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "\u2013 sein Gl\u00fcck! Sein Gl\u00fcck!", "tokens": ["\u2013", "sein", "Gl\u00fcck", "!", "Sein", "Gl\u00fcck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du bunte Bestie mit den tausend Armen!", "tokens": ["Du", "bun\u00b7te", "Be\u00b7stie", "mit", "den", "tau\u00b7send", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "CARD", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "W\u00e4r dieses Volk politisch stark und reif:", "tokens": ["W\u00e4r", "die\u00b7ses", "Volk", "po\u00b7li\u00b7tisch", "stark", "und", "reif", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "es ri\u00df die Fenster auf im stubenwarmen", "tokens": ["es", "ri\u00df", "die", "Fens\u00b7ter", "auf", "im", "stu\u00b7ben\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gemach \u2013 Luft! Luft! und Fr\u00fchjahrsreif!", "tokens": ["Ge\u00b7mach", "\u2013", "Luft", "!", "Luft", "!", "und", "Fr\u00fch\u00b7jahrs\u00b7reif", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$.", "NN", "$.", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du kehrtest nie zur\u00fcck.", "tokens": ["Du", "kehr\u00b7test", "nie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und keiner h\u00e4tte mit dir Vieh Erbarmen", "tokens": ["Und", "kei\u00b7ner", "h\u00e4t\u00b7te", "mit", "dir", "Vieh", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u2013 dein Gl\u00fcck! Dein Gl\u00fcck!", "tokens": ["\u2013", "dein", "Gl\u00fcck", "!", "Dein", "Gl\u00fcck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}