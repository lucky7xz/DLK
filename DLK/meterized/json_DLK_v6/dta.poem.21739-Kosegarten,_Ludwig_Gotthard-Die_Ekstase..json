{"dta.poem.21739": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Ekstase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verlohren!", "tokens": ["Ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Verlohren!", "tokens": ["Ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "O Lustgeschrey in meinen Ohren!", "tokens": ["O", "Lust\u00b7ge\u00b7schrey", "in", "mei\u00b7nen", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es singt um mich wie Brautgesang;", "tokens": ["Es", "singt", "um", "mich", "wie", "Braut\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es klingt der Hochzeitharfen Klang;", "tokens": ["Es", "klingt", "der", "Hoch\u00b7zeit\u00b7har\u00b7fen", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lodern schon die Kerzen.", "tokens": ["Es", "lo\u00b7dern", "schon", "die", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die muntern Jungfraun scherzen.", "tokens": ["Die", "mun\u00b7tern", "Jung\u00b7fraun", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Jungfraun, habt ihr Oels genug?", "tokens": ["Ihr", "Jung\u00b7fraun", ",", "habt", "ihr", "O\u00b7els", "ge\u00b7nug", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ach f\u00fcllt die Lampen, f\u00fcllt den Krug.", "tokens": ["Ach", "f\u00fcllt", "die", "Lam\u00b7pen", ",", "f\u00fcllt", "den", "Krug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Nacht ist s\u00fcss und schaurig,", "tokens": ["Die", "Nacht", "ist", "s\u00fcss", "und", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Braut so froh und traurig!", "tokens": ["Die", "Braut", "so", "froh", "und", "trau\u00b7rig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Welch Fl\u00fcstern,", "tokens": ["Welch", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "So l\u00fcstern", "tokens": ["So", "l\u00fcs\u00b7tern"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Raunt mir ins Ohr im D\u00fcstern!", "tokens": ["Raunt", "mir", "ins", "Ohr", "im", "D\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "\u201ekomm, holde Taube, s\u00fcsse Braut.", "tokens": ["\u201e", "komm", ",", "hol\u00b7de", "Tau\u00b7be", ",", "s\u00fcs\u00b7se", "Braut", "."], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u201eder Priester harrt. Der Priester traut.", "tokens": ["\u201e", "der", "Pries\u00b7ter", "harrt", ".", "Der", "Pries\u00b7ter", "traut", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201eder Teppich ist gebreitet.", "tokens": ["\u201e", "der", "Tep\u00b7pich", "ist", "ge\u00b7brei\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "\u201edas Bette ist bereitet.", "tokens": ["\u201e", "das", "Bet\u00b7te", "ist", "be\u00b7rei\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "\u201euns winkt des Lagers weicher Flaum", "tokens": ["\u201e", "uns", "winkt", "des", "La\u00b7gers", "wei\u00b7cher", "Flaum"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "\u201ezu s\u00fcssem Schlaf und s\u00fcsserm Traum.", "tokens": ["\u201e", "zu", "s\u00fcs\u00b7sem", "Schlaf", "und", "s\u00fcs\u00b7serm", "Traum", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u201ekommst, k\u00f6mmst du bald, du Fromme?\u201c \u2014", "tokens": ["\u201e", "kommst", ",", "k\u00f6mmst", "du", "bald", ",", "du", "From\u00b7me", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Ja, Br\u00e4utgam, ja, ich komme!", "tokens": ["Ja", ",", "Br\u00e4ut\u00b7gam", ",", "ja", ",", "ich", "kom\u00b7me", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}