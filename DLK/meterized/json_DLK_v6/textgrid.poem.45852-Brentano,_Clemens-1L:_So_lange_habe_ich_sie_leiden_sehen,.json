{"textgrid.poem.45852": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: So lange habe ich sie leiden sehen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lange habe ich sie leiden sehen,", "tokens": ["So", "lan\u00b7ge", "ha\u00b7be", "ich", "sie", "lei\u00b7den", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lange brennt ihr Schmerz in meiner Brust", "tokens": ["So", "lan\u00b7ge", "brennt", "ihr", "Schmerz", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gierig trank ich selbst mit frommer Lust", "tokens": ["Und", "gie\u00b7rig", "trank", "ich", "selbst", "mit", "from\u00b7mer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Gift aus ihrer Wunden tiefen Wehen.", "tokens": ["Das", "Gift", "aus", "ih\u00b7rer", "Wun\u00b7den", "tie\u00b7fen", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu allen M\u00e4chten drang mein kindisch Flehen,", "tokens": ["Zu", "al\u00b7len", "M\u00e4ch\u00b7ten", "drang", "mein", "kin\u00b7disch", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir war in meiner Unschuld nicht bewu\u00dft,", "tokens": ["Mir", "war", "in", "mei\u00b7ner", "Un\u00b7schuld", "nicht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mit der Freiheit traurigem Verlust", "tokens": ["Da\u00df", "mit", "der", "Frei\u00b7heit", "trau\u00b7ri\u00b7gem", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der Sch\u00f6nheit alle M\u00e4chte untergehen.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "al\u00b7le", "M\u00e4ch\u00b7te", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sieht mein Leiden, spricht mit hohen Blicken", "tokens": ["Sie", "sieht", "mein", "Lei\u00b7den", ",", "spricht", "mit", "ho\u00b7hen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O wehe! Was ich Sinkende ber\u00fchre", "tokens": ["O", "we\u00b7he", "!", "Was", "ich", "Sin\u00b7ken\u00b7de", "be\u00b7r\u00fch\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "PWS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich fest umklammernd in den Abgrund f\u00fchre.", "tokens": ["Ich", "fest", "um\u00b7klam\u00b7mernd", "in", "den", "Ab\u00b7grund", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da sprach ich k\u00fchn: mit schmerzlichem Entz\u00fccken", "tokens": ["Da", "sprach", "ich", "k\u00fchn", ":", "mit", "schmerz\u00b7li\u00b7chem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich an Deinem Grabe untergehen,", "tokens": ["Will", "ich", "an", "Dei\u00b7nem", "Gra\u00b7be", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will ich an Deiner Wiege auferstehen.", "tokens": ["Will", "ich", "an", "Dei\u00b7ner", "Wie\u00b7ge", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}