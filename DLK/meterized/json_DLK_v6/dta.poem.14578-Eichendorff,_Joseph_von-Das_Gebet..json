{"dta.poem.14578": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du seltner Pilger, la\u00df Dich warnen!", "tokens": ["Du", "selt\u00b7ner", "Pil\u00b7ger", ",", "la\u00df", "Dich", "war\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ird'scher Lust und Zauberei,", "tokens": ["Aus", "ird'\u00b7scher", "Lust", "und", "Zau\u00b7be\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die freud- und leidvoll Dich umgarnen,", "tokens": ["Die", "freu\u00b7d", "und", "leid\u00b7voll", "Dich", "um\u00b7gar\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Strecke zu Gott die Arme frei!", "tokens": ["Stre\u00b7cke", "zu", "Gott", "die", "Ar\u00b7me", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nichts mehr mu\u00dft Du hienieden haben,", "tokens": ["Nichts", "mehr", "mu\u00dft", "Du", "hien\u00b7ie\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VMFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Himmlisch betr\u00fcbt, verlassen, arm,", "tokens": ["Himm\u00b7lisch", "be\u00b7tr\u00fcbt", ",", "ver\u00b7las\u00b7sen", ",", "arm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ein treues Kind, dem Vater klagen", "tokens": ["Ein", "treu\u00b7es", "Kind", ",", "dem", "Va\u00b7ter", "kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ird'sche Lust, den ird'schen Harm.", "tokens": ["Die", "ird'\u00b7sche", "Lust", ",", "den", "ird'\u00b7schen", "Harm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}