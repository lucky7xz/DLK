{"textgrid.poem.43778": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach dem Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie ist es kalt geworden", "tokens": ["O", "wie", "ist", "es", "kalt", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so traurig, \u00f6d' und leer!", "tokens": ["Und", "so", "trau\u00b7rig", ",", "\u00f6d'", "und", "leer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rauhe Winde weh'n von Norden", "tokens": ["Rau\u00b7he", "Win\u00b7de", "weh'n", "von", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Sonne scheint nicht mehr.", "tokens": ["Und", "die", "Son\u00b7ne", "scheint", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf die Berge m\u00f6cht' ich fliegen,", "tokens": ["Auf", "die", "Ber\u00b7ge", "m\u00f6cht'", "ich", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chte seh'n ein gr\u00fcnes Thal,", "tokens": ["M\u00f6ch\u00b7te", "seh'n", "ein", "gr\u00fc\u00b7nes", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' in Gras und Blumen liegen", "tokens": ["M\u00f6cht'", "in", "Gras", "und", "Blu\u00b7men", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich freu'n am Sonnenstrahl;", "tokens": ["Und", "mich", "freu'n", "am", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00f6chte h\u00f6ren die Schalmeien", "tokens": ["M\u00f6ch\u00b7te", "h\u00f6\u00b7ren", "die", "Schal\u00b7mei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Und der Heerden Glockenklang,", "tokens": ["Und", "der", "Heer\u00b7den", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte freuen mich im Freien", "tokens": ["M\u00f6ch\u00b7te", "freu\u00b7en", "mich", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der V\u00f6gel s\u00fc\u00dfem Sang.", "tokens": ["An", "der", "V\u00f6\u00b7gel", "s\u00fc\u00b7\u00dfem", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6ner Fr\u00fchling, komm doch wieder,", "tokens": ["Sch\u00f6\u00b7ner", "Fr\u00fch\u00b7ling", ",", "komm", "doch", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber Fr\u00fchling, komm doch bald,", "tokens": ["Lie\u00b7ber", "Fr\u00fch\u00b7ling", ",", "komm", "doch", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bring' uns Blumen, Laub und Lieder,", "tokens": ["Bring'", "uns", "Blu\u00b7men", ",", "Laub", "und", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fccke wieder Feld und Wald!", "tokens": ["Schm\u00fc\u00b7cke", "wie\u00b7der", "Feld", "und", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, du bist uns treu geblieben,", "tokens": ["Ja", ",", "du", "bist", "uns", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst nun bald in Pracht und Glanz,", "tokens": ["Kommst", "nun", "bald", "in", "Pracht", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringst nun bald all deinen Lieben", "tokens": ["Bringst", "nun", "bald", "all", "dei\u00b7nen", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sang und Freude, Spiel und Tanz.", "tokens": ["Sang", "und", "Freu\u00b7de", ",", "Spiel", "und", "Tanz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}