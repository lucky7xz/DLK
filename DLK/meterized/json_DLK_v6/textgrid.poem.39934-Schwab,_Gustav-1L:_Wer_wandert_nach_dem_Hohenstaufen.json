{"textgrid.poem.39934": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer wandert nach dem Hohenstaufen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wandert nach dem Hohenstaufen", "tokens": ["Wer", "wan\u00b7dert", "nach", "dem", "Ho\u00b7hen\u00b7stau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch den verst\u00f6rten Tannenwald?", "tokens": ["Durch", "den", "ver\u00b7st\u00f6r\u00b7ten", "Tan\u00b7nen\u00b7wald", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die St\u00fcrme wehn, die B\u00e4ume traufen,", "tokens": ["Die", "St\u00fcr\u00b7me", "wehn", ",", "die", "B\u00e4u\u00b7me", "trau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Regen spinnt sich tr\u00fcb und kalt.", "tokens": ["Der", "Re\u00b7gen", "spinnt", "sich", "tr\u00fcb", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist kein Wetter mehr zum Reisen!", "tokens": ["Das", "ist", "kein", "Wet\u00b7ter", "mehr", "zum", "Rei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dort winkt ein gastlich helles Dach;", "tokens": ["Dort", "winkt", "ein", "gast\u00b7lich", "hel\u00b7les", "Dach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4\u00dft sich nicht in's Trockne weisen,", "tokens": ["Er", "l\u00e4\u00dft", "sich", "nicht", "in's", "Trock\u00b7ne", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es ruft der Wirt umsonst ihm nach.", "tokens": ["Es", "ruft", "der", "Wirt", "um\u00b7sonst", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das eben sei das rechte Wetter,", "tokens": ["Das", "e\u00b7ben", "sei", "das", "rech\u00b7te", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Meint er, zur alten Burg zu gehn;", "tokens": ["Meint", "er", ",", "zur", "al\u00b7ten", "Burg", "zu", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ruft des Donners dumpf Geschmetter,", "tokens": ["Wie", "ruft", "des", "Don\u00b7ners", "dumpf", "Ge\u00b7schmet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie mu\u00df sie sch\u00f6n im Blitze stehn!", "tokens": ["Wie", "mu\u00df", "sie", "sch\u00f6n", "im", "Blit\u00b7ze", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kl\u00e4nge sind es, die nicht altern,", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "sind", "es", ",", "die", "nicht", "al\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lichter, die nicht ausgebrannt,", "tokens": ["Die", "Lich\u00b7ter", ",", "die", "nicht", "aus\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und seit den ernsten Mittelaltern", "tokens": ["Und", "seit", "den", "erns\u00b7ten", "Mit\u00b7tel\u00b7al\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist droben wohl ihr Spiel bekannt.", "tokens": ["Ist", "dro\u00b7ben", "wohl", "ihr", "Spiel", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt ist er ganz hinauf geklommen,", "tokens": ["Jetzt", "ist", "er", "ganz", "hin\u00b7auf", "ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er stellt sich auf die Tr\u00fcmmer hin,", "tokens": ["Er", "stellt", "sich", "auf", "die", "Tr\u00fcm\u00b7mer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat ihn wahrlich mitgenommen", "tokens": ["Er", "hat", "ihn", "wahr\u00b7lich", "mit\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur rechten Statt den rechten Sinn.", "tokens": ["Zur", "rech\u00b7ten", "Statt", "den", "rech\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinem ernsten Angesichte,", "tokens": ["Mit", "sei\u00b7nem", "erns\u00b7ten", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinem sturmdurchwehten Kleid", "tokens": ["Mit", "sei\u00b7nem", "sturm\u00b7durch\u00b7weh\u00b7ten", "Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Steht er in dem Gewitterlichte", "tokens": ["Steht", "er", "in", "dem", "Ge\u00b7wit\u00b7ter\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Fast wie ein Geist aus jener Zeit.", "tokens": ["Fast", "wie", "ein", "Geist", "aus", "je\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie ein Lied aus jenen Tagen", "tokens": ["Und", "wie", "ein", "Lied", "aus", "je\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebt er seinen stolzen Sang,", "tokens": ["Er\u00b7hebt", "er", "sei\u00b7nen", "stol\u00b7zen", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ringt sich \u00fcber Leid und Klagen", "tokens": ["Der", "ringt", "sich", "\u00fc\u00b7ber", "Leid", "und", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinauf zum hellen Freudeklang;", "tokens": ["Hin\u00b7auf", "zum", "hel\u00b7len", "Freu\u00b7de\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hat von seiner Burg gesprochen,", "tokens": ["Er", "hat", "von", "sei\u00b7ner", "Burg", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sie der bittre Feind zerst\u00f6rt;", "tokens": ["Wie", "sie", "der", "bitt\u00b7re", "Feind", "zer\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er ruft mit Lust: sie ist zerbrochen,", "tokens": ["Er", "ruft", "mit", "Lust", ":", "sie", "ist", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil diese bessre mir geh\u00f6rt.", "tokens": ["Weil", "die\u00b7se", "bess\u00b7re", "mir", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann hat er weiter noch gesungen", "tokens": ["Dann", "hat", "er", "wei\u00b7ter", "noch", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von seiner ungetreuen Braut;", "tokens": ["Von", "sei\u00b7ner", "un\u00b7ge\u00b7treu\u00b7en", "Braut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da h\u00e4tte bald sein Lied geklungen,", "tokens": ["Da", "h\u00e4t\u00b7te", "bald", "sein", "Lied", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein bewegter Seufzerlaut.", "tokens": ["Wie", "ein", "be\u00b7weg\u00b7ter", "Seuf\u00b7zer\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch herrlich \u00fcber alle Schmerzen", "tokens": ["Doch", "herr\u00b7lich", "\u00fc\u00b7ber", "al\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empor das hohe Lied sich rei\u00dft,", "tokens": ["Em\u00b7por", "das", "ho\u00b7he", "Lied", "sich", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er singt von Ihr aus festem Herzen", "tokens": ["Er", "singt", "von", "Ihr", "aus", "fes\u00b7tem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als einem abgeschiednen Geist.", "tokens": ["Als", "ei\u00b7nem", "ab\u00b7ge\u00b7schied\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbist gleich mein Haus zerbrochen immer,", "tokens": ["\u00bb", "ist", "gleich", "mein", "Haus", "zer\u00b7bro\u00b7chen", "im\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerbrochen auch mein edles Herz,", "tokens": ["Zer\u00b7bro\u00b7chen", "auch", "mein", "ed\u00b7les", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ragen doch die hohen Tr\u00fcmmer", "tokens": ["So", "ra\u00b7gen", "doch", "die", "ho\u00b7hen", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lust und stolz noch himmelw\u00e4rts:", "tokens": ["Mit", "Lust", "und", "stolz", "noch", "him\u00b7mel\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hieher hab' ich mich gefl\u00fcchtet,", "tokens": ["Und", "hie\u00b7her", "hab'", "ich", "mich", "ge\u00b7fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Versto\u00dfen aus der neuen Welt:", "tokens": ["Ver\u00b7sto\u00b7\u00dfen", "aus", "der", "neu\u00b7en", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer je gek\u00e4mpft, geliebt, gedichtet,", "tokens": ["Wer", "je", "ge\u00b7k\u00e4mpft", ",", "ge\u00b7liebt", ",", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr den ist Wohnung hier bestellt.", "tokens": ["F\u00fcr", "den", "ist", "Woh\u00b7nung", "hier", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun denn, ihr alten Heldengeister,", "tokens": ["Nun", "denn", ",", "ihr", "al\u00b7ten", "Hel\u00b7den\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00e4mt euch des Genossen nicht!", "tokens": ["So", "sch\u00e4mt", "euch", "des", "Ge\u00b7nos\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr weitgepriesnen Sangesmeister,", "tokens": ["Ihr", "weit\u00b7ge\u00b7pri\u00b7es\u00b7nen", "San\u00b7ges\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nehmt freundlich mich in Lehr' und Pflicht!", "tokens": ["Nehmt", "freund\u00b7lich", "mich", "in", "Lehr'", "und", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O kommt hervor, ihr treuen Frauen,", "tokens": ["O", "kommt", "her\u00b7vor", ",", "ihr", "treu\u00b7en", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit hoher Minne Leid vertraut,", "tokens": ["Mit", "ho\u00b7her", "Min\u00b7ne", "Leid", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00dft mich in euer Antlitz schauen,", "tokens": ["La\u00dft", "mich", "in", "eu\u00b7er", "Ant\u00b7litz", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und tr\u00f6stet mich f\u00fcr meine Braut!\u00ab", "tokens": ["Und", "tr\u00f6s\u00b7tet", "mich", "f\u00fcr", "mei\u00b7ne", "Braut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Ritter hat schon lang geschwiegen,", "tokens": ["Der", "Rit\u00b7ter", "hat", "schon", "lang", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Donner rollt noch immer fort;", "tokens": ["Der", "Don\u00b7ner", "rollt", "noch", "im\u00b7mer", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sieht ihn oft im Blitze liegen,", "tokens": ["Man", "sieht", "ihn", "oft", "im", "Blit\u00b7ze", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz sanft und selig liegt er dort,", "tokens": ["Ganz", "sanft", "und", "se\u00b7lig", "liegt", "er", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geschlo\u00dfnen Auges, blasser Wangen;", "tokens": ["Ge\u00b7schlo\u00df\u00b7nen", "Au\u00b7ges", ",", "blas\u00b7ser", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist's Schlaf, ist's Tod, ich wei\u00df es kaum;", "tokens": ["Ist's", "Schlaf", ",", "ist's", "Tod", ",", "ich", "wei\u00df", "es", "kaum", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch sicher tr\u00e4umt er ohne Bangen", "tokens": ["Doch", "si\u00b7cher", "tr\u00e4umt", "er", "oh\u00b7ne", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Staufen einen lichten Traum!", "tokens": ["Von", "Stau\u00b7fen", "ei\u00b7nen", "lich\u00b7ten", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}