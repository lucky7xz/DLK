{"textgrid.poem.32846": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Clitandern macht sein Reichthum Sorgen:", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Clitandern macht sein Reichthum Sorgen:", "tokens": ["Cli\u00b7tan\u00b7dern", "macht", "sein", "Reicht\u00b7hum", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der bringt ihm Geld, und der will borgen,", "tokens": ["Der", "bringt", "ihm", "Geld", ",", "und", "der", "will", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "KON", "ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und jener handelt um den Pacht:", "tokens": ["Und", "je\u00b7ner", "han\u00b7delt", "um", "den", "Pacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O welche Quaal! um sie zu enden,", "tokens": ["O", "wel\u00b7che", "Qua\u00b7al", "!", "um", "sie", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$.", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Eilt er sein Reichthum zu verschwenden:", "tokens": ["Eilt", "er", "sein", "Reicht\u00b7hum", "zu", "ver\u00b7schwen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er gut gemacht!", "tokens": ["Das", "hat", "er", "gut", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Stax fordert Geld, um froh zu leben,", "tokens": ["Stax", "for\u00b7dert", "Geld", ",", "um", "froh", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Vater will ihm keines geben", "tokens": ["Sein", "Va\u00b7ter", "will", "ihm", "kei\u00b7nes", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Sch\u00e4tzen, die er toll bewacht:", "tokens": ["Von", "Sch\u00e4t\u00b7zen", ",", "die", "er", "toll", "be\u00b7wacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er thut dem Vater einen Possen,", "tokens": ["Er", "thut", "dem", "Va\u00b7ter", "ei\u00b7nen", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geht in den Krieg, und wird erschossen:", "tokens": ["Geht", "in", "den", "Krieg", ",", "und", "wird", "er\u00b7schos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er gut gemacht!", "tokens": ["Das", "hat", "er", "gut", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mops, in Carossen herzufahren,", "tokens": ["Mops", ",", "in", "Ca\u00b7ros\u00b7sen", "her\u00b7zu\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt sich ein Weib von siebzig Jahren,", "tokens": ["Nimmt", "sich", "ein", "Weib", "von", "sieb\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn sie hat Geld, und er liebt Pracht;", "tokens": ["Denn", "sie", "hat", "Geld", ",", "und", "er", "liebt", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie hat ihm mehr als jetzt gefehlet:", "tokens": ["Nie", "hat", "ihm", "mehr", "als", "jetzt", "ge\u00b7feh\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im kurzen ist er todt gequ\u00e4let,", "tokens": ["Im", "kur\u00b7zen", "ist", "er", "todt", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er gut gemacht!", "tokens": ["Das", "hat", "er", "gut", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jobst, seiner Kinder Gl\u00fcck zu heben,", "tokens": ["Jobst", ",", "sei\u00b7ner", "Kin\u00b7der", "Gl\u00fcck", "zu", "he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sich des Adels W\u00fcrde geben,", "tokens": ["L\u00e4\u00dft", "sich", "des", "A\u00b7dels", "W\u00fcr\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lehrt sie ihres Standes Macht:", "tokens": ["Und", "lehrt", "sie", "ih\u00b7res", "Stan\u00b7des", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch bey freyherrlichen Geberden", "tokens": ["Doch", "bey", "frey\u00b7herr\u00b7li\u00b7chen", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sieht sie die Welt zu Bettlern werden:", "tokens": ["Sieht", "sie", "die", "Welt", "zu", "Bett\u00b7lern", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er gut gemacht!", "tokens": ["Das", "hat", "er", "gut", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ruffin will als Minister sterben,", "tokens": ["Ruf\u00b7fin", "will", "als", "Mi\u00b7nis\u00b7ter", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KOUS", "NN", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sucht durch R\u00e4nke zu erwerben,", "tokens": ["Und", "sucht", "durch", "R\u00e4n\u00b7ke", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Worzu ihn kein Verdienst gebracht;", "tokens": ["Wor\u00b7zu", "ihn", "kein", "Ver\u00b7dienst", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er steigt am Hof und f\u00e4llt auch wieder,", "tokens": ["Er", "steigt", "am", "Hof", "und", "f\u00e4llt", "auch", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der H\u00f6fling tritt ihn stolz darnieder:", "tokens": ["Der", "H\u00f6f\u00b7ling", "tritt", "ihn", "stolz", "dar\u00b7nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er gut gemacht!", "tokens": ["Das", "hat", "er", "gut", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Veit, der der Zeiten Bosheit kannte,", "tokens": ["Veit", ",", "der", "der", "Zei\u00b7ten", "Bos\u00b7heit", "kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Freut sich jetzt seiner alten Tante,", "tokens": ["Freut", "sich", "jetzt", "sei\u00b7ner", "al\u00b7ten", "Tan\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die seine junge Frau bewacht:", "tokens": ["Die", "sei\u00b7ne", "jun\u00b7ge", "Frau", "be\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch die zu menschlich sie zu qu\u00e4len,", "tokens": ["Doch", "die", "zu", "menschlich", "sie", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKA", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hilft ihr die jungen Buhler w\u00e4hlen:", "tokens": ["Hilft", "ihr", "die", "jun\u00b7gen", "Buh\u00b7ler", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er gut gemacht!", "tokens": ["Das", "hat", "er", "gut", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}