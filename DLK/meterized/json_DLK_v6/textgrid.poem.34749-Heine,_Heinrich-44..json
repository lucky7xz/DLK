{"textgrid.poem.34749": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel grau und wochent\u00e4glich!", "tokens": ["Him\u00b7mel", "grau", "und", "wo\u00b7chen\u00b7t\u00e4g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Stadt ist noch dieselbe!", "tokens": ["Auch", "die", "Stadt", "ist", "noch", "die\u00b7sel\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PDAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und noch immer bl\u00f6d und kl\u00e4glich", "tokens": ["Und", "noch", "im\u00b7mer", "bl\u00f6d", "und", "kl\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spiegelt sie sich in der Elbe.", "tokens": ["Spie\u00b7gelt", "sie", "sich", "in", "der", "El\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lange Nasen, noch langweilig", "tokens": ["Lan\u00b7ge", "Na\u00b7sen", ",", "noch", "lang\u00b7wei\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden sie wie sonst geschneuzet,", "tokens": ["Wer\u00b7den", "sie", "wie", "sonst", "ge\u00b7schneu\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und das duckt sich noch scheinheilig,", "tokens": ["Und", "das", "duckt", "sich", "noch", "schein\u00b7hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder bl\u00e4ht sich, stolz gespreizet.", "tokens": ["O\u00b7der", "bl\u00e4ht", "sich", ",", "stolz", "ge\u00b7sprei\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ner S\u00fcden! wie verehr ich", "tokens": ["Sch\u00f6\u00b7ner", "S\u00fc\u00b7den", "!", "wie", "ver\u00b7ehr", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWAV", "ADJD", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Himmel, deine G\u00f6tter,", "tokens": ["Dei\u00b7nen", "Him\u00b7mel", ",", "dei\u00b7ne", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit ich diesen Menschenkehricht", "tokens": ["Seit", "ich", "die\u00b7sen", "Men\u00b7schen\u00b7keh\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wiederseh, und dieses Wetter!", "tokens": ["Wie\u00b7der\u00b7seh", ",", "und", "die\u00b7ses", "Wet\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}