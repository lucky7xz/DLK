{"textgrid.poem.66463": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du noch, wie ich am Felsen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du noch, wie ich am Felsen", "tokens": ["Wei\u00dft", "du", "noch", ",", "wie", "ich", "am", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei den Veilchen dich belauschte,", "tokens": ["Bei", "den", "Veil\u00b7chen", "dich", "be\u00b7lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dft du noch den Fliederstrauch,", "tokens": ["Wei\u00dft", "du", "noch", "den", "Flie\u00b7der\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Strom vor\u00fcber rauschte?", "tokens": ["Wo", "der", "Strom", "vor\u00b7\u00fc\u00b7ber", "rauschte", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dft du noch den Bergespfad,", "tokens": ["Wei\u00dft", "du", "noch", "den", "Ber\u00b7ge\u00b7spfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich um den Strau\u00df dich bat,", "tokens": ["Wo", "ich", "um", "den", "Strau\u00df", "dich", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wei\u00dft du noch?", "tokens": ["Wei\u00dft", "du", "noch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ach, es war ein s\u00fc\u00dfes Bild,", "tokens": ["Ach", ",", "es", "war", "ein", "s\u00fc\u00b7\u00dfes", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als du da err\u00f6thend standest,", "tokens": ["Als", "du", "da", "er\u00b7r\u00f6\u00b7thend", "stan\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zur Erde all die Blumen", "tokens": ["Und", "zur", "Er\u00b7de", "all", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fielen, die zum Strau\u00df du wandest,", "tokens": ["Fie\u00b7len", ",", "die", "zum", "Strau\u00df", "du", "wan\u00b7dest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPRART", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine liebe kleine Hand", "tokens": ["Dei\u00b7ne", "lie\u00b7be", "klei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Spielte mit dem blauen Band,", "tokens": ["Spiel\u00b7te", "mit", "dem", "blau\u00b7en", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00dft du noch?", "tokens": ["Wei\u00dft", "du", "noch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und es sahen Fels und Strom", "tokens": ["Und", "es", "sa\u00b7hen", "Fels", "und", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Err\u00f6then und dein Beben,", "tokens": ["Dein", "Er\u00b7r\u00f6\u00b7then", "und", "dein", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen auch den ersten Ku\u00df,", "tokens": ["Sa\u00b7hen", "auch", "den", "ers\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halb genommen, halb gegeben!", "tokens": ["Halb", "ge\u00b7nom\u00b7men", ",", "halb", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Himmels goldner Strahl", "tokens": ["Und", "des", "Him\u00b7mels", "gold\u00b7ner", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueberflog Gebirg und Thal,", "tokens": ["Ue\u00b7berf\u00b7log", "Ge\u00b7birg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00dft du noch?", "tokens": ["Wei\u00dft", "du", "noch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}