{"textgrid.poem.63757": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Schritt und Tritt, wohin die Augen schweifen,", "tokens": ["Auf", "Schritt", "und", "Tritt", ",", "wo\u00b7hin", "die", "Au\u00b7gen", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du hier Reiz und Sch\u00f6nheit zu bestaunen.", "tokens": ["Hast", "du", "hier", "Reiz", "und", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kommst du in grauen Locken oder braunen,", "tokens": ["Kommst", "du", "in", "grau\u00b7en", "Lo\u00b7cken", "o\u00b7der", "brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das alte \u00bbSieh und stirb!\u00ab wirst du begreifen.", "tokens": ["Das", "al\u00b7te", "\u00bb", "Sieh", "und", "stirb", "!", "\u00ab", "wirst", "du", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "NE", "KON", "VVIMP", "$.", "$(", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es lie\u00df der Himmel diese Perle reifen", "tokens": ["Es", "lie\u00df", "der", "Him\u00b7mel", "die\u00b7se", "Per\u00b7le", "rei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In der humansten seiner Sch\u00f6pferlaunen.", "tokens": ["In", "der", "hu\u00b7mans\u00b7ten", "sei\u00b7ner", "Sch\u00f6p\u00b7fer\u00b7lau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Sp\u00f6tter auch von ihren Flecken raunen,", "tokens": ["Was", "Sp\u00f6t\u00b7ter", "auch", "von", "ih\u00b7ren", "Fle\u00b7cken", "rau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird nicht den Glanz von ihrer Schale streifen.", "tokens": ["Wird", "nicht", "den", "Glanz", "von", "ih\u00b7rer", "Scha\u00b7le", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier findest du zu Kauf wie im Bazare", "tokens": ["Hier", "fin\u00b7dest", "du", "zu", "Kauf", "wie", "im", "Ba\u00b7za\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kunst und Natur, jedweden Schmuck des Lebens,", "tokens": ["Kunst", "und", "Na\u00b7tur", ",", "jed\u00b7we\u00b7den", "Schmuck", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da\u00df nichts dem schwelgendsten Bed\u00fcrfnis fehle.", "tokens": ["Da\u00df", "nichts", "dem", "schwel\u00b7gends\u00b7ten", "Be\u00b7d\u00fcrf\u00b7nis", "feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von allem auserlesne Exemplare.", "tokens": ["Von", "al\u00b7lem", "au\u00b7ser\u00b7les\u00b7ne", "Ex\u00b7emp\u00b7la\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur einen Reiz ersehnst du hier vergebens:", "tokens": ["Nur", "ei\u00b7nen", "Reiz", "er\u00b7sehnst", "du", "hier", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den schlichten Liebreiz einer sch\u00f6nen Seele.", "tokens": ["Den", "schlich\u00b7ten", "Lieb\u00b7reiz", "ei\u00b7ner", "sch\u00f6\u00b7nen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}