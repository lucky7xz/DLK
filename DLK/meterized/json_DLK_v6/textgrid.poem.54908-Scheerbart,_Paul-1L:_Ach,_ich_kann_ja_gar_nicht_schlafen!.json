{"textgrid.poem.54908": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, ich kann ja gar nicht schlafen!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ich kann ja gar nicht schlafen!", "tokens": ["Ach", ",", "ich", "kann", "ja", "gar", "nicht", "schla\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber dem dunkelgr\u00fcnen Myrtentor", "tokens": ["\u00dc\u00b7ber", "dem", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Myr\u00b7ten\u00b7tor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Thront ein dicker roter Mond. \u2013", "tokens": ["Thront", "ein", "di\u00b7cker", "ro\u00b7ter", "Mond", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob es sp\u00e4ter wohl noch lohnt,", "tokens": ["Ob", "es", "sp\u00e4\u00b7ter", "wohl", "noch", "lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn man auf dem Monde wohnt?", "tokens": ["Wenn", "man", "auf", "dem", "Mon\u00b7de", "wohnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber dem dunkelgr\u00fcnen Myrtentor?", "tokens": ["\u00dc\u00b7ber", "dem", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Myr\u00b7ten\u00b7tor", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "W\u00e4r's nicht m\u00f6glich, da\u00df uns dr\u00fcben", "tokens": ["W\u00e4r's", "nicht", "m\u00f6g\u00b7lich", ",", "da\u00df", "uns", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00bbl\u00e4ngre\u00ab Seligkeiten k\u00fc\u00dften?", "tokens": ["\u00bb", "l\u00e4ng\u00b7re", "\u00ab", "Se\u00b7lig\u00b7kei\u00b7ten", "k\u00fc\u00df\u00b7ten", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn wir das genauer w\u00fc\u00dften!", "tokens": ["Wenn", "wir", "das", "ge\u00b7nau\u00b7er", "w\u00fc\u00df\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hier ist alles zu schnell aus.", "tokens": ["Hier", "ist", "al\u00b7les", "zu", "schnell", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jeder lebt in Saus und Braus.", "tokens": ["Je\u00b7der", "lebt", "in", "Saus", "und", "Braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wem das schlie\u00dflich nicht gef\u00e4llt,", "tokens": ["Wem", "das", "schlie\u00df\u00b7lich", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00e4lt die ganze gro\u00dfe Welt", "tokens": ["H\u00e4lt", "die", "gan\u00b7ze", "gro\u00b7\u00dfe", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Auch blo\u00df f\u00fcr ein Narrenhaus!", "tokens": ["Auch", "blo\u00df", "f\u00fcr", "ein", "Nar\u00b7ren\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ach, ich kann ja gar nicht schlafen!", "tokens": ["Ach", ",", "ich", "kann", "ja", "gar", "nicht", "schla\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alter Mond, ich lach dich aus!", "tokens": ["Al\u00b7ter", "Mond", ",", "ich", "lach", "dich", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Doch du machst dir nichts daraus!", "tokens": ["Doch", "du", "machst", "dir", "nichts", "da\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}