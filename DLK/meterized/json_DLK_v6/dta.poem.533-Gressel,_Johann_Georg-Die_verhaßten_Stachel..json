{"dta.poem.533": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die verha\u00dften Stachel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was spitz und stachlicht ist/ da\u00df ha\u00dft das Frauen-Zimmer/", "tokens": ["Was", "spitz", "und", "stach\u00b7licht", "ist", "/", "da\u00df", "ha\u00dft", "das", "Frau\u00b7en\u00b7Zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVFIN", "VAFIN", "$(", "KOUS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schreit/ wenn ihre Hand ein scharffes Ding ber\u00fchrt.", "tokens": ["Es", "schreit", "/", "wenn", "ih\u00b7re", "Hand", "ein", "scharf\u00b7fes", "Ding", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn es die Rosen bricht schillt es die Stacheln immer", "tokens": ["Wenn", "es", "die", "Ro\u00b7sen", "bricht", "schillt", "es", "die", "Sta\u00b7cheln", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+---+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die deren Purpur-Zier zur Wache bey sich f\u00fchrt.", "tokens": ["Die", "de\u00b7ren", "Pur\u00b7pur\u00b7Zier", "zur", "Wa\u00b7che", "bey", "sich", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELAT", "NN", "APPRART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Honig liebt es zwar/ doch aber nicht die Bienen/", "tokens": ["Das", "Ho\u00b7nig", "liebt", "es", "zwar", "/", "doch", "a\u00b7ber", "nicht", "die", "Bie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADV", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn deren scharff Gewehr offt ihre Haut verletzt/", "tokens": ["Denn", "de\u00b7ren", "scharff", "Ge\u00b7wehr", "offt", "ih\u00b7re", "Haut", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es liebt das weiche Blat der gl\u00e4ntzenden ", "tokens": ["Es", "liebt", "das", "wei\u00b7che", "Blat", "der", "gl\u00e4nt\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Die Dornen aber nicht/ die in dem Zaun gesetzt.", "tokens": ["Die", "Dor\u00b7nen", "a\u00b7ber", "nicht", "/", "die", "in", "dem", "Zaun", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "$(", "ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Nadel mu\u00df ihm zwar im Putze Dienste leisten/", "tokens": ["Die", "Na\u00b7del", "mu\u00df", "ihm", "zwar", "im", "Put\u00b7ze", "Diens\u00b7te", "leis\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allein die Spitze ist/ so bald sie sticht veracht.", "tokens": ["Al\u00b7lein", "die", "Spit\u00b7ze", "ist", "/", "so", "bald", "sie", "sticht", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$(", "ADV", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dis ging\u2019 noch alles hin/ doch kr\u00e4nckt uns dis am meisten/", "tokens": ["Dis", "ging'", "noch", "al\u00b7les", "hin", "/", "doch", "kr\u00e4nckt", "uns", "dis", "am", "meis\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "PDS", "PTKA", "PIS", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df unsers Mundes-Zier auch wird dazu gemacht.", "tokens": ["Da\u00df", "un\u00b7sers", "Mun\u00b7des\u00b7Zier", "auch", "wird", "da\u00b7zu", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie m\u00f6gen hertzlich gern von uns die K\u00fcsse nehmen/", "tokens": ["Sie", "m\u00f6\u00b7gen", "hertz\u00b7lich", "gern", "von", "uns", "die", "K\u00fcs\u00b7se", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn unser Mannheit-Schmuck nicht um den Lippen steht/", "tokens": ["Wenn", "un\u00b7ser", "Mann\u00b7heit\u00b7Schmuck", "nicht", "um", "den", "Lip\u00b7pen", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn aber will der Mund sich nicht dazu bequehmen", "tokens": ["Denn", "a\u00b7ber", "will", "der", "Mund", "sich", "nicht", "da\u00b7zu", "be\u00b7queh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "PRF", "PTKNEG", "PAV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn ihnen nur der Bart in ihre Haut eingeht.", "tokens": ["Wenn", "ih\u00b7nen", "nur", "der", "Bart", "in", "ih\u00b7re", "Haut", "ein\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sind aber/ Kinder/ euch die Stachels auch verhasset/", "tokens": ["Sind", "a\u00b7ber", "/", "Kin\u00b7der", "/", "euch", "die", "Sta\u00b7chels", "auch", "ver\u00b7has\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "NN", "$(", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die eurer Jungferschafft den lieben Tod anthun?", "tokens": ["Die", "eu\u00b7rer", "Jung\u00b7fer\u00b7schafft", "den", "lie\u00b7ben", "Tod", "an\u00b7thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nein! denn er wird von euch an solchen Ort gefasset/", "tokens": ["Nein", "!", "denn", "er", "wird", "von", "euch", "an", "sol\u00b7chen", "Ort", "ge\u00b7fas\u00b7set", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "PPER", "VAFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df ihr im Stechen k\u00f6nnt in s\u00fcsser Wollust ruhn.", "tokens": ["Da\u00df", "ihr", "im", "Ste\u00b7chen", "k\u00f6nnt", "in", "s\u00fcs\u00b7ser", "Wol\u00b7lust", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Stachel ists/ der euch alleine will gefallen/", "tokens": ["Der", "Sta\u00b7chel", "ists", "/", "der", "euch", "al\u00b7lei\u00b7ne", "will", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da ander Stachels euch zu mahl verhasset seyn/", "tokens": ["Da", "an\u00b7der", "Sta\u00b7chels", "euch", "zu", "mahl", "ver\u00b7has\u00b7set", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "PPER", "APPR", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Stachel liebet ihr an uns vor andern allen/", "tokens": ["Den", "Sta\u00b7chel", "lie\u00b7bet", "ihr", "an", "uns", "vor", "an\u00b7dern", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "APPR", "PIS", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil er so z\u00e4rtlich sticht euch eine Wunde ein.", "tokens": ["Weil", "er", "so", "z\u00e4rt\u00b7lich", "sticht", "euch", "ei\u00b7ne", "Wun\u00b7de", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein ihr seyd beth\u00f6rt/ da\u00df ihr den Stachel liebet/", "tokens": ["Al\u00b7lein", "ihr", "seyd", "be\u00b7th\u00f6rt", "/", "da\u00df", "ihr", "den", "Sta\u00b7chel", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der mit dem s\u00fcssen Stich euch allzu sch\u00e4dlich ist/", "tokens": ["Der", "mit", "dem", "s\u00fcs\u00b7sen", "Stich", "euch", "all\u00b7zu", "sch\u00e4d\u00b7lich", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "PTKA", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gesetzt/ da\u00df euch ein Dorn/ ein Bart Verletzung giebet", "tokens": ["Ge\u00b7setzt", "/", "da\u00df", "euch", "ein", "Dorn", "/", "ein", "Bart", "Ver\u00b7let\u00b7zung", "gie\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "KOUS", "PPER", "ART", "NN", "$(", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So stirbt die Ehr doch nicht/ die bald das Grab-Mahl k\u00fc\u00dft.", "tokens": ["So", "stirbt", "die", "Ehr", "doch", "nicht", "/", "die", "bald", "das", "Grab\u00b7Mahl", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$(", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ha\u00dft doch die Stacheln nicht/ die euch nicht schaden k\u00fcnnen/", "tokens": ["Ha\u00dft", "doch", "die", "Sta\u00b7cheln", "nicht", "/", "die", "euch", "nicht", "scha\u00b7den", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PTKNEG", "$(", "PRELS", "PPER", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vertragt der Dornen-Stich des Bartes auch darbey/", "tokens": ["Ver\u00b7tragt", "der", "Dor\u00b7nen\u00b7Stich", "des", "Bar\u00b7tes", "auch", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Schmertz/ der davon kommt/ pflegt leichte zu zerrinnen/", "tokens": ["Der", "Schmertz", "/", "der", "da\u00b7von", "kommt", "/", "pflegt", "leich\u00b7te", "zu", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PAV", "VVFIN", "$(", "VVFIN", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und glaubt der s\u00fcsse Stich macht viel Beschwererey.", "tokens": ["Und", "glaubt", "der", "s\u00fcs\u00b7se", "Stich", "macht", "viel", "Be\u00b7schwe\u00b7re\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}