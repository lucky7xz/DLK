{"dta.poem.12822": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Judas-ku\u00df.  \n Madrigal.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du mehr als bittrer ku\u00df,", "tokens": ["Du", "mehr", "als", "bit\u00b7trer", "ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein wesen \u00fcbertrifft", "tokens": ["Dein", "we\u00b7sen", "\u00fc\u00b7bert\u00b7rifft"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den \u00e4rgsten spinnen-gifft,", "tokens": ["Den", "\u00e4rgs\u00b7ten", "spin\u00b7nen\u00b7gifft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wovon man bald erstarren mu\u00df.", "tokens": ["Wo\u00b7von", "man", "bald", "er\u00b7star\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat dir Satan zugepfiffen,", "tokens": ["Es", "hat", "dir", "Sa\u00b7tan", "zu\u00b7ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf w\u00fcrgen auszugehn.", "tokens": ["Auf", "w\u00fcr\u00b7gen", "aus\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du hinterl\u00e4st noch deines gleichen,", "tokens": ["Du", "hin\u00b7ter\u00b7l\u00e4st", "noch", "dei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So diese kunst so wol, als du, verstehn,", "tokens": ["So", "die\u00b7se", "kunst", "so", "wol", ",", "als", "du", ",", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und gar zu gern den n\u00e4chsten hinterschleichen,", "tokens": ["Und", "gar", "zu", "gern", "den", "n\u00e4chs\u00b7ten", "hin\u00b7ter\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ja durch das viele k\u00fcssen", "tokens": ["Ja", "durch", "das", "vie\u00b7le", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zugleich den tod mit einzuhauchen wissen.", "tokens": ["Zu\u00b7gleich", "den", "tod", "mit", "ein\u00b7zu\u00b7hau\u00b7chen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}