{"textgrid.poem.36146": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sturm der br\u00fcllenden Nacht, wie so entsetzlich sch\u00f6n", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sturm der br\u00fcllenden Nacht, wie so entsetzlich sch\u00f6n", "tokens": ["Sturm", "der", "br\u00fcl\u00b7len\u00b7den", "Nacht", ",", "wie", "so", "ent\u00b7setz\u00b7lich", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PWAV", "ADV", "ADJD", "ADJD"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Hallt dein Donner! Du br\u00fcllst tief in die Seele mir,", "tokens": ["Hallt", "dein", "Don\u00b7ner", "!", "Du", "br\u00fcllst", "tief", "in", "die", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie des Schlachtrufs Drommete,", "tokens": ["Wie", "des", "Schlach\u00b7trufs", "Drom\u00b7me\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "J\u00fcnglingswilde und Heldenkraft \u2013", "tokens": ["J\u00fcng\u00b7lings\u00b7wil\u00b7de", "und", "Hel\u00b7den\u00b7kraft", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen hauset die Nacht. Heulend und zischend f\u00e4hrt", "tokens": ["Drau\u00b7\u00dfen", "hau\u00b7set", "die", "Nacht", ".", "Heu\u00b7lend", "und", "zi\u00b7schend", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVPP", "KON", "ADJD", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "In allm\u00e4chtigem Pomp Sturm durch die Felder hin,", "tokens": ["In", "all\u00b7m\u00e4ch\u00b7ti\u00b7gem", "Pomp", "Sturm", "durch", "die", "Fel\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Beugt der Sch\u00f6pfung Gewalt'ge,", "tokens": ["Beugt", "der", "Sch\u00f6p\u00b7fung", "Ge\u00b7walt'\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "St\u00e4upt die Berge, und zaus't \u2013 den Wald.", "tokens": ["St\u00e4upt", "die", "Ber\u00b7ge", ",", "und", "zaus't", "\u2013", "den", "Wald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "$(", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Orkan! Orkan! was schl\u00e4gst du mir mein einsames", "tokens": ["Or\u00b7kan", "!", "Or\u00b7kan", "!", "was", "schl\u00e4gst", "du", "mir", "mein", "ein\u00b7sa\u00b7mes"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PWS", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rundumbr\u00fclltes Gemach, sch\u00fctterst die Scheiben mir", "tokens": ["Run\u00b7dum\u00b7br\u00fcll\u00b7tes", "Ge\u00b7mach", ",", "sch\u00fct\u00b7terst", "die", "Schei\u00b7ben", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In den Fenstern? \u2013 Ich komme,", "tokens": ["In", "den", "Fens\u00b7tern", "?", "\u2013", "Ich", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wogenst\u00fcrmer, und sprech' dir Hohn.", "tokens": ["Wo\u00b7gens\u00b7t\u00fcr\u00b7mer", ",", "und", "sprech'", "dir", "Hohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Schau! gewalt'ger Tyrann, schaue, da komm' ich, steh'", "tokens": ["Schau", "!", "ge\u00b7walt'\u00b7ger", "Ty\u00b7rann", ",", "schau\u00b7e", ",", "da", "komm'", "ich", ",", "steh'"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "ADJA", "NN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rings in Dunkel und Nacht, bebe nicht deiner Wuth,", "tokens": ["Rings", "in", "Dun\u00b7kel", "und", "Nacht", ",", "be\u00b7be", "nicht", "dei\u00b7ner", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die die Sterne des Himmels,", "tokens": ["Die", "die", "Ster\u00b7ne", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die die Fackel des Mond's erl\u00f6scht.", "tokens": ["Die", "die", "Fa\u00b7ckel", "des", "Mon\u00b7d's", "er\u00b7l\u00f6scht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Bebe nicht vor des Wald's tiefem Geheul, und nicht", "tokens": ["Be\u00b7be", "nicht", "vor", "des", "Wald's", "tie\u00b7fem", "Ge\u00b7heul", ",", "und", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,", "KON", "PTKNEG"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vor des zornigen Meer's D\u00fcstergebr\u00fcll, und nicht", "tokens": ["Vor", "des", "zor\u00b7ni\u00b7gen", "Meer's", "D\u00fcs\u00b7ter\u00b7ge\u00b7br\u00fcll", ",", "und", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,", "KON", "PTKNEG"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vor den Schreckenphantomen,", "tokens": ["Vor", "den", "Schre\u00b7cken\u00b7phan\u00b7to\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die die L\u00fcfte durchsegeln \u2013 \u2013 Doch", "tokens": ["Die", "die", "L\u00fcf\u00b7te", "durch\u00b7se\u00b7geln", "\u2013", "\u2013", "Doch"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ART", "NN", "VVINF", "$(", "$(", "KON"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Nein, ich hasse dich nicht, Starker! In deinem Pomp", "tokens": ["Nein", ",", "ich", "has\u00b7se", "dich", "nicht", ",", "Star\u00b7ker", "!", "In", "dei\u00b7nem", "Pomp"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sey mir festlich begr\u00fc\u00dft. Siehe! ich liebe dich,", "tokens": ["Sey", "mir", "fest\u00b7lich", "be\u00b7gr\u00fc\u00dft", ".", "Sie\u00b7he", "!", "ich", "lie\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$.", "VVIMP", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn im Segel der Windsbraut", "tokens": ["Wenn", "im", "Se\u00b7gel", "der", "Winds\u00b7braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gottes Sch\u00f6pfung dein Fu\u00df durchw\u00e4lzt.", "tokens": ["Got\u00b7tes", "Sch\u00f6p\u00b7fung", "dein", "Fu\u00df", "durch\u00b7w\u00e4lzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ist's, wenn du die Weitzensaat", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ist's", ",", "wenn", "du", "die", "Weit\u00b7zen\u00b7saat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Niederw\u00fchlst, wenn dein Arm herrisch den stolzen Wald", "tokens": ["Nie\u00b7der\u00b7w\u00fchlst", ",", "wenn", "dein", "Arm", "her\u00b7risch", "den", "stol\u00b7zen", "Wald"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dir zu huld'gen gebietet,", "tokens": ["Dir", "zu", "huld'\u00b7gen", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und die Kron' ihm zu Boden schmei\u00dft.", "tokens": ["Und", "die", "Kron'", "ihm", "zu", "Bo\u00b7den", "schmei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ist's, wenn du die Klippe spalt'st,", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ist's", ",", "wenn", "du", "die", "Klip\u00b7pe", "spalt'st", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn das grauliche Meer wuthsch\u00e4umt, dein Odem dann", "tokens": ["Wenn", "das", "grau\u00b7li\u00b7che", "Meer", "wuth\u00b7sch\u00e4umt", ",", "dein", "O\u00b7dem", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Seine Maste und Kiele,", "tokens": ["Sei\u00b7ne", "Mas\u00b7te", "und", "Kie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Zinnen der St\u00e4dte bricht \u2013", "tokens": ["Und", "die", "Zin\u00b7nen", "der", "St\u00e4d\u00b7te", "bricht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Orkan! Orkan! und mir, der ich dich liebe, mir", "tokens": ["Or\u00b7kan", "!", "Or\u00b7kan", "!", "und", "mir", ",", "der", "ich", "dich", "lie\u00b7be", ",", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "NE", "$.", "KON", "PPER", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "St\u00fcrmt dein Schreckenget\u00f6s' Freuden und wilde Lust", "tokens": ["St\u00fcrmt", "dein", "Schre\u00b7cken\u00b7ge\u00b7t\u00f6s'", "Freu\u00b7den", "und", "wil\u00b7de", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "ADJA", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In die Seele. Mir brennet", "tokens": ["In", "die", "See\u00b7le", ".", "Mir", "bren\u00b7net"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zwiefach Leben im Feueraug'", "tokens": ["Zwie\u00b7fach", "Le\u00b7ben", "im", "Feu\u00b7er\u00b7aug'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "O, mir hallt dein Gebr\u00fcll lieblich, wie Westgeweh,", "tokens": ["O", ",", "mir", "hallt", "dein", "Ge\u00b7br\u00fcll", "lieb\u00b7lich", ",", "wie", "West\u00b7ge\u00b7weh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "S\u00fc\u00df, die L\u00e4cheln der Braut, herrlich, wie Glockensturm,", "tokens": ["S\u00fc\u00df", ",", "die", "L\u00e4\u00b7cheln", "der", "Braut", ",", "herr\u00b7lich", ",", "wie", "Glo\u00b7cken\u00b7sturm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Gottesflamme und Starkmuth", "tokens": ["Got\u00b7tes\u00b7flam\u00b7me", "und", "Stark\u00b7muth"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Rei\u00dft mir m\u00e4chtig durch Mark und Bein,", "tokens": ["Rei\u00dft", "mir", "m\u00e4ch\u00b7tig", "durch", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Und ich st\u00fcrm' ins Gefild', heule mit dir, erklimm'", "tokens": ["Und", "ich", "st\u00fcrm'", "ins", "Ge\u00b7fild'", ",", "heu\u00b7le", "mit", "dir", ",", "er\u00b7klimm'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "APPR", "PPER", "$,", "VVFIN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hoch die Felsen, und renn' wieder hinab, und schrei',", "tokens": ["Hoch", "die", "Fel\u00b7sen", ",", "und", "renn'", "wie\u00b7der", "hin\u00b7ab", ",", "und", "schrei'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da\u00df die Halle der Berge", "tokens": ["Da\u00df", "die", "Hal\u00b7le", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mir ert\u00f6sen, da\u00df Wanderer", "tokens": ["Mir", "er\u00b7t\u00f6\u00b7sen", ",", "da\u00df", "Wan\u00b7de\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Bange lauschen \u2013 Doch, ha! rastlos durchschweift mein Fu\u00df", "tokens": ["Ban\u00b7ge", "lau\u00b7schen", "\u2013", "Doch", ",", "ha", "!", "rast\u00b7los", "durch\u00b7schweift", "mein", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$(", "KON", "$,", "ITJ", "$.", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die durchbr\u00fcllete Nacht, schmettert und rei\u00dft, und ras't;", "tokens": ["Die", "durch\u00b7br\u00fcl\u00b7le\u00b7te", "Nacht", ",", "schmet\u00b7tert", "und", "rei\u00dft", ",", "und", "ras't", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Denn noch bin ich ein J\u00fcngling,", "tokens": ["Denn", "noch", "bin", "ich", "ein", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stark wie der Sturm, und wild wie der.", "tokens": ["Stark", "wie", "der", "Sturm", ",", "und", "wild", "wie", "der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "KON", "ADJD", "KOKOM", "ART", "$."], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}}}}}