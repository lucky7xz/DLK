{"textgrid.poem.54509": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was haben wir doch hir als Tr\u00fcbsal/ ach/ vnd Bande?", "tokens": ["Was", "ha\u00b7ben", "wir", "doch", "hir", "als", "Tr\u00fcb\u00b7sal", "/", "ach", "/", "vnd", "Ban\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "KOUS", "NN", "$(", "XY", "$(", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schmacht die Seel' in Angst vnd stettem Trauren nicht/", "tokens": ["Doch", "schmacht", "die", "Seel'", "in", "Angst", "vnd", "stet\u00b7tem", "Trau\u00b7ren", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob schon vns Hertz vnd Fleisch vor Bangikeit zu bricht;", "tokens": ["Ob", "schon", "vns", "Hertz", "vnd", "Fleisch", "vor", "Ban\u00b7gi\u00b7keit", "zu", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "NN", "KON", "NN", "APPR", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reist kein verzweifeln eyn. Wir sind der Menschen Schande/", "tokens": ["Reist", "kein", "ver\u00b7zwei\u00b7feln", "eyn", ".", "Wir", "sind", "der", "Men\u00b7schen", "Schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man st\u00f6st als b\u00f6se Leut/ als Dieb' vns au\u00df dem Lande/", "tokens": ["Man", "st\u00f6st", "als", "b\u00f6\u00b7se", "Leut", "/", "als", "Dieb'", "vns", "au\u00df", "dem", "Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ADJA", "NN", "$(", "KOUS", "NN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd kr\u00e4nckt vns hir vnd dar/ doch wil de\u00df Himmels Licht/", "tokens": ["Vnd", "kr\u00e4nckt", "vns", "hir", "vnd", "dar", "/", "doch", "wil", "de\u00df", "Him\u00b7mels", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "PTKVZ", "$(", "ADV", "VMFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In Elend bey vns seyn/ ob auch die Welt vns richt/", "tokens": ["In", "E\u00b7lend", "bey", "vns", "seyn", "/", "ob", "auch", "die", "Welt", "vns", "richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VAINF", "$(", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd g\u00e4ntzlich vnterdruckt/ wir leben in dem Brande", "tokens": ["Vnd", "g\u00e4ntz\u00b7lich", "vn\u00b7ter\u00b7druckt", "/", "wir", "le\u00b7ben", "in", "dem", "Bran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir kommen keinmal vmb/ ob gleich de\u00df Herren Tod", "tokens": ["Wir", "kom\u00b7men", "kein\u00b7mal", "vmb", "/", "ob", "gleich", "de\u00df", "Her\u00b7ren", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "$(", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch so viel grimme Pein/ durch so viel grause Noth", "tokens": ["Durch", "so", "viel", "grim\u00b7me", "Pein", "/", "durch", "so", "viel", "grau\u00b7se", "Noth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$(", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Durch nicht erh\u00f6rten Zwang stets an vns wird erneuet:", "tokens": ["Durch", "nicht", "er\u00b7h\u00f6r\u00b7ten", "Zwang", "stets", "an", "vns", "wird", "er\u00b7neu\u00b7et", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "ADV", "APPR", "PPER", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil auch das herrlich seyn/ das Christi Creutz erwarb/", "tokens": ["Weil", "auch", "das", "herr\u00b7lich", "seyn", "/", "das", "Chris\u00b7ti", "Creutz", "er\u00b7warb", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "VAINF", "$(", "ART", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der auch in gr\u00f6ster Qvall in h\u00f6chstem Jammer starb", "tokens": ["Der", "auch", "in", "gr\u00f6s\u00b7ter", "Qvall", "in", "h\u00f6chs\u00b7tem", "Jam\u00b7mer", "starb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erneut an vns/ die nichts den Angst vnd Creutz erfreuet.", "tokens": ["Er\u00b7neut", "an", "vns", "/", "die", "nichts", "den", "Angst", "vnd", "Creutz", "er\u00b7freu\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$(", "ART", "PIS", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}