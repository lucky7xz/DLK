{"textgrid.poem.54110": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Leser hats gut: er kann sich seine Schriftsteller aussuchen.", "tokens": ["Ein", "Le\u00b7ser", "hats", "gut", ":", "er", "kann", "sich", "sei\u00b7ne", "Schrift\u00b7stel\u00b7ler", "aus\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VMFIN", "PRF", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+--+-+-+-++-++-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Der trockne Pedant hat gew\u00f6hnlich ein Ideal: den falschen Abenteurer.", "tokens": ["Der", "trock\u00b7ne", "Pe\u00b7dant", "hat", "ge\u00b7w\u00f6hn\u00b7lich", "ein", "I\u00b7deal", ":", "den", "fal\u00b7schen", "A\u00b7bent\u00b7eu\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-+-+-", "measure": "amphibrach.penta.plus"}}}}}