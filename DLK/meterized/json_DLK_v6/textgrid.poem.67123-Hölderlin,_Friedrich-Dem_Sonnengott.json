{"textgrid.poem.67123": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem Sonnengott", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du? trunken d\u00e4mmert die Seele mir", "tokens": ["Wo", "bist", "du", "?", "trun\u00b7ken", "d\u00e4m\u00b7mert", "die", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ADJD", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von aller deiner Wonne; denn eben ists,", "tokens": ["Von", "al\u00b7ler", "dei\u00b7ner", "Won\u00b7ne", ";", "denn", "e\u00b7ben", "ists", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$.", "KON", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich gesehn, wie, m\u00fcde seiner", "tokens": ["Da\u00df", "ich", "ge\u00b7sehn", ",", "wie", ",", "m\u00fc\u00b7de", "sei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "$,", "ADJD", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fahrt, der entz\u00fcckende G\u00f6tterj\u00fcngling", "tokens": ["Fahrt", ",", "der", "ent\u00b7z\u00fc\u00b7cken\u00b7de", "G\u00f6t\u00b7ter\u00b7j\u00fcng\u00b7ling"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die jungen Locken badet' im Goldgew\u00f6lk;", "tokens": ["Die", "jun\u00b7gen", "Lo\u00b7cken", "ba\u00b7det'", "im", "Gold\u00b7ge\u00b7w\u00f6lk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jetzt noch blickt mein Auge von selbst nach ihm;", "tokens": ["Und", "jetzt", "noch", "blickt", "mein", "Au\u00b7ge", "von", "selbst", "nach", "ihm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch fern ist er zu frommen V\u00f6lkern,", "tokens": ["Doch", "fern", "ist", "er", "zu", "from\u00b7men", "V\u00f6l\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihn noch ehren, hinweggegangen.", "tokens": ["Die", "ihn", "noch", "eh\u00b7ren", ",", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dich lieb ich, Erde! trauerst du doch mit mir!", "tokens": ["Dich", "lieb", "ich", ",", "Er\u00b7de", "!", "trau\u00b7erst", "du", "doch", "mit", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und unsre Trauer wandelt, wie Kinderschmerz,", "tokens": ["Und", "uns\u00b7re", "Trau\u00b7er", "wan\u00b7delt", ",", "wie", "Kin\u00b7der\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Schlummer sich, und wie die Winde", "tokens": ["In", "Schlum\u00b7mer", "sich", ",", "und", "wie", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "$,", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flattern und fl\u00fcstern im Saitenspiele,", "tokens": ["Flat\u00b7tern", "und", "fl\u00fcs\u00b7tern", "im", "Sai\u00b7ten\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Bis ihm des Meisters Finger den sch\u00f6nern Ton", "tokens": ["Bis", "ihm", "des", "Meis\u00b7ters", "Fin\u00b7ger", "den", "sch\u00f6\u00b7nern", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entlockt, so spielen Nebel und Tr\u00e4um um uns,", "tokens": ["Ent\u00b7lockt", ",", "so", "spie\u00b7len", "Ne\u00b7bel", "und", "Tr\u00e4um", "um", "uns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis der Geliebte wiederk\u00f6mmt und", "tokens": ["Bis", "der", "Ge\u00b7lieb\u00b7te", "wie\u00b7der\u00b7k\u00f6mmt", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leben und Geist sich in uns entz\u00fcndet.", "tokens": ["Le\u00b7ben", "und", "Geist", "sich", "in", "uns", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}