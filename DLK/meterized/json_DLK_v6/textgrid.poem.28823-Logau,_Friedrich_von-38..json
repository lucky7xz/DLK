{"textgrid.poem.28823": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Deutschen heissen sonst Garm\u00e4nner, und der Bart", "tokens": ["Die", "Deut\u00b7schen", "heis\u00b7sen", "sonst", "Gar\u00b7m\u00e4n\u00b7ner", ",", "und", "der", "Bart"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(hilt weiland man daf\u00fcr) vermehret m\u00e4nnlich Art.", "tokens": ["(", "hilt", "wei\u00b7land", "man", "da\u00b7f\u00fcr", ")", "ver\u00b7meh\u00b7ret", "m\u00e4nn\u00b7lich", "Art", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PIS", "PAV", "$(", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ietzt scheren wir den Bart so v\u00f6llig ab, so rein;", "tokens": ["Ietzt", "sche\u00b7ren", "wir", "den", "Bart", "so", "v\u00f6l\u00b7lig", "ab", ",", "so", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ey, wollen wir vielleicht Garweiber lieber seyn?", "tokens": ["Ey", ",", "wol\u00b7len", "wir", "viel\u00b7leicht", "Gar\u00b7wei\u00b7ber", "lie\u00b7ber", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VAINF", "$."], "meter": "-