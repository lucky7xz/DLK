{"textgrid.poem.35088": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tret in ein Gotteshaus ich ein,", "tokens": ["Tret", "in", "ein", "Got\u00b7tes\u00b7haus", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So soll es hell und freundlich sein.", "tokens": ["So", "soll", "es", "hell", "und", "freund\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dunkelheit, ich lieb sie nicht;", "tokens": ["Die", "Dun\u00b7kel\u00b7heit", ",", "ich", "lieb", "sie", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADJD", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will es um und in mir licht.", "tokens": ["Ich", "will", "es", "um", "und", "in", "mir", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "KON", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Tret in ein Gotteshaus ich ein,", "tokens": ["Tret", "in", "ein", "Got\u00b7tes\u00b7haus", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht dennoch es auch schattig sein.", "tokens": ["M\u00f6cht", "den\u00b7noch", "es", "auch", "schat\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "In Gottes Schatten ruht sich's lind;", "tokens": ["In", "Got\u00b7tes", "Schat\u00b7ten", "ruht", "sich's", "lind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich freue mich, wo ich ihn find.", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "wo", "ich", "ihn", "find", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Tret in ein Gotteshaus ich ein,", "tokens": ["Tret", "in", "ein", "Got\u00b7tes\u00b7haus", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll es des Vaters Haus mir sein.", "tokens": ["Soll", "es", "des", "Va\u00b7ters", "Haus", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn seine liebe Glocke schallt,", "tokens": ["Wenn", "sei\u00b7ne", "lie\u00b7be", "Glo\u00b7cke", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm ich gewi\u00df, komm alsobald.", "tokens": ["Komm", "ich", "ge\u00b7wi\u00df", ",", "komm", "al\u00b7so\u00b7bald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}