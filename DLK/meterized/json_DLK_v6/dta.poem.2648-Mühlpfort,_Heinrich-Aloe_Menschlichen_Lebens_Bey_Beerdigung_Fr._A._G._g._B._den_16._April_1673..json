{"dta.poem.2648": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aloe Menschlichen Lebens/  \n Bey Beerdigung Fr. A. G. g. B. den 16.  \n April 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es buhlet noch die Welt mit ihren sch\u00f6nen S\u00fcnden/", "tokens": ["Es", "buh\u00b7let", "noch", "die", "Welt", "mit", "ih\u00b7ren", "sch\u00f6\u00b7nen", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(wo Laster sch\u00f6ne sind/ und Tugend ungestalt)", "tokens": ["(", "wo", "Las\u00b7ter", "sch\u00f6\u00b7ne", "sind", "/", "und", "Tu\u00b7gend", "un\u00b7ge\u00b7stalt", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "ADJA", "VAFIN", "$(", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man wird die Aloe in solchem Mi\u00dfbrauch finden/", "tokens": ["Man", "wird", "die", "A\u00b7loe", "in", "sol\u00b7chem", "Mi\u00df\u00b7brauch", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als sie im Morgenland bey Uppigkeiten galt.", "tokens": ["Als", "sie", "im", "Mor\u00b7gen\u00b7land", "bey", "Up\u00b7pig\u00b7kei\u00b7ten", "galt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr Bette steht geschm\u00fcckt mit k\u00f6stlichen Tapeten/", "tokens": ["Ihr", "Bet\u00b7te", "steht", "ge\u00b7schm\u00fcckt", "mit", "k\u00f6st\u00b7li\u00b7chen", "Ta\u00b7pe\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Lager ist mit Myrrh und Aloe besprengt.", "tokens": ["Das", "La\u00b7ger", "ist", "mit", "Myrrh", "und", "A\u00b7loe", "be\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Auf! spricht ihr glatter Mund/ last uns die Sorgen t\u00f6dten/", "tokens": ["Auf", "!", "spricht", "ihr", "glat\u00b7ter", "Mund", "/", "last", "uns", "die", "Sor\u00b7gen", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil noch der Jugend-Strauch voll Purpur Rosen h\u00e4ngt!", "tokens": ["Weil", "noch", "der", "Ju\u00b7gend\u00b7Strauch", "voll", "Pur\u00b7pur", "Ro\u00b7sen", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Egypten nicht allein hat Schwelgerey getrieben/", "tokens": ["E\u00b7gyp\u00b7ten", "nicht", "al\u00b7lein", "hat", "Schwel\u00b7ge\u00b7rey", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zum Werckzeug s\u00fcsser Lust die Aloe gebraucht/", "tokens": ["Zum", "Wer\u00b7ck\u00b7zeug", "s\u00fcs\u00b7ser", "Lust", "die", "A\u00b7loe", "ge\u00b7braucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die sinds: Die in die Welt sich allzusehr verlieben/", "tokens": ["Die", "sinds", ":", "Die", "in", "die", "Welt", "sich", "all\u00b7zu\u00b7sehr", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "APPR", "ART", "NN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und derer Fleisch und Blut voll toller Br\u00fcnste raucht.", "tokens": ["Und", "de\u00b7rer", "Fleisch", "und", "Blut", "voll", "tol\u00b7ler", "Br\u00fcns\u00b7te", "raucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "KON", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es hat der Persier die G\u00f6tter mit vers\u00f6hnet/", "tokens": ["Es", "hat", "der", "Per\u00b7sier", "die", "G\u00f6t\u00b7ter", "mit", "ver\u00b7s\u00f6h\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und in die Opffer-Glut die Aloe gestreut.", "tokens": ["Und", "in", "die", "Opf\u00b7fer\u00b7Glut", "die", "A\u00b7loe", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Wo L\u00fcste G\u00f6tzen seyn/ so werden sie bekr\u00f6net/", "tokens": ["Wo", "L\u00fcs\u00b7te", "G\u00f6t\u00b7zen", "seyn", "/", "so", "wer\u00b7den", "sie", "be\u00b7kr\u00f6\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAINF", "$(", "ADV", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn unser Hertz und Sinn dem Zeitlichen sich weiht", "tokens": ["Wenn", "un\u00b7ser", "Hertz", "und", "Sinn", "dem", "Zeit\u00b7li\u00b7chen", "sich", "weiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Aloe wird alt/ eh\u2019 sie kan Fr\u00fcchte bringen/", "tokens": ["Die", "A\u00b7loe", "wird", "alt", "/", "eh'", "sie", "kan", "Fr\u00fcch\u00b7te", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "KOUS", "PPER", "VMFIN", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Viel Menschen die verbl\u00fchn im Leben ohne Frucht.", "tokens": ["Viel", "Men\u00b7schen", "die", "ver\u00b7bl\u00fchn", "im", "Le\u00b7ben", "oh\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJD", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Bl\u00fcth mu\u00df mit Geplatz au\u00df ihrer Knospe dringen:", "tokens": ["Die", "Bl\u00fcth", "mu\u00df", "mit", "Ge\u00b7platz", "au\u00df", "ih\u00b7rer", "Knos\u00b7pe", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von uns wird auch die Welt erst mit Geschrey besucht.", "tokens": ["Von", "uns", "wird", "auch", "die", "Welt", "erst", "mit", "Ge\u00b7schrey", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mehr ist die Aloe in sondrer Sch\u00e4rffe bitter:", "tokens": ["Mehr", "ist", "die", "A\u00b7loe", "in", "sond\u00b7rer", "Sch\u00e4rf\u00b7fe", "bit\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ach wie viel Bitterkeit qu\u00e4lt nicht de\u00df Lebens Ziel!", "tokens": ["Ach", "wie", "viel", "Bit\u00b7ter\u00b7keit", "qu\u00e4lt", "nicht", "de\u00df", "Le\u00b7bens", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "NN", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Pracht/ Hoffart/ Geitz und Neid/ die Stachel der Gem\u00fcther/", "tokens": ["Pracht", "/", "Hof\u00b7fart", "/", "Geitz", "und", "Neid", "/", "die", "Sta\u00b7chel", "der", "Ge\u00b7m\u00fc\u00b7ther", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Sind in uns/ da sie dort nur umb der Pflantze Stiel.", "tokens": ["Sind", "in", "uns", "/", "da", "sie", "dort", "nur", "umb", "der", "Pflant\u00b7ze", "Stiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So scheint die Aloe in den verkehrten Sinnen/", "tokens": ["So", "scheint", "die", "A\u00b7loe", "in", "den", "ver\u00b7kehr\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Wenn sie der Mi\u00dfbrauch nutzt/ und eitler Wahn erwegt;", "tokens": ["Wenn", "sie", "der", "Mi\u00df\u00b7brauch", "nutzt", "/", "und", "eit\u00b7ler", "Wahn", "er\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da wir die W\u00fcrckung doch viel anders finden k\u00f6nnen/", "tokens": ["Da", "wir", "die", "W\u00fcr\u00b7ckung", "doch", "viel", "an\u00b7ders", "fin\u00b7den", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil ihr de\u00df HErren Wort weit be\u00dfre Krafft zulegt.", "tokens": ["Weil", "ihr", "de\u00df", "Her\u00b7ren", "Wort", "weit", "be\u00df\u00b7re", "Krafft", "zu\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "R\u00fchmt nicht der Br\u00e4utigam der Sulamithin Garten?", "tokens": ["R\u00fchmt", "nicht", "der", "Br\u00e4u\u00b7ti\u00b7gam", "der", "Su\u00b7la\u00b7mi\u00b7thin", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "Wo ein versiegelt Brunn/ Gew\u00e4chse voller Zier/", "tokens": ["Wo", "ein", "ver\u00b7sie\u00b7gelt", "Brunn", "/", "Ge\u00b7w\u00e4ch\u00b7se", "vol\u00b7ler", "Zier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVFIN", "NE", "$(", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo die Granaten stehn/ und Fr\u00fcchte von viel Arten/", "tokens": ["Wo", "die", "Gra\u00b7na\u00b7ten", "stehn", "/", "und", "Fr\u00fcch\u00b7te", "von", "viel", "Ar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$(", "KON", "NN", "APPR", "PIAT", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Wo Myrrh und Aloe geht andern W\u00fcrtzen f\u00fcr.", "tokens": ["Wo", "Myrrh", "und", "A\u00b7loe", "geht", "an\u00b7dern", "W\u00fcrt\u00b7zen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "VVFIN", "ADJA", "NN", "APPR", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Ja ist nicht seine Braut mit Aloe gekleidet?", "tokens": ["Ja", "ist", "nicht", "sei\u00b7ne", "Braut", "mit", "A\u00b7loe", "ge\u00b7klei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Und l\u00e4st er seinen Leib darmit nicht salben ein?", "tokens": ["Und", "l\u00e4st", "er", "sei\u00b7nen", "Leib", "dar\u00b7mit", "nicht", "sal\u00b7ben", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PAV", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weil diese Pflantze nun nicht Stanck noch F\u00e4uln\u00fc\u00df leidet/", "tokens": ["Weil", "die\u00b7se", "Pflant\u00b7ze", "nun", "nicht", "Stanck", "noch", "F\u00e4ul\u00b7n\u00fc\u00df", "lei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Kan sie der Sterbligkeit ein klarer Spiegel seyn.", "tokens": ["Kan", "sie", "der", "Ster\u00b7blig\u00b7keit", "ein", "kla\u00b7rer", "Spie\u00b7gel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es darff die neue Welt uns selbte nicht gewehren/", "tokens": ["Es", "darff", "die", "neu\u00b7e", "Welt", "uns", "selb\u00b7te", "nicht", "ge\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Soccotra wird hierumb/ noch Spanien begr\u00fcst.", "tokens": ["Soc\u00b7cot\u00b7ra", "wird", "hie\u00b7rumb", "/", "noch", "Spa\u00b7ni\u00b7en", "be\u00b7gr\u00fcst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PAV", "$(", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nein unser Schlesien erf\u00fcllet das Begehren/", "tokens": ["Nein", "un\u00b7ser", "Schle\u00b7si\u00b7en", "er\u00b7f\u00fcl\u00b7let", "das", "Be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So aller Fruchtbarkeit gesegnet Schatz-Haus ist.", "tokens": ["So", "al\u00b7ler", "Frucht\u00b7bar\u00b7keit", "ge\u00b7seg\u00b7net", "Schatz\u00b7Haus", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie hatte dreyssig Jahr sich zur Geburt geschicket/", "tokens": ["Sie", "hat\u00b7te", "dreys\u00b7sig", "Jahr", "sich", "zur", "Ge\u00b7burt", "ge\u00b7schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"],