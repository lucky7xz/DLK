{"textgrid.poem.42577": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Am 18. Oktober 1815", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schlacht der V\u00f6lker ward geschlagen,", "tokens": ["Die", "Schlacht", "der", "V\u00f6l\u00b7ker", "ward", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fremde wich von deutscher Flur,", "tokens": ["Der", "Frem\u00b7de", "wich", "von", "deut\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch die befreiten Lande tragen", "tokens": ["Doch", "die", "be\u00b7frei\u00b7ten", "Lan\u00b7de", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch manches vor'gen Dranges Spur;", "tokens": ["Noch", "man\u00b7ches", "vor'\u00b7gen", "Dran\u00b7ges", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie man aus versunknen St\u00e4dten", "tokens": ["Und", "wie", "man", "aus", "ver\u00b7sun\u00b7knen", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erhabne G\u00f6tterbilder gr\u00e4bt,", "tokens": ["Er\u00b7hab\u00b7ne", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ist manch heilig Recht zu retten,", "tokens": ["So", "ist", "manch", "hei\u00b7lig", "Recht", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das unter w\u00fcsten Tr\u00fcmmern lebt.", "tokens": ["Das", "un\u00b7ter", "w\u00fcs\u00b7ten", "Tr\u00fcm\u00b7mern", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu retten gilt's und aufzubauen,", "tokens": ["Zu", "ret\u00b7ten", "gilt's", "und", "auf\u00b7zu\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch das Gedeihen bleibet fern,", "tokens": ["Doch", "das", "Ge\u00b7dei\u00b7hen", "blei\u00b7bet", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Liebe fehlet und Vertrauen", "tokens": ["Wo", "Lie\u00b7be", "feh\u00b7let", "und", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Eintracht zwischen Volk und Herrn.", "tokens": ["Und", "Ein\u00b7tracht", "zwi\u00b7schen", "Volk", "und", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Deutsche ehrt' in allen Zeiten", "tokens": ["Der", "Deut\u00b7sche", "ehrt'", "in", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der F\u00fcrsten heiligen Beruf,", "tokens": ["Der", "F\u00fcrs\u00b7ten", "hei\u00b7li\u00b7gen", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch liebt er frei einherzuschreiten", "tokens": ["Doch", "liebt", "er", "frei", "ein\u00b7her\u00b7zu\u00b7schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und aufrecht, wie ihn Gott erschuf.", "tokens": ["Und", "auf\u00b7recht", ",", "wie", "ihn", "Gott", "er\u00b7schuf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So wirkt auch ihr im festen Bunde,", "tokens": ["So", "wirkt", "auch", "ihr", "im", "fes\u00b7ten", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr guten H\u00fcter unsres Rechts!", "tokens": ["Ihr", "gu\u00b7ten", "H\u00fc\u00b7ter", "uns\u00b7res", "Rechts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr bauet auf dem alten Grunde", "tokens": ["Ihr", "bau\u00b7et", "auf", "dem", "al\u00b7ten", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wohl des k\u00fcnftigen Geschlechts.", "tokens": ["Das", "Wohl", "des", "k\u00fcnf\u00b7ti\u00b7gen", "Ge\u00b7schlechts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uneingedenk gemeinen Lohnes,", "tokens": ["Un\u00b7ein\u00b7ge\u00b7denk", "ge\u00b7mei\u00b7nen", "Loh\u00b7nes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seid ihr beharrlich, emsig, treu;", "tokens": ["Seid", "ihr", "be\u00b7harr\u00b7lich", ",", "em\u00b7sig", ",", "treu", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Volkes W\u00fcrde wie des Thrones", "tokens": ["Des", "Vol\u00b7kes", "W\u00fcr\u00b7de", "wie", "des", "Thro\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beachtet ihr mit heil'ger Scheu.", "tokens": ["Be\u00b7ach\u00b7tet", "ihr", "mit", "heil'\u00b7ger", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum, da wir heut das Fest begehen,", "tokens": ["Drum", ",", "da", "wir", "heut", "das", "Fest", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem tausend Freudenfeuer spr\u00fchn", "tokens": ["Dem", "tau\u00b7send", "Freu\u00b7den\u00b7feu\u00b7er", "spr\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wo sie nicht von Bergen wehen,", "tokens": ["Und", ",", "wo", "sie", "nicht", "von", "Ber\u00b7gen", "we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch tief in allen Herzen gl\u00fchn:", "tokens": ["Doch", "tief", "in", "al\u00b7len", "Her\u00b7zen", "gl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was kann so edlen Schmuck gew\u00e4hren", "tokens": ["Was", "kann", "so", "ed\u00b7len", "Schmuck", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Mahle, das uns hier vereint,", "tokens": ["Dem", "Mah\u00b7le", ",", "das", "uns", "hier", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als einen Mann bei uns zu ehren,", "tokens": ["Als", "ei\u00b7nen", "Mann", "bei", "uns", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der's so getreulich mit uns meint!", "tokens": ["Der's", "so", "ge\u00b7treu\u00b7lich", "mit", "uns", "meint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Den Mann, der, unsrer Stadt entsprossen,", "tokens": ["Den", "Mann", ",", "der", ",", "uns\u00b7rer", "Stadt", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stets ihres Wohles treu gedacht,", "tokens": ["Stets", "ih\u00b7res", "Woh\u00b7les", "treu", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem wir uns innig angeschlossen,", "tokens": ["Dem", "wir", "uns", "in\u00b7nig", "an\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der unser Teuerstes bewacht;", "tokens": ["Der", "un\u00b7ser", "Teu\u00b7ers\u00b7tes", "be\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der unersch\u00fcttert ausgehalten", "tokens": ["Der", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "aus\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Sturm der schreckensvollen Zeit", "tokens": ["Im", "Sturm", "der", "schre\u00b7ckens\u00b7vol\u00b7len", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und der auch jetzt mit kr\u00e4ft'gem Walten", "tokens": ["Und", "der", "auch", "jetzt", "mit", "kr\u00e4ft'\u00b7gem", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem neuen Werk sein Leben weiht!", "tokens": ["Dem", "neu\u00b7en", "Werk", "sein", "Le\u00b7ben", "weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nie kommt das Wort, ihr treuen V\u00e4ter!", "tokens": ["Nie", "kommt", "das", "Wort", ",", "ihr", "treu\u00b7en", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem hei\u00dfen Herzensdanke gleich,", "tokens": ["Dem", "hei\u00b7\u00dfen", "Her\u00b7zens\u00b7dan\u00b7ke", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie spricht es aus, ihr Volksvertreter!", "tokens": ["Nie", "spricht", "es", "aus", ",", "ihr", "Volks\u00b7ver\u00b7tre\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wir so eines sind mit euch.", "tokens": ["Wie", "wir", "so", "ei\u00b7nes", "sind", "mit", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIS", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als j\u00fcngst in hehren Tempelhallen", "tokens": ["Als", "j\u00fcngst", "in", "heh\u00b7ren", "Tem\u00b7pel\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Menge sich mit euch erbaut,", "tokens": ["Die", "Men\u00b7ge", "sich", "mit", "euch", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sprach das Schweigen \u00fcber allen", "tokens": ["Da", "sprach", "das", "Schwei\u00b7gen", "\u00fc\u00b7ber", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mehr als der hellste Jubellaut.", "tokens": ["Mehr", "als", "der", "hells\u00b7te", "Ju\u00b7bel\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So la\u00df dir's, Edler, denn gefallen", "tokens": ["So", "la\u00df", "dir's", ",", "Ed\u00b7ler", ",", "denn", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "NE", "$,", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei unsrem fr\u00f6hlichen Gelag,", "tokens": ["Bei", "uns\u00b7rem", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will dich d\u00fcstrer Ernst umwallen,", "tokens": ["Und", "will", "dich", "d\u00fcst\u00b7rer", "Ernst", "um\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So denk an k\u00fcnft'gen Festestag:", "tokens": ["So", "denk", "an", "k\u00fcnft'\u00b7gen", "Fes\u00b7tes\u00b7tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann jener Schlacht Gewittersegen", "tokens": ["Wann", "je\u00b7ner", "Schlacht", "Ge\u00b7wit\u00b7ter\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sichtbar auch unser Heil erneut,", "tokens": ["Sicht\u00b7bar", "auch", "un\u00b7ser", "Heil", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann sich die Saaten schwellend regen,", "tokens": ["Wann", "sich", "die", "Saa\u00b7ten", "schwel\u00b7lend", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ihr im ", "tokens": ["Die", "ihr", "im"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}