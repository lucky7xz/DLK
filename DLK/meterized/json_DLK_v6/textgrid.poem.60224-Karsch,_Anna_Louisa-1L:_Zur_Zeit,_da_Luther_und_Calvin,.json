{"textgrid.poem.60224": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zur Zeit, da Luther und Calvin,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Zeit, da Luther und Calvin,", "tokens": ["Zur", "Zeit", ",", "da", "Lu\u00b7ther", "und", "Cal\u00b7vin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "NE", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Gott ger\u00fcstet, sich bestrebten,", "tokens": ["Von", "Gott", "ge\u00b7r\u00fcs\u00b7tet", ",", "sich", "be\u00b7streb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die armen Menschen, die in dicker Blindheit lebten,", "tokens": ["Die", "ar\u00b7men", "Men\u00b7schen", ",", "die", "in", "di\u00b7cker", "Blind\u00b7heit", "leb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Aberglauben abzuziehn:", "tokens": ["Vom", "A\u00b7berg\u00b7lau\u00b7ben", "ab\u00b7zu\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da war ein Wunderbild, geschm\u00fcckt wie Kayserinnen.", "tokens": ["Da", "war", "ein", "Wun\u00b7der\u00b7bild", ",", "ge\u00b7schm\u00fcckt", "wie", "Kay\u00b7ser\u00b7in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lahmen beteten: Frau! heile meinen Fu\u00df!", "tokens": ["Die", "Lah\u00b7men", "be\u00b7te\u00b7ten", ":", "Frau", "!", "hei\u00b7le", "mei\u00b7nen", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Der taubgewordne gab der Erde manchen Ku\u00df,", "tokens": ["Der", "taub\u00b7ge\u00b7word\u00b7ne", "gab", "der", "Er\u00b7de", "man\u00b7chen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Um sein Geh\u00f6r hier wieder zu gewinnen;", "tokens": ["Um", "sein", "Ge\u00b7h\u00f6r", "hier", "wie\u00b7der", "zu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das unfruchtbare Weib verlie\u00df den alten Mann,", "tokens": ["Das", "un\u00b7frucht\u00b7ba\u00b7re", "Weib", "ver\u00b7lie\u00df", "den", "al\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und stellte grosse Wallfahrt an,", "tokens": ["Und", "stell\u00b7te", "gros\u00b7se", "Wall\u00b7fahrt", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit frommen J\u00fcnglingen, die auf der Mutter Rathen,", "tokens": ["Mit", "from\u00b7men", "J\u00fcng\u00b7lin\u00b7gen", ",", "die", "auf", "der", "Mut\u00b7ter", "Ra\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Bey diesem Gnadenbild um gute Weiber baten,", "tokens": ["Bey", "die\u00b7sem", "Gna\u00b7den\u00b7bild", "um", "gu\u00b7te", "Wei\u00b7ber", "ba\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die man so schwer erbitten kann!", "tokens": ["Die", "man", "so", "schwer", "er\u00b7bit\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An einem Fasttag kniete nieder", "tokens": ["An", "ei\u00b7nem", "Fast\u00b7tag", "knie\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ganzes Volk um den Altar.", "tokens": ["Ein", "gan\u00b7zes", "Volk", "um", "den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sangen Hymnen, sangen Lieder,", "tokens": ["Sie", "san\u00b7gen", "Hym\u00b7nen", ",", "san\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und an die Brust schlug sich, wer recht and\u00e4chtig war;", "tokens": ["Und", "an", "die", "Brust", "schlug", "sich", ",", "wer", "recht", "an\u00b7d\u00e4ch\u00b7tig", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "$,", "PWS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Am l\u00e4ngsten blieb zu ihrem Fusse", "tokens": ["Am", "l\u00e4ngs\u00b7ten", "blieb", "zu", "ih\u00b7rem", "Fus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein armer b\u00e4rtiger Soldat,", "tokens": ["Ein", "ar\u00b7mer", "b\u00e4r\u00b7ti\u00b7ger", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sie vielleicht im Thon der Busse", "tokens": ["Der", "sie", "viel\u00b7leicht", "im", "Thon", "der", "Bus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr seiner Jugend Schuld zur Mittlerin erbath.", "tokens": ["F\u00fcr", "sei\u00b7ner", "Ju\u00b7gend", "Schuld", "zur", "Mitt\u00b7le\u00b7rin", "er\u00b7bath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er ganz allein hat da gelegen,", "tokens": ["Er", "ganz", "al\u00b7lein", "hat", "da", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als schon die Priester allen Segen", "tokens": ["Als", "schon", "die", "Pries\u00b7ter", "al\u00b7len", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und allen Abla\u00df ausgetheilt,", "tokens": ["Und", "al\u00b7len", "Ab\u00b7la\u00df", "aus\u00b7ge\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und dann zum fetten Mahl und guten Wein geeilt.", "tokens": ["Und", "dann", "zum", "fet\u00b7ten", "Mahl", "und", "gu\u00b7ten", "Wein", "ge\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Tag ward zugebracht mit Freuden,", "tokens": ["Der", "Tag", "ward", "zu\u00b7ge\u00b7bracht", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und an dem andern Morgen fr\u00fch", "tokens": ["Und", "an", "dem", "an\u00b7dern", "Mor\u00b7gen", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gieng, unsre liebe Frau, ein Priester umzukleiden;", "tokens": ["Gieng", ",", "uns\u00b7re", "lie\u00b7be", "Frau", ",", "ein", "Pries\u00b7ter", "um\u00b7zu\u00b7klei\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn mehr als funfzig Kleider hatte sie.", "tokens": ["Denn", "mehr", "als", "funf\u00b7zig", "Klei\u00b7der", "hat\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "CARD", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "F\u00fcr Schrecken fuhr der Priester ganz zusammen.", "tokens": ["F\u00fcr", "Schre\u00b7cken", "fuhr", "der", "Pries\u00b7ter", "ganz", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbden frechen Dieb soll Gott verdammen!", "tokens": ["\u00bb", "den", "fre\u00b7chen", "Dieb", "soll", "Gott", "ver\u00b7dam\u00b7men", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hier fehlet eine Perlen-Schnur!\u00ab", "tokens": ["Hier", "feh\u00b7let", "ei\u00b7ne", "Per\u00b7len\u00b7Schnur", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So schrie er, als sein Herz in ihm zusammenfuhr:", "tokens": ["So", "schrie", "er", ",", "als", "sein", "Herz", "in", "ihm", "zu\u00b7sam\u00b7men\u00b7fuhr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es wurde nachgeforscht, und endlich ward befunden,", "tokens": ["Es", "wur\u00b7de", "nach\u00b7ge\u00b7forscht", ",", "und", "end\u00b7lich", "ward", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df lange nach den Andachts-Stunden,", "tokens": ["Da\u00df", "lan\u00b7ge", "nach", "den", "An\u00b7dachts\u00b7Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Noch ein Soldat vor ihr gekniet.", "tokens": ["Noch", "ein", "Sol\u00b7dat", "vor", "ihr", "ge\u00b7kniet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Er wird geholt; er kommt gebunden;", "tokens": ["Er", "wird", "ge\u00b7holt", ";", "er", "kommt", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und als er nun die Richter sieht,", "tokens": ["Und", "als", "er", "nun", "die", "Rich\u00b7ter", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "So spricht er: \u00bbJa! ich l\u00e4ugne nicht, zu haben", "tokens": ["So", "spricht", "er", ":", "\u00bb", "Ja", "!", "ich", "l\u00e4ug\u00b7ne", "nicht", ",", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die theure Perlen-Schnur. Doch ihre H\u00e4nde gaben", "tokens": ["Die", "theu\u00b7re", "Per\u00b7len\u00b7Schnur", ".", "Doch", "ih\u00b7re", "H\u00e4n\u00b7de", "ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mir selber diesen Schatz. Ich bin ein armer Mann", "tokens": ["Mir", "sel\u00b7ber", "die\u00b7sen", "Schatz", ".", "Ich", "bin", "ein", "ar\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PDAT", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Weib und Kinder hat, und sie nicht n\u00e4hren kann.", "tokens": ["Der", "Weib", "und", "Kin\u00b7der", "hat", ",", "und", "sie", "nicht", "n\u00e4h\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "$,", "KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich h\u00f6rte, da\u00df dies Bild so viele Wunder th\u00e4te,", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "da\u00df", "dies", "Bild", "so", "vie\u00b7le", "Wun\u00b7der", "th\u00e4\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDS", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Drum lieg ich lange da, und bete:", "tokens": ["Drum", "lieg", "ich", "lan\u00b7ge", "da", ",", "und", "be\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Ach! hilf mir liebe Frau! wenn du begabet bist", "tokens": ["Ach", "!", "hilf", "mir", "lie\u00b7be", "Frau", "!", "wenn", "du", "be\u00b7ga\u00b7bet", "bist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "VVFIN", "NN", "$.", "KOUS", "PPER", "VVFIN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mit solcher Gotteskraft auf Erden!", "tokens": ["Mit", "sol\u00b7cher", "Got\u00b7tes\u00b7kraft", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Mir hilft kein r\u00f6mischer, catholisch-guter Christ.", "tokens": ["Mir", "hilft", "kein", "r\u00f6\u00b7mi\u00b7scher", ",", "ca\u00b7tho\u00b7lischgu\u00b7ter", "Christ", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.35": {"text": "Wenn du nicht hilfst, so mu\u00df ich werden", "tokens": ["Wenn", "du", "nicht", "hilfst", ",", 