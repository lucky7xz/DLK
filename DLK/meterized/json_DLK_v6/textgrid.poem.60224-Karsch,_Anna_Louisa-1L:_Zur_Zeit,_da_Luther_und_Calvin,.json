tes langte", "tokens": ["Das", "gros\u00b7se", "Wun\u00b7der\u00b7werck", ".", "Die", "Mut\u00b7ter", "Got\u00b7tes", "lang\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mir diese Perlen-Schnur, die an dem Halse prangte,", "tokens": ["Mir", "die\u00b7se", "Per\u00b7len\u00b7Schnur", ",", "die", "an", "dem", "Hal\u00b7se", "prang\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Mit ihrer starken Hand herab,", "tokens": ["Mit", "ih\u00b7rer", "star\u00b7ken", "Hand", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Und sprach, indem sie mir sie gab:", "tokens": ["Und", "sprach", ",", "in\u00b7dem", "sie", "mir", "sie", "gab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Geh hin, und kaufe Brodt f\u00fcr Weib und Kinder!", "tokens": ["Geh", "hin", ",", "und", "kau\u00b7fe", "Brodt", "f\u00fcr", "Weib", "und", "Kin\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KON", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Nur werde kein verlohrner S\u00fcnder,", "tokens": ["Nur", "wer\u00b7de", "kein", "ver\u00b7lohr\u00b7ner", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Lauf niemals aus der Kirche Schoo\u00df!", "tokens": ["Lauf", "nie\u00b7mals", "aus", "der", "Kir\u00b7che", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Sie sprachs: Die Heiligen sind alle meine Zeugen;\u00ab", "tokens": ["Sie", "sprachs", ":", "Die", "Hei\u00b7li\u00b7gen", "sind", "al\u00b7le", "mei\u00b7ne", "Zeu\u00b7gen", ";", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die Richter h\u00f6rten dies, und alle mu\u00dften schweigen.", "tokens": ["Die", "Rich\u00b7ter", "h\u00f6r\u00b7ten", "dies", ",", "und", "al\u00b7le", "mu\u00df\u00b7ten", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "$,", "KON", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die Priester riefen aus: \u00bb", "tokens": ["Die", "Pries\u00b7ter", "rie\u00b7fen", "aus", ":", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}