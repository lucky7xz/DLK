{"textgrid.poem.24652": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Betr\u00fcbte und getr\u00f6stete Liebe/ bey dem Absterben eines Vaters", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiewohl der Eltern Hertz die Kinder st\u00e4rcker lieben/", "tokens": ["Wie\u00b7wohl", "der", "El\u00b7tern", "Hertz", "die", "Kin\u00b7der", "st\u00e4r\u00b7cker", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Sie derselben Tod auch mehr betr\u00fcben soll:", "tokens": ["Und", "Sie", "der\u00b7sel\u00b7ben", "Tod", "auch", "mehr", "be\u00b7tr\u00fc\u00b7ben", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDAT", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So macht des Vaters Huld/ die in mein Hertz geschrieben/", "tokens": ["So", "macht", "des", "Va\u00b7ters", "Huld", "/", "die", "in", "mein", "Hertz", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "ART", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch einer Tochter Brust itzt wieder Jammers voll/", "tokens": ["Doch", "ei\u00b7ner", "Toch\u00b7ter", "Brust", "itzt", "wie\u00b7der", "Jam\u00b7mers", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "ADV", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df Thr\u00e4nen sonder Zahl sie bey der Grufft vergiesset/", "tokens": ["Da\u00df", "Thr\u00e4\u00b7nen", "son\u00b7der", "Zahl", "sie", "bey", "der", "Grufft", "ver\u00b7gies\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil wahre Traurigkeit aus wahrer Liebe fliesset.", "tokens": ["Weil", "wah\u00b7re", "Trau\u00b7rig\u00b7keit", "aus", "wah\u00b7rer", "Lie\u00b7be", "flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wohlthat ist zu gro\u00df/ die theure V\u00e4ter geben/", "tokens": ["Die", "Wohlt\u00b7hat", "ist", "zu", "gro\u00df", "/", "die", "theu\u00b7re", "V\u00e4\u00b7ter", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie mein danckbar Hertz auch nach dem Tod verehrt.", "tokens": ["Da\u00df", "sie", "mein", "dan\u00b7ck\u00b7bar", "Hertz", "auch", "nach", "dem", "Tod", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gott gab dem Seeligen darum ein langes Leben/", "tokens": ["Gott", "gab", "dem", "See\u00b7li\u00b7gen", "da\u00b7rum", "ein", "lan\u00b7ges", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PAV", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Weil di\u00df den s\u00fcssen Trost in frommen Kindern mehrt:", "tokens": ["Weil", "di\u00df", "den", "s\u00fcs\u00b7sen", "Trost", "in", "from\u00b7men", "Kin\u00b7dern", "mehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er lie\u00df uns erst das Gl\u00fcck durch seine Sorgfalt schauen/", "tokens": ["Er", "lie\u00df", "uns", "erst", "das", "Gl\u00fcck", "durch", "sei\u00b7ne", "Sorg\u00b7falt", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wolt im Alter uns durch Gottesfurcht erbauen.", "tokens": ["Und", "wolt", "im", "Al\u00b7ter", "uns", "durch", "Got\u00b7tes\u00b7furcht", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein langes Leben wird doch gleichwohl kurtz gesch\u00e4tzet/", "tokens": ["Ein", "lan\u00b7ges", "Le\u00b7ben", "wird", "doch", "gleich\u00b7wohl", "kurtz", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seiner G\u00fcte nach noch l\u00e4nger k\u00f6nnen seyn.", "tokens": ["Das", "sei\u00b7ner", "G\u00fc\u00b7te", "nach", "noch", "l\u00e4n\u00b7ger", "k\u00f6n\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn mich nun die Natur in tiefes Trauren setzet/", "tokens": ["Wenn", "mich", "nun", "die", "Na\u00b7tur", "in", "tie\u00b7fes", "Trau\u00b7ren", "set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So nimmt die Tugend mich mit diesen Trost-Spruch ein:", "tokens": ["So", "nimmt", "die", "Tu\u00b7gend", "mich", "mit", "die\u00b7sen", "Trost\u00b7Spruch", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df/ wenn der Eltern Tod der Kinder Hertz will brechen/", "tokens": ["Da\u00df", "/", "wenn", "der", "El\u00b7tern", "Tod", "der", "Kin\u00b7der", "Hertz", "will", "bre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "ART", "NN", "NN", "ART", "NN", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Eltern Seeligkeit sie kan zufrieden sprechen.", "tokens": ["Der", "El\u00b7tern", "See\u00b7lig\u00b7keit", "sie", "kan", "zu\u00b7frie\u00b7den", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich liebt' ich/ Seeligster/ nun dich dein Jesus liebet/", "tokens": ["Dich", "liebt'", "ich", "/", "See\u00b7ligs\u00b7ter", "/", "nun", "dich", "dein", "Je\u00b7sus", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NN", "$(", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dessen Armen man dich in dem Sterben fand:", "tokens": ["In", "des\u00b7sen", "Ar\u00b7men", "man", "dich", "in", "dem", "Ster\u00b7ben", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIS", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lieb ich dich noch mehr/ weil der sich dir ergiebet/", "tokens": ["So", "lieb", "ich", "dich", "noch", "mehr", "/", "weil", "der", "sich", "dir", "er\u00b7gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "ADV", "ADV", "$(", "KOUS", "ART", "PRF", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der meine Seele hat in seiner Lieb' entbrandt.", "tokens": ["Der", "mei\u00b7ne", "See\u00b7le", "hat", "in", "sei\u00b7ner", "Lieb'", "ent\u00b7brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als Vater bleibst du mir in meine Brust geschrieben/", "tokens": ["Als", "Va\u00b7ter", "bleibst", "du", "mir", "in", "mei\u00b7ne", "Brust", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als einen Seeligen will ich dich ewig lieben.", "tokens": ["Als", "ei\u00b7nen", "See\u00b7li\u00b7gen", "will", "ich", "dich", "e\u00b7wig", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}}}}