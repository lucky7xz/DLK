{"textgrid.poem.36646": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnftes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als allerliebste B\u00e4urin", "tokens": ["Und", "als", "al\u00b7ler\u00b7liebs\u00b7te", "B\u00e4u\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Naht sie mir des andern Tages,", "tokens": ["Naht", "sie", "mir", "des", "an\u00b7dern", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern neckte Stab und Glocke,", "tokens": ["Ge\u00b7stern", "neck\u00b7te", "Stab", "und", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heut' ein artig Blumenk\u00f6rbchen,", "tokens": ["Heut'", "ein", "ar\u00b7tig", "Blu\u00b7men\u00b7k\u00f6rb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im wei\u00dfen Seidenhemde", "tokens": ["Und", "im", "wei\u00b7\u00dfen", "Sei\u00b7den\u00b7hem\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00fcpft heran die wohl erkannte", "tokens": ["H\u00fcpft", "he\u00b7ran", "die", "wohl", "er\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "ART", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00fcsterne Begleiterin", "tokens": ["L\u00fcs\u00b7ter\u00b7ne", "Be\u00b7glei\u00b7te\u00b7rin"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem wilden Tamburine.", "tokens": ["Mit", "dem", "wil\u00b7den", "Tam\u00b7bu\u00b7ri\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Voller dr\u00e4ngt sich's heut als gestern,", "tokens": ["Vol\u00b7ler", "dr\u00e4ngt", "sich's", "heut", "als", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "KOKOM", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und von tausend lust'gen B\u00e4chen", "tokens": ["Und", "von", "tau\u00b7send", "lust'\u00b7gen", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt vergr\u00f6\u00dfert, jauchzt und sch\u00e4umet", "tokens": ["Jetzt", "ver\u00b7gr\u00f6\u00b7\u00dfert", ",", "jauchzt", "und", "sch\u00e4u\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun der Strom des Bacchanals;", "tokens": ["Nun", "der", "Strom", "des", "Bac\u00b7cha\u00b7nals", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, der Gott ist im Gefolge", "tokens": ["Ja", ",", "der", "Gott", "ist", "im", "Ge\u00b7fol\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seiner taumelnden M\u00e4naden", "tokens": ["Sei\u00b7ner", "tau\u00b7meln\u00b7den", "M\u00e4\u00b7na\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selbst gekommen, um dem Volk", "tokens": ["Selbst", "ge\u00b7kom\u00b7men", ",", "um", "dem", "Volk"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ganz die Sinne zu ber\u00fccken.", "tokens": ["Ganz", "die", "Sin\u00b7ne", "zu", "be\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht die schreienden Doctoren,", "tokens": ["Seht", "die", "schrei\u00b7en\u00b7den", "Doc\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie ihre Weisheit pred'gen,", "tokens": ["Wie", "sie", "ih\u00b7re", "Weis\u00b7heit", "pre\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Einem h\u00fcbschen Schelmenkinde", "tokens": ["Ei\u00b7nem", "h\u00fcb\u00b7schen", "Schel\u00b7men\u00b7kin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier den zarten Puls bef\u00fchlen,", "tokens": ["Hier", "den", "zar\u00b7ten", "Puls", "be\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6rderische Instrumente,", "tokens": ["M\u00f6r\u00b7de\u00b7ri\u00b7sche", "Inst\u00b7ru\u00b7men\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6stliche Arzneien zeigen,", "tokens": ["K\u00f6st\u00b7li\u00b7che", "Arz\u00b7nei\u00b7en", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Apotheker sich", "tokens": ["Wie", "der", "A\u00b7pot\u00b7he\u00b7ker", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Durch des M\u00f6rsers Schall verk\u00fcndet.", "tokens": ["Durch", "des", "M\u00f6r\u00b7sers", "Schall", "ver\u00b7k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier wird ein Proce\u00df geschlichtet,", "tokens": ["Hier", "wird", "ein", "Pro\u00b7ce\u00df", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort ein anderer verwickelt;", "tokens": ["Dort", "ein", "an\u00b7de\u00b7rer", "ver\u00b7wi\u00b7ckelt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der jungen Eheh\u00e4lfte", "tokens": ["Mit", "der", "jun\u00b7gen", "E\u00b7he\u00b7h\u00e4lf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt sich der Papa im Schlafrock,", "tokens": ["Zeigt", "sich", "der", "Pa\u00b7pa", "im", "Schla\u00b7frock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Schalk, der Pulcinella,", "tokens": ["Und", "der", "Schalk", ",", "der", "Pul\u00b7ci\u00b7nel\u00b7la", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ueber seine Schulter guckt er", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "Schul\u00b7ter", "guckt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon mit einem Horn und setzt", "tokens": ["Schon", "mit", "ei\u00b7nem", "Horn", "und", "setzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihm aufs Haupt die Narrenkappe.", "tokens": ["Ihm", "aufs", "Haupt", "die", "Nar\u00b7ren\u00b7kap\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wandelnde Museen lassen", "tokens": ["Wan\u00b7deln\u00b7de", "Mu\u00b7se\u00b7en", "las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Ihre Rarit\u00e4ten sehen,", "tokens": ["Ih\u00b7re", "Ra\u00b7ri\u00b7t\u00e4\u00b7ten", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen B\u00fcndel Maccaroni", "tokens": ["Sei\u00b7nen", "B\u00fcn\u00b7del", "Mac\u00b7ca\u00b7ro\u00b7ni"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Speist aus dem geheimen Topfe", "tokens": ["Speist", "aus", "dem", "ge\u00b7hei\u00b7men", "Top\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Bajaccio, jener Kutscher", "tokens": ["Der", "Ba\u00b7ja\u00b7ccio", ",", "je\u00b7ner", "Kut\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "$,", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tr\u00e4gt die Windm\u00fchl' auf dem Hute;", "tokens": ["Tr\u00e4gt", "die", "Wind\u00b7m\u00fchl'", "auf", "dem", "Hu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und am Zopfe flattert dem", "tokens": ["Und", "am", "Zop\u00b7fe", "flat\u00b7tert", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Gar ein Dutzend Distelfinken.", "tokens": ["Gar", "ein", "Dut\u00b7zend", "Dis\u00b7tel\u00b7fin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Im zerlumpten Bettlerrocke,", "tokens": ["Im", "zer\u00b7lump\u00b7ten", "Bett\u00b7ler\u00b7ro\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gewalt'gem Lorbeerkranze", "tokens": ["Und", "ge\u00b7walt'\u00b7gem", "Lor\u00b7beer\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelt der Poet. Da ruft es:", "tokens": ["Wan\u00b7delt", "der", "Po\u00b7et", ".", "Da", "ruft", "es", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Platz gemacht! und mit der Brille,", "tokens": ["Platz", "ge\u00b7macht", "!", "und", "mit", "der", "Bril\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Perr\u00fccke Lockenturme", "tokens": ["Der", "Per\u00b7r\u00fc\u00b7cke", "Lo\u00b7cken\u00b7tur\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Kommt der Graf einhergeschritten,", "tokens": ["Kommt", "der", "Graf", "ein\u00b7her\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die derbe R\u00f6merwurst", "tokens": ["Und", "die", "der\u00b7be", "R\u00f6\u00b7mer\u00b7wurst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Guckt ihm aus der Seitentasche.", "tokens": ["Guckt", "ihm", "aus", "der", "Sei\u00b7ten\u00b7ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu des Dudelsackes Schnarren", "tokens": ["Zu", "des", "Du\u00b7del\u00b7sa\u00b7ckes", "Schnar\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt hier der Campagnenbauer", "tokens": ["Singt", "hier", "der", "Cam\u00b7pag\u00b7nen\u00b7bau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohlerfundne Ritornelle", "tokens": ["Woh\u00b7ler\u00b7fund\u00b7ne", "Ri\u00b7tor\u00b7nel\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenen Damen an dem Fenster;", "tokens": ["Je\u00b7nen", "Da\u00b7men", "an", "dem", "Fens\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit lieb\u00e4ugelndem Gesichte,", "tokens": ["Mit", "lie\u00b7b\u00e4u\u00b7geln\u00b7dem", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmeichelnden Manieren wandelt", "tokens": ["Schmei\u00b7cheln\u00b7den", "Ma\u00b7nie\u00b7ren", "wan\u00b7delt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dort ein sch\u00f6nes Kind; doch nein,", "tokens": ["Dort", "ein", "sch\u00f6\u00b7nes", "Kind", ";", "doch", "nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "ADV", "PTKANT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein vermummter h\u00fcbscher Junge.", "tokens": ["Ein", "ver\u00b7mumm\u00b7ter", "h\u00fcb\u00b7scher", "Jun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieh doch nur den schlauen Narren,", "tokens": ["Sieh", "doch", "nur", "den", "schlau\u00b7en", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Kutschentreppe steht er,", "tokens": ["Auf", "der", "Kut\u00b7schen\u00b7trep\u00b7pe", "steht", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Brittin einen Spiegel", "tokens": ["Je\u00b7ner", "Brit\u00b7tin", "ei\u00b7nen", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor die schlimme Larve haltend,", "tokens": ["Vor", "die", "schlim\u00b7me", "Lar\u00b7ve", "hal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder dort den Rechtsgelehrten,", "tokens": ["O\u00b7der", "dort", "den", "Rechts\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure"