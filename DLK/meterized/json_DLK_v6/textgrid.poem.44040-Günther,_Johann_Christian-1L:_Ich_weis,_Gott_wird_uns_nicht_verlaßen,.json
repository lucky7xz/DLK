{"textgrid.poem.44040": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich weis, Gott wird uns nicht verla\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich weis, Gott wird uns nicht verla\u00dfen,", "tokens": ["Ich", "weis", ",", "Gott", "wird", "uns", "nicht", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertrau ihm nur, besorgter Geist,", "tokens": ["Ver\u00b7trau", "ihm", "nur", ",", "be\u00b7sorg\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lerne dich geduldig fa\u00dfen,", "tokens": ["Und", "ler\u00b7ne", "dich", "ge\u00b7dul\u00b7dig", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So scharf auch das Verh\u00e4ngn\u00fc\u00df schmeist.", "tokens": ["So", "scharf", "auch", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "schmeist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bekenn, Gott rettet auch die S\u00fcnder", "tokens": ["Be\u00b7kenn", ",", "Gott", "ret\u00b7tet", "auch", "die", "S\u00fcn\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hebet die gefallnen Kinder", "tokens": ["Und", "he\u00b7bet", "die", "ge\u00b7fall\u00b7nen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus m\u00fctterlicher Regung auf.", "tokens": ["Aus", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "Re\u00b7gung", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach winck ihm nur mit Herz und Armen", "tokens": ["Ach", "win\u00b7ck", "ihm", "nur", "mit", "Herz", "und", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und locke sein getreu Erbarmen,", "tokens": ["Und", "lo\u00b7cke", "sein", "ge\u00b7treu", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er kommt dir selber in den Lauf.", "tokens": ["Er", "kommt", "dir", "sel\u00b7ber", "in", "den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast die Strafe l\u00e4ngst verschuldet", "tokens": ["Du", "hast", "die", "Stra\u00b7fe", "l\u00e4ngst", "ver\u00b7schul\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bist der Langmuth nicht mehr werth,", "tokens": ["Und", "bist", "der", "Lang\u00b7muth", "nicht", "mehr", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die unsre Mi\u00dfethaten duldet", "tokens": ["Die", "uns\u00b7re", "Mi\u00b7\u00dfet\u00b7ha\u00b7ten", "dul\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zur Bekehrung Zeit gew\u00e4hrt.", "tokens": ["Und", "zur", "Be\u00b7keh\u00b7rung", "Zeit", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie oft hat nicht dein leicht Gem\u00fcthe", "tokens": ["Wie", "oft", "hat", "nicht", "dein", "leicht", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PTKNEG", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Ohr der allerh\u00f6chsten G\u00fcte", "tokens": ["Das", "Ohr", "der", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Geteuscht, betrogen und geplagt", "tokens": ["Ge\u00b7teuscht", ",", "be\u00b7tro\u00b7gen", "und", "ge\u00b7plagt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, wenn die S\u00fcnden Noth gebahren,", "tokens": ["Und", ",", "wenn", "die", "S\u00fcn\u00b7den", "Noth", "ge\u00b7bah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus bl\u00f6der Furcht von Jahr zu Jahren", "tokens": ["Aus", "bl\u00f6\u00b7der", "Furcht", "von", "Jahr", "zu", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Be\u00dfrung f\u00e4lschlich zugesagt.", "tokens": ["Die", "Be\u00df\u00b7rung", "f\u00e4lschlich", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Thu einmal, was du stets beschlo\u00dfen,", "tokens": ["Thu", "ein\u00b7mal", ",", "was", "du", "stets", "be\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zwing Gewohnheit, Fleisch und Blut.", "tokens": ["Und", "zwing", "Ge\u00b7wohn\u00b7heit", ",", "Fleisch", "und", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bo\u00dfheit scherzt nicht ungeno\u00dfen,", "tokens": ["Die", "Bo\u00df\u00b7heit", "scherzt", "nicht", "un\u00b7ge\u00b7no\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es kommt ein Tag voll Zorn und Glut.", "tokens": ["Es", "kommt", "ein", "Tag", "voll", "Zorn", "und", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tod hat tausend Pfeil und Stricke,", "tokens": ["Der", "Tod", "hat", "tau\u00b7send", "Pfeil", "und", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An einem einzeln Augenblicke", "tokens": ["An", "ei\u00b7nem", "ein\u00b7zeln", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00e4ngt unser ewig Wohl und Weh.", "tokens": ["H\u00e4ngt", "un\u00b7ser", "e\u00b7wig", "Wohl", "und", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist noch Zeit; ach lauf und eile", "tokens": ["Es", "ist", "noch", "Zeit", ";", "ach", "lauf", "und", "ei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$.", "XY", "XY", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und greif nach angebothnem Heile,", "tokens": ["Und", "greif", "nach", "an\u00b7ge\u00b7both\u00b7nem", "Hei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Eh de\u00dfen Gnade weitergeh.", "tokens": ["Eh", "de\u00b7\u00dfen", "Gna\u00b7de", "wei\u00b7ter\u00b7geh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wie k\u00f6nnen doch die eitlen Sachen,", "tokens": ["Wie", "k\u00f6n\u00b7nen", "doch", "die", "eit\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wilde Lust, die Pracht der Welt,", "tokens": ["Die", "wil\u00b7de", "Lust", ",", "die", "Pracht", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich gar so blind und hizig machen,", "tokens": ["Dich", "gar", "so", "blind", "und", "hi\u00b7zig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da doch ihr Schein wie Glas zerf\u00e4llt!", "tokens": ["Da", "doch", "ihr", "Schein", "wie", "Glas", "zer\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Eckel kommt von geilen K\u00fc\u00dfen,", "tokens": ["Der", "E\u00b7ckel", "kommt", "von", "gei\u00b7len", "K\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ehrsucht f\u00fcllt ihr weit Gewi\u00dfen", "tokens": ["Die", "Ehr\u00b7sucht", "f\u00fcllt", "ihr", "weit", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit sp\u00e4ter Reu, die Hand mit Wind;", "tokens": ["Mit", "sp\u00e4\u00b7ter", "Reu", ",", "die", "Hand", "mit", "Wind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NE", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Schr\u00f6cken wacht bey gro\u00dfen Sch\u00e4zen,", "tokens": ["Das", "Schr\u00f6\u00b7cken", "wacht", "bey", "gro\u00b7\u00dfen", "Sch\u00e4\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die wir mit Schwei\u00df so lange nezen,", "tokens": ["Die", "wir", "mit", "Schwei\u00df", "so", "lan\u00b7ge", "ne\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bis Feind und Dieb die Frucht gewinnt.", "tokens": ["Bis", "Feind", "und", "Dieb", "die", "Frucht", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man lacht der Kinder in Gedancken,", "tokens": ["Man", "lacht", "der", "Kin\u00b7der", "in", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die leichte Kartenh\u00e4user baun,", "tokens": ["Die", "leich\u00b7te", "Kar\u00b7ten\u00b7h\u00e4u\u00b7ser", "baun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Nadeln, Schilf und Steinchen zancken", "tokens": ["Um", "Na\u00b7deln", ",", "Schilf", "und", "Stein\u00b7chen", "zan\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und allem, was nur schmeichelt, traun;", "tokens": ["Und", "al\u00b7lem", ",", "was", "nur", "schmei\u00b7chelt", ",", "traun", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch sind wir Alten wohl viel kl\u00fcger?", "tokens": ["Doch", "sind", "wir", "Al\u00b7ten", "wohl", "viel", "kl\u00fc\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir sehn die listigsten Betrieger", "tokens": ["Wir", "sehn", "die", "lis\u00b7tigs\u00b7ten", "Be\u00b7trie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zum \u00f6ftern vor Propheten an.", "tokens": ["Zum", "\u00f6f\u00b7tern", "vor", "Pro\u00b7phe\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Moden sind wir wie die Afen", "tokens": ["In", "Mo\u00b7den", "sind", "wir", "wie", "die", "A\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und machen uns so viel zu schafen,", "tokens": ["Und", "ma\u00b7chen", "uns", "so", "viel", "zu", "scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und endlich ist doch nichts gethan.", "tokens": ["Und", "end\u00b7lich", "ist", "doch", "nichts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was hilft das aufgeblasne Wi\u00dfen,", "tokens": ["Was", "hilft", "das", "auf\u00b7ge\u00b7blas\u00b7ne", "Wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Womit der Schulen Hochmuth prahlt,", "tokens": ["Wo\u00b7mit", "der", "Schu\u00b7len", "Hoch\u00b7muth", "prahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, wenn er Bauch und Kopf zerri\u00dfen,", "tokens": ["Der", ",", "wenn", "er", "Bauch", "und", "Kopf", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Volck mit Staub und Schimpf bezahlt?", "tokens": ["Sein", "Volck", 