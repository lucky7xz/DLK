, "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als durch ihr Schulgeschweze baun,", "tokens": ["Als", "durch", "ihr", "Schul\u00b7ge\u00b7schwe\u00b7ze", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So l\u00e4st dein Vorspruch und Erbarmen,", "tokens": ["So", "l\u00e4st", "dein", "Vor\u00b7spruch", "und", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du Heil der Welt und Schuz der Armen,", "tokens": ["Du", "Heil", "der", "Welt", "und", "Schuz", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mich doch ein holdes Antlitz schaun.", "tokens": ["Mich", "doch", "ein", "hol\u00b7des", "Ant\u00b7litz", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mein Unrecht hat wie jene Br\u00fcder", "tokens": ["Mein", "Un\u00b7recht", "hat", "wie", "je\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch dir, mein Joseph, Leid gethan;", "tokens": ["Auch", "dir", ",", "mein", "Jo\u00b7se\u00b7ph", ",", "Leid", "ge\u00b7than", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NE", "$,", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nun fall ich auch wie sie darnieder", "tokens": ["Nun", "fall", "ich", "auch", "wie", "sie", "dar\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PPER", "ADV", "KOKOM", "PPER", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klage mich err\u00f6thet an.", "tokens": ["Und", "kla\u00b7ge", "mich", "er\u00b7r\u00f6t\u00b7het", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du k\u00f6ntest alles m\u00e4chtig r\u00e4chen,", "tokens": ["Du", "k\u00f6n\u00b7test", "al\u00b7les", "m\u00e4ch\u00b7tig", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein du strafest mein Verbrechen", "tokens": ["Al\u00b7lein", "du", "stra\u00b7fest", "mein", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch Mitleid \u00fcber meine Noth.", "tokens": ["Durch", "Mit\u00b7leid", "\u00fc\u00b7ber", "mei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du weinest nebst dem Liebesku\u00dfe", "tokens": ["Du", "wei\u00b7nest", "nebst", "dem", "Lie\u00b7bes\u00b7ku\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor F