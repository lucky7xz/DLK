, "selbst", "wird", "eu\u00b7er", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von nun an in das Meer versencken,", "tokens": ["Von", "nun", "an", "in", "das", "Meer", "ver\u00b7sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Er macht mich durch sein Kind gerecht.", "tokens": ["Er", "macht", "mich", "durch", "sein", "Kind", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was wilstu mehr, besorgtes Herze?", "tokens": ["Was", "wils\u00b7tu", "mehr", ",", "be\u00b7sorg\u00b7tes", "Her\u00b7ze", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "$,", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nun fa\u00dfe dich in allem Schmerze", "tokens": ["Nun", "fa\u00b7\u00dfe", "dich", "in", "al\u00b7lem", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und leb auf Hofnung arm und schlecht.", "tokens": ["Und", "leb", "auf", "Hof\u00b7nung", "arm", "und", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}