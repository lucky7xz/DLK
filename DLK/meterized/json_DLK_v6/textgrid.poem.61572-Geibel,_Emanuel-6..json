{"textgrid.poem.61572": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich alter Zeit Geschichten", "tokens": ["Der", "ich", "al\u00b7ter", "Zeit", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrieb, als Schnee bedeckt die Flur,", "tokens": ["Schrieb", ",", "als", "Schnee", "be\u00b7deckt", "die", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt, o Fr\u00fchling, in Gedichten", "tokens": ["Jetzt", ",", "o", "Fr\u00fch\u00b7ling", ",", "in", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Taten schreib' ich nur.", "tokens": ["Dei\u00b7ne", "Ta\u00b7ten", "schreib'", "ich", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00e4glich merk' ich an, wie linder", "tokens": ["T\u00e4g\u00b7lich", "merk'", "ich", "an", ",", "wie", "lin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die Kraft der Sonne r\u00fchrt", "tokens": ["Sich", "die", "Kraft", "der", "Son\u00b7ne", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Blumen, deine Kinder,", "tokens": ["Und", "die", "Blu\u00b7men", ",", "dei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Tal zum Gipfel f\u00fchrt;", "tokens": ["Aus", "dem", "Tal", "zum", "Gip\u00b7fel", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie in tieferm Gr\u00fcn die Halde", "tokens": ["Wie", "in", "tie\u00b7ferm", "Gr\u00fcn", "die", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwellend prangt, vom Tau erfrischt,", "tokens": ["Schwel\u00b7lend", "prangt", ",", "vom", "Tau", "er\u00b7frischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie vollz\u00e4hl'ger stets im Walde", "tokens": ["Wie", "voll\u00b7z\u00e4hl'\u00b7ger", "stets", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich der Chor der Stimmen mischt.", "tokens": ["Sich", "der", "Chor", "der", "Stim\u00b7men", "mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heut aus zarter Knospenh\u00fclle", "tokens": ["Heut", "aus", "zar\u00b7ter", "Knos\u00b7pen\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df und dicht wie Silberschaum", "tokens": ["Wei\u00df", "und", "dicht", "wie", "Sil\u00b7ber\u00b7schaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KOKOM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brach des Birnbaums Bl\u00fctenf\u00fclle,", "tokens": ["Brach", "des", "Birn\u00b7baums", "Bl\u00fc\u00b7ten\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen bl\u00fcht der Apfelbaum.", "tokens": ["Mor\u00b7gen", "bl\u00fcht", "der", "Ap\u00b7fel\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wichtig f\u00fcr mein froh Verzeichnis", "tokens": ["Wich\u00b7tig", "f\u00fcr", "mein", "froh", "Ver\u00b7zeich\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deucht mir, was ich nur vernahm \u2013", "tokens": ["Deucht", "mir", ",", "was", "ich", "nur", "ver\u00b7nahm", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist's nicht auch ein Weltereignis,", "tokens": ["Ist's", "nicht", "auch", "ein", "Wel\u00b7ter\u00b7eig\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die erste Rose kam?", "tokens": ["Wenn", "die", "ers\u00b7te", "Ro\u00b7se", "kam", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}