{"textgrid.poem.60562": {"metadata": {"author": {"name": "Lange, Samuel Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sterbliche, den du der Begeisterung w\u00fcrdigst,", "genre": "verse", "period": "N.A.", "pub_year": 1746, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sterbliche, den du der Begeisterung w\u00fcrdigst,", "tokens": ["Der", "Sterb\u00b7li\u00b7che", ",", "den", "du", "der", "Be\u00b7geis\u00b7te\u00b7rung", "w\u00fcr\u00b7digst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O himmlisches Kind, o Dichtkunst eilet erhaben", "tokens": ["O", "himm\u00b7li\u00b7sches", "Kind", ",", "o", "Dicht\u00b7kunst", "ei\u00b7let", "er\u00b7ha\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "VVFIN", "ADJD"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Sternenban zu, und lernt die G\u00f6ttliche Tugend", "tokens": ["Der", "Ster\u00b7nen\u00b7ban", "zu", ",", "und", "lernt", "die", "G\u00f6tt\u00b7li\u00b7che", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und Weisheit verstehn.", "tokens": ["Und", "Weis\u00b7heit", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Sein horchendes Ohr sch\u00f6pft die geheimesten Lehren", "tokens": ["Sein", "hor\u00b7chen\u00b7des", "Ohr", "sch\u00f6pft", "die", "ge\u00b7hei\u00b7mes\u00b7ten", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es f\u00fchlet sein Hertz die \u00fcbermenschlichen Triebe.", "tokens": ["Es", "f\u00fch\u00b7let", "sein", "Hertz", "die", "\u00fc\u00b7ber\u00b7menschli\u00b7chen", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit englischer Kraft \u00fcbt und empfindet er Freundschaft.", "tokens": ["Mit", "eng\u00b7li\u00b7scher", "Kraft", "\u00fcbt", "und", "emp\u00b7fin\u00b7det", "er", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die niemand versteht.", "tokens": ["Die", "nie\u00b7mand", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Sein gantzes Hertz, voll der Gottheit, eilet den Menschen,", "tokens": ["Sein", "gant\u00b7zes", "Hertz", ",", "voll", "der", "Got\u00b7theit", ",", "ei\u00b7let", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die g\u00f6ttliche Kunst, durch Freundschaft gl\u00fccklich zu werden", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Kunst", ",", "durch", "Freund\u00b7schaft", "gl\u00fcck\u00b7lich", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "PTKZU", "VAINF"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu lehren, wie du, ", "tokens": ["Zu", "leh\u00b7ren", ",", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Lehrest und \u00fcbst.", "tokens": ["Leh\u00b7rest", "und", "\u00fcbst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Es wisse die Welt durch Thirsis Lieder bezaubert", "tokens": ["Es", "wis\u00b7se", "die", "Welt", "durch", "Thir\u00b7sis", "Lie\u00b7der", "be\u00b7zau\u00b7bert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "VVPP"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Stille und Kleist, Gleim, Germershausen und Sultzer", "tokens": ["Das", "Stil\u00b7le", "und", "Kleist", ",", "Gleim", ",", "Ger\u00b7mers\u00b7hau\u00b7sen", "und", "Sult\u00b7zer"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "NE", "$,", "NN", "KON", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Freundschaftliche Lust, durch weise Tugend empfinden.", "tokens": ["Freund\u00b7schaft\u00b7li\u00b7che", "Lust", ",", "durch", "wei\u00b7se", "Tu\u00b7gend", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "O w\u00fcrdiger Mann.", "tokens": ["O", "w\u00fcr\u00b7di\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Dein klopfendes Hertz schl\u00e4gt auch durch Freundschaft erhitzet", "tokens": ["Dein", "klop\u00b7fen\u00b7des", "Hertz", "schl\u00e4gt", "auch", "durch", "Freund\u00b7schaft", "er\u00b7hit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+--+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Thirsis Hertz that. Aus gleichen redlichen Trieben,", "tokens": ["Wie", "Thir\u00b7sis", "Hertz", "that", ".", "Aus", "glei\u00b7chen", "red\u00b7li\u00b7chen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$.", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus welchen er sang, f\u00e4rbt dich, durcheilet die Adern,", "tokens": ["Aus", "wel\u00b7chen", "er", "sang", ",", "f\u00e4rbt", "dich", ",", "durc\u00b7hei\u00b7let", "die", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein wallendes Blut.", "tokens": ["Dein", "wal\u00b7len\u00b7des", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Es wisse die Welt, da\u00df, weil ich gelebet, kein Weiser,", "tokens": ["Es", "wis\u00b7se", "die", "Welt", ",", "da\u00df", ",", "weil", "ich", "ge\u00b7le\u00b7bet", ",", "kein", "Wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "$,", "KOUS", "PPER", "VVPP", "$,", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein Tugendfreund war, den nicht die heilige Freundschaft", "tokens": ["Kein", "Tu\u00b7gend\u00b7freund", "war", ",", "den", "nicht", "die", "hei\u00b7li\u00b7ge", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit mir auch vereint, und da\u00df du, ", "tokens": ["Mit", "mir", "auch", "ver\u00b7eint", ",", "und", "da\u00df", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,", "KON", "KOUS", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mir r\u00fchmlich vermehrst.", "tokens": ["Mir", "r\u00fchm\u00b7lich", "ver\u00b7mehrst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Mein z\u00e4rtliches Hertz liest Thirsis Lieder mit Thr\u00e4nen", "tokens": ["Mein", "z\u00e4rt\u00b7li\u00b7ches", "Hertz", "liest", "Thir\u00b7sis", "Lie\u00b7der", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NE", "NN", "APPR", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und denckt denn an dich, mit einer traurigen Ruhe.", "tokens": ["Und", "denckt", "denn", "an", "dich", ",", "mit", "ei\u00b7ner", "trau\u00b7ri\u00b7gen", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O li\u00df sie, du siehst die Glut der Freundschaft die in mir", "tokens": ["O", "li\u00df", "sie", ",", "du", "siehst", "die", "Glut", "der", "Freund\u00b7schaft", "die", "in", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ART", "APPR", "PPER"], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Stets gegen dich brennt.", "tokens": ["Stets", "ge\u00b7gen", "dich", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}