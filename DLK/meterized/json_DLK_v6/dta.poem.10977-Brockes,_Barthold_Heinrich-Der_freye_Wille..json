{"dta.poem.10977": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der freye Wille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Lehre von dem freyen Willen, und da\u00df wir die Voll-\nkommenheit,", "tokens": ["Der", "Leh\u00b7re", "von", "dem", "frey\u00b7en", "Wil\u00b7len", ",", "und", "da\u00df", "wir", "die", "Voll", "kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "KOUS", "PPER", "ART", "TRUNC", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Aufs wenigst in so hohem Grad, nicht hegen,", "tokens": ["Aufs", "we\u00b7nigst", "in", "so", "ho\u00b7hem", "Grad", ",", "nicht", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "ADV", "ADJA", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scheint eine gro\u00dfe Schwierigkeit", "tokens": ["Scheint", "ei\u00b7ne", "gro\u00b7\u00dfe", "Schwie\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch sich in den Weg zu legen,", "tokens": ["Da\u00b7durch", "sich", "in", "den", "Weg", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df wenn wir unser Innerstes ergr\u00fcnden,", "tokens": ["Da\u00df", "wenn", "wir", "un\u00b7ser", "In\u00b7ners\u00b7tes", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir uns, uns selber zu vergn\u00fcgen,", "tokens": ["Wir", "uns", ",", "uns", "sel\u00b7ber", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Fast g\u00e4nzlich ungeschickt, und gar nicht f\u00e4hig finden,", "tokens": ["Fast", "g\u00e4nz\u00b7lich", "un\u00b7ge\u00b7schickt", ",", "und", "gar", "nicht", "f\u00e4\u00b7hig", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "KON", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So doch der kr\u00e4ftigste Beweis der Lehre", "tokens": ["So", "doch", "der", "kr\u00e4f\u00b7tigs\u00b7te", "Be\u00b7weis", "der", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Von eines Willens Freyheit w\u00e4re.", "tokens": ["Von", "ei\u00b7nes", "Wil\u00b7lens", "Frey\u00b7heit", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zumal wir selbst an Gottes Werken,", "tokens": ["Zu\u00b7mal", "wir", "selbst", "an", "Got\u00b7tes", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Worin er, seine Lieb und Allmacht zu bemerken,", "tokens": ["Wo\u00b7rin", "er", ",", "sei\u00b7ne", "Lieb", "und", "All\u00b7macht", "zu", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Uns so viel Proben wollen g\u00f6nnen,", "tokens": ["Uns", "so", "viel", "Pro\u00b7ben", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Woran wir uns vergn\u00fcgen sollen,", "tokens": ["Wo\u00b7ran", "wir", "uns", "ver\u00b7gn\u00fc\u00b7gen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wir doch daran, auch wenn wir wollen,", "tokens": ["Wir", "doch", "da\u00b7ran", ",", "auch", "wenn", "wir", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "$,", "ADV", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Uns dennoch nicht vergn\u00fcgen k\u00f6nnen.", "tokens": ["Uns", "den\u00b7noch", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}