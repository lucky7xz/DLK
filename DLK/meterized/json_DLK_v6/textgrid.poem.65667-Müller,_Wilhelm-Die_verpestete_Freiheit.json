{"textgrid.poem.65667": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die verpestete Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schreit das Pharis\u00e4ervolk so \u00e4ngstlich durch die L\u00e4nder,", "tokens": ["Was", "schreit", "das", "Pha\u00b7ri\u00b7s\u00e4er\u00b7volk", "so", "\u00e4ngst\u00b7lich", "durch", "die", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die H\u00e4upter dick mit Staub bestreut, zerrissen die Gew\u00e4nder?", "tokens": ["Die", "H\u00e4up\u00b7ter", "dick", "mit", "Staub", "be\u00b7streut", ",", "zer\u00b7ris\u00b7sen", "die", "Ge\u00b7w\u00e4n\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Sie schreien: Sperrt die H\u00e4fen zu, umzieht mit Quarant\u00e4nen", "tokens": ["Sie", "schrei\u00b7en", ":", "Sperrt", "die", "H\u00e4\u00b7fen", "zu", ",", "um\u00b7zieht", "mit", "Qua\u00b7ran\u00b7t\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Die Grenzen und die Ufer schnell vor Schiffen und vor K\u00e4hnen!", "tokens": ["Die", "Gren\u00b7zen", "und", "die", "U\u00b7fer", "schnell", "vor", "Schif\u00b7fen", "und", "vor", "K\u00e4h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Die Pest ist unter ihrer Schaar. Da seht die Strafgerichte,", "tokens": ["Die", "Pest", "ist", "un\u00b7ter", "ih\u00b7rer", "Schaar", ".", "Da", "seht", "die", "Straf\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Damit des Herrn gerechte Hand Emp\u00f6rer macht zu nichte!", "tokens": ["Da\u00b7mit", "des", "Herrn", "ge\u00b7rech\u00b7te", "Hand", "Em\u00b7p\u00f6\u00b7rer", "macht", "zu", "nich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "NN", "VVFIN", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Die Freiheit selber, wie es hei\u00dft, ist von der Pest befallen,", "tokens": ["Die", "Frei\u00b7heit", "sel\u00b7ber", ",", "wie", "es", "hei\u00dft", ",", "ist", "von", "der", "Pest", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Und fl\u00fcchtet sich nach Westen nun mit ihren J\u00fcngern allen.", "tokens": ["Und", "fl\u00fcch\u00b7tet", "sich", "nach", "Wes\u00b7ten", "nun", "mit", "ih\u00b7ren", "J\u00fcn\u00b7gern", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADV", "APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "O seht euch vor, da\u00df in das Land die Freiheit euch nicht schleiche,", "tokens": ["O", "seht", "euch", "vor", ",", "da\u00df", "in", "das", "Land", "die", "Frei\u00b7heit", "euch", "nicht", "schlei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Und der gesunden V\u00f6lker Herz mit ihrem Hauch erreiche!", "tokens": ["Und", "der", "ge\u00b7sun\u00b7den", "V\u00f6l\u00b7ker", "Herz", "mit", "ih\u00b7rem", "Hauch", "er\u00b7rei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Sie kleidet sich zu dieser Zeit in vielerlei Gestalten:", "tokens": ["Sie", "klei\u00b7det", "sich", "zu", "die\u00b7ser", "Zeit", "in", "vie\u00b7ler\u00b7lei", "Ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Bald Weib, bald Mann, bald nur ein Kind, bald hat sie greise Falten.", "tokens": ["Bald", "Weib", ",", "bald", "Mann", ",", "bald", "nur", "ein", "Kind", ",", "bald", "hat", "sie", "grei\u00b7se", "Fal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Drum lasset keinen Fl\u00fcchtling ein, der kommt vom Griechenlande,", "tokens": ["Drum", "las\u00b7set", "kei\u00b7nen", "Fl\u00fccht\u00b7ling", "ein", ",", "der", "kommt", "vom", "Grie\u00b7chen\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Da\u00df nicht die Freiheit ihre Pest bring' in die guten Lande!", "tokens": ["Da\u00df", "nicht", "die", "Frei\u00b7heit", "ih\u00b7re", "Pest", "bring'", "in", "die", "gu\u00b7ten", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}