{"dta.poem.21580": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Meinem Bruder Julius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Aus einem Stamm entsprossen,", "tokens": ["Aus", "ei\u00b7nem", "Stamm", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von einer Erde gen\u00e4hrt,", "tokens": ["Von", "ei\u00b7ner", "Er\u00b7de", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf Leben und Tod Genossen,", "tokens": ["Auf", "Le\u00b7ben", "und", "Tod", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von einer Gluth verkl\u00e4rt \u2014", "tokens": ["Von", "ei\u00b7ner", "Gluth", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So stehen wir beieinander", "tokens": ["So", "ste\u00b7hen", "wir", "bei\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schulter an Schulter gelehnt,", "tokens": ["Schul\u00b7ter", "an", "Schul\u00b7ter", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "So f\u00fchren wir aus selbander,", "tokens": ["So", "f\u00fch\u00b7ren", "wir", "aus", "sel\u00b7ban\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Was jeder von uns ersehnt.", "tokens": ["Was", "je\u00b7der", "von", "uns", "er\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ohne Dich, Du lodernd Feuer,", "tokens": ["Oh\u00b7ne", "Dich", ",", "Du", "lo\u00b7dernd", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erstarrte mir Hirn und Blut, \u2014", "tokens": ["Er\u00b7starr\u00b7te", "mir", "Hirn", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus der Hand s\u00e4nk\u2019 mir das Steuer,", "tokens": ["Aus", "der", "Hand", "s\u00e4nk'", "mir", "das", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spr\u00e4ch\u2019 mir Dein Mund nicht Muth.", "tokens": ["Spr\u00e4ch'", "mir", "Dein", "Mund", "nicht", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja, wir geh\u00f6ren zusammen,", "tokens": ["Ja", ",", "wir", "ge\u00b7h\u00f6\u00b7ren", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Wie Wind und Wellenschlag,", "tokens": ["Wie", "Wind", "und", "Wel\u00b7len\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie Himmel und Sternenflammen,", "tokens": ["Wie", "Him\u00b7mel", "und", "Ster\u00b7nen\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wie der Wald und der sch\u00e4umende Bach.", "tokens": ["Wie", "der", "Wald", "und", "der", "sch\u00e4u\u00b7men\u00b7de", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wir haben uns nichts geschworen,", "tokens": ["Wir", "ha\u00b7ben", "uns", "nichts", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kein Blutbund ging vorauf,", "tokens": ["Kein", "Blut\u00b7bund", "ging", "vor\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind zu eins geboren,", "tokens": ["Wir", "sind", "zu", "eins", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Quell, zwei Str\u00f6me, ein Lauf.", "tokens": ["Ein", "Quell", ",", "zwei", "Str\u00f6\u00b7me", ",", "ein", "Lauf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "O Bruder, was auch das Leben", "tokens": ["O", "Bru\u00b7der", ",", "was", "auch", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "F\u00fcr uns ernstwebend schafft:", "tokens": ["F\u00fcr", "uns", "ernst\u00b7we\u00b7bend", "schafft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Eins, eins sei unser Streben,", "tokens": ["Eins", ",", "eins", "sei", "un\u00b7ser", "Stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch zwiefach unsre Kraft.", "tokens": ["Doch", "zwie\u00b7fach", "uns\u00b7re", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Rings dr\u00e4ngt so viele Kleinheit", "tokens": ["Rings", "dr\u00e4ngt", "so", "vie\u00b7le", "Klein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In tausend Herzen sich,", "tokens": ["In", "tau\u00b7send", "Her\u00b7zen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wuchernd prahlt rings Gemeinheit,", "tokens": ["Wu\u00b7chernd", "prahlt", "rings", "Ge\u00b7mein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Alle Sehnsucht schier erblich,", "tokens": ["Al\u00b7le", "Sehn\u00b7sucht", "schier", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Sehnsucht nach des Sch\u00f6nen", "tokens": ["Al\u00b7le", "Sehn\u00b7sucht", "nach", "des", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unwandelbarem Licht,", "tokens": ["Un\u00b7wan\u00b7del\u00b7ba\u00b7rem", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur Schwerter h\u00f6r\u2019 ich dr\u00f6hnen,", "tokens": ["Nur", "Schwer\u00b7ter", "h\u00f6r'", "ich", "dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Helle Lieder h\u00f6r\u2019 ich nicht.", "tokens": ["Hel\u00b7le", "Lie\u00b7der", "h\u00f6r'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O Bruder, da gilt\u2019s zu ringen", "tokens": ["O", "Bru\u00b7der", ",", "da", "gilt's", "zu", "rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PIS", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einig mit zwiefacher Kraft, \u2014", "tokens": ["Ei\u00b7nig", "mit", "zwie\u00b7fac\u00b7her", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Dann werden wir Balsam bringen", "tokens": ["Dann", "wer\u00b7den", "wir", "Bal\u00b7sam", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeder Wunde, die fiebernd klafft,", "tokens": ["Je\u00b7der", "Wun\u00b7de", ",", "die", "fie\u00b7bernd", "klafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Dann werden mit brennenden Lettern", "tokens": ["Dann", "wer\u00b7den", "mit", "bren\u00b7nen\u00b7den", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Unsre Namen wir zeichnen ein", "tokens": ["Uns\u00b7re", "Na\u00b7men", "wir", "zeich\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "ART"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der Geschichte rauschenden Bl\u00e4ttern,", "tokens": ["Der", "Ge\u00b7schich\u00b7te", "rau\u00b7schen\u00b7den", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und in der Herzen Schrein.", "tokens": ["Und", "in", "der", "Her\u00b7zen", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}