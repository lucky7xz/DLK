{"textgrid.poem.55061": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gewohnt, getan", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe geliebet, nun lieb ich erst recht!", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7lie\u00b7bet", ",", "nun", "lieb", "ich", "erst", "recht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "ADJD", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erst war ich der Diener, nun bin ich der Knecht.", "tokens": ["Erst", "war", "ich", "der", "Die\u00b7ner", ",", "nun", "bin", "ich", "der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erst war ich der Diener von allen;", "tokens": ["Erst", "war", "ich", "der", "Die\u00b7ner", "von", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun fesselt mich diese scharmante Person,", "tokens": ["Nun", "fes\u00b7selt", "mich", "die\u00b7se", "schar\u00b7man\u00b7te", "Per\u00b7son", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie tut mir auch alles zur Liebe, zum Lohn,", "tokens": ["Sie", "tut", "mir", "auch", "al\u00b7les", "zur", "Lie\u00b7be", ",", "zum", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie kann nur allein mir gefallen.", "tokens": ["Sie", "kann", "nur", "al\u00b7lein", "mir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Ich habe geglaubet, nun glaub ich erst recht!", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7glau\u00b7bet", ",", "nun", "glaub", "ich", "erst", "recht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und geht es auch wunderlich, geht es auch schlecht,", "tokens": ["Und", "geht", "es", "auch", "wun\u00b7der\u00b7lich", ",", "geht", "es", "auch", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich bleibe beim gl\u00e4ubigen Orden:", "tokens": ["Ich", "blei\u00b7be", "beim", "gl\u00e4u\u00b7bi\u00b7gen", "Or\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So d\u00fcster es oft und so dunkel es war", "tokens": ["So", "d\u00fcs\u00b7ter", "es", "oft", "und", "so", "dun\u00b7kel", "es", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV", "KON", "ADV", "ADJD", "PPER", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "In dr\u00e4ngenden N\u00f6ten, in naher Gefahr,", "tokens": ["In", "dr\u00e4n\u00b7gen\u00b7den", "N\u00f6\u00b7ten", ",", "in", "na\u00b7her", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auf einmal ist's lichter geworden.", "tokens": ["Auf", "ein\u00b7mal", "ist's", "lich\u00b7ter", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich habe gespeiset, nun speis ich erst gut!", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7spei\u00b7set", ",", "nun", "speis", "ich", "erst", "gut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bei heiterem Sinne, mit fr\u00f6hlichem Blut", "tokens": ["Bei", "hei\u00b7te\u00b7rem", "Sin\u00b7ne", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ist alles an Tafel vergessen.", "tokens": ["Ist", "al\u00b7les", "an", "Ta\u00b7fel", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Jugend verschlingt nur, dann sauset sie fort;", "tokens": ["Die", "Ju\u00b7gend", "ver\u00b7schlingt", "nur", ",", "dann", "sau\u00b7set", "sie", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich liebe zu tafeln am lustigen Ort,", "tokens": ["Ich", "lie\u00b7be", "zu", "ta\u00b7feln", "am", "lus\u00b7ti\u00b7gen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ich kost und ich schmecke beim Essen.", "tokens": ["Ich", "kost", "und", "ich", "schme\u00b7cke", "beim", "Es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ich habe getrunken, nun trink ich erst gern!", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7trun\u00b7ken", ",", "nun", "trink", "ich", "erst", "gern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Wein, er erh\u00f6ht uns, er macht uns zum Herrn", "tokens": ["Der", "Wein", ",", "er", "er\u00b7h\u00f6ht", "uns", ",", "er", "macht", "uns", "zum", "Herrn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und l\u00f6set die sklavischen Zungen.", "tokens": ["Und", "l\u00f6\u00b7set", "die", "skla\u00b7vi\u00b7schen", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Ja, schonet nur nicht das erquickende Na\u00df:", "tokens": ["Ja", ",", "scho\u00b7net", "nur", "nicht", "das", "er\u00b7qui\u00b7cken\u00b7de", "Na\u00df", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Denn schwindet der \u00e4lteste Wein aus dem Fa\u00df,", "tokens": ["Denn", "schwin\u00b7det", "der", "\u00e4l\u00b7tes\u00b7te", "Wein", "aus", "dem", "Fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So altern dagegen die jungen.", "tokens": ["So", "al\u00b7tern", "da\u00b7ge\u00b7gen", "die", "jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich habe getanzt und dem Tanze gelobt,", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7tanzt", "und", "dem", "Tan\u00b7ze", "ge\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wird auch kein Schleifer, kein Walzer getobt,", "tokens": ["Und", "wird", "auch", "kein", "Schlei\u00b7fer", ",", "kein", "Wal\u00b7zer", "ge\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So drehn wir ein sittiges T\u00e4nzchen.", "tokens": ["So", "drehn", "wir", "ein", "sit\u00b7ti\u00b7ges", "T\u00e4nz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wer sich der Blumen recht viele verflicht", "tokens": ["Und", "wer", "sich", "der", "Blu\u00b7men", "recht", "vie\u00b7le", "ver\u00b7flicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und h\u00e4lt auch die ein und die andere nicht,", "tokens": ["Und", "h\u00e4lt", "auch", "die", "ein", "und", "die", "an\u00b7de\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "PTKVZ", "KON", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ihm bleibet ein munteres Kr\u00e4nzchen.", "tokens": ["Ihm", "blei\u00b7bet", "ein", "mun\u00b7te\u00b7res", "Kr\u00e4nz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Drum frisch nur aufs neue! Bedenke dich nicht:", "tokens": ["Drum", "frisch", "nur", "aufs", "neu\u00b7e", "!", "Be\u00b7den\u00b7ke", "dich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "APPRART", "ADJA", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denn wer sich die Rosen, die bl\u00fchenden, bricht,", "tokens": ["Denn", "wer", "sich", "die", "Ro\u00b7sen", ",", "die", "bl\u00fc\u00b7hen\u00b7den", ",", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Den kitzeln f\u00fcrwahr nur die Dornen.", "tokens": ["Den", "kit\u00b7zeln", "f\u00fcr\u00b7wahr", "nur", "die", "Dor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So heute wie gestern, es flimmert der Stern;", "tokens": ["So", "heu\u00b7te", "wie", "ge\u00b7stern", ",", "es", "flim\u00b7mert", "der", "Stern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Nur halte von h\u00e4ngenden K\u00f6pfen dich fern", "tokens": ["Nur", "hal\u00b7te", "von", "h\u00e4n\u00b7gen\u00b7den", "K\u00f6p\u00b7fen", "dich", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und lebe dir immer von vornen.", "tokens": ["Und", "le\u00b7be", "dir", "im\u00b7mer", "von", "vor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}