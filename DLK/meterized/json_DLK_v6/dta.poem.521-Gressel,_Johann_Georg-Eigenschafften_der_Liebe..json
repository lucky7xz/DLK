{"dta.poem.521": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eigenschafften der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe ist ein Feur/ das im verborgnen brennet/", "tokens": ["Die", "Lie\u00b7be", "ist", "ein", "Feur", "/", "das", "im", "ver\u00b7borg\u00b7nen", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein\u2019 Wunde/ die beliebt bey ihren Schmertzen ist/", "tokens": ["Ein'", "Wun\u00b7de", "/", "die", "be\u00b7liebt", "bey", "ih\u00b7ren", "Schmert\u00b7zen", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein wohlgeschmacktes Gifft/ das jeder gern genie\u00dft/", "tokens": ["Ein", "wohl\u00b7ge\u00b7schmack\u00b7tes", "Gifft", "/", "das", "je\u00b7der", "gern", "ge\u00b7nie\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Bitter-seyn/ das nicht die S\u00fcssigkeit verkennet/", "tokens": ["Ein", "Bit\u00b7ter\u00b7seyn", "/", "das", "nicht", "die", "S\u00fcs\u00b7sig\u00b7keit", "ver\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein\u2019 Kranckheit/ wo sich Lust nicht von der Marter trennet/", "tokens": ["Ein'", "Kran\u00b7ck\u00b7heit", "/", "wo", "sich", "Lust", "nicht", "von", "der", "Mar\u00b7ter", "tren\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PRF", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ein\u2019 Straffe/ die man gern ohn grosse Weigrung k\u00fc\u00dft/", "tokens": ["Ein'", "Straf\u00b7fe", "/", "die", "man", "gern", "ohn", "gros\u00b7se", "Wei\u00b7grung", "k\u00fc\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Tod/ der ob er gleich den Lebens-Drat zerfrist/", "tokens": ["Ein", "Tod", "/", "der", "ob", "er", "gleich", "den", "Le\u00b7bens\u00b7Drat", "zer\u00b7frist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dennoch von Liebenden wird angenehm genennet.", "tokens": ["Den\u00b7noch", "von", "Lie\u00b7ben\u00b7den", "wird", "an\u00b7ge\u00b7nehm", "ge\u00b7nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}