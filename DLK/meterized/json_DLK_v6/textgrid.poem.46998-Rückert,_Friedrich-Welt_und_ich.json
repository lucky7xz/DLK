{"textgrid.poem.46998": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Welt und ich", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo auf Weltverbesserung", "tokens": ["\u00bb", "wo", "auf", "Welt\u00b7ver\u00b7bes\u00b7se\u00b7rung"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PWAV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnsche k\u00fchn sich lenken,", "tokens": ["W\u00fcn\u00b7sche", "k\u00fchn", "sich", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Willst du nur auf W\u00e4sserung", "tokens": ["Willst", "du", "nur", "auf", "W\u00e4s\u00b7se\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Wieschens denken?", "tokens": ["Dei\u00b7nes", "Wie\u00b7schens", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn man erst die Welt gemacht", "tokens": ["Wenn", "man", "erst", "die", "Welt", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz zum Paradiese,", "tokens": ["Ganz", "zum", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kommt's von selber \u00fcber Nacht", "tokens": ["Kommt's", "von", "sel\u00b7ber", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch an deine Wiese.", "tokens": ["Auch", "an", "dei\u00b7ne", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch es mu\u00df zum gro\u00dfen Hort", "tokens": ["Doch", "es", "mu\u00df", "zum", "gro\u00b7\u00dfen", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei das Kleinste tragen;", "tokens": ["Bei", "das", "Kleins\u00b7te", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hast du nicht ein gutes Wort", "tokens": ["Hast", "du", "nicht", "ein", "gu\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwa mir zu sagen?", "tokens": ["Et\u00b7wa", "mir", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Auch das Wort ist eine That,", "tokens": ["Auch", "das", "Wort", "ist", "ei\u00b7ne", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sich mancher r\u00fchmet,", "tokens": ["Wie", "sich", "man\u00b7cher", "r\u00fch\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ein Hauch des Fr\u00fchlings hat", "tokens": ["Und", "ein", "Hauch", "des", "Fr\u00fch\u00b7lings", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets die Welt bebl\u00fcmet.\u00ab \u2013", "tokens": ["Stets", "die", "Welt", "be\u00b7bl\u00fc\u00b7met", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Bl\u00fche, was da bl\u00fchen mag,", "tokens": ["Bl\u00fc\u00b7he", ",", "was", "da", "bl\u00fc\u00b7hen", "mag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter euern Hauchen!", "tokens": ["Un\u00b7ter", "eu\u00b7ern", "Hau\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich will meines Herzens Schlag", "tokens": ["Ich", "will", "mei\u00b7nes", "Her\u00b7zens", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr mein Leben brauchen.", "tokens": ["F\u00fcr", "mein", "Le\u00b7ben", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "M\u00f6ge jeder still begl\u00fcckt", "tokens": ["M\u00f6\u00b7ge", "je\u00b7der", "still", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Freuden warten!", "tokens": ["Sei\u00b7ner", "Freu\u00b7den", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn die Rose selbst sich schm\u00fcckt,", "tokens": ["Wenn", "die", "Ro\u00b7se", "selbst", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fcckt sie auch den Garten.", "tokens": ["Schm\u00fcckt", "sie", "auch", "den", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}