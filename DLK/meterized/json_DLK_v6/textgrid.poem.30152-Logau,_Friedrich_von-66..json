{"textgrid.poem.30152": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Red und antwort ist zu geben", "tokens": ["Red", "und", "ant\u00b7wort", "ist", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beym Gericht in jenem Leben", "tokens": ["Beym", "Ge\u00b7richt", "in", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr gesamte n\u00fctze Gaben,", "tokens": ["F\u00fcr", "ge\u00b7sam\u00b7te", "n\u00fct\u00b7ze", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die wir her von oben haben.", "tokens": ["Die", "wir", "her", "von", "o\u00b7ben", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O gewi\u00df! das Zeit-vernichten", "tokens": ["O", "ge\u00b7wi\u00df", "!", "das", "Zeit\u00b7ver\u00b7nich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird man auch gar ernstlich richten.", "tokens": ["Wird", "man", "auch", "gar", "ernst\u00b7lich", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}