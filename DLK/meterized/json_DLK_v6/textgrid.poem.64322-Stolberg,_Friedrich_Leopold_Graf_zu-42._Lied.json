{"textgrid.poem.64322": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "42. Lied", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! mir ist das Herz so schwer!", "tokens": ["Ach", "!", "mir", "ist", "das", "Herz", "so", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Traurig irr' ich hin und her,", "tokens": ["Trau\u00b7rig", "irr'", "ich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suche Ruh, und finde keine,", "tokens": ["Su\u00b7che", "Ruh", ",", "und", "fin\u00b7de", "kei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geh ans Fenster hin, und weine!", "tokens": ["Geh", "ans", "Fens\u00b7ter", "hin", ",", "und", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00e4\u00dfest du auf meinem Scho\u00df", "tokens": ["S\u00e4\u00b7\u00dfest", "du", "auf", "mei\u00b7nem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrd' ich aller Sorgen los,", "tokens": ["W\u00fcrd'", "ich", "al\u00b7ler", "Sor\u00b7gen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus deinen blauen Augen", "tokens": ["Und", "aus", "dei\u00b7nen", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrd' ich Lieb' und Wonne saugen!", "tokens": ["W\u00fcrd'", "ich", "Lieb'", "und", "Won\u00b7ne", "sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt' ich doch, du s\u00fc\u00dfes Kind,", "tokens": ["K\u00f6nnt'", "ich", "doch", ",", "du", "s\u00fc\u00b7\u00dfes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliegen hin zu dir geschwind!", "tokens": ["Flie\u00b7gen", "hin", "zu", "dir", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich ewig dich umfangen,", "tokens": ["K\u00f6nnt'", "ich", "e\u00b7wig", "dich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an deinen Lippen hangen!", "tokens": ["Und", "an", "dei\u00b7nen", "Lip\u00b7pen", "han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}