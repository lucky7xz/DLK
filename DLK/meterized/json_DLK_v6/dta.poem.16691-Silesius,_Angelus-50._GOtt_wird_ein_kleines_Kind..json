{"dta.poem.16691": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "50.  GOtt wird ein kleines Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott schleust sich unerh\u00f6rt in Kindes kleinheit ein:", "tokens": ["Gott", "schleust", "sich", "un\u00b7er\u00b7h\u00f6rt", "in", "Kin\u00b7des", "klein\u00b7heit", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADJD", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach m\u00f6cht ich doch ein Kind in diesem Kinde seyn!", "tokens": ["Ach", "m\u00f6cht", "ich", "doch", "ein", "Kind", "in", "die\u00b7sem", "Kin\u00b7de", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}