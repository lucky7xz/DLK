{"textgrid.poem.38549": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An ein Dorf", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Edens Blumen bl\u00fchn in deinen Thalen,", "tokens": ["E\u00b7dens", "Blu\u00b7men", "bl\u00fchn", "in", "dei\u00b7nen", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Edens Silberquellen", "tokens": ["E\u00b7dens", "Sil\u00b7ber\u00b7quel\u00b7len"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schlingen sich durch deine Schattenhaine,", "tokens": ["Schlin\u00b7gen", "sich", "durch", "dei\u00b7ne", "Schat\u00b7ten\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Buschumkr\u00e4nztes D\u00f6rfchen!", "tokens": ["Bu\u00b7schum\u00b7kr\u00e4nz\u00b7tes", "D\u00f6rf\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gottes Friede schwebt um deine H\u00fctten,", "tokens": ["Got\u00b7tes", "Frie\u00b7de", "schwebt", "um", "dei\u00b7ne", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn der Morgenr\u00f6the", "tokens": ["Wenn", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Rosenfarbner, goldbes\u00e4umter Schleier", "tokens": ["Ro\u00b7sen\u00b7farb\u00b7ner", ",", "gold\u00b7be\u00b7s\u00e4um\u00b7ter", "Schlei\u00b7er"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lieblich dich umwallet.", "tokens": ["Lieb\u00b7lich", "dich", "um\u00b7wal\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gottes Friede schwebt um deine H\u00fctten,", "tokens": ["Got\u00b7tes", "Frie\u00b7de", "schwebt", "um", "dei\u00b7ne", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Nachtigallen,", "tokens": ["Wenn", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Im Get\u00f6n der dumpfen Abendglokke,", "tokens": ["Im", "Ge\u00b7t\u00f6n", "der", "dum\u00b7pfen", "A\u00b7bend\u00b7glo\u00b7kke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Schlummerlieder fl\u00f6ten.", "tokens": ["Schlum\u00b7mer\u00b7lie\u00b7der", "fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In der D\u00e4mmrung deiner B\u00e4ume wandeln,", "tokens": ["In", "der", "D\u00e4mm\u00b7rung", "dei\u00b7ner", "B\u00e4u\u00b7me", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Arm in Arm geschlungen,", "tokens": ["Arm", "in", "Arm", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwesterlich wie einst im Garten Adams,", "tokens": ["Schwes\u00b7ter\u00b7lich", "wie", "einst", "im", "Gar\u00b7ten", "A\u00b7dams", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "APPRART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seelenruh und Unschuld.", "tokens": ["See\u00b7len\u00b7ruh", "und", "Un\u00b7schuld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "An der Himmelst\u00f6chter Busen trinken", "tokens": ["An", "der", "Him\u00b7mel\u00b7st\u00f6ch\u00b7ter", "Bu\u00b7sen", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchen, Greis, und J\u00fcngling,", "tokens": ["M\u00e4d\u00b7chen", ",", "Greis", ",", "und", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach des schw\u00fclen Erntetages M\u00fchen", "tokens": ["Nach", "des", "schw\u00fc\u00b7len", "Ern\u00b7te\u00b7ta\u00b7ges", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kraft und s\u00fcsse Labung.", "tokens": ["Kraft", "und", "s\u00fcs\u00b7se", "La\u00b7bung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Unter deinen Halmend\u00e4chern wohnen", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "Hal\u00b7men\u00b7d\u00e4\u00b7chern", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zucht und deutsche Treue,", "tokens": ["Zucht", "und", "deut\u00b7sche", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sie, die stolzer Marmors\u00e4le wildes", "tokens": ["Sie", ",", "die", "stol\u00b7zer", "Mar\u00b7mor\u00b7s\u00e4\u00b7le", "wil\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lustget\u00fcmmel fliehen.", "tokens": ["Lust\u00b7ge\u00b7t\u00fcm\u00b7mel", "flie\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Sittsamkeit ist deiner T\u00f6chter Erbe,", "tokens": ["Sitt\u00b7sam\u00b7keit", "ist", "dei\u00b7ner", "T\u00f6ch\u00b7ter", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gleich des Maienmorgens", "tokens": ["Gleich", "des", "Mai\u00b7en\u00b7mor\u00b7gens"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Purpur, gl\u00fcht in keuscher Jugendr\u00f6the", "tokens": ["Pur\u00b7pur", ",", "gl\u00fcht", "in", "keu\u00b7scher", "Ju\u00b7gend\u00b7r\u00f6\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre holde Wange.", "tokens": ["Ih\u00b7re", "hol\u00b7de", "Wan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Edel sind und kraftvoll deine S\u00f6hne!", "tokens": ["E\u00b7del", "sind", "und", "kraft\u00b7voll", "dei\u00b7ne", "S\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihrer starken Rechte,", "tokens": ["Ih\u00b7rer", "star\u00b7ken", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ungeschw\u00e4cht vom Feuerhauch der Wollust,", "tokens": ["Un\u00b7ge\u00b7schw\u00e4cht", "vom", "Feu\u00b7er\u00b7hauch", "der", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bl\u00fcht die \u00f6de W\u00fcste.", "tokens": ["Bl\u00fcht", "die", "\u00f6\u00b7de", "W\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "O da\u00df mir, o Dorf, in deinen Schatten", "tokens": ["O", "da\u00df", "mir", ",", "o", "Dorf", ",", "in", "dei\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meines Lebens B\u00e4chlein", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "B\u00e4ch\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ungesehn verr\u00e4nne, rein wie jener", "tokens": ["Un\u00b7ge\u00b7sehn", "ver\u00b7r\u00e4n\u00b7ne", ",", "rein", "wie", "je\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADJD", "KOKOM", "PDS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wiesenquelle Silber.", "tokens": ["Wie\u00b7sen\u00b7quel\u00b7le", "Sil\u00b7ber", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "O da\u00df endlich hier am treuen Busen", "tokens": ["O", "da\u00df", "end\u00b7lich", "hier", "am", "treu\u00b7en", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eines edlen Weibes,", "tokens": ["Ei\u00b7nes", "ed\u00b7len", "Wei\u00b7bes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die beweinte, l\u00e4ngstverlorne Ruhe", "tokens": ["Die", "be\u00b7wein\u00b7te", ",", "l\u00e4ngst\u00b7ver\u00b7lor\u00b7ne", "Ru\u00b7he"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wieder mich umarmte!", "tokens": ["Wie\u00b7der", "mich", "um\u00b7arm\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Dort am Abhang jenes Blumenh\u00fcgels,", "tokens": ["Dort", "am", "Ab\u00b7hang", "je\u00b7nes", "Blu\u00b7men\u00b7h\u00fc\u00b7gels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo durch Erlenreihen", "tokens": ["Wo", "durch", "Er\u00b7len\u00b7rei\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Silberblinkend sich das B\u00e4chlein windet,", "tokens": ["Sil\u00b7ber\u00b7blin\u00b7kend", "sich", "das", "B\u00e4ch\u00b7lein", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo die Linde fl\u00fcstert,", "tokens": ["Wo", "die", "Lin\u00b7de", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Wo die Eiche D\u00e4mmrung streut und K\u00fchle,", "tokens": ["Wo", "die", "Ei\u00b7che", "D\u00e4mm\u00b7rung", "streut", "und", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo des Thals Gestr\u00e4uche", "tokens": ["Wo", "des", "Thals", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihre gr\u00fcnen Lokken in des Seees", "tokens": ["Ih\u00b7re", "gr\u00fc\u00b7nen", "Lok\u00b7ken", "in", "des", "See\u00b7es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blauen Fluthen spiegeln,", "tokens": ["Blau\u00b7en", "Flut\u00b7hen", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "St\u00fcnde meine weinumrankte H\u00fctte,", "tokens": ["St\u00fcn\u00b7de", "mei\u00b7ne", "wei\u00b7num\u00b7rank\u00b7te", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gr\u00fcnte meine Laube,", "tokens": ["Gr\u00fcn\u00b7te", "mei\u00b7ne", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fchten meines Blumengartens Beete,", "tokens": ["Bl\u00fch\u00b7ten", "mei\u00b7nes", "Blu\u00b7men\u00b7gar\u00b7tens", "Bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Reiften meine Saaten.", "tokens": ["Reif\u00b7ten", "mei\u00b7ne", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Jenes Buchenhaines Fr\u00fchges\u00e4nge", "tokens": ["Je\u00b7nes", "Bu\u00b7chen\u00b7hai\u00b7nes", "Fr\u00fch\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wekten mich am Morgen,", "tokens": ["Wek\u00b7ten", "mich", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jenes Apfelbaumes Nachtigallen", "tokens": ["Je\u00b7nes", "Ap\u00b7fel\u00b7bau\u00b7mes", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00f6nten mich in Schlummer!", "tokens": ["T\u00f6n\u00b7ten", "mich", "in", "Schlum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Aber ach! der Hofnung Aug' ist tr\u00fcbe!", "tokens": ["A\u00b7ber", "ach", "!", "der", "Hof\u00b7nung", "Aug'", "ist", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "NN", "NN", "VAFIN", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tief der Zukunft Dunkel!", "tokens": ["Tief", "der", "Zu\u00b7kunft", "Dun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwebt nicht auf des Weltgewimmels Wogen", "tokens": ["Schwebt", "nicht", "auf", "des", "Welt\u00b7ge\u00b7wim\u00b7mels", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unst\u00e4t noch mein Nachen?", "tokens": ["Un\u00b7st\u00e4t", "noch", "mein", "Na\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": 