{"textgrid.poem.41442": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der vom Hasen betrogene L\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sinnreich macht die Furcht! Nicht weit von Bagdad wohnte", "tokens": ["Wie", "sinn\u00b7reich", "macht", "die", "Furcht", "!", "Nicht", "weit", "von", "Bag\u00b7dad", "wohn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$.", "PTKNEG", "ADJD", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein L\u00f6we, dessen Grimm und Raubsucht nichts verschonte.", "tokens": ["Ein", "L\u00f6\u00b7we", ",", "des\u00b7sen", "Grimm", "und", "Raub\u00b7sucht", "nichts", "ver\u00b7schon\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELAT", "NN", "KON", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der fra\u00df ohn' Unterschied, was die Gewalt ihm gab.", "tokens": ["Der", "fra\u00df", "ohn'", "Un\u00b7ter\u00b7schied", ",", "was", "die", "Ge\u00b7walt", "ihm", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald schickt ein banges Reich an ihn Gesandten ab,", "tokens": ["Bald", "schickt", "ein", "ban\u00b7ges", "Reich", "an", "ihn", "Ge\u00b7sand\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und die erbieten sich, um sicherer zu leben,", "tokens": ["Und", "die", "er\u00b7bie\u00b7ten", "sich", ",", "um", "si\u00b7che\u00b7rer", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PRF", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihm jeden Tag ein Thier zum Unterhalt zu geben.", "tokens": ["Ihm", "je\u00b7den", "Tag", "ein", "Thier", "zum", "Un\u00b7ter\u00b7halt", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Antrag wird erh\u00f6rt: er nennt die Liefrungszeit.", "tokens": ["Der", "An\u00b7trag", "wird", "er\u00b7h\u00f6rt", ":", "er", "nennt", "die", "Lief\u00b7rungs\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Botschaft heulet Dank f\u00fcr so viel G\u00fctigkeit.", "tokens": ["Die", "Bot\u00b7schaft", "heu\u00b7let", "Dank", "f\u00fcr", "so", "viel", "G\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie nun allein das Loos des Opfers Wahl bestimmte,", "tokens": ["Wie", "nun", "al\u00b7lein", "das", "Loos", "des", "Op\u00b7fers", "Wahl", "be\u00b7stimm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So traf's den Hasen auch, der zwar sich traurig kr\u00fcmmte,", "tokens": ["So", "traf's", "den", "Ha\u00b7sen", "auch", ",", "der", "zwar", "sich", "trau\u00b7rig", "kr\u00fcmm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch diesen Trost sich gab: Was sein soll, mu\u00df geschehn.", "tokens": ["Doch", "die\u00b7sen", "Trost", "sich", "gab", ":", "Was", "sein", "soll", ",", "mu\u00df", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PRF", "VVFIN", "$.", "PWS", "VAINF", "VMFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Euch, Freunde, zu befrein, will ich zum W\u00fcrger gehn;", "tokens": ["Euch", ",", "Freun\u00b7de", ",", "zu", "be\u00b7fr\u00b7ein", ",", "will", "ich", "zum", "W\u00fcr\u00b7ger", "gehn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PTKA", "ADJD", "$,", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Doch nach der Mittagszeit: es wird, wie ich vermeine,", "tokens": ["Doch", "nach", "der", "Mit\u00b7tags\u00b7zeit", ":", "es", "wird", ",", "wie", "ich", "ver\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der K\u00f6nig hungrig sein, wann ich vor ihm erscheine.", "tokens": ["Der", "K\u00f6\u00b7nig", "hung\u00b7rig", "sein", ",", "wann", "ich", "vor", "ihm", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er reiset. Da der L\u00f6w' ihn nur erkennen kann;", "tokens": ["Er", "rei\u00b7set", ".", "Da", "der", "L\u00f6\u00b7w'", "ihn", "nur", "er\u00b7ken\u00b7nen", "kann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Wie, ruft er, k\u00f6mmst denn du so sp\u00e4t und langsam an?", "tokens": ["Wie", ",", "ruft", "er", ",", "k\u00f6mmst", "denn", "du", "so", "sp\u00e4t", "und", "lang\u00b7sam", "an", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "KON", "PPER", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich habe meinen Weg durch jenen Wald genommen,", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Weg", "durch", "je\u00b7nen", "Wald", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Versetzt das schlaue Thier: sonst w\u00e4r' ich l\u00e4ngst gekommen.", "tokens": ["Ver\u00b7setzt", "das", "schlau\u00b7e", "Thier", ":", "sonst", "w\u00e4r'", "ich", "l\u00e4ngst", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach aller M\u00f6glichkeit beschleunigt' ich den Lauf:", "tokens": ["Nach", "al\u00b7ler", "M\u00f6g\u00b7lich\u00b7keit", "be\u00b7schleu\u00b7nigt'", "ich", "den", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mich hielt ein andrer L\u00f6w', der dir recht gleichet, auf,", "tokens": ["Mich", "hielt", "ein", "an\u00b7drer", "L\u00f6\u00b7w'", ",", "der", "dir", "recht", "glei\u00b7chet", ",", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Und droht', und wollte mir durchaus den Ruhm nicht lassen,", "tokens": ["Und", "droht'", ",", "und", "woll\u00b7te", "mir", "durc\u00b7haus", "den", "Ruhm", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "F\u00fcr dich, nach meiner Pflicht, mit Ehrfurcht zu erblassen.", "tokens": ["F\u00fcr", "dich", ",", "nach", "mei\u00b7ner", "Pflicht", ",", "mit", "Ehr\u00b7furcht", "zu", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mit Zittern sag' ich es: von deiner Majest\u00e4t", "tokens": ["Mit", "Zit\u00b7tern", "sag'", "ich", "es", ":", "von", "dei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sprach er verkleinerlich; auf dich hat er geschm\u00e4ht.", "tokens": ["Sprach", "er", "ver\u00b7klei\u00b7ner\u00b7lich", ";", "auf", "dich", "hat", "er", "ge\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$.", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Ich widerstritt ihm zwar, doch so wie Schw\u00e4chre pflegen;", "tokens": ["Ich", "wi\u00b7der\u00b7stritt", "ihm", "zwar", ",", "doch", "so", "wie", "Schw\u00e4ch\u00b7re", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "O k\u00f6nnt' ich jetzt sein Haubt zu deinen F\u00fc\u00dfen legen!", "tokens": ["O", "k\u00f6nnt'", "ich", "jetzt", "sein", "Haubt", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der L\u00f6we z\u00fcrnt, und br\u00fcllt: der Frevler und der Thor!", "tokens": ["Der", "L\u00f6\u00b7we", "z\u00fcrnt", ",", "und", "br\u00fcllt", ":", "der", "Frev\u00b7ler", "und", "der", "Thor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "KON", "VVFIN", "$.", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wir Helden ziehn den Sieg dem besten Fra\u00dfe vor.", "tokens": ["Wir", "Hel\u00b7den", "ziehn", "den", "Sieg", "dem", "bes\u00b7ten", "Fra\u00b7\u00dfe", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dem Afterk\u00f6nige will ich den Nacken beugen.", "tokens": ["Dem", "Af\u00b7ter\u00b7k\u00f6\u00b7ni\u00b7ge", "will", "ich", "den", "Na\u00b7cken", "beu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Gleich sollst du mit mir gehn, und seinen Sitz mir zeigen.", "tokens": ["Gleich", "sollst", "du", "mit", "mir", "gehn", ",", "und", "sei\u00b7nen", "Sitz", "mir", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "KON", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er geht ihm r\u00fcstig nach, und die\u00df ungleiche Paar", "tokens": ["Er", "geht", "ihm", "r\u00fcs\u00b7tig", "nach", ",", "und", "die\u00df", "un\u00b7glei\u00b7che", "Paar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "K\u00f6mmt bald an einen Born, der tief und heiter war,", "tokens": ["K\u00f6mmt", "bald", "an", "ei\u00b7nen", "Born", ",", "der", "tief", "und", "hei\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NE", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hier sieht der L\u00f6we sich, und glaubt den Feind zu sehen,", "tokens": ["Hier", "sieht", "der", "L\u00f6\u00b7we", "sich", ",", "und", "glaubt", "den", "Feind", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "PRF", "$,", "KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und fordert ihn heraus, den Zweikampf anzugehen.", "tokens": ["Und", "for\u00b7dert", "ihn", "he\u00b7raus", ",", "den", "Zwei\u00b7kampf", "an\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Vergebens! da er nun sich in den Brunnen st\u00fcrzt,", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "da", "er", "nun", "sich", "in", "den", "Brun\u00b7nen", "st\u00fcrzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wird schnell sein Regiment, und aller Noth verk\u00fcrzt.", "tokens": ["Wird", "schnell", "sein", "Re\u00b7gi\u00b7ment", ",", "und", "al\u00b7ler", "Noth", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$,", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}