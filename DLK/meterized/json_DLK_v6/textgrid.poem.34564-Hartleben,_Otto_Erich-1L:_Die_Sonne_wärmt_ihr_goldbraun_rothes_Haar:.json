{"textgrid.poem.34564": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne w\u00e4rmt ihr goldbraun rothes Haar:", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne w\u00e4rmt ihr goldbraun rothes Haar:", "tokens": ["Die", "Son\u00b7ne", "w\u00e4rmt", "ihr", "gold\u00b7braun", "ro\u00b7thes", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das leuchtet nun so tief, das spr\u00fcht so reich \u2013", "tokens": ["das", "leuch\u00b7tet", "nun", "so", "tief", ",", "das", "spr\u00fcht", "so", "reich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Prunk der Feste flammt um ihre Schl\u00e4fen!", "tokens": ["der", "Prunk", "der", "Fes\u00b7te", "flammt", "um", "ih\u00b7re", "Schl\u00e4\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man darf ihr nicht verrathen, wie so hoch,", "tokens": ["Man", "darf", "ihr", "nicht", "ver\u00b7ra\u00b7then", ",", "wie", "so", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,", "PWAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie sie so herrlich thront vor meinen Sinnen \u2013", "tokens": ["wie", "sie", "so", "herr\u00b7lich", "thront", "vor", "mei\u00b7nen", "Sin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sonst k\u00fcsst sie meine H\u00e4nde mir nicht mehr", "tokens": ["sonst", "k\u00fcsst", "sie", "mei\u00b7ne", "H\u00e4n\u00b7de", "mir", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und lacht nicht mehr so wie die Kinder lachen,", "tokens": ["und", "lacht", "nicht", "mehr", "so", "wie", "die", "Kin\u00b7der", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und macht wohl fremde Seelen unterthan ...", "tokens": ["und", "macht", "wohl", "frem\u00b7de", "See\u00b7len", "un\u00b7ter\u00b7than", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "das will die Sonne nicht.", "tokens": ["das", "will", "die", "Son\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}