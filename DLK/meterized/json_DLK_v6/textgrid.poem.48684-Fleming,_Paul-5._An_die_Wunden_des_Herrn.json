{"textgrid.poem.48684": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. An die Wunden des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Zuflucht meiner Angst, ihr aufgetanen Ritze,", "tokens": ["Ihr", "Zu\u00b7flucht", "mei\u00b7ner", "Angst", ",", "ihr", "auf\u00b7ge\u00b7ta\u00b7nen", "Rit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darin ich sicher bin, wenn der erz\u00fcrnte Gott", "tokens": ["da\u00b7rin", "ich", "si\u00b7cher", "bin", ",", "wenn", "der", "er\u00b7z\u00fcrn\u00b7te", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um meine S\u00fcnde schilt, woraus ich Furcht und Not,", "tokens": ["um", "mei\u00b7ne", "S\u00fcn\u00b7de", "schilt", ",", "wo\u00b7raus", "ich", "Furcht", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die aus dem Tod entsteht, recht bieten kan die Spitze,", "tokens": ["die", "aus", "dem", "Tod", "ent\u00b7steht", ",", "recht", "bie\u00b7ten", "kan", "die", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wie selig bin ich doch, wenn ich Erl\u00f6ster sitze", "tokens": ["wie", "se\u00b7lig", "bin", "ich", "doch", ",", "wenn", "ich", "Er\u00b7l\u00f6s\u00b7ter", "sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in eurer H\u00f6len Scho\u00df, in welcher Himmelbrot", "tokens": ["in", "eu\u00b7rer", "H\u00f6\u00b7len", "Scho\u00df", ",", "in", "wel\u00b7cher", "Him\u00b7mel\u00b7brot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "f\u00fcr meine Seele w\u00e4chst und fleu\u00dft so wei\u00df und rot", "tokens": ["f\u00fcr", "mei\u00b7ne", "See\u00b7le", "w\u00e4chst", "und", "fleu\u00dft", "so", "wei\u00df", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ADV", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der s\u00fc\u00dfe Lebensquell, aus dem ich mich bespritze.", "tokens": ["der", "s\u00fc\u00b7\u00dfe", "Le\u00b7bens\u00b7quell", ",", "aus", "dem", "ich", "mich", "be\u00b7sprit\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schlie\u00dft eure Kammern auf, ihr Friedensh\u00e4user ihr,", "tokens": ["Schlie\u00dft", "eu\u00b7re", "Kam\u00b7mern", "auf", ",", "ihr", "Frie\u00b7dens\u00b7h\u00e4u\u00b7ser", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "la\u00dft euer B\u00fcrgerrecht auch widerfahren mir.", "tokens": ["la\u00dft", "eu\u00b7er", "B\u00fcr\u00b7ger\u00b7recht", "auch", "wi\u00b7der\u00b7fah\u00b7ren", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr sollt mein Vaterland und stete Wohnstat hei\u00dfen.", "tokens": ["Ihr", "sollt", "mein", "Va\u00b7ter\u00b7land", "und", "ste\u00b7te", "Wohn\u00b7stat", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie selig werd' ich denn, wie \u00fcberselig sein,", "tokens": ["Wie", "se\u00b7lig", "werd'", "ich", "denn", ",", "wie", "\u00fc\u00b7ber\u00b7se\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn, wie ihr seid mein Haus, so sein wolt auch mein Schrein,", "tokens": ["wenn", ",", "wie", "ihr", "seid", "mein", "Haus", ",", "so", "sein", "wolt", "auch", "mein", "Schrein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VAINF", "VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "aus dem mich auch die Hand der H\u00f6llen nicht wird rei\u00dfen!", "tokens": ["aus", "dem", "mich", "auch", "die", "Hand", "der", "H\u00f6l\u00b7len", "nicht", "wird", "rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}