{"dta.poem.8242": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Rechnenstunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist so ein kleines M\u00e4dchen,", "tokens": ["Du", "bist", "so", "ein", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hast schon so helle Augen;", "tokens": ["Und", "hast", "schon", "so", "hel\u00b7le", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Du bist so ein kleines M\u00e4dchen,", "tokens": ["Du", "bist", "so", "ein", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und hast schon so rothe Lippen!", "tokens": ["Und", "hast", "schon", "so", "ro\u00b7the", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Nun schau mich nur an, du Kleine,", "tokens": ["Nun", "schau", "mich", "nur", "an", ",", "du", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auch ich hab' helle Augen,", "tokens": ["Auch", "ich", "hab'", "hel\u00b7le", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df dir Alles deuten \u2014", "tokens": ["Und", "la\u00df", "dir", "Al\u00b7les", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch ich hab' rothe Lippen.", "tokens": ["Auch", "ich", "hab'", "ro\u00b7the", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun rechne mir doch zusammen!", "tokens": ["Nun", "rech\u00b7ne", "mir", "doch", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vier Augen, die geben? \u2014 Blicke!", "tokens": ["Vier", "Au\u00b7gen", ",", "die", "ge\u00b7ben", "?", "Bli\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "VVINF", "$.", "$(", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u2014 mach' mir keinen Fehler!", "tokens": ["Und", "mach'", "mir", "kei\u00b7nen", "Feh\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vier Lippen, die geben? \u2014 K\u00fcsse!", "tokens": ["Vier", "Lip\u00b7pen", ",", "die", "ge\u00b7ben", "?", "K\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "VVINF", "$.", "$(", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}