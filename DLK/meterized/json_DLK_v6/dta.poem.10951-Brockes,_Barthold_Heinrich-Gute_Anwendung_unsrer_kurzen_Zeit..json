{"dta.poem.10951": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gute Anwendung  \n unsrer kurzen Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Nichts von aller Ewigkeit,", "tokens": ["Ich", "war", "ein", "Nichts", "von", "al\u00b7ler", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und werde wenigstens auf dieser Erden", "tokens": ["Und", "wer\u00b7de", "we\u00b7nigs\u00b7tens", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Nichts aufs neue wieder werden.", "tokens": ["Ein", "Nichts", "aufs", "neu\u00b7e", "wie\u00b7der", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "ADJA", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die du mir, blo\u00df aus Gnad, hienieden,", "tokens": ["Die", "du", "mir", ",", "blo\u00df", "aus", "Gnad", ",", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PPER", "$,", "ADV", "APPR", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu leben, und zu seyn, beschieden,", "tokens": ["Zu", "le\u00b7ben", ",", "und", "zu", "seyn", ",", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "PTKZU", "VAINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu deinem Ruhm und Preis zu leben,", "tokens": ["Zu", "dei\u00b7nem", "Ruhm", "und", "Preis", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In deiner weisen Macht Betrachtung, mich bestreben!", "tokens": ["In", "dei\u00b7ner", "wei\u00b7sen", "Macht", "Be\u00b7trach\u00b7tung", ",", "mich", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}