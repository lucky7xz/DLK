{"textgrid.poem.57451": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Andrer Vater deutscher Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andrer Vater deutscher Lieder,", "tokens": ["A\u00b7ndrer", "Va\u00b7ter", "deut\u00b7scher", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edler ", "tokens": ["Ed\u00b7ler"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Komm, erneure doch den Ton", "tokens": ["Komm", ",", "er\u00b7neu\u00b7re", "doch", "den", "Ton"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner edlen Laute wieder!", "tokens": ["Dei\u00b7ner", "ed\u00b7len", "Lau\u00b7te", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast du ", "tokens": ["Hast", "du"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und die wilde ", "tokens": ["Und", "die", "wil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ja die ", "tokens": ["Ja", "die"], "token_info": ["word", "word"], "pos": ["PTKANT", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Was die Kunst der Musen sey;", "tokens": ["Was", "die", "Kunst", "der", "Mu\u00b7sen", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O so stimme, wie vorzeiten,", "tokens": ["O", "so", "stim\u00b7me", ",", "wie", "vor\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meiner Zither schlaffe Seyten.", "tokens": ["Mei\u00b7ner", "Zi\u00b7ther", "schlaf\u00b7fe", "Sey\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stund nicht Liefland halb vernarret?", "tokens": ["Stund", "nicht", "Lie\u00b7fland", "halb", "ver\u00b7nar\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stutzte nicht der kalte Belt?", "tokens": ["Stutz\u00b7te", "nicht", "der", "kal\u00b7te", "Belt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War die halbe Norderwelt,", "tokens": ["War", "die", "hal\u00b7be", "Nor\u00b7der\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zur ", "tokens": ["Bis", "zur"], "token_info": ["word", "word"], "pos": ["APPR", "APPRART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Deines hohen Geistes Feuer", "tokens": ["Dei\u00b7nes", "ho\u00b7hen", "Geis\u00b7tes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmelzte Ru\u00dflands tiefsten Schnee;", "tokens": ["Schmelz\u00b7te", "Ru\u00df\u00b7lands", "tiefs\u00b7ten", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja das Eis ward endlich theuer", "tokens": ["Ja", "das", "Eis", "ward", "end\u00b7lich", "theu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An der runden ", "tokens": ["An", "der", "run\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "O wo ist von deinen Trieben", "tokens": ["O", "wo", "ist", "von", "dei\u00b7nen", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die verglimmte Glut geblieben.", "tokens": ["Die", "ver\u00b7glimm\u00b7te", "Glut", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eben die besch\u00e4umten Wellen,", "tokens": ["E\u00b7ben", "die", "be\u00b7sch\u00e4um\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Spiel ich itzo bin,", "tokens": ["De\u00b7ren", "Spiel", "ich", "it\u00b7zo", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah ja dein gesetzter Sinn", "tokens": ["Sah", "ja", "dein", "ge\u00b7setz\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vormals zu den Wolken schwellen.", "tokens": ["Vor\u00b7mals", "zu", "den", "Wol\u00b7ken", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber wenn sie rauschend rollten,", "tokens": ["A\u00b7ber", "wenn", "sie", "rau\u00b7schend", "roll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und durch ihrer Fluthen Fall", "tokens": ["Und", "durch", "ih\u00b7rer", "Flut\u00b7hen", "Fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Lieder d\u00e4mpfen wollten,", "tokens": ["Dei\u00b7ne", "Lie\u00b7der", "d\u00e4mp\u00b7fen", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "D\u00e4mpfte ", "tokens": ["D\u00e4mpf\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wenn sie dich von weiten h\u00f6rte.", "tokens": ["Wenn", "sie", "dich", "von", "wei\u00b7ten", "h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sagt, ihr blaulichten ", "tokens": ["Sagt", ",", "ihr", "blau\u00b7lich\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Warum h\u00f6rt ihr mir nicht zu?", "tokens": ["Wa\u00b7rum", "h\u00f6rt", "ihr", "mir", "nicht", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum wollt ihr meine Ruh", "tokens": ["Wa\u00b7rum", "wollt", "ihr", "mei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht so wohl, als ", "tokens": ["Nicht", "so", "wohl", ",", "als"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADV", "ADV", "$,", "KOUS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Doch ich kann es leichtlich merken:", "tokens": ["Doch", "ich", "kann", "es", "leicht\u00b7lich", "mer\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnt ich nur mein Seytenspiel", "tokens": ["K\u00f6nnt", "ich", "nur", "mein", "Sey\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Recht nach seiner Laute st\u00e4rken,", "tokens": ["Recht", "nach", "sei\u00b7ner", "Lau\u00b7te", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die euch damals wohlgefiel;", "tokens": ["Die", "euch", "da\u00b7mals", "wohl\u00b7ge\u00b7fiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00fcrdet ihr zu meinem Dichten", "tokens": ["W\u00fcr\u00b7det", "ihr", "zu", "mei\u00b7nem", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Williger die Ohren richten.", "tokens": ["Wil\u00b7li\u00b7ger", "die", "Oh\u00b7ren", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Raast denn, raast, ihr Wasserwogen!", "tokens": ["Raast", "denn", ",", "raast", ",", "ihr", "Was\u00b7ser\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spritzt und sch\u00e4umet noch so viel;", "tokens": ["Spritzt", "und", "sch\u00e4u\u00b7met", "noch", "so", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein verwerflich Seytenspiel", "tokens": ["Mein", "ver\u00b7werf\u00b7lich", "Sey\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mit eigner Hand bezogen.", "tokens": ["Ist", "mit", "eig\u00b7ner", "Hand", "be\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4lzt euch, ihr gesalznen H\u00fcgel!", "tokens": ["W\u00e4lzt", "euch", ",", "ihr", "ge\u00b7salz\u00b7nen", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwemmt mein Schiff durch Sturm und Wind,", "tokens": ["Schwemmt", "mein", "Schiff", "durch", "Sturm", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dessen ausgespannte Fl\u00fcgel", "tokens": ["Des\u00b7sen", "aus\u00b7ge\u00b7spann\u00b7te", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eure nasse Nachbarn sind;", "tokens": ["Eu\u00b7re", "nas\u00b7se", "Nach\u00b7barn", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber endlich legt euch wieder,", "tokens": ["A\u00b7ber", "end\u00b7lich", "legt", "euch", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und vernehmt auch meine Lieder.", "tokens": ["Und", "ver\u00b7nehmt", "auch", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich besinge mit Vergn\u00fcgen", "tokens": ["Ich", "be\u00b7sin\u00b7ge", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein verla\u00dfnes Vaterland,", "tokens": ["Mein", "ver\u00b7la\u00df\u00b7nes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich an ", "tokens": ["Wo", "ich", "an"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Den Parna\u00df zuerst bestiegen;", "tokens": ["Den", "Par\u00b7na\u00df", "zu\u00b7erst", "be\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Der des Pregels breiter R\u00fccken", "tokens": ["Der", "des", "Pre\u00b7gels", "brei\u00b7ter", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle L\u00e4nder zinsbar macht;", "tokens": ["Al\u00b7le", "L\u00e4n\u00b7der", "zins\u00b7bar", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wo mit ge\u00fcbten Zungen", "tokens": ["Und", "wo", "mit", "ge\u00b7\u00fcb\u00b7ten", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann erheb ich, ausser Preu\u00dfen,", "tokens": ["Dann", "er\u00b7heb", "ich", ",", "aus\u00b7ser", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sachsens sch\u00f6ne Lindenstadt,", "tokens": ["Sach\u00b7sens", "sch\u00f6\u00b7ne", "Lin\u00b7den\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das gl\u00fcckerf\u00fcllte Mei\u00dfen.", "tokens": ["Und", "das", "gl\u00fc\u00b7cker\u00b7f\u00fcll\u00b7te", "Mei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas besser ausgespielt,", "tokens": ["Et\u00b7was", "bes\u00b7ser", "aus\u00b7ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im Chore der Poeten", "tokens": ["Und", "im", "Cho\u00b7re", "der", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Manches Kenners Lob erzielt;", "tokens": ["Man\u00b7ches", "Ken\u00b7ners", "Lob", "er\u00b7zielt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als von ", "tokens": ["Als", "von"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Mir ein Heldenlied gerathen.", "tokens": ["Mir", "ein", "Hel\u00b7den\u00b7lied", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {