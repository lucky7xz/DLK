{"textgrid.poem.39423": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft hab' ich dich rauh gescholten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft hab' ich dich rauh gescholten", "tokens": ["Oft", "hab'", "ich", "dich", "rauh", "ge\u00b7schol\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Muttersprache, so vertraut!", "tokens": ["Mut\u00b7ter\u00b7spra\u00b7che", ",", "so", "ver\u00b7traut", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6her h\u00e4tte mir gegolten", "tokens": ["H\u00f6\u00b7her", "h\u00e4t\u00b7te", "mir", "ge\u00b7gol\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fcdlicher Sirenenlaut.", "tokens": ["S\u00fcd\u00b7li\u00b7cher", "Si\u00b7re\u00b7nen\u00b7laut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Und nun irr' ich in der Ferne", "tokens": ["Und", "nun", "irr'", "ich", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudenlos von Ort zu Ort,", "tokens": ["Freu\u00b7den\u00b7los", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vern\u00e4hm', ach wie so gerne!", "tokens": ["Und", "ver\u00b7n\u00e4hm'", ",", "ach", "wie", "so", "ger\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "KOKOM", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein einzig deutsches Wort.", "tokens": ["Nur", "ein", "ein\u00b7zig", "deut\u00b7sches", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manches regt sich mir im Innern,", "tokens": ["Man\u00b7ches", "regt", "sich", "mir", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch wie schaff' ich hier ihm Luft?", "tokens": ["Doch", "wie", "schaff'", "ich", "hier", "ihm", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein kindliches Erinnern", "tokens": ["All", "mein", "kind\u00b7li\u00b7ches", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Findet in mir seine Gruft.", "tokens": ["Fin\u00b7det", "in", "mir", "sei\u00b7ne", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einsam schweif' ich in die Felder,", "tokens": ["Ein\u00b7sam", "schweif'", "ich", "in", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Such' ein Echo der Natur;", "tokens": ["Such'", "ein", "E\u00b7cho", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber B\u00e4che, Winde, W\u00e4lder", "tokens": ["A\u00b7ber", "B\u00e4\u00b7che", ",", "Win\u00b7de", ",", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauschen fremd auf dieser Flur.", "tokens": ["Rau\u00b7schen", "fremd", "auf", "die\u00b7ser", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unverstanden, unbeachtet,", "tokens": ["Un\u00b7ver\u00b7stan\u00b7den", ",", "un\u00b7be\u00b7ach\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mein deutsches Lied verhallt,", "tokens": ["Wie", "mein", "deut\u00b7sches", "Lied", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibt es, wann mein Busen schmachtet,", "tokens": ["Bleibt", "es", ",", "wann", "mein", "Bu\u00b7sen", "schmach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in bangem Sehnen wallt.", "tokens": ["Und", "in", "ban\u00b7gem", "Seh\u00b7nen", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}