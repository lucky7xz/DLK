{"textgrid.poem.51037": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Mittagsglast, auf des Gebirges Grat", "tokens": ["Im", "Mit\u00b7tags\u00b7glast", ",", "auf", "des", "Ge\u00b7bir\u00b7ges", "Grat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schlief unter alten Fichten m\u00fcd ich ein;", "tokens": ["Schlief", "un\u00b7ter", "al\u00b7ten", "Fich\u00b7ten", "m\u00fcd", "ich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schlief und tr\u00e4umte bis zum Abendschein", "tokens": ["Ich", "schlief", "und", "tr\u00e4um\u00b7te", "bis", "zum", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von leerem Hoffen und verlorner Tat.", "tokens": ["Von", "lee\u00b7rem", "Hof\u00b7fen", "und", "ver\u00b7lor\u00b7ner", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schlaftrunken und verwirrt erwacht ich spat:", "tokens": ["Schlaf\u00b7trun\u00b7ken", "und", "ver\u00b7wirrt", "er\u00b7wacht", "ich", "spat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ger\u00f6tet war ringsum Geb\u00fcsch und Stein,", "tokens": ["Ge\u00b7r\u00f6\u00b7tet", "war", "ring\u00b7sum", "Ge\u00b7b\u00fcsch", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Des Hochgebirges Eishaupt und Gebein,", "tokens": ["Des", "Hoch\u00b7ge\u00b7bir\u00b7ges", "Eis\u00b7haupt", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Horizont ein spr\u00fchend Feuerrad.", "tokens": ["Der", "Ho\u00b7ri\u00b7zont", "ein", "spr\u00fc\u00b7hend", "Feu\u00b7er\u00b7rad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und rascher f\u00fchlt ich meine Pulse gehen,", "tokens": ["Und", "ra\u00b7scher", "f\u00fchlt", "ich", "mei\u00b7ne", "Pul\u00b7se", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hielt die Glut f\u00fcr lichtes Morgenrot,", "tokens": ["Ich", "hielt", "die", "Glut", "f\u00fcr", "lich\u00b7tes", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erharrend nun der Sonne Auferstehen.", "tokens": ["Er\u00b7har\u00b7rend", "nun", "der", "Son\u00b7ne", "Auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch Berg um Berg versank in Schlaf und Tod,", "tokens": ["Doch", "Berg", "um", "Berg", "ver\u00b7sank", "in", "Schlaf", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht stieg auf mit frostig rauhem Wehen", "tokens": ["Die", "Nacht", "stieg", "auf", "mit", "fros\u00b7tig", "rau\u00b7hem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit dem Mond des Herzens alte Not.", "tokens": ["Und", "mit", "dem", "Mond", "des", "Her\u00b7zens", "al\u00b7te", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}