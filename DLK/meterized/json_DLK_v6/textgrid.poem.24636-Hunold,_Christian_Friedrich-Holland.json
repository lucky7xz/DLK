{"textgrid.poem.24636": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Holland", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? wo ist unser Held/ so mit dem L\u00f6wen wacht/", "tokens": ["Wie", "?", "wo", "ist", "un\u00b7ser", "Held", "/", "so", "mit", "dem", "L\u00f6\u00b7wen", "wacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinen Ruhm bey uns in vollen Flor gebracht?", "tokens": ["Und", "sei\u00b7nen", "Ruhm", "bey", "uns", "in", "vol\u00b7len", "Flor", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schon vor l\u00e4ngsten hat so manche Helden-Proben", "tokens": ["Der", "schon", "vor", "l\u00e4ngs\u00b7ten", "hat", "so", "man\u00b7che", "Hel\u00b7den\u00b7Pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Braband abgelegt/ den Flandern wei\u00df zu loben?", "tokens": ["In", "Bra\u00b7band", "ab\u00b7ge\u00b7legt", "/", "den", "Flan\u00b7dern", "wei\u00df", "zu", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$(", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der tapffre General/ Durchlauchtigsten Gebl\u00fcts/", "tokens": ["Der", "tapf\u00b7fre", "Ge\u00b7ne\u00b7ral", "/", "Durch\u00b7lauch\u00b7tigs\u00b7ten", "Ge\u00b7bl\u00fcts", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Von munterem Gesicht/ und redlichen Gem\u00fcths/", "tokens": ["Von", "mun\u00b7te\u00b7rem", "Ge\u00b7sicht", "/", "und", "red\u00b7li\u00b7chen", "Ge\u00b7m\u00fcths", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der gr\u00fcne Rauten Zweig der h\u00f6chst belobten Sachsen?", "tokens": ["Der", "gr\u00fc\u00b7ne", "Rau\u00b7ten", "Zweig", "der", "h\u00f6chst", "be\u00b7lob\u00b7ten", "Sach\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein theures F\u00fcrsten-Blut soll ferner bey uns wachsen.", "tokens": ["Sein", "theu\u00b7res", "F\u00fcrs\u00b7ten\u00b7Blut", "soll", "fer\u00b7ner", "bey", "uns", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}