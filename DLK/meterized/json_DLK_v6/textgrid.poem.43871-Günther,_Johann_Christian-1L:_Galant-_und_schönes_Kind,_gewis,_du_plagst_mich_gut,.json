{"textgrid.poem.43871": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Galant- und sch\u00f6nes Kind, gewis, du plagst mich gut,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Galant- und sch\u00f6nes Kind, gewis, du plagst mich gut,", "tokens": ["Ga\u00b7lant", "und", "sch\u00f6\u00b7nes", "Kind", ",", "ge\u00b7wis", ",", "du", "plagst", "mich", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$,", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir den versprochnen Reim, doch sonder Eil, zu machen;", "tokens": ["Dir", "den", "ver\u00b7sproch\u00b7nen", "Reim", ",", "doch", "son\u00b7der", "Eil", ",", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein was denckstu denn dergleichen albre Sachen?", "tokens": ["Al\u00b7lein", "was", "dencks\u00b7tu", "denn", "derg\u00b7lei\u00b7chen", "alb\u00b7re", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PDS", "ADV", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du weist ja, da\u00df kein Mensch etwas umsonsten thut;", "tokens": ["Du", "weist", "ja", ",", "da\u00df", "kein", "Mensch", "et\u00b7was", "um\u00b7sons\u00b7ten", "thut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIAT", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch kan ich dadurch mir einen Ku\u00df verdienen,", "tokens": ["Je\u00b7doch", "kan", "ich", "da\u00b7durch", "mir", "ei\u00b7nen", "Ku\u00df", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So soltu eilends sehn, was jezt noch nicht erschienen.", "tokens": ["So", "sol\u00b7tu", "ei\u00b7lends", "sehn", ",", "was", "jezt", "noch", "nicht", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VVINF", "$,", "PRELS", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}