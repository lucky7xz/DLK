{"textgrid.poem.44454": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1. B\u00f6se Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begeisterung, was ruf ich dir", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", ",", "was", "ruf", "ich", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fleh dich fruchtlos an?", "tokens": ["Und", "fleh", "dich", "frucht\u00b7los", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begeisterung? Wornach? Wof\u00fcr?", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "?", "Wor\u00b7nach", "?", "Wo\u00b7f\u00fcr", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PWAV", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du selbst\u00e4ndig au\u00dfer mir?", "tokens": ["Bist", "du", "selb\u00b7st\u00e4n\u00b7dig", "au\u00b7\u00dfer", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dir? Und wo und wann?", "tokens": ["In", "dir", "?", "Und", "wo", "und", "wann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "KON", "PWAV", "KON", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sag mir, wo du dein Haus gebaut,", "tokens": ["Sag", "mir", ",", "wo", "du", "dein", "Haus", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch Zauber dich bewacht;", "tokens": ["Welch", "Zau\u00b7ber", "dich", "be\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voraus dich nehmend, hochvertraut,", "tokens": ["Vo\u00b7raus", "dich", "neh\u00b7mend", ",", "hoch\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hol ich begeistert dich als Braut,", "tokens": ["Hol", "ich", "be\u00b7geis\u00b7tert", "dich", "als", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Durch Sturm und Kampf und Nacht.", "tokens": ["Durch", "Sturm", "und", "Kampf", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Begeistert f\u00fcr Begeisterung?", "tokens": ["Be\u00b7geis\u00b7tert", "f\u00fcr", "Be\u00b7geis\u00b7te\u00b7rung", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weg zugleich das Ziel?", "tokens": ["Der", "Weg", "zu\u00b7gleich", "das", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer ist so unge\u00fcbt und jung,", "tokens": ["Wer", "ist", "so", "un\u00b7ge\u00b7\u00fcbt", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nicht gewahrt den argen Sprung?", "tokens": ["Der", "nicht", "ge\u00b7wahrt", "den", "ar\u00b7gen", "Sprung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Du also selber fehlest nicht.", "tokens": ["Du", "al\u00b7so", "sel\u00b7ber", "feh\u00b7lest", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sonst denn, wenn ich kalt? \u2013", "tokens": ["Was", "sonst", "denn", ",", "wenn", "ich", "kalt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4rst etwa du die Flamm am Licht,", "tokens": ["W\u00e4rst", "et\u00b7wa", "du", "die", "Flamm", "am", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verl\u00f6schend, wenns an Stoff gebricht,", "tokens": ["Ver\u00b7l\u00f6\u00b7schend", ",", "wenns", "an", "Stoff", "ge\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Nahrung, an Gehalt?", "tokens": ["An", "Nah\u00b7rung", ",", "an", "Ge\u00b7halt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "W\u00e4rst du das Wie und brauchst ein Was?", "tokens": ["W\u00e4rst", "du", "das", "Wie", "und", "brauchst", "ein", "Was", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Was durch ein Warum?", "tokens": ["Nur", "Was", "durch", "ein", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "ART", "PWAV", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wer Wasser sch\u00f6pft ohn Unterla\u00df", "tokens": ["Wer", "Was\u00b7ser", "sch\u00f6pft", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00f6pft ins Danaidenfa\u00df,", "tokens": ["Und", "sch\u00f6pft", "ins", "Da\u00b7na\u00b7i\u00b7den\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Treibt wohl sich fruchtlos um.", "tokens": ["Treibt", "wohl", "sich", "frucht\u00b7los", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum auf ins Leben, mutbew\u00e4hrt!", "tokens": ["Drum", "auf", "ins", "Le\u00b7ben", ",", "mut\u00b7be\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "APPR", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gestrebt, geliebt, geha\u00dft!", "tokens": ["Ge\u00b7strebt", ",", "ge\u00b7liebt", ",", "ge\u00b7ha\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist dir der Stoff erst, der sie n\u00e4hrt,", "tokens": ["Ist", "dir", "der", "Stoff", "erst", ",", "der", "sie", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4llt Glut vom Himmel auf den Herd", "tokens": ["F\u00e4llt", "Glut", "vom", "Him\u00b7mel", "auf", "den", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lodert ohne Rast.", "tokens": ["Und", "lo\u00b7dert", "oh\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}