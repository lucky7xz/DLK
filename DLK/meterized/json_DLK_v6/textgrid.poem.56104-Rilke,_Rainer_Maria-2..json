{"textgrid.poem.56104": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat bedacht, da\u00df bis zu ihrem Kommen", "tokens": ["Wer", "hat", "be\u00b7dacht", ",", "da\u00df", "bis", "zu", "ih\u00b7rem", "Kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "VVPP", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der viele Himmel unvollst\u00e4ndig war?", "tokens": ["der", "vie\u00b7le", "Him\u00b7mel", "un\u00b7voll\u00b7st\u00e4n\u00b7dig", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Auferstandne hatte Platz genommen,", "tokens": ["Der", "Auf\u00b7er\u00b7stand\u00b7ne", "hat\u00b7te", "Platz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "doch neben ihm, durch vierundzwanzig Jahr,", "tokens": ["doch", "ne\u00b7ben", "ihm", ",", "durch", "vie\u00b7rund\u00b7zwan\u00b7zig", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "APPR", "CARD", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "war leer der Sitz. Und sie begannen schon", "tokens": ["war", "leer", "der", "Sitz", ".", "Und", "sie", "be\u00b7gan\u00b7nen", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "sich an die reine L\u00fccke zu gew\u00f6hnen,", "tokens": ["sich", "an", "die", "rei\u00b7ne", "L\u00fc\u00b7cke", "zu", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "die wie verheilt war, denn mit seinem sch\u00f6nen", "tokens": ["die", "wie", "ver\u00b7heilt", "war", ",", "denn", "mit", "sei\u00b7nem", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "VVPP", "VAFIN", "$,", "KON", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hin\u00fcberscheinen f\u00fcllte sie der Sohn.", "tokens": ["Hin\u00b7\u00fc\u00b7bersc\u00b7hei\u00b7nen", "f\u00fcll\u00b7te", "sie", "der", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ging auch sie, die in die Himmel trat,", "tokens": ["So", "ging", "auch", "sie", ",", "die", "in", "die", "Him\u00b7mel", "trat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nicht auf ihn zu, so sehr es sie verlangte;", "tokens": ["nicht", "auf", "ihn", "zu", ",", "so", "sehr", "es", "sie", "ver\u00b7lang\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "PTKVZ", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dort war kein Platz, nur ", "tokens": ["dort", "war", "kein", "Platz", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "mit einer Strahlung, die ihr wehe tat.", "tokens": ["mit", "ei\u00b7ner", "Strah\u00b7lung", ",", "die", "ihr", "we\u00b7he", "tat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch da sie jetzt, die r\u00fchrende Gestalt,", "tokens": ["Doch", "da", "sie", "jetzt", ",", "die", "r\u00fch\u00b7ren\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.6": {"text": "sich zu den neuen Seligen gesellte", "tokens": ["sich", "zu", "den", "neu\u00b7en", "Se\u00b7li\u00b7gen", "ge\u00b7sell\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und unauff\u00e4llig, licht zu licht, sich stellte,", "tokens": ["und", "un\u00b7auf\u00b7f\u00e4l\u00b7lig", ",", "licht", "zu", "licht", ",", "sich", "stell\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "APPR", "NN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "da brach aus ihrem Sein ein Hinterhalt", "tokens": ["da", "brach", "aus", "ih\u00b7rem", "Sein", "ein", "Hin\u00b7ter\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "von solchem Glanz, da\u00df der von ihr erhellte", "tokens": ["von", "sol\u00b7chem", "Glanz", ",", "da\u00df", "der", "von", "ihr", "er\u00b7hell\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "ART", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Engel geblendet aufschrie: Wer ist die?", "tokens": ["En\u00b7gel", "ge\u00b7blen\u00b7det", "auf\u00b7schrie", ":", "Wer", "ist", "die", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VVFIN", "$.", "PWS", "VAFIN", "ART", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Ein Staunen war. Dann sahn sie alle, wie", "tokens": ["Ein", "Stau\u00b7nen", "war", ".", "Dann", "sahn", "sie", "al\u00b7le", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "PIS", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Gott-Vater oben unsern Herrn verhielt,", "tokens": ["Got\u00b7tVa\u00b7ter", "o\u00b7ben", "un\u00b7sern", "Herrn", "ver\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "so da\u00df, von milder D\u00e4mmerung umspielt,", "tokens": ["so", "da\u00df", ",", "von", "mil\u00b7der", "D\u00e4m\u00b7me\u00b7rung", "um\u00b7spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "die leere Stelle wie ein wenig Leid", "tokens": ["die", "lee\u00b7re", "Stel\u00b7le", "wie", "ein", "we\u00b7nig", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "sich zeigte, eine Spur von Einsamkeit,", "tokens": ["sich", "zeig\u00b7te", ",", "ei\u00b7ne", "Spur", "von", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "wie etwas, was er noch ertrug, ein Rest", "tokens": ["wie", "et\u00b7was", ",", "was", "er", "noch", "er\u00b7trug", ",", "ein", "Rest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "irdischer Zeit, ein trockenes Gebrest \u2013.", "tokens": ["ir\u00b7di\u00b7scher", "Zeit", ",", "ein", "tro\u00b7cke\u00b7nes", "Ge\u00b7brest", "\u2013", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$(", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Man sah nach ihr; sie schaute \u00e4ngstlich hin,", "tokens": ["Man", "sah", "nach", "ihr", ";", "sie", "schau\u00b7te", "\u00e4ngst\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "weit vorgeneigt, als f\u00fchlte sie: ", "tokens": ["weit", "vor\u00b7ge\u00b7neigt", ",", "als", "f\u00fchl\u00b7te", "sie", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "sein l\u00e4ngster Schmerz \u2013: und st\u00fcrzte pl\u00f6tzlich vor.", "tokens": ["sein", "l\u00e4ngs\u00b7ter", "Schmerz", "\u2013", ":", "und", "st\u00fcrz\u00b7te", "pl\u00f6tz\u00b7lich", "vor", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "$.", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die Engel aber nahmen sie zu sich", "tokens": ["Die", "En\u00b7gel", "a\u00b7ber", "nah\u00b7men", "sie", "zu", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "und st\u00fctzten sie und sangen seliglich", "tokens": ["und", "st\u00fctz\u00b7ten", "sie", "und", "san\u00b7gen", "se\u00b7lig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "und trugen sie das letzte St\u00fcck empor.", "tokens": ["und", "tru\u00b7gen", "sie", "das", "letz\u00b7te", "St\u00fcck", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}