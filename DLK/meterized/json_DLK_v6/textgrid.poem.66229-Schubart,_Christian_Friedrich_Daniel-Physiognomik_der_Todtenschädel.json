{"textgrid.poem.66229": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Physiognomik der Todtensch\u00e4del", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gro\u00dfe Sch\u00e4del, nur halb kahl,", "tokens": ["Der", "gro\u00b7\u00dfe", "Sch\u00e4\u00b7del", ",", "nur", "halb", "kahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit breiter Stirne, hart, wie Stahl,", "tokens": ["Mit", "brei\u00b7ter", "Stir\u00b7ne", ",", "hart", ",", "wie", "Stahl", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diese Knochen, fest, wie Stein:", "tokens": ["Und", "die\u00b7se", "Kno\u00b7chen", ",", "fest", ",", "wie", "Stein", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PTKVZ", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wem m\u00f6gen die gewesen sein?", "tokens": ["Wem", "m\u00f6\u00b7gen", "die", "ge\u00b7we\u00b7sen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dumpf sprach der Genius, der um das Beinhaus schwebt:", "tokens": ["Dumpf", "sprach", "der", "Ge\u00b7nius", ",", "der", "um", "das", "Be\u00b7in\u00b7haus", "schwebt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Es war ein Deutscher, der naturgem\u00e4\u00df gelebt.", "tokens": ["Es", "war", "ein", "Deut\u00b7scher", ",", "der", "na\u00b7tur\u00b7ge\u00b7m\u00e4\u00df", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dieses Sch\u00e4delchen hier,", "tokens": ["Und", "die\u00b7ses", "Sch\u00e4\u00b7del\u00b7chen", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So wei\u00df, so d\u00fcnn, wie Postpapier;", "tokens": ["So", "wei\u00df", ",", "so", "d\u00fcnn", ",", "wie", "Post\u00b7pa\u00b7pier", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diese Gebeinlein dabei,", "tokens": ["Und", "die\u00b7se", "Ge\u00b7bein\u00b7lein", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PAV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wie Marzipan wei\u00df, und weich wie Brei:", "tokens": ["Wie", "Mar\u00b7zi\u00b7pan", "wei\u00df", ",", "und", "weich", "wie", "Brei", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wer war denn die\u00df?", "tokens": ["Wer", "war", "denn", "die\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Geck aus Paris!", "tokens": ["Ein", "Geck", "aus", "Pa\u00b7ris", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}