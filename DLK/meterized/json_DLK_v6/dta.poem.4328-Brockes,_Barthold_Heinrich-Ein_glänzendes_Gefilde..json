{"dta.poem.4328": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein gl\u00e4nzendes Gefilde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein noch nicht lang' gepfl\u00fcgter Acker, woraus jedoch, zu", "tokens": ["Ein", "noch", "nicht", "lang'", "ge\u00b7pfl\u00fcg\u00b7ter", "A\u00b7cker", ",", "wo\u00b7raus", "je\u00b7doch", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "PTKNEG", "ADV", "ADJA", "NN", "$,", "PWAV", "ADV", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "meiner Freude,", "tokens": ["mei\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Von dem darinn ges\u00e4eten schon aufgelaufenen Getrayde", "tokens": ["Von", "dem", "da\u00b7rinn", "ge\u00b7s\u00e4e\u00b7ten", "schon", "auf\u00b7ge\u00b7lau\u00b7fe\u00b7nen", "Ge\u00b7tray\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PAV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+-+---+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die gr\u00fcnen Spitzen sich schon zeigten, war fr\u00fche ganz", "tokens": ["Die", "gr\u00fc\u00b7nen", "Spit\u00b7zen", "sich", "schon", "zeig\u00b7ten", ",", "war", "fr\u00fc\u00b7he", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$,", "VAFIN", "ADJA", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "von Frost bereift.", "tokens": ["von", "Frost", "be\u00b7reift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Reif war in der Mittags-Stunde geschmolzen, so da\u00df", "tokens": ["Der", "Reif", "war", "in", "der", "Mit\u00b7tags\u00b7Stun\u00b7de", "ge\u00b7schmol\u00b7zen", ",", "so", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "KOUS"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "an den Spitzen,", "tokens": ["an", "den", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wohin des Wassers Feuchtigkeit, so sich daselbst gemach", "tokens": ["Wo\u00b7hin", "des", "Was\u00b7sers", "Feuch\u00b7tig\u00b7keit", ",", "so", "sich", "da\u00b7selbst", "ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ADV", "PRF", "PAV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "geh\u00e4uft,", "tokens": ["ge\u00b7h\u00e4uft", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "In K\u00fcgelgen sich hingesenkt. Hiedurch entstand ein bun-", "tokens": ["In", "K\u00fc\u00b7gel\u00b7gen", "sich", "hin\u00b7ge\u00b7senkt", ".", "Hie\u00b7durch", "ent\u00b7stand", "ein", "bun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "VVPP", "$.", "PAV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "tes Blitzen", "tokens": ["tes", "Blit\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Vom Strahl der ganz entw\u00f6lkten Sonnen. Weil nun ein", "tokens": ["Vom", "Strahl", "der", "ganz", "ent\u00b7w\u00f6lk\u00b7ten", "Son\u00b7nen", ".", "Weil", "nun", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADV", "ADJA", "NN", "$.", "KOUS", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "jeder Tr\u00f6pfgen gr\u00f6sser,", "tokens": ["je\u00b7der", "Tr\u00f6pf\u00b7gen", "gr\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Als wie man sie im Thau sonst sieht; so war derselben", "tokens": ["Als", "wie", "man", "sie", "im", "Thau", "sonst", "sieht", ";", "so", "war", "der\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PIS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$.", "ADV", "VAFIN", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Glanz auch besser,", "tokens": ["Glanz", "auch", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Und flammte noch vortreflicher, so da\u00df ein Tr\u00f6pfgen nicht", "tokens": ["Und", "flamm\u00b7te", "noch", "vor\u00b7tre\u00b7fli\u00b7cher", ",", "so", "da\u00df", "ein", "Tr\u00f6pf\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "ADV", "KOUS", "ART", "NN", "PTKNEG"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "allein", "tokens": ["al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "F\u00fcr sich in bunten Flammen spielte; ein schnell aus ihm", "tokens": ["F\u00fcr", "sich", "in", "bun\u00b7ten", "Flam\u00b7men", "spiel\u00b7te", ";", "ein", "schnell", "aus", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$.", "ART", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "erzeugter Schein", "tokens": ["er\u00b7zeug\u00b7ter", "Schein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Umgab in einem Strahlen-Kreise, der oft sechs Zoll im", "tokens": ["Um\u00b7gab", "in", "ei\u00b7nem", "Strah\u00b7len\u00b7Krei\u00b7se", ",", "der", "oft", "sechs", "Zoll", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "CARD", "NN", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Durchschnitt gro\u00df,", "tokens": ["Durch\u00b7schnitt", "gro\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Den kleinen hellen Mittelpunct. Der Bl\u00e4ttergen Be-", "tokens": ["Den", "klei\u00b7nen", "hel\u00b7len", "Mit\u00b7tel\u00b7punct", ".", "Der", "Bl\u00e4t\u00b7ter\u00b7gen", "Be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "weglichkeit,", "tokens": ["weg\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Nebst einem von den regen L\u00fcften oft wiederholten sanften", "tokens": ["Nebst", "ei\u00b7nem", "von", "den", "re\u00b7gen", "L\u00fcf\u00b7ten", "oft", "wie\u00b7der\u00b7hol\u00b7ten", "sanf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.25": {"text": "Sto\u00df,", "tokens": ["Sto\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Erregt ein stets beweglichs Gl\u00e4nzen, ein unaufh\u00f6rlich", "tokens": ["Er\u00b7regt", "ein", "stets", "be\u00b7weg\u00b7lichs", "Gl\u00e4n\u00b7zen", ",", "ein", "un\u00b7auf\u00b7h\u00f6r\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$,", "ART", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "funkelnd Zittern,", "tokens": ["fun\u00b7kelnd", "Zit\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Nicht anders, als wenn, voller Gluht, bewegte Zitter-", "tokens": ["Nicht", "an\u00b7ders", ",", "als", "wenn", ",", "vol\u00b7ler", "Gluht", ",", "be\u00b7weg\u00b7te", "Zit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOKOM", "KOUS", "$,", "ADJA", "NN", "$,", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nadeln sch\u00fcttern.", "tokens": ["Na\u00b7deln", "sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Daher entstand fast \u00fcberall, durch einen wandelbaren", "tokens": ["Da\u00b7her", "ent\u00b7stand", "fast", "\u00fc\u00b7be\u00b7rall", ",", "durch", "ei\u00b7nen", "wan\u00b7del\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Schein,", "tokens": ["Schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und stets ver\u00e4nderliche Farben, ein Funkeln, welches", "tokens": ["Und", "stets", "ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7che", "Far\u00b7ben", ",", "ein", "Fun\u00b7keln", ",", "wel\u00b7ches"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "allgemein.", "tokens": ["all\u00b7ge\u00b7mein", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ein jeder Tropfen schien, belebt, sich gleichsam emsig zu", "tokens": ["Ein", "je\u00b7der", "Trop\u00b7fen", "schien", ",", "be\u00b7lebt", ",", "sich", "gleich\u00b7sam", "em\u00b7sig", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "VVPP", "$,", "PRF", "ADJD", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "bem\u00fch'n,", "tokens": ["be\u00b7m\u00fch'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Der fast erstaunten Schauer Augen recht in die Wett\u2019", "tokens": ["Der", "fast", "er\u00b7staun\u00b7ten", "Schau\u00b7er", "Au\u00b7gen", "recht", "in", "die", "Wett'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "auf sich zu zieh'n.", "tokens": ["auf", "sich", "zu", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PRF", "APPR", "NE", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Des dunklen Bodens brauner Grund erhub ihr Licht noch", "tokens": ["Des", "dunk\u00b7len", "Bo\u00b7dens", "brau\u00b7ner", "Grund", "er\u00b7hub", "ihr", "Licht", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "desto mehr,", "tokens": ["des\u00b7to", "mehr", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Und weil er nicht, wie sonst im Thau, bedeckt, nein, hin", "tokens": ["Und", "weil", "er", "nicht", ",", "wie", "sonst", "im", "Thau", ",", "be\u00b7deckt", ",", "nein", ",", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "$,", "PWAV", "ADV", "APPRART", "NN", "$,", "VVPP", "$,", "PTKANT", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und wieder leer;", "tokens": ["und", "wie\u00b7der", "leer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "So schienen recht, anstatt der Bluhmen, die sonst in sch\u00f6-", "tokens": ["So", "schie\u00b7nen", "recht", ",", "an\u00b7statt", "der", "Bluh\u00b7men", ",", "die", "sonst", "in", "sch\u00f6"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "KOUI", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "TRUNC"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "nen Farben gl\u00fch'n,", "tokens": ["nen", "Far\u00b7ben", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Auf hohen Stengeln ganze Heere von Diamanten hier", "tokens": ["Auf", "ho\u00b7hen", "Sten\u00b7geln", "gan\u00b7ze", "Hee\u00b7re", "von", "Di\u00b7a\u00b7man\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "zu bl\u00fch'n.", "tokens": ["zu", "bl\u00fch'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["APPR", "NE", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Das herrlich funkelnde Gefilde, das solch ein bunter", "tokens": ["Das", "herr\u00b7lich", "fun\u00b7keln\u00b7de", "Ge\u00b7fil\u00b7de", ",", "das", "solch", "ein", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$,", "PRELS", "PIAT", "ART", "NN"], "meter": "-+-+---+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Glanz bemahlte,", "tokens": ["Glanz", "be\u00b7mahl\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und mir, durch seine Feuer-Farben, voll Anmuht in die", "tokens": ["Und", "mir", ",", "durch", "sei\u00b7ne", "Feu\u00b7er\u00b7Fa\u00b7rben", ",", "voll", "An\u00b7muht", "in", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "NN", "APPR", "ART"], "meter": "-+-+-+--+-+-++", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Seele strahlte,", "tokens": ["See\u00b7le", "strahl\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Erregt\u2019 ein Andacht-Feur darinn, so da\u00df sie Dem, Der", "tokens": ["Er\u00b7regt'", "ein", "An\u00b7dacht\u00b7Feur", "da\u00b7rinn", ",", "so", "da\u00df", "sie", "Dem", ",", "Der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,", "ADV", "KOUS", "PPER", "PDS", "$,", "ART"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Farb' und Licht", "tokens": ["Fa\u00b7rb'", "und", "Licht"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Der Sonnen C\u00f6rper eingesenkt,", "tokens": ["Der", "Son\u00b7nen", "C\u00f6r\u00b7per", "ein\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und das betrachtende Gesicht,", "tokens": ["Und", "das", "be\u00b7trach\u00b7ten\u00b7de", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Um alle Herrlichkeit zu sehen, und zu geniessen, mir ge-", "tokens": ["Um", "al\u00b7le", "Herr\u00b7lich\u00b7keit", "zu", "se\u00b7hen", ",", "und", "zu", "ge\u00b7nies\u00b7sen", ",", "mir", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "schenkt,", "tokens": ["schenkt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Recht inniglich daf\u00fcr zu danken ein\u2019 angenehme Sucht", "tokens": ["Recht", "in\u00b7nig\u00b7lich", "da\u00b7f\u00fcr", "zu", "dan\u00b7ken", "ein'", "an\u00b7ge\u00b7neh\u00b7me", "Sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PAV", "PTKZU", "VVINF", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.28": {"text": "versp\u00fchrte.", "tokens": ["ver\u00b7sp\u00fchr\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Da denn, indem ich sie betrachte,", "tokens": ["Da", "denn", ",", "in\u00b7dem", "ich", "sie", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und ihr gef\u00e4rbter Glanz mich r\u00fchrte,", "tokens": ["Und", "ihr", "ge\u00b7f\u00e4rb\u00b7ter", "Glanz", "mich", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Der sie mit solchem Schimmer zierte,", "tokens": ["Der", "sie", "mit", "sol\u00b7chem", "Schim\u00b7mer", "zier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Er mich zu dem Erwegen brachte:", "tokens": ["Er", "mich", "zu", "dem", "Er\u00b7we\u00b7gen", "brach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}