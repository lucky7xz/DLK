{"textgrid.poem.52357": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ick heww en Fr\u00fcnd, en l\u00fctten, ollen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ick heww en Fr\u00fcnd, en l\u00fctten, ollen,", "tokens": ["Ick", "heww", "en", "Fr\u00fcnd", ",", "en", "l\u00fct\u00b7ten", ",", "ol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De h\u00f6llt in allen Dingen dicht,", "tokens": ["De", "h\u00f6llt", "in", "al\u00b7len", "Din\u00b7gen", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dat Dichten blot, dat kann hei nich recht hollen,", "tokens": ["Dat", "Dich\u00b7ten", "blot", ",", "dat", "kann", "hei", "nich", "recht", "hol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PAV", "VMFIN", "NE", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Un wenn hei't mit dat Dichten kriggt,", "tokens": ["Un", "wenn", "hei't", "mit", "dat", "Dich\u00b7ten", "kriggt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Denn brus't hei \u00e4wer, as wenn so'n", "tokens": ["Denn", "brus't", "hei", "\u00e4\u00b7wer", ",", "as", "wenn", "so'n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "$,", "NE", "KOUS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Teeketel \u00e4werkaken deiht,", "tokens": ["Tee\u00b7ke\u00b7tel", "\u00e4\u00b7wer\u00b7ka\u00b7ken", "deiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Bet allens unner Water steiht.", "tokens": ["Bet", "al\u00b7lens", "un\u00b7ner", "Wa\u00b7ter", "steiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Na, ick was mal mit desen ollen Sweden", "tokens": ["Na", ",", "ick", "was", "mal", "mit", "de\u00b7sen", "ol\u00b7len", "Swe\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "PIS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In eine grot Gesellschaft beden", "tokens": ["In", "ei\u00b7ne", "grot", "Ge\u00b7sell\u00b7schaft", "be\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bi minen Fr\u00fcnd, den riken Gaudsbesitzer,", "tokens": ["Bi", "mi\u00b7nen", "Fr\u00fcnd", ",", "den", "ri\u00b7ken", "Gauds\u00b7be\u00b7sit\u00b7zer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bekannt d\u00f6rch sine slichten Witze.", "tokens": ["Be\u00b7kannt", "d\u00f6rch", "si\u00b7ne", "slich\u00b7ten", "Wit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dat Gl\u00fcck, dat wull, dat ick mit unsen Dichter", "tokens": ["Dat", "Gl\u00fcck", ",", "dat", "wull", ",", "dat", "ick", "mit", "un\u00b7sen", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bi't Abendbrod tausamen sat.", "tokens": ["Bi't", "A\u00b7bend\u00b7brod", "tau\u00b7sa\u00b7men", "sat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un wohrt nich lang', dunn makt hei so'n Gesichter", "tokens": ["Un", "wohrt", "nich", "lang'", ",", "dunn", "makt", "hei", "so'n", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "As \u00bbPa\u00df mal up, nu k\u00fcmmt'e wat\u00ab;", "tokens": ["As", "\u00bb", "Pa\u00df", "mal", "up", ",", "nu", "k\u00fcmmt'e", "wat", "\u00ab", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "NN", "ADV", "NE", "$,", "FM.la", "FM.la", "FM.la", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Hei kek so stiw rup nah den B\u00e4hn", "tokens": ["Hei", "kek", "so", "stiw", "rup", "nah", "den", "B\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un brummte Rimels mang de T\u00e4hn", "tokens": ["Un", "brumm\u00b7te", "Ri\u00b7mels", "mang", "de", "T\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Un tippte mit de Fingern up den Disch,", "tokens": ["Un", "tipp\u00b7te", "mit", "de", "Fin\u00b7gern", "up", "den", "Disch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "NN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dr\u00fcnk ut Verseihn ut sines Nahwers Glas,", "tokens": ["Dr\u00fcnk", "ut", "Ver\u00b7seihn", "ut", "si\u00b7nes", "Nah\u00b7wers", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Kolt w\u00fcrd sin Brad un kolt sin Fisch;", "tokens": ["Kolt", "w\u00fcrd", "sin", "Brad", "un", "kolt", "sin", "Fisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Kort, ein kunn seihn, dat hei in't Dichten was.", "tokens": ["Kort", ",", "ein", "kunn", "seihn", ",", "dat", "hei", "in't", "Dich\u00b7ten", "was", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVINF", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mit einmal \u00e4wer sprung hei up \u2013", "tokens": ["Mit", "ein\u00b7mal", "\u00e4\u00b7wer", "sprung", "hei", "up", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "FM", "FM", "FM", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hei hett en slicht beh\u00f6llern Kopp", "tokens": ["Hei", "hett", "en", "slicht", "be\u00b7h\u00f6l\u00b7lern", "Kopp"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Un f\u00fcrcht't, dat hei behacken blew,", "tokens": ["Un", "f\u00fcrcht't", ",", "dat", "hei", "be\u00b7ha\u00b7cken", "blew", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Wenn hei sin Vers' nich nedder schrew \u2013", "tokens": ["Wenn", "hei", "sin", "Ver\u00b7s'", "nich", "ned\u00b7der", "schrew", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "PTKNEG", "ADV", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Un lep herute ut de D\u00f6r", "tokens": ["Un", "lep", "he\u00b7ru\u00b7te", "ut", "de", "D\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Un r\u00f6nnt dor buten hen un her", "tokens": ["Un", "r\u00f6nnt", "dor", "bu\u00b7ten", "hen", "un", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "APZR"], "meter": "+--+--++", "measure": "dactylic.di.plus"}, "line.21": {"text": "Un kakelt r\u00fcm un hadd't so hill", "tokens": ["Un", "ka\u00b7kelt", "r\u00fcm", "un", "hadd't", "so", "hill"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "FM", "FM", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Grad as en Haun, wenn't leggen will.", "tokens": ["Grad", "as", "en", "Haun", ",", "wenn't", "leg\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Taum Gl\u00fccken s\u00fcht de Stuwendirn", "tokens": ["Taum", "Gl\u00fc\u00b7cken", "s\u00fcht", "de", "Stu\u00b7wen\u00b7dirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Em dor her\u00fcmmer patrullier'n;", "tokens": ["Em", "dor", "he\u00b7r\u00fcm\u00b7mer", "pat\u00b7rul\u00b7lier'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sei fr\u00f6ggt, wat em gef\u00e4llig wir.", "tokens": ["Sei", "fr\u00f6ggt", ",", "wat", "em", "ge\u00b7f\u00e4l\u00b7lig", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "FM.la", "FM.la", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "\u00bbach Gott doch, Kind, sput' dich un mach,", "tokens": ["\u00bb", "ach", "Gott", "doch", ",", "Kind", ",", "sput'", "dich", "un", "mach", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "FM", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Zeig mir doch ein besonderes Gemach,", "tokens": ["Zeig", "mir", "doch", "ein", "be\u00b7son\u00b7de\u00b7res", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und bring ein Licht und auch Papier!\u00ab", "tokens": ["Und", "bring", "ein", "Licht", "und", "auch", "Pa\u00b7pier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Na, Guste, na, de krischt tau H\u00f6cht", "tokens": ["Na", ",", "Gus\u00b7te", ",", "na", ",", "de", "krischt", "tau", "H\u00f6cht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "ITJ", "$,", "NE", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Un deckt de Hand sick \u00e4w're Ogen", "tokens": ["Un", "deckt", "de", "Hand", "sick", "\u00e4w'\u00b7re", "O\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Un kickt versch\u00e4mt un lacht un seggt:", "tokens": ["Un", "kickt", "ver\u00b7sch\u00e4mt", "un", "lacht", "un", "seggt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "\u00bbna, t\u00e4uwen S' man, ick raup Sei Jochen.\u00ab", "tokens": ["\u00bb", "na", ",", "t\u00e4u\u00b7wen", "S'", "man", ",", "ick", "raup", "Sei", "Jo\u00b7chen", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPER", "PIS", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Un Jochen k\u00fcmmt un winkt den ollen Fr\u00fcnd:", "tokens": ["Un", "Jo\u00b7chen", "k\u00fcmmt", "un", "winkt", "den", "ol\u00b7len", "Fr\u00fcnd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "FM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u00bboh, kamen S' hir man mit entlang!\u00ab", "tokens": ["\u00bb", "oh", ",", "ka\u00b7men", "S'", "hir", "man", "mit", "ent\u00b7lang", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "VVFIN", "PPER", "ADV", "PIS", "APPR", "ADJD", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Un bringt em d\u00f6rch en langen Gang,", "tokens": ["Un", "bringt", "em", "d\u00f6rch", "en", "lan\u00b7gen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Un as sei den tau En'n s\u00fcnd,", "tokens": ["Un", "as", "sei", "den", "tau", "En'n", "s\u00fcnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Dunn sl\u00fctt hei up 'ne l\u00fctte D\u00f6r:", "tokens": ["Dunn", "sl\u00fctt", "hei", "up", "'ne", "l\u00fct\u00b7te", "D\u00f6r", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "\u00bbhir, Herr, hir setten S' sick man her!\u00ab", "tokens": ["\u00bb", "hir", ",", "Herr", ",", "hir", "set\u00b7ten", "S'", "sick", "man", "her", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PIS", "PTKVZ", "$.", "$("], "meter": "+--+----+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Un dr\u00fcckt em in de Hand Poppier un Licht;", "tokens": ["Un", "dr\u00fcckt", "em", "in", "de", "Hand", "Pop\u00b7pier", "un", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "NE", "NN", "NN", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Weg was dunn ", "tokens": ["Weg", "was", "dunn"], "token_info": ["word", "word", "word"], "pos": ["NN", "PWS", "ADJA"], "meter": "+--", "measure": "dactylic.init"}}}}}