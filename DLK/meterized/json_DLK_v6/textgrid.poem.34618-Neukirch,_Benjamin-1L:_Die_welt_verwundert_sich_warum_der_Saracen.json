{"textgrid.poem.34618": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die welt verwundert sich/ warum der Saracen", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die welt verwundert sich/ warum der Saracen", "tokens": ["Die", "welt", "ver\u00b7wun\u00b7dert", "sich", "/", "wa\u00b7rum", "der", "Sa\u00b7ra\u00b7cen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Franckreich b\u00fcndni\u00df sucht/ und Franckreich es beliebet:", "tokens": ["An", "Fran\u00b7ck\u00b7reich", "b\u00fcn\u00b7dni\u00df", "sucht", "/", "und", "Fran\u00b7ck\u00b7reich", "es", "be\u00b7lie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$(", "KON", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Noch mehr/ da\u00df Ludewig ihm selber lehren giebet/", "tokens": ["Noch", "mehr", "/", "da\u00df", "Lu\u00b7de\u00b7wig", "ihm", "sel\u00b7ber", "leh\u00b7ren", "gie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "NE", "PPER", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie er den Christen recht soll in die flancken gehn.", "tokens": ["Wie", "er", "den", "Chris\u00b7ten", "recht", "soll", "in", "die", "flan\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VMFIN", "APPR", "ART", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verwundert euch nur nicht/ und lebet ohne sorgen;", "tokens": ["Ver\u00b7wun\u00b7dert", "euch", "nur", "nicht", "/", "und", "le\u00b7bet", "oh\u00b7ne", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr wi\u00dft/ da\u00df Ludewig will eine sonne seyn/", "tokens": ["Ihr", "wi\u00dft", "/", "da\u00df", "Lu\u00b7de\u00b7wig", "will", "ei\u00b7ne", "son\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NE", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die T\u00fcrcken sind der mond; drum trifft es billig ein:", "tokens": ["Die", "T\u00fcr\u00b7cken", "sind", "der", "mond", ";", "drum", "trifft", "es", "bil\u00b7lig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "PAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein monde mu\u00df sein licht ja von der sonne borgen.", "tokens": ["Ein", "mon\u00b7de", "mu\u00df", "sein", "licht", "ja", "von", "der", "son\u00b7ne", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}