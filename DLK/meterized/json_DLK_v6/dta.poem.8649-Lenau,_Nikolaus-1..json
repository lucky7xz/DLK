{"dta.poem.8649": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht umschweigt mein Krankenlager,", "tokens": ["Nacht", "um\u00b7schweigt", "mein", "Kran\u00b7ken\u00b7la\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der morschen Diele nur", "tokens": ["An", "der", "mor\u00b7schen", "Die\u00b7le", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reget sich der kleine Nager,", "tokens": ["Re\u00b7get", "sich", "der", "klei\u00b7ne", "Na\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es pickt die Pendeluhr,", "tokens": ["Und", "es", "pickt", "die", "Pen\u00b7de\u00b7luhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die eint\u00f6nig mir bedeutet,", "tokens": ["Die", "ein\u00b7t\u00f6\u00b7nig", "mir", "be\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df das Leben weiter schreitet.", "tokens": ["Da\u00df", "das", "Le\u00b7ben", "wei\u00b7ter", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber tr\u00fcbe, heitre Stellen", "tokens": ["Ue\u00b7ber", "tr\u00fc\u00b7be", ",", "heit\u00b7re", "Stel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreitet's unaufhaltsam hin,", "tokens": ["Schrei\u00b7tet's", "un\u00b7auf\u00b7halt\u00b7sam", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie des Stromes rasche Wellen", "tokens": ["Wie", "des", "Stro\u00b7mes", "ra\u00b7sche", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blum' und Dorn vor\u00fcberzieh'n.", "tokens": ["Blum'", "und", "Dorn", "vor\u00b7\u00fcber\u00b7zieh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "VVFIN", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Immer senkt die Bahn sich j\u00e4her,", "tokens": ["Im\u00b7mer", "senkt", "die", "Bahn", "sich", "j\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt der Schritt dem Orkus n\u00e4her.", "tokens": ["Kommt", "der", "Schritt", "dem", "Or\u00b7kus", "n\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir auch senkt sie sich, und schaurig", "tokens": ["Mir", "auch", "senkt", "sie", "sich", ",", "und", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weht es aus der Niederung;", "tokens": ["Weht", "es", "aus", "der", "Nie\u00b7de\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und, noch J\u00fcngling, h\u00f6r' ich traurig,", "tokens": ["Und", ",", "noch", "J\u00fcng\u00b7ling", ",", "h\u00f6r'", "ich", "trau\u00b7rig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie aus banger D\u00e4mmerung", "tokens": ["Wie", "aus", "ban\u00b7ger", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Meines Herzens matten Schl\u00e4gen", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "mat\u00b7ten", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Cocytus rauscht entgegen.", "tokens": ["Der", "Co\u00b7cy\u00b7tus", "rauscht", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}