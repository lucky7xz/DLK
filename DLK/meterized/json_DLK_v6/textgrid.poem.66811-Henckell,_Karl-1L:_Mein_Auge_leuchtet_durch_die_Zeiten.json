{"textgrid.poem.66811": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Auge leuchtet durch die Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Auge leuchtet durch die Zeiten", "tokens": ["Mein", "Au\u00b7ge", "leuch\u00b7tet", "durch", "die", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Denkern, die das All gebar,", "tokens": ["Den", "Den\u00b7kern", ",", "die", "das", "All", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu hohen Zielen seh ich schreiten", "tokens": ["Zu", "ho\u00b7hen", "Zie\u00b7len", "seh", "ich", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Zug der Geistesheldenschar.", "tokens": ["Den", "Zug", "der", "Geis\u00b7te\u00b7shel\u00b7den\u00b7schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Losung h\u00f6r' ich vorw\u00e4rts klingen", "tokens": ["Die", "Lo\u00b7sung", "h\u00f6r'", "ich", "vor\u00b7w\u00e4rts", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom F\u00e4hnlein, das dem Blick entflieht,", "tokens": ["Vom", "F\u00e4hn\u00b7lein", ",", "das", "dem", "Blick", "ent\u00b7flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf \u00e4therzarten Sonnenschwingen", "tokens": ["Auf", "\u00e4t\u00b7herz\u00b7ar\u00b7ten", "Son\u00b7nen\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Banner, das vor\u00fcberzieht.", "tokens": ["Zum", "Ban\u00b7ner", ",", "das", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die heiligen Melodieen rauschen,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hochzeitsmarsch der Weltidee,", "tokens": ["Ein", "Hoch\u00b7zeits\u00b7marsch", "der", "Wel\u00b7ti\u00b7dee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Lichtposaunen mu\u00df ich lauschen,", "tokens": ["Den", "Licht\u00b7po\u00b7sau\u00b7nen", "mu\u00df", "ich", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich des Spieles Sinn versteh.", "tokens": ["Bis", "ich", "des", "Spie\u00b7les", "Sinn", "ver\u00b7steh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer ahnt, wie sich der Mensch enthoben", "tokens": ["Wer", "ahnt", ",", "wie", "sich", "der", "Mensch", "ent\u00b7ho\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Mutterscho\u00df der Gottnatur?", "tokens": ["Dem", "Mut\u00b7ter\u00b7scho\u00df", "der", "Gott\u00b7na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind aus Urweltglut gewoben", "tokens": ["Wir", "sind", "aus", "Ur\u00b7welt\u00b7glut", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sausen auf der Sonne Spur.", "tokens": ["Und", "sau\u00b7sen", "auf", "der", "Son\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus Dumpfheit schwang zu Dichterstirnen", "tokens": ["Aus", "Dum\u00b7pf\u00b7heit", "schwang", "zu", "Dich\u00b7ter\u00b7stir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Empor sich sch\u00f6pferische Glut,", "tokens": ["Em\u00b7por", "sich", "sch\u00f6p\u00b7fe\u00b7ri\u00b7sche", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00fchn ragen der Gedanken Firnen,", "tokens": ["K\u00fchn", "ra\u00b7gen", "der", "Ge\u00b7dan\u00b7ken", "Fir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Get\u00fcrmt wie von Titanenmut.", "tokens": ["Ge\u00b7t\u00fcrmt", "wie", "von", "Ti\u00b7ta\u00b7nen\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Schacht der Wahrheit zu erschlie\u00dfen,", "tokens": ["Den", "Schacht", "der", "Wahr\u00b7heit", "zu", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward edler Forscher Lust und Pflicht,", "tokens": ["Ward", "ed\u00b7ler", "For\u00b7scher", "Lust", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus der Erkenntnis Gr\u00fcnden sprie\u00dfen", "tokens": ["Aus", "der", "Er\u00b7kennt\u00b7nis", "Gr\u00fcn\u00b7den", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freiheit Segnungen zum Licht.", "tokens": ["Der", "Frei\u00b7heit", "Seg\u00b7nun\u00b7gen", "zum", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was tiefer Sehersinn ersonnen,", "tokens": ["Was", "tie\u00b7fer", "Se\u00b7her\u00b7sinn", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachwelt ruft den Traum zur Tat,", "tokens": ["Die", "Nach\u00b7welt", "ruft", "den", "Traum", "zur", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was der Genius gewonnen,", "tokens": ["Und", "was", "der", "Ge\u00b7nius", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weit wird es ausgestreut als Saat.", "tokens": ["Weit", "wird", "es", "aus\u00b7ge\u00b7streut", "als", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Niedre welkt. Voll bl\u00fcht zum Sch\u00f6nen,", "tokens": ["Das", "Nied\u00b7re", "welkt", ".", "Voll", "bl\u00fcht", "zum", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$.", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was h\u00e4\u00dflich und gemein noch ringt,", "tokens": ["Was", "h\u00e4\u00df\u00b7lich", "und", "ge\u00b7mein", "noch", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Dom der Zukunft h\u00f6r' ich t\u00f6nen", "tokens": ["Den", "Dom", "der", "Zu\u00b7kunft", "h\u00f6r'", "ich", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Psalmen, die die Menschheit singt.", "tokens": ["Von", "Psal\u00b7men", ",", "die", "die", "Menschheit", "singt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}