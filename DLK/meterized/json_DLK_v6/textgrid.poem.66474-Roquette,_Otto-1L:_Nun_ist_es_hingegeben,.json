{"textgrid.poem.66474": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ist es hingegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es hingegeben,", "tokens": ["Nun", "ist", "es", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verweint mit Weh und Ach,", "tokens": ["Ver\u00b7weint", "mit", "Weh", "und", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Gl\u00fcck, das f\u00fcr ein Leben", "tokens": ["Das", "Gl\u00fcck", ",", "das", "f\u00fcr", "ein", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu dauern dir versprach.", "tokens": ["Zu", "dau\u00b7ern", "dir", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun hast du \u00fcberwunden,", "tokens": ["Nun", "hast", "du", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und doch, du f\u00fchlest bang", "tokens": ["Und", "doch", ",", "du", "f\u00fch\u00b7lest", "bang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In einsam stillen Stunden", "tokens": ["In", "ein\u00b7sam", "stil\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des alten Kummers Hang.", "tokens": ["Des", "al\u00b7ten", "Kum\u00b7mers", "Hang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du siehst der Seel' entsteigen", "tokens": ["Du", "siehst", "der", "Seel'", "ent\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein theures Trauerbild,", "tokens": ["Ein", "theu\u00b7res", "Trau\u00b7er\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit vorwurfsvollem Schweigen,", "tokens": ["Mit", "vor\u00b7wurfs\u00b7vol\u00b7lem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Thr\u00e4nen ungestillt.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "un\u00b7ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du wirst ihm nicht entrinnen,", "tokens": ["Du", "wirst", "ihm", "nicht", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob l\u00e4ngst auch abgeschafft", "tokens": ["Ob", "l\u00e4ngst", "auch", "ab\u00b7ge\u00b7schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Schmerzensgluth der Sinnen,", "tokens": ["Die", "Schmer\u00b7zens\u00b7gluth", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Wahn und Leidenschaft.", "tokens": ["Und", "Wahn", "und", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo du dein ganzes Wesen", "tokens": ["Wo", "du", "dein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergabst mit Allgewalt,", "tokens": ["Ver\u00b7gabst", "mit", "All\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kannst du's zur\u00fcck nicht l\u00f6sen", "tokens": ["Kannst", "du's", "zu\u00b7r\u00fcck", "nicht", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PTKVZ", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In fr\u00fcherer Gestalt.", "tokens": ["In", "fr\u00fc\u00b7he\u00b7rer", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es nimmt aus jener Wonne", "tokens": ["Es", "nimmt", "aus", "je\u00b7ner", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit sich ein Schattenbild,", "tokens": ["Mit", "sich", "ein", "Schat\u00b7ten\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das keine Lebenssonne", "tokens": ["Das", "kei\u00b7ne", "Le\u00b7bens\u00b7son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit warmem Licht durchquillt.", "tokens": ["Mit", "war\u00b7mem", "Licht", "durch\u00b7quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}