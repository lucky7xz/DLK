{"dta.poem.13665": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Vielem will die Heimath mich erfreuen,", "tokens": ["Mit", "Vie\u00b7lem", "will", "die", "Hei\u00b7math", "mich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein heitres Schlo\u00df an blaugewund'nem Flusse,", "tokens": ["Ein", "heit\u00b7res", "Schlo\u00df", "an", "blau\u00b7ge\u00b7wun\u00b7d'\u00b7nem", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gesell'ge Lust, Muthwill und frohe Mu\u00dfe,", "tokens": ["Ge\u00b7sell'\u00b7ge", "Lust", ",", "Mut\u00b7hwill", "und", "fro\u00b7he", "Mu\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe heitres Spiel, s\u00fc\u00df zu zerstreuen.", "tokens": ["Der", "Lie\u00b7be", "heit\u00b7res", "Spiel", ",", "s\u00fc\u00df", "zu", "zer\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}