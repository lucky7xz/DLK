{"dta.poem.1626": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ogro\u00dfe N\u00e4hrerin ber\u00fchmter Helden/ ", "tokens": ["O\u00b7gro\u00b7\u00dfe", "N\u00e4h\u00b7re\u00b7rin", "be\u00b7r\u00fchm\u00b7ter", "Hel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh entz\u00fcckt und mit Verwundern an/", "tokens": ["Ich", "seh", "ent\u00b7z\u00fcckt", "und", "mit", "Ver\u00b7wun\u00b7dern", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "KON", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was man von deiner Pracht noch weisen kan/", "tokens": ["Was", "man", "von", "dei\u00b7ner", "Pracht", "noch", "wei\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und was uns mehr von dir die Schrifften melden:", "tokens": ["Und", "was", "uns", "mehr", "von", "dir", "die", "Schriff\u00b7ten", "mel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich ehre deinen Grau\u00df/ dein Grab/ den Todten-Schaten/", "tokens": ["Ich", "eh\u00b7re", "dei\u00b7nen", "Grau\u00df", "/", "dein", "Grab", "/", "den", "Tod\u00b7ten\u00b7Scha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch noch vielmehr als di\u00df den Ruhm der hohen Thaten.", "tokens": ["Doch", "noch", "viel\u00b7mehr", "als", "di\u00df", "den", "Ruhm", "der", "ho\u00b7hen", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOKOM", "PDS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Betracht ich aber wie dein Glantz erblichen/", "tokens": ["Be\u00b7tracht", "ich", "a\u00b7ber", "wie", "dein", "Glantz", "er\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie man die Tapfferkeit erloschen sieht/", "tokens": ["Wie", "man", "die", "Tapf\u00b7fer\u00b7keit", "er\u00b7lo\u00b7schen", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie deiner Kinder Ruhm mit dir verbl\u00fcht/", "tokens": ["Wie", "dei\u00b7ner", "Kin\u00b7der", "Ruhm", "mit", "dir", "ver\u00b7bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Ehr und alte Treu von dir entwichen/", "tokens": ["Wie", "Ehr", "und", "al\u00b7te", "Treu", "von", "dir", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADJA", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So sag ich thr\u00e4nende: Was will man Ewigs finden/", "tokens": ["So", "sag", "ich", "thr\u00e4\u00b7nen\u00b7de", ":", "Was", "will", "man", "E\u00b7wigs", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$.", "PWS", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn mit den St\u00e4dten auch die Tugenden verschwinden?", "tokens": ["Wenn", "mit", "den", "St\u00e4d\u00b7ten", "auch", "die", "Tu\u00b7gen\u00b7den", "ver\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}