{"textgrid.poem.57273": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die Strahlen vor der D\u00e4mrung nun entfliehn und der Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Strahlen vor der D\u00e4mrung nun entfliehn und der Abendstern", "tokens": ["Wenn", "die", "Strah\u00b7len", "vor", "der", "D\u00e4m\u00b7rung", "nun", "ent\u00b7fliehn", "und", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "ADV", "VVINF", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die sanfteren, entw\u00f6lkten, die erfrischenden Schimmer nun", "tokens": ["Die", "sanf\u00b7te\u00b7ren", ",", "ent\u00b7w\u00f6lk\u00b7ten", ",", "die", "er\u00b7fri\u00b7schen\u00b7den", "Schim\u00b7mer", "nun"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Nieder zu dem Haine der Barden senkt,", "tokens": ["Nie\u00b7der", "zu", "dem", "Hai\u00b7ne", "der", "Bar\u00b7den", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und melodisch in dem Hain die Quell' ihm ert\u00f6nt;", "tokens": ["Und", "me\u00b7lo\u00b7disch", "in", "dem", "Hain", "die", "Quell'", "ihm", "er\u00b7t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "So entsenket die Erscheinung des Thuiskon, wie Silber st\u00e4ubt", "tokens": ["So", "ent\u00b7sen\u00b7ket", "die", "Er\u00b7schei\u00b7nung", "des", "Thu\u00b7is\u00b7kon", ",", "wie", "Sil\u00b7ber", "st\u00e4ubt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PWAV", "NN", "VVFIN"], "meter": "------+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von fallendem Gew\u00e4sser, sich dem Himmel, und komt zu euch,", "tokens": ["Von", "fal\u00b7len\u00b7dem", "Ge\u00b7w\u00e4s\u00b7ser", ",", "sich", "dem", "Him\u00b7mel", ",", "und", "komt", "zu", "euch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRF", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dichter, und zur Quelle. Die Eiche weht", "tokens": ["Dich\u00b7ter", ",", "und", "zur", "Quel\u00b7le", ".", "Die", "Ei\u00b7che", "weht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPRART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ihm Gelispel. So erklang der Schwan Venusin,", "tokens": ["Ihm", "Ge\u00b7lis\u00b7pel", ".", "So", "er\u00b7klang", "der", "Schwan", "Ve\u00b7nu\u00b7sin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Da verwandelt er dahin flog. Und Thuiskon vernimts, und schwebt", "tokens": ["Da", "ver\u00b7wan\u00b7delt", "er", "da\u00b7hin", "flog", ".", "Und", "Thu\u00b7is\u00b7kon", "ver\u00b7nimts", ",", "und", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "VVFIN", "$.", "KON", "NE", "NE", "$,", "KON", "VVFIN"], "meter": "--+--+-+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In wehendem Ger\u00e4usche des begr\u00fcssenden Hains, und horcht;", "tokens": ["In", "we\u00b7hen\u00b7dem", "Ge\u00b7r\u00e4u\u00b7sche", "des", "be\u00b7gr\u00fcs\u00b7sen\u00b7den", "Hains", ",", "und", "horcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Aber nun empfangen, mit lauterm Gruss,", "tokens": ["A\u00b7ber", "nun", "emp\u00b7fan\u00b7gen", ",", "mit", "lau\u00b7term", "Gruss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit der Sait' ihn und Gesang, die Enkel um ihn.", "tokens": ["Mit", "der", "Sait'", "ihn", "und", "Ge\u00b7sang", ",", "die", "En\u00b7kel", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "KON", "NN", "$,", "ART", "NN", "APPR", "PPER", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Melodieen, wie der Telyn in Walhalla, ert\u00f6nen ihm", "tokens": ["Me\u00b7lo\u00b7di\u00b7e\u00b7en", ",", "wie", "der", "Te\u00b7lyn", "in", "Wal\u00b7hal\u00b7la", ",", "er\u00b7t\u00f6\u00b7nen", "ihm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPR", "NE", "$,", "VVFIN", "PPER"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Des wechselnden, des k\u00fchneren, deutscheren Odenflugs,", "tokens": ["Des", "wech\u00b7seln\u00b7den", ",", "des", "k\u00fch\u00b7ne\u00b7ren", ",", "deut\u00b7sche\u00b7ren", "O\u00b7den\u00b7flugs", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+--++-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Welcher, wie der Adler zur Wolk' itzt steigt,", "tokens": ["Wel\u00b7cher", ",", "wie", "der", "Ad\u00b7ler", "zur", "Wol\u00b7k'", "itzt", "steigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dann herunter zu der Eiche Wipfel sich senkt.", "tokens": ["Dann", "her\u00b7un\u00b7ter", "zu", "der", "Ei\u00b7che", "Wip\u00b7fel", "sich", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}