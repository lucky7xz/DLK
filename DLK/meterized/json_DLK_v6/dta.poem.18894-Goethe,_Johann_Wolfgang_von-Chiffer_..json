{"dta.poem.18894": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Chiffer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dir zu er\u00f6ffnen", "tokens": ["Dir", "zu", "er\u00b7\u00f6ff\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mein Herz verlangt mich;", "tokens": ["Mein", "Herz", "ver\u00b7langt", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "H\u00f6rt\u2019 ich von deinem,", "tokens": ["H\u00f6rt'", "ich", "von", "dei\u00b7nem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Darnach verlangt mich;", "tokens": ["Dar\u00b7nach", "ver\u00b7langt", "mich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie blickt so traurig", "tokens": ["Wie", "blickt", "so", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Welt mich an.", "tokens": ["Die", "Welt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In meinem Sinne", "tokens": ["In", "mei\u00b7nem", "Sin\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wohnet mein Freund nur,", "tokens": ["Woh\u00b7net", "mein", "Freund", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und sonsten keiner", "tokens": ["Und", "sons\u00b7ten", "kei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und keine Feindspur.", "tokens": ["Und", "kei\u00b7ne", "Feinds\u00b7pur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie Sonnenaufgang", "tokens": ["Wie", "Son\u00b7nen\u00b7auf\u00b7gang"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ward mir ein Vorsatz!", "tokens": ["Ward", "mir", "ein", "Vor\u00b7satz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein Leben will ich", "tokens": ["Mein", "Le\u00b7ben", "will", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nur zum Gesch\u00e4fte", "tokens": ["Nur", "zum", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von seiner Liebe", "tokens": ["Von", "sei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Von heut an machen.", "tokens": ["Von", "heut", "an", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ich denke seiner,", "tokens": ["Ich", "den\u00b7ke", "sei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mir blutet\u2019s Herz.", "tokens": ["Mir", "blu\u00b7tet's", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}