{"textgrid.poem.66604": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Im Kasernenarrest", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sonnenseliger Pfingstentag!", "tokens": ["O", "son\u00b7nen\u00b7se\u00b7li\u00b7ger", "Pfings\u00b7ten\u00b7tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun lacht und kost es in Flur und Hag.", "tokens": ["Nun", "lacht", "und", "kost", "es", "in", "Flur", "und", "Hag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verlassen ist Stube und B\u00fcrgerhaus,", "tokens": ["Ver\u00b7las\u00b7sen", "ist", "Stu\u00b7be", "und", "B\u00fcr\u00b7ger\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nun ziehen sie alle hinaus, hinaus.", "tokens": ["Nun", "zie\u00b7hen", "sie", "al\u00b7le", "hin\u00b7aus", ",", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wer sonst des Lebens nie sich freut,", "tokens": ["Wer", "sonst", "des", "Le\u00b7bens", "nie", "sich", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er pfl\u00fcckt die Rose der Freude heut.", "tokens": ["Er", "pfl\u00fcckt", "die", "Ro\u00b7se", "der", "Freu\u00b7de", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nur ich allein, nur ich allein", "tokens": ["Nur", "ich", "al\u00b7lein", ",", "nur", "ich", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "$,", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Darf nicht im fr\u00f6hlichen Bunde sein,", "tokens": ["Darf", "nicht", "im", "fr\u00f6h\u00b7li\u00b7chen", "Bun\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPRART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und der das schn\u00f6de Gebot gegeben,", "tokens": ["Und", "der", "das", "schn\u00f6\u00b7de", "Ge\u00b7bot", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Stiehlt mir einen sch\u00f6nen Tag im Leben,", "tokens": ["Stiehlt", "mir", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Tag", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und das verzeih' ihm Gott!", "tokens": ["Und", "das", "ver\u00b7zeih'", "ihm", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}