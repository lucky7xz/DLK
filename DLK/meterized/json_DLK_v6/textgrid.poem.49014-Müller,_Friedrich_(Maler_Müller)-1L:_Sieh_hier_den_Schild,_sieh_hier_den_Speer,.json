{"textgrid.poem.49014": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh hier den Schild, sieh hier den Speer,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh hier den Schild, sieh hier den Speer,", "tokens": ["Sieh", "hier", "den", "Schild", ",", "sieh", "hier", "den", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "VVIMP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Schwert, so er gef\u00fchret,", "tokens": ["Dies", "Schwert", ",", "so", "er", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und diesen Helm, den ich vorher", "tokens": ["Und", "die\u00b7sen", "Helm", ",", "den", "ich", "vor\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Palmen sch\u00f6n gezieret,", "tokens": ["Mit", "Pal\u00b7men", "sch\u00f6n", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von seinem Heldenblute rot.", "tokens": ["Von", "sei\u00b7nem", "Hel\u00b7den\u00b7blu\u00b7te", "rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er starb \u2013 la\u00df dir's erz\u00e4hlen,", "tokens": ["Er", "starb", "\u2013", "la\u00df", "dir's", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVIMP", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein letztes Wort war noch im Tod:", "tokens": ["Sein", "letz\u00b7tes", "Wort", "war", "noch", "im", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir sollten uns verm\u00e4hlen.", "tokens": ["Wir", "soll\u00b7ten", "uns", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo irr' ich um den Muschelstrand,", "tokens": ["Wo", "irr'", "ich", "um", "den", "Mu\u00b7schel\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo find' ich, Armer, sie?", "tokens": ["Wo", "find'", "ich", ",", "Ar\u00b7mer", ",", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach \u00fcber Strom und \u00fcber Land", "tokens": ["Ach", "\u00fc\u00b7ber", "Strom", "und", "\u00fc\u00b7ber", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich such' und suche sie.", "tokens": ["Ich", "such'", "und", "su\u00b7che", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein M\u00e4dchen, das von Treu' bewegt,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "von", "Treu'", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mich ihr lieben richt't,", "tokens": ["Auf", "mich", "ihr", "lie\u00b7ben", "richt't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Herzen s\u00fc\u00dfen Himmel tr\u00e4gt", "tokens": ["Im", "Her\u00b7zen", "s\u00fc\u00b7\u00dfen", "Him\u00b7mel", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ruh' im Angesicht,", "tokens": ["Und", "Ruh'", "im", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mir werter noch, als Kron' und Welt,", "tokens": ["Mir", "wer\u00b7ter", "noch", ",", "als", "Kron'", "und", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Liebe mich beschirmt,", "tokens": ["Mit", "Lie\u00b7be", "mich", "be\u00b7schirmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sanft mich in ihren Armen h\u00e4lt,", "tokens": ["Sanft", "mich", "in", "ih\u00b7ren", "Ar\u00b7men", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's hier im Busen st\u00fcrmt,", "tokens": ["Wenn's", "hier", "im", "Bu\u00b7sen", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, mit mir weinet, alles stillt,", "tokens": ["Ach", ",", "mit", "mir", "wei\u00b7net", ",", "al\u00b7les", "stillt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPER", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kummer und Verdru\u00df,", "tokens": ["Den", "Kum\u00b7mer", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies ganze, warme Herz erf\u00fcllt,", "tokens": ["Dies", "gan\u00b7ze", ",", "war\u00b7me", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das lieben will und mu\u00df?", "tokens": ["Das", "lie\u00b7ben", "will", "und", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O T\u00e4ubchen auf dem d\u00fcrren Ast,", "tokens": ["O", "T\u00e4ub\u00b7chen", "auf", "dem", "d\u00fcr\u00b7ren", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeig' doch das M\u00e4dchen mir!", "tokens": ["Zeig'", "doch", "das", "M\u00e4d\u00b7chen", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verberget ihr den lieben Gast,", "tokens": ["Ver\u00b7ber\u00b7get", "ihr", "den", "lie\u00b7ben", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr schlanken Ulmen hier?", "tokens": ["Ihr", "schlan\u00b7ken", "Ul\u00b7men", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Lang stand ich dort am Dornenhang", "tokens": ["Lang", "stand", "ich", "dort", "am", "Dor\u00b7nen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wo der Kibitz baut,", "tokens": ["Und", "wo", "der", "Ki\u00b7bitz", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lang irrt' ich durch den Klippengang", "tokens": ["Lang", "irrt'", "ich", "durch", "den", "Klip\u00b7pen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rief ihr \u00fcberlaut.", "tokens": ["Und", "rief", "ihr", "\u00fc\u00b7berl\u00b7aut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach nimmermehr! \u2013 Am Muschelstrand,", "tokens": ["Ach", "nim\u00b7mer\u00b7mehr", "!", "\u2013", "Am", "Mu\u00b7schel\u00b7strand", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "$(", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo find' ich, Armer, die?", "tokens": ["Wo", "find'", "ich", ",", "Ar\u00b7mer", ",", "die", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo \u00fcber Strom und \u00fcber Land?", "tokens": ["Wo", "\u00fc\u00b7ber", "Strom", "und", "\u00fc\u00b7ber", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich such' und suche sie.", "tokens": ["Ich", "such'", "und", "su\u00b7che", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}