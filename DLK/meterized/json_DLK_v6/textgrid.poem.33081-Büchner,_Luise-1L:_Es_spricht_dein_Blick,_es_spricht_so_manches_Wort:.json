{"textgrid.poem.33081": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es spricht dein Blick, es spricht so manches Wort:", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es spricht dein Blick, es spricht so manches Wort:", "tokens": ["Es", "spricht", "dein", "Blick", ",", "es", "spricht", "so", "man\u00b7ches", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich liebe dich! und dennoch treibt mich's fort.", "tokens": ["Ich", "lie\u00b7be", "dich", "!", "und", "den\u00b7noch", "treibt", "mich's", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du willst nicht l\u00fcgen, nein, doch was mich qu\u00e4lt,", "tokens": ["Du", "willst", "nicht", "l\u00fc\u00b7gen", ",", "nein", ",", "doch", "was", "mich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "$,", "PTKANT", "$,", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist, ob nicht selber dir die Klarheit fehlt?", "tokens": ["Ist", ",", "ob", "nicht", "sel\u00b7ber", "dir", "die", "Klar\u00b7heit", "fehlt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PTKNEG", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df, dein Herz ist gut und warm und reich,", "tokens": ["Ich", "wei\u00df", ",", "dein", "Herz", "ist", "gut", "und", "warm", "und", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Allein, bleibt sich dies Herz auch immer gleich?", "tokens": ["Al\u00b7lein", ",", "bleibt", "sich", "dies", "Herz", "auch", "im\u00b7mer", "gleich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PRF", "PDS", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gleicht's nicht der Wolke, die sich wechselnd malt,", "tokens": ["Gleicht's", "nicht", "der", "Wol\u00b7ke", ",", "die", "sich", "wech\u00b7selnd", "malt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Je nach dem Lichte, welches sie umstrahlt?", "tokens": ["Je", "nach", "dem", "Lich\u00b7te", ",", "wel\u00b7ches", "sie", "um\u00b7strahlt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie tr\u00e4gt nicht Schuld an diesem Wechselschein,", "tokens": ["Sie", "tr\u00e4gt", "nicht", "Schuld", "an", "die\u00b7sem", "Wech\u00b7sel\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In ihrem Wesen liegt es, so zu sein.", "tokens": ["In", "ih\u00b7rem", "We\u00b7sen", "liegt", "es", ",", "so", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum schelt' ich dich ja nicht, wenn du ihr gleich,", "tokens": ["Drum", "schelt'", "ich", "dich", "ja", "nicht", ",", "wenn", "du", "ihr", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Trifft mich dies Wort auch wie ein Todesstreich;", "tokens": ["Trifft", "mich", "dies", "Wort", "auch", "wie", "ein", "To\u00b7des\u00b7streich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Macht's mich auch selbst zur Wolke, d\u00fcstergrau,", "tokens": ["Macht's", "mich", "auch", "selbst", "zur", "Wol\u00b7ke", ",", "d\u00fcs\u00b7ter\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die auf sich l\u00f6st in bitt'rer Thr\u00e4nen Thau!", "tokens": ["Die", "auf", "sich", "l\u00f6st", "in", "bitt'\u00b7rer", "Thr\u00e4\u00b7nen", "Thau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}