{"textgrid.poem.46604": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach, da\u00df dich noch mit fester]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df dich noch mit fester", "tokens": ["Ach", ",", "da\u00df", "dich", "noch", "mit", "fes\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewalt das Leben zog,", "tokens": ["Ge\u00b7walt", "das", "Le\u00b7ben", "zog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als aufw\u00e4rts deine Schwester", "tokens": ["Als", "auf\u00b7w\u00e4rts", "dei\u00b7ne", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon vierzehn Tage flog.", "tokens": ["Schon", "vier\u00b7zehn", "Ta\u00b7ge", "flog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fcrchte, liebster Bester,", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "liebs\u00b7ter", "Bes\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du noch l\u00e4nger weilst,", "tokens": ["Wenn", "du", "noch", "l\u00e4n\u00b7ger", "weilst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du mir dort die Schwester", "tokens": ["Da\u00df", "du", "mir", "dort", "die", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Flug nicht mehr ereilst.", "tokens": ["Im", "Flug", "nicht", "mehr", "er\u00b7eilst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O warte, liebe Schwester,", "tokens": ["O", "war\u00b7te", ",", "lie\u00b7be", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf deinen Bruder nur!", "tokens": ["Auf", "dei\u00b7nen", "Bru\u00b7der", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und baut dort eure Nester", "tokens": ["Und", "baut", "dort", "eu\u00b7re", "Nes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie hier, auf Einer Flur!", "tokens": ["Wie", "hier", ",", "auf", "Ei\u00b7ner", "Flur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}