{"textgrid.poem.60235": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du auf Steinen und auf graugewordnen M\u00fcnzen", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du auf Steinen und auf graugewordnen M\u00fcnzen", "tokens": ["Der", "du", "auf", "Stei\u00b7nen", "und", "auf", "grau\u00b7ge\u00b7word\u00b7nen", "M\u00fcn\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Alterthums ber\u00fchmte K\u00f6pfe kennst,", "tokens": ["Des", "Al\u00b7ter\u00b7thums", "be\u00b7r\u00fchm\u00b7te", "K\u00f6p\u00b7fe", "kennst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unter allen verg\u00f6tterten Prinzen", "tokens": ["Und", "un\u00b7ter", "al\u00b7len", "ver\u00b7g\u00f6t\u00b7ter\u00b7ten", "Prin\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den j\u00fcngern C\u00e4sar g\u00f6ttlich nennst;", "tokens": ["Den", "j\u00fcn\u00b7gern", "C\u00e4\u00b7sar", "g\u00f6tt\u00b7lich", "nennst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ihn Horaz und Maro sangen,", "tokens": ["Weil", "ihn", "Ho\u00b7raz", "und", "Ma\u00b7ro", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "KON", "NE", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und beyde S\u00e4nger auf ihr goldnes Saytenspiel", "tokens": ["Und", "bey\u00b7de", "S\u00e4n\u00b7ger", "auf", "ihr", "gold\u00b7nes", "Say\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Sonnenblick von seiner Huld empfangen.", "tokens": ["Den", "Son\u00b7nen\u00b7blick", "von", "sei\u00b7ner", "Huld", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freund, solcher Prinzen giebts nicht viel", "tokens": ["Freund", ",", "sol\u00b7cher", "Prin\u00b7zen", "giebts", "nicht", "viel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "In alten und in neuern Zeiten.", "tokens": ["In", "al\u00b7ten", "und", "in", "neu\u00b7ern", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch l\u00e4\u00dft nicht C\u00e4sar Joseph itzt", "tokens": ["Doch", "l\u00e4\u00dft", "nicht", "C\u00e4\u00b7sar", "Jo\u00b7se\u00b7ph", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "NE", "NE", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die deutsche Musen, die sein Doppeladler sch\u00fctzt,", "tokens": ["Die", "deut\u00b7sche", "Mu\u00b7sen", ",", "die", "sein", "Dop\u00b7pe\u00b7lad\u00b7ler", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In hundert Wettges\u00e4ngen streiten?", "tokens": ["In", "hun\u00b7dert", "Wett\u00b7ge\u00b7s\u00e4n\u00b7gen", "strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erweckt das L\u00e4cheln seiner Gunst", "tokens": ["Er\u00b7weckt", "das", "L\u00e4\u00b7cheln", "sei\u00b7ner", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht edlen Ehrgeiz in dem Spieler auf der B\u00fchne?", "tokens": ["Nicht", "ed\u00b7len", "Ehr\u00b7geiz", "in", "dem", "Spie\u00b7ler", "auf", "der", "B\u00fch\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er strebet, da\u00df er sich durch Molierens Kunst", "tokens": ["Er", "stre\u00b7bet", ",", "da\u00df", "er", "sich", "durch", "Mo\u00b7lie\u00b7rens", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "NE", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Zwiefachen Lorbeer, so wie Molier, verdiene \u2013", "tokens": ["Zwie\u00b7fa\u00b7chen", "Lor\u00b7beer", ",", "so", "wie", "Mo\u00b7lier", ",", "ver\u00b7die\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "KOKOM", "NE", "$,", "VVFIN", "$("], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Und muntern nicht zu neuen Liedern mich", "tokens": ["Und", "mun\u00b7tern", "nicht", "zu", "neu\u00b7en", "Lie\u00b7dern", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Helden auf vom Gwelfenstamme?", "tokens": ["Die", "Hel\u00b7den", "auf", "vom", "Gwel\u00b7fen\u00b7stam\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie frisches Oel die zitternde Flamme", "tokens": ["Wie", "fri\u00b7sches", "O\u00b7el", "die", "zit\u00b7tern\u00b7de", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der sterbenden Ampel belebt;", "tokens": ["Der", "ster\u00b7ben\u00b7den", "Am\u00b7pel", "be\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Also beleben mein sinkendes Feuer", "tokens": ["Al\u00b7so", "be\u00b7le\u00b7ben", "mein", "sin\u00b7ken\u00b7des", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.16": {"text": "Die Prinzen durch ihren huldwinkenden Blick.", "tokens": ["Die", "Prin\u00b7zen", "durch", "ih\u00b7ren", "huld\u00b7win\u00b7ken\u00b7den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.17": {"text": "Auch Dessaus F\u00fcrstinn, \u2013 Welch ein Gl\u00fcck! \u2013", "tokens": ["Auch", "Des\u00b7saus", "F\u00fcrs\u00b7tinn", ",", "\u2013", "Welch", "ein", "Gl\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$,", "$(", "PIAT", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wirft meiner beneideten Leyer", "tokens": ["Wirft", "mei\u00b7ner", "be\u00b7nei\u00b7de\u00b7ten", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Die selbst der Pariser h\u00f6rt,", "tokens": ["Die", "selbst", "der", "Pa\u00b7ri\u00b7ser", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Oft Blumen zu vom f\u00fchlenden Busen", "tokens": ["Oft", "Blu\u00b7men", "zu", "vom", "f\u00fch\u00b7len\u00b7den", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Im Arme Leopolds, und lehrt", "tokens": ["Im", "Ar\u00b7me", "Leo\u00b7polds", ",", "und", "lehrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "NE", "$,", "KON", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Sanft ihren ersten Sohn \u2013 Kind, opfre fr\u00fch den Musen", "tokens": ["Sanft", "ih\u00b7ren", "ers\u00b7ten", "Sohn", "\u2013", "Kind", ",", "opf\u00b7re", "fr\u00fch", "den", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "NN", "$,", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und Grazien, und horche gern, wie wir,", "tokens": ["Und", "Gra\u00b7zi\u00b7en", ",", "und", "hor\u00b7che", "gern", ",", "wie", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die vaterl\u00e4ndischen Ges\u00e4nge.", "tokens": ["Die", "va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7schen", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Kein fremdes Lied, kein gallisch Spiel verdr\u00e4nge", "tokens": ["Kein", "frem\u00b7des", "Lied", ",", "kein", "gal\u00b7lisch", "Spiel", "ver\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Das Lied Teutoniens bey dir.", "tokens": ["Das", "Lied", "Teu\u00b7to\u00b7ni\u00b7ens", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}