{"textgrid.poem.36953": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl in jedem deutschen Lande", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl in jedem deutschen Lande", "tokens": ["Wohl", "in", "je\u00b7dem", "deut\u00b7schen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht noch eines S\u00e4ngers Haus,", "tokens": ["Steht", "noch", "ei\u00b7nes", "S\u00e4n\u00b7gers", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singend seines Volkes Schande", "tokens": ["Sin\u00b7gend", "sei\u00b7nes", "Vol\u00b7kes", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegen seine Lieder aus,", "tokens": ["Flie\u00b7gen", "sei\u00b7ne", "Lie\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es theilen alle Herzen", "tokens": ["Und", "es", "thei\u00b7len", "al\u00b7le", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sangbegeistert seine Schmerzen", "tokens": ["Sang\u00b7be\u00b7geis\u00b7tert", "sei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur; so viel in Dir auch leiern,", "tokens": ["Nur", ";", "so", "viel", "in", "Dir", "auch", "lei\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du bist \u00f6de, kalt und leer!", "tokens": ["Du", "bist", "\u00f6\u00b7de", ",", "kalt", "und", "leer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Armes Bayern, armes Bayern,", "tokens": ["Ar\u00b7mes", "Bay\u00b7ern", ",", "ar\u00b7mes", "Bay\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Du hast keinen Dichter mehr!", "tokens": ["Du", "hast", "kei\u00b7nen", "Dich\u00b7ter", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl schaut man von Deinen Bergen", "tokens": ["Wohl", "schaut", "man", "von", "Dei\u00b7nen", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In ein bl\u00fchend Gartenland;", "tokens": ["In", "ein", "bl\u00fc\u00b7hend", "Gar\u00b7ten\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch von Deines K\u00f6nigs Schergen", "tokens": ["Doch", "von", "Dei\u00b7nes", "K\u00f6\u00b7nigs", "Scher\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die S\u00e4nger draus verbannt!", "tokens": ["Sind", "die", "S\u00e4n\u00b7ger", "draus", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Prangst Du auch in gr\u00fcnem Kleide,", "tokens": ["Prangst", "Du", "auch", "in", "gr\u00fc\u00b7nem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steckst du doch in tiefem Leide,", "tokens": ["Steckst", "du", "doch", "in", "tie\u00b7fem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn nur von bezahlten Schreiern", "tokens": ["Denn", "nur", "von", "be\u00b7zahl\u00b7ten", "Schrei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "T\u00f6nt es widrig ringsumher!", "tokens": ["T\u00f6nt", "es", "wid\u00b7rig", "rings\u00b7um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Armes Bayern, armes Bayern,", "tokens": ["Ar\u00b7mes", "Bay\u00b7ern", ",", "ar\u00b7mes", "Bay\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Du hast keinen Dichter mehr!", "tokens": ["Du", "hast", "kei\u00b7nen", "Dich\u00b7ter", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus dem kalten Steine baust Du", "tokens": ["Aus", "dem", "kal\u00b7ten", "Stei\u00b7ne", "baust", "Du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dir kein warmes Lebenshaus;", "tokens": ["Dir", "kein", "war\u00b7mes", "Le\u00b7bens\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem todten Steine haust Du", "tokens": ["Aus", "dem", "tod\u00b7ten", "Stei\u00b7ne", "haust", "Du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gro\u00dfe Todte Dir nur aus;", "tokens": ["Gro\u00b7\u00dfe", "Tod\u00b7te", "Dir", "nur", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leichen prangen dort in Galla", "tokens": ["Lei\u00b7chen", "pran\u00b7gen", "dort", "in", "Gal\u00b7la"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Grabmal der Walhalla!", "tokens": ["In", "dem", "Grab\u00b7mal", "der", "Wal\u00b7hal\u00b7la", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Aller Jugend, allem Neuern", "tokens": ["Al\u00b7ler", "Ju\u00b7gend", ",", "al\u00b7lem", "Neu\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Droht dies stumme Geisterheer;", "tokens": ["Droht", "dies", "stum\u00b7me", "Geis\u00b7ter\u00b7heer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Armes Bayern, armes Bayern,", "tokens": ["Ar\u00b7mes", "Bay\u00b7ern", ",", "ar\u00b7mes", "Bay\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Du hast keinen Dichter mehr!", "tokens": ["Du", "hast", "kei\u00b7nen", "Dich\u00b7ter", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus dem Fett der M\u00f6nche lodert", "tokens": ["Aus", "dem", "Fett", "der", "M\u00f6n\u00b7che", "lo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die Flamme Deines Ruhms,", "tokens": ["Nicht", "die", "Flam\u00b7me", "Dei\u00b7nes", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Dein Christenthum vermodert", "tokens": ["Und", "Dein", "Chris\u00b7ten\u00b7thum", "ver\u00b7mo\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Sumpf des Pfaffenthums;", "tokens": ["In", "dem", "Sumpf", "des", "Pfaf\u00b7fen\u00b7thums", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Lichtesfeinde Gr\u00f6\u00dfter", "tokens": ["Al\u00b7ler", "Lich\u00b7tes\u00b7fein\u00b7de", "Gr\u00f6\u00df\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Baute die verfluchten Kl\u00f6ster,", "tokens": ["Bau\u00b7te", "die", "ver\u00b7fluch\u00b7ten", "Kl\u00f6s\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sich Deinem Fluge bleiern", "tokens": ["Da\u00df", "sich", "Dei\u00b7nem", "Flu\u00b7ge", "blei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Anh\u00e4ngt der Jesuiten-Heer!", "tokens": ["An\u00b7h\u00e4ngt", "der", "Je\u00b7su\u00b7i\u00b7ten\u00b7Heer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Armes Bayern, armes Bayern,", "tokens": ["Ar\u00b7mes", "Bay\u00b7ern", ",", "ar\u00b7mes", "Bay\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Du hast keinen Dichter mehr!", "tokens": ["Du", "hast", "kei\u00b7nen", "Dich\u00b7ter", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weh! in deinen Kammern dreschen", "tokens": ["Weh", "!", "in", "dei\u00b7nen", "Kam\u00b7mern", "dre\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau' ich Deine Besten Stroh,", "tokens": ["Schau'", "ich", "Dei\u00b7ne", "Bes\u00b7ten", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Hopfensaft erl\u00f6schen", "tokens": ["In", "dem", "Hop\u00b7fen\u00b7saft", "er\u00b7l\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes hei\u00dfe Ach und O,", "tokens": ["Je\u00b7des", "hei\u00b7\u00dfe", "Ach", "und", "O", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dumpfer werden Deine Geister,", "tokens": ["Dum\u00b7pfer", "wer\u00b7den", "Dei\u00b7ne", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Zwingherrn dreist und dreister!", "tokens": ["Dei\u00b7ne", "Zwing\u00b7herrn", "dreist", "und", "dreis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor dem Bild des allzutheuern", "tokens": ["Vor", "dem", "Bild", "des", "all\u00b7zu\u00b7theu\u00b7ern"], "token_info": ["word", "wo