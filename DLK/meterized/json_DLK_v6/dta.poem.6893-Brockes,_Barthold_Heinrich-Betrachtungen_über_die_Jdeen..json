{"dta.poem.6893": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In allen Handlungen der Sinnen", "tokens": ["In", "al\u00b7len", "Hand\u00b7lun\u00b7gen", "der", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Macht man den Unterscheid,", "tokens": ["Macht", "man", "den", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von einer dunckelen Empfindlichkeit,", "tokens": ["Von", "ei\u00b7ner", "dun\u00b7cke\u00b7len", "Emp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch welche man von ihnen wird ger\u00fchret,", "tokens": ["Durch", "wel\u00b7che", "man", "von", "ih\u00b7nen", "wird", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu einem deutlichen Begriff, den man von innen,", "tokens": ["Zu", "ei\u00b7nem", "deut\u00b7li\u00b7chen", "Be\u00b7griff", ",", "den", "man", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In unsrer Seele selbst versp\u00fcret.", "tokens": ["In", "uns\u00b7rer", "See\u00b7le", "selbst", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein jeder Vorwurff, den wir sehen,", "tokens": ["Ein", "je\u00b7der", "Vor\u00b7wurff", ",", "den", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erreget und erweckt in uns Jdeen.", "tokens": ["Er\u00b7re\u00b7get", "und", "er\u00b7weckt", "in", "uns", "Jdeen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Wofern man \u00fcberleget,", "tokens": ["Wo\u00b7fern", "man", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df es der Geist allein, der sie erreget,", "tokens": ["Da\u00df", "es", "der", "Geist", "al\u00b7lein", ",", "der", "sie", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So m\u00fcssen sie vom Leib wol unterschieden seyn.", "tokens": ["So", "m\u00fcs\u00b7sen", "sie", "vom", "Leib", "wol", "un\u00b7ter\u00b7schie\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man h\u00fcte sich,", "tokens": ["Man", "h\u00fc\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Da\u00df man statt seiner nicht erwehle", "tokens": ["Da\u00df", "man", "statt", "sei\u00b7ner", "nicht", "er\u00b7weh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die Striche, welche c\u00f6rperlich,", "tokens": ["Die", "Stri\u00b7che", ",", "wel\u00b7che", "c\u00f6r\u00b7per\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Aus welchen mit so vielen Bildern,", "tokens": ["Aus", "wel\u00b7chen", "mit", "so", "vie\u00b7len", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Vorw\u00fcrff\u2019 im Gehirn sich schildern.", "tokens": ["Die", "Vor\u00b7w\u00fcrff'", "im", "Ge\u00b7hirn", "sich", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}