{"textgrid.poem.44410": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Mitternacht, in Habsburgs alten Mauern,", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Mitternacht, in Habsburgs alten Mauern,", "tokens": ["Zu", "Mit\u00b7ter\u00b7nacht", ",", "in", "Habs\u00b7burgs", "al\u00b7ten", "Mau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht ein Verh\u00fcllter, r\u00e4tselhaft zu sehn,", "tokens": ["Geht", "ein", "Ver\u00b7h\u00fcll\u00b7ter", ",", "r\u00e4t\u00b7sel\u00b7haft", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sieht ihn schreiten, weilen nun und lauern,", "tokens": ["Man", "sieht", "ihn", "schrei\u00b7ten", ",", "wei\u00b7len", "nun", "und", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann heben seinen Fu\u00df und weitergehn.", "tokens": ["Dann", "he\u00b7ben", "sei\u00b7nen", "Fu\u00df", "und", "wei\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vom Haupte zu den tr\u00e4gen Fersen nieder,", "tokens": ["Vom", "Haup\u00b7te", "zu", "den", "tr\u00e4\u00b7gen", "Fer\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Umh\u00fcllend rings, flie\u00dft n\u00e4chtiges Gewand,", "tokens": ["Um\u00b7h\u00fcl\u00b7lend", "rings", ",", "flie\u00dft", "n\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Falten scharf, so zeichnen sich nicht Glieder,", "tokens": ["Die", "Fal\u00b7ten", "scharf", ",", "so", "zeich\u00b7nen", "sich", "nicht", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo Leben noch die straffen Formen spannt!", "tokens": ["Wo", "Le\u00b7ben", "noch", "die", "straf\u00b7fen", "For\u00b7men", "spannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was h\u00e4lt er? Ists ein Stab? Es blinkt wie Waffen! \u2013", "tokens": ["Was", "h\u00e4lt", "er", "?", "Ists", "ein", "Stab", "?", "Es", "blinkt", "wie", "Waf\u00b7fen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NE", "ART", "NN", "$.", "PPER", "VVFIN", "KOKOM", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schnitters Waffe haltend, zieht er ein,", "tokens": ["Des", "Schnit\u00b7ters", "Waf\u00b7fe", "hal\u00b7tend", ",", "zieht", "er", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo des Mantels S\u00e4um im Gehen klaffen,", "tokens": ["Und", "wo", "des", "Man\u00b7tels", "S\u00e4um", "im", "Ge\u00b7hen", "klaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Blickt kahl entgegen fleischentbl\u00f6\u00dft Gebein.", "tokens": ["Blickt", "kahl", "ent\u00b7ge\u00b7gen", "flei\u00b7schent\u00b7bl\u00f6\u00dft", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich kenne dich, du W\u00fcrger der Lebendgen!", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "du", "W\u00fcr\u00b7ger", "der", "Le\u00b7bend\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was suchst im Heiligtume, Scheusal, du?", "tokens": ["Was", "suchst", "im", "Hei\u00b7lig\u00b7tu\u00b7me", ",", "Scheu\u00b7sal", ",", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier darf das Alter nur die Tage endgen;", "tokens": ["Hier", "darf", "das", "Al\u00b7ter", "nur", "die", "Ta\u00b7ge", "end\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Pflicht zu leben gibt ein Recht dazu.", "tokens": ["Die", "Pflicht", "zu", "le\u00b7ben", "gibt", "ein", "Recht", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt steht er still dort, wo das Pf\u00f6rtchen schlie\u00dfet.", "tokens": ["Jetzt", "steht", "er", "still", "dort", ",", "wo", "das", "Pf\u00f6rt\u00b7chen", "schlie\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O schlie\u00dfe gut! O Pf\u00f6rtchen, schlie\u00df ihn aus!", "tokens": ["O", "schlie\u00b7\u00dfe", "gut", "!", "O", "Pf\u00f6rt\u00b7chen", ",", "schlie\u00df", "ihn", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$.", "NE", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch aus dem Kleide, das ihn rings umflie\u00dfet,", "tokens": ["Doch", "aus", "dem", "Klei\u00b7de", ",", "das", "ihn", "rings", "um\u00b7flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Streckt er die d\u00fcrre Knochenhand heraus.", "tokens": ["Streckt", "er", "die", "d\u00fcr\u00b7re", "Kno\u00b7chen\u00b7hand", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie an die Fl\u00fcgel er den Finger stellet,", "tokens": ["Wie", "an", "die", "Fl\u00fc\u00b7gel", "er", "den", "Fin\u00b7ger", "stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da springen sie weitg\u00e4hnend aus dem Schlo\u00df,", "tokens": ["Da", "sprin\u00b7gen", "sie", "weit\u00b7g\u00e4h\u00b7nend", "aus", "dem", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ein Gemach, von Lampenschein erhellet,", "tokens": ["Und", "ein", "Ge\u00b7mach", ",", "von", "Lam\u00b7pen\u00b7schein", "er\u00b7hel\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Liegt seinem Aug, liegt seinem Arme blo\u00df.", "tokens": ["Liegt", "sei\u00b7nem", "Aug", ",", "liegt", "sei\u00b7nem", "Ar\u00b7me", "blo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und drin ein Mann auf seinem Schmerzensbette.", "tokens": ["Und", "drin", "ein", "Mann", "auf", "sei\u00b7nem", "Schmer\u00b7zens\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ist die edle Stirn von Tropfen feucht!", "tokens": ["Wie", "ist", "die", "ed\u00b7le", "Stirn", "von", "Trop\u00b7fen", "feucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwei Frauen neben ihm. Wer sahs und h\u00e4tte", "tokens": ["Zwei", "Frau\u00b7en", "ne\u00b7ben", "ihm", ".", "Wer", "sahs", "und", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "PPER", "$.", "PWS", "VVFIN", "KON", "VAFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Gattin nicht erkannt, die Mutter leicht?", "tokens": ["Die", "Gat\u00b7tin", "nicht", "er\u00b7kannt", ",", "die", "Mut\u00b7ter", "leicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und eine Krone liegt zu Bettes F\u00fc\u00dfen.", "tokens": ["Und", "ei\u00b7ne", "Kro\u00b7ne", "liegt", "zu", "Bet\u00b7tes", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbdas ist ein K\u00f6nig\u00ab, spricht der bleiche Gast,", "tokens": ["\u00bb", "das", "ist", "ein", "K\u00f6\u00b7nig", "\u00ab", ",", "spricht", "der", "blei\u00b7che", "Gast", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbund zwar ein guter, soll ich glauben m\u00fcssen,", "tokens": ["\u00bb", "und", "zwar", "ein", "gu\u00b7ter", ",", "soll", "ich", "glau\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "ADJA", "$,", "VMFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das fr\u00fchergraute Haar zeugt nicht von Rast.", "tokens": ["Das", "fr\u00fc\u00b7her\u00b7grau\u00b7te", "Haar", "zeugt", "nicht", "von", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wohl auch als Gatte mocht er sich bew\u00e4hren,", "tokens": ["Wohl", "auch", "als", "Gat\u00b7te", "mocht", "er", "sich", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darum bewacht die Gattin jeden Hauch.", "tokens": ["Da\u00b7rum", "be\u00b7wacht", "die", "Gat\u00b7tin", "je\u00b7den", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchs Schlo\u00df erschallen Seufzer, flie\u00dfen Z\u00e4hren,", "tokens": ["Durchs", "Schlo\u00df", "er\u00b7schal\u00b7len", "Seuf\u00b7zer", ",", "flie\u00b7\u00dfen", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein guter Herr und Vater also auch!", "tokens": ["Ein", "gu\u00b7ter", "Herr", "und", "Va\u00b7ter", "al\u00b7so", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und dennoch kann das alles mich nicht hindern,", "tokens": ["Und", "den\u00b7noch", "kann", "das", "al\u00b7les", "mich", "nicht", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PDS", "PIS", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Gattin Tr\u00e4nen halten mich nicht auf;", "tokens": ["Der", "Gat\u00b7tin", "Tr\u00e4\u00b7nen", "hal\u00b7ten", "mich", "nicht", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Vater raub ich t\u00e4glich seinen Kindern,", "tokens": ["Den", "Va\u00b7ter", "raub", "ich", "t\u00e4g\u00b7lich", "sei\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was vorbestimmt ist, habe seinen Lauf!\u00ab", "tokens": ["Was", "vor\u00b7be\u00b7stimmt", "ist", ",", "ha\u00b7be", "sei\u00b7nen", "Lauf", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und er tritt ein; da summen leise Kl\u00e4nge", "tokens": ["Und", "er", "tritt", "ein", ";", "da", "sum\u00b7men", "lei\u00b7se", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Schlo\u00dfhof her in sein gespanntes Ohr.", "tokens": ["Vom", "Schlo\u00df\u00b7hof", "her", "in", "sein", "ge\u00b7spann\u00b7tes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort woget Volk, kaum fa\u00dft der Raum die Menge,", "tokens": ["Dort", "wo\u00b7get", "Volk", ",", "kaum", "fa\u00dft", "der", "Raum", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder forscht und jeder blickt empor.", "tokens": ["Und", "je\u00b7der", "forscht", "und", "je\u00b7der", "blickt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Weinender fr\u00e4gt einen, der da weinet,", "tokens": ["Ein", "Wei\u00b7nen\u00b7der", "fr\u00e4gt", "ei\u00b7nen", ",", "der", "da", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$,", "PRELS", "ADV", "VVFIN", "$,"]