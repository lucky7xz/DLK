{"textgrid.poem.62959": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von einem Hund geleitet, schlich", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einem Hund geleitet, schlich", "tokens": ["Von", "ei\u00b7nem", "Hund", "ge\u00b7lei\u00b7tet", ",", "schlich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein blinder Greis an seinem Stabe", "tokens": ["Ein", "blin\u00b7der", "Greis", "an", "sei\u00b7nem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch eine Stadt. Ein frecher Knabe,", "tokens": ["Durch", "ei\u00b7ne", "Stadt", ".", "Ein", "fre\u00b7cher", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Spitzbarts Israelchen glich,", "tokens": ["Der", "Spitz\u00b7barts", "Is\u00b7ra\u00b7el\u00b7chen", "glich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schnitt, um sich einen Spa\u00df zu machen", "tokens": ["Schnitt", ",", "um", "sich", "ei\u00b7nen", "Spa\u00df", "zu", "ma\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUI", "PRF", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Manns Compa\u00df, den Strick entzwey,", "tokens": ["Des", "Manns", "Com\u00b7pa\u00df", ",", "den", "Strick", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Flieh, sprach er, Philax, du bist frey;", "tokens": ["Flieh", ",", "sprach", "er", ",", "Phi\u00b7lax", ",", "du", "bist", "frey", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "NE", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Graukopf mag sich selbst bewachen.", "tokens": ["Dein", "Grau\u00b7kopf", "mag", "sich", "selbst", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Pommer fuhr dem kleinen Wicht", "tokens": ["Der", "Pom\u00b7mer", "fuhr", "dem", "klei\u00b7nen", "Wicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Voll edeln Grimmes an die Waden,", "tokens": ["Voll", "e\u00b7deln", "Grim\u00b7mes", "an", "die", "Wa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und sagte: nein, ich fliehe nicht,", "tokens": ["Und", "sag\u00b7te", ":", "nein", ",", "ich", "flie\u00b7he", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Du willst mir wohlthun, um zu schaden.", "tokens": ["Du", "willst", "mir", "wohl\u00b7thun", ",", "um", "zu", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}