{"textgrid.poem.61019": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist wie die dunkle Nacht,", "tokens": ["Mein", "Herz", "ist", "wie", "die", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle Wipfel rauschen;", "tokens": ["Wenn", "al\u00b7le", "Wip\u00b7fel", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da steigt der Mond in voller Pracht", "tokens": ["Da", "steigt", "der", "Mond", "in", "vol\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Wolken sacht -", "tokens": ["Aus", "Wol\u00b7ken", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und sieh, der Wald verstummt in tiefem Lauschen.", "tokens": ["Und", "sieh", ",", "der", "Wald", "ver\u00b7stummt", "in", "tie\u00b7fem", "Lau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mond, der helle Mond bist du:", "tokens": ["Der", "Mond", ",", "der", "hel\u00b7le", "Mond", "bist", "du", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deiner Liebesf\u00fclle", "tokens": ["Aus", "dei\u00b7ner", "Lie\u00b7bes\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirf einen, einen Blick mir zu", "tokens": ["Wirf", "ei\u00b7nen", ",", "ei\u00b7nen", "Blick", "mir", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "$,", "ART", "NN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Himmelsruh -", "tokens": ["Voll", "Him\u00b7mels\u00b7ruh"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und sieh, dies ungest\u00fcme Herz wird stille.", "tokens": ["Und", "sieh", ",", "dies", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Herz", "wird", "stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PDS", "ADJA", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}