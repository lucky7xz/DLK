{"textgrid.poem.34925": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mythologie", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Europa ist erlegen \u2013", "tokens": ["Ja", ",", "Eu\u00b7ro\u00b7pa", "ist", "er\u00b7le\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer kann Ochsen widerstehen?", "tokens": ["Wer", "kann", "Och\u00b7sen", "wi\u00b7der\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir verzeihen auch Dan\u00e4en \u2013", "tokens": ["Wir", "ver\u00b7zei\u00b7hen", "auch", "Da\u00b7n\u00e4\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie erlag dem goldnen Regen!", "tokens": ["Sie", "er\u00b7lag", "dem", "gold\u00b7nen", "Re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Semele lie\u00df sich verf\u00fchren \u2013", "tokens": ["Se\u00b7me\u00b7le", "lie\u00df", "sich", "ver\u00b7f\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Denn sie dachte: eine Wolke,", "tokens": ["Denn", "sie", "dach\u00b7te", ":", "ei\u00b7ne", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ideale Himmelswolke,", "tokens": ["I\u00b7dea\u00b7le", "Him\u00b7mels\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Kann uns nicht kompromittieren.", "tokens": ["Kann", "uns", "nicht", "kom\u00b7pro\u00b7mit\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber tief mu\u00df uns emp\u00f6ren", "tokens": ["A\u00b7ber", "tief", "mu\u00df", "uns", "em\u00b7p\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wir von der Leda lesen \u2013", "tokens": ["Was", "wir", "von", "der", "Le\u00b7da", "le\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Welche Gans bist du gewesen,", "tokens": ["Wel\u00b7che", "Gans", "bist", "du", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ein Schwan dich konnt bet\u00f6ren!", "tokens": ["Da\u00df", "ein", "Schwan", "dich", "konnt", "be\u00b7t\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}