{"textgrid.poem.51073": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch \u2013 endlich zittert es durch meine Bretter!", "tokens": ["Horch", "\u2013", "end\u00b7lich", "zit\u00b7tert", "es", "durch", "mei\u00b7ne", "Bret\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was f\u00fcr ein zauberhaft metallner Klang,", "tokens": ["Was", "f\u00fcr", "ein", "zau\u00b7ber\u00b7haft", "me\u00b7tall\u00b7ner", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ist das f\u00fcr ein unterirdisch Wetter,", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "un\u00b7ter\u00b7ir\u00b7disch", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mir ersch\u00fctternd in die Ohren drang?", "tokens": ["Das", "mir", "er\u00b7sch\u00fct\u00b7ternd", "in", "die", "Oh\u00b7ren", "drang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jach unterbrach es meine bangen Klagen,", "tokens": ["Jach", "un\u00b7ter\u00b7brach", "es", "mei\u00b7ne", "ban\u00b7gen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich lauschte z\u00e4hlend, still, fast hoffnungsvoll:", "tokens": ["Ich", "lauschte", "z\u00e4h\u00b7lend", ",", "still", ",", "fast", "hoff\u00b7nungs\u00b7voll", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Eilf \u2013 zw\u00f6lf \u2013 wahrhaftig, es hat zw\u00f6lf geschlagen,", "tokens": ["Eilf", "\u2013", "zw\u00f6lf", "\u2013", "wahr\u00b7haf\u00b7tig", ",", "es", "hat", "zw\u00f6lf", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "CARD", "$(", "ADJD", "$,", "PPER", "VAFIN", "CARD", "VVPP", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Das war die Turmuhr, die so dr\u00f6hnend scholl!", "tokens": ["Das", "war", "die", "Tur\u00b7muhr", ",", "die", "so", "dr\u00f6h\u00b7nend", "scholl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist die gro\u00dfe Glock', das Kind der L\u00fcfte,", "tokens": ["Es", "ist", "die", "gro\u00b7\u00dfe", "Glock'", ",", "das", "Kind", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das klingt ins tiefste Fundament herab,", "tokens": ["Das", "klingt", "ins", "tiefs\u00b7te", "Fun\u00b7da\u00b7ment", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bahnt sich den Weg durch Mauern und durch Gr\u00fcfte", "tokens": ["Bahnt", "sich", "den", "Weg", "durch", "Mau\u00b7ern", "und", "durch", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und singt sein Lied in mein verla\u00dfnes Grab.", "tokens": ["Und", "singt", "sein", "Lied", "in", "mein", "ver\u00b7la\u00df\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gewi\u00df sind jetzt die D\u00e4cher warm beschienen", "tokens": ["Ge\u00b7wi\u00df", "sind", "jetzt", "die", "D\u00e4\u00b7cher", "warm", "be\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom sonnigen Lenz, vom lichten \u00c4therblau;", "tokens": ["Vom", "son\u00b7ni\u00b7gen", "Lenz", ",", "vom", "lich\u00b7ten", "\u00c4\u00b7ther\u00b7blau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun kr\u00e4uselt sich der Rauch aus den Kaminen,", "tokens": ["Nun", "kr\u00e4u\u00b7selt", "sich", "der", "Rauch", "aus", "den", "Ka\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Leute lockend von der gr\u00fcnen Au.", "tokens": ["Die", "Leu\u00b7te", "lo\u00b7ckend", "von", "der", "gr\u00fc\u00b7nen", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was h\u00f6hnst du mich, du Glockenlied, im Grabe,", "tokens": ["Was", "h\u00f6hnst", "du", "mich", ",", "du", "Glo\u00b7cken\u00b7lied", ",", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "PPER", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Rufer in des Herrgotts Speisesaal,", "tokens": ["Du", "Ru\u00b7fer", "in", "des", "Herr\u00b7gotts", "Spei\u00b7se\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mahnst ungebeten, da\u00df ich Hunger habe", "tokens": ["Mahnst", "un\u00b7ge\u00b7be\u00b7ten", ",", "da\u00df", "ich", "Hun\u00b7ger", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "$,", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht kann hin zum \u00e4rmlich stillen Mahl? \u2013", "tokens": ["Und", "nicht", "kann", "hin", "zum", "\u00e4rm\u00b7lich", "stil\u00b7len", "Mahl", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "ADV", "APPRART", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}