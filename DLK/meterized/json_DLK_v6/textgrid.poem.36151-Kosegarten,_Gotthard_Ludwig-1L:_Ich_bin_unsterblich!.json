{"textgrid.poem.36151": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin unsterblich!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin unsterblich!", "tokens": ["Ich", "bin", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchl' es, meine selige Seele!", "tokens": ["F\u00fchl'", "es", ",", "mei\u00b7ne", "se\u00b7li\u00b7ge", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die du durstest den hei\u00dfen Durst nach Ewigkeit, f\u00fchl' es ganz:", "tokens": ["Die", "du", "durs\u00b7test", "den", "hei\u00b7\u00dfen", "Durst", "nach", "E\u00b7wig\u00b7keit", ",", "f\u00fchl'", "es", "ganz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Du bist unsterblich!", "tokens": ["Du", "bist", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was du gesehnet, gedurstet du hast", "tokens": ["Was", "du", "ge\u00b7seh\u00b7net", ",", "ge\u00b7durs\u00b7tet", "du", "hast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "VAFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem Durste des J\u00fcnglings nach dem ersten Ku\u00df der Erw\u00e4hlten \u2013", "tokens": ["Mit", "dem", "Durs\u00b7te", "des", "J\u00fcng\u00b7lings", "nach", "dem", "ers\u00b7ten", "Ku\u00df", "der", "Er\u00b7w\u00e4hl\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Siehe, das ist nun kommen,", "tokens": ["Sie\u00b7he", ",", "das", "ist", "nun", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PDS", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Anschau'n worden die Ahndung.", "tokens": ["Und", "An\u00b7schau'n", "wor\u00b7den", "die", "Ahn\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAPP", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was du erflehet, erbetet du hast,", "tokens": ["Was", "du", "er\u00b7fle\u00b7het", ",", "er\u00b7be\u00b7tet", "du", "hast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kommt \u00fcber dich heut', wie \u00fcber den J\u00fcngling die Wonne,", "tokens": ["Kommt", "\u00fc\u00b7ber", "dich", "heut'", ",", "wie", "\u00fc\u00b7ber", "den", "J\u00fcng\u00b7ling", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die errungne, erst\u00fcrmte Wonn', ein Strahl vom Himmel, kommt,", "tokens": ["Die", "er\u00b7rung\u00b7ne", ",", "er\u00b7st\u00fcrm\u00b7te", "Wonn'", ",", "ein", "Strahl", "vom", "Him\u00b7mel", ",", "kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df die ihn liebe, die seine Seele sich erkor.", "tokens": ["Da\u00df", "die", "ihn", "lie\u00b7be", ",", "die", "sei\u00b7ne", "See\u00b7le", "sich", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "PRF", "NE", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie den Wiedergebornen der Gnade Gef\u00fchl", "tokens": ["Wie", "den", "Wie\u00b7der\u00b7ge\u00b7bor\u00b7nen", "der", "Gna\u00b7de", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Ueberdr\u00e4ngt mit Seligkeitschauern,", "tokens": ["Ue\u00b7ber\u00b7dr\u00e4ngt", "mit", "Se\u00b7lig\u00b7keit\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So \u00fcberdr\u00e4ngt mich mit Schauern Gottes,", "tokens": ["So", "\u00fc\u00b7berd\u00b7r\u00e4ngt", "mich", "mit", "Schau\u00b7ern", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unsterblichkeit, dein gro\u00dfes Gef\u00fchl.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", ",", "dein", "gro\u00b7\u00dfes", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Ich ahndet' 's, ich hofft' es \u2013 nun wei\u00df ich, da\u00df ich bin!", "tokens": ["Ich", "ahn\u00b7det'", "'", "s", ",", "ich", "hofft", "'", "es", "\u2013", "nun", "wei\u00df", "ich", ",", "da\u00df", "ich", "bin", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "$,", "PPER", "VVFIN", "$(", "PPER", "$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich wei\u00df und f\u00fchl' es, da\u00df ich ewig bin! \u2013", "tokens": ["Ich", "wei\u00df", "und", "f\u00fchl'", "es", ",", "da\u00df", "ich", "e\u00b7wig", "bin", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Neige deine Wipfel, Eiche!", "tokens": ["Nei\u00b7ge", "dei\u00b7ne", "Wip\u00b7fel", ",", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Unsterblicher wandelt unter dir!", "tokens": ["Ein", "U\u00b7nsterb\u00b7li\u00b7cher", "wan\u00b7delt", "un\u00b7ter", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "R\u00fcnde die silberne Scheibe, Mond.", "tokens": ["R\u00fcn\u00b7de", "die", "sil\u00b7ber\u00b7ne", "Schei\u00b7be", ",", "Mond", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Entblinket dem Nachtged\u00fcft, schimmer\u00e4ugige Sterne!", "tokens": ["Ent\u00b7blin\u00b7ket", "dem", "Nacht\u00b7ge\u00b7d\u00fcft", ",", "schim\u00b7mer\u00b7\u00e4u\u00b7gi\u00b7ge", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sirius, w\u00e4lze dein Flammenend'. Glanzgeg\u00fcrteter Orion,", "tokens": ["Si\u00b7rius", ",", "w\u00e4l\u00b7ze", "dein", "Flam\u00b7me\u00b7nend'", ".", "Glanz\u00b7ge\u00b7g\u00fcr\u00b7te\u00b7ter", "O\u00b7rion", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$.", "NN", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wandle staatlich den Riesengang.", "tokens": ["Wand\u00b7le", "staat\u00b7lich", "den", "Rie\u00b7sen\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Wonne! Stolz! Entz\u00fccken!", "tokens": ["Won\u00b7ne", "!", "Stolz", "!", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich bin unsterblich!", "tokens": ["Ich", "bin", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mehr als Eich' und Mond, mehr als Orion und Sirius", "tokens": ["Mehr", "als", "Eich'", "und", "Mond", ",", "mehr", "als", "O\u00b7rion", "und", "Si\u00b7rius"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "NN", "KON", "NN", "$,", "ADV", "KOUS", "NN", "KON", "NE"], "meter": "+-+-+-+++--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Bin ich \u2013 bin unsterblich!", "tokens": ["Bin", "ich", "\u2013", "bin", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "VAFIN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Himmel und Erde vergeh'n.", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ich vergehe nicht \u2013 \u2013", "tokens": ["Ich", "ver\u00b7ge\u00b7he", "nicht", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach! wenn ich verginge \u2013", "tokens": ["Ach", "!", "wenn", "ich", "ver\u00b7gin\u00b7ge", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Quell der ewigen Leben! wer w\u00e4r' ich dann?", "tokens": ["Quell", "der", "e\u00b7wi\u00b7gen", "Le\u00b7ben", "!", "wer", "w\u00e4r'", "ich", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Staub, Traum, Nichts,", "tokens": ["Staub", ",", "Traum", ",", "Nichts", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PIS", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Gestern gerufen aus dem Nichts,", "tokens": ["Ge\u00b7stern", "ge\u00b7ru\u00b7fen", "aus", "dem", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Morgen wieder hingeschleudert in das todte Nichts \u2013", "tokens": ["Mor\u00b7gen", "wie\u00b7der", "hin\u00b7ge\u00b7schleu\u00b7dert", "in", "das", "tod\u00b7te", "Nichts", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Der w\u00e4r' ich!", "tokens": ["Der", "w\u00e4r'", "ich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Ach! und weniger noch, als Gras und Staub!", "tokens": ["Ach", "!", "und", "we\u00b7ni\u00b7ger", "noch", ",", "als", "Gras", "und", "Staub", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADV", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Elender noch, als der Kiesel der Gasse.", "tokens": ["E\u00b7len\u00b7der", "noch", ",", "als", "der", "Kie\u00b7sel", "der", "Gas\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Denn wer nicht gef\u00fchlt hat des Daseyns Entz\u00fcckungen,", "tokens": ["Denn", "wer", "nicht", "ge\u00b7f\u00fchlt", "hat", "des", "Da\u00b7seyns", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kann das Grauen des Nichtmehrseyns nicht f\u00fchlen.", "tokens": ["Kann", "das", "Grau\u00b7en", "des", "Nicht\u00b7mehr\u00b7seyns", "nicht", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Ach! wenn ich unsterblich nicht w\u00e4re,", "tokens": ["Ach", "!", "wenn", "ich", "uns\u00b7terb\u00b7lich", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "PTKNEG", "VAFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So heult' ich dem kommenden Tag'", "tokens": ["So", "heult'", "ich", "dem", "kom\u00b7men\u00b7den", "Tag'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Entgegen, so heult' ich, k\u00e4me die Nacht,", "tokens": ["Ent\u00b7ge\u00b7gen", ",", "so", "heult'", "ich", ",", "k\u00e4\u00b7me", "die", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und verh\u00fcllte mich, und schwiege verzweifelnd,", "tokens": ["Und", "ver\u00b7h\u00fcll\u00b7te", "mich", ",", "und", "schwie\u00b7ge", "ver\u00b7zwei\u00b7felnd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "So w\u00fcrd' ich unter die Blumen des Fr\u00fchlings", "tokens": ["So", "w\u00fcrd'", "ich", "un\u00b7ter", "die", "Blu\u00b7men", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich legen und mich winden; und die Blume beneiden,", "tokens": ["Mich", "le\u00b7gen", "und", "mich", "win\u00b7den", ";", "und", "die", "Blu\u00b7me", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "$.", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So w\u00fcrd' mir Verwesung duften der Bl\u00fcthenbaum,", "tokens": ["So", "w\u00fcrd'", "mir", "Ver\u00b7we\u00b7sung", "duf\u00b7ten", "der", "Bl\u00fc\u00b7then\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Todeston mir heulen die Nachtigall.", "tokens": ["To\u00b7des\u00b7ton", "mir", "heu\u00b7len", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "So w\u00fcrd' ich diese herrliche Kraft,", "tokens": ["So", "w\u00fcrd'", "ich", "die\u00b7se", "herr\u00b7li\u00b7che", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die du gegeben, Ewiger, mir hast,", "tokens": ["Die", "du", "ge\u00b7ge\u00b7ben", ",", "E\u00b7wi\u00b7ger", ",", "mir", "hast", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADJA", "$,", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ersticken in Rausch, Wollust, Taumel,", "tokens": ["Er\u00b7sti\u00b7cken", "in", "Rausch", ",", "Wol\u00b7lust", ",", "Tau\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da\u00df mich nicht tr\u00e4fe der Donnergedanke: Vernichtung.", "tokens": ["Da\u00df", "mich", "nicht", "tr\u00e4\u00b7fe", "der", "Don\u00b7ner\u00b7ge\u00b7dan\u00b7ke", ":", "Ver\u00b7nich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$.", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.14": {"line.1": {"text": "Aber er tr\u00e4fe mich doch!", "tokens": ["A\u00b7ber", "er", "tr\u00e4\u00b7fe", "mich", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Packete mit Tiegerkralle", "tokens": ["Pa\u00b7cke\u00b7te", "mit", "Tie\u00b7ger\u00b7kral\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Freude Schallgel\u00e4chter an der Kehle mich, br\u00fcllte mir zu:", "tokens": ["In", "der", "Freu\u00b7de", "Schall\u00b7ge\u00b7l\u00e4ch\u00b7ter", "an", "der", "Keh\u00b7le", "mich", ",", "br\u00fcll\u00b7te", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ART", "NN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Elender, bald wirst du nicht mehr seyn.", "tokens": ["E\u00b7len\u00b7der", ",", "bald", "wirst", "du", "nicht", "mehr", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Und der Kelch s\u00e4nke mir schnell aus der zitternden Hand.", "tokens": ["Und", "der", "Kelch", "s\u00e4n\u00b7ke", "mir", "schnell", "aus", "der", "zit\u00b7tern\u00b7den", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich d\u00e4uchte Schierlingssaft sein goldner Wein!", "tokens": ["Mich", "d\u00e4uch\u00b7te", "Schier\u00b7lings\u00b7saft", "sein", "gold\u00b7ner", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Grabesmoder die Speisen!", "tokens": ["Gra\u00b7bes\u00b7mo\u00b7der", "die", "Spei\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Grabgeheul die Musik!", "tokens": ["Grab\u00b7ge\u00b7heul", "die", "Mu\u00b7sik", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Hassen w\u00fcrd' ich, wenn ich unsterblich nicht w\u00e4re,", "tokens": ["Has\u00b7sen", "w\u00fcrd'", "ich", ",", "wenn", "ich", "uns\u00b7terb\u00b7lich", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VAFIN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Euch alle, die itzt ich liebe!", "tokens": ["Euch", "al\u00b7le", ",", "die", "itzt", "ich", "lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df, k\u00e4me der Tag, der vernichtende, gr\u00e4\u00dfliche Tag,", "tokens": ["Da\u00df", ",", "k\u00e4\u00b7me", "der", "Tag", ",", "der", "ver\u00b7nich\u00b7ten\u00b7de", ",", "gr\u00e4\u00df\u00b7li\u00b7che", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Ich nicht heulen d\u00fcrfte Verzweiflung, da\u00df, die ich liebte, ewig ich verlie\u00df'.", "tokens": ["Ich", "nicht", "heu\u00b7len", "d\u00fcrf\u00b7te", "Ver\u00b7zwei\u00b7flung", ",", "da\u00df", ",", "die", "ich", "lieb\u00b7te", ",", "e\u00b7wig", "ich", "ver\u00b7lie\u00df'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "VMFIN", "NN", "$,", "KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.17": {"line.1": {"text": "Und k\u00e4me nun der vernichtende Tag,", "tokens": ["Und", "k\u00e4\u00b7me", "nun", "der", "ver\u00b7nich\u00b7ten\u00b7de", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "St\u00fcnden um den Verzweiflung knirschenden die bleichen Freunde \u2013 \u2013", "tokens": ["St\u00fcn\u00b7den", "um", "den", "Ver\u00b7zwei\u00b7flung", "knir\u00b7schen\u00b7den", "die", "blei\u00b7chen", "Freun\u00b7de", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$(", "$("], "meter": "+-++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Halte, halte, mein Geist! Denke den H\u00f6llengedanken nicht aus! \u2013", "tokens": ["Hal\u00b7te", ",", "hal\u00b7te", ",", "mein", "Geist", "!", "Den\u00b7ke", "den", "H\u00f6l\u00b7len\u00b7ge\u00b7dan\u00b7ken", "nicht", "aus", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$.", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "+-+--++--+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Du bist unsterblich!", "tokens": ["Du", "bist", "uns\u00b7terb\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Du bist's! du bist's! und ich f\u00fchle, da\u00df du es bist,", "tokens": ["Du", "bist's", "!", "du", "bist's", "!", "und", "ich", "f\u00fch\u00b7le", ",", "da\u00df", "du", "es", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "PPER", "NE", "$.", "KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als rief' es mir der Himmel, als zeugt' es mir die Erde!", "tokens": ["Als", "rie\u00b7f'", "es", "mir", "der", "Him\u00b7mel", ",", "als", "zeugt'", "es", "mir", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und rufen sie es nicht? und zeugen sie es nicht?", "tokens": ["Und", "ru\u00b7fen", "sie", "es", "nicht", "?", "und", "zeu\u00b7gen", "sie", "es", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schwor der Ewiglebende es nicht bei seinem Leben?", "tokens": ["Und", "schwor", "der", "E\u00b7wig\u00b7le\u00b7ben\u00b7de", "es", "nicht", "bei", "sei\u00b7nem", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.19": {"line.1": {"text": "Leises Gef\u00fchl", "tokens": ["Lei\u00b7ses", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Lispelt es der Seele des Edlern.", "tokens": ["Lis\u00b7pelt", "es", "der", "See\u00b7le", "des", "Ed\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Dem Denker strahlt es die Fackel: Vernunft,", "tokens": ["Dem", "Den\u00b7ker", "strahlt", "es", "die", "Fa\u00b7ckel", ":", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Gl\u00e4ubigen die Sonn': Offenbarung.", "tokens": ["Dem", "Gl\u00e4u\u00b7bi\u00b7gen", "die", "Sonn'", ":", "Of\u00b7fen\u00b7ba\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.20": {"line.1": {"text": "Ist hienieden auch Tod?", "tokens": ["Ist", "hien\u00b7ie\u00b7den", "auch", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auch Untergang hienieden, und Nichtmehrseyn?", "tokens": ["Auch", "Un\u00b7ter\u00b7gang", "hien\u00b7ie\u00b7den", ",", "und", "Nicht\u00b7mehr\u00b7seyn", "?"], "toke