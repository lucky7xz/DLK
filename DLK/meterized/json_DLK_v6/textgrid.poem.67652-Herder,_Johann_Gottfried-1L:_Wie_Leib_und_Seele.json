{"textgrid.poem.67652": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Leib und Seele", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Leib und Seele", "tokens": ["Wie", "Leib", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sich so verschieden f\u00fcgt!", "tokens": ["Sich", "so", "ver\u00b7schie\u00b7den", "f\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die eine liegt", "tokens": ["Die", "ei\u00b7ne", "liegt"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "In Moderh\u00f6hle,", "tokens": ["In", "Mo\u00b7der\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die andre wie ein Engel fliegt.", "tokens": ["Die", "and\u00b7re", "wie", "ein", "En\u00b7gel", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}