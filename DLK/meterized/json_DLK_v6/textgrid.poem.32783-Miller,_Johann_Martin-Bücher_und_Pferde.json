{"textgrid.poem.32783": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "B\u00fccher und Pferde", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pfarrer Blarr hat B\u00fccher mancher Art", "tokens": ["Der", "Pfar\u00b7rer", "Blarr", "hat", "B\u00fc\u00b7cher", "man\u00b7cher", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und, sie zu kaufen, nie sein m\u00fc\u00dfig Geld gespart;", "tokens": ["Und", ",", "sie", "zu", "kau\u00b7fen", ",", "nie", "sein", "m\u00fc\u00b7\u00dfig", "Geld", "ge\u00b7spart", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "PTKZU", "VVINF", "$,", "ADV", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch, sagt man, geht es ihm, wie seinem Edelmann,", "tokens": ["Doch", ",", "sagt", "man", ",", "geht", "es", "ihm", ",", "wie", "sei\u00b7nem", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "$,", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der viele Pferde hat, und sie nicht reiten kann.", "tokens": ["Der", "vie\u00b7le", "Pfer\u00b7de", "hat", ",", "und", "sie", "nicht", "rei\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "KON", "PPER", "PTKNEG", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}