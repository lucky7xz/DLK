{"textgrid.poem.60553": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Gewitter", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Seele klarer Himmel h\u00e4ngt", "tokens": ["Der", "See\u00b7le", "kla\u00b7rer", "Him\u00b7mel", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll z\u00fcrnender Gedanken,", "tokens": ["Voll", "z\u00fcr\u00b7nen\u00b7der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mancher laute Seufzer dr\u00e4ngt", "tokens": ["Und", "man\u00b7cher", "lau\u00b7te", "Seuf\u00b7zer", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich aus der Brust, der kranken.", "tokens": ["Sich", "aus", "der", "Brust", ",", "der", "kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Unheimlich blitzt des Auges Stern", "tokens": ["Un\u00b7heim\u00b7lich", "blitzt", "des", "Au\u00b7ges", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sucht umsonst nach Zielen.", "tokens": ["Und", "sucht", "um\u00b7sonst", "nach", "Zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df ab! ich wei\u00df, du m\u00f6chtest gern", "tokens": ["La\u00df", "ab", "!", "ich", "wei\u00df", ",", "du", "m\u00f6ch\u00b7test", "gern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich an der Tr\u00e4ne k\u00fchlen.", "tokens": ["Dich", "an", "der", "Tr\u00e4\u00b7ne", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie blinkt nicht mehr im Auge hohl,", "tokens": ["Sie", "blinkt", "nicht", "mehr", "im", "Au\u00b7ge", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dem die Gluten zehren! \u2013", "tokens": ["An", "dem", "die", "Glu\u00b7ten", "zeh\u00b7ren", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch jetzt, wie wirds der Brust so wohl!", "tokens": ["Doch", "jetzt", ",", "wie", "wirds", "der", "Brust", "so", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darf sie vom Schmerz sich leeren?", "tokens": ["Darf", "sie", "vom", "Schmerz", "sich", "lee\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Auge gl\u00fcht das Abendrot,", "tokens": ["Im", "Au\u00b7ge", "gl\u00fcht", "das", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolken sind zerrissen.", "tokens": ["Die", "Wol\u00b7ken", "sind", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verspricht dies Leben oder Tod?", "tokens": ["Ver\u00b7spricht", "dies", "Le\u00b7ben", "o\u00b7der", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die bange Brust m\u00f6chts wissen.", "tokens": ["Die", "ban\u00b7ge", "Brust", "m\u00f6chts", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verkl\u00e4rten sich die Wolken nur,", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "sich", "die", "Wol\u00b7ken", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tod mir anzusagen?", "tokens": ["Den", "Tod", "mir", "an\u00b7zu\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! oder ists die erste Spur", "tokens": ["Ach", "!", "o\u00b7der", "ists", "die", "ers\u00b7te", "Spur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von sch\u00f6nern Erdentagen?", "tokens": ["Von", "sch\u00f6\u00b7nern", "Er\u00b7den\u00b7ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von neuem Gl\u00fcck! Die Glut erlischt,", "tokens": ["Von", "neu\u00b7em", "Gl\u00fcck", "!", "Die", "Glut", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie soll nichts mehr verheeren;", "tokens": ["Sie", "soll", "nichts", "mehr", "ver\u00b7hee\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn in die junge Freude mischt", "tokens": ["Denn", "in", "die", "jun\u00b7ge", "Freu\u00b7de", "mischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mild der Tau der Z\u00e4hren!", "tokens": ["Sich", "mild", "der", "Tau", "der", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}