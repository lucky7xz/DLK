{"textgrid.poem.41823": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1. Bl\u00fctezeit", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bl\u00fcht das Korn, nun bl\u00fcht der Wein,", "tokens": ["Nun", "bl\u00fcht", "das", "Korn", ",", "nun", "bl\u00fcht", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All um ein lieblich D\u00fcften,", "tokens": ["All", "um", "ein", "lieb\u00b7lich", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man atmet lauter Segen ein", "tokens": ["Man", "at\u00b7met", "lau\u00b7ter", "Se\u00b7gen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In linden Abendl\u00fcften.", "tokens": ["In", "lin\u00b7den", "A\u00b7ben\u00b7dl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch bl\u00fcht das Korn, noch reift es nicht,", "tokens": ["Noch", "bl\u00fcht", "das", "Korn", ",", "noch", "reift", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will selbst sich duftend weihen,", "tokens": ["Will", "selbst", "sich", "duf\u00b7tend", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durchgl\u00fcht vom warmen Sonnenlicht", "tokens": ["Durch\u00b7gl\u00fcht", "vom", "war\u00b7men", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Segen prophezeien.", "tokens": ["Den", "Se\u00b7gen", "pro\u00b7phe\u00b7zei\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir bitten all um t\u00e4glich Brot \u2013", "tokens": ["Wir", "bit\u00b7ten", "all", "um", "t\u00e4g\u00b7lich", "Brot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch doppelt ist's gesegnet,", "tokens": ["Doch", "dop\u00b7pelt", "ist's", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn uns im Juni-Abendrot", "tokens": ["Wenn", "uns", "im", "Ju\u00b7ni\u00b7A\u00b7bend\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Aehren Duft begegnet", "tokens": ["Der", "A\u00b7eh\u00b7ren", "Duft", "be\u00b7geg\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Wir trinken ihn mit Wonne ein:", "tokens": ["Wir", "trin\u00b7ken", "ihn", "mit", "Won\u00b7ne", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bl\u00fch'nden Aehren K\u00fcssen", "tokens": ["Der", "bl\u00fch'n\u00b7den", "A\u00b7eh\u00b7ren", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Soll unserm Leben heilsam sein \u2013", "tokens": ["Soll", "un\u00b7serm", "Le\u00b7ben", "heil\u00b7sam", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So will im Volk man wissen.", "tokens": ["So", "will", "im", "Volk", "man", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Volkesglaube rein und gut!", "tokens": ["O", "Vol\u00b7kes\u00b7glau\u00b7be", "rein", "und", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur der ist reich zu nennen,", "tokens": ["Nur", "der", "ist", "reich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dem Bl\u00fcten geben Hoffnungsmut,", "tokens": ["Dem", "Bl\u00fc\u00b7ten", "ge\u00b7ben", "Hoff\u00b7nungs\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' noch die Frucht zu kennen.", "tokens": ["Eh'", "noch", "die", "Frucht", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch an der ", "tokens": ["Auch", "an", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Hervor die Tr\u00e4ubchen sprie\u00dfen", "tokens": ["Her\u00b7vor", "die", "Tr\u00e4ub\u00b7chen", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und golden sich im Sonnenlicht", "tokens": ["Und", "gol\u00b7den", "sich", "im", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00fcten schon erschlie\u00dfen.", "tokens": ["Die", "Bl\u00fc\u00b7ten", "schon", "er\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft du Aehrenfeld,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "du", "A\u00b7eh\u00b7ren\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "PPER", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mit deinen leisen Wogen,", "tokens": ["Mit", "dei\u00b7nen", "lei\u00b7sen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Samt deiner blauen Blumenwelt,", "tokens": ["Samt", "dei\u00b7ner", "blau\u00b7en", "Blu\u00b7men\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich hinein verflogen,", "tokens": ["Die", "sich", "hin\u00b7ein", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APZR", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Duft, berauschend s\u00fc\u00dfer Art", "tokens": ["Ein", "Duft", ",", "be\u00b7rau\u00b7schend", "s\u00fc\u00b7\u00dfer", "Art"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchzieht die Rebeng\u00e4nge,", "tokens": ["Durch\u00b7zieht", "die", "Re\u00b7ben\u00b7g\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Sommers nahe Gegenwart", "tokens": ["Des", "Som\u00b7mers", "na\u00b7he", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcgt sich zum Lenzgepr\u00e4nge.", "tokens": ["F\u00fcgt", "sich", "zum", "Lenz\u00b7ge\u00b7pr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O sch\u00f6ne Zeit! es bl\u00fcht der Wein", "tokens": ["O", "sch\u00f6\u00b7ne", "Zeit", "!", "es", "bl\u00fcht", "der", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Sang der Nachtigallen,", "tokens": ["Beim", "Sang", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn im gold'nen Sonnenschein", "tokens": ["Und", "wenn", "im", "gold'\u00b7nen", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lerchenlieder schallen.", "tokens": ["Die", "Ler\u00b7chen\u00b7lie\u00b7der", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wenn sp\u00e4ter im Pokale", "tokens": ["Wenn", "sp\u00e4\u00b7ter", "im", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wein erfreut der Menschen Brust,", "tokens": ["Der", "Wein", "er\u00b7freut", "der", "Men\u00b7schen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Belebt mit einem Male.", "tokens": ["Be\u00b7lebt", "mit", "ei\u00b7nem", "Ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und gr\u00fc\u00dft dann die Erinnerung", "tokens": ["Und", "gr\u00fc\u00dft", "dann", "die", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Zeiten, da er bl\u00fchte,", "tokens": ["An", "Zei\u00b7ten", ",", "da", "er", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So schafft sie die Begeisterung,", "tokens": ["So", "schafft", "sie", "die", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nur f\u00fcr H\u00f6chstes gl\u00fchte.", "tokens": ["Die", "nur", "f\u00fcr", "H\u00f6chs\u00b7tes", "gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So mag des rechten Lebens Born", "tokens": ["So", "mag", "des", "rech\u00b7ten", "Le\u00b7bens", "Born"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn in uns \u00fcbergehen:", "tokens": ["Denn", "in", "uns", "\u00fc\u00b7ber\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}