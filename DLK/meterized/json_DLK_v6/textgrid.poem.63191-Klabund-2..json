{"textgrid.poem.63191": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott der Kindheit, darf man dir noch glauben?", "tokens": ["Gott", "der", "Kind\u00b7heit", ",", "darf", "man", "dir", "noch", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VMFIN", "PIS", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach ich kenne dich nicht mehr.", "tokens": ["Ach", "ich", "ken\u00b7ne", "dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sind deiner Herrschaft milde Tauben", "tokens": ["Wo", "sind", "dei\u00b7ner", "Herr\u00b7schaft", "mil\u00b7de", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Weines goldgegorne Trauben", "tokens": ["Und", "des", "Wei\u00b7nes", "gold\u00b7ge\u00b7gor\u00b7ne", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und des Fr\u00fchlings frohe Wiederkehr?", "tokens": ["Und", "des", "Fr\u00fch\u00b7lings", "fro\u00b7he", "Wie\u00b7der\u00b7kehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Falten trage ich und rauhe Runzeln,", "tokens": ["Fal\u00b7ten", "tra\u00b7ge", "ich", "und", "rau\u00b7he", "Run\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mein Sch\u00e4del ist mit Moos gestopft.", "tokens": ["Und", "mein", "Sch\u00e4\u00b7del", "ist", "mit", "Moos", "ge\u00b7stopft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bei der Kerze abendrotem Funzeln", "tokens": ["Bei", "der", "Ker\u00b7ze", "a\u00b7ben\u00b7dro\u00b7tem", "Fun\u00b7zeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denk ich l\u00e4chelnd an mein Beet Rapunzeln,", "tokens": ["Denk", "ich", "l\u00e4\u00b7chelnd", "an", "mein", "Beet", "Ra\u00b7pun\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00dcber dem der Juniregen tropft.", "tokens": ["\u00dc\u00b7ber", "dem", "der", "Ju\u00b7ni\u00b7re\u00b7gen", "tropft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}