{"textgrid.poem.25091": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach des Fr\u00fchlings bl\u00fchendem Gl\u00fcck", "tokens": ["Nach", "des", "Fr\u00fch\u00b7lings", "bl\u00fc\u00b7hen\u00b7dem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und des Herbstes strotzenden Garben", "tokens": ["Und", "des", "Herbs\u00b7tes", "strot\u00b7zen\u00b7den", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun Felder, kalt vom November bereift!", "tokens": ["Nun", "Fel\u00b7der", ",", "kalt", "vom", "No\u00b7vem\u00b7ber", "be\u00b7reift", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch Nebel und st\u00e4ubende Flocken schweift", "tokens": ["Durch", "Ne\u00b7bel", "und", "st\u00e4u\u00b7ben\u00b7de", "Flo\u00b7cken", "schweift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mein Blick in d\u00e4mmernde Fernen zur\u00fcck", "tokens": ["Mein", "Blick", "in", "d\u00e4m\u00b7mern\u00b7de", "Fer\u00b7nen", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zu Wonnen, die lang erstarben.", "tokens": ["Zu", "Won\u00b7nen", ",", "die", "lang", "er\u00b7star\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nach des Morgens tauigem Gl\u00fchn", "tokens": ["Nach", "des", "Mor\u00b7gens", "tau\u00b7i\u00b7gem", "Gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und des Mittags leuchtendem Strahle", "tokens": ["Und", "des", "Mit\u00b7tags", "leuch\u00b7ten\u00b7dem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun Nacht und des Mondes eisiger Schein \u2013", "tokens": ["Nun", "Nacht", "und", "des", "Mon\u00b7des", "ei\u00b7si\u00b7ger", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Mitte des Friedhofs steh' ich allein", "tokens": ["In", "Mit\u00b7te", "des", "Fried\u00b7hofs", "steh'", "ich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und kr\u00e4nze mit dunklem Cypressengr\u00fcn", "tokens": ["Und", "kr\u00e4n\u00b7ze", "mit", "dunk\u00b7lem", "Cyp\u00b7res\u00b7sen\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verwitternde Totenmale.", "tokens": ["Ver\u00b7wit\u00b7tern\u00b7de", "To\u00b7ten\u00b7ma\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}