{"textgrid.poem.29337": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott hat seinen Sohn gesand, uns zu retten au\u00df der Noth;", "tokens": ["Gott", "hat", "sei\u00b7nen", "Sohn", "ge\u00b7sand", ",", "uns", "zu", "ret\u00b7ten", "au\u00df", "der", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Noth hat seinen Sohn erbarmt, drum zu leiden bittren Tod;", "tokens": ["Noth", "hat", "sei\u00b7nen", "Sohn", "er\u00b7barmt", ",", "drum", "zu", "lei\u00b7den", "bit\u00b7tren", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "PAV", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Tod wird schlecht von uns bedanckt, mehrentheils mit Fluch und Spot;", "tokens": ["Tod", "wird", "schlecht", "von", "uns", "be\u00b7danckt", ",", "meh\u00b7ren\u00b7theils", "mit", "Fluch", "und", "Spot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Spot darff leichte rechnen so ewig mit Spot, Tod, Noth, Gott.", "tokens": ["Spot", "darff", "leich\u00b7te", "rech\u00b7nen", "so", "e\u00b7wig", "mit", "Spot", ",", "Tod", ",", "Noth", ",", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "ADJA", "ADJA", "ADV", "ADJD", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}