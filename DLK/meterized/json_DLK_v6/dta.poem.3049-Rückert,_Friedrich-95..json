{"dta.poem.3049": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Such' alles, was du machst, aufs beste nur zu machen;", "tokens": ["Such'", "al\u00b7les", ",", "was", "du", "machst", ",", "aufs", "bes\u00b7te", "nur", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber, fragst du, ist die beste Art der Sachen?", "tokens": ["Was", "a\u00b7ber", ",", "fragst", "du", ",", "ist", "die", "bes\u00b7te", "Art", "der", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist etwas gut genug, so la\u00df es fein dabei,", "tokens": ["Ist", "et\u00b7was", "gut", "ge\u00b7nug", ",", "so", "la\u00df", "es", "fein", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "$,", "ADV", "VVIMP", "PPER", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frage nicht, ob es noch besser m\u00f6glich sei.", "tokens": ["Und", "fra\u00b7ge", "nicht", ",", "ob", "es", "noch", "bes\u00b7ser", "m\u00f6g\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}