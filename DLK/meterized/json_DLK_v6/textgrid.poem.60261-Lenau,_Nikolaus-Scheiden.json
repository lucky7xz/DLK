{"textgrid.poem.60261": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Scheiden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dahin sind Bl\u00fcten jetzt und Nachtigallen,", "tokens": ["Da\u00b7hin", "sind", "Bl\u00fc\u00b7ten", "jetzt", "und", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch den kahlen, sangverla\u00dfnen Strauch", "tokens": ["Und", "durch", "den", "kah\u00b7len", ",", "sang\u00b7ver\u00b7la\u00df\u00b7nen", "Strauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weht nun des Herbstes einsam k\u00fchler Hauch;", "tokens": ["Weht", "nun", "des", "Herbs\u00b7tes", "ein\u00b7sam", "k\u00fch\u00b7ler", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Gl\u00fcck ist mit dem Laube abgefallen!", "tokens": ["Mein", "Gl\u00fcck", "ist", "mit", "dem", "Lau\u00b7be", "ab\u00b7ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist der Hain, wo ich mit dir oft weilte,", "tokens": ["Das", "ist", "der", "Hain", ",", "wo", "ich", "mit", "dir", "oft", "weil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Das ist der B\u00fcsche wonnigliche Haft,", "tokens": ["Das", "ist", "der", "B\u00fc\u00b7sche", "won\u00b7nig\u00b7li\u00b7che", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo uns am Flehen s\u00fc\u00dfer Leidenschaft", "tokens": ["Wo", "uns", "am", "Fle\u00b7hen", "s\u00fc\u00b7\u00dfer", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unfesselbar die Zeit vor\u00fcbereilte.", "tokens": ["Un\u00b7fes\u00b7sel\u00b7bar", "die", "Zeit", "vor\u00b7\u00fc\u00b7be\u00b7reil\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du wanderst fort, du willst die Welt durchmessen;", "tokens": ["Du", "wan\u00b7derst", "fort", ",", "du", "willst", "die", "Welt", "durch\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier ist der Pfad, so schlangenkrumm und kalt,", "tokens": ["Hier", "ist", "der", "Pfad", ",", "so", "schlan\u00b7gen\u00b7krumm", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der dich, Geliebter, locket mit Gewalt", "tokens": ["Der", "dich", ",", "Ge\u00b7lieb\u00b7ter", ",", "lo\u00b7cket", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und fortf\u00fchrt in die Fremde, ins Vergessen! \u2013", "tokens": ["Und", "fort\u00b7f\u00fchrt", "in", "die", "Frem\u00b7de", ",", "ins", "Ver\u00b7ges\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbdas Schiff bewegt mit seinem Reisedrange", "tokens": ["\u00bb", "das", "Schiff", "be\u00b7wegt", "mit", "sei\u00b7nem", "Rei\u00b7se\u00b7dran\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00f6rt empor die See aus glatter Ruh;", "tokens": ["Und", "st\u00f6rt", "em\u00b7por", "die", "See", "aus", "glat\u00b7ter", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ist es fort, schlie\u00dft sich die Welle zu,", "tokens": ["Doch", "ist", "es", "fort", ",", "schlie\u00dft", "sich", "die", "Wel\u00b7le", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gleichg\u00fcltig wallt sie fort im alten Gange.", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "wallt", "sie", "fort", "im", "al\u00b7ten", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Siehst du von jenem Baum den Raben fliegen?", "tokens": ["Siehst", "du", "von", "je\u00b7nem", "Baum", "den", "Ra\u00b7ben", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von seinem Fortschwung wankt und bebt der Ast", "tokens": ["Von", "sei\u00b7nem", "Fort\u00b7schwung", "wankt", "und", "bebt", "der", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Weilchen noch und kehrt zur alten Rast;", "tokens": ["Ein", "Weil\u00b7chen", "noch", "und", "kehrt", "zur", "al\u00b7ten", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Klagen werden bald versiegen!\u00ab", "tokens": ["Und", "dei\u00b7ne", "Kla\u00b7gen", "wer\u00b7den", "bald", "ver\u00b7sie\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}