{"textgrid.poem.42962": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Wohlgemeint an Biedermann", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh doch einmal ins Gegenteil", "tokens": ["Geh", "doch", "ein\u00b7mal", "ins", "Ge\u00b7gen\u00b7teil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df dich etwas kitzeln!", "tokens": ["Und", "la\u00df", "dich", "et\u00b7was", "kit\u00b7zeln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind oft unbefriedigt, weil", "tokens": ["Wir", "sind", "oft", "un\u00b7be\u00b7frie\u00b7digt", ",", "weil"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir \u00fcbersicher witzeln.", "tokens": ["Wir", "\u00fc\u00b7bers\u00b7i\u00b7cher", "wit\u00b7zeln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir ziehen satt in geregeltem Trott", "tokens": ["Wir", "zie\u00b7hen", "satt", "in", "ge\u00b7re\u00b7gel\u00b7tem", "Trott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Wegen, die scheinbar nie krumm gehn,", "tokens": ["Auf", "We\u00b7gen", ",", "die", "schein\u00b7bar", "nie", "krumm", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eine br\u00f6cklige Gipsb\u00fcste von Gott,", "tokens": ["Ei\u00b7ne", "br\u00f6ck\u00b7li\u00b7ge", "Gips\u00b7b\u00fcs\u00b7te", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nach der wir uns gar nicht mehr umsehn.", "tokens": ["Nach", "der", "wir", "uns", "gar", "nicht", "mehr", "um\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sage \u00bbwir\u00ab, und ich meine dabei", "tokens": ["Ich", "sa\u00b7ge", "\u00bb", "wir", "\u00ab", ",", "und", "ich", "mei\u00b7ne", "da\u00b7bei"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "$(", "$,", "KON", "PPER", "VVFIN", "PAV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbgut mittelbeamtlich erzogen\u00ab", "tokens": ["\u00bb", "gut", "mit\u00b7tel\u00b7beamt\u00b7lich", "er\u00b7zo\u00b7gen", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Sinne von Kirche, Staat, Polizei.", "tokens": ["Im", "Sin\u00b7ne", "von", "Kir\u00b7che", ",", "Staat", ",", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Alles andre ist ja erlogen.", "tokens": ["Al\u00b7les", "and\u00b7re", "ist", "ja", "er\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach reise doch mal nach Andrerseits", "tokens": ["Ach", "rei\u00b7se", "doch", "mal", "nach", "A\u00b7ndrer\u00b7seits"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und freue dich mit Verdammten,", "tokens": ["Und", "freu\u00b7e", "dich", "mit", "Ver\u00b7damm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "W\u00e4r's nur an einem \u00bbBeinespreiz\u00ab", "tokens": ["W\u00e4r's", "nur", "an", "ei\u00b7nem", "\u00bb", "Bei\u00b7nes\u00b7preiz", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr die mittleren Beamten.", "tokens": ["F\u00fcr", "die", "mitt\u00b7le\u00b7ren", "Be\u00b7am\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}