{"textgrid.poem.47013": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df du doch nur w\u00fc\u00dftest jene Sagen", "tokens": ["O", "da\u00df", "du", "doch", "nur", "w\u00fc\u00df\u00b7test", "je\u00b7ne", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von G\u00f6ttern, die entstiegen ihrem Reiche,", "tokens": ["Von", "G\u00f6t\u00b7tern", ",", "die", "ent\u00b7stie\u00b7gen", "ih\u00b7rem", "Rei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um unterm Schatten der arkad'schen Eiche", "tokens": ["Um", "un\u00b7term", "Schat\u00b7ten", "der", "ar\u00b7ka\u00b7d'\u00b7schen", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "An Kronen-Statt den Sch\u00e4ferhut zu tragen;", "tokens": ["An", "Kro\u00b7nen\u00b7Statt", "den", "Sch\u00e4\u00b7fer\u00b7hut", "zu", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du w\u00fcrdest nicht den Blick so niederschlagen,", "tokens": ["Du", "w\u00fcr\u00b7dest", "nicht", "den", "Blick", "so", "nie\u00b7der\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df einer jetzt auch \u2013 nicht vom Himmelreiche \u2013", "tokens": ["Da\u00df", "ei\u00b7ner", "jetzt", "auch", "\u2013", "nicht", "vom", "Him\u00b7mel\u00b7rei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "$(", "PTKNEG", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu deiner Th\u00fcr auf Liebespfaden schleiche,", "tokens": ["Zu", "dei\u00b7ner", "Th\u00fcr", "auf", "Lie\u00b7be\u00b7spfa\u00b7den", "schlei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht w\u00fcrdest du halb trotzen so, halb zagen.", "tokens": ["Nicht", "w\u00fcr\u00b7dest", "du", "halb", "trot\u00b7zen", "so", ",", "halb", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "ADJD", "VVFIN", "ADV", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geb\u00e4rdest du dich doch, als ob ein Sperber", "tokens": ["Ge\u00b7b\u00e4r\u00b7dest", "du", "dich", "doch", ",", "als", "ob", "ein", "Sper\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit blut'gen Krallen ich herniederstie\u00dfe,", "tokens": ["Mit", "blut'\u00b7gen", "Kral\u00b7len", "ich", "her\u00b7nie\u00b7ders\u00b7tie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich zu zerfleischen, scheueste der Tauben,", "tokens": ["Dich", "zu", "zer\u00b7flei\u00b7schen", ",", "scheu\u00b7es\u00b7te", "der", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als ob ein n\u00e4cht'ger Blitz ich, ein Verderber,", "tokens": ["Als", "ob", "ein", "n\u00e4cht'\u00b7ger", "Blitz", "ich", ",", "ein", "Ver\u00b7der\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Wolken zuck' und meinen Strahl nur schie\u00dfe,", "tokens": ["Aus", "Wol\u00b7ken", "zuck'", "und", "mei\u00b7nen", "Strahl", "nur", "schie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um anzuz\u00fcnden hier dein Dach von Schauben.", "tokens": ["Um", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "hier", "dein", "Dach", "von", "Schau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}