{"textgrid.poem.50758": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl ist die Lilie wunderbar,", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist die Lilie wunderbar,", "tokens": ["Wohl", "ist", "die", "Li\u00b7lie", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn stolz sie sich im Garten wiegt,", "tokens": ["Wenn", "stolz", "sie", "sich", "im", "Gar\u00b7ten", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrem Kelche sonnenklar", "tokens": ["In", "ih\u00b7rem", "Kel\u00b7che", "son\u00b7nen\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Langsam der Morgentau versiegt;", "tokens": ["Lang\u00b7sam", "der", "Mor\u00b7gen\u00b7tau", "ver\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch mag ich gehn und wandern,", "tokens": ["Doch", "mag", "ich", "gehn", "und", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So weit nur Lilien stehn:", "tokens": ["So", "weit", "nur", "Li\u00b7li\u00b7en", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ist keine vor der andern", "tokens": ["Ist", "kei\u00b7ne", "vor", "der", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit h\u00f6herm Schmuck versehn!", "tokens": ["Mit", "h\u00f6\u00b7herm", "Schmuck", "ver\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von Glanz und Lust und Klarheit voll", "tokens": ["Von", "Glanz", "und", "Lust", "und", "Klar\u00b7heit", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist alle diese reiche Welt;", "tokens": ["Ist", "al\u00b7le", "die\u00b7se", "rei\u00b7che", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wei\u00df nicht, wo ich mich wenden soll,", "tokens": ["Wei\u00df", "nicht", ",", "wo", "ich", "mich", "wen\u00b7den", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Sch\u00f6nheit nicht sich vor mich stellt.", "tokens": ["Da\u00df", "Sch\u00f6n\u00b7heit", "nicht", "sich", "vor", "mich", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur du, nur du alleine", "tokens": ["Nur", "du", ",", "nur", "du", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In all der Zier und Pracht", "tokens": ["In", "all", "der", "Zier", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gleichst noch dem Mondenscheine", "tokens": ["Gleichst", "noch", "dem", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In heitrer Sternennacht!", "tokens": ["In", "hei\u00b7trer", "Ster\u00b7nen\u00b7nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O lieblichste Vollkommenheit,", "tokens": ["O", "lieb\u00b7lichs\u00b7te", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die niemand, als mein Herz, erkennt!", "tokens": ["Die", "nie\u00b7mand", ",", "als", "mein", "Herz", ",", "er\u00b7kennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat dies stille Licht geweiht,", "tokens": ["Wer", "hat", "dies", "stil\u00b7le", "Licht", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das nur f\u00fcr mich im Weltall brennt?", "tokens": ["Das", "nur", "f\u00fcr", "mich", "im", "Wel\u00b7tall", "brennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fchl es st\u00e4rker immer,", "tokens": ["Ich", "f\u00fchl", "es", "st\u00e4r\u00b7ker", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Das ist nicht Zufall, nicht Natur,", "tokens": ["Das", "ist", "nicht", "Zu\u00b7fall", ",", "nicht", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was aus den blauen Augen strahlt!", "tokens": ["Was", "aus", "den", "blau\u00b7en", "Au\u00b7gen", "strahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der Gottheit Sonnenspur,", "tokens": ["Das", "ist", "der", "Got\u00b7theit", "Son\u00b7nen\u00b7spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich in dieser Seele malt!", "tokens": ["Die", "sich", "in", "die\u00b7ser", "See\u00b7le", "malt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich ahn es licht und lichter,", "tokens": ["Ich", "ahn", "es", "licht", "und", "lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Herz, nun gib es zu:", "tokens": ["Mein", "Herz", ",", "nun", "gib", "es", "zu", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier ist ein andrer Dichter", "tokens": ["Hier", "ist", "ein", "an\u00b7drer", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und m\u00e4chtiger als du!", "tokens": ["Und", "m\u00e4ch\u00b7ti\u00b7ger", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}