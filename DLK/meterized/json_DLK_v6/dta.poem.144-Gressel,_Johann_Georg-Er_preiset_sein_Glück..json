{"dta.poem.144": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er preiset sein Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.85", "so:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So kan ich nun", "tokens": ["So", "kan", "ich", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Holdselige ", "tokens": ["Hold\u00b7se\u00b7li\u00b7ge"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Auf deiner Liljen B\u00fchne", "tokens": ["Auf", "dei\u00b7ner", "Lil\u00b7jen", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der zarten Br\u00fcste ruhn.", "tokens": ["Der", "zar\u00b7ten", "Br\u00fcs\u00b7te", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es machet mir die Lust", "tokens": ["Es", "ma\u00b7chet", "mir", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf deiner Schwaanen-Brust/", "tokens": ["Auf", "dei\u00b7ner", "Schwaa\u00b7nen\u00b7Brust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein sanfftes Rosen-Bette/", "tokens": ["Ein", "sanff\u00b7tes", "Ro\u00b7sen\u00b7Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Ballen prangen nette/", "tokens": ["Die", "Bal\u00b7len", "pran\u00b7gen", "net\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und machen ihre Freude kund", "tokens": ["Und", "ma\u00b7chen", "ih\u00b7re", "Freu\u00b7de", "kund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn mein erhitzter Mund", "tokens": ["Wenn", "mein", "er\u00b7hitz\u00b7ter", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Den sch\u00f6nen Ort bek\u00fc\u00dft.", "tokens": ["Den", "sch\u00f6\u00b7nen", "Ort", "be\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Sie springen in die H\u00f6he", "tokens": ["Sie", "sprin\u00b7gen", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wie ein erfreutes Rehe", "tokens": ["Wie", "ein", "er\u00b7freu\u00b7tes", "Re\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Das aus dem Netz entgangen ist.", "tokens": ["Das", "aus", "dem", "Netz", "ent\u00b7gan\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Auf diesen Liljen-Auen", "tokens": ["Auf", "die\u00b7sen", "Lil\u00b7jen\u00b7Au\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Sieht man nur ", "tokens": ["Sieht", "man", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Der meiner Seelen schmeckt", "tokens": ["Der", "mei\u00b7ner", "See\u00b7len", "schmeckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Als wie ein k\u00f6stliches ", "tokens": ["Als", "wie", "ein", "k\u00f6st\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Ich bin im Paradies/", "tokens": ["Ich", "bin", "im", "Pa\u00b7ra\u00b7dies", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Und kan mein g\u00fcldnes Vlie\u00df", "tokens": ["Und", "kan", "mein", "g\u00fcld\u00b7nes", "Vlie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "In deinen ", "tokens": ["In", "dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Mein liebster Engel finden.", "tokens": ["Mein", "liebs\u00b7ter", "En\u00b7gel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Nichts ist die Garten-Pracht", "tokens": ["Nichts", "ist", "die", "Gar\u00b7ten\u00b7Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Vor dieser Brust", "tokens": ["Vor", "die\u00b7ser", "Brust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Vergehet deren Lust.", "tokens": ["Ver\u00b7ge\u00b7het", "de\u00b7ren", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Es zeiget zwar nur Schnee", "tokens": ["Es", "zei\u00b7get", "zwar", "nur", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Die Alabaster H\u00f6h/", "tokens": ["Die", "A\u00b7la\u00b7bas\u00b7ter", "H\u00f6h", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Doch pr\u00e4chtige ", "tokens": ["Doch", "pr\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Mit Liljen untermischt hier bey ", "tokens": ["Mit", "Lil\u00b7jen", "un\u00b7ter\u00b7mischt", "hier", "bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die sch\u00f6ne W\u00fcsteney", "tokens": ["Die", "sch\u00f6\u00b7ne", "W\u00fcs\u00b7te\u00b7ney"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Und das ", "tokens": ["Und", "das"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Der allersch\u00f6nsten Br\u00fcste", "tokens": ["Der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Hegt rechte Himmels-L\u00fcste.", "tokens": ["Hegt", "rech\u00b7te", "Him\u00b7mels\u00b7L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Das Ey\u00df-beseelte Werck", "tokens": ["Das", "Ey\u00df\u00b7be\u00b7seel\u00b7te", "Werck"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Und der beschneete Berg/", "tokens": ["Und", "der", "be\u00b7schnee\u00b7te", "Berg", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Hat unsch\u00e4tzbahre Sch\u00e4tze/", "tokens": ["Hat", "un\u00b7sch\u00e4tz\u00b7bah\u00b7re", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Sie sind der Wollust Lager-Pl\u00e4tze.", "tokens": ["Sie", "sind", "der", "Wol\u00b7lust", "La\u00b7ger\u00b7Pl\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Es weicht das ", "tokens": ["Es", "weicht", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Sie sind bekr\u00f6net mit Corallen.", "tokens": ["Sie", "sind", "be\u00b7kr\u00f6\u00b7net", "mit", "Co\u00b7ral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Cleopatra mag nur mit ihren Perlen prangen/", "tokens": ["Cleo\u00b7pat\u00b7ra", "mag", "nur", "mit", "ih\u00b7ren", "Per\u00b7len", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.42": {"text": "Ich kan an solchen hangen/", "tokens": ["Ich", "kan", "an", "sol\u00b7chen", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Die mehr/ als ihre/ sch\u00f6ne sind.", "tokens": ["Die", "mehr", "/", "als", "ih\u00b7re", "/", "sch\u00f6\u00b7ne", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "KOKOM", "PPOSAT", "$(", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Rubinen und ", "tokens": ["Ru\u00b7bi\u00b7nen", "und"], "token_info": ["word", "word"], "pos": ["NN", "KON"], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "Die m\u00fcssen diese Perlen zieren", "tokens": ["Die", "m\u00fcs\u00b7sen", "die\u00b7se", "Per\u00b7len", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Die man auf deinen Br\u00fcsten findt.", "tokens": ["Die", "man", "auf", "dei\u00b7nen", "Br\u00fcs\u00b7ten", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Die Wollust selbst kan nicht vergn\u00fcgter ruhn/", "tokens": ["Die", "Wol\u00b7lust", "selbst", "kan", "nicht", "ver\u00b7gn\u00fcg\u00b7ter", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Als wie ich nun", "tokens": ["Als", "wie", "ich", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Annehmliche ", "tokens": ["An\u00b7nehm\u00b7li\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.50": {"text": "Auf dieser Liljen-B\u00fchne.", "tokens": ["Auf", "die\u00b7ser", "Lil\u00b7jen\u00b7B\u00fch\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Ich lieg im ", "tokens": ["Ich", "lieg", "im"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.52": {"text": "Und diese macht mir blo\u00df", "tokens": ["Und", "die\u00b7se", "macht", "mir", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Die wunder-sch\u00f6nen Gaben/", "tokens": ["Die", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "So Hertz und Sinnen laben.", "tokens": ["So", "Hertz", "und", "Sin\u00b7nen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Der Wollust-Zucker quilt aus diesen Ballen/", "tokens": ["Der", "Wol\u00b7lust\u00b7Zu\u00b7cker", "quilt", "aus", "die\u00b7sen", "Bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Cupido nehret sich mit dem Erquickungs-Safft", "tokens": ["Cu\u00b7pi\u00b7do", "neh\u00b7ret", "sich", "mit", "dem", "Er\u00b7qui\u00b7ckungs\u00b7\u00b7S\u00b7afft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.57": {"text": "So ihm dein Busen schafft", "tokens": ["So", "ihm", "dein", "Bu\u00b7sen", "schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Aus den Corallen.", "tokens": ["Aus", "den", "Co\u00b7ral\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.59": {"text": "Dir sind die Brust-Granaten", "tokens": ["Dir", "sind", "die", "Brust\u00b7Gra\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Belline wohlgerahten.", "tokens": ["Bel\u00b7li\u00b7ne", "wohl\u00b7ge\u00b7rah\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.61": {"text": "Cupido l\u00e4st die Brust der g\u00fctigen Dionen", "tokens": ["Cu\u00b7pi\u00b7do", "l\u00e4st", "die", "Brust", "der", "g\u00fc\u00b7ti\u00b7gen", "Di\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und will mit mir in deinem Busen wohnen.", "tokens": ["Und", "will", "mit", "mir", "in", "dei\u00b7nem", "Bu\u00b7sen", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}