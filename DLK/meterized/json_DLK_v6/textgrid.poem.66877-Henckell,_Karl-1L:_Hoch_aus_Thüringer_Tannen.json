{"textgrid.poem.66877": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch aus Th\u00fcringer Tannen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch aus Th\u00fcringer Tannen", "tokens": ["Hoch", "aus", "Th\u00fc\u00b7rin\u00b7ger", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Saust ein Sang:", "tokens": ["Saust", "ein", "Sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "\u00bbzeiten verrannen,", "tokens": ["\u00bb", "zei\u00b7ten", "ver\u00b7ran\u00b7nen", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ewig w\u00e4hrt lang.", "tokens": ["E\u00b7wig", "w\u00e4hrt", "lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Sahn seit tausend Jahren", "tokens": ["Sahn", "seit", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Viele schon", "tokens": ["Vie\u00b7le", "schon"], "token_info": ["word", "word"], "pos": ["PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Weltwald durchfahren,", "tokens": ["Welt\u00b7wald", "durch\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Karren und Thron.", "tokens": ["Kar\u00b7ren", "und", "Thron", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Prachtschimmernde Kaiser", "tokens": ["Pracht\u00b7schim\u00b7mern\u00b7de", "Kai\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Reiten stolz,", "tokens": ["Rei\u00b7ten", "stolz", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Armenkinder Reiser", "tokens": ["Ar\u00b7men\u00b7kin\u00b7der", "Rei\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Raffen, Hungerholz.", "tokens": ["Raf\u00b7fen", ",", "Hun\u00b7ger\u00b7holz", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Goldene Zepter fielen,", "tokens": ["Gol\u00b7de\u00b7ne", "Zep\u00b7ter", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Zapfen gleich \u2013", "tokens": ["Zap\u00b7fen", "gleich", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sturmwinde spielen", "tokens": ["Sturm\u00b7win\u00b7de", "spie\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit arm und reich.", "tokens": ["Mit", "arm", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "M\u00e4chtige Sturmwinde fegen", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "Sturm\u00b7win\u00b7de", "fe\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Volk und Land,", "tokens": ["Volk", "und", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wesen sich regen,", "tokens": ["We\u00b7sen", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Blitzverwandt.", "tokens": ["Blitz\u00b7ver\u00b7wandt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Forschende Menschenk\u00f6pfe", "tokens": ["For\u00b7schen\u00b7de", "Men\u00b7schen\u00b7k\u00f6p\u00b7fe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schaffen Licht:", "tokens": ["Schaf\u00b7fen", "Licht", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wirkende Geistgesch\u00f6pfe", "tokens": ["Wir\u00b7ken\u00b7de", "Geist\u00b7ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wandeln das Erdgesicht.", "tokens": ["Wan\u00b7deln", "das", "Erd\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "W\u00fctende K\u00e4mpfe rasen", "tokens": ["W\u00fc\u00b7ten\u00b7de", "K\u00e4mp\u00b7fe", "ra\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Immerfort,", "tokens": ["Im\u00b7mer\u00b7fort", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schalmeien blasen", "tokens": ["Schal\u00b7mei\u00b7en", "bla\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mitten durch Mord.", "tokens": ["Mit\u00b7ten", "durch", "Mord", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Milde Schalmeien schallen", "tokens": ["Mil\u00b7de", "Schal\u00b7mei\u00b7en", "schal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hell und klar,", "tokens": ["Hell", "und", "klar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wahnfesten fallen \u2013", "tokens": ["Wahn\u00b7fes\u00b7ten", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das ist wahr.", "tokens": ["Das", "ist", "wahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Wie die morschen Ruinen", "tokens": ["Wie", "die", "mor\u00b7schen", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf den H\u00f6hn,", "tokens": ["Auf", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wo die Eisenschienen", "tokens": ["Wo", "die", "Ei\u00b7sen\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vor\u00fcbergehn.", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Eiserne Schienen spannen", "tokens": ["Ei\u00b7ser\u00b7ne", "Schie\u00b7nen", "span\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weit ihr Netz", "tokens": ["Weit", "ihr", "Netz"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "\u00dcber die h\u00f6chsten Tannen \u2013", "tokens": ["\u00dc\u00b7ber", "die", "h\u00f6chs\u00b7ten", "Tan\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Menschenkraft ist Gesetz.", "tokens": ["Men\u00b7schen\u00b7kraft", "ist", "Ge\u00b7setz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "Weltverkehr ist das Zeichen,", "tokens": ["Welt\u00b7ver\u00b7kehr", "ist", "das", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Blitz das Band,", "tokens": ["Blitz", "das", "Band", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Fichte und Palme reichen", "tokens": ["Fich\u00b7te", "und", "Pal\u00b7me", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich die Hand.", "tokens": ["Sich", "die", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Mensch will Mensch sich verbinden,", "tokens": ["Mensch", "will", "Mensch", "sich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Fremder Ha\u00df", "tokens": ["Frem\u00b7der", "Ha\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "M\u00e4hlich verwinden \u2013", "tokens": ["M\u00e4h\u00b7lich", "ver\u00b7win\u00b7den", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wahr ist das.", "tokens": ["Wahr", "ist", "das", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDS", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Droht noch roher Gewalten", "tokens": ["Droht", "noch", "ro\u00b7her", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Urkraftgroll \u2013", "tokens": ["Ur\u00b7kraft\u00b7groll", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Feiner will sich entfalten,", "tokens": ["Fei\u00b7ner", "will", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was wachsen soll.", "tokens": ["Was", "wach\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Die sich m\u00fchen in Tiefen,", "tokens": ["Die", "sich", "m\u00fc\u00b7hen", "in", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die da frei", "tokens": ["Die", "da", "frei"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wirken im Licht, sie riefen:", "tokens": ["Wir\u00b7ken", "im", "Licht", ",", "sie", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Not geht vorbei!", "tokens": ["Not", "geht", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Nicht dem\u00fctig hinkeuchen,", "tokens": ["Nicht", "de\u00b7m\u00fc\u00b7tig", "hin\u00b7keu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sei das Los!", "tokens": ["Sei", "das", "Los", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Knechtschaft k\u00fchn verscheuchen,", "tokens": ["Knecht\u00b7schaft", "k\u00fchn", "ver\u00b7scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Macht das Leben gro\u00df.", "tokens": ["Macht", "das", "Le\u00b7ben", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Luft und Lichtung bereiten", "tokens": ["Luft", "und", "Lich\u00b7tung", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Junger Saat,", "tokens": ["Jun\u00b7ger", "Saat", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df die Zweige sich weiten", "tokens": ["Da\u00df", "die", "Zwei\u00b7ge", "sich", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hoch und grad.", "tokens": ["Hoch", "und", "grad", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.17": {"line.1": {"text": "Bis die Wachstumsgenossen,", "tokens": ["Bis", "die", "Wachs\u00b7tums\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Lichtgekr\u00f6nt,", "tokens": ["Licht\u00b7ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wahren Bund geschlossen,", "tokens": ["Wah\u00b7ren", "Bund", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zwist vers\u00f6hnt.", "tokens": ["Zwist", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.18": {"line.1": {"text": "Bis ein heiliges Rauschen", "tokens": ["Bis", "ein", "hei\u00b7li\u00b7ges", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Alles eint,", "tokens": ["Al\u00b7les", "eint", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wodans Raben lauschen,", "tokens": ["Wo\u00b7dans", "Ra\u00b7ben", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und der leuchtende Siegfried der Welt erscheint.\u00ab", "tokens": ["Und", "der", "leuch\u00b7ten\u00b7de", "Sieg\u00b7fried", "der", "Welt", "er\u00b7scheint", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+--++-+-+", "measure": "anapaest.di.plus"}}}}}