{"textgrid.poem.24320": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Diskret", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei M\u00e4dchen, die einen schlechten Lebenswandel f\u00fchren", "tokens": ["Bei", "M\u00e4d\u00b7chen", ",", "die", "ei\u00b7nen", "schlech\u00b7ten", "Le\u00b7bens\u00b7wan\u00b7del", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sich dabei nicht einmal zieren,", "tokens": ["Und", "sich", "da\u00b7bei", "nicht", "ein\u00b7mal", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei M\u00e4dchen, die, wenn es d\u00e4mmert, spazieren,", "tokens": ["Bei", "M\u00e4d\u00b7chen", ",", "die", ",", "wenn", "es", "d\u00e4m\u00b7mert", ",", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Indem sie sich in den H\u00fcften wiegen,", "tokens": ["In\u00b7dem", "sie", "sich", "in", "den", "H\u00fcf\u00b7ten", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "W\u00e4hrend sie sonst meistens im Bette liegen", "tokens": ["W\u00e4h\u00b7rend", "sie", "sonst", "meis\u00b7tens", "im", "Bet\u00b7te", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Oder Patience legen", "tokens": ["O\u00b7der", "Pa\u00b7ti\u00b7en\u00b7ce", "le\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder einer Lekt\u00fcre pflegen,", "tokens": ["O\u00b7der", "ei\u00b7ner", "Lek\u00b7t\u00fc\u00b7re", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Die man nicht anders als mi\u00dfbilligen kann,", "tokens": ["Die", "man", "nicht", "an\u00b7ders", "als", "mi\u00df\u00b7bil\u00b7li\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "KOKOM", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Weil sie die Seele nicht hinan,", "tokens": ["Weil", "sie", "die", "See\u00b7le", "nicht", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sondern hinunter f\u00fchrt in Sph\u00e4ren,", "tokens": ["Son\u00b7dern", "hin\u00b7un\u00b7ter", "f\u00fchrt", "in", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Wo reine Seelen niemals verkehren,", "tokens": ["Wo", "rei\u00b7ne", "See\u00b7len", "nie\u00b7mals", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Bei M\u00e4dchen, sag ich, solcher Sorte,", "tokens": ["Bei", "M\u00e4d\u00b7chen", ",", "sag", "ich", ",", "sol\u00b7cher", "Sor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df, sie nach Geb\u00fchr zu charakterisieren,", "tokens": ["Da\u00df", ",", "sie", "nach", "Ge\u00b7b\u00fchr", "zu", "cha\u00b7rak\u00b7te\u00b7ri\u00b7sie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Die deutsche Sprache ermangelt der Worte,", "tokens": ["Die", "deut\u00b7sche", "Spra\u00b7che", "er\u00b7man\u00b7gelt", "der", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Weil sich sogar die \u00e4ltesten Adjektive genieren,", "tokens": ["Weil", "sich", "so\u00b7gar", "die", "\u00e4l\u00b7tes\u00b7ten", "Ad\u00b7jek\u00b7ti\u00b7ve", "ge\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Sieht man, legt man seinen Hut", "tokens": ["Sieht", "man", ",", "legt", "man", "sei\u00b7nen", "Hut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf der Kommode oder sonstwo nieder,", "tokens": ["Auf", "der", "Kom\u00b7mo\u00b7de", "o\u00b7der", "sonst\u00b7wo", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Hergestellt durch Photographie", "tokens": ["Her\u00b7ge\u00b7stellt", "durch", "Pho\u00b7to\u00b7gra\u00b7phie"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Oft eine ganze Bildergalerie", "tokens": ["Oft", "ei\u00b7ne", "gan\u00b7ze", "Bil\u00b7der\u00b7ga\u00b7le\u00b7rie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Von M\u00e4nnerantlitzen brav und bieder.", "tokens": ["Von", "M\u00e4n\u00b7ne\u00b7rant\u00b7lit\u00b7zen", "brav", "und", "bie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zumal die Armee und Reichsmarine", "tokens": ["Zu\u00b7mal", "die", "Ar\u00b7mee", "und", "Reichs\u00b7ma\u00b7ri\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Steuert dazu bei manche stolze Miene", "tokens": ["Steu\u00b7ert", "da\u00b7zu", "bei", "man\u00b7che", "stol\u00b7ze", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Von kriegerischer Entschlossenheit", "tokens": ["Von", "krie\u00b7ge\u00b7ri\u00b7scher", "Ent\u00b7schlos\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Schnurrbartesisterreichtigkeit.", "tokens": ["Und", "Schnurr\u00b7bar\u00b7te\u00b7sis\u00b7ter\u00b7reich\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Aus allen Truppen treffen sich hier", "tokens": ["Aus", "al\u00b7len", "Trup\u00b7pen", "tref\u00b7fen", "sich", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Korporal, Sergeant und Unteroffizier,", "tokens": ["Kor\u00b7po\u00b7ral", ",", "Ser\u00b7ge\u00b7ant", "und", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Wobei nat\u00fcrlich der Kavallerist", "tokens": ["Wo\u00b7bei", "na\u00b7t\u00fcr\u00b7lich", "der", "Ka\u00b7val\u00b7le\u00b7rist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Immer bei weitem der sch\u00f6nste ist.", "tokens": ["Im\u00b7mer", "bei", "wei\u00b7tem", "der", "sch\u00f6ns\u00b7te", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "ART", "ADJA", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Das Zivil ist nicht so stark vertreten,", "tokens": ["Das", "Zi\u00b7vil", "ist", "nicht", "so", "stark", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Und wenn, so sind es meist Athleten,", "tokens": ["Und", "wenn", ",", "so", "sind", "es", "meist", "Ath\u00b7le\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die auf dem Bizeps eingegraben", "tokens": ["Die", "auf", "dem", "Bi\u00b7zeps", "ein\u00b7ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Einen blauen Reichsadler haben", "tokens": ["Ei\u00b7nen", "blau\u00b7en", "Reich\u00b7sad\u00b7ler", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Oder das Bildnis seiner Majest\u00e4t", "tokens": ["O\u00b7der", "das", "Bild\u00b7nis", "sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Oder eine sch\u00f6ne Nudit\u00e4t", "tokens": ["O\u00b7der", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Nu\u00b7di\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Oder ein n\u00fctzliches Ger\u00e4t,", "tokens": ["O\u00b7der", "ein", "n\u00fctz\u00b7li\u00b7ches", "Ge\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Z.B. einen Anker oder eine Kanone.", "tokens": ["Z.", "B.", "ei\u00b7nen", "An\u00b7ker", "o\u00b7der", "ei\u00b7ne", "Ka\u00b7no\u00b7ne", "."], "token_info": ["abbreviation", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Zum Beweise aber, da\u00df auch Gef\u00fchl in ihnen wohne,", "tokens": ["Zum", "Be\u00b7wei\u00b7se", "a\u00b7ber", ",", "da\u00df", "auch", "Ge\u00b7f\u00fchl", "in", "ih\u00b7nen", "woh\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "ADV", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Sieht man auf ihrer Mannesbrust zumeist", "tokens": ["Sieht", "man", "auf", "ih\u00b7rer", "Man\u00b7nes\u00b7brust", "zu\u00b7meist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Herz, aus dem eine Flamme schl\u00e4gt,", "tokens": ["Ein", "Herz", ",", "aus", "dem", "ei\u00b7ne", "Flam\u00b7me", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Was, gut und richtig ausgelegt,", "tokens": ["Was", ",", "gut", "und", "rich\u00b7tig", "aus\u00b7ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Auch bei Athleten soviel wie Liebe hei\u00dft.", "tokens": ["Auch", "bei", "Ath\u00b7le\u00b7ten", "so\u00b7viel", "wie", "Lie\u00b7be", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PIAT", "KOKOM", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Trotzdem ist, wenn man den M\u00e4dchen glaubt,", "tokens": ["Trotz\u00b7dem", "ist", ",", "wenn", "man", "den", "M\u00e4d\u00b7chen", "glaubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der Gedanke an Liebe hier nicht erlaubt.", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", "an", "Lie\u00b7be", "hier", "nicht", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Alle diese Gefreiten und Sergeanten", "tokens": ["Al\u00b7le", "die\u00b7se", "Ge\u00b7frei\u00b7ten", "und", "Ser\u00b7ge\u00b7an\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PDAT", "NN", "KON", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Geh\u00f6ren zu Annas Anverwandten,", "tokens": ["Ge\u00b7h\u00f6\u00b7ren", "zu", "An\u00b7nas", "An\u00b7ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Desgleichen ein jeder Kraftathlet", "tokens": ["Des\u00b7glei\u00b7chen", "ein", "je\u00b7der", "Kraf\u00b7tath\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "PIAT", "NN"], "meter": "-+--+----", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Zu ihr in Verwandtschaftsbeziehungen steht", "tokens": ["Zu", "ihr", "in", "Ver\u00b7wandt\u00b7schafts\u00b7be\u00b7zie\u00b7hun\u00b7gen", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "(woraus ein jeder ersehen mag:", "tokens": ["(", "wo\u00b7raus", "ein", "je\u00b7der", "er\u00b7se\u00b7hen", "mag", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Sie ist von einem guten Schlag),", "tokens": ["Sie", "ist", "von", "ei\u00b7nem", "gu\u00b7ten", "Schlag", ")", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch ist es besser, sie offenbart", "tokens": ["Doch", "ist", "es", "bes\u00b7ser", ",", "sie", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ihre t\u00fcchtige Herkunft auf aktive Art,", "tokens": ["Ih\u00b7re", "t\u00fcch\u00b7ti\u00b7ge", "Her\u00b7kunft", "auf", "ak\u00b7ti\u00b7ve", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Denn, ohne ungalant zu sein,", "tokens": ["Denn", ",", "oh\u00b7ne", "un\u00b7ga\u00b7lant", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word