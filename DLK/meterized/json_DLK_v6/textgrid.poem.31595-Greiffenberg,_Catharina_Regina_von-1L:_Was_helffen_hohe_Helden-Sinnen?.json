{"textgrid.poem.31595": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was helffen hohe Helden-Sinnen?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was helffen hohe Helden-Sinnen?", "tokens": ["Was", "helf\u00b7fen", "ho\u00b7he", "Hel\u00b7den\u00b7Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was nutzt ein edler Tugend-Muht?", "tokens": ["was", "nutzt", "ein", "ed\u00b7ler", "Tu\u00b7gen\u00b7d\u00b7M\u00b7uht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "was hilfft / das Herz voll Himmel Glut /", "tokens": ["was", "hilfft", "/", "das", "Herz", "voll", "Him\u00b7mel", "Glut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ART", "NN", "ADJD", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Augen voller Heroinnen?", "tokens": ["die", "Au\u00b7gen", "vol\u00b7ler", "He\u00b7ro\u00b7in\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wann solche in den F\u00e4sseln brinnen!", "tokens": ["wann", "sol\u00b7che", "in", "den", "F\u00e4s\u00b7seln", "brin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist das selbste Gut nicht gut:", "tokens": ["Es", "ist", "das", "selbs\u00b7te", "Gut", "nicht", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "man wirfft es willig in die Flut /", "tokens": ["man", "wirfft", "es", "wil\u00b7lig", "in", "die", "Flut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "dem Schiffbruchs-Vngl\u00fcck zu entrinnen.", "tokens": ["dem", "Schiff\u00b7bruchs\u00b7\u00b7V\u00b7ng\u00b7l\u00fcck", "zu", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Nicht Laster nur / auch Tugend bringet", "tokens": ["Nicht", "Las\u00b7ter", "nur", "/", "auch", "Tu\u00b7gend", "brin\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "$(", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "zur Vnzeit h\u00f6chstes Vngemach:", "tokens": ["zur", "Vn\u00b7zeit", "h\u00f6chs\u00b7tes", "Vn\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "wann sie nicht noch was h\u00f6hers zwinget /", "tokens": ["wann", "sie", "nicht", "noch", "was", "h\u00f6\u00b7hers", "zwin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "dem Stand und Zeit zu geben nach.", "tokens": ["dem", "Stand", "und", "Zeit", "zu", "ge\u00b7ben", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "die aufgehebte Lanz man senkt /", "tokens": ["die", "auf\u00b7ge\u00b7heb\u00b7te", "Lanz", "man", "senkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "wann man den Sieg zu kriegen denkt.", "tokens": ["wann", "man", "den", "Sieg", "zu", "krie\u00b7gen", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}