{"textgrid.poem.49178": {"metadata": {"author": {"name": "Lautensack, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Heiligsprechung der Het\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst: ob je mich einer lind verf\u00fchrt?", "tokens": ["Du", "fragst", ":", "ob", "je", "mich", "ei\u00b7ner", "lind", "ver\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "ADV", "PRF", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(oh wie so oft, da\u00df mich wer lind verf\u00fchrte! ...)", "tokens": ["(", "oh", "wie", "so", "oft", ",", "da\u00df", "mich", "wer", "lind", "ver\u00b7f\u00fchr\u00b7te", "!", "...", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "KOKOM", "ADV", "ADV", "$,", "KOUS", "PPER", "PWS", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fragst: ob je mich wer rauh anger\u00fchrt?", "tokens": ["Und", "fragst", ":", "ob", "je", "mich", "wer", "rauh", "an\u00b7ge\u00b7r\u00fchrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "ADV", "PPER", "PWS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "(wie \u00f6fter noch, da\u00df mich wer rauh anr\u00fchrte! ...)", "tokens": ["(", "wie", "\u00f6f\u00b7ter", "noch", ",", "da\u00df", "mich", "wer", "rauh", "an\u00b7r\u00fchr\u00b7te", "!", "...", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KOKOM", "ADV", "ADV", "$,", "KOUS", "PPER", "PWS", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schw\u00f6r es dir \u2013 bei was du auch verlangst:", "tokens": ["Ich", "schw\u00f6r", "es", "dir", "\u2013", "bei", "was", "du", "auch", "ver\u00b7langst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$(", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur meine Seele ist nicht frei von Makel", "tokens": ["Nur", "mei\u00b7ne", "See\u00b7le", "ist", "nicht", "frei", "von", "Ma\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und windet sich darob in H\u00f6llenangst \u2013 \u2013 \u2013 \u2013", "tokens": ["und", "win\u00b7det", "sich", "da\u00b7rob", "in", "H\u00f6l\u00b7len\u00b7angst", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "NN", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "jedoch mein ", "tokens": ["je\u00b7doch", "mein"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}}}}