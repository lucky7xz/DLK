{"dta.poem.10512": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wir fuhren allein im dunkeln", "tokens": ["Wir", "fuh\u00b7ren", "al\u00b7lein", "im", "dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Postwagen die ganze Nacht;", "tokens": ["Post\u00b7wa\u00b7gen", "die", "gan\u00b7ze", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir ruhten einander am Herzen,", "tokens": ["Wir", "ruh\u00b7ten", "ein\u00b7an\u00b7der", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir haben gescherzt und gelacht.", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7scherzt", "und", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Doch als es Morgens tagte,", "tokens": ["Doch", "als", "es", "Mor\u00b7gens", "tag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Kind, wie staunten wir!", "tokens": ["Mein", "Kind", ",", "wie", "staun\u00b7ten", "wir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn zwischen uns sa\u00df Amor,", "tokens": ["Denn", "zwi\u00b7schen", "uns", "sa\u00df", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der blinde Passagier.", "tokens": ["Der", "blin\u00b7de", "Pas\u00b7sa\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}