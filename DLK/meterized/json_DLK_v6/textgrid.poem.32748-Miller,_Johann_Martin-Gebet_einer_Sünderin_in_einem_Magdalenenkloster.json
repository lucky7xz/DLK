{"textgrid.poem.32748": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Gebet einer S\u00fcnderin in einem Magdalenenkloster", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich diesen Schleier? Durft' ich, ihn", "tokens": ["Ich", "die\u00b7sen", "Schlei\u00b7er", "?", "Durft'", "ich", ",", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "PDAT", "NN", "$.", "VMFIN", "PPER", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir umzuh\u00fcllen wagen?", "tokens": ["Mir", "um\u00b7zu\u00b7h\u00fcl\u00b7len", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darf eine freche S\u00fcnderin", "tokens": ["Darf", "ei\u00b7ne", "fre\u00b7che", "S\u00fcn\u00b7de\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Bild der Unschuld tragen?", "tokens": ["Dies", "Bild", "der", "Un\u00b7schuld", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Gott! Noch immer w\u00fctet hier", "tokens": ["O", "Gott", "!", "Noch", "im\u00b7mer", "w\u00fc\u00b7tet", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Innersten die H\u00f6lle!", "tokens": ["Im", "In\u00b7ners\u00b7ten", "die", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Noch folgt der S\u00fcnde Schrecken mir", "tokens": ["Noch", "folgt", "der", "S\u00fcn\u00b7de", "Schre\u00b7cken", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis tief in diese Zelle!", "tokens": ["Bis", "tief", "in", "die\u00b7se", "Zel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So manche tr\u00fcbe N\u00e4chte flohn,", "tokens": ["So", "man\u00b7che", "tr\u00fc\u00b7be", "N\u00e4ch\u00b7te", "flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner Qual belastet;", "tokens": ["Mit", "mei\u00b7ner", "Qual", "be\u00b7las\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So manche Tage schwanden schon,", "tokens": ["So", "man\u00b7che", "Ta\u00b7ge", "schwan\u00b7den", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Thr\u00e4nen durchgefastet!", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "durch\u00b7ge\u00b7fas\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und keine Gnad'! O la\u00df einmal,", "tokens": ["Und", "kei\u00b7ne", "Gnad'", "!", "O", "la\u00df", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$.", "NE", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarmer, dich erflehen!", "tokens": ["Er\u00b7bar\u00b7mer", ",", "dich", "er\u00b7fle\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df einmal einen Freudenstrahl", "tokens": ["La\u00df", "ein\u00b7mal", "ei\u00b7nen", "Freu\u00b7den\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Arme wieder sehen!", "tokens": ["Mich", "Ar\u00b7me", "wie\u00b7der", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hat doch, o Magdalena, dich", "tokens": ["Hat", "doch", ",", "o", "Mag\u00b7da\u00b7le\u00b7na", ",", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "$,", "FM", "FM", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Heiland angenommen!", "tokens": ["Dein", "Hei\u00b7land", "an\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und keine Rettung soll f\u00fcr mich,", "tokens": ["Und", "kei\u00b7ne", "Ret\u00b7tung", "soll", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, ewig keine kommen?", "tokens": ["Ach", ",", "e\u00b7wig", "kei\u00b7ne", "kom\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "PIAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}