{"textgrid.poem.62414": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Warum er ins Feld zog", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich zieh' ins Feld, mich hat geladen", "tokens": ["Ich", "zieh'", "ins", "Feld", ",", "mich", "hat", "ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein heiliges geliebtes Haupt;", "tokens": ["Ein", "hei\u00b7li\u00b7ges", "ge\u00b7lieb\u00b7tes", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Dank den ew'gen Himmelsgnaden,", "tokens": ["O", "Dank", "den", "ew'\u00b7gen", "Him\u00b7mels\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein K\u00f6nig hat den Kampf erlaubt.", "tokens": ["Mein", "K\u00f6\u00b7nig", "hat", "den", "Kampf", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich zieh' ins Feld f\u00fcr meinen Glauben,", "tokens": ["Ich", "zieh'", "ins", "Feld", "f\u00fcr", "mei\u00b7nen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr aller Welten h\u00f6chstes Gut;", "tokens": ["F\u00fcr", "al\u00b7ler", "Wel\u00b7ten", "h\u00f6chs\u00b7tes", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Nile schwur der Feind zu rauben", "tokens": ["Am", "Ni\u00b7le", "schwur", "der", "Feind", "zu", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns vom Altar des Heilands Blut.", "tokens": ["Uns", "vom", "Al\u00b7tar", "des", "Hei\u00b7lands", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich zieh' ins Feld f\u00fcr ew'ges Leben,", "tokens": ["Ich", "zieh'", "ins", "Feld", "f\u00fcr", "ew'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Freiheit und uraltes Recht;", "tokens": ["F\u00fcr", "Frei\u00b7heit", "und", "ur\u00b7al\u00b7tes", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In frischer Kraft soll sich erheben", "tokens": ["In", "fri\u00b7scher", "Kraft", "soll", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch, zu lange schon ein Knecht.", "tokens": ["Der", "Mensch", ",", "zu", "lan\u00b7ge", "schon", "ein", "Knecht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich zieh' ins Feld um Himmelsg\u00fcter", "tokens": ["Ich", "zieh'", "ins", "Feld", "um", "Him\u00b7mels\u00b7g\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht um F\u00fcrstenlohn und Ruhm;", "tokens": ["Und", "nicht", "um", "F\u00fcrs\u00b7ten\u00b7lohn", "und", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Ritter ist geborner H\u00fcter", "tokens": ["Ein", "Rit\u00b7ter", "ist", "ge\u00b7bor\u00b7ner", "H\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von jedem wahren Heiligthum.", "tokens": ["Von", "je\u00b7dem", "wah\u00b7ren", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich zieh' ins Feld f\u00fcr Deutschlands Ehre,", "tokens": ["Ich", "zieh'", "ins", "Feld", "f\u00fcr", "Deutschlands", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Lustspiel alter Heldenwelt,", "tokens": ["Das", "Lust\u00b7spiel", "al\u00b7ter", "Hel\u00b7den\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Lied und Minne wiederkehre", "tokens": ["Da\u00df", "Lied", "und", "Min\u00b7ne", "wie\u00b7der\u00b7keh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unser gr\u00fcnes Eichenzelt.", "tokens": ["In", "un\u00b7ser", "gr\u00fc\u00b7nes", "Ei\u00b7chen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich zieh' ins Feld mit freien Bauern", "tokens": ["Ich", "zieh'", "ins", "Feld", "mit", "frei\u00b7en", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ehrenwerther B\u00fcrgerzunft;", "tokens": ["Und", "eh\u00b7ren\u00b7wert\u00b7her", "B\u00fcr\u00b7ger\u00b7zunft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ernster Schlachtruf ist ihr Trauern", "tokens": ["Ein", "erns\u00b7ter", "Schlach\u00b7truf", "ist", "ihr", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um alter Zeiten Wiederkunft.", "tokens": ["Um", "al\u00b7ter", "Zei\u00b7ten", "Wie\u00b7der\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich zieh' ins Feld, da\u00df ferner gelte", "tokens": ["Ich", "zieh'", "ins", "Feld", ",", "da\u00df", "fer\u00b7ner", "gel\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Adel, meine Wappenzier,", "tokens": ["Mein", "A\u00b7del", ",", "mei\u00b7ne", "Wap\u00b7pen\u00b7zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mich der Ahnen keiner schelte", "tokens": ["Da\u00df", "mich", "der", "Ah\u00b7nen", "kei\u00b7ner", "schel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einst an des Paradieses Th\u00fcr.", "tokens": ["Einst", "an", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich zieh' ins Feld f\u00fcr meine Dame,", "tokens": ["Ich", "zieh'", "ins", "Feld", "f\u00fcr", "mei\u00b7ne", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6nste weit im ganzen Land,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "weit", "im", "gan\u00b7zen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ohne Tadel sei der Name", "tokens": ["Da\u00df", "oh\u00b7ne", "Ta\u00b7del", "sei", "der", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den sie zu tragen w\u00fcrdig fand.", "tokens": ["Den", "sie", "zu", "tra\u00b7gen", "w\u00fcr\u00b7dig", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich zieh' ins Feld, wo tausend sinken", "tokens": ["Ich", "zieh'", "ins", "Feld", ",", "wo", "tau\u00b7send", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als B\u00fcrger einer bessern Welt;", "tokens": ["Als", "B\u00fcr\u00b7ger", "ei\u00b7ner", "bes\u00b7sern", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll mir der Todesengel winken,", "tokens": ["Soll", "mir", "der", "To\u00b7de\u00b7sen\u00b7gel", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier bin ich, Herr, ich zieh' ins Feld.", "tokens": ["Hier", "bin", "ich", ",", "Herr", ",", "ich", "zieh'", "ins", "Feld", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}