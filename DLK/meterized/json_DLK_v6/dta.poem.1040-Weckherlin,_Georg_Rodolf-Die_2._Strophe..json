{"dta.poem.1040": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 2. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie/ grosser freund/ Was will dzsagt", "tokens": ["Wie", "/", "gros\u00b7ser", "freund", "/", "Was", "will", "dz\u00b7sagt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$(", "ADJA", "NN", "$(", "PWS", "VMFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das du allein in diser fraid", "tokens": ["Das", "du", "al\u00b7lein", "in", "di\u00b7ser", "fraid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Runtzlest dein angesicht mit laid/", "tokens": ["Runtz\u00b7lest", "dein", "an\u00b7ge\u00b7sicht", "mit", "laid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NE", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als ob du ein vrsach zuklagen?", "tokens": ["Als", "ob", "du", "ein", "vr\u00b7sach", "zu\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Oder mag es wol m\u00fcglich sein/", "tokens": ["O\u00b7der", "mag", "es", "wol", "m\u00fcg\u00b7lich", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das dein geh\u00f6r noch nicht vernommen/", "tokens": ["Das", "dein", "ge\u00b7h\u00f6r", "noch", "nicht", "ver\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie dise stund alher soll kommen", "tokens": ["Wie", "di\u00b7se", "stund", "al\u00b7her", "soll", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "VVFIN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der augen lust/ der sehlen schein?", "tokens": ["Der", "au\u00b7gen", "lust", "/", "der", "seh\u00b7len", "schein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es ist ein F\u00fcrstin ausserkoren/", "tokens": ["Es", "ist", "ein", "F\u00fcrs\u00b7tin", "aus\u00b7ser\u00b7ko\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von k\u00f6niglichem blut geboren/", "tokens": ["Von", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "blut", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die gr\u00f6ste Zier au\u00df Engelland/", "tokens": ["Die", "gr\u00f6s\u00b7te", "Zier", "au\u00df", "En\u00b7gel\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nu aber die erst in Teutschland.", "tokens": ["Nu", "a\u00b7ber", "die", "erst", "in", "Teutschland", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADV", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Der P\u00f6felpfleget Sie zu nennen", "tokens": ["Der", "P\u00f6\u00b7fel\u00b7pfle\u00b7get", "Sie", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Churf\u00fcrstin Elisabeth/", "tokens": ["Die", "Chur\u00b7f\u00fcrs\u00b7tin", "E\u00b7lis\u00b7a\u00b7beth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die aber so sie besser kennen", "tokens": ["Die", "a\u00b7ber", "so", "sie", "bes\u00b7ser", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der Sch\u00f6n/ vnd Keuschheit Mayesteth.", "tokens": ["Der", "Sch\u00f6n", "/", "vnd", "Keuschheit", "Ma\u00b7ye\u00b7steth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "KON", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}