{"textgrid.poem.42078": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson schreitet den Zaun eines Geheges ab", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df einer sonst, wohin die Zeit ihm schwand?", "tokens": ["Wei\u00df", "ei\u00b7ner", "sonst", ",", "wo\u00b7hin", "die", "Zeit", "ihm", "schwand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fand er wie ich sie je versammelt wieder?", "tokens": ["Fand", "er", "wie", "ich", "sie", "je", "ver\u00b7sam\u00b7melt", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dies ist der Zaun um ein betreutes Land.", "tokens": ["Dies", "ist", "der", "Zaun", "um", "ein", "be\u00b7treu\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Schar von V\u00f6geln lie\u00df sich nieder", "tokens": ["Wie", "ei\u00b7ne", "Schar", "von", "V\u00f6\u00b7geln", "lie\u00df", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Leben hier: ein Tag auf jedem Pfahl,", "tokens": ["Mein", "Le\u00b7ben", "hier", ":", "ein", "Tag", "auf", "je\u00b7dem", "Pfahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$.", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sie sind aufgereiht zu langen Jahren...", "tokens": ["Und", "sie", "sind", "auf\u00b7ge\u00b7reiht", "zu", "lan\u00b7gen", "Jah\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "PTKZU", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O seltsame Verwandlung! Aus der Qual", "tokens": ["O", "selt\u00b7sa\u00b7me", "Ver\u00b7wand\u00b7lung", "!", "Aus", "der", "Qual"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Sinnloser Fron mu\u00df ich zuletzt erfahren", "tokens": ["Sinn\u00b7lo\u00b7ser", "Fron", "mu\u00df", "ich", "zu\u00b7letzt", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Ein Gl\u00fcck. Verst\u00e4ndlicher um wenig zwar", "tokens": ["Ein", "Gl\u00fcck", ".", "Ver\u00b7st\u00e4nd\u00b7li\u00b7cher", "um", "we\u00b7nig", "zwar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "APPR", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als jene Last. Jedoch ein Gl\u00fcck. Als zeugte", "tokens": ["Als", "je\u00b7ne", "Last", ".", "Je\u00b7doch", "ein", "Gl\u00fcck", ".", "Als", "zeug\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$.", "ADV", "ART", "NN", "$.", "KOUS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Einst einem Fragenden der Zaun: Er war.", "tokens": ["Einst", "ei\u00b7nem", "Fra\u00b7gen\u00b7den", "der", "Zaun", ":", "Er", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}