{"textgrid.poem.31484": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Nacht! du kanst dem Tag' ansiegen:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Nacht! du kanst dem Tag' ansiegen:", "tokens": ["O", "Nacht", "!", "du", "kanst", "dem", "Tag'", "an\u00b7sie\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Himmels-Sonn' in dir anbricht.", "tokens": ["die", "Him\u00b7mels\u00b7Sonn'", "in", "dir", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bringst der Herz-und Heiden Liecht /", "tokens": ["Du", "bringst", "der", "Her\u00b7z\u00b7und", "Hei\u00b7den", "Liecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "von dem wir alle Klarheit kriegen.", "tokens": ["von", "dem", "wir", "al\u00b7le", "Klar\u00b7heit", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wunder-Kunst wolts also f\u00fcgen /", "tokens": ["Die", "Wun\u00b7der\u00b7Kunst", "wolts", "al\u00b7so", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "das dieses wurd' in dir verricht:", "tokens": ["das", "die\u00b7ses", "wurd'", "in", "dir", "ver\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "weil in den dunklen Wort man sicht", "tokens": ["weil", "in", "den", "dunk\u00b7len", "Wort", "man", "sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die gr\u00f6sten Sch\u00e4tz der Wei\u00dfheit liegen.", "tokens": ["die", "gr\u00f6s\u00b7ten", "Sch\u00e4tz", "der", "Wei\u00df\u00b7heit", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O Ehren-helle Freuden-Racht!", "tokens": ["O", "Eh\u00b7ren\u00b7hel\u00b7le", "Freu\u00b7den\u00b7Racht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "du hast den rechten Tag gebracht.", "tokens": ["du", "hast", "den", "rech\u00b7ten", "Tag", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie kund' es Gott doch Ewig schaffen /", "tokens": ["Wie", "kund'", "es", "Gott", "doch", "E\u00b7wig", "schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "das Er gew\u00fcnschter k\u00e4m' herzu:", "tokens": ["das", "Er", "ge\u00b7w\u00fcnschter", "k\u00e4m'", "her\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "in dem wir gleich am b\u00e4sten schlaffen /", "tokens": ["in", "dem", "wir", "gleich", "am", "b\u00e4s\u00b7ten", "schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "schenkt Er die allers\u00fcsste Ruh.", "tokens": ["schenkt", "Er", "die", "al\u00b7ler\u00b7s\u00fcss\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}