{"textgrid.poem.65834": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "108", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was w\u00e4r im Hirn, in Tinte nur zu kleiden,", "tokens": ["Was", "w\u00e4r", "im", "Hirn", ",", "in", "Tin\u00b7te", "nur", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "$,", "APPR", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das dir mein ganzes Herz nicht schon beschrieb?", "tokens": ["Das", "dir", "mein", "gan\u00b7zes", "Herz", "nicht", "schon", "be\u00b7schrieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was k\u00f6nnt' ich Neues sagen, was bedeuten,", "tokens": ["Was", "k\u00f6nnt'", "ich", "Neu\u00b7es", "sa\u00b7gen", ",", "was", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$,", "PWS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das deinem Wert entspr\u00e4ch und meinem Trieb?", "tokens": ["Das", "dei\u00b7nem", "Wert", "ent\u00b7spr\u00e4ch", "und", "mei\u00b7nem", "Trieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nichts, teurer Knab'! und drum nur eines alle Stunden,", "tokens": ["Nichts", ",", "teu\u00b7rer", "Knab'", "!", "und", "drum", "nur", "ei\u00b7nes", "al\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADJA", "NN", "$.", "KON", "PAV", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie fromm Gebet zu sagen bleibt mir s\u00fc\u00df.", "tokens": ["Wie", "fromm", "Ge\u00b7bet", "zu", "sa\u00b7gen", "bleibt", "mir", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nichts Altes gilt mir alt, sind wir verbunden,", "tokens": ["Nichts", "Al\u00b7tes", "gilt", "mir", "alt", ",", "sind", "wir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie da zuerst dein sch\u00f6n Gestirn ich pries.", "tokens": ["Wie", "da", "zu\u00b7erst", "dein", "sch\u00f6n", "Ge\u00b7stirn", "ich", "pries", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So ew'ge Lieb' in frischen Liebesbanden,", "tokens": ["So", "ew'\u00b7ge", "Lieb'", "in", "fri\u00b7schen", "Lie\u00b7bes\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "W\u00e4gt keines Alters Staub noch Feindlichkeit:", "tokens": ["W\u00e4gt", "kei\u00b7nes", "Al\u00b7ters", "Staub", "noch", "Feind\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Notwend'ge Runzeln sind ihr nicht vorhanden;", "tokens": ["Not\u00b7wen\u00b7d'\u00b7ge", "Run\u00b7zeln", "sind", "ihr", "nicht", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Zum Knecht auf immer macht sie sich die Zeit:", "tokens": ["Zum", "Knecht", "auf", "im\u00b7mer", "macht", "sie", "sich", "die", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sie, deren Keim sich da geboren f\u00fchlt,", "tokens": ["Sie", ",", "de\u00b7ren", "Keim", "sich", "da", "ge\u00b7bo\u00b7ren", "f\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PRF", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wo Zeit und Au\u00dfenwelt f\u00fcr tot ihn hielt.", "tokens": ["Wo", "Zeit", "und", "Au\u00b7\u00dfen\u00b7welt", "f\u00fcr", "tot", "ihn", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}