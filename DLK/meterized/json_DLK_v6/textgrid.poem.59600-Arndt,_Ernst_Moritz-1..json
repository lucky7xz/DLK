{"textgrid.poem.59600": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hebt das Herz! Hebt die Hand!", "tokens": ["Hebt", "das", "Herz", "!", "Hebt", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Schw\u00f6ret f\u00fcr die gro\u00dfe Sache,", "tokens": ["Schw\u00f6\u00b7ret", "f\u00fcr", "die", "gro\u00b7\u00dfe", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00f6rt den heil'gen Schwur der Rache!", "tokens": ["Schw\u00f6rt", "den", "heil'\u00b7gen", "Schwur", "der", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schw\u00f6ret auf das Vaterland!", "tokens": ["Schw\u00f6\u00b7ret", "auf", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schw\u00f6ret auf den Ruhm der Ahnen,", "tokens": ["Schw\u00f6\u00b7ret", "auf", "den", "Ruhm", "der", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf die deutsche Redlichkeit,", "tokens": ["Auf", "die", "deut\u00b7sche", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf die Freiheit der Germanen,", "tokens": ["Auf", "die", "Frei\u00b7heit", "der", "Ger\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf das H\u00f6chste schw\u00f6ret heut!", "tokens": ["Auf", "das", "H\u00f6chs\u00b7te", "schw\u00f6\u00b7ret", "heut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hebt das Herz! Hebt die Hand!", "tokens": ["Hebt", "das", "Herz", "!", "Hebt", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Erd' und Himmel soll ihn h\u00f6ren", "tokens": ["Erd'", "und", "Him\u00b7mel", "soll", "ihn", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsern hohen Schwur der Ehren,", "tokens": ["Un\u00b7sern", "ho\u00b7hen", "Schwur", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsern Schwur f\u00fcrs Vaterland.", "tokens": ["Un\u00b7sern", "Schwur", "f\u00fcrs", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Glorreich schwebe, stolzes Zeichen,", "tokens": ["Glor\u00b7reich", "schwe\u00b7be", ",", "stol\u00b7zes", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das voran im Streite weht!", "tokens": ["Das", "vo\u00b7ran", "im", "Strei\u00b7te", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Keiner soll von hinnen weichen,", "tokens": ["Kei\u00b7ner", "soll", "von", "hin\u00b7nen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo sich dies Panier erh\u00f6ht!", "tokens": ["Wo", "sich", "dies", "Pa\u00b7nier", "er\u00b7h\u00f6ht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PDS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hebt das Herz! Hebt die Hand!", "tokens": ["Hebt", "das", "Herz", "!", "Hebt", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wehe mutig, edle Fahne!", "tokens": ["We\u00b7he", "mu\u00b7tig", ",", "ed\u00b7le", "Fah\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sich jede Brust ermahne", "tokens": ["Da\u00df", "sich", "je\u00b7de", "Brust", "er\u00b7mah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das heil'ge Vaterland!", "tokens": ["F\u00fcr", "das", "heil'\u00b7ge", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mache, stolzes Ehrenzeichen,", "tokens": ["Ma\u00b7che", ",", "stol\u00b7zes", "Eh\u00b7ren\u00b7zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle M\u00e4nner ehrenfest,", "tokens": ["Al\u00b7le", "M\u00e4n\u00b7ner", "eh\u00b7ren\u00b7fest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie tausendmal erbleichen,", "tokens": ["Da\u00df", "sie", "tau\u00b7send\u00b7mal", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh' nur einer dich verl\u00e4\u00dft!", "tokens": ["Eh'", "nur", "ei\u00b7ner", "dich", "ver\u00b7l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hebt das Herz! Hebt die Hand!", "tokens": ["Hebt", "das", "Herz", "!", "Hebt", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Heil uns dieser Ehrenweihe!", "tokens": ["Heil", "uns", "die\u00b7ser", "Eh\u00b7ren\u00b7wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig lebe deutsche Treue!", "tokens": ["E\u00b7wig", "le\u00b7be", "deut\u00b7sche", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig bl\u00fche deutsches Land!", "tokens": ["E\u00b7wig", "bl\u00fc\u00b7he", "deut\u00b7sches", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freiheit, deutsche Freiheit, schwebe", "tokens": ["Frei\u00b7heit", ",", "deut\u00b7sche", "Frei\u00b7heit", ",", "schwe\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die H\u00fctten, um den Thron!", "tokens": ["Um", "die", "H\u00fct\u00b7ten", ",", "um", "den", "Thron", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trug und Lug und Schande bebe!", "tokens": ["Trug", "und", "Lug", "und", "Schan\u00b7de", "be\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zur H\u00f6lle fahre Hohn!", "tokens": ["Und", "zur", "H\u00f6l\u00b7le", "fah\u00b7re", "Hohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hebt das Herz! Hebt die Hand!", "tokens": ["Hebt", "das", "Herz", "!", "Hebt", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hebt sie zu der Welten Meister!", "tokens": ["Hebt", "sie", "zu", "der", "Wel\u00b7ten", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt sie zu dem Geist der Geister!", "tokens": ["Hebt", "sie", "zu", "dem", "Geist", "der", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hebt sie hoch vom Erdentand!", "tokens": ["Hebt", "sie", "hoch", "vom", "Er\u00b7den\u00b7tand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir's treu und heilig halten", "tokens": ["Da\u00df", "wir's", "treu", "und", "hei\u00b7lig", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Gedanken, Wort und Tat:", "tokens": ["In", "Ge\u00b7dan\u00b7ken", ",", "Wort", "und", "Tat", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}