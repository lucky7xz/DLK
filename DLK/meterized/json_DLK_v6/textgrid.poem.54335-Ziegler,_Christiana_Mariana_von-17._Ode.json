{"textgrid.poem.54335": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "17. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, nein, es bleibet doch dabey,", "tokens": ["Nein", ",", "nein", ",", "es", "blei\u00b7bet", "doch", "da\u00b7bey", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So stark ihr mir auch wiederstreitet,", "tokens": ["So", "stark", "ihr", "mir", "auch", "wie\u00b7der\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df keine Neigung \u00e4rger sey,", "tokens": ["Da\u00df", "kei\u00b7ne", "Nei\u00b7gung", "\u00e4r\u00b7ger", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als die, so uns zum Spiel verleitet.", "tokens": ["Als", "die", ",", "so", "uns", "zum", "Spiel", "ver\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist nicht die Spielsucht eine Pest,", "tokens": ["Ist", "nicht", "die", "Spiel\u00b7sucht", "ei\u00b7ne", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sich nicht wieder d\u00e4mpfen l\u00e4\u00dft,", "tokens": ["Die", "sich", "nicht", "wie\u00b7der", "d\u00e4mp\u00b7fen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wofern sie einmal Platz genommen?", "tokens": ["Wo\u00b7fern", "sie", "ein\u00b7mal", "Platz", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier ist ein solches Labyrinth,", "tokens": ["Hier", "ist", "ein", "sol\u00b7ches", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Darinn man keinen Ausgang findt,", "tokens": ["Da\u00b7rinn", "man", "kei\u00b7nen", "Aus\u00b7gang", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist man einmal darein gekommen.", "tokens": ["Ist", "man", "ein\u00b7mal", "da\u00b7rein", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "PAV", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Seuche rei\u00dft entsetzlich ein,", "tokens": ["Die", "Seu\u00b7che", "rei\u00dft", "ent\u00b7setz\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es will das halbe Rund der Erden", "tokens": ["Es", "will", "das", "hal\u00b7be", "Rund", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der eitlen Lust gewidmet seyn;", "tokens": ["Der", "eit\u00b7len", "Lust", "ge\u00b7wid\u00b7met", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem grossen Spielhaus werden:", "tokens": ["Zu", "ei\u00b7nem", "gros\u00b7sen", "Spiel\u00b7haus", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was nur die Finger regen kann,", "tokens": ["Was", "nur", "die", "Fin\u00b7ger", "re\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sieht die stummen G\u00f6tzen an,", "tokens": ["Das", "sieht", "die", "stum\u00b7men", "G\u00f6t\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die bunt gemahlt auf Bl\u00e4ttern stehen.", "tokens": ["Die", "bunt", "ge\u00b7mahlt", "auf", "Bl\u00e4t\u00b7tern", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies ist das Buch, das alle Welt,", "tokens": ["Dies", "ist", "das", "Buch", ",", "das", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vor so beliebt und edel h\u00e4lt,", "tokens": ["Vor", "so", "be\u00b7liebt", "und", "e\u00b7del", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Dies mu\u00df oft \u00fcber alles gehen.", "tokens": ["Dies", "mu\u00df", "oft", "\u00fc\u00b7ber", "al\u00b7les", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So mancher Ballen von Papier", "tokens": ["So", "man\u00b7cher", "Bal\u00b7len", "von", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch kein Rechner hat summiret,", "tokens": ["Die", "noch", "kein", "Rech\u00b7ner", "hat", "sum\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wird aus den M\u00fchlen dort und hier", "tokens": ["Wird", "aus", "den", "M\u00fch\u00b7len", "dort", "und", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Drucker t\u00e4glich zugef\u00fchret.", "tokens": ["Dem", "Dru\u00b7cker", "t\u00e4g\u00b7lich", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Pressen schwitzen Tag und Nacht;", "tokens": ["Die", "Pres\u00b7sen", "schwit\u00b7zen", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schwengel wird ganz lahm gemacht,", "tokens": ["Der", "Schwen\u00b7gel", "wird", "ganz", "lahm", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dieweil itzt jedermann durch Schriften", "tokens": ["Die\u00b7weil", "itzt", "je\u00b7der\u00b7mann", "durch", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich bey der Welt, wenn sein Gebein", "tokens": ["Sich", "bey", "der", "Welt", ",", "wenn", "sein", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mu\u00df Grab und W\u00fcrmern zinsbar seyn,", "tokens": ["Mu\u00df", "Grab", "und", "W\u00fcr\u00b7mern", "zins\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Will einen grossen Namen stiften.", "tokens": ["Will", "ei\u00b7nen", "gros\u00b7sen", "Na\u00b7men", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jedoch so viel sich Bogen auch", "tokens": ["Je\u00b7doch", "so", "viel", "sich", "Bo\u00b7gen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PRF", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Schriften angef\u00fcllet zeigen,", "tokens": ["Von", "Schrif\u00b7ten", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird doch der h\u00e4ufige Gebrauch", "tokens": ["Wird", "doch", "der", "h\u00e4u\u00b7fi\u00b7ge", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Karten jenen \u00fcbersteigen;", "tokens": ["Der", "Kar\u00b7ten", "je\u00b7nen", "\u00fc\u00b7bers\u00b7tei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was f\u00fcr Papier wird angeschafft,", "tokens": ["Was", "f\u00fcr", "Pa\u00b7pier", "wird", "an\u00b7ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das nur der Spielgott zu sich rafft!", "tokens": ["Das", "nur", "der", "Spiel\u00b7gott", "zu", "sich", "rafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Welch Buch, und schrieben es Sybillen,", "tokens": ["Welch", "Buch", ",", "und", "schrie\u00b7ben", "es", "Sy\u00b7bil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird wohl so \u00f6fters aufgelegt?", "tokens": ["Wird", "wohl", "so", "\u00f6f\u00b7ters", "auf\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Meister der die Bilder pr\u00e4gt,", "tokens": ["Der", "Meis\u00b7ter", "der", "die", "Bil\u00b7der", "pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kann kaum der K\u00e4ufer H\u00e4nde f\u00fcllen.", "tokens": ["Kann", "kaum", "der", "K\u00e4u\u00b7fer", "H\u00e4n\u00b7de", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zieht, bitt ich, jenen Vorhang weg,", "tokens": ["Zieht", ",", "bitt", "ich", ",", "je\u00b7nen", "Vor\u00b7hang", "weg", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sind die, die beysammen sitzen?", "tokens": ["Wer", "sind", "die", ",", "die", "bey\u00b7sam\u00b7men", "sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was thun sie da? was ist ihr Zweck?", "tokens": ["Was", "thun", "sie", "da", "?", "was", "ist", "ihr", "Zweck", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seht, wie sie vor Begierde schwitzen;", "tokens": ["Seht", ",", "wie", "sie", "vor", "Be\u00b7gier\u00b7de", "schwit\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ehrerbiethig sind sie nicht?", "tokens": ["Wie", "ehr\u00b7er\u00b7biet\u00b7hig", "sind", "sie", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier ist ein Altar aufgericht,", "tokens": ["Hier", "ist", "ein", "Al\u00b7tar", "auf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den diese fromme Schaar umringet;", "tokens": ["Den", "die\u00b7se", "from\u00b7me", "Schaar", "um\u00b7rin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schaut, wie ein jedes reichlich giebt,", "tokens": ["Schaut", ",", "wie", "ein", "je\u00b7des", "reich\u00b7lich", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und mehr auf selbigen hinschiebt,", "tokens": ["Und", "mehr", "auf", "sel\u00b7bi\u00b7gen", "hin\u00b7schiebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Als man sonst in den Tempel bringet.", "tokens": ["Als", "man", "sonst", "in", "den", "Tem\u00b7pel", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ist dieses nicht des G\u00f6tzen Hayn,", "tokens": ["Ist", "die\u00b7ses", "nicht", "des", "G\u00f6t\u00b7zen", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem man hier t\u00e4glich r\u00e4uchern siehet,", "tokens": ["Dem", "man", "hier", "t\u00e4g\u00b7lich", "r\u00e4u\u00b7chern", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den alle Welt sucht anzuschreyn,", "tokens": ["Den", "al\u00b7le", "Welt", "sucht", "an\u00b7zu\u00b7schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich um seine Gunst bem\u00fchet?", "tokens": ["Und", "sich", "um", "sei\u00b7ne", "Gunst", "be\u00b7m\u00fc\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, ja, 