{"textgrid.poem.53709": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wat Grotmudder vertellt", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausmarsch:", "tokens": ["Aus\u00b7marsch", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "\u00bbmanch einer aber ballte die Finger zur Faust und pre\u00dfte den Ha\u00df auf England durch seine Lippen.\u00ab", "tokens": ["\u00bb", "manch", "ei\u00b7ner", "a\u00b7ber", "ball\u00b7te", "die", "Fin\u00b7ger", "zur", "Faust", "und", "pre\u00df\u00b7te", "den", "Ha\u00df", "auf", "En\u00b7gland", "durch", "sei\u00b7ne", "Lip\u00b7pen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "PIS", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "APPR", "NE", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Fahrt (in Zivil) direkt zur Front. Suwalki.", "tokens": ["Fahrt", "(", "in", "Zi\u00b7vil", ")", "di\u00b7rekt", "zur", "Front", ".", "Su\u00b7wal\u00b7ki", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "APPR", "NE", "$(", "ADJD", "APPRART", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbbettelvolk empfing uns . . . Es bat um Brot.\u00ab", "tokens": ["\u00bb", "bet\u00b7tel\u00b7volk", "emp\u00b7fing", "uns", ".", ".", ".", "Es", "bat", "um", "Brot", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$.", "$.", "$.", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Durch wen wars dazu geworden?", "tokens": ["Durch", "wen", "wars", "da\u00b7zu", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VAFIN", "PAV", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwir k\u00e4mpften nicht um \u203adie\u2039 Kultur, sondern wir k\u00e4mpften um die deutsche Kultur.\u00ab", "tokens": ["\u00bb", "wir", "k\u00e4mpf\u00b7ten", "nicht", "um", "\u203a", "die", "\u2039", "Kul\u00b7tur", ",", "son\u00b7dern", "wir", "k\u00e4mpf\u00b7ten", "um", "die", "deut\u00b7sche", "Kul\u00b7tur", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPR", "$(", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+--+---+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Das d\u00fcrfte \u2013 soweit dieses Milit\u00e4r in Frage kommt \u2013 eine contradictio in adiecto sein.", "tokens": ["Das", "d\u00fcrf\u00b7te", "\u2013", "so\u00b7weit", "die\u00b7ses", "Mi\u00b7li\u00b7t\u00e4r", "in", "Fra\u00b7ge", "kommt", "\u2013", "ei\u00b7ne", "con\u00b7tra\u00b7dic\u00b7tio", "in", "a\u00b7diec\u00b7to", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$(", "APPR", "PDAT", "NN", "APPR", "NN", "VVFIN", "$(", "ART", "FM", "FM", "FM", "VAINF", "$."], "meter": "-+--+--+-+-+--+-+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Von Gefangenen:", "tokens": ["Von", "Ge\u00b7fan\u00b7ge\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Und dieses Kapitel schlie\u00dft:", "tokens": ["Und", "die\u00b7ses", "Ka\u00b7pi\u00b7tel", "schlie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbwir harren des ersten Schusses, der uns weckt.\u00ab", "tokens": ["\u00bb", "wir", "har\u00b7ren", "des", "ers\u00b7ten", "Schus\u00b7ses", ",", "der", "uns", "weckt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Harre, meine Seele \u2013!", "tokens": ["Har\u00b7re", ",", "mei\u00b7ne", "See\u00b7le", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}