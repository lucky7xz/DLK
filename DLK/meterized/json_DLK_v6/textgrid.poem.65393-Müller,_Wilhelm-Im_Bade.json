{"textgrid.poem.65393": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Im Bade", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaltes \u00fcber kaltes Wasser gie\u00df' ich in das Bad hinein:", "tokens": ["Kal\u00b7tes", "\u00fc\u00b7ber", "kal\u00b7tes", "Was\u00b7ser", "gie\u00df'", "ich", "in", "das", "Bad", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Es verdampft, wie eingesogen von der Wanne hei\u00dfem Stein;", "tokens": ["Es", "ver\u00b7dampft", ",", "wie", "ein\u00b7ge\u00b7so\u00b7gen", "von", "der", "Wan\u00b7ne", "hei\u00b7\u00dfem", "Stein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und er kann den Brand nicht stillen in der Fluth, die ihn umsp\u00fclt,", "tokens": ["Und", "er", "kann", "den", "Brand", "nicht", "stil\u00b7len", "in", "der", "Fluth", ",", "die", "ihn", "um\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Seit er einmal ihres Leibes vollen Flammendruck gef\u00fchlt.", "tokens": ["Seit", "er", "ein\u00b7mal", "ih\u00b7res", "Lei\u00b7bes", "vol\u00b7len", "Flam\u00b7men\u00b7druck", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ach, in diesem Feuersprudel soll ich baden meine Brust?", "tokens": ["Ach", ",", "in", "die\u00b7sem", "Feu\u00b7er\u00b7spru\u00b7del", "soll", "ich", "ba\u00b7den", "mei\u00b7ne", "Brust", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "K\u00fchlung such' ich in dem Wasser, und es gl\u00fcht von Liebeslust.", "tokens": ["K\u00fch\u00b7lung", "such'", "ich", "in", "dem", "Was\u00b7ser", ",", "und", "es", "gl\u00fcht", "von", "Lie\u00b7bes\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Herz, wo willst du hin dich retten? Werde Wasser, werde Stein,", "tokens": ["Herz", ",", "wo", "willst", "du", "hin", "dich", "ret\u00b7ten", "?", "Wer\u00b7de", "Was\u00b7ser", ",", "wer\u00b7de", "Stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$.", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auch im Stein und Wasser z\u00fcndet sie der Liebe s\u00fc\u00dfe Pein.", "tokens": ["Auch", "im", "Stein", "und", "Was\u00b7ser", "z\u00fcn\u00b7det", "sie", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}