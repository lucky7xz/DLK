{"textgrid.poem.35257": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich steige himmelauf, doch ohn' Geleit und Leiter,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steige himmelauf, doch ohn' Geleit und Leiter,", "tokens": ["Ich", "stei\u00b7ge", "him\u00b7me\u00b7lauf", ",", "doch", "ohn'", "Ge\u00b7leit", "und", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin der Kranken Arzt, der Armen Trostbereiter.", "tokens": ["Ich", "bin", "der", "Kran\u00b7ken", "Arzt", ",", "der", "Ar\u00b7men", "Trost\u00b7be\u00b7rei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der All's verloren hat, verlieret mich doch nicht;", "tokens": ["Der", "All's", "ver\u00b7lo\u00b7ren", "hat", ",", "ver\u00b7lie\u00b7ret", "mich", "doch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den S\u00fcnder s\u00f6hn' ich aus vor Gottes Strafgericht.", "tokens": ["Den", "S\u00fcn\u00b7der", "s\u00f6hn'", "ich", "aus", "vor", "Got\u00b7tes", "Straf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "A