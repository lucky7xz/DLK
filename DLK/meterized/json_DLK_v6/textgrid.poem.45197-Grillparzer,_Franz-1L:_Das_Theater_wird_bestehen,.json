{"textgrid.poem.45197": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Theater wird bestehen,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Theater wird bestehen,", "tokens": ["Das", "The\u00b7a\u00b7ter", "wird", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob Geschlechter auch vergehen;", "tokens": ["Ob", "Ge\u00b7schlech\u00b7ter", "auch", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht die Geister sterben aus,", "tokens": ["Nicht", "die", "Geis\u00b7ter", "ster\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leer doch steht der Geister Haus;", "tokens": ["Leer", "doch", "steht", "der", "Geis\u00b7ter", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn \u2013 Kom\u00f6die \u2013 ohne Ruhm", "tokens": ["Denn", "\u2013", "Ko\u00b7m\u00f6\u00b7die", "\u2013", "oh\u00b7ne", "Ruhm"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$(", "NN", "$(", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Spielt mit sich das Publikum.", "tokens": ["Spielt", "mit", "sich", "das", "Pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}