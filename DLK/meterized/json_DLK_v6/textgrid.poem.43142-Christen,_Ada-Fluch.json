{"textgrid.poem.43142": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Fluch", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Liebesfluch hat Euer Band gewoben,", "tokens": ["Ein", "Lie\u00b7bes\u00b7fluch", "hat", "Eu\u00b7er", "Band", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Reuethr\u00e4nen ist es hei\u00df benetzt;", "tokens": ["Mit", "Reu\u00b7et\u00b7hr\u00e4\u00b7nen", "ist", "es", "hei\u00df", "be\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob auch des Lebens St\u00fcrme trennend toben,", "tokens": ["Ob", "auch", "des", "Le\u00b7bens", "St\u00fcr\u00b7me", "tren\u00b7nend", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob Elend gei\u00dfelnd auseinanderhetzt,", "tokens": ["Ob", "E\u00b7lend", "gei\u00b7\u00df\u00b7elnd", "aus\u00b7ein\u00b7an\u00b7der\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ob Ihr verachtet oder gl\u00fcckgehoben,", "tokens": ["Ob", "Ihr", "ver\u00b7ach\u00b7tet", "o\u00b7der", "gl\u00fcck\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ob Zorn, ob Rachelust die Brust zerfetzt,", "tokens": ["Ob", "Zorn", ",", "ob", "Ra\u00b7che\u00b7lust", "die", "Brust", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ob Ihr zu hassen frevelnd m\u00f6gt geloben:", "tokens": ["Ob", "Ihr", "zu", "has\u00b7sen", "fre\u00b7velnd", "m\u00f6gt", "ge\u00b7lo\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVPP", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der alte Fluch besiegt Euch doch zuletzt.", "tokens": ["Der", "al\u00b7te", "Fluch", "be\u00b7siegt", "Euch", "doch", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}