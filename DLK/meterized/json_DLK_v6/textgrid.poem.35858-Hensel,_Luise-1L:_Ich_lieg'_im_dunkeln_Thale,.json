{"textgrid.poem.35858": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lieg' im dunkeln Thale,", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lieg' im dunkeln Thale,", "tokens": ["Ich", "lieg'", "im", "dun\u00b7keln", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So \u00f6d' und schauerlich,", "tokens": ["So", "\u00f6d'", "und", "schau\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehne nach dem Strahle", "tokens": ["Und", "seh\u00b7ne", "nach", "dem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des neuen Morgens mich.", "tokens": ["Des", "neu\u00b7en", "Mor\u00b7gens", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat mit Eis umgeben", "tokens": ["Es", "hat", "mit", "Eis", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Winter meine Brust;", "tokens": ["Der", "Win\u00b7ter", "mei\u00b7ne", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schwieg in mir das Leben,", "tokens": ["Es", "schwieg", "in", "mir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe reine Lust.", "tokens": ["Der", "Lie\u00b7be", "rei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nach Dem ich mich mu\u00df bangen,", "tokens": ["Nach", "Dem", "ich", "mich", "mu\u00df", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der einzig treu und rein:", "tokens": ["Der", "ein\u00b7zig", "treu", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann Ihn nicht erlangen", "tokens": ["Ich", "kann", "Ihn", "nicht", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kann nicht ohn' Ihn sein. \u2013", "tokens": ["Und", "kann", "nicht", "ohn'", "Ihn", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "PPER", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Weht hin, ihr milden L\u00fcfte,", "tokens": ["Weht", "hin", ",", "ihr", "mil\u00b7den", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Seiner Locken Zier", "tokens": ["Durch", "Sei\u00b7ner", "Lo\u00b7cken", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bringt der s\u00fc\u00dfen D\u00fcfte", "tokens": ["Und", "bringt", "der", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Seinen Bergen mir! \u2013", "tokens": ["Von", "Sei\u00b7nen", "Ber\u00b7gen", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich schau' empor, die H\u00fcgel,", "tokens": ["Ich", "schau'", "em\u00b7por", ",", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie stehn so eng' um mich;", "tokens": ["Sie", "stehn", "so", "eng'", "um", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, h\u00e4tt' ich Taubenfl\u00fcgel,", "tokens": ["O", ",", "h\u00e4tt'", "ich", "Tau\u00b7ben\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Freund! ich f\u00e4nde Dich. \u2013", "tokens": ["Mein", "Freund", "!", "ich", "f\u00e4n\u00b7de", "Dich", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wann kommst Du, mein Verlangen?", "tokens": ["Wann", "kommst", "Du", ",", "mein", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann k\u00fcsset mich Dein Mund?", "tokens": ["Wann", "k\u00fcs\u00b7set", "mich", "Dein", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann wird von stetem Bangen", "tokens": ["Wann", "wird", "von", "ste\u00b7tem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein sterbend Herz gesund?", "tokens": ["Mein", "ster\u00b7bend", "Herz", "ge\u00b7sund", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O, m\u00f6cht' ich Deine Spuren", "tokens": ["O", ",", "m\u00f6cht'", "ich", "Dei\u00b7ne", "Spu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dieser W\u00fcst' ersp\u00e4hn:", "tokens": ["In", "die\u00b7ser", "W\u00fcst'", "er\u00b7sp\u00e4hn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es w\u00fcrden bald die Fluren", "tokens": ["Es", "w\u00fcr\u00b7den", "bald", "die", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In hellen Bl\u00fcthen stehn.", "tokens": ["In", "hel\u00b7len", "Bl\u00fc\u00b7then", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O, d\u00fcrft' ich Licht und Wonne", "tokens": ["O", ",", "d\u00fcrft'", "ich", "Licht", "und", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Deinen Augen ziehn,", "tokens": ["Aus", "Dei\u00b7nen", "Au\u00b7gen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir brauchte keine Sonne", "tokens": ["Mir", "brauch\u00b7te", "kei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Himmel mehr zu gl\u00fchn! \u2013", "tokens": ["Am", "Him\u00b7mel", "mehr", "zu", "gl\u00fchn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ohn' Ihn ist Alles tr\u00fcbe,", "tokens": ["Ohn'", "Ihn", "ist", "Al\u00b7les", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz so krank und schwer,", "tokens": ["Das", "Herz", "so", "krank", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Trost und keine Liebe", "tokens": ["Kein", "Trost", "und", "kei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf weiter Erde mehr.", "tokens": ["Auf", "wei\u00b7ter", "Er\u00b7de", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O, da\u00df die Still' erbebte", "tokens": ["O", ",", "da\u00df", "die", "Still'", "er\u00b7beb\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Seinem s\u00fc\u00dfen Ruf!", "tokens": ["Von", "Sei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Ruf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, da\u00df Er mich belebte,", "tokens": ["O", ",", "da\u00df", "Er", "mich", "be\u00b7leb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der mir das Leben schuf!", "tokens": ["Der", "mir", "das", "Le\u00b7ben", "schuf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}