{"textgrid.poem.33684": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Bekenntnisse", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir oft davon erz\u00e4hlt,", "tokens": ["Du", "hast", "mir", "oft", "da\u00b7von", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie h\u00fcbsch du schon als Kind gewesen,", "tokens": ["Wie", "h\u00fcbsch", "du", "schon", "als", "Kind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "KOUS", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie b\u00f6se Krankheit dich gequ\u00e4lt,", "tokens": ["Wie", "b\u00f6\u00b7se", "Krank\u00b7heit", "dich", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie du langsam nur genesen.", "tokens": ["Und", "wie", "du", "lang\u00b7sam", "nur", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald kamen in dein Elternhaus", "tokens": ["Bald", "ka\u00b7men", "in", "dein", "El\u00b7tern\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Unzahl alt' und junge Freier,", "tokens": ["In", "Un\u00b7zahl", "alt'", "und", "jun\u00b7ge", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber schlugest jeden aus", "tokens": ["Du", "a\u00b7ber", "schlu\u00b7gest", "je\u00b7den", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PIAT", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nahmst sogar beinah den Schleier.", "tokens": ["Und", "nahmst", "so\u00b7gar", "bei\u00b7nah", "den", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht Einer konnte sich erfreun", "tokens": ["Nicht", "Ei\u00b7ner", "konn\u00b7te", "sich", "er\u00b7freun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch nur der kleinsten Gunstbezeigung.", "tokens": ["Auch", "nur", "der", "kleins\u00b7ten", "Gunst\u00b7be\u00b7zei\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wolltest nicht dein Herz zerstreun,", "tokens": ["Du", "woll\u00b7test", "nicht", "dein", "Herz", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du spieltest nicht mit deiner Neigung ...", "tokens": ["Du", "spiel\u00b7test", "nicht", "mit", "dei\u00b7ner", "Nei\u00b7gung", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6r' dich gern, ich horche stumm", "tokens": ["Ich", "h\u00f6r'", "dich", "gern", ",", "ich", "hor\u00b7che", "stumm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf deine holde Kindersage,", "tokens": ["Auf", "dei\u00b7ne", "hol\u00b7de", "Kin\u00b7der\u00b7sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie auf der Biene s\u00fc\u00df Gesumm", "tokens": ["Wie", "auf", "der", "Bie\u00b7ne", "s\u00fc\u00df", "Ge\u00b7summ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einem sch\u00f6nen Sommertage.", "tokens": ["An", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}