{"dta.poem.12361": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kukuk .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kukuk auf dem Birnbaum sa\u00df,               ", "tokens": ["Der", "Ku\u00b7kuk", "auf", "dem", "Birn\u00b7baum", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk, es mag schneien oder regnen, so wird er nicht na\u00df.", "tokens": ["Ku\u00b7kuk", ",", "es", "mag", "schnei\u00b7en", "o\u00b7der", "reg\u00b7nen", ",", "so", "wird", "er", "nicht", "na\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "VVINF", "KON", "ADJA", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Kukuk rief, wird na\u00df.", "tokens": ["Der", "Ku\u00b7kuk", "rief", ",", "wird", "na\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kukuk fliegt \u00fcbers Nachbar sein Haus,", "tokens": ["Der", "Ku\u00b7kuk", "fliegt", "\u00fc\u00b7bers", "Nach\u00b7bar", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kukuk, sch\u00f6n Sch\u00e4tzel, bist drinnen, komm zu mir heraus,", "tokens": ["Ku\u00b7kuk", ",", "sch\u00f6n", "Sch\u00e4t\u00b7zel", ",", "bist", "drin\u00b7nen", ",", "komm", "zu", "mir", "he\u00b7raus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "NN", "$,", "VAFIN", "ADV", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Kukuk, der Kukuk ist draus.", "tokens": ["Der", "Ku\u00b7kuk", ",", "der", "Ku\u00b7kuk", "ist", "draus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ich steh dir nicht auf und la\u00df dich nicht rein,", "tokens": ["Ich", "steh", "dir", "nicht", "auf", "und", "la\u00df", "dich", "nicht", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "KON", "VVIMP", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kukuk, du m\u00f6chst mir der rechte Kukuk nicht seyn,", "tokens": ["Ku\u00b7kuk", ",", "du", "m\u00f6chst", "mir", "der", "rech\u00b7te", "Ku\u00b7kuk", "nicht", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VAINF", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Kukuk, der Kukuk nicht seyn.", "tokens": ["Der", "Ku\u00b7kuk", ",", "der", "Ku\u00b7kuk", "nicht", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Der rechte Kukuk der bin ich ja schon,", "tokens": ["Der", "rech\u00b7te", "Ku\u00b7kuk", "der", "bin", "ich", "ja", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kukuk, bin ich doch meines Vaters sein einziger Sohn,", "tokens": ["Ku\u00b7kuk", ",", "bin", "ich", "doch", "mei\u00b7nes", "Va\u00b7ters", "sein", "ein\u00b7zi\u00b7ger", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Des Kukuk, des Kukuk sein Sohn.", "tokens": ["Des", "Ku\u00b7kuk", ",", "des", "Ku\u00b7kuk", "sein", "Sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Sein einziger Sohn der bin ich ja schon.", "tokens": ["Sein", "ein\u00b7zi\u00b7ger", "Sohn", "der", "bin", "ich", "ja", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kukuk, zieh nur beim Schn\u00fcrlein,", "tokens": ["Ku\u00b7kuk", ",", "zieh", "nur", "beim", "Schn\u00fcr\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Geh rein zum Th\u00fcrlein,", "tokens": ["Geh", "rein", "zum", "Th\u00fcr\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geh selber herein,", "tokens": ["Geh", "sel\u00b7ber", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Kukuk ist mein.", "tokens": ["Der", "Ku\u00b7kuk", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}