{"dta.poem.23185": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "265. Wer von der Liebe Gottes gebunde\u0303.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seel die nichts als Gott ged\u00e4nkt zu allen stunde\u0303/", "tokens": ["Die", "Seel", "die", "nichts", "als", "Gott", "ge\u00b7d\u00e4nkt", "zu", "al\u00b7len", "stun\u00b7d\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "KOKOM", "NN", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ist von seiner Lieb bestrikket und gebunden!", "tokens": ["Die", "ist", "von", "sei\u00b7ner", "Lieb", "be\u00b7strik\u00b7ket", "und", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}