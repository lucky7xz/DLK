{"textgrid.poem.44659": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ins Stammbuch eines weiblichen Badegastes zu Gastein, die oft \u00fcber meine \u00fcble Laune ungehalten war", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gastuna ist die sch\u00f6nste nicht", "tokens": ["Gas\u00b7tu\u00b7na", "ist", "die", "sch\u00f6ns\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "PTKNEG"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Aus der Najaden Schar,", "tokens": ["Aus", "der", "Na\u00b7ja\u00b7den", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar tr\u00fcb ist oft ihr Angesicht,", "tokens": ["Gar", "tr\u00fcb", "ist", "oft", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schilt und tobt und rei\u00dft und bricht,", "tokens": ["Sie", "schilt", "und", "tobt", "und", "rei\u00dft", "und", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch meint sies gut f\u00fcrwahr.", "tokens": ["Doch", "meint", "sies", "gut", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kannst du ob dem nun, ", "tokens": ["Kannst", "du", "ob", "dem", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "ART", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Verzeihen, ", "tokens": ["Ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So bist du dem, der dieses schrieb,", "tokens": ["So", "bist", "du", "dem", ",", "der", "die\u00b7ses", "schrieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob er gleich ernst und st\u00f6rrisch blieb,", "tokens": ["Ob", "er", "gleich", "ernst", "und", "st\u00f6r\u00b7risch", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch auch ein bi\u00dfchen gut.", "tokens": ["Doch", "auch", "ein", "bi\u00df\u00b7chen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}