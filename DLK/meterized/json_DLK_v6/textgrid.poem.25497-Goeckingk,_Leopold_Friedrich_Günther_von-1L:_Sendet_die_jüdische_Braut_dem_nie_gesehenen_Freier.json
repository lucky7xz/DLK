{"textgrid.poem.25497": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sendet die j\u00fcdische Braut dem nie gesehenen Freier", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sendet die j\u00fcdische Braut dem nie gesehenen Freier", "tokens": ["Sen\u00b7det", "die", "j\u00fc\u00b7di\u00b7sche", "Braut", "dem", "nie", "ge\u00b7se\u00b7he\u00b7nen", "Frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ihr versch\u00f6nertes Bild: b\u00fc\u00dfen einst wird sie daf\u00fcr!", "tokens": ["Ihr", "ver\u00b7sch\u00f6\u00b7ner\u00b7tes", "Bild", ":", "b\u00fc\u00b7\u00dfen", "einst", "wird", "sie", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVFIN", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "--+---+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "H\u00e4\u00dflicher als sie ist, wird seine Verlobte ihm scheinen,", "tokens": ["H\u00e4\u00df\u00b7li\u00b7cher", "als", "sie", "ist", ",", "wird", "sei\u00b7ne", "Ver\u00b7lob\u00b7te", "ihm", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wenn er das Original nun mit dem Bilde vergleicht.", "tokens": ["Wenn", "er", "das", "O\u00b7rig\u00b7i\u00b7nal", "nun", "mit", "dem", "Bil\u00b7de", "ver\u00b7gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dann beneidet er selbst die Sitte des Orientalen,", "tokens": ["Dann", "be\u00b7nei\u00b7det", "er", "selbst", "die", "Sit\u00b7te", "des", "O\u00b7rien\u00b7ta\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der auf gutes Gl\u00fcck zitternd entschleiert die Braut.", "tokens": ["Der", "auf", "gu\u00b7tes", "Gl\u00fcck", "zit\u00b7ternd", "ent\u00b7schlei\u00b7ert", "die", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "--+-+---+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Doch was k\u00fcmmert es viel den Maler, ob der Get\u00e4uschte", "tokens": ["Doch", "was", "k\u00fcm\u00b7mert", "es", "viel", "den", "Ma\u00b7ler", ",", "ob", "der", "Ge\u00b7t\u00e4uschte"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das betr\u00fcgende Bild und die Gemalte verw\u00fcnscht?", "tokens": ["Das", "be\u00b7tr\u00fc\u00b7gen\u00b7de", "Bild", "und", "die", "Ge\u00b7mal\u00b7te", "ver\u00b7w\u00fcnscht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Haben doch ihren Zweck sie beid' erreichet; vertrage", "tokens": ["Ha\u00b7ben", "doch", "ih\u00b7ren", "Zweck", "sie", "beid'", "er\u00b7rei\u00b7chet", ";", "ver\u00b7tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PPER", "PIS", "VVFIN", "$.", "VVFIN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Mit der Wirklichkeit sich jener, so gut er nur kann.", "tokens": ["Mit", "der", "Wirk\u00b7lich\u00b7keit", "sich", "je\u00b7ner", ",", "so", "gut", "er", "nur", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PDAT", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nein, Dorette! so soll dich kein Gem\u00e4lde der Landschaft,", "tokens": ["Nein", ",", "Do\u00b7ret\u00b7te", "!", "so", "soll", "dich", "kein", "Ge\u00b7m\u00e4l\u00b7de", "der", "Land\u00b7schaft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "ADV", "VMFIN", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Die mich jetzt umgibt, t\u00e4uschen, damit du nur kommst.", "tokens": ["Die", "mich", "jetzt", "um\u00b7gibt", ",", "t\u00e4u\u00b7schen", ",", "da\u00b7mit", "du", "nur", "kommst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+---+-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Die Natur gab hier dem Menschen gerade so viel nur,", "tokens": ["Die", "Na\u00b7tur", "gab", "hier", "dem", "Men\u00b7schen", "ge\u00b7ra\u00b7de", "so", "viel", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df er aus Hunger nicht stirbt, oder im Winter erfriert.", "tokens": ["Da\u00df", "er", "aus", "Hun\u00b7ger", "nicht", "stirbt", ",", "o\u00b7der", "im", "Win\u00b7ter", "er\u00b7friert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Warum sollte sie denn noch mehr ihm spenden? Ein Felsen", "tokens": ["Wa\u00b7rum", "soll\u00b7te", "sie", "denn", "noch", "mehr", "ihm", "spen\u00b7den", "?", "Ein", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "PPER", "VVINF", "$.", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Tr\u00e4get kein Korn; Ein See bringet sehr wenig nur ein;", "tokens": ["Tr\u00e4\u00b7get", "kein", "Korn", ";", "Ein", "See", "brin\u00b7get", "sehr", "we\u00b7nig", "nur", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Zwei Jahrhunderte braucht der Wald von Eichen zum Wachsthum';", "tokens": ["Zwei", "Jahr\u00b7hun\u00b7der\u00b7te", "braucht", "der", "Wald", "von", "Ei\u00b7chen", "zum", "Wachst\u00b7hum'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Berge? Ja die ersteigt ohne Beschwerde man nicht. \u2013", "tokens": ["Ber\u00b7ge", "?", "Ja", "die", "er\u00b7steigt", "oh\u00b7ne", "Be\u00b7schwer\u00b7de", "man", "nicht", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PTKANT", "ART", "VVFIN", "APPR", "NN", "PIS", "PTKNEG", "$.", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Von dem herrschenden Edelmann' an, bis dienenden Bauer,", "tokens": ["Von", "dem", "herr\u00b7schen\u00b7den", "E\u00b7del\u00b7mann'", "an", ",", "bis", "die\u00b7nen\u00b7den", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "W\u00fcnschet jeder sich nur Boden, um Weitzen zu baun.", "tokens": ["W\u00fcn\u00b7schet", "je\u00b7der", "sich", "nur", "Bo\u00b7den", ",", "um", "Weit\u00b7zen", "zu", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "NN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer die Reitze von dir, Natur! verachtet, den strafst du!", "tokens": ["Wer", "die", "Reit\u00b7ze", "von", "dir", ",", "Na\u00b7tur", "!", "ver\u00b7ach\u00b7tet", ",", "den", "strafst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPER", "$,", "NN", "$.", "VVFIN", "$,", "ART", "VVFIN", "PPER", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mag er auf sandiger Flur pfl\u00fcgen mit magerem Stier',", "tokens": ["Mag", "er", "auf", "san\u00b7di\u00b7ger", "Flur", "pfl\u00fc\u00b7gen", "mit", "ma\u00b7ge\u00b7rem", "Stier'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--++-++--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Seine Rinderherd' er weiden auf gelblichem Grase,", "tokens": ["Sei\u00b7ne", "Rin\u00b7der\u00b7herd'", "er", "wei\u00b7den", "auf", "gelb\u00b7li\u00b7chem", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Kaum mit L\u00f6wenzahn h\u00f6hnisch von Floren bestreut.", "tokens": ["Kaum", "mit", "L\u00f6\u00b7wen\u00b7zahn", "h\u00f6h\u00b7nisch", "von", "Flo\u00b7ren", "be\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Siehe! wie schleichet der Bach so stumm am nackenden Ufer!", "tokens": ["Sie\u00b7he", "!", "wie", "schlei\u00b7chet", "der", "Bach", "so", "stumm", "am", "na\u00b7cken\u00b7den", "U\u00b7fer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Keine Forelle, kein Schmerl springet und scherzet in ihm.", "tokens": ["Kei\u00b7ne", "Fo\u00b7rel\u00b7le", ",", "kein", "Schmerl", "sprin\u00b7get", "und", "scher\u00b7zet", "in", "ihm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Kein Vergi\u00dfmeinnicht verlanget sich in ihm zu spiegeln,", "tokens": ["Kein", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "ver\u00b7lan\u00b7get", "sich", "in", "ihm", "zu", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Die geborstene Weid' ist seine Freundin allein.", "tokens": ["Die", "ge\u00b7bors\u00b7te\u00b7ne", "Weid'", "ist", "sei\u00b7ne", "Freun\u00b7din", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wald genug siehst du umher, doch ach! von traurigen Kiefern,", "tokens": ["Wald", "ge\u00b7nug", "siehst", "du", "um\u00b7her", ",", "doch", "ach", "!", "von", "trau\u00b7ri\u00b7gen", "Kie\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADV", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Nur von Raupen besucht, oder noch h\u00f6chstens vom Specht'.", "tokens": ["Nur", "von", "Rau\u00b7pen", "be\u00b7sucht", ",", "o\u00b7der", "noch", "h\u00f6chs\u00b7tens", "vom", "Specht'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,", "KON", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Hier bringt Zephyr nicht die Zweige zu s\u00fc\u00dfem Gefl\u00fcster,", "tokens": ["Hier", "bringt", "Ze\u00b7phyr", "nicht", "die", "Zwei\u00b7ge", "zu", "s\u00fc\u00b7\u00dfem", "Ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und der Nachtigall Lied wurde hier nimmer geh\u00f6rt.", "tokens": ["Und", "der", "Nach\u00b7ti\u00b7gall", "Lied", "wur\u00b7de", "hier", "nim\u00b7mer", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Fluchend der dr\u00fcckenden Luft und brennenden Nadeln des Bodens,", "tokens": ["Flu\u00b7chend", "der", "dr\u00fc\u00b7cken\u00b7den", "Luft", "und", "bren\u00b7nen\u00b7den", "Na\u00b7deln", "des", "Bo\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Eilt mit verdoppeltem Schritt' lechzend der Pilger hindurch.", "tokens": ["Eilt", "mit", "ver\u00b7dop\u00b7pel\u00b7tem", "Schritt'", "lech\u00b7zend", "der", "Pil\u00b7ger", "hin\u00b7durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJD", "ART", "NN", "PAV", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Kann dein Auge nur sonst das Blenden des Saudes ertragen,", "tokens": ["Kann", "dein", "Au\u00b7ge", "nur", "sonst", "das", "Blen\u00b7den", "des", "Sau\u00b7des", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Nun, so siehe dort links H\u00fcgel mit Reben bepflanzt.", "tokens": ["Nun", ",", "so", "sie\u00b7he", "dort", "links", "H\u00fc\u00b7gel", "mit", "Re\u00b7ben", "be\u00b7pflanzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVIMP", "ADV", "ADV", "NN", "APPR", "NN", "VVPP", "$."], "meter": "----+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Aber merkest du wohl: sie lesen die kleinlichen Trauben", "tokens": ["A\u00b7ber", "mer\u00b7kest", "du", "wohl", ":", "sie", "le\u00b7sen", "die", "klein\u00b7li\u00b7chen", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Ohne Gesang und Klang. Sch\u00e4mt man sich ihrer vielleicht?", "tokens": ["Oh\u00b7ne", "Ge\u00b7sang", "und", "Klang", ".", "Sch\u00e4mt", "man", "sich", "ih\u00b7rer", "viel\u00b7leicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "VVFIN", "PIS", "PRF", "PPOSAT", "ADV", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Nur der Kr\u00e4mer sch\u00e4met sich nicht, den gewachsenen Essig", "tokens": ["Nur", "der", "Kr\u00e4\u00b7mer", "sch\u00e4\u00b7met", "sich", "nicht", ",", "den", "ge\u00b