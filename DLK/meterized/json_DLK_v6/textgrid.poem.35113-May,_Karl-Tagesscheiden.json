{"textgrid.poem.35113": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Tagesscheiden", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun gehst du hin in Frieden,", "tokens": ["Nun", "gehst", "du", "hin", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du sch\u00f6ner, goldner Tag.", "tokens": ["Du", "sch\u00f6\u00b7ner", ",", "gold\u00b7ner", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bist du von uns geschieden,", "tokens": ["Bist", "du", "von", "uns", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich doch nicht trauern mag.", "tokens": ["Ich", "doch", "nicht", "trau\u00b7ern", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Du kehrst doch morgen wieder;", "tokens": ["Du", "kehrst", "doch", "mor\u00b7gen", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht ewig w\u00e4hrt die Nacht;", "tokens": ["Nicht", "e\u00b7wig", "w\u00e4hrt", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann steigst du vom Himmel hernieder", "tokens": ["Dann", "steigst", "du", "vom", "Him\u00b7mel", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "In neuer uns segnender Pracht.", "tokens": ["In", "neu\u00b7er", "uns", "seg\u00b7nen\u00b7der", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "So werd auch ich in Frieden", "tokens": ["So", "werd", "auch", "ich", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von hinnen scheiden gehn;", "tokens": ["Von", "hin\u00b7nen", "schei\u00b7den", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es giebt doch schon hienieden", "tokens": ["Es", "giebt", "doch", "schon", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein geistig Auferstehn.", "tokens": ["Ein", "geis\u00b7tig", "Auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Firmament geschrieben", "tokens": ["Am", "Fir\u00b7ma\u00b7ment", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steht mein und euer Gl\u00fcck:", "tokens": ["Steht", "mein", "und", "eu\u00b7er", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als segnender Engel, ihr Lieben,", "tokens": ["Als", "seg\u00b7nen\u00b7der", "En\u00b7gel", ",", "ihr", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Kehr t\u00e4glich zu euch ich zur\u00fcck.", "tokens": ["Kehr", "t\u00e4g\u00b7lich", "zu", "euch", "ich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}