{"textgrid.poem.67751": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An K\u2013 \u2013's Erstgebornen", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Du auch seist, Du kleiner Ungeborner,", "tokens": ["Wer", "Du", "auch", "seist", ",", "Du", "klei\u00b7ner", "Un\u00b7ge\u00b7bor\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wachs' und bl\u00fche in der Eltern Brust", "tokens": ["So", "wachs'", "und", "bl\u00fc\u00b7he", "in", "der", "El\u00b7tern", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00fcpfe, wie Johannes, vor Lust;", "tokens": ["Und", "h\u00fcp\u00b7fe", ",", "wie", "Jo\u00b7han\u00b7nes", ",", "vor", "Lust", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Denn Du bist R\u2013 \u2013's Erstgeborner.", "tokens": ["Denn", "Du", "bist", "R", "\u2013", "\u2013", "'s", "Erst\u00b7ge\u00b7bor\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NE", "$(", "$(", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Vaters Geist, der Mutter Tugendblicke,", "tokens": ["Des", "Va\u00b7ters", "Geist", ",", "der", "Mut\u00b7ter", "Tu\u00b7gend\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie liegen schon im reichen Keim in Dir.", "tokens": ["Sie", "lie\u00b7gen", "schon", "im", "rei\u00b7chen", "Keim", "in", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O bitte, Welt, um ihn mit mir,", "tokens": ["O", "bit\u00b7te", ",", "Welt", ",", "um", "ihn", "mit", "mir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "KOUI", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gott ihn, Dir zum Heil, begl\u00fccke!", "tokens": ["Da\u00df", "Gott", "ihn", ",", "Dir", "zum", "Heil", ",", "be\u00b7gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "PPER", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Stimm, R\u2013 \u2013, zum Geburtsfest Orpheus' Saiten!", "tokens": ["Stimm", ",", "R", "\u2013", "\u2013", ",", "zum", "Ge\u00b7burts\u00b7fest", "Or\u00b7pheus'", "Sai\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "XY", "$(", "$(", "$,", "APPRART", "NN", "NE", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich seh', Dein Kind dr\u00e4ngt weinend sich ans Licht;", "tokens": ["Ich", "seh'", ",", "Dein", "Kind", "dr\u00e4ngt", "wei\u00b7nend", "sich", "ans", "Licht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du spielst, es h\u00f6rt und weinet nicht;", "tokens": ["Du", "spielst", ",", "es", "h\u00f6rt", "und", "wei\u00b7net", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter f\u00fchlt nicht Schmerz, nur Z\u00e4rtlichkeiten.", "tokens": ["Die", "Mut\u00b7ter", "f\u00fchlt", "nicht", "Schmerz", ",", "nur", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich seh's, es lallt selbst zum Klavier, zu spielen,", "tokens": ["Ich", "seh's", ",", "es", "lallt", "selbst", "zum", "Kla\u00b7vier", ",", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wie des Helden Kind mit Waffen spielt,", "tokens": ["So", "wie", "des", "Hel\u00b7den", "Kind", "mit", "Waf\u00b7fen", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und z\u00e4rtlich, wie die Mutter f\u00fchlt,", "tokens": ["Und", "z\u00e4rt\u00b7lich", ",", "wie", "die", "Mut\u00b7ter", "f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lehrt sie sein Herz die Tugend f\u00fchlen.", "tokens": ["Lehrt", "sie", "sein", "Herz", "die", "Tu\u00b7gend", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Lust, sein Kind ans Herz zu dr\u00fccken,", "tokens": ["O", "Lust", ",", "sein", "Kind", "ans", "Herz", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von s\u00fc\u00dfen Regungen sanft beseelt!", "tokens": ["Von", "s\u00fc\u00b7\u00dfen", "Re\u00b7gun\u00b7gen", "sanft", "be\u00b7seelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Welch Gl\u00fcck, als w\u00e4ren sie gew\u00e4hlt,", "tokens": ["Welch", "Gl\u00fcck", ",", "als", "w\u00e4\u00b7ren", "sie", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Eltern beste zu erblicken!", "tokens": ["Der", "El\u00b7tern", "bes\u00b7te", "zu", "er\u00b7bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}