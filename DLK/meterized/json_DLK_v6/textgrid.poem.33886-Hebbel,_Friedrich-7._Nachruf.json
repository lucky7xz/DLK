{"textgrid.poem.33886": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7. Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, die ungern mir voran gegangen,", "tokens": ["O", "du", ",", "die", "un\u00b7gern", "mir", "vo\u00b7ran", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst du wohl noch des Erdentraums gedenken?", "tokens": ["Wirst", "du", "wohl", "noch", "des", "Er\u00b7den\u00b7traums", "ge\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fchlst du wohl, den Flug zur\u00fcck zu lenken,", "tokens": ["Und", "f\u00fchlst", "du", "wohl", ",", "den", "Flug", "zu\u00b7r\u00fcck", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zuweilen noch ein fl\u00fcchtiges Verlangen?", "tokens": ["Zu\u00b7wei\u00b7len", "noch", "ein", "fl\u00fcch\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gewi\u00df! Du kennst ja meiner Seele Bangen,", "tokens": ["Ge\u00b7wi\u00df", "!", "Du", "kennst", "ja", "mei\u00b7ner", "See\u00b7le", "Ban\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst einen letzten Gru\u00df ihr gerne schenken,", "tokens": ["Wirst", "ei\u00b7nen", "letz\u00b7ten", "Gru\u00df", "ihr", "ger\u00b7ne", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann aber wirst du auf dein Grab dich senken,", "tokens": ["Dann", "a\u00b7ber", "wirst", "du", "auf", "dein", "Grab", "dich", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn die\u00df, du wei\u00dft es, h\u00e4lt mich stets gefangen.", "tokens": ["Denn", "die\u00df", ",", "du", "wei\u00dft", "es", ",", "h\u00e4lt", "mich", "stets", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wenn du nun in n\u00e4chtlich-heil'ger Stille", "tokens": ["Doch", "wenn", "du", "nun", "in", "n\u00e4cht\u00b7lich\u00b7heil'\u00b7ger", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hernieder schwebst, ein L\u00fcftchen deine H\u00fclle,", "tokens": ["Her\u00b7nie\u00b7der", "schwebst", ",", "ein", "L\u00fcft\u00b7chen", "dei\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was wird mir deine Gegenwart verk\u00fcnden?", "tokens": ["Was", "wird", "mir", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", "ver\u00b7k\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, dieses, da\u00df sich Gram und Wehmuth legen,", "tokens": ["Ach", ",", "die\u00b7ses", ",", "da\u00df", "sich", "Gram", "und", "Weh\u00b7muth", "le\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDAT", "$,", "KOUS", "PRF", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Funken sich von neuer Wonne regen,", "tokens": ["Da\u00df", "Fun\u00b7ken", "sich", "von", "neu\u00b7er", "Won\u00b7ne", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn deine N\u00e4he nur kann sie entz\u00fcnden.", "tokens": ["Denn", "dei\u00b7ne", "N\u00e4\u00b7he", "nur", "kann", "sie", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}