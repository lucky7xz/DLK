{"textgrid.poem.52316": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "Dat K\u00fcssen ut Leiw", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eins satt ick bi em ganz allein,", "tokens": ["Eins", "satt", "ick", "bi", "em", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un hei vertellt mi des' Geschicht:", "tokens": ["Un", "hei", "ver\u00b7tellt", "mi", "des'", "Ge\u00b7schicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsehn Sie die Stein, die gro\u00dfi Stein?", "tokens": ["\u00bb", "sehn", "Sie", "die", "Stein", ",", "die", "gro\u00b7\u00dfi", "Stein", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die gro\u00dfi Stein, die is mir dicht", "tokens": ["Die", "gro\u00b7\u00dfi", "Stein", ",", "die", "is", "mir", "dicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "FM", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An meini Kopf vorbei gesprung'.", "tokens": ["An", "mei\u00b7ni", "Kopf", "vor\u00b7bei", "ge\u00b7sprung'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sitz i hier in die Stub', da schmei\u00dft mir so'ne Jung'", "tokens": ["Sitz", "i", "hier", "in", "die", "Stub'", ",", "da", "schmei\u00dft", "mir", "so'\u00b7ne", "Jung'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bei helle Tag in Fenster rein.", "tokens": ["Bei", "hel\u00b7le", "Tag", "in", "Fens\u00b7ter", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ei Sackerment! I fix zu Bein", "tokens": ["Ei", "Sa\u00b7cker\u00b7ment", "!", "I", "fix", "zu", "Bein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "XY", "XY", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und auf die Stra\u00df' un krieg mir ein", "tokens": ["Und", "auf", "die", "Stra\u00df'", "un", "krieg", "mir", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "FM", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von die verdammti Jung' bei G'nick.", "tokens": ["Von", "die", "ver\u00b7damm\u00b7ti", "Jung'", "bei", "G'\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "'carnalli!' sag ick, 'hab ick dick?'", "tokens": ["'car\u00b7nal\u00b7li", "!", "'", "sag", "ick", ",", "'hab", "ick", "dick", "?", "'"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "$(", "VVIMP", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "'ne', sagt die Jung', 'i hab das nicht getut,", "tokens": ["'", "ne", "'", ",", "sagt", "die", "Jung'", ",", "'i", "hab", "das", "nicht", "ge\u00b7tut", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "ART", "NN", "$,", "NE", "VAFIN", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dat is die Schneider Jentzen ihr.'", "tokens": ["Dat", "is", "die", "Schnei\u00b7der", "Jent\u00b7zen", "ihr", ".", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["FM", "FM", "ART", "NE", "NE", "PPER", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "'wo is die sackermentsche Jung', die Tunichgut?'", "tokens": ["'", "wo", "is", "die", "sa\u00b7cker\u00b7ment\u00b7sche", "Jung", "'", ",", "die", "Tu\u00b7nich\u00b7gut", "?", "'"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PWAV", "FM", "ART", "ADJA", "NN", "$(", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Je, die verdammti Jung' war nich mehr hier.", "tokens": ["Je", ",", "die", "ver\u00b7damm\u00b7ti", "Jung'", "war", "nich", "mehr", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "I geh nu zu die Schneider Jentzen hin,", "tokens": ["I", "geh", "nu", "zu", "die", "Schnei\u00b7der", "Jent\u00b7zen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NE", "NE", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Un als i in die Stuben bin,", "tokens": ["Un", "als", "i", "in", "die", "Stu\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Da steht die Jung', die Galgenstrick,", "tokens": ["Da", "steht", "die", "Jung'", ",", "die", "Gal\u00b7gen\u00b7strick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un schreit denn gottserb\u00e4rmenlick.", "tokens": ["Un", "schreit", "denn", "gotts\u00b7er\u00b7b\u00e4r\u00b7men\u00b7lick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "'frau Schneider Jentzen, diese Stein',", "tokens": ["'", "frau", "Schnei\u00b7der", "Jent\u00b7zen", ",", "die\u00b7se", "Stein", "'", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NE", "NE", "$,", "PDAT", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sag i, 'die hat mir Ihre Jung'", "tokens": ["Sag", "i", ",", "'", "die", "hat", "mir", "Ih\u00b7re", "Jung", "'"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "$(", "PDS", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Geschmei\u00dfen in mein Fenster rein.'", "tokens": ["Ge\u00b7schmei\u00b7\u00dfen", "in", "mein", "Fens\u00b7ter", "rein", ".", "'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Un als i das gesagt, dunn schlung", "tokens": ["Un", "als", "i", "das", "ge\u00b7sagt", ",", "dunn", "schlung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOUS", "NE", "PDS", "VVPP", "$,", "ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.24": {"text": "Die Frau mir ihre Arm um meini Nacken", "tokens": ["Die", "Frau", "mir", "ih\u00b7re", "Arm", "um", "mei\u00b7ni", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Un k\u00fc\u00dfte mir auf beidi Backen.", "tokens": ["Un", "k\u00fc\u00df\u00b7te", "mir", "auf", "bei\u00b7di", "Ba\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und als sie so auf mir kommt los", "tokens": ["Und", "als", "sie", "so", "auf", "mir", "kommt", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und ihre Arm so um mi schlung,", "tokens": ["Und", "ih\u00b7re", "Arm", "so", "um", "mi", "schlung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Da denk i noch, sie ku\u00dft mi blo\u00df", "tokens": ["Da", "denk", "i", "noch", ",", "sie", "ku\u00dft", "mi", "blo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$,", "PPER", "VVFIN", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Vor die Erb\u00e4rmlichkeit von ihre Jung';", "tokens": ["Vor", "die", "Er\u00b7b\u00e4rm\u00b7lich\u00b7keit", "von", "ih\u00b7re", "Jung'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Doch als ich sie zur\u00fcck nu schieb,", "tokens": ["Doch", "als", "ich", "sie", "zu\u00b7r\u00fcck", "nu", "schieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Da kann i sehn, sie ku\u00dft mi vor die Lieb.\u00ab", "tokens": ["Da", "kann", "i", "sehn", ",", "sie", "ku\u00dft", "mi", "vor", "die", "Lieb", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "NE", "VVINF", "$,", "PPER", "VVFIN", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}