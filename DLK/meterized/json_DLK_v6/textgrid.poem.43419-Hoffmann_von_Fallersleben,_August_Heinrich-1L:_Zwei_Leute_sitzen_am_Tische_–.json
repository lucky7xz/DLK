{"textgrid.poem.43419": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Leute sitzen am Tische \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Leute sitzen am Tische \u2013", "tokens": ["Zwei", "Leu\u00b7te", "sit\u00b7zen", "am", "Ti\u00b7sche", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Abend ist so sch\u00f6n:", "tokens": ["Der", "A\u00b7bend", "ist", "so", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weht wie mit Thaues Frische", "tokens": ["Es", "weht", "wie", "mit", "Thau\u00b7es", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herab von des Berges H\u00f6hn.", "tokens": ["Her\u00b7ab", "von", "des", "Ber\u00b7ges", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Schon tr\u00e4umt nach des Tages Schw\u00fcle", "tokens": ["Schon", "tr\u00e4umt", "nach", "des", "Ta\u00b7ges", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Erde von k\u00fchler Lust.", "tokens": ["Die", "Er\u00b7de", "von", "k\u00fch\u00b7ler", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es erwachen die mildern Gef\u00fchle", "tokens": ["Es", "er\u00b7wa\u00b7chen", "die", "mil\u00b7dern", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "In jeder Menschenbrust.", "tokens": ["In", "je\u00b7der", "Men\u00b7schen\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Abendglocken erschallen,", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verk\u00fcnden Fried' und Ruh;", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "Fried'", "und", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fl\u00f6ten die Nachtigallen", "tokens": ["Es", "fl\u00f6\u00b7ten", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und die Blumen nicken dazu.", "tokens": ["Und", "die", "Blu\u00b7men", "ni\u00b7cken", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Die beiden sitzen und zanken", "tokens": ["Die", "bei\u00b7den", "sit\u00b7zen", "und", "zan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich \u00fcber einen Bericht.", "tokens": ["Sich", "\u00fc\u00b7ber", "ei\u00b7nen", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es fallen ihre Gedanken", "tokens": ["Es", "fal\u00b7len", "ih\u00b7re", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf etwas anderes nicht.", "tokens": ["Auf", "et\u00b7was", "an\u00b7de\u00b7res", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Da beginnen sie aufzustehen", "tokens": ["Da", "be\u00b7gin\u00b7nen", "sie", "auf\u00b7zu\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Als triebe sie Ein Gef\u00fchl:", "tokens": ["Als", "trie\u00b7be", "sie", "Ein", "Ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Herr College, wir wollen gehen!", "tokens": ["Herr", "Col\u00b7le\u00b7ge", ",", "wir", "wol\u00b7len", "ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird nach gerade k\u00fchl.", "tokens": ["Es", "wird", "nach", "ge\u00b7ra\u00b7de", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}