{"textgrid.poem.38559": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Abendschimmer wallt der Quell", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Abendschimmer wallt der Quell", "tokens": ["Im", "A\u00b7bend\u00b7schim\u00b7mer", "wallt", "der", "Quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Wiesenblumen purpurhell,", "tokens": ["Durch", "Wie\u00b7sen\u00b7blu\u00b7men", "pur\u00b7pur\u00b7hell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Pappelweide wechselnd Gr\u00fcn", "tokens": ["Der", "Pap\u00b7pel\u00b7wei\u00b7de", "wech\u00b7selnd", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weht ruhelispelnd dr\u00fcber hin.", "tokens": ["Weht", "ru\u00b7he\u00b7lis\u00b7pelnd", "dr\u00fc\u00b7ber", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Lenzhauch webt der Geist des Herr'n!", "tokens": ["Im", "Lenz\u00b7hauch", "webt", "der", "Geist", "des", "Herr'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh! Auferstehung nah und fern,", "tokens": ["Sieh", "!", "Auf\u00b7er\u00b7ste\u00b7hung", "nah", "und", "fern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh! Lebensodem, Sch\u00f6nheitsmeer", "tokens": ["Sieh", "!", "Le\u00b7ben\u00b7so\u00b7dem", ",", "Sch\u00f6n\u00b7heits\u00b7meer"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$.", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Jugendf\u00fclle ringsumher!", "tokens": ["Und", "Ju\u00b7gend\u00b7f\u00fcl\u00b7le", "rings\u00b7um\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich blikke her, ich blikke hin,", "tokens": ["Ich", "blik\u00b7ke", "her", ",", "ich", "blik\u00b7ke", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer h\u00f6her schwebt mein Sinn!", "tokens": ["Und", "im\u00b7mer", "h\u00f6\u00b7her", "schwebt", "mein", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Tand sind Gold und Pracht und Ruhm,", "tokens": ["O", "Tand", "sind", "Gold", "und", "Pracht", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Natur, in deinem Heiligthum!", "tokens": ["Na\u00b7tur", ",", "in", "dei\u00b7nem", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von dir gedr\u00fckt ans Mutterherz", "tokens": ["Von", "dir", "ge\u00b7dr\u00fckt", "ans", "Mut\u00b7ter\u00b7herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt sich die Seele sonnenw\u00e4rts!", "tokens": ["Hebt", "sich", "die", "See\u00b7le", "son\u00b7nen\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Himmels Ahndung den umweht,", "tokens": ["Des", "Him\u00b7mels", "Ahn\u00b7dung", "den", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der deinen Liebeston versteht!", "tokens": ["Der", "dei\u00b7nen", "Lie\u00b7be\u00b7ston", "ver\u00b7steht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}