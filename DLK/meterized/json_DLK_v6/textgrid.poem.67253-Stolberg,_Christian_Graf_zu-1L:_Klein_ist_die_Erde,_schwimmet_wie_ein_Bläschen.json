{"textgrid.poem.67253": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Klein ist die Erde, schwimmet wie ein Bl\u00e4schen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klein ist die Erde, schwimmet wie ein Bl\u00e4schen", "tokens": ["Klein", "ist", "die", "Er\u00b7de", ",", "schwim\u00b7met", "wie", "ein", "Bl\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf den Wogen des Welten-Oceanes,", "tokens": ["Auf", "den", "Wo\u00b7gen", "des", "Wel\u00b7ten\u00b7O\u00b7cea\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dessen kleinsten Busen der Mensch ersp\u00e4het:", "tokens": ["Des\u00b7sen", "kleins\u00b7ten", "Bu\u00b7sen", "der", "Mensch", "er\u00b7sp\u00e4\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Klein ist die Erde!", "tokens": ["Klein", "ist", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber der kleinen Erde Str\u00f6me trennen", "tokens": ["A\u00b7ber", "der", "klei\u00b7nen", "Er\u00b7de", "Str\u00f6\u00b7me", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von dem Freunde den Freund; sie sondern Br\u00fcder.", "tokens": ["Von", "dem", "Freun\u00b7de", "den", "Freund", ";", "sie", "son\u00b7dern", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "PPER", "KON", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Elb' und Weser sondern auch mich vom Bruder.", "tokens": ["Elb'", "und", "We\u00b7ser", "son\u00b7dern", "auch", "mich", "vom", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Gro\u00df ist die Erde!", "tokens": ["Gro\u00df", "ist", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Bl\u00fchet dereinst ein Strauch auf meinem Grabe,", "tokens": ["Bl\u00fc\u00b7het", "de\u00b7reinst", "ein", "Strauch", "auf", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weithin \u00fcber's Gebein des Todten schattend;", "tokens": ["Weit\u00b7hin", "\u00fc\u00b7ber's", "Ge\u00b7bein", "des", "Tod\u00b7ten", "schat\u00b7tend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dann wird mir des winzigen H\u00fcgels Erde", "tokens": ["Dann", "wird", "mir", "des", "win\u00b7zi\u00b7gen", "H\u00fc\u00b7gels", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Reichlich gen\u00fcgen!", "tokens": ["Reich\u00b7lich", "ge\u00b7n\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber des Welten-Oceanes Inseln", "tokens": ["A\u00b7ber", "des", "Wel\u00b7ten\u00b7O\u00b7cea\u00b7nes", "In\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wird der fliegende Geist dereinst ersp\u00e4hen,", "tokens": ["Wird", "der", "flie\u00b7gen\u00b7de", "Geist", "de\u00b7reinst", "er\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wird zu weiten Fl\u00fcgen auf kurze Zeiten", "tokens": ["Wird", "zu", "wei\u00b7ten", "Fl\u00fc\u00b7gen", "auf", "kur\u00b7ze", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "L\u00e4chelnd sich trennen.", "tokens": ["L\u00e4\u00b7chelnd", "sich", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}