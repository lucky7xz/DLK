{"textgrid.poem.46015": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am Charsamstag 1818", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum er mich verlassen,", "tokens": ["Wa\u00b7rum", "er", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00dft' ich zum Vater schrein,", "tokens": ["Mu\u00dft'", "ich", "zum", "Va\u00b7ter", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und du willst dich nicht fassen,", "tokens": ["Und", "du", "willst", "dich", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Willst niemals einsam sein.", "tokens": ["Willst", "nie\u00b7mals", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Siehst du denn nicht die Kerzen", "tokens": ["Siehst", "du", "denn", "nicht", "die", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An meinem Grabe hier,", "tokens": ["An", "mei\u00b7nem", "Gra\u00b7be", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was suchst du mich von Herzen,", "tokens": ["Was", "suchst", "du", "mich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und weinest vor der T\u00fcr?", "tokens": ["Und", "wei\u00b7nest", "vor", "der", "T\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Tritt ein du wirst mich finden,", "tokens": ["Tritt", "ein", "du", "wirst", "mich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So weit dein Glaube reicht,", "tokens": ["So", "weit", "dein", "Glau\u00b7be", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Bekenne deine S\u00fcnden,", "tokens": ["Be\u00b7ken\u00b7ne", "dei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So wird dein Hoffen leicht,", "tokens": ["So", "wird", "dein", "Hof\u00b7fen", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und wollen deine Augen", "tokens": ["Und", "wol\u00b7len", "dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mich liebend dann nicht sehn,", "tokens": ["Mich", "lie\u00b7bend", "dann", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Soll dir der Glaube taugen", "tokens": ["Soll", "dir", "der", "Glau\u00b7be", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Blind zu dem Tisch zu gehn.", "tokens": ["Blind", "zu", "dem", "Tisch", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Das ist die rechte Liebe,", "tokens": ["Das", "ist", "die", "rech\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die alles Dunkels lacht,", "tokens": ["Die", "al\u00b7les", "Dun\u00b7kels", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die die vorwitz'gen Triebe", "tokens": ["Die", "die", "vor\u00b7witz'\u00b7gen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehorsam glaubend macht", "tokens": ["Ge\u00b7hor\u00b7sam", "glau\u00b7bend", "macht"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann werden alle Sinnen", "tokens": ["Dann", "wer\u00b7den", "al\u00b7le", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In meinem Hiersein neu", "tokens": ["In", "mei\u00b7nem", "Hier\u00b7sein", "neu"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann denkt man nicht von hinnen", "tokens": ["Dann", "denkt", "man", "nicht", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf da\u00df man heilig sei.", "tokens": ["Auf", "da\u00df", "man", "hei\u00b7lig", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Will Glauben, Lieben, Loben", "tokens": ["Will", "Glau\u00b7ben", ",", "Lie\u00b7ben", ",", "Lo\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "NN", "$,", "ADJA", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Hoffen noch verstehn", "tokens": ["Und", "Hof\u00b7fen", "noch", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wollen sie nach oben", "tokens": ["So", "wol\u00b7len", "sie", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vorbei beim Heiland gehn.", "tokens": ["Vor\u00b7bei", "beim", "Hei\u00b7land", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du brauchst nicht so zu schreien", "tokens": ["Du", "brauchst", "nicht", "so", "zu", "schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die T\u00fcre schlie\u00df' ich nicht,", "tokens": ["Die", "T\u00fc\u00b7re", "schlie\u00df'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn tausend Teufel dr\u00e4uen,", "tokens": ["Wenn", "tau\u00b7send", "Teu\u00b7fel", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie l\u00f6schen mir kein Licht.", "tokens": ["Sie", "l\u00f6\u00b7schen", "mir", "kein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer will dich mir begraben,", "tokens": ["Wer", "will", "dich", "mir", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Braut, der ich verm\u00e4hlt,", "tokens": ["Die", "Braut", ",", "der", "ich", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der kannst du mich haben,", "tokens": ["Mit", "der", "kannst", "du", "mich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hast du mich recht erw\u00e4hlt,", "tokens": ["Hast", "du", "mich", "recht", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Kirche, die sie schm\u00e4hen,", "tokens": ["Die", "Kir\u00b7che", ",", "die", "sie", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie ist die Mutter dein,", "tokens": ["Sie", "ist", "die", "Mut\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie lehrt dich auferstehen", "tokens": ["Sie", "lehrt", "dich", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie lehrt dich selig sein.", "tokens": ["Sie", "lehrt", "dich", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}