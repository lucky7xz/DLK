{"textgrid.poem.60497": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Nie zur\u00fcck!", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Cherub aus dem Paradies", "tokens": ["Als", "der", "Che\u00b7rub", "aus", "dem", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn und seine Klagen streng verwies,", "tokens": ["Ihn", "und", "sei\u00b7ne", "Kla\u00b7gen", "streng", "ver\u00b7wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weinte Adam noch am Gartensaume", "tokens": ["Wein\u00b7te", "A\u00b7dam", "noch", "am", "Gar\u00b7ten\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Still zur\u00fcck nach seinem sch\u00f6nen Traume.", "tokens": ["Still", "zu\u00b7r\u00fcck", "nach", "sei\u00b7nem", "sch\u00f6\u00b7nen", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und durch einen weichen Morgenwind", "tokens": ["Und", "durch", "ei\u00b7nen", "wei\u00b7chen", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sandten Rosen ihm erbarmungslind", "tokens": ["Sand\u00b7ten", "Ro\u00b7sen", "ihm", "er\u00b7bar\u00b7mungs\u00b7lind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Duftend ihre s\u00fc\u00dfen Scheidek\u00fcsse,", "tokens": ["Duf\u00b7tend", "ih\u00b7re", "s\u00fc\u00b7\u00dfen", "Schei\u00b7de\u00b7k\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Paradiesesv\u00f6gel letzte Gr\u00fc\u00dfe.", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7v\u00f6\u00b7gel", "letz\u00b7te", "Gr\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie er trauernd an der Grenze stand,", "tokens": ["Wie", "er", "trau\u00b7ernd", "an", "der", "Gren\u00b7ze", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie er tief das \u203aNie zur\u00fcck!\u2039 empfand! \u2013", "tokens": ["Wie", "er", "tief", "das", "\u203a", "Nie", "zu\u00b7r\u00fcck", "!", "\u2039", "emp\u00b7fand", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "$(", "ADV", "PTKVZ", "$.", "$(", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als ich mu\u00dft auf immer von dir scheiden.", "tokens": ["Als", "ich", "mu\u00dft", "auf", "im\u00b7mer", "von", "dir", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mir auch ward zum milden Scheidegru\u00df", "tokens": ["Mir", "auch", "ward", "zum", "mil\u00b7den", "Schei\u00b7de\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Lippenrosen noch ein Ku\u00df,", "tokens": ["Dei\u00b7ner", "Lip\u00b7pen\u00b7ro\u00b7sen", "noch", "ein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und wie Edens V\u00f6gel ihn umsungen,", "tokens": ["Und", "wie", "E\u00b7dens", "V\u00f6\u00b7gel", "ihn", "um\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "NN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kam dein Lebewohl mir nachgeklungen.", "tokens": ["Kam", "dein", "Le\u00b7be\u00b7wohl", "mir", "nach\u00b7ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}