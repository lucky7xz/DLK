{"textgrid.poem.37531": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein Schneiderlein", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "Schnei\u00b7derl\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Nadel und mit Scher,", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der liebt ein M\u00e4del h\u00fcbsch und fein", "tokens": ["Der", "liebt", "ein", "M\u00e4\u00b7del", "h\u00fcbsch", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sehr, ach Gott, so sehr.", "tokens": ["So", "sehr", ",", "ach", "Gott", ",", "so", "sehr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "XY", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er kam zu ihr in sp\u00e4ter Stund", "tokens": ["Er", "kam", "zu", "ihr", "in", "sp\u00e4\u00b7ter", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und red't so hin und her,", "tokens": ["Und", "red't", "so", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob er ihr etwa helfen kunnt", "tokens": ["Ob", "er", "ihr", "et\u00b7wa", "hel\u00b7fen", "kunnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Nadel und mit Scher.", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da dreht das M\u00e4del sich herum:", "tokens": ["Da", "dreht", "das", "M\u00e4\u00b7del", "sich", "he\u00b7rum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo je, o jemine!", "tokens": ["\u00bb", "o", "je", ",", "o", "je\u00b7mi\u00b7ne", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADV", "$,", "FM", "FM", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Deine Nadel ist ja schon ganz krumm,", "tokens": ["Dei\u00b7ne", "Na\u00b7del", "ist", "ja", "schon", "ganz", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geh geh, mein Schneider, geh!\u00ab", "tokens": ["Geh", "geh", ",", "mein", "Schnei\u00b7der", ",", "geh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NE", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Schneider schrie: \u00bbDu falsche Dirn,", "tokens": ["Der", "Schnei\u00b7der", "schrie", ":", "\u00bb", "Du", "fal\u00b7sche", "Dirn", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$.", "$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4tt ich dich nie gekannt!\u00ab", "tokens": ["H\u00e4tt", "ich", "dich", "nie", "ge\u00b7kannt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er kauft sich einen Faden Zwirn", "tokens": ["Er", "kauft", "sich", "ei\u00b7nen", "Fa\u00b7den", "Zwirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4ngt sich an die Wand.", "tokens": ["Und", "h\u00e4ngt", "sich", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}