{"textgrid.poem.24534": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Alter hat die Ehre.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Alter hat die Ehre.", "tokens": ["Das", "Al\u00b7ter", "hat", "die", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum gib dein Hertz der Jugend nicht so bald/", "tokens": ["Drum", "gib", "dein", "Hertz", "der", "Ju\u00b7gend", "nicht", "so", "bald", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVIMP", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bedencke doch/ du wirst auch einsten alt.", "tokens": ["Be\u00b7den\u00b7cke", "doch", "/", "du", "wirst", "auch", "eins\u00b7ten", "alt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man vor dich denn unempfindlich w\u00e4re:", "tokens": ["Wenn", "man", "vor", "dich", "denn", "un\u00b7emp\u00b7find\u00b7lich", "w\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So w\u00fcrdest du zu deiner Pein/", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "zu", "dei\u00b7ner", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch ohne Trost/ und ohn Erbarmen seyn.", "tokens": ["Auch", "oh\u00b7ne", "Trost", "/", "und", "ohn", "Er\u00b7bar\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$(", "KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}