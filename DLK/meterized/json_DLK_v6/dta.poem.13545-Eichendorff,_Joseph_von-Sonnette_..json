{"dta.poem.13545": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O Herr! gib Demuth denen, die da irren,", "tokens": ["O", "Herr", "!", "gib", "De\u00b7muth", "de\u00b7nen", ",", "die", "da", "ir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "NN", "PDS", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df, wenn ihr' K\u00fcnste all' zu Schanden werden,", "tokens": ["Da\u00df", ",", "wenn", "ih\u00b7r'", "K\u00fcns\u00b7te", "all'", "zu", "Schan\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPOSAT", "NN", "PIS", "APPR", "NN", "VAINF", "$,"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Sie th\u00f6richt nicht den Gott in sich verfluchen!", "tokens": ["Sie", "th\u00f6\u00b7richt", "nicht", "den", "Gott", "in", "sich", "ver\u00b7flu\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}