{"dta.poem.10905": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem rauhen Fichten-Wald, der sehr verwildert ist, doch\nsch\u00f6n,", "tokens": ["In", "ei\u00b7nem", "rau\u00b7hen", "Fich\u00b7ten\u00b7Wald", ",", "der", "sehr", "ver\u00b7wil\u00b7dert", "ist", ",", "doch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Seh ich hier einen edlen Hirsch, so, wie es scheint, voll Unmuth", "tokens": ["Seh", "ich", "hier", "ei\u00b7nen", "ed\u00b7len", "Hirsch", ",", "so", ",", "wie", "es", "scheint", ",", "voll", "Un\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Er scharrt begierig in den Sand, ich h\u00f6r ihn fast f\u00fcr Eifer", "tokens": ["Er", "scharrt", "be\u00b7gie\u00b7rig", "in", "den", "Sand", ",", "ich", "h\u00f6r", "ihn", "fast", "f\u00fcr", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Sein scharf Geweih durchf\u00e4hrt und st\u00f6rt den ihm gesunden", "tokens": ["Sein", "scharf", "Ge\u00b7weih", "durch\u00b7f\u00e4hrt", "und", "st\u00f6rt", "den", "ihm", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "KON", "VVFIN", "ART", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man kann, in seinem strengen Blick, so gar die Leidenschaften", "tokens": ["Man", "kann", ",", "in", "sei\u00b7nem", "stren\u00b7gen", "Blick", ",", "so", "gar", "die", "Lei\u00b7den\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Erwege, der du dieses siehest, wie weit die Wissenschaften gehn,", "tokens": ["Er\u00b7we\u00b7ge", ",", "der", "du", "die\u00b7ses", "sie\u00b7hest", ",", "wie", "weit", "die", "Wis\u00b7sen\u00b7schaf\u00b7ten", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PDS", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Wie sich die Kunst so fern erstrecket! So wie, durch schwarze", "tokens": ["Wie", "sich", "die", "Kunst", "so", "fern", "er\u00b7stre\u00b7cket", "!", "So", "wie", ",", "durch", "schwar\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "ADJD", "VVFIN", "$.", "ADV", "KOKOM", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Unsichtbare Gedanken bilden, und Geister fast verk\u00f6rpern kann,", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "Ge\u00b7dan\u00b7ken", "bil\u00b7den", ",", "und", "Geis\u00b7ter", "fast", "ver\u00b7k\u00f6r\u00b7pern", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "KON", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Das ja gewi\u00df ein Wunder ist: So stellt, durch schwarze Far-", "tokens": ["Das", "ja", "ge\u00b7wi\u00df", "ein", "Wun\u00b7der", "ist", ":", "So", "stellt", ",", "durch", "schwar\u00b7ze", "Fa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Des K\u00fcnstlers Hand, auf diesem Blatt, die K\u00f6rper fast be-", "tokens": ["Des", "K\u00fcnst\u00b7lers", "Hand", ",", "auf", "die\u00b7sem", "Blatt", ",", "die", "K\u00f6r\u00b7per", "fast", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PDAT", "NN", "$,", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit wie viel F\u00e4higkeiten nun, und Kr\u00e4ften unsrer Geister", "tokens": ["Mit", "wie", "viel", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", "nun", ",", "und", "Kr\u00e4f\u00b7ten", "uns\u00b7rer", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "ADV", "$,", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Von einem h\u00f6hern Geist begabt, giebt diese Bilder - Schrift", "tokens": ["Von", "ei\u00b7nem", "h\u00f6\u00b7hern", "Geist", "be\u00b7gabt", ",", "giebt", "die\u00b7se", "Bil\u00b7der", "Schrift"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,", "VVFIN", "PDAT", "NN", "$(", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Man kann sich, wenn wir die\u00df Pappier, als der Natur Copie,", "tokens": ["Man", "kann", "sich", ",", "wenn", "wir", "die\u00df", "Pap\u00b7pier", ",", "als", "der", "Na\u00b7tur", "Co\u00b7pie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "$,", "KOUS", "PPER", "PDS", "NN", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Des Urbilds Sch\u00f6pfer selbst zum Ruhm, daran ergetzen und", "tokens": ["Des", "Ur\u00b7bilds", "Sch\u00f6p\u00b7fer", "selbst", "zum", "Ruhm", ",", "da\u00b7ran", "er\u00b7get\u00b7zen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "APPRART", "NN", "$,", "PAV", "VVINF", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}