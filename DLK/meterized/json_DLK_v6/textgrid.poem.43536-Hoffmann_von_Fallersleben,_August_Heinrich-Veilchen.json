{"textgrid.poem.43536": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Veilchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veilchen, wie so schweigend,", "tokens": ["Veil\u00b7chen", ",", "wie", "so", "schwei\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie so still dich neigend", "tokens": ["Wie", "so", "still", "dich", "nei\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In das gr\u00fcne Moos!", "tokens": ["In", "das", "gr\u00fc\u00b7ne", "Moos", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Veilchen, sag was sinnst du,", "tokens": ["Veil\u00b7chen", ",", "sag", "was", "sinnst", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PIS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sag mir, was beginnst du,", "tokens": ["Sag", "mir", ",", "was", "be\u00b7ginnst", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Scheinst so freudenlos?", "tokens": ["Scheinst", "so", "freu\u00b7den\u00b7los", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbla\u00df mich! still und bange", "tokens": ["\u00bb", "la\u00df", "mich", "!", "still", "und", "ban\u00b7ge"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$.", "ADJD", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lausch' ich dem Gesange", "tokens": ["Lausch'", "ich", "dem", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jener Nachtigall.", "tokens": ["Je\u00b7ner", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn sie singt, so schweig' ich,", "tokens": ["Wenn", "sie", "singt", ",", "so", "schweig'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn sie singt, so neig' ich", "tokens": ["Wenn", "sie", "singt", ",", "so", "neig'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ihrem Sang und Schall.\u00ab", "tokens": ["Ih\u00b7rem", "Sang", "und", "Schall", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}