{"dta.poem.19168": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Unschuld liebt im Thier Mensch\u00e4hnliches zu sehn,", "tokens": ["Die", "Un\u00b7schuld", "liebt", "im", "Thier", "Men\u00b7sch\u00e4hn\u00b7li\u00b7ches", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bosheit im Menschen Thierverwandtes zu ersp\u00e4hn.", "tokens": ["Bos\u00b7heit", "im", "Men\u00b7schen", "Thier\u00b7ver\u00b7wand\u00b7tes", "zu", "er\u00b7sp\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Und leicht ist eines auch aufs andre auszulegen,", "tokens": ["Und", "leicht", "ist", "ei\u00b7nes", "auch", "aufs", "and\u00b7re", "aus\u00b7zu\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADV", "APPRART", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil beides \u00fcberall in beidem ist zugegen,", "tokens": ["Weil", "bei\u00b7des", "\u00fc\u00b7be\u00b7rall", "in", "bei\u00b7dem", "ist", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da das geringste Thier schon auf den Menschen deutet,", "tokens": ["Da", "das", "ge\u00b7rings\u00b7te", "Thier", "schon", "auf", "den", "Men\u00b7schen", "deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selten sich ein Mensch hat ganz vom Thier geh\u00e4utet.", "tokens": ["Und", "sel\u00b7ten", "sich", "ein", "Mensch", "hat", "ganz", "vom", "Thier", "ge\u00b7h\u00e4u\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus jedem Thiere guckt ein St\u00fcckchen Mensch hervor,", "tokens": ["Aus", "je\u00b7dem", "Thie\u00b7re", "guckt", "ein", "St\u00fcck\u00b7chen", "Mensch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeden Menschen zupft die Thierheit noch am Ohr.", "tokens": ["Und", "je\u00b7den", "Men\u00b7schen", "zupft", "die", "Thier\u00b7heit", "noch", "am", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn Scharfsinn und Verstand nun liebet Unterscheidung,", "tokens": ["Wenn", "Scharf\u00b7sinn", "und", "Ver\u00b7stand", "nun", "lie\u00b7bet", "Un\u00b7ter\u00b7schei\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So liebt dagegen Witz und Fantasie Verkleidung.", "tokens": ["So", "liebt", "da\u00b7ge\u00b7gen", "Witz", "und", "Fan\u00b7ta\u00b7sie", "Ver\u00b7klei\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch edler als die Luft an der Karrikatur", "tokens": ["Doch", "ed\u00b7ler", "als", "die", "Luft", "an", "der", "Kar\u00b7ri\u00b7ka\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist harmlos spielende Begeistrung der Natur,", "tokens": ["Ist", "harm\u00b7los", "spie\u00b7len\u00b7de", "Be\u00b7geis\u00b7trung", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die lieber Niederes um eine Stufe r\u00fcckt", "tokens": ["Die", "lie\u00b7ber", "Nie\u00b7de\u00b7res", "um", "ei\u00b7ne", "Stu\u00b7fe", "r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herauf, als H\u00f6heres hinab um eine dr\u00fcckt;", "tokens": ["Her\u00b7auf", ",", "als", "H\u00f6\u00b7he\u00b7res", "hin\u00b7ab", "um", "ei\u00b7ne", "dr\u00fcckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KOUS", "NN", "ADV", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Kinderm\u00e4rchenwelt tiefsinnige Betrachtung,", "tokens": ["Der", "Kin\u00b7der\u00b7m\u00e4r\u00b7chen\u00b7welt", "tief\u00b7sin\u00b7ni\u00b7ge", "Be\u00b7trach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und des Brahmanen draus entsprungne Thierweltachtung.", "tokens": ["Und", "des", "Brah\u00b7ma\u00b7nen", "draus", "ent\u00b7sprung\u00b7ne", "Thier\u00b7wel\u00b7tach\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}