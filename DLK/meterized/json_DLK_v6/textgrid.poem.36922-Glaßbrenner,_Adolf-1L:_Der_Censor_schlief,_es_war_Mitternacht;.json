{"textgrid.poem.36922": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Censor schlief, es war Mitternacht;", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Censor schlief, es war Mitternacht;", "tokens": ["Der", "Cen\u00b7sor", "schlief", ",", "es", "war", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da regt sich's in seinen Schranken;", "tokens": ["Da", "regt", "sich's", "in", "sei\u00b7nen", "Schran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da standen die bleichen Geister auf,", "tokens": ["Da", "stan\u00b7den", "die", "blei\u00b7chen", "Geis\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ermordeten Gedanken.", "tokens": ["Die", "er\u00b7mor\u00b7de\u00b7ten", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie seufzten tief, sie seufzten schwer;", "tokens": ["Sie", "seufz\u00b7ten", "tief", ",", "sie", "seufz\u00b7ten", "schwer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wankten und schwankten hin und her,", "tokens": ["Sie", "wank\u00b7ten", "und", "schwank\u00b7ten", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und: wehe! wehe! wehe!", "tokens": ["Und", ":", "we\u00b7he", "!", "we\u00b7he", "!", "we\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$.", "ADJD", "$.", "ADJD", "$.", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erscholl's in des M\u00f6rder's N\u00e4he.", "tokens": ["Er\u00b7scholl's", "in", "des", "M\u00f6r\u00b7der's", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbich hatte das arme Volk zu lieb!\u00ab", "tokens": ["\u00bb", "ich", "hat\u00b7te", "das", "ar\u00b7me", "Volk", "zu", "lieb", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "PTKA", "ADJD", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhub der Eine die Stimme.", "tokens": ["Er\u00b7hub", "der", "Ei\u00b7ne", "die", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbich forderte das versprochene Gl\u00fcck", "tokens": ["\u00bb", "ich", "for\u00b7der\u00b7te", "das", "ver\u00b7spro\u00b7che\u00b7ne", "Gl\u00fcck"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit schlecht verbi\u00dfenem Grimme.\u00ab", "tokens": ["Mit", "schlecht", "ver\u00b7bi\u00b7\u00dfe\u00b7nem", "Grim\u00b7me", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Dritte sprach: \u00bbIch war munteres Blut,", "tokens": ["Der", "Drit\u00b7te", "sprach", ":", "\u00bb", "Ich", "war", "mun\u00b7te\u00b7res", "Blut", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich verwechselte ein Mal Scepter und Knut'!\u00ab", "tokens": ["Ich", "ver\u00b7wech\u00b7sel\u00b7te", "ein", "Mal", "Scep\u00b7ter", "und", "Knut'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der Vierte: \u00bbIch war ein Tadel", "tokens": ["Der", "Vier\u00b7te", ":", "\u00bb", "Ich", "war", "ein", "Ta\u00b7del"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VAFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gegen den l\u00e4stigen Adel.\u00ab", "tokens": ["Ge\u00b7gen", "den", "l\u00e4s\u00b7ti\u00b7gen", "A\u00b7del", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbich forderte keck das freie Wort!\u00ab", "tokens": ["\u00bb", "ich", "for\u00b7der\u00b7te", "keck", "das", "frei\u00b7e", "Wort", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbund ich die Gleichheit der Rechte.\u00ab", "tokens": ["\u00bb", "und", "ich", "die", "Gleich\u00b7heit", "der", "Rech\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbich sagte: die F\u00fcrsten geh\u00f6rten dem Volk:\u00ab", "tokens": ["\u00bb", "ich", "sag\u00b7te", ":", "die", "F\u00fcrs\u00b7ten", "ge\u00b7h\u00f6r\u00b7ten", "dem", "Volk", ":", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbund ich: wir w\u00e4ren keine Knechte!\u00ab", "tokens": ["\u00bb", "und", "ich", ":", "wir", "w\u00e4\u00b7ren", "kei\u00b7ne", "Knech\u00b7te", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "$.", "PPER", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbich h\u00f6hnte die traurige Petition.\u00ab", "tokens": ["\u00bb", "ich", "h\u00f6hn\u00b7te", "die", "trau\u00b7ri\u00b7ge", "Pe\u00b7ti\u00b7ti\u00b7on", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbich aber rief: habt ihr vergessen schon?", "tokens": ["\u00bb", "ich", "a\u00b7ber", "rief", ":", "habt", "ihr", "ver\u00b7ges\u00b7sen", "schon", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVFIN", "$.", "VAFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Unterdr\u00fcckt, verbietet nur flei\u00dfig:", "tokens": ["Un\u00b7ter\u00b7dr\u00fcckt", ",", "ver\u00b7bie\u00b7tet", "nur", "flei\u00b7\u00dfig", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Ein Tausend Acht hundert und Drei\u00dfig!\u00ab", "tokens": ["Ein", "Tau\u00b7send", "Acht", "hun\u00b7dert", "und", "Drei\u00b7\u00dfig", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "CARD", "CARD", "KON", "CARD", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "So sprachen sie alle in finsterm Groll,", "tokens": ["So", "spra\u00b7chen", "sie", "al\u00b7le", "in", "fins\u00b7term", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schwuren Rache zum Himmel;", "tokens": ["Und", "schwu\u00b7ren", "Ra\u00b7che", "zum", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drauf wirrt's und schwirrt's um des Schl\u00e4fers Kopf", "tokens": ["Drauf", "wirrt's", "und", "schwirrt's", "um", "des", "Schl\u00e4\u00b7fers", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das b\u00f6se Geister-Gewimmel.", "tokens": ["Das", "b\u00f6\u00b7se", "Geis\u00b7ter\u00b7Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie krochen durch Nase, durch Ohr und Mund;", "tokens": ["Sie", "kro\u00b7chen", "durch", "Na\u00b7se", ",", "durch", "Ohr", "und", "Mund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie ri\u00dfen am Haar ihn, sie stopften den Schlund,", "tokens": ["Sie", "ri\u00b7\u00dfen", "am", "Haar", "ihn", ",", "sie", "stopf\u00b7ten", "den", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Sie tobten auf seiner Stirne,", "tokens": ["Sie", "tob\u00b7ten", "auf", "sei\u00b7ner", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sie schrieen in seinem Gehirne.", "tokens": ["Sie", "schri\u00b7een", "in", "sei\u00b7nem", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Fr\u00fch Morgens wurde dem Censor verliehn", "tokens": ["Fr\u00fch", "Mor\u00b7gens", "wur\u00b7de", "dem", "Cen\u00b7sor", "ver\u00b7liehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ein gro\u00dfer, langer Orden;", "tokens": ["Ein", "gro\u00b7\u00dfer", ",", "lan\u00b7ger", "Or\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er aber sah stier auf das bunte Band,", "tokens": ["Er", "a\u00b7ber", "sah", "stier", "auf", "das", "bun\u00b7te", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn er war wahnsinnig worden. \u2013", "tokens": ["Denn", "er", "war", "wahn\u00b7sin\u00b7nig", "wor\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VAPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An jenem Schrank', in der Nacht darauf,", "tokens": ["An", "je\u00b7nem", "Schrank'", ",", "in", "der", "Nacht", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hing er mit dem Ordensbande sich auf,", "tokens": ["Hing", "er", "mit", "dem", "Or\u00b7dens\u00b7ban\u00b7de", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Und drau\u00dfen h\u00f6rte der W\u00e4chter", "tokens": ["Und", "drau\u00b7\u00dfen", "h\u00f6r\u00b7te", "der", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ein f\u00fcrchterliches Gel\u00e4chter.", "tokens": ["Ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}