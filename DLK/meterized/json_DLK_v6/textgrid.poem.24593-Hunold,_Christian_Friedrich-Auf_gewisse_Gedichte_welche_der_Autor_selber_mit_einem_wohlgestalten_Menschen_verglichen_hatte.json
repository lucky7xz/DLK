{"textgrid.poem.24593": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf gewisse Gedichte/ welche der Autor selber mit einem wohlgestalten Menschen verglichen hatte", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Leib ist gar geschickt/ und alles wohl erfunden/", "tokens": ["Der", "Leib", "ist", "gar", "ge\u00b7schickt", "/", "und", "al\u00b7les", "wohl", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "KON", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was zur Annehmlichkeit der Bildung dienen kan.", "tokens": ["Was", "zur", "An\u00b7nehm\u00b7lich\u00b7keit", "der", "Bil\u00b7dung", "die\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Auch stehn die Kleider gut. Er hat in zweyen Stunden/", "tokens": ["Auch", "stehn", "die", "Klei\u00b7der", "gut", ".", "Er", "hat", "in", "zwe\u00b7yen", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf zwanzig Meilen weit schon seinen Weg gethan/", "tokens": ["Auf", "zwan\u00b7zig", "Mei\u00b7len", "weit", "schon", "sei\u00b7nen", "Weg", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So eilig l\u00e4uft der Kerl. Nur schade/ da\u00df darneben", "tokens": ["So", "ei\u00b7lig", "l\u00e4uft", "der", "Kerl", ".", "Nur", "scha\u00b7de", "/", "da\u00df", "dar\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "$(", "KOUS", "PAV"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der Seelen die Gestalt vom Teufel ist gegeben.", "tokens": ["Der", "See\u00b7len", "die", "Ge\u00b7stalt", "vom", "Teu\u00b7fel", "ist", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}