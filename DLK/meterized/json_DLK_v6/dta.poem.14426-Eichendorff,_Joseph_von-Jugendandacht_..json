{"dta.poem.14426": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Umfangen f\u00fchl' ich innigst mich erwarmen,", "tokens": ["Um\u00b7fan\u00b7gen", "f\u00fchl'", "ich", "in\u00b7nigst", "mich", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ber\u00fchrt mit gold'nen Strahlen mich das Leben,", "tokens": ["Be\u00b7r\u00fchrt", "mit", "gold'\u00b7nen", "Strah\u00b7len", "mich", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! da\u00df ich ewig Dir am Herzen bliebe!", "tokens": ["Ach", "!", "da\u00df", "ich", "e\u00b7wig", "Dir", "am", "Her\u00b7zen", "blie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}