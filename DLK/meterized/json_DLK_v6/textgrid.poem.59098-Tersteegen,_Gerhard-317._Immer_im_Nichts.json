{"textgrid.poem.59098": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "317. Immer im Nichts", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gott dir gute Gaben giebet,", "tokens": ["Wenn", "Gott", "dir", "gu\u00b7te", "Ga\u00b7ben", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sink sanft ins Nichts und dich ausleer;", "tokens": ["Sink", "sanft", "ins", "Nichts", "und", "dich", "aus\u00b7leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "KON", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft er dich arm, sei unbetr\u00fcbet,", "tokens": ["L\u00e4\u00dft", "er", "dich", "arm", ",", "sei", "un\u00b7be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sink sanft ins Nichts und nichts begehr!", "tokens": ["Sink", "sanft", "ins", "Nichts", "und", "nichts", "be\u00b7gehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}