{"textgrid.poem.50883": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie schlafend unterm Fl\u00fcgel ein Pfau den Schnabel h\u00e4lt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schlafend unterm Fl\u00fcgel ein Pfau den Schnabel h\u00e4lt,", "tokens": ["Wie", "schla\u00b7fend", "un\u00b7term", "Fl\u00fc\u00b7gel", "ein", "Pfau", "den", "Schna\u00b7bel", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von luft'gen Vogeltr\u00e4umen die blaue Brust geschwellt,", "tokens": ["Von", "luft'\u00b7gen", "Vo\u00b7gel\u00b7tr\u00e4u\u00b7men", "die", "blau\u00b7e", "Brust", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Geduckt auf einem Fu\u00dfe, dann pl\u00f6tzlich oft einmal,", "tokens": ["Ge\u00b7duckt", "auf", "ei\u00b7nem", "Fu\u00b7\u00dfe", ",", "dann", "pl\u00f6tz\u00b7lich", "oft", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Traume phantasierend, das Funkelrad er stellt:", "tokens": ["Im", "Trau\u00b7me", "phan\u00b7ta\u00b7sie\u00b7rend", ",", "das", "Fun\u00b7kel\u00b7rad", "er", "stellt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So hing bet\u00e4ubt und trunken, ausreckend Berg und Tal,", "tokens": ["So", "hing", "be\u00b7t\u00e4ubt", "und", "trun\u00b7ken", ",", "aus\u00b7re\u00b7ckend", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "KON", "ADJD", "$,", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der gro\u00dfe Wundervogel in tiefem Schlaf, die Welt.", "tokens": ["Der", "gro\u00b7\u00dfe", "Wun\u00b7der\u00b7vo\u00b7gel", "in", "tie\u00b7fem", "Schlaf", ",", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "So schwoll der blaue Himmel von Tr\u00e4umen ohne Zahl,", "tokens": ["So", "schwoll", "der", "blau\u00b7e", "Him\u00b7mel", "von", "Tr\u00e4u\u00b7men", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mit leisem Knistern schlug er ein Rad, das Sternenzelt.", "tokens": ["Mit", "lei\u00b7sem", "Knis\u00b7tern", "schlug", "er", "ein", "Rad", ",", "das", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}