{"textgrid.poem.52670": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebt zu sein, du sch\u00f6ne Kunde,", "tokens": ["Ge\u00b7liebt", "zu", "sein", ",", "du", "sch\u00f6\u00b7ne", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n wie die Hand, die das Gest\u00e4ndni\u00df schrieb!", "tokens": ["Sch\u00f6n", "wie", "die", "Hand", ",", "die", "das", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "schrieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nur sch\u00f6ner ist, wenn vom beseelten Munde", "tokens": ["Nur", "sch\u00f6\u00b7ner", "ist", ",", "wenn", "vom", "be\u00b7seel\u00b7ten", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entschwebet erst die s\u00fc\u00dfe, frohste Kunde \u2013", "tokens": ["Ent\u00b7schwe\u00b7bet", "erst", "die", "s\u00fc\u00b7\u00dfe", ",", "frohs\u00b7te", "Kun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit stummen Zeichen nehm ich nicht vorlieb.", "tokens": ["Mit", "stum\u00b7men", "Zei\u00b7chen", "nehm", "ich", "nicht", "vor\u00b7lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Freundschaft ist in wirren Tagen", "tokens": ["Die", "Freund\u00b7schaft", "ist", "in", "wir\u00b7ren", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein heilig Gut und will der Priester viel;", "tokens": ["Ein", "hei\u00b7lig", "Gut", "und", "will", "der", "Pries\u00b7ter", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "KON", "VMFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Laut ist ihr Ruf, ihr stolzes Banner tragen", "tokens": ["Laut", "ist", "ihr", "Ruf", ",", "ihr", "stol\u00b7zes", "Ban\u00b7ner", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Besten hoch in unsern wirren Tagen \u2013", "tokens": ["Die", "Bes\u00b7ten", "hoch", "in", "un\u00b7sern", "wir\u00b7ren", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Arbeit vereint und K\u00e4mpfer schafft das Ziel.", "tokens": ["Ar\u00b7beit", "ver\u00b7eint", "und", "K\u00e4mp\u00b7fer", "schafft", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Doch kaum ist uns der Freund verbunden,", "tokens": ["Doch", "kaum", "ist", "uns", "der", "Freund", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist auch der Trennung sichre Stunde da,", "tokens": ["Ist", "auch", "der", "Tren\u00b7nung", "sich\u00b7re", "Stun\u00b7de", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe nur kennt nicht so bittre Stunden,", "tokens": ["Die", "Lie\u00b7be", "nur", "kennt", "nicht", "so", "bitt\u00b7re", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es bleibt das Weib, das ewig uns verbunden,", "tokens": ["Es", "bleibt", "das", "Weib", ",", "das", "e\u00b7wig", "uns", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe dauert aus, die Lieb ist nah.", "tokens": ["Die", "Lie\u00b7be", "dau\u00b7ert", "aus", ",", "die", "Lieb", "ist", "nah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geliebt zu sein vom sch\u00f6nen Weibe,", "tokens": ["Ge\u00b7liebt", "zu", "sein", "vom", "sch\u00f6\u00b7nen", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mehr als Gl\u00fcck, ist volle Seligkeit.", "tokens": ["Ist", "mehr", "als", "Gl\u00fcck", ",", "ist", "vol\u00b7le", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOUS", "NN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df sie rein, und stets erf\u00fcllet bleibe,", "tokens": ["Auf", "da\u00df", "sie", "rein", ",", "und", "stets", "er\u00b7f\u00fcl\u00b7let", "blei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "$,", "KON", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gab hin Natur die Gegenwart dem Weibe", "tokens": ["Gab", "hin", "Na\u00b7tur", "die", "Ge\u00b7gen\u00b7wart", "dem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und lie\u00df uns Zukunft und Vergangenheit.", "tokens": ["Und", "lie\u00df", "uns", "Zu\u00b7kunft", "und", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}