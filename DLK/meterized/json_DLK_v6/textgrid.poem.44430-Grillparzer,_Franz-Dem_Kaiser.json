{"textgrid.poem.44430": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Dem Kaiser", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du Segen str\u00f6mst auf alle", "tokens": ["Der", "du", "Se\u00b7gen", "str\u00f6mst", "auf", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf mich allein,", "tokens": ["Und", "auf", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zieht das Jahr beim Jubelschalle", "tokens": ["Zieht", "das", "Jahr", "beim", "Ju\u00b7bel\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausender dir ein;", "tokens": ["Tau\u00b7sen\u00b7der", "dir", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ich darf doch in n\u00e4herm Falle", "tokens": ["Ich", "darf", "doch", "in", "n\u00e4\u00b7herm", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doppelt dankbar sein,", "tokens": ["Dop\u00b7pelt", "dank\u00b7bar", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Danken dir zuerst wie alle,", "tokens": ["Dan\u00b7ken", "dir", "zu\u00b7erst", "wie", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KOKOM", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann f\u00fcr mich allein.", "tokens": ["Dann", "f\u00fcr", "mich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}