{"dta.poem.21015": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXIX.  \n Eines Todtengr\u00e4bers.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Bader w\u00e4scht sich selbst/ der Schneider kan sich", "tokens": ["Der", "Ba\u00b7der", "w\u00e4scht", "sich", "selbst", "/", "der", "Schnei\u00b7der", "kan", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$(", "ART", "NE", "VMFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kleiden/ (den.", "tokens": ["klei\u00b7den", "/", "(", "den", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVINF", "$(", "$(", "ART", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Der Koch darff ohne Koch nicht seine Mahlzeit mei-", "tokens": ["Der", "Koch", "darff", "oh\u00b7ne", "Koch", "nicht", "sei\u00b7ne", "Mahl\u00b7zeit", "mei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VMFIN", "APPR", "NE", "PTKNEG", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich der ich vor begrab die Klugen/ und die Narren/", "tokens": ["Ich", "der", "ich", "vor", "be\u00b7grab", "die", "Klu\u00b7gen", "/", "und", "die", "Nar\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "APPR", "NN", "ART", "NN", "$(", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kan nun wie sichs geb\u00fchrt mich selber nicht verschar-", "tokens": ["Kan", "nun", "wie", "sichs", "ge\u00b7b\u00fchrt", "mich", "sel\u00b7ber", "nicht", "ver\u00b7scha\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "KOKOM", "PIS", "VVFIN", "PPER", "ADV", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ren.", "tokens": ["ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}