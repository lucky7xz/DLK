{"textgrid.poem.54006": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Was dem Lakaien seine Livree", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da stehn in Panzer und Lederkollern", "tokens": ["Da", "stehn", "in", "Pan\u00b7zer", "und", "Le\u00b7der\u00b7kol\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die bessern Herren von den Hohenzollern.", "tokens": ["die", "bes\u00b7sern", "Her\u00b7ren", "von", "den", "Ho\u00b7hen\u00b7zol\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und man fragt sich, sieht man die Schnallen und Maschen:", "tokens": ["Und", "man", "fragt", "sich", ",", "sieht", "man", "die", "Schnal\u00b7len", "und", "Ma\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "$,", "VVFIN", "PIS", "ART", "NN", "KON", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wann hat sich der Junge eigentlich gewaschen?", "tokens": ["Wann", "hat", "sich", "der", "Jun\u00b7ge", "ei\u00b7gent\u00b7lich", "ge\u00b7wa\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wahrscheinlich an hohen Feiertagen,", "tokens": ["Wahr\u00b7schein\u00b7lich", "an", "ho\u00b7hen", "Fei\u00b7er\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wenn er hinging, sein Vaterunser zu sagen.", "tokens": ["wenn", "er", "hin\u00b7ging", ",", "sein", "Va\u00b7ter\u00b7un\u00b7ser", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Was heute Kino ist und Theater,", "tokens": ["Was", "heu\u00b7te", "Ki\u00b7no", "ist", "und", "The\u00b7a\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "daf\u00fcr hatte er diesen heiligen Vater.", "tokens": ["da\u00b7f\u00fcr", "hat\u00b7te", "er", "die\u00b7sen", "hei\u00b7li\u00b7gen", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der war in jener vergangenen Zeit", "tokens": ["Der", "war", "in", "je\u00b7ner", "ver\u00b7gan\u00b7ge\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ein fetter Mime der Fr\u00f6mmigkeit.", "tokens": ["ein", "fet\u00b7ter", "Mi\u00b7me", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Segnend ragt seine Unterlippe . . .", "tokens": ["Seg\u00b7nend", "ragt", "sei\u00b7ne", "Un\u00b7ter\u00b7lip\u00b7pe", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "mit den F\u00fcrsten machte die Kirche Kippe:", "tokens": ["mit", "den", "F\u00fcrs\u00b7ten", "mach\u00b7te", "die", "Kir\u00b7che", "Kip\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "dir ein Halb und mir ein Halb \u2013", "tokens": ["dir", "ein", "Halb", "und", "mir", "ein", "Halb", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "der Untertan war ein gutes Kalb.", "tokens": ["der", "Un\u00b7ter\u00b7tan", "war", "ein", "gu\u00b7tes", "Kalb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und wenn es einmal im Volke erbraust:", "tokens": ["Und", "wenn", "es", "ein\u00b7mal", "im", "Vol\u00b7ke", "er\u00b7braust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dann haben wir hier die starke Faust.", "tokens": ["Dann", "ha\u00b7ben", "wir", "hier", "die", "star\u00b7ke", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Das Volk? Das werden wir doch noch kriegen!", "tokens": ["Das", "Volk", "?", "Das", "wer\u00b7den", "wir", "doch", "noch", "krie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "In diesem Zeichen wollen wir siegen!", "tokens": ["In", "die\u00b7sem", "Zei\u00b7chen", "wol\u00b7len", "wir", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir w\u00fcrgen sie, bis sie nicht mehr jappen . . .", "tokens": ["Wir", "w\u00fcr\u00b7gen", "sie", ",", "bis", "sie", "nicht", "mehr", "jap\u00b7pen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "$.", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hier ist das echte preu\u00dfische Wappen.", "tokens": ["Hier", "ist", "das", "ech\u00b7te", "preu\u00b7\u00dfi\u00b7sche", "Wap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein b\u00f6ses Vieh. Es beraubt und hackt", "tokens": ["Ein", "b\u00f6\u00b7ses", "Vieh", ".", "Es", "be\u00b7raubt", "und", "hackt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVPP", "KON", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "jeden, der sich mit Arbeit plackt.", "tokens": ["je\u00b7den", ",", "der", "sich", "mit", "Ar\u00b7beit", "plackt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Da steht die Allee. Es zaudern die zahmen", "tokens": ["Da", "steht", "die", "Al\u00b7lee", ".", "Es", "zau\u00b7dern", "die", "zah\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Republikaner vor Marmorreklamen.", "tokens": ["Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", "vor", "Mar\u00b7mor\u00b7re\u00b7kla\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Im Namen der Wehrmacht \u2013", "tokens": ["Im", "Na\u00b7men", "der", "Wehr\u00b7macht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "in Alt-Deutschlands Namen.", "tokens": ["in", "Al\u00b7tDeut\u00b7schlands", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "AmEN!", "tokens": ["A\u00b7mEN", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}