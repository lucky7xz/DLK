{"textgrid.poem.41161": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Als ich es sah, da wurde ich leichenbla\u00df.", "tokens": ["Als", "ich", "es", "sah", ",", "da", "wur\u00b7de", "ich", "lei\u00b7chen\u00b7bla\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus dem Kopfe kommen schwarze D\u00fcnste,", "tokens": ["Aus", "dem", "Kop\u00b7fe", "kom\u00b7men", "schwar\u00b7ze", "D\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Arznei \u2013 und Dichtkunst schlechte K\u00fcnste,", "tokens": ["Der", "Arz\u00b7nei", "\u2013", "und", "Dicht\u00b7kunst", "schlech\u00b7te", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Meines ganzen eitlen Lebens Dunst,", "tokens": ["Mei\u00b7nes", "gan\u00b7zen", "eit\u00b7len", "Le\u00b7bens", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Scham, da\u00df ich unwert so vieler Gunst.", "tokens": ["Scham", ",", "da\u00df", "ich", "un\u00b7wert", "so", "vie\u00b7ler", "Gunst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Schaut den alten Leib, der ein Gerippe,", "tokens": ["Schaut", "den", "al\u00b7ten", "Leib", ",", "der", "ein", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "W\u00e4hrend ich am Lebensbaum noch nippe,", "tokens": ["W\u00e4h\u00b7rend", "ich", "am", "Le\u00b7bens\u00b7baum", "noch", "nip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "An den F\u00fc\u00dfen schaut die Erdenschwere,", "tokens": ["An", "den", "F\u00fc\u00b7\u00dfen", "schaut", "die", "Er\u00b7den\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "O! wenn die noch abzustreifen w\u00e4re!", "tokens": ["O", "!", "wenn", "die", "noch", "ab\u00b7zu\u00b7strei\u00b7fen", "w\u00e4\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ART", "ADV", "VVIZU", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ich vermag es nicht, und ihre Macht", "tokens": ["Ich", "ver\u00b7mag", "es", "nicht", ",", "und", "ih\u00b7re", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Zieht mich nieder in des Hades Nacht.", "tokens": ["Zieht", "mich", "nie\u00b7der", "in", "des", "Ha\u00b7des", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}