{"textgrid.poem.59683": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtrheinfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei schlug's nach Mitternacht, wohl sieben Meilen", "tokens": ["Zwei", "schlug's", "nach", "Mit\u00b7ter\u00b7nacht", ",", "wohl", "sie\u00b7ben", "Mei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "ADJA", "APPR", "NN", "$,", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hatt' ich am hei\u00dfen Sommertag vollendet,", "tokens": ["Hatt'", "ich", "am", "hei\u00b7\u00dfen", "Som\u00b7mer\u00b7tag", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sahen, wo die Sieg zum Rhein sich wendet,", "tokens": ["Da", "sa\u00b7hen", ",", "wo", "die", "Sieg", "zum", "Rhein", "sich", "wen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "APPRART", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Mond und Sterne mich nach Mondorf eilen.", "tokens": ["Nur", "Mond", "und", "Ster\u00b7ne", "mich", "nach", "Mon\u00b7dorf", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es schliefen Mensch und Tier und Wald und B\u00e4ume,", "tokens": ["Es", "schlie\u00b7fen", "Mensch", "und", "Tier", "und", "Wald", "und", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die V\u00f6glein bargen unter stillen Fl\u00fcgeln", "tokens": ["Die", "V\u00f6\u00b7glein", "bar\u00b7gen", "un\u00b7ter", "stil\u00b7len", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schn\u00e4bel und die Stimmen, aus den Spiegeln", "tokens": ["Die", "Schn\u00e4\u00b7bel", "und", "die", "Stim\u00b7men", ",", "aus", "den", "Spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Tages spielten Bilderspiel die Tr\u00e4ume.", "tokens": ["Des", "Ta\u00b7ges", "spiel\u00b7ten", "Bil\u00b7der\u00b7spiel", "die", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich rief dem Fergen, doch mir scholl's entgegen:", "tokens": ["Ich", "rief", "dem", "Fer\u00b7gen", ",", "doch", "mir", "scholl's", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bber liegt am Ufer jenseits eingeschlafen,", "tokens": ["\u00bb", "er", "liegt", "am", "U\u00b7fer", "jen\u00b7seits", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn selten kommt zu unserm kleinen Hafen", "tokens": ["Denn", "sel\u00b7ten", "kommt", "zu", "un\u00b7serm", "klei\u00b7nen", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wandrer hin auf mittern\u00e4cht'gen Wegen.", "tokens": ["Ein", "Wand\u00b7rer", "hin", "auf", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7gen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch steht ein Eichstock an der Sieg Gestade,", "tokens": ["Doch", "steht", "ein", "Eich\u00b7stock", "an", "der", "Sieg", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und macht das schmale Fahrzeug Euch kein Grauen,", "tokens": ["Und", "macht", "das", "schma\u00b7le", "Fahr\u00b7zeug", "Euch", "kein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00f6gt Ihr meiner Armeskraft vertrauen,", "tokens": ["So", "m\u00f6gt", "Ihr", "mei\u00b7ner", "Ar\u00b7mes\u00b7kraft", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich rudr' Euch mutig durch die Wellenpfade.\u00ab", "tokens": ["Ich", "rudr'", "Euch", "mu\u00b7tig", "durch", "die", "Wel\u00b7len\u00b7pfa\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbgeh! Hol'!\u00ab \u2013 Er ging. Doch unterdes erbla\u00dften", "tokens": ["\u00bb", "geh", "!", "Hol'", "!", "\u00ab", "\u2013", "Er", "ging", ".", "Doch", "un\u00b7ter\u00b7des", "er\u00b7bla\u00df\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "NN", "$.", "$(", "$(", "PPER", "VVFIN", "$.", "KON", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mond und Gestirne, schwarze Wolken zogen", "tokens": ["Mond", "und", "Ge\u00b7stir\u00b7ne", ",", "schwar\u00b7ze", "Wol\u00b7ken", "zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gewitternacht zusammen, Blitze flogen,", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7nacht", "zu\u00b7sam\u00b7men", ",", "Blit\u00b7ze", "flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich vom Ost zum West umarmend fa\u00dften.", "tokens": ["Die", "sich", "vom", "Ost", "zum", "West", "um\u00b7ar\u00b7mend", "fa\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Eichstock kam. Sein blitzerhellter Treiber", "tokens": ["Der", "Eich\u00b7stock", "kam", ".", "Sein", "blit\u00b7zer\u00b7hell\u00b7ter", "Trei\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschien mir nun, ein Mann gewalt'ger Knochen,", "tokens": ["Er\u00b7schien", "mir", "nun", ",", "ein", "Mann", "ge\u00b7walt'\u00b7ger", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schwarz, d\u00fcster, gleich dem F\u00e4hrmann viel besprochen,", "tokens": ["Schwarz", ",", "d\u00fcs\u00b7ter", ",", "gleich", "dem", "F\u00e4hr\u00b7mann", "viel", "be\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der weiland Geister f\u00fchrte d\u00fcnnster Leiber.", "tokens": ["Der", "wei\u00b7land", "Geis\u00b7ter", "f\u00fchr\u00b7te", "d\u00fcnns\u00b7ter", "Lei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Frisch sprang ich doch in diesen Charonsnachen,", "tokens": ["Frisch", "sprang", "ich", "doch", "in", "die\u00b7sen", "Cha\u00b7rons\u00b7na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch kaum das Viertel meines Wegs gefahren,", "tokens": ["Doch", "kaum", "das", "Vier\u00b7tel", "mei\u00b7nes", "Wegs", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erpfiff ein Lispelwind, er pfiff Gefahren,", "tokens": ["Er\u00b7pfiff", "ein", "Lis\u00b7pel\u00b7wind", ",", "er", "pfiff", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die bald als Sturm und Donner sollten krachen.", "tokens": ["Die", "bald", "als", "Sturm", "und", "Don\u00b7ner", "soll\u00b7ten", "kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Schon bebet die Natur, die V\u00f6gel sausen", "tokens": ["Schon", "be\u00b7bet", "die", "Na\u00b7tur", ",", "die", "V\u00f6\u00b7gel", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch wilde Luft, mit Bellen, Heulen, St\u00f6hnen", "tokens": ["Durch", "wil\u00b7de", "Luft", ",", "mit", "Bel\u00b7len", ",", "Heu\u00b7len", ",", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwacht die Kreatur in Klaget\u00f6nen,", "tokens": ["Er\u00b7wacht", "die", "Kre\u00b7a\u00b7tur", "in", "Kla\u00b7ge\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die kurz verhallend durcheinander brausen.", "tokens": ["Die", "kurz", "ver\u00b7hal\u00b7lend", "durch\u00b7ein\u00b7an\u00b7der", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Ruf der W\u00e4chter, die die Nacht durchschreiten,", "tokens": ["Der", "Ruf", "der", "W\u00e4ch\u00b7ter", ",", "die", "die", "Nacht", "durch\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schreit in geschwinder Angst aus dumpfem Horne,", "tokens": ["Schreit", "in", "ge\u00b7schwin\u00b7der", "Angst", "aus", "dum\u00b7pfem", "Hor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als bliesen sie ein Lied von Gottes Zorne,", "tokens": ["Als", "blie\u00b7sen", "sie", "ein", "Lied", "von", "Got\u00b7tes", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den J\u00fcngsten Tag, den Untergang der Zeiten.", "tokens": ["Den", "J\u00fcng\u00b7sten", "Tag", ",", "den", "Un\u00b7ter\u00b7gang", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und krach, schl\u00e4gt's ein vor uns, die Wellen spritzen,", "tokens": ["Und", "krach", ",", "schl\u00e4gt's", "ein", "vor", "uns", ",", "die", "Wel\u00b7len", "sprit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ART", "APPR", "PPER", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Nachen b\u00e4umt sich, wie zum letzten Sprunge", "tokens": ["Der", "Na\u00b7chen", "b\u00e4umt", "sich", ",", "wie", "zum", "letz\u00b7ten", "Sprun\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein fallend Ro\u00df, und aus dem Ruderschwunge", "tokens": ["Ein", "fal\u00b7lend", "Ro\u00df", ",", "und", "aus", "dem", "Ru\u00b7der\u00b7schwun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entst\u00fcrzen beide wir zugleich den Sitzen.", "tokens": ["Ent\u00b7st\u00fcr\u00b7zen", "bei\u00b7de", "wir", "zu\u00b7gleich", "den", "Sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ein Ruder brach, ein Vogel ohne Fl\u00fcgel", "tokens": ["Ein", "Ru\u00b7der", "brach", ",", "ein", "Vo\u00b7gel", "oh\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fliegt nun das Schifflein fort. \u00bbGott sei uns gn\u00e4dig!\u00ab", "tokens": ["Fliegt", "nun", "das", "Schif\u00b7flein", "fort", ".", "\u00bb", "Gott", "sei", "uns", "gn\u00e4\u00b7dig", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "$(", "NN", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So rufen wir kleinm\u00fctig und kleinredig:", "tokens": ["So", "ru\u00b7fen", "wir", "klein\u00b7m\u00fc\u00b7tig", "und", "klein\u00b7re\u00b7dig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--++--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbder Wogenturm wird uns zum Grabesh\u00fcgel.\u00ab", "tokens": ["\u00bb", "der", "Wo\u00b7gen\u00b7turm", "wird", "uns", "zum", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Doch Wunder! Wie wir kaum das Wort gesprochen,", "tokens": ["Doch", "Wun\u00b7der", "!", "Wie", "wir", "kaum", "das", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"