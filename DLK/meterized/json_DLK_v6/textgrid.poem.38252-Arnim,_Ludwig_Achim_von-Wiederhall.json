{"textgrid.poem.38252": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wiederhall", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem gr\u00fcnen Wald,", "tokens": ["In", "die\u00b7sem", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir wollen fr\u00f6hlich singen,", "tokens": ["Wir", "wol\u00b7len", "fr\u00f6h\u00b7lich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt wie es wiederhallt,", "tokens": ["H\u00f6rt", "wie", "es", "wie\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und fr\u00f6hlich thut erklingen.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "thut", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach wie ein Lieblichkeit", "tokens": ["Ach", "wie", "ein", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und holdseliges Leben", "tokens": ["Und", "hold\u00b7se\u00b7li\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6ne Sommerzeit", "tokens": ["Die", "sch\u00f6\u00b7ne", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und helle Sonn thut geben.", "tokens": ["Und", "hel\u00b7le", "Sonn", "thut", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dieweil die V\u00f6gel all", "tokens": ["Die\u00b7weil", "die", "V\u00f6\u00b7gel", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Luft und Freuden schweben;", "tokens": ["In", "Luft", "und", "Freu\u00b7den", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voraus die Nachtigall", "tokens": ["Vo\u00b7raus", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Stimmlein thut erheben.", "tokens": ["Ihr", "Stimm\u00b7lein", "thut", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Warum soll uns denn nicht,", "tokens": ["Wa\u00b7rum", "soll", "uns", "denn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Sang aus uns erfreuen;", "tokens": ["Der", "Sang", "aus", "uns", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt Echo widerspricht,", "tokens": ["H\u00f6rt", "E\u00b7cho", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und will uns \u00fcberschreien.", "tokens": ["Und", "will", "uns", "\u00fc\u00b7bersc\u00b7hrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Herr vom Himmelsthron", "tokens": ["Der", "Herr", "vom", "Him\u00b7melst\u00b7hron"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Woll seine Gnade geben,", "tokens": ["Woll", "sei\u00b7ne", "Gna\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir den Sommer schon", "tokens": ["Da\u00df", "wir", "den", "Som\u00b7mer", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Oefter mit Freud erleben.", "tokens": ["Oef\u00b7ter", "mit", "Freud", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}