{"textgrid.poem.61739": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied von D\u00fcppel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was klingt aus den St\u00e4dten wie helles Festgel\u00e4ut?", "tokens": ["Was", "klingt", "aus", "den", "St\u00e4d\u00b7ten", "wie", "hel\u00b7les", "Fest\u00b7ge\u00b7l\u00e4ut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Pauken und Drommeten, was jubeln sie heut?", "tokens": ["Die", "Pau\u00b7ken", "und", "Drom\u00b7me\u00b7ten", ",", "was", "ju\u00b7beln", "sie", "heut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was brausen und jagen die Wasser der Schlei?", "tokens": ["Was", "brau\u00b7sen", "und", "ja\u00b7gen", "die", "Was\u00b7ser", "der", "Schlei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Feind ist geschlagen, und Schleswig ist frei?", "tokens": ["Der", "Feind", "ist", "ge\u00b7schla\u00b7gen", ",", "und", "Schles\u00b7wig", "ist", "frei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Bei D\u00fcppel dort am Meere, vor Alsen am Sund,", "tokens": ["Bei", "D\u00fcp\u00b7pel", "dort", "am", "Mee\u00b7re", ",", "vor", "Al\u00b7sen", "am", "Sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPRART", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da rangen die Heere auf blutgetr\u00e4nktem Grund;", "tokens": ["Da", "ran\u00b7gen", "die", "Hee\u00b7re", "auf", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7tem", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da galt's, auf die Schanzen im Siegessturmgewog'", "tokens": ["Da", "galt's", ",", "auf", "die", "Schan\u00b7zen", "im", "Sie\u00b7ges\u00b7sturm\u00b7ge\u00b7wo\u00b7g'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Adler zu pflanzen anstatt des Danebrog.", "tokens": ["Den", "Ad\u00b7ler", "zu", "pflan\u00b7zen", "an\u00b7statt", "des", "Da\u00b7ne\u00b7brog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Von Kugeln umsungen, vom hei\u00dfen Tod umkracht", "tokens": ["Von", "Ku\u00b7geln", "um\u00b7sun\u00b7gen", ",", "vom", "hei\u00b7\u00dfen", "Tod", "um\u00b7kracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die m\u00e4rkischen Jungen, wie stritten sie mit Macht!", "tokens": ["Die", "m\u00e4r\u00b7ki\u00b7schen", "Jun\u00b7gen", ",", "wie", "strit\u00b7ten", "sie", "mit", "Macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie lernten sie das Steigen auf schl\u00fcpfriger Bahn!", "tokens": ["Wie", "lern\u00b7ten", "sie", "das", "Stei\u00b7gen", "auf", "schl\u00fcpf\u00b7ri\u00b7ger", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es ging wie im Reigen; der Beeren war voran.", "tokens": ["Es", "ging", "wie", "im", "Rei\u00b7gen", ";", "der", "Bee\u00b7ren", "war", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wohl mancher der Braven sank mit ihm in den Sand;", "tokens": ["Wohl", "man\u00b7cher", "der", "Bra\u00b7ven", "sank", "mit", "ihm", "in", "den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du fielst, o tapfrer Raven, das Schwert in der Hand.", "tokens": ["Du", "fielst", ",", "o", "tapf\u00b7rer", "Ra\u00b7ven", ",", "das", "Schwert", "in", "der", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und du am Pulverfasse, getreuer Winkelried!", "tokens": ["Und", "du", "am", "Pul\u00b7ver\u00b7fas\u00b7se", ",", "ge\u00b7treu\u00b7er", "Win\u00b7kel\u00b7ried", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Klinkeschen Gasse gedenkt noch manch ein Lied.", "tokens": ["Der", "Klin\u00b7ke\u00b7schen", "Gas\u00b7se", "ge\u00b7denkt", "noch", "manch", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Doch als auf den W\u00e4llen nun flog das Siegspanier,", "tokens": ["Doch", "als", "auf", "den", "W\u00e4l\u00b7len", "nun", "flog", "das", "Siegs\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da bliesen die Gesellen: \u00bbHerr Gott, dich loben wir!\u00ab", "tokens": ["Da", "blie\u00b7sen", "die", "Ge\u00b7sel\u00b7len", ":", "\u00bb", "Herr", "Gott", ",", "dich", "lo\u00b7ben", "wir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das hat sich erschwungen wie Abels Opferbrand,", "tokens": ["Das", "hat", "sich", "er\u00b7schwun\u00b7gen", "wie", "A\u00b7bels", "Op\u00b7fer\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "VVPP", "KOKOM", "NN", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das ist hinausgeklungen bis tief ins deutsche Land.", "tokens": ["Das", "ist", "hin\u00b7aus\u00b7ge\u00b7klun\u00b7gen", "bis", "tief", "ins", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Im sonnigen Meere nun spiegelt sich aufs neu'", "tokens": ["Im", "son\u00b7ni\u00b7gen", "Mee\u00b7re", "nun", "spie\u00b7gelt", "sich", "aufs", "neu'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PRF", "APPRART", "ADJA"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die preu\u00dfische Ehre, die alte deutsche Treu';", "tokens": ["Die", "preu\u00b7\u00dfi\u00b7sche", "Eh\u00b7re", ",", "die", "al\u00b7te", "deut\u00b7sche", "Treu'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und war sie gesch\u00e4ndet, wie strahlt sie doppelt rein!", "tokens": ["Und", "war", "sie", "ge\u00b7sch\u00e4n\u00b7det", ",", "wie", "strahlt", "sie", "dop\u00b7pelt", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und habt ihr sie verpf\u00e4ndet, ihr l\u00f6stet sie ein.", "tokens": ["Und", "habt", "ihr", "sie", "ver\u00b7pf\u00e4n\u00b7det", ",", "ihr", "l\u00f6s\u00b7tet", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Ihr Meister der Staaten, und geht ihr nun und tagt,", "tokens": ["Ihr", "Meis\u00b7ter", "der", "Staa\u00b7ten", ",", "und", "geht", "ihr", "nun", "und", "tagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So woll' euch Gott beraten, auf da\u00df ihr nicht zagt!", "tokens": ["So", "woll'", "euch", "Gott", "be\u00b7ra\u00b7ten", ",", "auf", "da\u00df", "ihr", "nicht", "zagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVFIN", "$,", "APPR", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprecht: Nichts von Vertragen! Nun bleibt es dabei,", "tokens": ["Sprecht", ":", "Nichts", "von", "Ver\u00b7tra\u00b7gen", "!", "Nun", "bleibt", "es", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Feind ist geschlagen, und Schleswig ist frei.", "tokens": ["Der", "Feind", "ist", "ge\u00b7schla\u00b7gen", ",", "und", "Schles\u00b7wig", "ist", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}