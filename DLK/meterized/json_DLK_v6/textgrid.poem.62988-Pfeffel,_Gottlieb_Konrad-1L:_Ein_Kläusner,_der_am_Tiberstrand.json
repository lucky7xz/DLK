{"textgrid.poem.62988": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kl\u00e4usner, der am Tiberstrand", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kl\u00e4usner, der am Tiberstrand", "tokens": ["Ein", "Kl\u00e4us\u00b7ner", ",", "der", "am", "Ti\u00b7ber\u00b7strand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst fischte, zog in seinem Netze", "tokens": ["Einst", "fischte", ",", "zog", "in", "sei\u00b7nem", "Net\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den sch\u00f6nsten Hecht erfreut ans Land.", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Hecht", "er\u00b7freut", "ans", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbverwegner! sprach der Fisch, verletze", "tokens": ["\u00bb", "ver\u00b7weg\u00b7ner", "!", "sprach", "der", "Fisch", ",", "ver\u00b7let\u00b7ze"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADJD", "$.", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht meine heilige Person!", "tokens": ["Nicht", "mei\u00b7ne", "hei\u00b7li\u00b7ge", "Per\u00b7son", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du wei\u00dft die ganze Passion,", "tokens": ["Du", "wei\u00dft", "die", "gan\u00b7ze", "Pas\u00b7si\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Kelch, den Schwamm, das Kreuz, die Lanze,", "tokens": ["Den", "Kelch", ",", "den", "Schwamm", ",", "das", "Kreuz", ",", "die", "Lan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die N\u00e4gel samt dem Dornenkranze", "tokens": ["Die", "N\u00e4\u00b7gel", "samt", "dem", "Dor\u00b7nen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hab ich im Kopfe.\u00ab Wunderlich!", "tokens": ["Hab", "ich", "im", "Kop\u00b7fe", ".", "\u00ab", "Wun\u00b7der\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$.", "$(", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Versetzt der Greis: doch darf ich fragen,", "tokens": ["Ver\u00b7setzt", "der", "Greis", ":", "doch", "darf", "ich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was hast du hier im vollen Magen?", "tokens": ["Was", "hast", "du", "hier", "im", "vol\u00b7len", "Ma\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sprich oder ich zergliedre dich! \u2013", "tokens": ["Sprich", "o\u00b7der", "ich", "zer\u00b7glied\u00b7re", "dich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "KON", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbach nichts; ein Nest mit jungen Aalen,", "tokens": ["\u00bb", "ach", "nichts", ";", "ein", "Nest", "mit", "jun\u00b7gen", "Aa\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Hochw\u00fcrdiger Herr Eremit,", "tokens": ["Hoch\u00b7w\u00fcr\u00b7di\u00b7ger", "Herr", "E\u00b7re\u00b7mit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein kleines Fr\u00fchst\u00fcck\u00ab. \u2013 Ha, Bandit!", "tokens": ["Ein", "klei\u00b7nes", "Fr\u00fch\u00b7st\u00fcck", "\u00ab", ".", "\u2013", "Ha", ",", "Ban\u00b7dit", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$.", "$(", "ITJ", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ich dacht es wohl ihr Kannibalen", "tokens": ["Ich", "dacht", "es", "wohl", "ihr", "Kan\u00b7ni\u00b7ba\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Tragt die Religion im Kopf,", "tokens": ["Tragt", "die", "Re\u00b7li\u00b7gi\u00b7on", "im", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Und in dem Busen das Verderben. \u2013", "tokens": ["Und", "in", "dem", "Bu\u00b7sen", "das", "Ver\u00b7der\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hier warf er ihn in seinen Topf", "tokens": ["Hier", "warf", "er", "ihn", "in", "sei\u00b7nen", "Topf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und lie\u00df ihn wie Sanct Vitus sterben.", "tokens": ["Und", "lie\u00df", "ihn", "wie", "Sanct", "Vi\u00b7tus", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "VVFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}