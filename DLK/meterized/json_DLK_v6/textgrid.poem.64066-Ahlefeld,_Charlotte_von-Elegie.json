{"textgrid.poem.64066": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandelnd im Schimmer des Mondes, und tief in Gedanken versunken,", "tokens": ["Wan\u00b7delnd", "im", "Schim\u00b7mer", "des", "Mon\u00b7des", ",", "und", "tief", "in", "Ge\u00b7dan\u00b7ken", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,", "KON", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "F\u00fchrt mich der l\u00e4ndliche Pfad \u00fcber das schweigende Feld,", "tokens": ["F\u00fchrt", "mich", "der", "l\u00e4nd\u00b7li\u00b7che", "Pfad", "\u00fc\u00b7ber", "das", "schwei\u00b7gen\u00b7de", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Und in der Stille des Abends, die mich balsamisch ums\u00e4uselt,", "tokens": ["Und", "in", "der", "Stil\u00b7le", "des", "A\u00b7bends", ",", "die", "mich", "bal\u00b7sa\u00b7misch", "um\u00b7s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+--+--+--++-+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Nahet dem sehnenden Geist mancher Erinnerung Bild.", "tokens": ["Na\u00b7het", "dem", "seh\u00b7nen\u00b7den", "Geist", "man\u00b7cher", "E\u00b7rin\u00b7ne\u00b7rung", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie voll Schwermuth und Ernst auf n\u00e4chtliche Stille des Grabes", "tokens": ["Wie", "voll", "Schwer\u00b7muth", "und", "Ernst", "auf", "n\u00e4cht\u00b7li\u00b7che", "Stil\u00b7le", "des", "Gra\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Luna den trauernden Blick senkt von der ewigen Bahn,", "tokens": ["Lu\u00b7na", "den", "trau\u00b7ern\u00b7den", "Blick", "senkt", "von", "der", "e\u00b7wi\u00b7gen", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der die Gefilde erhellt mit bleichem Glanze, so blicket", "tokens": ["Der", "die", "Ge\u00b7fil\u00b7de", "er\u00b7hellt", "mit", "blei\u00b7chem", "Glan\u00b7ze", ",", "so", "bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Auf vergangene Zeit tr\u00fcbe umw\u00f6lket mein Sinn.", "tokens": ["Auf", "ver\u00b7gan\u00b7ge\u00b7ne", "Zeit", "tr\u00fc\u00b7be", "um\u00b7w\u00f6l\u00b7ket", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Bilder der Freundschaft und Liebe, und all' ihr l\u00e4chelnden Tr\u00e4ume,", "tokens": ["Bil\u00b7der", "der", "Freund\u00b7schaft", "und", "Lie\u00b7be", ",", "und", "all'", "ihr", "l\u00e4\u00b7cheln\u00b7den", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,", "KON", "PIS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die ihr mein Leben geschm\u00fcckt, eilet, und kehret zur\u00fcck.", "tokens": ["Die", "ihr", "mein", "Le\u00b7ben", "ge\u00b7schm\u00fcckt", ",", "ei\u00b7let", ",", "und", "keh\u00b7ret", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Stillet mit freundlichem Trug die blutenden Wunden der Sehnsucht,", "tokens": ["Stil\u00b7let", "mit", "freund\u00b7li\u00b7chem", "Trug", "die", "blu\u00b7ten\u00b7den", "Wun\u00b7den", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da mir die wirkliche Welt meine ertr\u00e4umte versagt.", "tokens": ["Da", "mir", "die", "wirk\u00b7li\u00b7che", "Welt", "mei\u00b7ne", "er\u00b7tr\u00e4um\u00b7te", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, ihr erscheinet mir nun \u2013 doch nicht mehr im rosigen Schimmer", "tokens": ["Ja", ",", "ihr", "er\u00b7schei\u00b7net", "mir", "nun", "\u2013", "doch", "nicht", "mehr", "im", "ro\u00b7si\u00b7gen", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$(", "ADV", "PTKNEG", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jugendlich gl\u00fchender Lust, die aus der Hoffnung entsprang.", "tokens": ["Ju\u00b7gend\u00b7lich", "gl\u00fc\u00b7hen\u00b7der", "Lust", ",", "die", "aus", "der", "Hoff\u00b7nung", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "N\u00e4chtlich, und tr\u00fcbe verh\u00fcllt in melancholische Schleier", "tokens": ["N\u00e4cht\u00b7lich", ",", "und", "tr\u00fc\u00b7be", "ver\u00b7h\u00fcllt", "in", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "ADJA", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Sinnender Wehmuth, erscheint ihr meinem thr\u00e4nenden Blick.", "tokens": ["Sin\u00b7nen\u00b7der", "Weh\u00b7muth", ",", "er\u00b7scheint", "ihr", "mei\u00b7nem", "thr\u00e4\u00b7nen\u00b7den", "Blick", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--++-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.5": {"line.1": {"text": "Seyd mir dennoch willkommen, auch im Geleite der Trauer,", "tokens": ["Seyd", "mir", "den\u00b7noch", "will\u00b7kom\u00b7men", ",", "auch", "im", "Ge\u00b7lei\u00b7te", "der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADJD", "$,", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Die an den Wechsel der Zeit mahnet mit schweigendem Ernst.", "tokens": ["Die", "an", "den", "Wech\u00b7sel", "der", "Zeit", "mah\u00b7net", "mit", "schwei\u00b7gen\u00b7dem", "Ernst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach es lindert so sanft den zehrenden Schmerz des Entbehrens", "tokens": ["Ach", "es", "lin\u00b7dert", "so", "sanft", "den", "zeh\u00b7ren\u00b7den", "Schmerz", "des", "Ent\u00b7beh\u00b7rens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Der Erinnerung Gl\u00fcck, ist's auch mit Wermuth vermischt.", "tokens": ["Der", "E\u00b7rin\u00b7ne\u00b7rung", "Gl\u00fcck", ",", "ist's", "auch", "mit", "Wer\u00b7muth", "ver\u00b7mischt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Unbefangene Tage der fr\u00fchen, bl\u00fchenden Jugend,", "tokens": ["Un\u00b7be\u00b7fan\u00b7ge\u00b7ne", "Ta\u00b7ge", "der", "fr\u00fc\u00b7hen", ",", "bl\u00fc\u00b7hen\u00b7den", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Als mein fr\u00f6hliches Herz lauter zu klopfen begann!", "tokens": ["Als", "mein", "fr\u00f6h\u00b7li\u00b7ches", "Herz", "lau\u00b7ter", "zu", "klop\u00b7fen", "be\u00b7gann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Als die Erstlingsgef\u00fchle der Liebe sich regten im Busen,", "tokens": ["Als", "die", "Erst\u00b7lings\u00b7ge\u00b7f\u00fch\u00b7le", "der", "Lie\u00b7be", "sich", "reg\u00b7ten", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Als mir die Freundschaft zuerst reichte den duftenden Kranz!", "tokens": ["Als", "mir", "die", "Freund\u00b7schaft", "zu\u00b7erst", "reich\u00b7te", "den", "duf\u00b7ten\u00b7den", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "K\u00f6nnt' ich Eurer vergessen \u2013 vergessen der Stunden voll Wonne,", "tokens": ["K\u00f6nnt'", "ich", "Eu\u00b7rer", "ver\u00b7ges\u00b7sen", "\u2013", "ver\u00b7ges\u00b7sen", "der", "Stun\u00b7den", "voll", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "VVPP", "$(", "VVPP", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Denen die Hoffnung einst lieh ihr verwelkendes Gr\u00fcn?", "tokens": ["De\u00b7nen", "die", "Hoff\u00b7nung", "einst", "lieh", "ihr", "ver\u00b7wel\u00b7ken\u00b7des", "Gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "O dann h\u00e4tte ich nie Euch zu durchleben verdienet,", "tokens": ["O", "dann", "h\u00e4t\u00b7te", "ich", "nie", "Euch", "zu", "durch\u00b7le\u00b7ben", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+--+-+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und der Vergangenheit Reiz w\u00e4re ein Schatten mir nur.", "tokens": ["Und", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "Reiz", "w\u00e4\u00b7re", "ein", "Schat\u00b7ten", "mir", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Treu in liebender Brust bewahret hab' ich Dein Bildniss,", "tokens": ["Treu", "in", "lie\u00b7ben\u00b7der", "Brust", "be\u00b7wah\u00b7ret", "hab'", "ich", "Dein", "Bild\u00b7niss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Unvergessliche Zeit, die Du so eilig entflohst.", "tokens": ["Un\u00b7ver\u00b7gess\u00b7li\u00b7che", "Zeit", ",", "die", "Du", "so", "ei\u00b7lig", "ent\u00b7flohst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Oft erscheinst Du mir heiter \u2013 doch \u00f6fter in Nebel und Wolken,", "tokens": ["Oft", "er\u00b7scheinst", "Du", "mir", "hei\u00b7ter", "\u2013", "doch", "\u00f6f\u00b7ter", "in", "Ne\u00b7bel", "und", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$(", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Aber wie Du auch nahst \u2013 freundlich empfang' ich Dich stets.", "tokens": ["A\u00b7ber", "wie", "Du", "auch", "nahst", "\u2013", "freund\u00b7lich", "emp\u00b7fang'", "ich", "Dich", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$(", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Tief in d\u00e4mmernder Nacht der undurchdringlichen Zukunft,", "tokens": ["Tief", "in", "d\u00e4m\u00b7mern\u00b7der", "Nacht", "der", "un\u00b7durch\u00b7dring\u00b7li\u00b7chen", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Bleibt die Erinnerung mir ewig ein leuchtender Stern.", "tokens": ["Bleibt", "die", "E\u00b7rin\u00b7ne\u00b7rung", "mir", "e\u00b7wig", "ein", "leuch\u00b7ten\u00b7der", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wend' ich mein Auge zu ihm, so strahlt er mir Frieden entgegen,", "tokens": ["Wend'", "ich", "mein", "Au\u00b7ge", "zu", "ihm", ",", "so", "strahlt", "er", "mir", "Frie\u00b7den", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,"], "meter": "++-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und das Dunkel umher hellet sein sonniger Glanz.", "tokens": ["Und", "das", "Dun\u00b7kel", "um\u00b7her", "hel\u00b7let", "sein", "son\u00b7ni\u00b7ger", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Darum will ich die Blumen, die n\u00e4chsten, welche mir bl\u00fchen,", "tokens": ["Da\u00b7rum", "will", "ich", "die", "Blu\u00b7men", ",", "die", "n\u00e4chs\u00b7ten", ",", "wel\u00b7che", "mir", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind sie auch sparsam verstreut, dennoch mit sorgender Hand", "tokens": ["Sind", "sie", "auch", "spar\u00b7sam", "ver\u00b7streut", ",", "den\u00b7noch", "mit", "sor\u00b7gen\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "wor