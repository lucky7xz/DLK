{"textgrid.poem.44292": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Kampf der Leidenschaften", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha! welch unnennbare Gef\u00fchle", "tokens": ["Ha", "!", "welch", "un\u00b7nenn\u00b7ba\u00b7re", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Durchstr\u00f6men meine Brust!", "tokens": ["Durch\u00b7str\u00f6\u00b7men", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es paart im schrecklichen Gew\u00fchle", "tokens": ["Es", "paart", "im", "schreck\u00b7li\u00b7chen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verzweiflung sich mit Lust.", "tokens": ["Ver\u00b7zwei\u00b7flung", "sich", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ha! welch ein Chaos nun in meinem Herzen", "tokens": ["Ha", "!", "welch", "ein", "Chaos", "nun", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Innerstes durchbebt!", "tokens": ["Mein", "In\u00b7ners\u00b7tes", "durch\u00b7bebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie unter nie gef\u00fchlten Schmerzen", "tokens": ["Wie", "un\u00b7ter", "nie", "ge\u00b7f\u00fchl\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Brust sich st\u00f6hnend hebt!", "tokens": ["Die", "Brust", "sich", "st\u00f6h\u00b7nend", "hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie wenn in Lybiens grausen W\u00fcsten,", "tokens": ["Wie", "wenn", "in", "Ly\u00b7bi\u00b7ens", "grau\u00b7sen", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Im gl\u00fchend hei\u00dfen Sand,", "tokens": ["Im", "gl\u00fc\u00b7hend", "hei\u00b7\u00dfen", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich gattend ein Paar Schlangen nisten,", "tokens": ["Sich", "gat\u00b7tend", "ein", "Paar", "Schlan\u00b7gen", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verderbend Flur und Land.", "tokens": ["Ver\u00b7der\u00b7bend", "Flur", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn dann in fr\u00f6hlichem Get\u00fcmmel", "tokens": ["Wenn", "dann", "in", "fr\u00f6h\u00b7li\u00b7chem", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Schlang an Schlange schmiegt,", "tokens": ["Sich", "Schlang", "an", "Schlan\u00b7ge", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nun in scheu\u00dflichem Gewimmel", "tokens": ["Und", "nun", "in", "scheu\u00df\u00b7li\u00b7chem", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Brut im Neste liegt.", "tokens": ["Die", "Brut", "im", "Nes\u00b7te", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So windet glimmend sich zusammen", "tokens": ["So", "win\u00b7det", "glim\u00b7mend", "sich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Leidenschaften Wut,", "tokens": ["Der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Wut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch endlich spr\u00fcht der Funke Flammen", "tokens": ["Doch", "end\u00b7lich", "spr\u00fcht", "der", "Fun\u00b7ke", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcbrall tobt die Glut.", "tokens": ["Und", "\u00fcb\u00b7rall", "tobt", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nur du kannst diese Flamme d\u00e4mpfen,", "tokens": ["Nur", "du", "kannst", "die\u00b7se", "Flam\u00b7me", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Hoffnung, nur dein Strahl", "tokens": ["O", "Hoff\u00b7nung", ",", "nur", "dein", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann die Verzweifelung bek\u00e4mpfen", "tokens": ["Kann", "die", "Ver\u00b7zwei\u00b7fe\u00b7lung", "be\u00b7k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lindern meine Qual.", "tokens": ["Und", "lin\u00b7dern", "mei\u00b7ne", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}