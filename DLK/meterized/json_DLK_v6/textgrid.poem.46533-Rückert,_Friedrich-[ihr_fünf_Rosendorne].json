{"textgrid.poem.46533": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ihr f\u00fcnf Rosendorne]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr f\u00fcnf Rosendorne,", "tokens": ["Ihr", "f\u00fcnf", "Ro\u00b7sen\u00b7dor\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihr f\u00fcnf Rittersporne,", "tokens": ["Ihr", "f\u00fcnf", "Rit\u00b7ter\u00b7spor\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr f\u00fcnf Eisenh\u00fctchen,", "tokens": ["Ihr", "f\u00fcnf", "Ei\u00b7sen\u00b7h\u00fct\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "L\u00f6wenrachenbl\u00fctchen!", "tokens": ["L\u00f6\u00b7wen\u00b7ra\u00b7chen\u00b7bl\u00fct\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr f\u00fcnf Rosendorne", "tokens": ["Ihr", "f\u00fcnf", "Ro\u00b7sen\u00b7dor\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Was habt ihr im Zorne", "tokens": ["Was", "habt", "ihr", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "R\u00f6slein nicht vertheidigt,", "tokens": ["R\u00f6s\u00b7lein", "nicht", "ver\u00b7thei\u00b7digt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als es ward beleidigt?", "tokens": ["Als", "es", "ward", "be\u00b7lei\u00b7digt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr gespornten Ritter,", "tokens": ["Ihr", "ge\u00b7sporn\u00b7ten", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihr, als euch vom Schnitter", "tokens": ["Ihr", ",", "als", "euch", "vom", "Schnit\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ward die Braut entf\u00fchret,", "tokens": ["Ward", "die", "Braut", "ent\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Habt euch nicht ger\u00fchret.", "tokens": ["Habt", "euch", "nicht", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O ihr Eisenh\u00fctchen,", "tokens": ["O", "ihr", "Ei\u00b7sen\u00b7h\u00fct\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "K\u00fchlet euer M\u00fcthchen", "tokens": ["K\u00fch\u00b7let", "eu\u00b7er", "M\u00fcth\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch am Sensenmann,", "tokens": ["Doch", "am", "Sen\u00b7sen\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der sie h\u00e4lt im Bann!", "tokens": ["Der", "sie", "h\u00e4lt", "im", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ach ihr L\u00f6wenm\u00fcndchen", "tokens": ["Ach", "ihr", "L\u00f6\u00b7wen\u00b7m\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Klafft wie L\u00f6wenh\u00fcndchen,", "tokens": ["Klafft", "wie", "L\u00f6\u00b7wen\u00b7h\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ihr sie im Rachen", "tokens": ["Wie", "ihr", "sie", "im", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Seht dem grausen Drachen.", "tokens": ["Seht", "dem", "grau\u00b7sen", "Dra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "J\u00fcngster wollt' es wagen,", "tokens": ["J\u00fcngs\u00b7ter", "wollt'", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich f\u00fcr sie zu schlagen,", "tokens": ["Sich", "f\u00fcr", "sie", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Aber mitgefangen", "tokens": ["A\u00b7ber", "mit\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist er mitgegangen.", "tokens": ["Ist", "er", "mit\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Bleibt, da\u00df ich verliere", "tokens": ["Bleibt", ",", "da\u00df", "ich", "ver\u00b7lie\u00b7re"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nicht auch euch, ihr viere!", "tokens": ["Nicht", "auch", "euch", ",", "ihr", "vie\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "$,", "PPER", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bleibt mit mir zu trauern", "tokens": ["Bleibt", "mit", "mir", "zu", "trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unter Regenschauern!", "tokens": ["Un\u00b7ter", "Re\u00b7gen\u00b7schau\u00b7ern", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}