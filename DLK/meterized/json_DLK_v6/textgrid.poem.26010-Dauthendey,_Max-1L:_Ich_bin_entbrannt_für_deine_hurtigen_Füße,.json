{"textgrid.poem.26010": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin entbrannt f\u00fcr deine hurtigen F\u00fc\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin entbrannt f\u00fcr deine hurtigen F\u00fc\u00dfe,", "tokens": ["Ich", "bin", "ent\u00b7brannt", "f\u00fcr", "dei\u00b7ne", "hur\u00b7ti\u00b7gen", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es ist, als tr\u00fcge jeder Fu\u00df ein Herz,", "tokens": ["Es", "ist", ",", "als", "tr\u00fc\u00b7ge", "je\u00b7der", "Fu\u00df", "ein", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie so schnell zu mir eilen.", "tokens": ["Da\u00df", "sie", "so", "schnell", "zu", "mir", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn dein L\u00e4cheln \u00fcber die Berge geht, wird der Wein s\u00fc\u00df und schwer,", "tokens": ["Wenn", "dein", "L\u00e4\u00b7cheln", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "geht", ",", "wird", "der", "Wein", "s\u00fc\u00df", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+-+--++-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und die Welt sieht gro\u00df und neu aus.", "tokens": ["Und", "die", "Welt", "sieht", "gro\u00df", "und", "neu", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der weithallende Wald atmet meine Brust bei dir auf;", "tokens": ["Wie", "der", "weit\u00b7hal\u00b7len\u00b7de", "Wald", "at\u00b7met", "mei\u00b7ne", "Brust", "bei", "dir", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "---+--++-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Darf ich deinen Namen nennen,", "tokens": ["Darf", "ich", "dei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Wird meine Zunge eine s\u00fc\u00dfe Frucht in meinem Mund.", "tokens": ["Wird", "mei\u00b7ne", "Zun\u00b7ge", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Frucht", "in", "mei\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}