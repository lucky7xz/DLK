{"textgrid.poem.46895": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du brauchst nicht deinen Schmerz zu n\u00e4hren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du brauchst nicht deinen Schmerz zu n\u00e4hren,", "tokens": ["Du", "brauchst", "nicht", "dei\u00b7nen", "Schmerz", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird von selbst im Herzen w\u00e4hren.", "tokens": ["Er", "wird", "von", "selbst", "im", "Her\u00b7zen", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du brauchst ihn auch nicht zu bek\u00e4mpfen,", "tokens": ["Du", "brauchst", "ihn", "auch", "nicht", "zu", "be\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst ihn doch nicht niederd\u00e4mpfen.", "tokens": ["Du", "wirst", "ihn", "doch", "nicht", "nie\u00b7der\u00b7d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was sollst du thun? la\u00df dir's erkl\u00e4ren:", "tokens": ["Was", "sollst", "du", "thun", "?", "la\u00df", "dir's", "er\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Schmerz in dir zur Lust verkl\u00e4ren.", "tokens": ["Den", "Schmerz", "in", "dir", "zur", "Lust", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}