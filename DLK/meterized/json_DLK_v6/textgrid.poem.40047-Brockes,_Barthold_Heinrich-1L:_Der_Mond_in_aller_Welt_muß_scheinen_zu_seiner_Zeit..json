{"textgrid.poem.40047": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mond in aller Welt mu\u00df scheinen zu seiner Zeit.", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond in aller Welt mu\u00df scheinen zu seiner Zeit.", "tokens": ["Der", "Mond", "in", "al\u00b7ler", "Welt", "mu\u00df", "schei\u00b7nen", "zu", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VMFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Kaum hatte sich die Nacht zu zeigen angefangen,", "tokens": ["Kaum", "hat\u00b7te", "sich", "die", "Nacht", "zu", "zei\u00b7gen", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, nach der Hitze Last, der K\u00fchlung Lust verhie\u00df;", "tokens": ["Die", ",", "nach", "der", "Hit\u00b7ze", "Last", ",", "der", "K\u00fch\u00b7lung", "Lust", "ver\u00b7hie\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "NE", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als sich ein neuer Tag, dem Schein nach, sehen lie\u00df.", "tokens": ["Als", "sich", "ein", "neu\u00b7er", "Tag", ",", "dem", "Schein", "nach", ",", "se\u00b7hen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$,", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der volle Mond war aus dem grauen Duft,", "tokens": ["Der", "vol\u00b7le", "Mond", "war", "aus", "dem", "grau\u00b7en", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der, nach des Tages schw\u00fcler Luft,", "tokens": ["Der", ",", "nach", "des", "Ta\u00b7ges", "schw\u00fc\u00b7ler", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Purpur untermischt, den Horizont bedeckte,", "tokens": ["Mit", "Pur\u00b7pur", "un\u00b7ter\u00b7mischt", ",", "den", "Ho\u00b7ri\u00b7zont", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sich rings um die Erde streckte,", "tokens": ["Und", "sich", "rings", "um", "die", "Er\u00b7de", "streck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wie r\u00f6thlich Gold, nur eben aufgegangen;", "tokens": ["Wie", "r\u00f6th\u00b7lich", "Gold", ",", "nur", "e\u00b7ben", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Aus dessen wandelbarem Kreise,", "tokens": ["Aus", "des\u00b7sen", "wan\u00b7del\u00b7ba\u00b7rem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der alles in der Nacht mit Licht und Schimmer f\u00fcllt,", "tokens": ["Der", "al\u00b7les", "in", "der", "Nacht", "mit", "Licht", "und", "Schim\u00b7mer", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf eine wunderbare Weise", "tokens": ["Auf", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mehr Anmuth noch, als Licht und Schimmer, quillt,", "tokens": ["Mehr", "An\u00b7muth", "noch", ",", "als", "Licht", "und", "Schim\u00b7mer", ",", "quillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die grosse Scheibe gl\u00e4ntzt' in einer g\u00fcld'nen Gl\u00e4tte,", "tokens": ["Die", "gros\u00b7se", "Schei\u00b7be", "gl\u00e4ntzt'", "in", "ei\u00b7ner", "g\u00fcld'\u00b7nen", "Gl\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Worauf jedoch recht hell und klar,", "tokens": ["Wo\u00b7rauf", "je\u00b7doch", "recht", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als ob sie ein Gepr\u00e4ge h\u00e4tte,", "tokens": ["Als", "ob", "sie", "ein", "Ge\u00b7pr\u00e4\u00b7ge", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein schattigtes Gesicht zu sehen war.", "tokens": ["Ein", "schat\u00b7tig\u00b7tes", "Ge\u00b7sicht", "zu", "se\u00b7hen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Es war die heit're Luft gantz rein:", "tokens": ["Es", "war", "die", "heit'\u00b7re", "Luft", "gantz", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Man sah von Duft und Wolcken nichts:", "tokens": ["Man", "sah", "von", "Duft", "und", "Wol\u00b7cken", "nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "An statt des Monds sonst kalt- und blassen Lichts", "tokens": ["An", "statt", "des", "Monds", "sonst", "kal\u00b7t", "und", "blas\u00b7sen", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "TRUNC", "KON", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Schien ein fast r\u00f6thlich-gelber Schein", "tokens": ["Schien", "ein", "fast", "r\u00f6\u00b7th\u00b7lich\u00b7gel\u00b7ber", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Dem warmen Ur-Stral gleich zu seyn.", "tokens": ["Dem", "war\u00b7men", "Ur\u00b7S\u00b7tral", "gleich", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hiedurch gereitzt und halb entz\u00fccket", "tokens": ["Hie\u00b7durch", "ge\u00b7reitzt", "und", "halb", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVPP", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gieng Thirsis auf das Feld,", "tokens": ["Gieng", "Thir\u00b7sis", "auf", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um, bey so heit'rer Nacht,", "tokens": ["Um", ",", "bey", "so", "heit'\u00b7rer", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die, durch des Mondes helle Pracht,", "tokens": ["Die", ",", "durch", "des", "Mon\u00b7des", "hel\u00b7le", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Nutz und Schmuck bethaute Welt,", "tokens": ["Zum", "Nutz", "und", "Schmuck", "be\u00b7thau\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die man nicht gnug bewundern kann,", "tokens": ["Die", "man", "nicht", "gnug", "be\u00b7wun\u00b7dern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In stiller Andacht zu betrachten;", "tokens": ["In", "stil\u00b7ler", "An\u00b7dacht", "zu", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da viele Gegen-W\u00fcrf' ihn dann", "tokens": ["Da", "vie\u00b7le", "Ge\u00b7gen\u00b7W\u00fcr\u00b7f'", "ihn", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Auf folgende Gedancken brachten:", "tokens": ["Auf", "fol\u00b7gen\u00b7de", "Ge\u00b7dan\u00b7cken", "brach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "In dieser lichten Dunckelheit", "tokens": ["In", "die\u00b7ser", "lich\u00b7ten", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zweifelhaften Heiterkeit", "tokens": ["Und", "zwei\u00b7fel\u00b7haf\u00b7ten", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entf\u00e4rben sich die Farben, und verschwinden:", "tokens": ["Ent\u00b7f\u00e4r\u00b7ben", "sich", "die", "Far\u00b7ben", ",", "und", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Man kann nicht einst die Spur von ihrem Wesen finden.", "tokens": ["Man", "kann", "nicht", "einst", "die", "Spur", "von", "ih\u00b7rem", "We\u00b7sen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}