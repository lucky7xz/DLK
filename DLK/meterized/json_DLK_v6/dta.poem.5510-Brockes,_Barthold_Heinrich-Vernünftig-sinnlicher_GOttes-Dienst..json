{"dta.poem.5510": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vern\u00fcnftig-sinnlicher GOttes-Dienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist doch das Geschenck der Sinnen so herrlich,\nwenn mans recht ermi\u00dft!", "tokens": ["Wie", "ist", "doch", "das", "Ge\u00b7schenck", "der", "Sin\u00b7nen", "so", "herr\u00b7lich", ",", "wenn", "mans", "recht", "er\u00b7mi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ach, da\u00df die Menschheit GOtt, dem Geber, daf\u00fcr so un-", "tokens": ["Ach", ",", "da\u00df", "die", "Menschheit", "Gott", ",", "dem", "Ge\u00b7ber", ",", "da\u00b7f\u00fcr", "so", "un"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "PAV", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich finde, da\u00df auch Hunde riechen; ich sehe, wie auch", "tokens": ["Ich", "fin\u00b7de", ",", "da\u00df", "auch", "Hun\u00b7de", "rie\u00b7chen", ";", "ich", "se\u00b7he", ",", "wie", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "NN", "VVINF", "$.", "PPER", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Sonnen Licht und Gras und Blumen, so wol als", "tokens": ["Der", "Son\u00b7nen", "Licht", "und", "Gras", "und", "Blu\u00b7men", ",", "so", "wol", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "KON", "NN", "$,", "ADV", "ADV", "KOKOM"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was machst du denn f\u00fcr Wercks davon? ", "tokens": ["Was", "machst", "du", "denn", "f\u00fcr", "Wercks", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zwar noch wol so gut als wir, auch ofters besser", "tokens": ["Und", "zwar", "noch", "wol", "so", "gut", "als", "wir", ",", "auch", "of\u00b7ters", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll zwischen uns und ihnen denn so gar kein Unterschied", "tokens": ["Soll", "zwi\u00b7schen", "uns", "und", "ih\u00b7nen", "denn", "so", "gar", "kein", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "KON", "PPER", "ADV", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Da\u00df willst du ja wol eben nicht. Nun kann ja der in", "tokens": ["Da\u00df", "willst", "du", "ja", "wol", "e\u00b7ben", "nicht", ".", "Nun", "kann", "ja", "der", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "$.", "ADV", "VMFIN", "ADV", "ART", "APPR"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Als eben darin nur bestehen,", "tokens": ["Als", "e\u00b7ben", "da\u00b7rin", "nur", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df wir auf andre Weis\u2019, als sie, empfinden, schmecken,", "tokens": ["Da\u00df", "wir", "auf", "and\u00b7re", "Weis'", ",", "als", "sie", ",", "emp\u00b7fin\u00b7den", ",", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gebrauchen wir durch die Vernunft die Sinnen anders", "tokens": ["Ge\u00b7brau\u00b7chen", "wir", "durch", "die", "Ver\u00b7nunft", "die", "Sin\u00b7nen", "an\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So folgt der Schlu\u00df von selbst: der Mensch ist auch nicht", "tokens": ["So", "folgt", "der", "Schlu\u00df", "von", "selbst", ":", "der", "Mensch", "ist", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Will man sich aber von demselben, wie es ja unsre Pflicht,", "tokens": ["Will", "man", "sich", "a\u00b7ber", "von", "dem\u00b7sel\u00b7ben", ",", "wie", "es", "ja", "uns\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "APPR", "PDAT", "$,", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.12": {"text": "La\u00dft uns die Sinnen, GOtt zum Ruhm, der sie uns", "tokens": ["La\u00dft", "uns", "die", "Sin\u00b7nen", ",", "Gott", "zum", "Ruhm", ",", "der", "sie", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "NN", "APPRART", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Die\u00df kann nun GOtt-gef\u00e4lliger auf andre Weise nicht", "tokens": ["Die\u00df", "kann", "nun", "Got\u00b7tge\u00b7f\u00e4l\u00b7li\u00b7ger", "auf", "and\u00b7re", "Wei\u00b7se", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "NN", "APPR", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Als wenn durch des Verstandes Licht wir w\u00fcrcklich sehen,", "tokens": ["Als", "wenn", "durch", "des", "Ver\u00b7stan\u00b7des", "Licht", "wir", "w\u00fcrck\u00b7lich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ART", "NN", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Empfinden ", "tokens": ["Emp\u00b7fin\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Alsdann wird man durch Seel\u2019 und Leib, die GOtt uns bei-", "tokens": ["Als\u00b7dann", "wird", "man", "durch", "Seel'", "und", "Leib", ",", "die", "Gott", "uns", "bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "PPER", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weil wir so dann in allem Weisheit und Liebe, ja ihn", "tokens": ["Weil", "wir", "so", "dann", "in", "al\u00b7lem", "Weis\u00b7heit", "und", "Lie\u00b7be", ",", "ja", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PIS", "NN", "KON", "NN", "$,", "ADV", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Durch nichts wird unser GOtt auf Erden in unsren See-", "tokens": ["Durch", "nichts", "wird", "un\u00b7ser", "Gott", "auf", "Er\u00b7den", "in", "un\u00b7sren", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die\u00df hei\u00dft, nach Davids Regel: ", "tokens": ["Die\u00df", "hei\u00dft", ",", "nach", "Da\u00b7vids", "Re\u00b7gel", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}