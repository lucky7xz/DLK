{"dta.poem.21035": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcIX.  \n Eines Hundes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Das Bette macht ich mir auf meiner Frauen-Brust/", "tokens": ["Das", "Bet\u00b7te", "macht", "ich", "mir", "auf", "mei\u00b7ner", "Frau\u00b7en\u00b7Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Z\u00fcnglein war ihr Schwamm/ ihr B\u00e4chlein", "tokens": ["Mein", "Z\u00fcn\u00b7glein", "war", "ihr", "Schwamm", "/", "ihr", "B\u00e4ch\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "meine Kost/", "tokens": ["mei\u00b7ne", "Kost", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Nun Leser wilst du nicht der schlechten Leiche lachen/", "tokens": ["Nun", "Le\u00b7ser", "wilst", "du", "nicht", "der", "schlech\u00b7ten", "Lei\u00b7che", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wil ich dir allein die Lagerstadt vermachen.", "tokens": ["So", "wil", "ich", "dir", "al\u00b7lein", "die", "La\u00b7ger\u00b7stadt", "ver\u00b7ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}