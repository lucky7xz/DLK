{"textgrid.poem.47471": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[viel bin ich umhergewandert]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel bin ich umhergewandert,", "tokens": ["Viel", "bin", "ich", "um\u00b7her\u00b7ge\u00b7wan\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um zum Heile zu gelangen,", "tokens": ["Um", "zum", "Hei\u00b7le", "zu", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Betagten und Erfahrnen", "tokens": ["Mit", "Be\u00b7tag\u00b7ten", "und", "Er\u00b7fahr\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich eifrigst umgegangen,", "tokens": ["Bin", "ich", "eif\u00b7rigst", "um\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stillte nicht der Seele Bangen,", "tokens": ["Still\u00b7te", "nicht", "der", "See\u00b7le", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reiste nicht zum Lichte so,", "tokens": ["Reis\u00b7te", "nicht", "zum", "Lich\u00b7te", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und am Ende blieb ich, wo? \u2013", "tokens": ["Und", "am", "En\u00b7de", "blieb", "ich", ",", "wo", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "An Suleima's Lippe hangen,", "tokens": ["An", "Su\u00b7lei\u00b7ma's", "Lip\u00b7pe", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Meines Seins erst jetzo froh.", "tokens": ["Mei\u00b7nes", "Seins", "erst", "jet\u00b7zo", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}