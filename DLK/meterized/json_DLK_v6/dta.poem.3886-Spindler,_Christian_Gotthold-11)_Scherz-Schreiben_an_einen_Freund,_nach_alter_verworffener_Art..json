{"dta.poem.3886": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "11) Scherz-Schreiben an einen Freund,  \n nach alter verworffener Art.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.85", "pt:0.14"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "O! Fratris charitas! dein letzter Brief und", "tokens": ["O", "!", "Frat\u00b7ris", "cha\u00b7ri\u00b7tas", "!", "dein", "letz\u00b7ter", "Brief", "und"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "NE", "$.", "PPOSAT", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schrifft,", "tokens": ["Schrifft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Tuaque Lenitas, die sich ein Denckmahl stifft,", "tokens": ["Tua\u00b7que", "Le\u00b7ni\u00b7tas", ",", "die", "sich", "ein", "Denck\u00b7mahl", "stifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Quod immortale est, zeigt mir so Freund als Liebe,", "tokens": ["Quod", "im\u00b7mor\u00b7ta\u00b7le", "est", ",", "zeigt", "mir", "so", "Freund", "als", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Quod admir andum est, die reinsten Freundschaffts-", "tokens": ["Quod", "ad\u00b7mir", "an\u00b7dum", "est", ",", "die", "reins\u00b7ten", "Freund\u00b7schaffts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Triebe.", "tokens": ["Trie\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "O! mi fratercule! dein redlich treues Hertz", "tokens": ["O", "!", "mi", "fra\u00b7ter\u00b7cu\u00b7le", "!", "dein", "red\u00b7lich", "treu\u00b7es", "Hertz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "VVFIN", "$.", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "O! Frater optime! zeigt mir den reinsten Schertz.", "tokens": ["O", "!", "Fra\u00b7ter", "op\u00b7ti\u00b7me", "!", "zeigt", "mir", "den", "reins\u00b7ten", "Schertz", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Tu habes gaudia, ich armer B\u00e4hrenheuter,", "tokens": ["Tu", "ha\u00b7bes", "gau\u00b7dia", ",", "ich", "ar\u00b7mer", "B\u00e4h\u00b7ren\u00b7heu\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Amitto omnia, ja ich verschmachte leider,", "tokens": ["A\u00b7mit\u00b7to", "om\u00b7nia", ",", "ja", "ich", "ver\u00b7schmach\u00b7te", "lei\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Qui malis premitur, das ist ein armer Tropff,", "tokens": ["Qui", "ma\u00b7lis", "pre\u00b7mi\u00b7tur", ",", "das", "ist", "ein", "ar\u00b7mer", "Tropff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Qui malis angitur, dem schmertzt der schwache Kopff.", "tokens": ["Qui", "ma\u00b7lis", "an\u00b7gi\u00b7tur", ",", "dem", "schmertzt", "der", "schwa\u00b7che", "Kopff", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sic nemo potest plus, wenn ihn die Nothdurfft qu\u00e4lt,", "tokens": ["Sic", "ne\u00b7mo", "po\u00b7test", "plus", ",", "wenn", "ihn", "die", "Noth\u00b7durfft", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Si quoque nummulus in seinem Beutel fehlt.", "tokens": ["Si", "quo\u00b7que", "num\u00b7mu\u00b7lus", "in", "sei\u00b7nem", "Beu\u00b7tel", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "En, itaque, ergo, nun kanst du leicht ersehen,", "tokens": ["En", ",", "i\u00b7ta\u00b7que", ",", "er\u00b7go", ",", "nun", "kanst", "du", "leicht", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "NE", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In tali jubilo! wie sie am Berge stehen.", "tokens": ["In", "ta\u00b7li", "ju\u00b7bi\u00b7lo", "!", "wie", "sie", "am", "Ber\u00b7ge", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "$.", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "En! coeli gratia! sey deine Krafft und St\u00e4rck,", "tokens": ["En", "!", "coe\u00b7li", "gra\u00b7tia", "!", "sey", "dei\u00b7ne", "Krafft", "und", "St\u00e4rck", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "FM", "FM", "$.", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Ejus Potentia, die f\u00f6rdere dein Werck.", "tokens": ["E\u00b7jus", "Po\u00b7ten\u00b7tia", ",", "die", "f\u00f6r\u00b7de\u00b7re", "dein", "Werck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Ego ad ultimum bey diesem schlechten Schreiben", "tokens": ["E\u00b7go", "ad", "ul\u00b7ti\u00b7mum", "bey", "die\u00b7sem", "schlech\u00b7ten", "Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ad mortis halitum, will ich dein Knecht verbleiben.", "tokens": ["Ad", "mor\u00b7tis", "ha\u00b7li\u00b7tum", ",", "will", "ich", "dein", "Knecht", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}