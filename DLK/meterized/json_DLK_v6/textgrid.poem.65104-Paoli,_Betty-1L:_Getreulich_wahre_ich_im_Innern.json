{"textgrid.poem.65104": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Getreulich wahre ich im Innern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getreulich wahre ich im Innern", "tokens": ["Ge\u00b7treu\u00b7lich", "wah\u00b7re", "ich", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein heiter gr\u00fcnendes Erinnern", "tokens": ["Ein", "hei\u00b7ter", "gr\u00fc\u00b7nen\u00b7des", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An jenen Tag voll Licht und Gluth,", "tokens": ["An", "je\u00b7nen", "Tag", "voll", "Licht", "und", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo in der Stadt wir der Pal\u00e4ste,", "tokens": ["Wo", "in", "der", "Stadt", "wir", "der", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Trauerort entschwund'ner Feste,", "tokens": ["Dem", "Trau\u00b7er\u00b7ort", "ent\u00b7schwun\u00b7d'\u00b7ner", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hinwogten auf smaragd'ner Fluth.", "tokens": ["Hin\u00b7wog\u00b7ten", "auf", "sma\u00b7ragd'\u00b7ner", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie feierlich war mir zu Muthe", "tokens": ["Wie", "fei\u00b7er\u00b7lich", "war", "mir", "zu", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als m\u00e4chtig sich mir der Salute", "tokens": ["Als", "m\u00e4ch\u00b7tig", "sich", "mir", "der", "Sa\u00b7lu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PRF", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Harmonisch edler Bau erschlo\u00df,", "tokens": ["Har\u00b7mo\u00b7nisch", "ed\u00b7ler", "Bau", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vecellis strahlende Madonne", "tokens": ["Ve\u00b7cel\u00b7lis", "strah\u00b7len\u00b7de", "Ma\u00b7don\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sch\u00f6nheit Licht wie eine Sonne", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Licht", "wie", "ei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In mein geblendet Auge go\u00df!", "tokens": ["In", "mein", "ge\u00b7blen\u00b7det", "Au\u00b7ge", "go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als, der schon in den Erdentagen", "tokens": ["Als", ",", "der", "schon", "in", "den", "Er\u00b7den\u00b7ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmel in der Brust getragen,", "tokens": ["Den", "Him\u00b7mel", "in", "der", "Brust", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine Bl\u00fcthen tr\u00e4umend brach,", "tokens": ["Und", "sei\u00b7ne", "Bl\u00fc\u00b7then", "tr\u00e4u\u00b7mend", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gian Bellin, der Sanfte, Milde,", "tokens": ["Gi\u00b7an", "Bel\u00b7lin", ",", "der", "Sanf\u00b7te", ",", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus seinem still durchseelten Bilde", "tokens": ["Aus", "sei\u00b7nem", "still", "durch\u00b7seel\u00b7ten", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Engelslauten zu mir sprach!", "tokens": ["Mit", "En\u00b7gels\u00b7lau\u00b7ten", "zu", "mir", "sprach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als, dem zu fr\u00fcher Tod die Krone", "tokens": ["Als", ",", "dem", "zu", "fr\u00fc\u00b7her", "Tod", "die", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein entwand, Giorgione", "tokens": ["Al\u00b7lein", "ent\u00b7wand", ",", "Gi\u00b7or\u00b7gio\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit seines Geistes tiefem Brand,", "tokens": ["Mit", "sei\u00b7nes", "Geis\u00b7tes", "tie\u00b7fem", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner Leidenschaft Gewalten,", "tokens": ["Mit", "sei\u00b7ner", "Lei\u00b7den\u00b7schaft", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die d\u00fcster sch\u00f6nste der Gestalten", "tokens": ["Die", "d\u00fcs\u00b7ter", "sch\u00f6ns\u00b7te", "der", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor meinem trunk'nen Blicke stand!", "tokens": ["Vor", "mei\u00b7nem", "trunk'\u00b7nen", "Bli\u00b7cke", "stand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie f\u00fchlt' ich meine Pulse fliegen!", "tokens": ["Wie", "f\u00fchlt'", "ich", "mei\u00b7ne", "Pul\u00b7se", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bew\u00e4ltigt glaubt ich zu erliegen", "tokens": ["Be\u00b7w\u00e4l\u00b7tigt", "glaubt", "ich", "zu", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von einer fremden Macht durchgraut,", "tokens": ["Von", "ei\u00b7ner", "frem\u00b7den", "Macht", "durch\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Einer, der zur Todesfeier,", "tokens": ["Wie", "Ei\u00b7ner", ",", "der", "zur", "To\u00b7des\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der H\u00fcllen baar und baar der Schleier,", "tokens": ["Der", "H\u00fcl\u00b7len", "baar", "und", "baar", "der", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Angesicht der Gottheit schaut.", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "der", "Got\u00b7theit", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist's nicht im Lichtgebild des Sch\u00f6nen,", "tokens": ["Ist's", "nicht", "im", "Licht\u00b7ge\u00b7bild", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich des Staubes armen S\u00f6hnen", "tokens": ["Da\u00df", "sich", "des", "Stau\u00b7bes", "ar\u00b7men", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ew'ge Glanz des Himmels zeigt?", "tokens": ["Der", "ew'\u00b7ge", "Glanz", "des", "Him\u00b7mels", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dft' in seines Menschthums Schw\u00e4chen", "tokens": ["Und", "mu\u00dft'", "in", "sei\u00b7nes", "Menscht\u00b7hums", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz vergehen nicht und brechen,", "tokens": ["Das", "Herz", "ver\u00b7ge\u00b7hen", "nicht", "und", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKNEG", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu dem die Gottheit sich geneigt? \u2013 \u2013", "tokens": ["Zu", "dem", "die", "Got\u00b7theit", "sich", "ge\u00b7neigt", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch du, mein herrlicher Begleiter!", "tokens": ["Doch", "du", ",", "mein", "herr\u00b7li\u00b7cher", "Be\u00b7glei\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du standest sinnend, ernst und heiter,", "tokens": ["Du", "stan\u00b7dest", "sin\u00b7nend", ",", "ernst", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fast wie ein Priester am Altar!", "tokens": ["Fast", "wie", "ein", "Pries\u00b7ter", "am", "Al\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Heimath war dir die\u00df Gebiethe,", "tokens": ["Denn", "Hei\u00b7math", "war", "dir", "die\u00df", "Ge\u00b7bie\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In welchem ich nur Neophyte,", "tokens": ["In", "wel\u00b7chem", "ich", "nur", "Neo\u00b7phy\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und ungeweihter Fremdling war.", "tokens": ["Und", "un\u00b7ge\u00b7weih\u00b7ter", "Fremd\u00b7ling", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du aber sprengtest mir die Riegel,", "tokens": ["Du", "a\u00b7ber", "spreng\u00b7test", "mir", "die", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du l\u00f6s'test die geheimsten Siegel", "tokens": ["Du", "l\u00f6s'\u00b7test", "die", "ge\u00b7heims\u00b7ten", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deines Wissens Zauberspruch.", "tokens": ["Mit", "dei\u00b7nes", "Wis\u00b7sens", "Zau\u00b7ber\u00b7spruch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr verwirrt und \u00fcbermeistert,", "tokens": ["Nicht", "mehr", "ver\u00b7wirrt", "und", "\u00fc\u00b7ber\u00b7meis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und d'rum nur flammender begeistert", "tokens": ["Und", "d'\u00b7rum", "nur", "flam\u00b7men\u00b7der", "be\u00b7geis\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Las ich in dem geweihten Buch!", "tokens": ["Las", "ich", "in", "dem", "ge\u00b7weih\u00b7ten", "Buch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie d\u00fcrstend lauscht' ich deiner Worte!", "tokens": ["Wie", "d\u00fcrs\u00b7tend", "lauscht'", "ich", "dei\u00b7ner", "Wor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie selig trat ich durch die Pforte", "tokens": ["Wie", "se\u00b7lig", "trat", "ich", "durch", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "In das erschloss'ne Heiligthum!", "tokens": ["In", "das", "er\u00b7schloss'\u00b7ne", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja! ich empfing von deinem Munde,", "tokens": ["Ja", "!", "ich", "emp\u00b7fing", "von", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die freudenvolle Lebenskunde,", "tokens": ["Die", "freu\u00b7den\u00b7vol\u00b7le", "Le\u00b7bens\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sch\u00f6nheit Evangelium!", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und dieses menschlich edle Streben,", "tokens": ["Und", "die\u00b7ses", "menschlich", "ed\u00b7le", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich bis zu ihr hinanzuheben,", "tokens": ["Mich", "bis", "zu", "ihr", "hi\u00b7nan\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", 