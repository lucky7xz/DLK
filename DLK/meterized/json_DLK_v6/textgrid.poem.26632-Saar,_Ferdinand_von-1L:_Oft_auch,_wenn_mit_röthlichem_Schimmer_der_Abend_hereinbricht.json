{"textgrid.poem.26632": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft auch, wenn mit r\u00f6thlichem Schimmer der Abend hereinbricht", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft auch, wenn mit r\u00f6thlichem Schimmer der Abend hereinbricht", "tokens": ["Oft", "auch", ",", "wenn", "mit", "r\u00f6th\u00b7li\u00b7chem", "Schim\u00b7mer", "der", "A\u00b7bend", "her\u00b7ein\u00b7bricht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Und aufathmet die Stadt, wandl' ich betrachtend umher;", "tokens": ["Und", "auf\u00b7ath\u00b7met", "die", "Stadt", ",", "wandl'", "ich", "be\u00b7trach\u00b7tend", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wandle nach rechts hin, oder nach links hin durch jene Bezirke,", "tokens": ["Wand\u00b7le", "nach", "rechts", "hin", ",", "o\u00b7der", "nach", "links", "hin", "durch", "je\u00b7ne", "Be\u00b7zir\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "PTKVZ", "$,", "KON", "APPR", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Die sich im Laufe der Zeit, wachsend zum Ganzen vereint.", "tokens": ["Die", "sich", "im", "Lau\u00b7fe", "der", "Zeit", ",", "wach\u00b7send", "zum", "Gan\u00b7zen", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sieh, da sind sie ja noch, die Vorstadtstra\u00dfen, die alten,", "tokens": ["Sieh", ",", "da", "sind", "sie", "ja", "noch", ",", "die", "Vor\u00b7stadt\u00b7stra\u00b7\u00dfen", ",", "die", "al\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+----+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die jetzt mit schwellender Fracht klingelnd die Trambahn bef\u00e4hrt.", "tokens": ["Die", "jetzt", "mit", "schwel\u00b7len\u00b7der", "Fracht", "klin\u00b7gelnd", "die", "Tram\u00b7bahn", "be\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVPP", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Freilich prunken auch sie schon mit neuem und neuestem Wesen,", "tokens": ["Frei\u00b7lich", "prun\u00b7ken", "auch", "sie", "schon", "mit", "neu\u00b7em", "und", "neu\u00b7es\u00b7tem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Aber ich sp\u00fcre den Hauch fr\u00fcherer Tage darin.", "tokens": ["A\u00b7ber", "ich", "sp\u00fc\u00b7re", "den", "Hauch", "fr\u00fc\u00b7he\u00b7rer", "Ta\u00b7ge", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "PAV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Frohsinn herrscht hier noch, es waltet der Segen der Arbeit,", "tokens": ["Froh\u00b7sinn", "herrscht", "hier", "noch", ",", "es", "wal\u00b7tet", "der", "Se\u00b7gen", "der", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Die den Genu\u00df nicht verwehrt, weil man sie reichlich belohnt.", "tokens": ["Die", "den", "Ge\u00b7nu\u00df", "nicht", "ver\u00b7wehrt", ",", "weil", "man", "sie", "reich\u00b7lich", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVPP", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Satte Gesichter ringsum, beleibte M\u00e4nner und Frauen,", "tokens": ["Sat\u00b7te", "Ge\u00b7sich\u00b7ter", "ring\u00b7sum", ",", "be\u00b7leib\u00b7te", "M\u00e4n\u00b7ner", "und", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Rosige M\u00e4dchen und hold bl\u00fchendes Kindergeschlecht.", "tokens": ["Ro\u00b7si\u00b7ge", "M\u00e4d\u00b7chen", "und", "hold", "bl\u00fc\u00b7hen\u00b7des", "Kin\u00b7der\u00b7ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Doch je weiter ich schreite, je mehr verwirrt sich der Anblick;", "tokens": ["Doch", "je", "wei\u00b7ter", "ich", "schrei\u00b7te", ",", "je", "mehr", "ver\u00b7wirrt", "sich", "der", "An\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADJD", "PRF", "ART", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Menschen in steigender Zahl, aber auch w\u00fcster das Bild.", "tokens": ["Men\u00b7schen", "in", "stei\u00b7gen\u00b7der", "Zahl", ",", "a\u00b7ber", "auch", "w\u00fcs\u00b7ter", "das", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Wimmelnd bev\u00f6lkert sind Gassen und H\u00e4user, aus zahllosen Fenstern", "tokens": ["Wim\u00b7melnd", "be\u00b7v\u00f6l\u00b7kert", "sind", "Gas\u00b7sen", "und", "H\u00e4u\u00b7ser", ",", "aus", "zahl\u00b7lo\u00b7sen", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VAFIN", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--++-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.16": {"text": "Blicken die Sorgen und M\u00fch'n \u00e4rmlichen Lebens hervor.", "tokens": ["Bli\u00b7cken", "die", "Sor\u00b7gen", "und", "M\u00fch'n", "\u00e4rm\u00b7li\u00b7chen", "Le\u00b7bens", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Hier, in billigster Miethe, wohnt eng der kleine Beamte,", "tokens": ["Hier", ",", "in", "bil\u00b7ligs\u00b7ter", "Mie\u00b7the", ",", "wohnt", "eng", "der", "klei\u00b7ne", "Be\u00b7am\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Haust bescheidene Kunst, emsig bei Tag und bei Nacht;", "tokens": ["Haust", "be\u00b7schei\u00b7de\u00b7ne", "Kunst", ",", "em\u00b7sig", "bei", "Tag", "und", "bei", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Hier erwirbt auch die Frau, es erwirbt die \u00e4lteste Tochter,", "tokens": ["Hier", "er\u00b7wirbt", "auch", "die", "Frau", ",", "es", "er\u00b7wirbt", "die", "\u00e4l\u00b7tes\u00b7te", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Ob sie die Feder bereits, oder die Nadel noch f\u00fchrt.", "tokens": ["Ob", "sie", "die", "Fe\u00b7der", "be\u00b7reits", ",", "o\u00b7der", "die", "Na\u00b7del", "noch", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "KON", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Kleine Fabriken gewahrt man, das kleine und kleinste Gewerbe,", "tokens": ["Klei\u00b7ne", "Fab\u00b7ri\u00b7ken", "ge\u00b7wahrt", "man", ",", "das", "klei\u00b7ne", "und", "kleins\u00b7te", "Ge\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "PIS", "$,", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-----+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Das verdrossen und stumpf lebt von der Hand in den Mund.", "tokens": ["Das", "ver\u00b7dros\u00b7sen", "und", "stumpf", "lebt", "von", "der", "Hand", "in", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Aber der Kr\u00e4mer gedeiht, es gedeiht der schmunzelnde Gastwirth,", "tokens": ["A\u00b7ber", "der", "Kr\u00e4\u00b7mer", "ge\u00b7deiht", ",", "es", "ge\u00b7deiht", "der", "schmun\u00b7zeln\u00b7de", "Gast\u00b7wirth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.24": {"text": "Dem das Gartenlokal immer des Abends gef\u00fcllt. \u2013", "tokens": ["Dem", "das", "Gar\u00b7ten\u00b7lo\u00b7kal", "im\u00b7mer", "des", "A\u00b7bends", "ge\u00b7f\u00fcllt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Doch schon weist sich die Noth im h\u00e4rtesten Kampf um ein Dasein,", "tokens": ["Doch", "schon", "weist", "sich", "die", "Noth", "im", "h\u00e4r\u00b7tes\u00b7ten", "Kampf", "um", "ein", "Da\u00b7sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+--+-++-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Das, des Athmens nicht werth, dennoch Befriedigung heischt.", "tokens": ["Das", ",", "des", "Ath\u00b7mens", "nicht", "werth", ",", "den\u00b7noch", "Be\u00b7frie\u00b7di\u00b7gung", "heischt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "PTKNEG", "ADJD", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Sieh nur die H\u00e4user! Neubauten mit rissigen, br\u00f6ckelnden Simsen;", "tokens": ["Sieh", "nur", "die", "H\u00e4u\u00b7ser", "!", "Neu\u00b7bau\u00b7ten", "mit", "ris\u00b7si\u00b7gen", ",", "br\u00f6\u00b7ckeln\u00b7den", "Sim\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$.", "NN", "APPR", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "In noch feuchtem Gela\u00df richtet das Elend sich ein.", "tokens": ["In", "noch", "feuch\u00b7tem", "Ge\u00b7la\u00df", "rich\u00b7tet", "das", "E\u00b7lend", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Nieder schl\u00e4gt sich der Rauch aus ragenden Schloten der Arbeit,", "tokens": ["Nie\u00b7der", "schl\u00e4gt", "sich", "der", "Rauch", "aus", "ra\u00b7gen\u00b7den", "Schlo\u00b7ten", "der", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Welche Maschinen zun\u00e4chst, aber auch H\u00e4nde verlangt.", "tokens": ["Wel\u00b7che", "Ma\u00b7schi\u00b7nen", "zu\u00b7n\u00e4chst", ",", "a\u00b7ber", "auch", "H\u00e4n\u00b7de", "ver\u00b7langt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADV", "$,", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "D\u00fcster f\u00e4rbt sie den Himmel, die Mauern, die Menschen und treibt sie", "tokens": ["D\u00fcs\u00b7ter", "f\u00e4rbt", "sie", "den", "Him\u00b7mel", ",", "die", "Mau\u00b7ern", ",", "die", "Men\u00b7schen", "und", "treibt", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Zu ingrimmigem Ha\u00df, weil sie verzehrt, nicht ern\u00e4hrt.", "tokens": ["Zu", "in\u00b7grim\u00b7mi\u00b7gem", "Ha\u00df", ",", "weil", "sie", "ver\u00b7zehrt", ",", "nicht", "er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "VVPP", "$,", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Blick' in die Buden und Schenken! Best\u00e4ubte, verdorbene Waaren,", "tokens": ["Blick'", "in", "die", "Bu\u00b7den", "und", "Schen\u00b7ken", "!", "Be\u00b7st\u00e4ub\u00b7te", ",", "ver\u00b7dor\u00b7be\u00b7ne", "Waa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "$.", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Die der Hunger verschlingt, wenn er zu zahlen vermag;", "tokens": ["Die", "der", "Hun\u00b7ger", "ver\u00b7schlingt", ",", "wenn", "er", "zu", "zah\u00b7len", "ver\u00b7mag", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Koste die Jauche des Bier's in tr\u00fcben und schartigen Gl\u00e4sern,", "tokens": ["Kos\u00b7te", "die", "Jau\u00b7che", "des", "Bier's", "in", "tr\u00fc\u00b7ben", "und", "schar\u00b7ti\u00b7gen", "Gl\u00e4\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Pr\u00fcfe den schillernden Wein, der nie die Kelter geseh'n!", "tokens": ["Pr\u00fc\u00b7fe", "den", "schil\u00b7lern\u00b7den", "Wein", ",", "der", "nie", "die", "Kel\u00b7ter", "ge\u00b7seh'n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Kann es verwundern, wenn endlich das Gift bet\u00e4ubenden Fusels", "tokens": ["Kann", "es", "ver\u00b7wun\u00b7dern", ",", "wenn", "end\u00b7lich", "das", "Gift", "be\u00b7t\u00e4u\u00b7ben\u00b7den", "Fu\u00b7sels"], "token_info": ["word", "word"