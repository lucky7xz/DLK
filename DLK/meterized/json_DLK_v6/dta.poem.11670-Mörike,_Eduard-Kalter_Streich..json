{"dta.poem.11670": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Kalter Streich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaub's; die lieben Eltern gegenw\u00e4rtig", "tokens": ["Ich", "glaub's", ";", "die", "lie\u00b7ben", "El\u00b7tern", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$.", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind selber ungemein davon charmirt,", "tokens": ["Sind", "sel\u00b7ber", "un\u00b7ge\u00b7mein", "da\u00b7von", "char\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn bei der s\u00fc\u00dfen Jugend allzeit' fertig", "tokens": ["Wenn", "bei", "der", "s\u00fc\u00b7\u00dfen", "Ju\u00b7gend", "all\u00b7zeit'", "fer\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Amor dem Hymen pr\u00e4numerirt.", "tokens": ["A\u00b7mor", "dem", "Hy\u00b7men", "pr\u00e4\u00b7nu\u00b7mer\u00b7irt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}