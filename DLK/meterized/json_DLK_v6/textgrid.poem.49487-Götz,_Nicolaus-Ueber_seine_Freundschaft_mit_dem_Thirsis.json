{"textgrid.poem.49487": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ueber seine Freundschaft mit dem Thirsis", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sasen wir beysammen", "tokens": ["Hier", "sa\u00b7sen", "wir", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am kleinen Wasserfall,", "tokens": ["Am", "klei\u00b7nen", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sangen unsre Flammen", "tokens": ["Und", "san\u00b7gen", "uns\u00b7re", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem blumenvollen Thal.", "tokens": ["Dem", "blu\u00b7men\u00b7vol\u00b7len", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die s\u00e4umende Narcisse,", "tokens": ["Die", "s\u00e4u\u00b7men\u00b7de", "Nar\u00b7cis\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bog, wo mein Thirsis sas,", "tokens": ["Bog", ",", "wo", "mein", "Thir\u00b7sis", "sas", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Beschwert durch Thr\u00e4neng\u00fcsse", "tokens": ["Be\u00b7schwert", "durch", "Thr\u00e4\u00b7nen\u00b7g\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das sch\u00f6ne Haupt ins Gras.", "tokens": ["Das", "sch\u00f6\u00b7ne", "Haupt", "ins", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sahet ihrs, ihr Heiden,", "tokens": ["Da", "sa\u00b7het", "ihrs", ",", "ihr", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich dr\u00fcckt ihm seine Hand,", "tokens": ["Ich", "dr\u00fcckt", "ihm", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wandt, reich an Pein und Freuden,", "tokens": ["Wandt", ",", "reich", "an", "Pein", "und", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Den Blick zum Vaterland,", "tokens": ["Den", "Blick", "zum", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sprach mit leisem Thone:", "tokens": ["Und", "sprach", "mit", "lei\u00b7sem", "Tho\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Tugend segne mich,", "tokens": ["Die", "Tu\u00b7gend", "seg\u00b7ne", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und gebe mir zum Lohne,", "tokens": ["Und", "ge\u00b7be", "mir", "zum", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein zarter Freund, nur dich.", "tokens": ["Mein", "zar\u00b7ter", "Freund", ",", "nur", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bekennen will ichs gerne,", "tokens": ["Be\u00b7ken\u00b7nen", "will", "ichs", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin nicht deiner werth,", "tokens": ["Ich", "bin", "nicht", "dei\u00b7ner", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch g\u00e4ben mir die Sterne,", "tokens": ["Doch", "g\u00e4\u00b7ben", "mir", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "(was ich zwar nie begehrt)", "tokens": ["(", "was", "ich", "zwar", "nie", "be\u00b7gehrt", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Glantz, Sch\u00f6nheit, hohe Gaben,", "tokens": ["Glantz", ",", "Sch\u00f6n\u00b7heit", ",", "ho\u00b7he", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was See, und Erdreich hat,", "tokens": ["Was", "See", ",", "und", "Er\u00b7dreich", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Sucht ich doch dich zu haben,", "tokens": ["Sucht", "ich", "doch", "dich", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dich nur fr\u00fch und spath.", "tokens": ["Und", "dich", "nur", "fr\u00fch", "und", "spath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, Freund, bey diesen Matten,", "tokens": ["Ja", ",", "Freund", ",", "bey", "die\u00b7sen", "Mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey meinen Z\u00e4hren hier,", "tokens": ["Bey", "mei\u00b7nen", "Z\u00e4h\u00b7ren", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unsrer V\u00e4ter Schatten", "tokens": ["Und", "uns\u00b7rer", "V\u00e4\u00b7ter", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bezeug und schw\u00f6r ich dir,", "tokens": ["Be\u00b7zeug", "und", "schw\u00f6r", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dir hab ich mich ergeben;", "tokens": ["Dir", "hab", "ich", "mich", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur dich lieb ich, n\u00e4chst Gott;", "tokens": ["Nur", "dich", "lieb", "ich", ",", "n\u00e4chst", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Darf ich bey dir nicht leben,", "tokens": ["Darf", "ich", "bey", "dir", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "So f\u00fchl ich stets den Tod.", "tokens": ["So", "f\u00fchl", "ich", "stets", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein g\u00fctiges Geschicke", "tokens": ["Ein", "g\u00fc\u00b7ti\u00b7ges", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verkn\u00fcpfte mich mit dir.", "tokens": ["Ver\u00b7kn\u00fcpf\u00b7te", "mich", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Leben ist mein Gl\u00fccke;", "tokens": ["Dein", "Le\u00b7ben", "ist", "mein", "Gl\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo du bist, da ist mir", "tokens": ["Wo", "du", "bist", ",", "da", "ist", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Himmel in der N\u00e4he.", "tokens": ["Der", "Him\u00b7mel", "in", "der", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch jedes K\u00f6rngen Zeit,", "tokens": ["Doch", "je\u00b7des", "K\u00f6rn\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wofern ich dich nicht sehe,", "tokens": ["Wo\u00b7fern", "ich", "dich", "nicht", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird mir zur Ewigkeit.", "tokens": ["Wird", "mir", "zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gesundheit, Kind des Himmels,", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Kind", "des", "Him\u00b7mels", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die auch der Weise sucht,", "tokens": ["Die", "auch", "der", "Wei\u00b7se", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und du, Feind des Get\u00fcmmels,", "tokens": ["Und", "du", ",", "Feind", "des", "Ge\u00b7t\u00fcm\u00b7mels", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schlaf, der Gesundheit Frucht,", "tokens": ["Schlaf", ",", "der", "Ge\u00b7sund\u00b7heit", "Frucht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ihr flieht vor meinen Blicken;", "tokens": ["Ihr", "flieht", "vor", "mei\u00b7nen", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O flieht, mit st\u00e4tem Flug!", "tokens": ["O", "flieht", ",", "mit", "st\u00e4\u00b7tem", "Flug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich ewig zu begl\u00fccken,", "tokens": ["Mich", "e\u00b7wig", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist Thirsis schon genug.", "tokens": ["Ist", "Thir\u00b7sis", "schon", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}