{"textgrid.poem.46617": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun la\u00df den Handel uns beendigen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun la\u00df den Handel uns beendigen,", "tokens": ["Nun", "la\u00df", "den", "Han\u00b7del", "uns", "be\u00b7en\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ist noch f\u00e4llig deinen Ordern?", "tokens": ["Was", "ist", "noch", "f\u00e4l\u00b7lig", "dei\u00b7nen", "Or\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Leicht ist's und kurz uns zu verst\u00e4ndigen;", "tokens": ["Leicht", "ist's", "und", "kurz", "uns", "zu", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu lassen hab' ich, du zu fordern.", "tokens": ["Zu", "las\u00b7sen", "hab'", "ich", ",", "du", "zu", "for\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nimm, was du noch willst nehmen, kann ich's wehren?", "tokens": ["Nimm", ",", "was", "du", "noch", "willst", "neh\u00b7men", ",", "kann", "ich's", "weh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du mir l\u00e4ssest, ist Gewinn.", "tokens": ["Was", "du", "mir", "l\u00e4s\u00b7sest", ",", "ist", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur, da\u00df ich mich einrichte zum Entbehren,", "tokens": ["Nur", ",", "da\u00df", "ich", "mich", "ein\u00b7rich\u00b7te", "zum", "Ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df mich erfahren bald, wie arm ich bin!", "tokens": ["La\u00df", "mich", "er\u00b7fah\u00b7ren", "bald", ",", "wie", "arm", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}