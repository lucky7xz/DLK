{"textgrid.poem.60581": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der wahre Muth", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit blindem Ungest\u00fcm, in zweifelhaften Schlachten,", "tokens": ["Mit", "blin\u00b7dem", "Un\u00b7ge\u00b7st\u00fcm", ",", "in", "zwei\u00b7fel\u00b7haf\u00b7ten", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die drohende Gefahr verachten,", "tokens": ["Die", "dro\u00b7hen\u00b7de", "Ge\u00b7fahr", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Dem Tod entgegen gehn, ist oft erkaufte Wuth,", "tokens": ["Dem", "Tod", "ent\u00b7ge\u00b7gen", "gehn", ",", "ist", "oft", "er\u00b7kauf\u00b7te", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVINF", "$,", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht Lorbeernwerther Heldenmuth.", "tokens": ["Nicht", "Lor\u00b7beern\u00b7wert\u00b7her", "Hel\u00b7den\u00b7muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort, wo die Menschheit schl\u00e4ft, in einer Welt von Wilden,", "tokens": ["Dort", ",", "wo", "die", "Menschheit", "schl\u00e4ft", ",", "in", "ei\u00b7ner", "Welt", "von", "Wil\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In Irokesischen Gefilden,", "tokens": ["In", "I\u00b7ro\u00b7ke\u00b7si\u00b7schen", "Ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Verstr\u00f6mt ein Barbar oft so freudig, als der Held,", "tokens": ["Ver\u00b7str\u00f6mt", "ein", "Bar\u00b7bar", "oft", "so", "freu\u00b7dig", ",", "als", "der", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Blut aufs Leichenvolle Feld.", "tokens": ["Sein", "Blut", "aufs", "Lei\u00b7chen\u00b7vol\u00b7le", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch soll ich wahren Muth mit g\u00fcldnen Saiten preisen,", "tokens": ["Doch", "soll", "ich", "wah\u00b7ren", "Muth", "mit", "g\u00fcld\u00b7nen", "Sai\u00b7ten", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo find ich ihn, als bey dem Weisen,", "tokens": ["Wo", "find", "ich", "ihn", ",", "als", "bey", "dem", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der mit Gelassenheit, nicht stoisch aufgebl\u00e4ht,", "tokens": ["Der", "mit", "Ge\u00b7las\u00b7sen\u00b7heit", ",", "nicht", "sto\u00b7isch", "auf\u00b7ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An sein bestimmtes Leiden geht?", "tokens": ["An", "sein", "be\u00b7stimm\u00b7tes", "Lei\u00b7den", "geht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Tod umschattet ihn mit schnellen Finsternissen,", "tokens": ["Der", "Tod", "um\u00b7schat\u00b7tet", "ihn", "mit", "schnel\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ruft unbewegt in seinen Schl\u00fcssen,", "tokens": ["Ruft", "un\u00b7be\u00b7wegt", "in", "sei\u00b7nen", "Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn aus der Freundschaft Arm, und aus der Liebe Schoos,", "tokens": ["Ihn", "aus", "der", "Freund\u00b7schaft", "Arm", ",", "und", "aus", "der", "Lie\u00b7be", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$,", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und findet ihn bereit und gro\u00df:", "tokens": ["Und", "fin\u00b7det", "ihn", "be\u00b7reit", "und", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gro\u00df, wann voll Furcht und Angst die K\u00f6nige der Erden", "tokens": ["Gro\u00df", ",", "wann", "voll", "Furcht", "und", "Angst", "die", "K\u00f6\u00b7ni\u00b7ge", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "NN", "KON", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So klein, als ihre Sklaven werden,", "tokens": ["So", "klein", ",", "als", "ih\u00b7re", "Skla\u00b7ven", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor dem tr\u00fcben Blick, gleich einem Traum, verfliegt,", "tokens": ["Und", "vor", "dem", "tr\u00fc\u00b7ben", "Blick", ",", "gleich", "ei\u00b7nem", "Traum", ",", "ver\u00b7fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was den betrognen Stolz vergn\u00fcgt.", "tokens": ["Was", "den", "be\u00b7trog\u00b7nen", "Stolz", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Als Held stirbt Sokrates, der f\u00fcr die Tugend leidet,", "tokens": ["Als", "Held", "stirbt", "Sok\u00b7ra\u00b7tes", ",", "der", "f\u00fcr", "die", "Tu\u00b7gend", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, wann er aus dem Leben scheidet,", "tokens": ["Und", ",", "wann", "er", "aus", "dem", "Le\u00b7ben", "schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein be\u00dfres Leben hofft, und seiner Ewigkeit", "tokens": ["Ein", "be\u00df\u00b7res", "Le\u00b7ben", "hofft", ",", "und", "sei\u00b7ner", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich, ihrer werth, entgegen freut.", "tokens": ["Sich", ",", "ih\u00b7rer", "werth", ",", "ent\u00b7ge\u00b7gen", "freut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "PPOSAT", "ADJD", "$,", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Athen hat ihn verdammt, die Wahrheit losgesprochen:", "tokens": ["A\u00b7then", "hat", "ihn", "ver\u00b7dammt", ",", "die", "Wahr\u00b7heit", "los\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sein letzter Tag ist angebrochen:", "tokens": ["Sein", "letz\u00b7ter", "Tag", "ist", "an\u00b7ge\u00b7bro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freunde stehn um ihn; ihr m\u00e4nnlich Auge weint", "tokens": ["Die", "Freun\u00b7de", "stehn", "um", "ihn", ";", "ihr", "m\u00e4nn\u00b7lich", "Au\u00b7ge", "weint"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um einen Lehrer, einen Freund.", "tokens": ["Um", "ei\u00b7nen", "Leh\u00b7rer", ",", "ei\u00b7nen", "Freund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er l\u00e4chelt: klagt ihr auch? Gerecht ist eure Klage,", "tokens": ["Er", "l\u00e4\u00b7chelt", ":", "klagt", "ihr", "auch", "?", "Ge\u00b7recht", "ist", "eu\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Sokrates an diesem Tage,", "tokens": ["Wenn", "Sok\u00b7ra\u00b7tes", "an", "die\u00b7sem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ganze Sokrates durch ckaltes Gift erbleicht,", "tokens": ["Der", "gan\u00b7ze", "Sok\u00b7ra\u00b7tes", "durch", "ckal\u00b7tes", "Gift", "er\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in sein erstes Nichts entweicht.", "tokens": ["Und", "in", "sein", "ers\u00b7tes", "Nichts", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich f\u00fchle, da\u00df in mir ein g\u00f6ttlich Etwas lodert,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "da\u00df", "in", "mir", "ein", "g\u00f6tt\u00b7lich", "Et\u00b7was", "lo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER", "ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das lebt, wann seine H\u00fclle modert!", "tokens": ["Das", "lebt", ",", "wann", "sei\u00b7ne", "H\u00fcl\u00b7le", "mo\u00b7dert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir lispelt die Natur jetzt lauter, als zuvor:", "tokens": ["Mir", "lis\u00b7pelt", "die", "Na\u00b7tur", "jetzt", "lau\u00b7ter", ",", "als", "zu\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist unsterblich! in das Ohr.", "tokens": ["Du", "bist", "uns\u00b7terb\u00b7lich", "!", "in", "das", "Ohr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Selbst meine Seele zeugt von ihrer hohen W\u00fcrde:", "tokens": ["Selbst", "mei\u00b7ne", "See\u00b7le", "zeugt", "von", "ih\u00b7rer", "ho\u00b7hen", "W\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst diese brennende Begierde", "tokens": ["Selbst", "die\u00b7se", "bren\u00b7nen\u00b7de", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nach Wahrheit, welche flieht, verh\u00fcllt in Dunkelheit,", "tokens": ["Nach", "Wahr\u00b7heit", ",", "wel\u00b7che", "flieht", ",", "ver\u00b7h\u00fcllt", "in", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist Ahndung der Unsterblichkeit.", "tokens": ["Ist", "Ahn\u00b7dung", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Wir steigen stuffenweis zu stets erhabnern Sph\u00e4ren:", "tokens": ["Wir", "stei\u00b7gen", "stuf\u00b7fen\u00b7weis", "zu", "stets", "er\u00b7hab\u00b7nern", "Sph\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lang die Pilgrimsjahre w\u00e4hren,", "tokens": ["So", "lang", "die", "Pil\u00b7grims\u00b7jah\u00b7re", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Irr ich im dunkeln Wald, wo zweifelhaftes Licht", "tokens": ["Irr", "ich", "im", "dun\u00b7keln", "Wald", ",", "wo", "zwei\u00b7fel\u00b7haf\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch dichte Zweige d\u00e4mmernd bricht.", "tokens": ["Durch", "dich\u00b7te", "Zwei\u00b7ge", "d\u00e4m\u00b7mernd", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Bald, bald wird mich der Tod, obgleich auf schwarzen Schwingen,", "tokens": ["Bald", ",", "bald", "wird", "mich", "der", "Tod", ",", "ob\u00b7gleich", "auf", "schwar\u00b7zen", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem hellern Auftritt bringen,", "tokens": ["Zu", "ei\u00b7nem", "hel\u00b7lern", "Auf\u00b7tritt", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ewiger Mittag, der nicht an Schatten gr\u00e4nzt,", "tokens": ["Wo", "e\u00b7wi\u00b7ger", "Mit\u00b7tag", ",", "der", "nicht", "an", "Schat\u00b7ten", "gr\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Voll Klarheit in die Seele gl\u00e4nzt.", "tokens": ["Voll", "Klar\u00b7heit", "in", "die", "See\u00b7le", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da jenseits meines Grabs ich weis' und gl\u00fccklich werde,", "tokens": ["Da", "jen\u00b7seits", "mei\u00b7nes", "Grabs", "ich", "weis'", "und", "gl\u00fcck\u00b7lich", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geh ich fr\u00f6hlich von der Erde.", "tokens": ["So", "geh", "ich", "fr\u00f6h\u00b7lich", "von", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor diesem dunkeln Weg beb' an des Lasters Brust", "tokens": ["Vor", "die\u00b7sem", "dun\u00b7keln", "Weg", "beb'", "an", "des", "Las\u00b7ters", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der feige Sklave niedrer Lust!", "tokens": ["Der", "fei\u00b7ge", "Skla\u00b7ve", "nie\u00b7drer", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die falschen Freuden fliehn, gleich den gescheuchten Schafen;", "tokens": ["Die", "fal\u00b7schen", "Freu\u00b7den", "fliehn", ",", "gleich", "den", "ge\u00b7scheuch\u00b7ten", "Scha\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihn erwarten schwere Strafen,", "tokens": ["Und", "ihn", "er\u00b7war\u00b7ten", "schwe\u00b7re", "Stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erwartet, nach dem Tod, die strenge Nemesis,", "tokens": ["Er\u00b7war\u00b7tet", ",", "nach", "dem", "Tod", ",", "die", "stren\u00b7ge", "Ne\u00b7me\u00b7sis", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Gegenden der Finsterni\u00df.", "tokens": ["In", "Ge\u00b7gen\u00b7den", "der", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Doch Seelen, die im Leib nicht blo\u00df dem Leibe lebten,", "tokens": ["Doch", "Se