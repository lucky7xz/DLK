{"textgrid.poem.46545": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gestorben sein, mu\u00df eine Wonne sein]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestorben sein, mu\u00df eine Wonne sein,", "tokens": ["Ge\u00b7stor\u00b7ben", "sein", ",", "mu\u00df", "ei\u00b7ne", "Won\u00b7ne", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sterben auch ist keine gro\u00dfe Pein;", "tokens": ["Zu", "ster\u00b7ben", "auch", "ist", "kei\u00b7ne", "gro\u00b7\u00dfe", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sterben schwerer ist es, sterben sehn", "tokens": ["Als", "ster\u00b7ben", "schwe\u00b7rer", "ist", "es", ",", "ster\u00b7ben", "sehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "VAFIN", "PPER", "$,", "VVINF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das, was man liebt, doch wird's vor\u00fcbergehn;", "tokens": ["Das", ",", "was", "man", "liebt", ",", "doch", "wird's", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sie uns dann nur Ruhe lie\u00dfen haben,", "tokens": ["Wenn", "sie", "uns", "dann", "nur", "Ru\u00b7he", "lie\u00b7\u00dfen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "NN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn nicht das Aergste w\u00e4re das Begraben;", "tokens": ["Wenn", "nicht", "das", "A\u00b7ergs\u00b7te", "w\u00e4\u00b7re", "das", "Be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wo von Zudr\u00e4ngern r\u00fcckt, von M\u00fc\u00dfigg\u00e4ngern", "tokens": ["Wo", "von", "Zu\u00b7dr\u00e4n\u00b7gern", "r\u00fcckt", ",", "von", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Heer einher, die Qual dir zu verl\u00e4ngern,", "tokens": ["Ein", "Heer", "ein\u00b7her", ",", "die", "Qual", "dir", "zu", "ver\u00b7l\u00e4n\u00b7gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die zur Hinrichtung macht den Leichenzug,", "tokens": ["Die", "zur", "Hin\u00b7rich\u00b7tung", "macht", "den", "Lei\u00b7chen\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.10": {"text": "Bis endlich die Erl\u00f6sungsglock' anschlug,", "tokens": ["Bis", "end\u00b7lich", "die", "Er\u00b7l\u00f6\u00b7sungs\u00b7glock'", "an\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df man dein Liebstes nur trag' aus dem Haus,", "tokens": ["Da\u00df", "man", "dein", "Liebs\u00b7tes", "nur", "trag'", "aus", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Das du nun selber w\u00fcnschen mu\u00dft hinaus,", "tokens": ["Das", "du", "nun", "sel\u00b7ber", "w\u00fcn\u00b7schen", "mu\u00dft", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df aus nur sei, nur aus", "tokens": ["Da\u00df", "aus", "nur", "sei", ",", "nur", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "VAFIN", "$,", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Das Gr\u00e4\u00dfliche, der Leichenschmaus,", "tokens": ["Das", "Gr\u00e4\u00df\u00b7li\u00b7che", ",", "der", "Lei\u00b7chen\u00b7schmaus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Was schauderhaft dein Innerstes emp\u00f6rt,", "tokens": ["Was", "schau\u00b7der\u00b7haft", "dein", "In\u00b7ners\u00b7tes", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Dir die Besinnung, dir die Andacht st\u00f6rt,", "tokens": ["Dir", "die", "Be\u00b7sin\u00b7nung", ",", "dir", "die", "An\u00b7dacht", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.17": {"text": "Die dieser Stunde wohl geh\u00f6rt;", "tokens": ["Die", "die\u00b7ser", "Stun\u00b7de", "wohl", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als h\u00e4tten wir f\u00fcr Thoren", "tokens": ["Als", "h\u00e4t\u00b7ten", "wir", "f\u00fcr", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Gezeugt nur und geboren,", "tokens": ["Ge\u00b7zeugt", "nur", "und", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Erzogen und verloren", "tokens": ["Er\u00b7zo\u00b7gen", "und", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ein Kind, damit sie m\u00f6chten fein versuchen", "tokens": ["Ein", "Kind", ",", "da\u00b7mit", "sie", "m\u00f6ch\u00b7ten", "fein", "ver\u00b7su\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die Kindtaufs- erst und nun die Leichenkuchen!", "tokens": ["Die", "Kind\u00b7taufs", "erst", "und", "nun", "die", "Lei\u00b7chen\u00b7ku\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Still, bete, Herz, damit du nicht mu\u00dft fluchen!", "tokens": ["Still", ",", "be\u00b7te", ",", "Herz", ",", "da\u00b7mit", "du", "nicht", "mu\u00dft", "flu\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}