{"textgrid.poem.56670": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Stein", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du mir das Symbolum der Welt,", "tokens": ["So", "bist", "du", "mir", "das", "Sym\u00b7bo\u00b7lum", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein Zwitter frostiger Erhabenheit", "tokens": ["ein", "Zwit\u00b7ter", "fros\u00b7ti\u00b7ger", "Er\u00b7ha\u00b7ben\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und zynisch schweigender Gleichg\u00fcltigkeit;", "tokens": ["und", "zy\u00b7nisch", "schwei\u00b7gen\u00b7der", "Gleich\u00b7g\u00fcl\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gef\u00e4hrlich nahe schon dem Nichts gesellt", "tokens": ["ge\u00b7f\u00e4hr\u00b7lich", "na\u00b7he", "schon", "dem", "Nichts", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "hast du dich auf den h\u00f6chsten Stolz gestellt", "tokens": ["hast", "du", "dich", "auf", "den", "h\u00f6chs\u00b7ten", "Stolz", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und hebst dich herrisch aus dem Strom der Zeit", "tokens": ["und", "hebst", "dich", "her\u00b7risch", "aus", "dem", "Strom", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und \u00fcber des Geschehns Formlosigkeit", "tokens": ["und", "\u00fc\u00b7ber", "des", "Ge\u00b7schehns", "Form\u00b7lo\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bleibst du der Einzige, der Form beh\u00e4lt.", "tokens": ["bleibst", "du", "der", "Ein\u00b7zi\u00b7ge", ",", "der", "Form", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Oh kalten Gleichmuts lautberedter Hohn,", "tokens": ["Oh", "kal\u00b7ten", "Gleich\u00b7muts", "laut\u00b7be\u00b7red\u00b7ter", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des Unbegreifbarn greifbare Erscheinung", "tokens": ["des", "Un\u00b7be\u00b7greif\u00b7barn", "greif\u00b7ba\u00b7re", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hast du gepre\u00dft in einen Klumpen Ton", "tokens": ["hast", "du", "ge\u00b7pre\u00dft", "in", "ei\u00b7nen", "Klum\u00b7pen", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und \u2013 nur ein Ding, ein Nichts in unsrer Meinung", "tokens": ["und", "\u2013", "nur", "ein", "Ding", ",", "ein", "Nichts", "in", "uns\u00b7rer", "Mei\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "ADV", "ART", "NN", "$,", "ART", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "stehst du auf deiner Weisheit kahlem Thron", "tokens": ["stehst", "du", "auf", "dei\u00b7ner", "Weis\u00b7heit", "kah\u00b7lem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als dieser Welt sarkastischste Verneinung.", "tokens": ["als", "die\u00b7ser", "Welt", "sar\u00b7kas\u00b7tischs\u00b7te", "Ver\u00b7nei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}