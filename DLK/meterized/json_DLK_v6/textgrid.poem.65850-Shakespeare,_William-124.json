{"textgrid.poem.65850": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "124", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist meine Liebe nur ein Kind der Welt,", "tokens": ["Ist", "mei\u00b7ne", "Lie\u00b7be", "nur", "ein", "Kind", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann nennt sie Gl\u00fccksbastard, den nie ein Vater k\u00fc\u00dft,", "tokens": ["Dann", "nennt", "sie", "Gl\u00fccks\u00b7bas\u00b7tard", ",", "den", "nie", "ein", "Va\u00b7ter", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Zeiten Ha\u00df und Liebe blo\u00dfgestellt,", "tokens": ["Der", "Zei\u00b7ten", "Ha\u00df", "und", "Lie\u00b7be", "blo\u00df\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie man den Dorn zum Dorn, zu Rosen Rosen liest.", "tokens": ["Wie", "man", "den", "Dorn", "zum", "Dorn", ",", "zu", "Ro\u00b7sen", "Ro\u00b7sen", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein, sie erwuchs vom Zufall fern: ihr droht", "tokens": ["Nein", ",", "sie", "er\u00b7wuchs", "vom", "Zu\u00b7fall", "fern", ":", "ihr", "droht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein prunkend L\u00e4cheln, nicht zu Boden schlagen", "tokens": ["Kein", "prun\u00b7kend", "L\u00e4\u00b7cheln", ",", "nicht", "zu", "Bo\u00b7den", "schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "$,", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kann sie mit finstern Mienen ein Despot,", "tokens": ["Kann", "sie", "mit", "fins\u00b7tern", "Mie\u00b7nen", "ein", "Des\u00b7pot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie tr\u00e4gt nicht Fesseln, wie die Zeiten tragen.", "tokens": ["Sie", "tr\u00e4gt", "nicht", "Fes\u00b7seln", ",", "wie", "die", "Zei\u00b7ten", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht f\u00fcrchtet sie den Ketzer Politik,", "tokens": ["Nicht", "f\u00fcrch\u00b7tet", "sie", "den", "Ket\u00b7zer", "Po\u00b7li\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der kurzgeme\u00dfner Stunden Sold erkeuchet;", "tokens": ["Der", "kurz\u00b7ge\u00b7me\u00df\u00b7ner", "Stun\u00b7den", "Sold", "er\u00b7keu\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie selber ist der Staatskunst Meisterst\u00fcck,", "tokens": ["Sie", "sel\u00b7ber", "ist", "der", "Staats\u00b7kunst", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das weder W\u00e4rme n\u00e4hrt noch Regen beuget. \u2013", "tokens": ["Das", "we\u00b7der", "W\u00e4r\u00b7me", "n\u00e4hrt", "noch", "Re\u00b7gen", "beu\u00b7get", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "KON", "NN", "VVFIN", "ADV", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ihr Narr'n der Zeit, dies wi\u00dft ihr und bezeugt;", "tokens": ["Ihr", "Narr'n", "der", "Zeit", ",", "dies", "wi\u00dft", "ihr", "und", "be\u00b7zeugt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die ihr f\u00fcr Laster lebt, f\u00fcr Tugenden erbleicht.", "tokens": ["Die", "ihr", "f\u00fcr", "Las\u00b7ter", "lebt", ",", "f\u00fcr", "Tu\u00b7gen\u00b7den", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}