{"dta.poem.19936": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenblicke im Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was bringet mir den alten Muth", "tokens": ["Was", "brin\u00b7get", "mir", "den", "al\u00b7ten", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Mitten meines Lebens Tr\u00fcbe?", "tokens": ["In", "Mit\u00b7ten", "mei\u00b7nes", "Le\u00b7bens", "Tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sinn' und wei\u00df nicht, wer es thut,", "tokens": ["Ich", "sinn'", "und", "wei\u00df", "nicht", ",", "wer", "es", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was wieder weckt des Lebens Liebe.", "tokens": ["Was", "wie\u00b7der", "weckt", "des", "Le\u00b7bens", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Erde, wei\u00df ich, ist es nicht,", "tokens": ["Die", "Er\u00b7de", ",", "wei\u00df", "ich", ",", "ist", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Hoffnung ist's, die mich begl\u00fccket,", "tokens": ["Nicht", "Hoff\u00b7nung", "ist's", ",", "die", "mich", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist des Himmels liebes Licht,", "tokens": ["Es", "ist", "des", "Him\u00b7mels", "lie\u00b7bes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das einmal wieder mailich blicket.", "tokens": ["Das", "ein\u00b7mal", "wie\u00b7der", "mai\u00b7lich", "bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}