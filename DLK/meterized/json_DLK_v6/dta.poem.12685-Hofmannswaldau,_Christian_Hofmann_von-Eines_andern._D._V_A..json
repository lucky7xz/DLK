{"dta.poem.12685": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines andern.  \n  D.  V A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kan nicht anders seyn; Adonis mu\u00df noch leben.", "tokens": ["Es", "kan", "nicht", "an\u00b7ders", "seyn", ";", "A\u00b7do\u00b7nis", "mu\u00df", "noch", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$.", "NE", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Venus dieser stadt steht keinem andern an.", "tokens": ["Die", "Ve\u00b7nus", "die\u00b7ser", "stadt", "steht", "kei\u00b7nem", "an\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "PIAT", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ist es, der auch wol was anders dencken kan?", "tokens": ["Wer", "ist", "es", ",", "der", "auch", "wol", "was", "an\u00b7ders", "den\u00b7cken", "kan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die g\u00f6ttin wird ihm ja vor g\u00f6ttern selbst gegeben.", "tokens": ["Die", "g\u00f6t\u00b7tin", "wird", "ihm", "ja", "vor", "g\u00f6t\u00b7tern", "selbst", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}