{"dta.poem.18383": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich vor dem blitz entsetzte.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein auge/ das nur steckt voll list und voll gefahr/", "tokens": ["Dein", "au\u00b7ge", "/", "das", "nur", "steckt", "voll", "list", "und", "voll", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "ADV", "VVFIN", "ADJD", "NN", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit geschwinder glut mein d\u00fcrres hertz bezwinget/", "tokens": ["Das", "mit", "ge\u00b7schwin\u00b7der", "glut", "mein", "d\u00fcr\u00b7res", "hertz", "be\u00b7zwin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ohne rettung mich fast zur verzweiflung bringet/", "tokens": ["Und", "oh\u00b7ne", "ret\u00b7tung", "mich", "fast", "zur", "ver\u00b7zwei\u00b7flung", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lacht meiner doch darzu gantz frey und offenbar.", "tokens": ["Lacht", "mei\u00b7ner", "doch", "dar\u00b7zu", "gantz", "frey", "und", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "PAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach? aber nim bey dir es selber einmahl wahr/", "tokens": ["Ach", "?", "a\u00b7ber", "nim", "bey", "dir", "es", "sel\u00b7ber", "ein\u00b7mahl", "wahr", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "APPR", "PPER", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann blitz und hei\u00dfes feur dir ins gesichte dringet/", "tokens": ["Wann", "blitz", "und", "hei\u00b7\u00dfes", "feur", "dir", "ins", "ge\u00b7sich\u00b7te", "drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJA", "NN", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob du nicht wirst mit angst und bleicher furcht umbringet/", "tokens": ["Ob", "du", "nicht", "wirst", "mit", "angst", "und", "blei\u00b7cher", "furcht", "um\u00b7brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "APPR", "VVPP", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deine sicherheit bald suchest hier/ bald dar;", "tokens": ["Und", "dei\u00b7ne", "si\u00b7cher\u00b7heit", "bald", "su\u00b7chest", "hier", "/", "bald", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "ADV", "$(", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wisse dann vielmehr/ da\u00df ich noch schwerer lei\u016f/", "tokens": ["So", "wis\u00b7se", "dann", "viel\u00b7mehr", "/", "da\u00df", "ich", "noch", "schwe\u00b7rer", "lei\u016f", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$(", "KOUS", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und da\u00df der blitz/ der dir aus schwartzen wolcken dr\u00e4ut/", "tokens": ["Und", "da\u00df", "der", "blitz", "/", "der", "dir", "aus", "schwart\u00b7zen", "wol\u00b7cken", "dr\u00e4ut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur sey ein blosses nichts/ das in der lufft verschwindet;", "tokens": ["Nur", "sey", "ein", "blos\u00b7ses", "nichts", "/", "das", "in", "der", "lufft", "ver\u00b7schwin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PIS", "$(", "ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hingegen dieser/ der aus deinen augen f\u00e4hrt/", "tokens": ["Hin\u00b7ge\u00b7gen", "die\u00b7ser", "/", "der", "aus", "dei\u00b7nen", "au\u00b7gen", "f\u00e4hrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$(", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Ist ein durchdringend feur/ das marck und blut verzehrt/", "tokens": ["Ist", "ein", "durch\u00b7drin\u00b7gend", "feur", "/", "das", "marck", "und", "blut", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "PTKVZ", "$(", "ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und durch verborgne krafft die Seele selbst entz\u00fcndet.", "tokens": ["Und", "durch", "ver\u00b7borg\u00b7ne", "krafft", "die", "See\u00b7le", "selbst", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}