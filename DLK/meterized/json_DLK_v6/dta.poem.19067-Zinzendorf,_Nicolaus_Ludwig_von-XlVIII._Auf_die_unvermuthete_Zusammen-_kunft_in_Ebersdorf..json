{"dta.poem.19067": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.   Auf die unvermuthete Zusammen-  \n kunft in Ebersdorf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "O Liebe! wunderbares Gut,", "tokens": ["O", "Lie\u00b7be", "!", "wun\u00b7der\u00b7ba\u00b7res", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was giebst du denen nicht zu schmecken,", "tokens": ["Was", "giebst", "du", "de\u00b7nen", "nicht", "zu", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sich durch deine Liebes-Glut,", "tokens": ["Die", "sich", "durch", "dei\u00b7ne", "Lie\u00b7bes\u00b7Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir nachzufolgen, lassen wecken!", "tokens": ["Dir", "nach\u00b7zu\u00b7fol\u00b7gen", ",", "las\u00b7sen", "we\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lieblich wirst du nicht erkannt", "tokens": ["Wie", "lieb\u00b7lich", "wirst", "du", "nicht", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von allen, die dich je gef\u00fchlet,", "tokens": ["Von", "al\u00b7len", ",", "die", "dich", "je", "ge\u00b7f\u00fch\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und derer Geist aufs Vaterland", "tokens": ["Und", "de\u00b7rer", "Geist", "aufs", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Di\u00df unsichtbare Reich gezielet!", "tokens": ["Di\u00df", "un\u00b7sicht\u00b7ba\u00b7re", "Reich", "ge\u00b7zie\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie k\u00f6nnen deinen Rath", "tokens": ["Sie", "k\u00f6n\u00b7nen", "dei\u00b7nen", "Rath"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "In mancher grossen That", "tokens": ["In", "man\u00b7cher", "gros\u00b7sen", "That"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "So wunderbar, so seltsam mercken!", "tokens": ["So", "wun\u00b7der\u00b7bar", ",", "so", "selt\u00b7sam", "mer\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Doch pflegst du ihren Muth,", "tokens": ["Doch", "pflegst", "du", "ih\u00b7ren", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was nicht die Liebe thut!", "tokens": ["Was", "nicht", "die", "Lie\u00b7be", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Durch Kleinigkeiten auch zu st\u00e4rcken.", "tokens": ["Durch", "Klei\u00b7nig\u00b7kei\u00b7ten", "auch", "zu", "st\u00e4r\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du auserkohrner Seelen-Freund,", "tokens": ["Du", "au\u00b7ser\u00b7kohr\u00b7ner", "See\u00b7len\u00b7Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Einfalts-volle treue Liebe,", "tokens": ["Du", "Ein\u00b7falts\u00b7vol\u00b7le", "treu\u00b7e", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sehr dein Wesen schlecht erscheint,", "tokens": ["So", "sehr", "dein", "We\u00b7sen", "schlecht", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ungek\u00fcnstelt deine Triebe;", "tokens": ["So", "un\u00b7ge\u00b7k\u00fcns\u00b7telt", "dei\u00b7ne", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bist du doch zu gleicher Zeit", "tokens": ["So", "bist", "du", "doch", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein GOtt der Ordnung, Maa\u00df und Zieles.", "tokens": ["Ein", "Gott", "der", "Ord\u00b7nung", ",", "Maa\u00df", "und", "Zie\u00b7les", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Menschen Unbesonnenheit", "tokens": ["Der", "Men\u00b7schen", "Un\u00b7be\u00b7son\u00b7nen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vers\u00e4umt und \u00fcbergehet vieles,", "tokens": ["Ver\u00b7s\u00e4umt", "und", "\u00fc\u00b7berg\u00b7e\u00b7het", "vie\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Uberlegung braucht,", "tokens": ["Das", "U\u00b7ber\u00b7le\u00b7gung", "braucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und uns erstaunlich daucht,", "tokens": ["Und", "uns", "er\u00b7staun\u00b7lich", "daucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "So bald wirs ehrerbietig messen.", "tokens": ["So", "bald", "wirs", "ehr\u00b7er\u00b7bie\u00b7tig", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da uns nun dieser Tag", "tokens": ["Da", "uns", "nun", "die\u00b7ser", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "So seltsam scheinen mag:", "tokens": ["So", "selt\u00b7sam", "schei\u00b7nen", "mag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Wer wolte deines Raths vergessen?", "tokens": ["Wer", "wol\u00b7te", "dei\u00b7nes", "Raths", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lange man noch immer will,", "tokens": ["So", "lan\u00b7ge", "man", "noch", "im\u00b7mer", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lange mag man sich bek\u00fcmmern.", "tokens": ["So", "lan\u00b7ge", "mag", "man", "sich", "be\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum wird das Hertz vom W\u00fcrcken still,", "tokens": ["Kaum", "wird", "das", "Hertz", "vom", "W\u00fcr\u00b7cken", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4ngt GOtt an unser Gl\u00fcck zu zimmern.", "tokens": ["F\u00e4ngt", "Gott", "an", "un\u00b7ser", "Gl\u00fcck", "zu", "zim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der allerangenehmste Blick,", "tokens": ["Der", "al\u00b7ler\u00b7an\u00b7ge\u00b7nehms\u00b7te", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den wir erd\u00fcrstet und erdrungen,", "tokens": ["Den", "wir", "er\u00b7d\u00fcrs\u00b7tet", "und", "er\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Giebt wenig Lieblichkeit zur\u00fcck,", "tokens": ["Giebt", "we\u00b7nig", "Lieb\u00b7lich\u00b7keit", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und scheint uns allzusehr gezwungen,", "tokens": ["Und", "scheint", "uns", "all\u00b7zu\u00b7sehr", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was unser eigner Rath mit M\u00fch ersonnen hat,", "tokens": ["Was", "un\u00b7ser", "eig\u00b7ner", "Rath", "mit", "M\u00fch", "er\u00b7son\u00b7nen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was unsre eigne Faust erk\u00e4mpfet,", "tokens": ["Was", "uns\u00b7re", "eig\u00b7ne", "Faust", "er\u00b7k\u00e4mp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "F\u00fchlt ein geschw\u00e4chter Geist,", "tokens": ["F\u00fchlt", "ein", "ge\u00b7schw\u00e4ch\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Hand ist matt, und schwei\u00dft,", "tokens": ["Die", "Hand", "ist", "matt", ",", "und", "schwei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Drum ist die Armuth sehr ged\u00e4mpfet.", "tokens": ["Drum", "ist", "die", "Ar\u00b7muth", "sehr", "ge\u00b7d\u00e4mp\u00b7fet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Freude, die der Freuden-Quell", "tokens": ["Die", "Freu\u00b7de", ",", "die", "der", "Freu\u00b7den\u00b7Quell"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns diesen Abend \u00fcber g\u00f6nnet,", "tokens": ["Uns", "die\u00b7sen", "A\u00b7bend", "\u00fc\u00b7ber", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat denn erst ihre rechte Stell,", "tokens": ["Hat", "denn", "erst", "ih\u00b7re", "rech\u00b7te", "Stell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie auf Hertz-Alt\u00e4ren brennet:", "tokens": ["Wenn", "sie", "auf", "Hertz\u00b7Al\u00b7t\u00e4\u00b7ren", "bren\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da wird das allerh\u00f6chste Gut", "tokens": ["Da", "wird", "das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In allen Gaben recht geschmeck\u2019t,", "tokens": ["In", "al\u00b7len", "Ga\u00b7ben", "recht", "ge\u00b7schmeck't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allda wird der geheimste Muth", "tokens": ["All\u00b7da", "wird", "der", "ge\u00b7heims\u00b7te", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "In Lieb entflammt, zum Lob erweck\u2019t;", "tokens": ["In", "Lieb", "ent\u00b7flammt", ",", "zum", "Lob", "er\u00b7weck't", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die dem Jmmanuel zur Magd erkaufte Seel", "tokens": ["Die", "dem", "Jm\u00b7ma\u00b7nu\u00b7el", "zur", "Magd", "er\u00b7kauf\u00b7te", "Seel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Eilt aus der W\u00fcsten ihrer Stille,", "tokens": ["Eilt", "aus", "der", "W\u00fcs\u00b7ten", "ih\u00b7rer", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Steigt auf nach Geistes Brauch,", "tokens": ["Steigt", "auf", "nach", "Geis\u00b7tes", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Als ein gerader Rauch,", "tokens": ["Als", "ein", "ge\u00b7ra\u00b7der", "Rauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Jhr Liebes-Ernst steht in der F\u00fclle.", "tokens": ["Ihr", "Lie\u00b7bes\u00b7Ernst", "steht", "in", "der", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur unsre Hertzen sollen sich", "tokens": ["Nur", "uns\u00b7re", "Hert\u00b7zen", "sol\u00b7len", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An diesem Abende verbinden,", "tokens": ["An", "die\u00b7sem", "A\u00b7ben\u00b7de", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Jhr Gut und Wollust ewiglich", "tokens": ["Ihr", "Gut", "und", "Wol\u00b7lust", "e\u00b7wig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+"