{"textgrid.poem.47208": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist vergangen, eh' ich's gedacht,", "tokens": ["Du", "bist", "ver\u00b7gan\u00b7gen", ",", "eh'", "ich's", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie eine Blume verbl\u00fcht \u00fcber Nacht.", "tokens": ["Wie", "ei\u00b7ne", "Blu\u00b7me", "ver\u00b7bl\u00fcht", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie eine Blum' \u00fcber Nacht verbl\u00fcht,", "tokens": ["Wie", "ei\u00b7ne", "Blum'", "\u00fc\u00b7ber", "Nacht", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf die umsonst der Fr\u00fchtau spr\u00fcht.", "tokens": ["Auf", "die", "um\u00b7sonst", "der", "Fr\u00fch\u00b7tau", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es spr\u00fcht umsonst der fr\u00fche Tau,", "tokens": ["Es", "spr\u00fcht", "um\u00b7sonst", "der", "fr\u00fc\u00b7he", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie auf dich meine Thr\u00e4nen lau.", "tokens": ["Wie", "auf", "dich", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "lau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es spr\u00fchn meine Thr\u00e4nen lau auf dich,", "tokens": ["Es", "spr\u00fchn", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "lau", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und du bist nicht erwacht f\u00fcr mich.", "tokens": ["Und", "du", "bist", "nicht", "er\u00b7wacht", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und du bist nicht f\u00fcr mich erwacht,", "tokens": ["Und", "du", "bist", "nicht", "f\u00fcr", "mich", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Meine Blume, verbl\u00fcht \u00fcber Nacht!", "tokens": ["Mei\u00b7ne", "Blu\u00b7me", ",", "ver\u00b7bl\u00fcht", "\u00fc\u00b7ber", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}