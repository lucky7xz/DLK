{"textgrid.poem.59439": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Staunend auf den G\u00f6ttersitzen", "tokens": ["Stau\u00b7nend", "auf", "den", "G\u00f6t\u00b7ter\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Unsterblichen nun stehn,", "tokens": ["Die", "U\u00b7nsterb\u00b7li\u00b7chen", "nun", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sehn den Morgen dr\u00fcben blitzen,", "tokens": ["Sehn", "den", "Mor\u00b7gen", "dr\u00fc\u00b7ben", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlen Duft her\u00fcberwehn,", "tokens": ["F\u00fch\u00b7len", "Duft", "her\u00b7\u00fc\u00b7berw\u00b7ehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so s\u00fc\u00dfes Weh sie sp\u00fcren,", "tokens": ["Und", "so", "s\u00fc\u00b7\u00dfes", "Weh", "sie", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00f6sen leis ihr Schiff vom Strand,", "tokens": ["L\u00f6\u00b7sen", "leis", "ihr", "Schiff", "vom", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die L\u00fcfte sie verf\u00fchren", "tokens": ["Und", "die", "L\u00fcf\u00b7te", "sie", "ver\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Fern durchs Meer zum jungen Land.", "tokens": ["Fern", "durchs", "Meer", "zum", "jun\u00b7gen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie da die Quellen sprangen", "tokens": ["O", "wie", "da", "die", "Quel\u00b7len", "spran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die tiefe Bl\u00fctenpracht", "tokens": ["In", "die", "tie\u00b7fe", "Bl\u00fc\u00b7ten\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Lianen dort sich schlangen", "tokens": ["Und", "Li\u00b7a\u00b7nen", "dort", "sich", "schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "PRF", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gl\u00fchend durch die Waldesnacht!", "tokens": ["Gl\u00fc\u00b7hend", "durch", "die", "Wal\u00b7des\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Wandrer trunken lauschen,", "tokens": ["Und", "die", "Wand\u00b7rer", "trun\u00b7ken", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Wasserf\u00e4lle gehn,", "tokens": ["Wo", "die", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis sie in dem Fr\u00fchlingsrauschen", "tokens": ["Bis", "sie", "in", "dem", "Fr\u00fch\u00b7lings\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pl\u00f6tzlich all erschrocken stehn:", "tokens": ["Pl\u00f6tz\u00b7lich", "all", "er\u00b7schro\u00b7cken", "stehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn sie sehn zum ersten Male", "tokens": ["Denn", "sie", "sehn", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun die Sonne niedergehn", "tokens": ["Nun", "die", "Son\u00b7ne", "nie\u00b7der\u00b7gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verwundert Berg' und Tale", "tokens": ["Und", "ver\u00b7wun\u00b7dert", "Ber\u00b7g'", "und", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tief im Abendrote stehn,", "tokens": ["Tief", "im", "A\u00b7ben\u00b7dro\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der sch\u00f6nste Gott von allen", "tokens": ["Und", "der", "sch\u00f6ns\u00b7te", "Gott", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sank erbleichend in den Duft,", "tokens": ["Sank", "er\u00b7blei\u00b7chend", "in", "den", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn dem Tode ist verfallen,", "tokens": ["Denn", "dem", "To\u00b7de", "ist", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer geatmet ird'sche Luft.", "tokens": ["Wer", "ge\u00b7at\u00b7met", "ird'\u00b7sche", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Genossen fa\u00dft ein Grauen,", "tokens": ["Die", "Ge\u00b7nos\u00b7sen", "fa\u00dft", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie fahren weit ins Meer,", "tokens": ["Und", "sie", "fah\u00b7ren", "weit", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach des Vaters Haus sie schauen,", "tokens": ["Nach", "des", "Va\u00b7ters", "Haus", "sie", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch sie finden's nimmermehr.", "tokens": ["Doch", "sie", "fin\u00b7den's", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00dften aus den Wogenw\u00fcsten", "tokens": ["Mu\u00df\u00b7ten", "aus", "den", "Wo\u00b7gen\u00b7w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrer Schiffe Schn\u00e4bel drehn", "tokens": ["Ih\u00b7rer", "Schif\u00b7fe", "Schn\u00e4\u00b7bel", "drehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieder nach des Eilands K\u00fcsten,", "tokens": ["Wie\u00b7der", "nach", "des", "Ei\u00b7lands", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, das war so falsch und sch\u00f6n!", "tokens": ["Ach", ",", "das", "war", "so", "falsch", "und", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und f\u00fcr immer da verschlagen", "tokens": ["Und", "f\u00fcr", "im\u00b7mer", "da", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blieben sie im fremden Land,", "tokens": ["Blie\u00b7ben", "sie", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rten nachts des Vaters Klagen", "tokens": ["H\u00f6r\u00b7ten", "nachts", "des", "Va\u00b7ters", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft noch fern vom G\u00f6tterstrand. \u2013", "tokens": ["Oft", "noch", "fern", "vom", "G\u00f6t\u00b7ter\u00b7strand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nun Kindeskinder m\u00fcssen", "tokens": ["Und", "nun", "Kin\u00b7des\u00b7kin\u00b7der", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der Heimat sehn ins Meer,", "tokens": ["Nach", "der", "Hei\u00b7mat", "sehn", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es kommt im Wind ein Gr\u00fc\u00dfen,", "tokens": ["Und", "es", "kommt", "im", "Wind", "ein", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie wissen nicht woher.", "tokens": ["Und", "sie", "wis\u00b7sen", "nicht", "wo\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}