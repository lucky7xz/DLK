{"dta.poem.13007": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leanders gegen-antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer den geschickten kiel so fl\u00fcchtig wei\u00df zu f\u00fchren,", "tokens": ["Wer", "den", "ge\u00b7schick\u00b7ten", "kiel", "so", "fl\u00fcch\u00b7tig", "wei\u00df", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeigt, da\u00df sein hertze nicht gantz unbeweglich sey.", "tokens": ["Zeigt", ",", "da\u00df", "sein", "hert\u00b7ze", "nicht", "gantz", "un\u00b7be\u00b7weg\u00b7lich", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wiewol, Florette l\u00e4st mehr h\u00e4rt als schw\u00e4che sp\u00fcren,", "tokens": ["Wie\u00b7wol", ",", "Flo\u00b7ret\u00b7te", "l\u00e4st", "mehr", "h\u00e4rt", "als", "schw\u00e4\u00b7che", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Berelis\u2019 und du sind gar nicht einerley.", "tokens": ["Denn", "Be\u00b7re\u00b7lis'", "und", "du", "sind", "gar", "nicht", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie war ein weiches wachs, du wilst dem marmel gleichen;", "tokens": ["Sie", "war", "ein", "wei\u00b7ches", "wachs", ",", "du", "wilst", "dem", "mar\u00b7mel", "glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch zeit und regen hat auch marmel ausgeh\u00f6lt.", "tokens": ["Doch", "zeit", "und", "re\u00b7gen", "hat", "auch", "mar\u00b7mel", "aus\u00b7ge\u00b7h\u00f6lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "VAFIN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und endlich kan der mensch nicht aller regung weichen,", "tokens": ["Und", "end\u00b7lich", "kan", "der", "mensch", "nicht", "al\u00b7ler", "re\u00b7gung", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "PTKNEG", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ihn der himmel selbst mit ihrer glut beseelt.", "tokens": ["Weil", "ihn", "der", "him\u00b7mel", "selbst", "mit", "ih\u00b7rer", "glut", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer allzu leichte glaubt, geht freylich allzu sicher.", "tokens": ["Wer", "all\u00b7zu", "leich\u00b7te", "glaubt", ",", "geht", "frey\u00b7lich", "all\u00b7zu", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "ADJA", "VVFIN", "$,", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die hertzen stimmen nicht stets mit den lippen ein.", "tokens": ["Die", "hert\u00b7zen", "stim\u00b7men", "nicht", "stets", "mit", "den", "lip\u00b7pen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die unbedachtsamkeit w\u00fcrckt lauter thr\u00e4nen-t\u00fccher,", "tokens": ["Die", "un\u00b7be\u00b7dacht\u00b7sam\u00b7keit", "w\u00fcrckt", "lau\u00b7ter", "thr\u00e4\u00b7nen\u00b7t\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum mu\u00df der klugheit licht der liebe leitstern seyn.", "tokens": ["Drum", "mu\u00df", "der", "klug\u00b7heit", "licht", "der", "lie\u00b7be", "leits\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df Ariadne sich so bald verlassen schaute,", "tokens": ["Da\u00df", "A\u00b7riad\u00b7ne", "sich", "so", "bald", "ver\u00b7las\u00b7sen", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Und ehre, gl\u00fcck und ruh an einen nagel hieng,", "tokens": ["Und", "eh\u00b7re", ",", "gl\u00fcck", "und", "ruh", "an", "ei\u00b7nen", "na\u00b7gel", "hieng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das machte, weil sie gleich der blinden regung traute,", "tokens": ["Das", "mach\u00b7te", ",", "weil", "sie", "gleich", "der", "blin\u00b7den", "re\u00b7gung", "trau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und nicht mit der vernunfft vorher zu rathe gieng.", "tokens": ["Und", "nicht", "mit", "der", "ver\u00b7nunfft", "vor\u00b7her", "zu", "ra\u00b7the", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "ADV", "PTKZU", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die wahre liebe geht nicht mit verbundnen augen,", "tokens": ["Die", "wah\u00b7re", "lie\u00b7be", "geht", "nicht", "mit", "ver\u00b7bund\u00b7nen", "au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie sucht vor, eh sie liebt, was liebens-w\u00fcrdig ist.", "tokens": ["Sie", "sucht", "vor", ",", "eh", "sie", "liebt", ",", "was", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn l\u00e4\u00dft einander ihm schon iede dirne taugen,", "tokens": ["Denn", "l\u00e4\u00dft", "ein\u00b7an\u00b7der", "ihm", "schon", "ie\u00b7de", "dir\u00b7ne", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPER", "ADV", "PIAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So hat Leander doch ein edler ziel erkiest.", "tokens": ["So", "hat", "Le\u00b7an\u00b7der", "doch", "ein", "ed\u00b7ler", "ziel", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es mag Florettens mund die wetter-h\u00e4ne schelten,", "tokens": ["Es", "mag", "Flo\u00b7ret\u00b7tens", "mund", "die", "wet\u00b7ter\u00b7h\u00e4\u00b7ne", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie wisse gantz genau, wie ihre kreide schreibt,", "tokens": ["Sie", "wis\u00b7se", "gantz", "ge\u00b7nau", ",", "wie", "ih\u00b7re", "krei\u00b7de", "schreibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die welt sey ihrer voll; ich kan es nicht entgelten,", "tokens": ["Die", "welt", "sey", "ih\u00b7rer", "voll", ";", "ich", "kan", "es", "nicht", "ent\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJD", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil mein magnet, wie vor, bey seinem pole bleibt.", "tokens": ["Weil", "mein", "mag\u00b7net", ",", "wie", "vor", ",", "bey", "sei\u00b7nem", "po\u00b7le", "bleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "$,", "PWAV", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "L\u00e4st eine Helena den Menelaus sitzen,", "tokens": ["L\u00e4st", "ei\u00b7ne", "He\u00b7le\u00b7na", "den", "Me\u00b7ne\u00b7laus", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Und st\u00f6st Semiramis den Ninus in die grufft;", "tokens": ["Und", "st\u00f6st", "Se\u00b7mi\u00b7ra\u00b7mis", "den", "Ni\u00b7nus", "in", "die", "grufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So kan doch jener schuld Alcesten nicht beschmitzen,", "tokens": ["So", "kan", "doch", "je\u00b7ner", "schuld", "Al\u00b7ces\u00b7ten", "nicht", "be\u00b7schmit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PDAT", "ADJD", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn ihre liebe drang bi\u00df in die todes-klufft.", "tokens": ["Denn", "ih\u00b7re", "lie\u00b7be", "drang", "bi\u00df", "in", "die", "to\u00b7des\u00b7klufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das was cometen dr\u00e4un, den sternen zuzuschreiben", "tokens": ["Das", "was", "co\u00b7me\u00b7ten", "dr\u00e4un", ",", "den", "ster\u00b7nen", "zu\u00b7zu\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "ADV", "$,", "ART", "ADJA", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "W\u00e4r allzu ungerecht, und kein erwogner schlu\u00df.", "tokens": ["W\u00e4r", "all\u00b7zu", "un\u00b7ge\u00b7recht", ",", "und", "kein", "er\u00b7wog\u00b7ner", "schlu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "$,", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mag doch Aeneas falsch, und Theseus untreu bleiben;", "tokens": ["Mag", "doch", "A\u00b7e\u00b7neas", "falsch", ",", "und", "The\u00b7seus", "un\u00b7treu", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "ADJD", "$,", "KON", "NE", "ADJD", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "De\u00dfwegen h\u00e4lt Aruntz und Cimber dennoch fu\u00df.", "tokens": ["De\u00df\u00b7we\u00b7gen", "h\u00e4lt", "A\u00b7runtz", "und", "Cim\u00b7ber", "den\u00b7noch", "fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "KON", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drum \u00fcbereil dich nicht, vern\u00fcnfftige Florette", "tokens": ["Drum", "\u00fc\u00b7be\u00b7reil", "dich", "nicht", ",", "ver\u00b7n\u00fcnff\u00b7ti\u00b7ge", "Flo\u00b7ret\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "KOUS", "PPER", "PTKNEG", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Folg\u2019 Aramenen nach und pr\u00fcfe hertz und sinn.", "tokens": ["Fol\u00b7g'", "A\u00b7ra\u00b7me\u00b7nen", "nach", "und", "pr\u00fc\u00b7fe", "hertz", "und", "sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.35": {"text": "Die tugend ist mein zug, dein witz ist meine kette.", "tokens": ["Die", "tu\u00b7gend", "ist", "mein", "zug", ",", "dein", "witz", "ist", "mei\u00b7ne", "ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dergleichen fessel wirfft kein kluger leichtlich hin.", "tokens": ["Derg\u00b7lei\u00b7chen", "fes\u00b7sel", "wirfft", "kein", "klu\u00b7ger", "leicht\u00b7lich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVFIN", "PIAT", "ADJA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du wilst den m\u00e4nnern zwar nur untreu zu erkennen,", "tokens": ["Du", "wilst", "den", "m\u00e4n\u00b7nern", "zwar", "nur", "un\u00b7treu", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und meynest: ihre glut sey nur ein kurtzer schein;", "tokens": ["Und", "mey\u00b7nest", ":", "ih\u00b7re", "glut", "sey", "nur", "ein", "kurt\u00b7zer", "schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch wenn die meisten schon in falschen flammen brennen,", "tokens": ["Doch", "wenn", "die", "meis\u00b7ten", "schon", "in", "fal\u00b7schen", "flam\u00b7men", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "VVFIN", "ADV", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So kan Leander doch treu und best\u00e4ndig seyn.", "tokens": ["So", "kan", "Le\u00b7an\u00b7der", "doch", "treu", "und", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}