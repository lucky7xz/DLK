{"textgrid.poem.27403": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Monodistichon", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcnscht ihr den Musageten zu sehn, gebt Boden und Freiheit;", "tokens": ["W\u00fcnscht", "ihr", "den", "Mu\u00b7sa\u00b7ge\u00b7ten", "zu", "sehn", ",", "gebt", "Bo\u00b7den", "und", "Frei\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier auf dem schmalen Rain ist f\u00fcr den Sch\u00fctzen nur Platz.", "tokens": ["Hier", "auf", "dem", "schma\u00b7len", "Rain", "ist", "f\u00fcr", "den", "Sch\u00fct\u00b7zen", "nur", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}