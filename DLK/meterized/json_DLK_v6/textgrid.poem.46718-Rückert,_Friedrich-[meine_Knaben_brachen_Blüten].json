{"textgrid.poem.46718": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[meine Knaben brachen Bl\u00fcten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Knaben brachen Bl\u00fcten,", "tokens": ["Mei\u00b7ne", "Kna\u00b7ben", "bra\u00b7chen", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche mir am Herzen lagen,", "tokens": ["Wel\u00b7che", "mir", "am", "Her\u00b7zen", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wollte sie beh\u00fcten,", "tokens": ["Und", "ich", "woll\u00b7te", "sie", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber doch den Grund nicht sagen.", "tokens": ["A\u00b7ber", "doch", "den", "Grund", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich sprach: dem Strauch wenn heute", "tokens": ["Und", "ich", "sprach", ":", "dem", "Strauch", "wenn", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "NN", "KOUS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr die Bl\u00fcten habt genommen,", "tokens": ["Ihr", "die", "Bl\u00fc\u00b7ten", "habt", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird euch keine Frucht zur Beute,", "tokens": ["Wird", "euch", "kei\u00b7ne", "Frucht", "zur", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn im Herbst wir wiederkommen.", "tokens": ["Wenn", "im", "Herbst", "wir", "wie\u00b7der\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ich dachte jener Bl\u00fcten,", "tokens": ["Doch", "ich", "dach\u00b7te", "je\u00b7ner", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich vor dem Tod, dem frechen,", "tokens": ["Die", "ich", "vor", "dem", "Tod", ",", "dem", "fre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leider konnte nicht beh\u00fcten,", "tokens": ["Lei\u00b7der", "konn\u00b7te", "nicht", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als er wollte Rosen brechen.", "tokens": ["Als", "er", "woll\u00b7te", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ihnen wollt' ich's schenken,", "tokens": ["A\u00b7ber", "ih\u00b7nen", "wollt'", "ich's", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht bei welken Blumenstielen", "tokens": ["Nicht", "bei", "wel\u00b7ken", "Blu\u00b7men\u00b7stie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Lust daran zu denken,", "tokens": ["In", "der", "Lust", "da\u00b7ran", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df des Todes Amt sie spielen.", "tokens": ["Da\u00df", "des", "To\u00b7des", "Amt", "sie", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sollt' ich diesen tr\u00fcben Schatten", "tokens": ["Sollt'", "ich", "die\u00b7sen", "tr\u00fc\u00b7ben", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werfen auf die frohen Knaben?", "tokens": ["Wer\u00b7fen", "auf", "die", "fro\u00b7hen", "Kna\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott sei Dank, auf gr\u00fcnen Matten", "tokens": ["Gott", "sei", "Dank", ",", "auf", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie spielen unbegraben!", "tokens": ["Da\u00df", "sie", "spie\u00b7len", "un\u00b7be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}