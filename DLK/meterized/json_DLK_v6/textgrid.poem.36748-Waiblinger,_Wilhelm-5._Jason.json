{"textgrid.poem.36748": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "5. Jason", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nenn' ich dich einen Hero'n? Hast du nicht den rohen Barbaren,", "tokens": ["Nenn'", "ich", "dich", "ei\u00b7nen", "He\u00b7ro'n", "?", "Hast", "du", "nicht", "den", "ro\u00b7hen", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$.", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Hast du das goldene Vlie\u00df ihnen der Kunst nicht geraubt?", "tokens": ["Hast", "du", "das", "gol\u00b7de\u00b7ne", "Vlie\u00df", "ih\u00b7nen", "der", "Kunst", "nicht", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wahrlich, du mu\u00dftest's zuvor, und eine Zauberin half dir,", "tokens": ["Wahr\u00b7lich", ",", "du", "mu\u00df\u00b7test's", "zu\u00b7vor", ",", "und", "ei\u00b7ne", "Zau\u00b7be\u00b7rin", "half", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn wie h\u00e4ttest du sonst Jason der Nachwelt geschenkt?", "tokens": ["Denn", "wie", "h\u00e4t\u00b7test", "du", "sonst", "Ja\u00b7son", "der", "Nach\u00b7welt", "ge\u00b7schenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "ADV", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}