{"textgrid.poem.62699": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Universit\u00e4t Jena", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist gleich Athen mit seinen Mauren,", "tokens": ["Ist", "gleich", "A\u00b7then", "mit", "sei\u00b7nen", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und Heiligth\u00fcmern umgerissen;", "tokens": ["Und", "Hei\u00b7ligt\u00b7h\u00fc\u00b7mern", "um\u00b7ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist der Schade doch nicht zu betauren,", "tokens": ["So", "ist", "der", "Scha\u00b7de", "doch", "nicht", "zu", "be\u00b7tau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dieweil an Th\u00fcrings Fl\u00fcssen,", "tokens": ["Die\u00b7weil", "an", "Th\u00fc\u00b7rings", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Weisheitsreiche Jena liegt;", "tokens": ["Das", "Weis\u00b7heits\u00b7rei\u00b7che", "Je\u00b7na", "liegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So des zerst\u00f6hrten Vorrecht kriegt.", "tokens": ["So", "des", "zer\u00b7st\u00f6hr\u00b7ten", "Vor\u00b7recht", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da ist der Kern der Lehrer und der Lehren:", "tokens": ["Da", "ist", "der", "Kern", "der", "Leh\u00b7rer", "und", "der", "Leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Drum mag es wohl durch seinen Glanz und Schein,", "tokens": ["Drum", "mag", "es", "wohl", "durch", "sei\u00b7nen", "Glanz", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das andere Athen und Ph\u00f6bens Tempel seyn.", "tokens": ["Das", "an\u00b7de\u00b7re", "A\u00b7then", "und", "Ph\u00f6\u00b7bens", "Tem\u00b7pel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "NN", "VAINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}