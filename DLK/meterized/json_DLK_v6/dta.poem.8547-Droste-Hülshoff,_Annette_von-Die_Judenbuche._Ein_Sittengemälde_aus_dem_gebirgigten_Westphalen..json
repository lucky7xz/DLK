{"dta.poem.8547": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Judenbuche.  \n Ein Sittengem\u00e4lde aus dem gebirgigten Westphalen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kindelein so l\u00f6belich", "tokens": ["Ein", "Kin\u00b7de\u00b7lein", "so", "l\u00f6\u00b7be\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist uns geboren heute,", "tokens": ["Ist", "uns", "ge\u00b7bo\u00b7ren", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von einer Jungfran s\u00e4uberlich,", "tokens": ["Von", "ei\u00b7ner", "Jung\u00b7fran", "s\u00e4u\u00b7ber\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df freu\u2019n sich alle Leute;", "tokens": ["De\u00df", "freu'n", "sich", "al\u00b7le", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und w\u00e4r das Kindlein nicht gebor\u2019n,", "tokens": ["Und", "w\u00e4r", "das", "Kin\u00b7dlein", "nicht", "ge\u00b7bor'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4ren wir alle zusammen verlor\u2019n:", "tokens": ["So", "w\u00e4\u00b7ren", "wir", "al\u00b7le", "zu\u00b7sam\u00b7men", "ver\u00b7lor'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Das Heil ist unser Aller.", "tokens": ["Das", "Heil", "ist", "un\u00b7ser", "Al\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O du mein liebster Jesu Christ,", "tokens": ["O", "du", "mein", "liebs\u00b7ter", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der du als Mensch geboren bist,", "tokens": ["Der", "du", "als", "Mensch", "ge\u00b7bo\u00b7ren", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erl\u00f6s uns von der H\u00f6lle!", "tokens": ["Er\u00b7l\u00f6s", "uns", "von", "der", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}