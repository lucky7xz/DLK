{"dta.poem.13906": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Der alte Held .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dort hat er nach Lust und Streiten", "tokens": ["Dort", "hat", "er", "nach", "Lust", "und", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Panner aufgestellt,", "tokens": ["Das", "Pan\u00b7ner", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die auf dem Strome der Zeiten", "tokens": ["Und", "die", "auf", "dem", "Stro\u00b7me", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Am Felsen vor\u00fcbergleiten,", "tokens": ["Am", "Fel\u00b7sen", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie gr\u00fc\u00dfen den alten Held.", "tokens": ["Sie", "gr\u00fc\u00b7\u00dfen", "den", "al\u00b7ten", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}