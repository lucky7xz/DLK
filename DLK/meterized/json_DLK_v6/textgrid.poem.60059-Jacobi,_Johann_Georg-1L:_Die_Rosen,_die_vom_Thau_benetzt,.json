{"textgrid.poem.60059": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Rosen, die vom Thau benetzt,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rosen, die vom Thau benetzt,", "tokens": ["Die", "Ro\u00b7sen", ",", "die", "vom", "Thau", "be\u00b7netzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An jedem Bl\u00e4ttchen unverletzt,", "tokens": ["An", "je\u00b7dem", "Bl\u00e4tt\u00b7chen", "un\u00b7ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zu den frischen Nelken", "tokens": ["Ich", "zu", "den", "fri\u00b7schen", "Nel\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Morgenroth zu pfl\u00fccken ging,", "tokens": ["Im", "Mor\u00b7gen\u00b7roth", "zu", "pfl\u00fc\u00b7cken", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und k\u00fcssend um dein Bildni\u00df hing;", "tokens": ["Und", "k\u00fcs\u00b7send", "um", "dein", "Bild\u00b7ni\u00df", "hing", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Chloe! wie sie welken!", "tokens": ["O", "Chloe", "!", "wie", "sie", "wel\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "So welken, wo ich Blumen brach,", "tokens": ["So", "wel\u00b7ken", ",", "wo", "ich", "Blu\u00b7men", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So welken alle, nach und nach,", "tokens": ["So", "wel\u00b7ken", "al\u00b7le", ",", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wiesen mit den Hainen;", "tokens": ["Die", "Wie\u00b7sen", "mit", "den", "Hai\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis endlich die getreue Hand,", "tokens": ["Bis", "end\u00b7lich", "die", "ge\u00b7treu\u00b7e", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis, gleich den Kr\u00e4nzen, die sie band ...", "tokens": ["Bis", ",", "gleich", "den", "Kr\u00e4n\u00b7zen", ",", "die", "sie", "band", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du aber sollst nicht weinen!", "tokens": ["Du", "a\u00b7ber", "sollst", "nicht", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O n\u00e4hm' ein froher Engel dann", "tokens": ["O", "n\u00e4hm'", "ein", "fro\u00b7her", "En\u00b7gel", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich meiner j\u00fcngsten Lieder an!", "tokens": ["Sich", "mei\u00b7ner", "j\u00fcng\u00b7sten", "Lie\u00b7der", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr frohen Engel! bliebe", "tokens": ["Ihr", "fro\u00b7hen", "En\u00b7gel", "!", "blie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch sie dem guten M\u00e4dchen doch", "tokens": ["Durch", "sie", "dem", "gu\u00b7ten", "M\u00e4d\u00b7chen", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In k\u00fcnftigem Gesange noch", "tokens": ["In", "k\u00fcnf\u00b7ti\u00b7gem", "Ge\u00b7san\u00b7ge", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Nachhall meiner Liebe!", "tokens": ["Ein", "Nach\u00b7hall", "mei\u00b7ner", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}