{"dta.poem.13809": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder 1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "af:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da ward die Welt so tr\u00fcbe,", "tokens": ["Da", "ward", "die", "Welt", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rings stiegen Wetter von der Berge Spitzen,", "tokens": ["Rings", "stie\u00b7gen", "Wet\u00b7ter", "von", "der", "Ber\u00b7ge", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel borst in Blitzen,", "tokens": ["Der", "Him\u00b7mel", "borst", "in", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df neugest\u00e4rkt sich Deutschland d'raus erh\u00fcbe. \u2014", "tokens": ["Da\u00df", "neu\u00b7ge\u00b7st\u00e4rkt", "sich", "Deutschland", "d'raus", "er\u00b7h\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "NE", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun ist das Schiff zerschlagen,", "tokens": ["Nun", "ist", "das", "Schiff", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie soll ich ohne ", "tokens": ["Wie", "soll", "ich", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}}}}}