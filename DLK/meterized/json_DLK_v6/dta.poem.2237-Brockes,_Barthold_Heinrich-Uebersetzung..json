{"dta.poem.2237": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein blo\u00dfer Mathematicus scheint einer Spinnen gleich", "tokens": ["Ein", "blo\u00b7\u00dfer", "Ma\u00b7the\u00b7ma\u00b7ti\u00b7cus", "scheint", "ei\u00b7ner", "Spin\u00b7nen", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "gesinnt,", "tokens": ["ge\u00b7sinnt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die nichts als lange Linien und runde Zirkel aus sich", "tokens": ["Die", "nichts", "als", "lan\u00b7ge", "Li\u00b7ni\u00b7en", "und", "run\u00b7de", "Zir\u00b7kel", "aus", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "KOKOM", "ADV", "NN", "KON", "ADJA", "NN", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "spinnt;", "tokens": ["spinnt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nur mit dem Unterscheid allein, wenn ihr darauf recht", "tokens": ["Nur", "mit", "dem", "Un\u00b7ter\u00b7scheid", "al\u00b7lein", ",", "wenn", "ihr", "da\u00b7rauf", "recht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "PAV", "ADJD"], "meter": "+--+-+-+--++-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Achtung gebet,", "tokens": ["Ach\u00b7tung", "ge\u00b7bet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Da\u00df sie ihr Werk aus ihrem Bauch, er seines aus dem", "tokens": ["Da\u00df", "sie", "ihr", "Werk", "aus", "ih\u00b7rem", "Bauch", ",", "er", "sei\u00b7nes", "aus", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kopfe, webet.", "tokens": ["Kop\u00b7fe", ",", "we\u00b7bet", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}