{"textgrid.poem.35630": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Grodek", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abend t\u00f6nen die herbstlichen W\u00e4lder", "tokens": ["Am", "A\u00b7bend", "t\u00f6\u00b7nen", "die", "herbst\u00b7li\u00b7chen", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von t\u00f6dlichen Waffen, die goldnen Ebenen", "tokens": ["Von", "t\u00f6d\u00b7li\u00b7chen", "Waf\u00b7fen", ",", "die", "gold\u00b7nen", "E\u00b7be\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und blauen Seen, dar\u00fcber die Sonne", "tokens": ["Und", "blau\u00b7en", "Seen", ",", "da\u00b7r\u00fc\u00b7ber", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "D\u00fcstrer hinrollt; umf\u00e4ngt die Nacht", "tokens": ["D\u00fcst\u00b7rer", "hin\u00b7rollt", ";", "um\u00b7f\u00e4ngt", "die", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Sterbende Krieger, die wilde Klage", "tokens": ["Ster\u00b7ben\u00b7de", "Krie\u00b7ger", ",", "die", "wil\u00b7de", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ihrer zerbrochenen M\u00fcnder.", "tokens": ["Ih\u00b7rer", "zer\u00b7bro\u00b7che\u00b7nen", "M\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Doch stille sammelt im Weidengrund", "tokens": ["Doch", "stil\u00b7le", "sam\u00b7melt", "im", "Wei\u00b7den\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Rotes Gew\u00f6lk, darin ein z\u00fcrnender Gott wohnt", "tokens": ["Ro\u00b7tes", "Ge\u00b7w\u00f6lk", ",", "da\u00b7rin", "ein", "z\u00fcr\u00b7nen\u00b7der", "Gott", "wohnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+--++", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Das vergo\u00dfne Blut sich, mondne K\u00fchle;", "tokens": ["Das", "ver\u00b7go\u00df\u00b7ne", "Blut", "sich", ",", "mond\u00b7ne", "K\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Alle Stra\u00dfen m\u00fcnden in schwarze Verwesung.", "tokens": ["Al\u00b7le", "Stra\u00b7\u00dfen", "m\u00fcn\u00b7den", "in", "schwar\u00b7ze", "Ver\u00b7we\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Unter goldnem Gezweig der Nacht und Sternen", "tokens": ["Un\u00b7ter", "gold\u00b7nem", "Ge\u00b7zweig", "der", "Nacht", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "KON", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Es schwankt der Schwester Schatten durch den schweigenden Hain,", "tokens": ["Es", "schwankt", "der", "Schwes\u00b7ter", "Schat\u00b7ten", "durch", "den", "schwei\u00b7gen\u00b7den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Zu gr\u00fc\u00dfen die Geister der Helden, die blutenden H\u00e4upter;", "tokens": ["Zu", "gr\u00fc\u00b7\u00dfen", "die", "Geis\u00b7ter", "der", "Hel\u00b7den", ",", "die", "blu\u00b7ten\u00b7den", "H\u00e4up\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.14": {"text": "Und leise t\u00f6nen im Rohr die dunkeln Fl\u00f6ten des Herbstes.", "tokens": ["Und", "lei\u00b7se", "t\u00f6\u00b7nen", "im", "Rohr", "die", "dun\u00b7keln", "Fl\u00f6\u00b7ten", "des", "Herbs\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "O stolzere Trauer! ihr ehernen Alt\u00e4re", "tokens": ["O", "stol\u00b7ze\u00b7re", "Trau\u00b7er", "!", "ihr", "e\u00b7her\u00b7nen", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Die hei\u00dfe Flamme des Geistes n\u00e4hrt heute ein gewaltiger Schmerz,", "tokens": ["Die", "hei\u00b7\u00dfe", "Flam\u00b7me", "des", "Geis\u00b7tes", "n\u00e4hrt", "heu\u00b7te", "ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Die ungebornen Enkel.", "tokens": ["Die", "un\u00b7ge\u00b7bor\u00b7nen", "En\u00b7kel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}