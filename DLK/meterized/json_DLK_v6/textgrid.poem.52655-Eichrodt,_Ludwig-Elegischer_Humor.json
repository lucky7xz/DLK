{"textgrid.poem.52655": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Elegischer Humor", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange sind wir nicht gesessen", "tokens": ["Lan\u00b7ge", "sind", "wir", "nicht", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lange nicht beim k\u00fchlen Wein,", "tokens": ["Lan\u00b7ge", "nicht", "beim", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habens ganz und gar vergessen,", "tokens": ["Ha\u00b7bens", "ganz", "und", "gar", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgelassen lustig sein.", "tokens": ["Aus\u00b7ge\u00b7las\u00b7sen", "lus\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eilet drum zur alten Schenke!", "tokens": ["Ei\u00b7let", "drum", "zur", "al\u00b7ten", "Schen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lasset Alles gehn und stehn,", "tokens": ["Las\u00b7set", "Al\u00b7les", "gehn", "und", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zum berauschenden Getr\u00e4nke", "tokens": ["Zum", "be\u00b7rau\u00b7schen\u00b7den", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Raset ihr Vortrefflichen!", "tokens": ["Ra\u00b7set", "ihr", "Vor\u00b7treff\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+--++--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Reinigt eure S\u00fcnderseelen,", "tokens": ["Rei\u00b7nigt", "eu\u00b7re", "S\u00fcn\u00b7der\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilet euer krank Gem\u00fcth,", "tokens": ["Hei\u00b7let", "eu\u00b7er", "krank", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waschet eure trocknen Kehlen,", "tokens": ["Wa\u00b7schet", "eu\u00b7re", "trock\u00b7nen", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr ein \u00fcberschw\u00e4nglich Lied!", "tokens": ["F\u00fcr", "ein", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In die Schenke, in die Schenke!", "tokens": ["In", "die", "Schen\u00b7ke", ",", "in", "die", "Schen\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde, seid ihr alle da?", "tokens": ["Freun\u00b7de", ",", "seid", "ihr", "al\u00b7le", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich alter Zeit gedenke,", "tokens": ["Wenn", "ich", "al\u00b7ter", "Zeit", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "T\u00f6nt mein Herz Halleluja!", "tokens": ["T\u00f6nt", "mein", "Herz", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wandle mit bed\u00e4chtgem Schritte,", "tokens": ["Wand\u00b7le", "mit", "be\u00b7d\u00e4cht\u00b7gem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer sich selber nie vergi\u00dft!", "tokens": ["Wer", "sich", "sel\u00b7ber", "nie", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberfl\u00fcssig ist die Sitte,", "tokens": ["Ue\u00b7ber\u00b7fl\u00fcs\u00b7sig", "ist", "die", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn das Herz betrunken ist.", "tokens": ["Wenn", "das", "Herz", "be\u00b7trun\u00b7ken", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Umgekehrt von euerm Spasse", "tokens": ["Um\u00b7ge\u00b7kehrt", "von", "eu\u00b7erm", "Spas\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schalle dieses frohe Haus,", "tokens": ["Schal\u00b7le", "die\u00b7ses", "fro\u00b7he", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Strahl aus vollem Fasse", "tokens": ["Wie", "der", "Strahl", "aus", "vol\u00b7lem", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprudle euer Witz heraus!", "tokens": ["Sprud\u00b7le", "eu\u00b7er", "Witz", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lachen, da\u00df erdr\u00f6hnt die Halle,", "tokens": ["La\u00b7chen", ",", "da\u00df", "er\u00b7dr\u00f6hnt", "die", "Hal\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mich sch\u00fcttelt Rippenweh,", "tokens": ["Da\u00df", "mich", "sch\u00fct\u00b7telt", "Rip\u00b7pen\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich laut, wenn ich so Alle,", "tokens": ["Mu\u00df", "ich", "laut", ",", "wenn", "ich", "so", "Al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcder euch beisammen seh.", "tokens": ["Br\u00fc\u00b7der", "euch", "bei\u00b7sam\u00b7men", "seh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Achtung Allem, was zu achten!", "tokens": ["Ach\u00b7tung", "Al\u00b7lem", ",", "was", "zu", "ach\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PRELS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber lachen mu\u00df ich \u2013 als", "tokens": ["A\u00b7ber", "la\u00b7chen", "mu\u00df", "ich", "\u2013", "als"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$(", "KOKOM"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6tter im Olympus lachten,", "tokens": ["G\u00f6t\u00b7ter", "im", "O\u00b7lym\u00b7pus", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lachten sie aus vollem Hals.", "tokens": ["Lach\u00b7ten", "sie", "aus", "vol\u00b7lem", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6r ich solchen L\u00e4rm verf\u00fchren", "tokens": ["H\u00f6r", "ich", "sol\u00b7chen", "L\u00e4rm", "ver\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter so vern\u00fcnftge Leut,", "tokens": ["Lau\u00b7ter", "so", "ver\u00b7n\u00fcnft\u00b7ge", "Leut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll mich gleich der Donner r\u00fchren,", "tokens": ["Soll", "mich", "gleich", "der", "Don\u00b7ner", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterb ich nicht aus purer Freud!", "tokens": ["Sterb", "ich", "nicht", "aus", "pu\u00b7rer", "Freud", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Holla, wackere Gesellen,", "tokens": ["Hol\u00b7la", ",", "wa\u00b7cke\u00b7re", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlagt Gesang an, Hollahoh!", "tokens": ["Schlagt", "Ge\u00b7sang", "an", ",", "Hol\u00b7la\u00b7hoh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die T\u00f6ne hoch anschwellen,", "tokens": ["Wenn", "die", "T\u00f6\u00b7ne", "hoch", "an\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlt sich Jeder lebensfroh.", "tokens": ["F\u00fchlt", "sich", "Je\u00b7der", "le\u00b7bens\u00b7froh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sagt mir an, gelehrte H\u00e4upter,", "tokens": ["Sagt", "mir", "an", ",", "ge\u00b7lehr\u00b7te", "H\u00e4up\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, o Dummkopf, sag mir an,", "tokens": ["Du", ",", "o", "Dumm\u00b7kopf", ",", "sag", "mir", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steh mir Rede, Wohlbeleibter,", "tokens": ["Steh", "mir", "Re\u00b7de", ",", "Wohl\u00b7be\u00b7leib\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprich du lendenarmer Mann,", "tokens": ["Sprich", "du", "len\u00b7de\u00b7nar\u00b7mer", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Gebt, ihr edlen Trinker alle,", "tokens": ["Gebt", ",", "ihr", "ed\u00b7len", "Trin\u00b7ker", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebt mir Antwort, wie und wann", "tokens": ["Gebt", "mir", "Ant\u00b7wort", ",", "wie", "und", "wann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "$,", "PWAV", "KON", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt der biedre Mensch zu Falle,", "tokens": ["Kommt", "der", "bied\u00b7re", "Mensch", "zu", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und warum, das sagt mir an!", "tokens": ["Und", "wa\u00b7rum", ",", "das", "sagt", "mir", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Wie aus einem Mund genommen,", "tokens": ["Wie", "aus", "ei\u00b7nem", "Mund", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6r ich das Orakel, dann", "tokens": ["H\u00f6r", "ich", "das", "O\u00b7ra\u00b7kel", ",", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "ART", "NN", "$,", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wird der Mensch zu Falle kommen,", "tokens": ["Wird", "der", "Mensch", "zu", "Fal\u00b7le", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er nicht mehr trinken kann!", "tokens": ["Wenn", "er", "nicht", "mehr", "trin\u00b7ken", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann auch, wenn mit ihm \u2013 o wische", "tokens": ["Dann", "auch", ",", "wenn", "mit", "ihm", "\u2013", "o", "wi\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPR", "PPER", "$(", "FM", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Freund die Augen, da\u00df er sinkt \u2013", "tokens": ["Freund", "die", "Au\u00b7gen", ",", "da\u00df", "er", "sinkt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Niemand mehr an einem Tische", "tokens": ["Nie\u00b7mand", "mehr", "an", "ei\u00b7nem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und aus einem Glase trinkt!", "tokens": ["Und", "aus", "ei\u00b7nem", "Gla\u00b7se", "trinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Einst in gl\u00fccklicheren Tagen,", "tokens": ["Einst", "in", "gl\u00fcck\u00b7li\u00b7che\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Derer Geist f\u00fcr immer schwand,", "tokens": ["De\u00b7rer", "Geist", "f\u00fcr", "im\u00b7mer", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannte man nur wenig Plagen", "tokens": ["Kann\u00b7te", "man", "nur", "we\u00b7nig", "Pla\