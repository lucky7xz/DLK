{"dta.poem.23683": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Verliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.85", "tl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, seht doch den J&#252;ngling!", "tokens": ["Ach", ",", "seht", "doch", "den", "J", "&#252;", "ng\u00b7ling", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NE", "$(", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er wird ia so stille,", "tokens": ["Er", "wird", "i\u00b7a", "so", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schleicht in die Winkel,", "tokens": ["Er", "schleicht", "in", "die", "Win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er spricht mit sich selber,", "tokens": ["Er", "spricht", "mit", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Er trauret, er sinnet,", "tokens": ["Er", "trau\u00b7ret", ",", "er", "sin\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Wangen erblassen,", "tokens": ["Die", "Wan\u00b7gen", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Er scherzt nicht mehr feurig,", "tokens": ["Er", "scherzt", "nicht", "mehr", "feu\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Er h\u00f6rt nicht die Freunde,", "tokens": ["Er", "h\u00f6rt", "nicht", "die", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Er sieht nicht die Gl\u00e4ser,", "tokens": ["Er", "sieht", "nicht", "die", "Gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Er sieht nur das M\u00e4dchen.", "tokens": ["Er", "sieht", "nur", "das", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ach, seht doch den J\u00fcngling,", "tokens": ["Ach", ",", "seht", "doch", "den", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Er hat sich verliebet!", "tokens": ["Er", "hat", "sich", "ver\u00b7lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}