{"textgrid.poem.61528": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ich fuhr von St. Goar", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich fuhr von Sankt Goar", "tokens": ["Ich", "fuhr", "von", "Sankt", "Go\u00b7ar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den gr\u00fcnen Rhein zu Berge;", "tokens": ["Den", "gr\u00fc\u00b7nen", "Rhein", "zu", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Greis im Silberhaar", "tokens": ["Ein", "Greis", "im", "Sil\u00b7ber\u00b7haar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "War meines Nachens Ferge.", "tokens": ["War", "mei\u00b7nes", "Na\u00b7chens", "Fer\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir plauderten nicht viel,", "tokens": ["Wir", "plau\u00b7der\u00b7ten", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.2": {"text": "Die Felsen sah ich gleiten", "tokens": ["Die", "Fel\u00b7sen", "sah", "ich", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dahin im Wellenspiel", "tokens": ["Da\u00b7hin", "im", "Wel\u00b7len\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["PAV", "APPRART", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Und dachte vor'ger Zeiten.", "tokens": ["Und", "dach\u00b7te", "vor'\u00b7ger", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als wir an der Pfalz", "tokens": ["Und", "als", "wir", "an", "der", "Pfalz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bei Kaub vor\u00fcber waren,", "tokens": ["Bei", "Kaub", "vor\u00b7\u00fc\u00b7ber", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam hellen Liederschalls", "tokens": ["Kam", "hel\u00b7len", "Lie\u00b7der\u00b7schalls"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schiff zu Tal gefahren.", "tokens": ["Ein", "Schiff", "zu", "Tal", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ins wei\u00dfe Segel schien", "tokens": ["Ins", "wei\u00b7\u00dfe", "Se\u00b7gel", "schien"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Abend, da\u00df er gl\u00fchte;", "tokens": ["Der", "A\u00b7bend", ",", "da\u00df", "er", "gl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Studenten sa\u00dfen drin,", "tokens": ["Stu\u00b7den\u00b7ten", "sa\u00b7\u00dfen", "drin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit Laub umkr\u00e4nzt die H\u00fcte.", "tokens": ["Mit", "Laub", "um\u00b7kr\u00e4nzt", "die", "H\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da ging von Hand zu Hand", "tokens": ["Da", "ging", "von", "Hand", "zu", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Kelch von gr\u00fcnem Glaste;", "tokens": ["Der", "Kelch", "von", "gr\u00fc\u00b7nem", "Glas\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sch\u00f6nste M\u00e4gdlein stand", "tokens": ["Das", "sch\u00f6ns\u00b7te", "M\u00e4gd\u00b7lein", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In goldnem Haar am Maste;", "tokens": ["In", "gold\u00b7nem", "Haar", "am", "Mas\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie streute Rosen rot", "tokens": ["Sie", "streu\u00b7te", "Ro\u00b7sen", "rot"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hinunter in die Wogen", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fc\u00dfte, wie im Boot", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", ",", "wie", "im", "Boot"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir sacht vor\u00fcberzogen.", "tokens": ["Wir", "sacht", "vor\u00b7\u00fc\u00b7berz\u00b7o\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und horch, nun unterschied", "tokens": ["Und", "horch", ",", "nun", "un\u00b7ter\u00b7schied"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Singen ich der andern:", "tokens": ["Das", "Sin\u00b7gen", "ich", "der", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war's mein eigen Lied,", "tokens": ["Da", "wa\u00b7r's", "mein", "ei\u00b7gen", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich sang es einst vom Wandern;", "tokens": ["Ich", "sang", "es", "einst", "vom", "Wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich sang's vor manchem Jahr,", "tokens": ["Ich", "sang's", "vor", "man\u00b7chem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Berauscht vom Maienscheine,", "tokens": ["Be\u00b7rauscht", "vom", "Mai\u00b7en\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ich gleich jenen war", "tokens": ["Da", "ich", "gleich", "je\u00b7nen", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PDS", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Student zu Bonn am Rheine.", "tokens": ["Stu\u00b7dent", "zu", "Bonn", "am", "Rhei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Wie seltsam traf's das Ohr", "tokens": ["Wie", "selt\u00b7sam", "traf's", "das", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir jetzt aus fremdem Munde!", "tokens": ["Mir", "jetzt", "aus", "frem\u00b7dem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Heimweh zuckt' empor", "tokens": ["Ein", "Heim\u00b7weh", "zuckt'", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In meines Herzens Grunde.", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich lauschte, bis der Klang", "tokens": ["Ich", "lauschte", ",", "bis", "der", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zerflo\u00df in Windesweben;", "tokens": ["Zer\u00b7flo\u00df", "in", "Win\u00b7des\u00b7we\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sah ich drauf noch lang", "tokens": ["Doch", "sah", "ich", "drauf", "noch", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Schifflein gl\u00e4nzend schweben.", "tokens": ["Das", "Schif\u00b7flein", "gl\u00e4n\u00b7zend", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es zog dahin, dahin \u2013", "tokens": ["Es", "zog", "da\u00b7hin", ",", "da\u00b7hin", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Still sa\u00df ich, r\u00fcckw\u00e4rts lugend;", "tokens": ["Still", "sa\u00df", "ich", ",", "r\u00fcck\u00b7w\u00e4rts", "lu\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir war's, als f\u00fchre drin", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "f\u00fch\u00b7re", "drin"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dannen meine Jugend.", "tokens": ["Von", "dan\u00b7nen", "mei\u00b7ne", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}