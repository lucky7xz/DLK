{"textgrid.poem.36760": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Landschaftmalerische Kuriosit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid n\u00e4rrische Leute, da wandelt ihr durch die Campagna,", "tokens": ["Ihr", "seid", "n\u00e4r\u00b7ri\u00b7sche", "Leu\u00b7te", ",", "da", "wan\u00b7delt", "ihr", "durch", "die", "Cam\u00b7pag\u00b7na", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wie durch die Gallerie, immer mit kritischem Blick.", "tokens": ["Wie", "durch", "die", "Gal\u00b7le\u00b7rie", ",", "im\u00b7mer", "mit", "kri\u00b7ti\u00b7schem", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Das sind Linien, Gr\u00fcnde, Gebirge, Beleuchtungen, Tinten,", "tokens": ["Das", "sind", "Li\u00b7ni\u00b7en", ",", "Gr\u00fcn\u00b7de", ",", "Ge\u00b7bir\u00b7ge", ",", "Be\u00b7leuch\u00b7tun\u00b7gen", ",", "Tin\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Aber das ist nicht nach Clauds, das nicht nach Poussins Geschmack.", "tokens": ["A\u00b7ber", "das", "ist", "nicht", "nach", "Clauds", ",", "das", "nicht", "nach", "Pous\u00b7sins", "Ge\u00b7schmack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PTKNEG", "APPR", "NE", "$,", "PRELS", "PTKNEG", "APPR", "NE", "NN", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}