{"dta.poem.21268": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Herrn Christian Gottfried Krause.  \n  1762.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Krause, den nicht der Themis\nOrakel,", "tokens": ["Mein", "Krau\u00b7se", ",", "den", "nicht", "der", "The\u00b7mis", "O\u00b7ra\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Zank am Altar\u2019, im Tempel der", "tokens": ["Der", "Zank", "am", "Al\u00b7ta\u00b7r'", ",", "im", "Tem\u00b7pel", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ART"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Entw\u00f6hnten z\u00e4rtliche Lieder", "tokens": ["Ent\u00b7w\u00f6hn\u00b7ten", "z\u00e4rt\u00b7li\u00b7che", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus siebenfach t\u00f6nenden Saiten zu", "tokens": ["Aus", "sie\u00b7ben\u00b7fach", "t\u00f6\u00b7nen\u00b7den", "Sai\u00b7ten", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJA", "NN", "PTKZU"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Lass andre den Sieg des feurigen Heinrichs,", "tokens": ["Lass", "and\u00b7re", "den", "Sieg", "des", "feu\u00b7ri\u00b7gen", "Hein\u00b7richs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den schnellen Triumph des L\u00f6wen besingen,", "tokens": ["Den", "schnel\u00b7len", "Tri\u00b7umph", "des", "L\u00f6\u00b7wen", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der, selbst im Schlummer erschrecklich,", "tokens": ["Der", ",", "selbst", "im", "Schlum\u00b7mer", "er\u00b7schreck\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Lybischen W\u00fcsten in Ehrfurcht", "tokens": ["Die", "Ly\u00b7bi\u00b7schen", "W\u00fcs\u00b7ten", "in", "Ehr\u00b7furcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und endlich, gereizt vom drohenden Pan-", "tokens": ["Und", "end\u00b7lich", ",", "ge\u00b7reizt", "vom", "dro\u00b7hen\u00b7den", "Pan"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADJD", "APPRART", "ADJA", "TRUNC"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den nimmer umsonst gewageten Sprung", "tokens": ["Den", "nim\u00b7mer", "um\u00b7sonst", "ge\u00b7wa\u00b7ge\u00b7ten", "Sprung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Bauch des Feindes die Klauen,", "tokens": ["Im", "Bauch", "des", "Fein\u00b7des", "die", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Nacken den z\u00e4hnebewaffneten", "tokens": ["Im", "Na\u00b7cken", "den", "z\u00e4h\u00b7ne\u00b7be\u00b7waff\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ich singe mit dir die sanfteren Siege", "tokens": ["Ich", "sin\u00b7ge", "mit", "dir", "die", "sanf\u00b7te\u00b7ren", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Daphne, das Gl\u00fcck um Iris zu", "tokens": ["Der", "Daph\u00b7ne", ",", "das", "Gl\u00fcck", "um", "I\u00b7ris", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NE", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um euch, ihr leuchtenden Augen!", "tokens": ["Um", "euch", ",", "ihr", "leuch\u00b7ten\u00b7den", "Au\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich, strebender Busen! dich, Gra-", "tokens": ["Dich", ",", "stre\u00b7ben\u00b7der", "Bu\u00b7sen", "!", "dich", ",", "Gra"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "PPER", "$,", "TRUNC"], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}