{"textgrid.poem.57347": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht Troph\u00e4en, des Bluts Schleyer, verf\u00fchren ihn", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Troph\u00e4en, des Bluts Schleyer, verf\u00fchren ihn", "tokens": ["Nicht", "Trop\u00b7h\u00e4\u00b7en", ",", "des", "Bluts", "Schle\u00b7yer", ",", "ver\u00b7f\u00fch\u00b7ren", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ART", "NN", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Erobrung, er schwazt niemals von Mark Aurel,", "tokens": ["Zu", "E\u00b7rob\u00b7rung", ",", "er", "schwazt", "nie\u00b7mals", "von", "Mark", "Au\u00b7rel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "NE", "NE", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00fcllt den Mund nicht der Sage,", "tokens": ["F\u00fcllt", "den", "Mund", "nicht", "der", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt dem schimmernden Hofe nicht:", "tokens": ["Gl\u00e4nzt", "dem", "schim\u00b7mern\u00b7den", "Ho\u00b7fe", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Aber Ludewig ruft M\u00e4nner des Volks, dass sie", "tokens": ["A\u00b7ber", "Lu\u00b7de\u00b7wig", "ruft", "M\u00e4n\u00b7ner", "des", "Volks", ",", "dass", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "NN", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihm die Lasten des Volks leichten, lind weisen Bund", "tokens": ["Ihm", "die", "Las\u00b7ten", "des", "Volks", "leich\u00b7ten", ",", "lind", "wei\u00b7sen", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVINF", "$,", "ADJD", "ADJA", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zwischen Vater, und Kindern", "tokens": ["Zwi\u00b7schen", "Va\u00b7ter", ",", "und", "Kin\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Fest ihm setzen, Verhalt, gestimt", "tokens": ["Fest", "ihm", "set\u00b7zen", ",", "Ver\u00b7halt", ",", "ge\u00b7stimt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "NN", "$,", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wie in G\u00f6ttermusik; (Gl\u00fcckliche Zeit, und ich", "tokens": ["Wie", "in", "G\u00f6t\u00b7ter\u00b7mu\u00b7sik", ";(", "Gl\u00fcck\u00b7li\u00b7che", "Zeit", ",", "und", "ich"], "token_info": ["word", "word", "word", "emoticon", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$(", "ADJA", "NN", "$,", "KON", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gl\u00fccklich, der sie noch sah!) ruft sie, damit der Saat", "tokens": ["Gl\u00fcck\u00b7lich", ",", "der", "sie", "noch", "sah", "!", ")", "ruft", "sie", ",", "da\u00b7mit", "der", "Saat"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$.", "$(", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sie ihm streuen, aus der sich", "tokens": ["Sie", "ihm", "streu\u00b7en", ",", "aus", "der", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "$,", "APPR", "PRELS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch die goldene \u00c4hr' einst hebt.", "tokens": ["Hoch", "die", "gol\u00b7de\u00b7ne", "\u00c4hr'", "einst", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Ach ich sehe sie schon, h\u00f6re die wogenden", "tokens": ["Ach", "ich", "se\u00b7he", "sie", "schon", ",", "h\u00f6\u00b7re", "die", "wo\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Felder rauschen; sie komt, Wonne! die Erndte komt;", "tokens": ["Fel\u00b7der", "rau\u00b7schen", ";", "sie", "komt", ",", "Won\u00b7ne", "!", "die", "Ernd\u00b7te", "komt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "PPER", "VVFIN", "$,", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schnitter tragen, der K\u00f6nig", "tokens": ["Schnit\u00b7ter", "tra\u00b7gen", ",", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tr\u00e4gt den lieblichen blauen Kranz!", "tokens": ["Tr\u00e4gt", "den", "lieb\u00b7li\u00b7chen", "blau\u00b7en", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "So wie Zesar vordem weint' an des Drachensohns", "tokens": ["So", "wie", "Ze\u00b7sar", "vor\u00b7dem", "weint'", "an", "des", "Dra\u00b7chen\u00b7sohns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NE", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bilde: J\u00fcngling nicht mehr hab' er noch nichts gethan!", "tokens": ["Bil\u00b7de", ":", "J\u00fcng\u00b7ling", "nicht", "mehr", "hab'", "er", "noch", "nichts", "ge\u00b7than", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PTKNEG", "ADV", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Also weint an des edlern", "tokens": ["Al\u00b7so", "weint", "an", "des", "ed\u00b7lern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Denkmaal einst der Eroberer.", "tokens": ["Denk\u00b7maal", "einst", "der", "Er\u00b7o\u00b7be\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}