{"textgrid.poem.35597": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Abendl\u00e4ndisches Lied", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O der Seele n\u00e4chtlicher Fl\u00fcgelschlag:", "tokens": ["O", "der", "See\u00b7le", "n\u00e4cht\u00b7li\u00b7cher", "Fl\u00fc\u00b7gel\u00b7schlag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hirten gingen wir einst an d\u00e4mmernden W\u00e4ldern hin", "tokens": ["Hir\u00b7ten", "gin\u00b7gen", "wir", "einst", "an", "d\u00e4m\u00b7mern\u00b7den", "W\u00e4l\u00b7dern", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und es folgte das rote Wild, die gr\u00fcne Blume und der lallende Quell", "tokens": ["Und", "es", "folg\u00b7te", "das", "ro\u00b7te", "Wild", ",", "die", "gr\u00fc\u00b7ne", "Blu\u00b7me", "und", "der", "lal\u00b7len\u00b7de", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+-+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Demutsvoll. O, der uralte Ton des Heimchens,", "tokens": ["De\u00b7muts\u00b7voll", ".", "O", ",", "der", "ur\u00b7al\u00b7te", "Ton", "des", "Heim\u00b7chens", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Blut bl\u00fchend am Opferstein", "tokens": ["Blut", "bl\u00fc\u00b7hend", "am", "Op\u00b7fer\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPRART", "NN"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und der Schrei des einsamen Vogels \u00fcber der gr\u00fcnen Stille des Teichs.", "tokens": ["Und", "der", "Schrei", "des", "ein\u00b7sa\u00b7men", "Vo\u00b7gels", "\u00fc\u00b7ber", "der", "gr\u00fc\u00b7nen", "Stil\u00b7le", "des", "Teichs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O, ihr Kreuzz\u00fcge und gl\u00fchenden Martern", "tokens": ["O", ",", "ihr", "Kreuz\u00b7z\u00fc\u00b7ge", "und", "gl\u00fc\u00b7hen\u00b7den", "Mar\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Des Fleisches, Fallen purpurner Fr\u00fcchte", "tokens": ["Des", "Flei\u00b7sches", ",", "Fal\u00b7len", "pur\u00b7pur\u00b7ner", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Abendgarten, wo vor Zeiten die frommen J\u00fcnger gegangen,", "tokens": ["Im", "A\u00b7bend\u00b7gar\u00b7ten", ",", "wo", "vor", "Zei\u00b7ten", "die", "from\u00b7men", "J\u00fcn\u00b7ger", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Kriegsleute nun, erwachend aus Wunden und Sternentr\u00e4umen.", "tokens": ["Kriegs\u00b7leu\u00b7te", "nun", ",", "er\u00b7wa\u00b7chend", "aus", "Wun\u00b7den", "und", "Ster\u00b7nen\u00b7tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "O, das sanfte Zyanenb\u00fcndel der Nacht.", "tokens": ["O", ",", "das", "sanf\u00b7te", "Zy\u00b7a\u00b7nen\u00b7b\u00fcn\u00b7del", "der", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O, ihr Zeiten der Stille und goldener Herbste,", "tokens": ["O", ",", "ihr", "Zei\u00b7ten", "der", "Stil\u00b7le", "und", "gol\u00b7de\u00b7ner", "Herbs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da wir friedliche M\u00f6nche die purpurne Traube gekeltert;", "tokens": ["Da", "wir", "fried\u00b7li\u00b7che", "M\u00f6n\u00b7che", "die", "pur\u00b7pur\u00b7ne", "Trau\u00b7be", "ge\u00b7kel\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und rings ergl\u00e4nzten H\u00fcgel und Wald.", "tokens": ["Und", "rings", "er\u00b7gl\u00e4nz\u00b7ten", "H\u00fc\u00b7gel", "und", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "O, ihr Jagden und Schl\u00f6sser; Ruh des Abends,", "tokens": ["O", ",", "ihr", "Jag\u00b7den", "und", "Schl\u00f6s\u00b7ser", ";", "Ruh", "des", "A\u00b7bends", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da in seiner Kammer der Mensch Gerechtes sann,", "tokens": ["Da", "in", "sei\u00b7ner", "Kam\u00b7mer", "der", "Mensch", "Ge\u00b7rech\u00b7tes", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In stummem Gebet um Gottes lebendiges Haupt rang.", "tokens": ["In", "stum\u00b7mem", "Ge\u00b7bet", "um", "Got\u00b7tes", "le\u00b7ben\u00b7di\u00b7ges", "Haupt", "rang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "O, die bittere Stunde des Untergangs,", "tokens": ["O", ",", "die", "bit\u00b7te\u00b7re", "Stun\u00b7de", "des", "Un\u00b7ter\u00b7gangs", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da wir ein steinernes Antlitz in schwarzen Wassern beschaun.", "tokens": ["Da", "wir", "ein", "stei\u00b7ner\u00b7nes", "Ant\u00b7litz", "in", "schwar\u00b7zen", "Was\u00b7sern", "be\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber strahlend heben die silbernen Lider die Liebenden:", "tokens": ["A\u00b7ber", "strah\u00b7lend", "he\u00b7ben", "die", "sil\u00b7ber\u00b7nen", "Li\u00b7der", "die", "Lie\u00b7ben\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+--+--", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein Geschlecht. Weihrauch str\u00f6mt von rosigen Kissen", "tokens": ["Ein", "Ge\u00b7schlecht", ".", "Weih\u00b7rauch", "str\u00f6mt", "von", "ro\u00b7si\u00b7gen", "Kis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und der s\u00fc\u00dfe Gesang der Auferstandenen.", "tokens": ["Und", "der", "s\u00fc\u00b7\u00dfe", "Ge\u00b7sang", "der", "Auf\u00b7er\u00b7stan\u00b7de\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}}}}}