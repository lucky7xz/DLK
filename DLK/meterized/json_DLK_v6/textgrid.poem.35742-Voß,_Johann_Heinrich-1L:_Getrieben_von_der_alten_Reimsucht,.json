{"textgrid.poem.35742": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Getrieben von der alten Reimsucht,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getrieben von der alten Reimsucht,", "tokens": ["Ge\u00b7trie\u00b7ben", "von", "der", "al\u00b7ten", "Reim\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Irrst du umher, und brummst voll Angst,", "tokens": ["Irrst", "du", "um\u00b7her", ",", "und", "brummst", "voll", "Angst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich einem B\u00e4r, der Honigseim sucht,", "tokens": ["Gleich", "ei\u00b7nem", "B\u00e4r", ",", "der", "Ho\u00b7ni\u00b7gseim", "sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bis du den neuen Reim erlangst.", "tokens": ["Bis", "du", "den", "neu\u00b7en", "Reim", "er\u00b7langst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald siehst du Hilfe bei dem Aga", "tokens": ["Bald", "siehst", "du", "Hil\u00b7fe", "bei", "dem", "A\u00b7ga"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schar am Aganippeborn,", "tokens": ["Der", "Schar", "am", "A\u00b7gan\u00b7ip\u00b7pe\u00b7born", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bald hofierst du Deutschlands Braga", "tokens": ["Und", "bald", "ho\u00b7fierst", "du", "Deutschlands", "Bra\u00b7ga"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem Auerochsenhorn.", "tokens": ["Mit", "dei\u00b7nem", "Au\u00b7e\u00b7roch\u00b7sen\u00b7horn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Itzt fei'rt die Lyra Zeus im Erzschild,", "tokens": ["Itzt", "fei'rt", "die", "Ly\u00b7ra", "Zeus", "im", "Erz\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie er erhebt die Flammenfaust,", "tokens": ["Wie", "er", "er\u00b7hebt", "die", "Flam\u00b7men\u00b7faust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus Gewittern niederw\u00e4rts schilt,", "tokens": ["Und", "aus", "Ge\u00b7wit\u00b7tern", "nie\u00b7der\u00b7w\u00e4rts", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df Berg und Wald und Fels zergraust;", "tokens": ["Da\u00df", "Berg", "und", "Wald", "und", "Fels", "zer\u00b7graust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Bardenschrei schreit itzt vom Harzwald", "tokens": ["Dein", "Bar\u00b7den\u00b7schrei", "schreit", "itzt", "vom", "Harz\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Wutgeheul des Weserstroms,", "tokens": ["Ins", "Wut\u00b7ge\u00b7heul", "des", "We\u00b7ser\u00b7stroms", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der von dem Blute Varus' schwarz wallt,", "tokens": ["Der", "von", "dem", "Blu\u00b7te", "Va\u00b7rus'", "schwarz", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ins Angstgeheul der H\u00fcgel Roms;", "tokens": ["Ins", "Angst\u00b7ge\u00b7heul", "der", "H\u00fc\u00b7gel", "Roms", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Itzt bitterst du dein Lied mit Wermut,", "tokens": ["Itzt", "bit\u00b7terst", "du", "dein", "Lied", "mit", "Wer\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00e4chzest, wie vom Turm der Kauz,", "tokens": ["Und", "\u00e4ch\u00b7zest", ",", "wie", "vom", "Turm", "der", "Kauz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mondaufheult mit erhobner Schnauz.", "tokens": ["Mond\u00b7auf\u00b7heult", "mit", "er\u00b7hob\u00b7ner", "Schnauz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Allein was marterst du und grillst dich?", "tokens": ["Al\u00b7lein", "was", "mar\u00b7terst", "du", "und", "grillst", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ruhm, wonach du ringst, ist Luft,", "tokens": ["Der", "Ruhm", ",", "wo\u00b7nach", "du", "ringst", ",", "ist", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Seifenblase, steigt schw\u00fclstig,", "tokens": ["Ist", "Sei\u00b7fen\u00b7bla\u00b7se", ",", "steigt", "schw\u00fcls\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schwimmt fort, und schimmert, und zerpufft!", "tokens": ["Schwimmt", "fort", ",", "und", "schim\u00b7mert", ",", "und", "zer\u00b7pufft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Lies die Dramaturgie von Lessing:", "tokens": ["Lies", "die", "Dra\u00b7ma\u00b7tur\u00b7gie", "von", "Les\u00b7sing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ist er auf Dichternamen stolz?", "tokens": ["Ist", "er", "auf", "Dich\u00b7ter\u00b7na\u00b7men", "stolz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Poetenruhm gl\u00e4nzt nur wie Messing;", "tokens": ["Poe\u00b7ten\u00b7ruhm", "gl\u00e4nzt", "nur", "wie", "Mes\u00b7sing", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KOKOM", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Brotweisheit hat den Wert des Golds!", "tokens": ["Brot\u00b7we\u00b7is\u00b7heit", "hat", "den", "Wert", "des", "Golds", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Wenn nun begeistert du gewaltsam", "tokens": ["Wenn", "nun", "be\u00b7geis\u00b7tert", "du", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Feder k\u00e4ust, den Boden stampfst,", "tokens": ["Die", "Fe\u00b7der", "k\u00e4ust", ",", "den", "Bo\u00b7den", "stampfst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Augen drehst, und unaufhaltsam", "tokens": ["Die", "Au\u00b7gen", "drehst", ",", "und", "un\u00b7auf\u00b7halt\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus offnem Schlund die Glut verdampfst;", "tokens": ["Aus", "off\u00b7nem", "Schlund", "die", "Glut", "ver\u00b7dampfst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und jetzt nach langer langer Arbeit", "tokens": ["Und", "jetzt", "nach", "lan\u00b7ger", "lan\u00b7ger", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein P\u00e4an sich herausgew\u00fcrgt,", "tokens": ["Ein", "P\u00e4\u00b7an", "sich", "her\u00b7aus\u00b7ge\u00b7w\u00fcrgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Trotz der ganzen Musenschar beut,", "tokens": ["Der", "Trotz", "der", "gan\u00b7zen", "Mu\u00b7sen\u00b7schar", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem schamrot Pindar selbst sich birgt:", "tokens": ["Dem", "scham\u00b7rot", "Pin\u00b7dar", "selbst", "sich", "birgt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was hast du denn, als Kopf- und Bauchweh,", "tokens": ["Was", "hast", "du", "denn", ",", "als", "Kopf", "und", "Bauch\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "KOUS", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Aschgesicht und schwindelnd Hirn?", "tokens": ["Und", "Aschge\u00b7sicht", "und", "schwin\u00b7delnd", "Hirn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ja Daum und Finger thut dir auch weh,", "tokens": ["Ja", "Daum", "und", "Fin\u00b7ger", "thut", "dir", "auch", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und macht dich gleich dem Kranich girrn!", "tokens": ["Und", "macht", "dich", "gleich", "dem", "Kra\u00b7nich", "girrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mit schiefem Geifermaul umquackt dich", "tokens": ["Mit", "schie\u00b7fem", "Gei\u00b7fer\u00b7maul", "um\u00b7quackt", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Froschgeschw\u00e4tz der krit'schen Zunft,", "tokens": ["Das", "Froschge\u00b7schw\u00e4tz", "der", "krit'\u00b7schen", "Zunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und jeder kahle Hundsfott plackt dich", "tokens": ["Und", "je\u00b7der", "kah\u00b7le", "Hunds\u00b7fott", "plackt", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und zeiht dein Lied der Unvernunft.", "tokens": ["Und", "zeiht", "dein", "Lied", "der", "Un\u00b7ver\u00b7nunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Drum jag die Musen all zum Satan,", "tokens": ["Drum", "jag", "die", "Mu\u00b7sen", "all", "zum", "Sa\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PIAT", "APPRART", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein Raub der gl\u00fchnden B\u00e4rentatz,", "tokens": ["Ein", "Raub", "der", "gl\u00fchn\u00b7den", "B\u00e4\u00b7ren\u00b7tatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimm, o ", "tokens": ["Und", "nimm", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVIMP", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der ba\u00df dir thun wird, dann Horaz.", "tokens": ["Der", "ba\u00df", "dir", "thun", "wird", ",", "dann", "Ho\u00b7raz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "VAFIN", "$,", "ADV", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Zeuch aus den Flausrock deiner Drangsal,", "tokens": ["Zeuch", "aus", "den", "Flaus\u00b7rock", "dei\u00b7ner", "Dran\u00b7gsal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und putze dich, und eile flugs", "tokens": ["Und", "put\u00b7ze", "dich", ",", "und", "ei\u00b7le", "flugs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum kerzerhellten bunten Klangsaal,", "tokens": ["Zum", "ker\u00b7zer\u00b7hell\u00b7ten", "bun\u00b7ten", "Klang\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Durcht\u00f6nt von Erz und Darm und Bux!", "tokens": ["Durch\u00b7t\u00f6nt", "von", "Erz", "und", "Darm", "und", "Bux", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.14": {"line.1": {"text": "Dort geiget heut der gro\u00dfe ", "tokens": ["Dort", "gei\u00b7get", "heut", "der", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(wem schwand nicht schnell der schwarze Harm,", "tokens": ["(", "wem", "schwand", "nicht", "schnell", "der", "schwar\u00b7ze", "Harm", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ohren ", "tokens": ["Der", "Oh\u00b7ren