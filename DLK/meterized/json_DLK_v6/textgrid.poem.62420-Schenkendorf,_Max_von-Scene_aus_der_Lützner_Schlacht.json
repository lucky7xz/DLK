{"textgrid.poem.62420": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Scene aus der L\u00fctzner Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sprenget auf dem stolzen Ro\u00df", "tokens": ["Wer", "spren\u00b7get", "auf", "dem", "stol\u00b7zen", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis in die vordern Reihen,", "tokens": ["Bis", "in", "die", "vor\u00b7dern", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und will dem Eisen, dem Gescho\u00df", "tokens": ["Und", "will", "dem", "Ei\u00b7sen", ",", "dem", "Ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das muntre Leben weihen?", "tokens": ["Das", "mun\u00b7tre", "Le\u00b7ben", "wei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das ist ein junger K\u00f6nigssohn,", "tokens": ["Das", "ist", "ein", "jun\u00b7ger", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erbe von dem Preu\u00dfenthron.", "tokens": ["Der", "Er\u00b7be", "von", "dem", "Preu\u00b7\u00dfen\u00b7thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drob z\u00fcrnet ihm des K\u00f6nigs Muth", "tokens": ["Drob", "z\u00fcr\u00b7net", "ihm", "des", "K\u00f6\u00b7nigs", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und straft mit mildem Worte:", "tokens": ["Und", "straft", "mit", "mil\u00b7dem", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbzur\u00fcck, du junges Zollernblut,", "tokens": ["\u00bb", "zu\u00b7r\u00fcck", ",", "du", "jun\u00b7ges", "Zol\u00b7lern\u00b7blut", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum angewies'nen Orte.", "tokens": ["Zum", "an\u00b7ge\u00b7wies'\u00b7nen", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du rascher, junger K\u00f6nigssohn", "tokens": ["Du", "ra\u00b7scher", ",", "jun\u00b7ger", "K\u00f6\u00b7nigs\u00b7sohn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00dft erben ja den hohen Thron.", "tokens": ["Mu\u00dft", "er\u00b7ben", "ja", "den", "ho\u00b7hen", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O reite junges, edles Wild,", "tokens": ["O", "rei\u00b7te", "jun\u00b7ges", ",", "ed\u00b7les", "Wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du ritterlicher Degen, \u2013", "tokens": ["Du", "rit\u00b7ter\u00b7li\u00b7cher", "De\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Himmel schaut ein sel'ges Bild", "tokens": ["Vom", "Him\u00b7mel", "schaut", "ein", "sel'\u00b7ges", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lust nach deinen Wegen,", "tokens": ["Mit", "Lust", "nach", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Mutter sch\u00fctzt den K\u00f6nigssohn,", "tokens": ["Die", "Mut\u00b7ter", "sch\u00fctzt", "den", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du erbest doch der V\u00e4ter Thron.", "tokens": ["Du", "er\u00b7best", "doch", "der", "V\u00e4\u00b7ter", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du wirst uns lang' im Ehrenfeld", "tokens": ["Du", "wirst", "uns", "lang'", "im", "Eh\u00b7ren\u00b7feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blick und Schwert regieren,", "tokens": ["Mit", "Blick", "und", "Schwert", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In sp\u00e4ten Jahren, werther Held,", "tokens": ["In", "sp\u00e4\u00b7ten", "Jah\u00b7ren", ",", "wert\u00b7her", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein frommes Scepter f\u00fchren.", "tokens": ["Ein", "from\u00b7mes", "Scep\u00b7ter", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du rascher, lieber K\u00f6nigssohn,", "tokens": ["Du", "ra\u00b7scher", ",", "lie\u00b7ber", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir retten auch f\u00fcr dich den Thron.", "tokens": ["Wir", "ret\u00b7ten", "auch", "f\u00fcr", "dich", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}