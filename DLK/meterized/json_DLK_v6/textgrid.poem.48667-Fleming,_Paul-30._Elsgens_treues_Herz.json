{"textgrid.poem.48667": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "30. Elsgens treues Herz", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein getreues Herze wissen", "tokens": ["Ein", "ge\u00b7treu\u00b7es", "Her\u00b7ze", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hat des h\u00f6chsten Schatzes Preis.", "tokens": ["hat", "des", "h\u00f6chs\u00b7ten", "Schat\u00b7zes", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ist selig zu begr\u00fc\u00dfen,", "tokens": ["Der", "ist", "se\u00b7lig", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der ein treues Herze wei\u00df.", "tokens": ["der", "ein", "treu\u00b7es", "Her\u00b7ze", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir ist wol bei h\u00f6chstem Schmerze,", "tokens": ["Mir", "ist", "wol", "bei", "h\u00f6chs\u00b7tem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "denn ich wei\u00df ein treues Herze.", "tokens": ["denn", "ich", "wei\u00df", "ein", "treu\u00b7es", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4uft das Gl\u00fccke gleich zu Zeiten", "tokens": ["L\u00e4uft", "das", "Gl\u00fc\u00b7cke", "gleich", "zu", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "anders, als man will und meint,", "tokens": ["an\u00b7ders", ",", "als", "man", "will", "und", "meint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "VMFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ein getreues Herz' hilft streiten", "tokens": ["ein", "ge\u00b7treu\u00b7es", "Her\u00b7z'", "hilft", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wider Alles, was ist Feind.", "tokens": ["wi\u00b7der", "Al\u00b7les", ",", "was", "ist", "Feind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir ist wol bei h\u00f6chstem Schmerze,", "tokens": ["Mir", "ist", "wol", "bei", "h\u00f6chs\u00b7tem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "denn ich wei\u00df ein treues Herze.", "tokens": ["denn", "ich", "wei\u00df", "ein", "treu\u00b7es", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein Vergn\u00fcgen steht alleine", "tokens": ["Sein", "Ver\u00b7gn\u00fc\u00b7gen", "steht", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in des andern Redligkeit,", "tokens": ["in", "des", "an\u00b7dern", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "h\u00e4lt des Andern Not f\u00fcr seine,", "tokens": ["h\u00e4lt", "des", "An\u00b7dern", "Not", "f\u00fcr", "sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weicht nicht auch bei b\u00f6ser Zeit.", "tokens": ["weicht", "nicht", "auch", "bei", "b\u00f6\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir ist wol bei h\u00f6chstem Schmerze,", "tokens": ["Mir", "ist", "wol", "bei", "h\u00f6chs\u00b7tem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "denn ich wei\u00df ein treues Herze.", "tokens": ["denn", "ich", "wei\u00df", "ein", "treu\u00b7es", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gunst, die kehrt sich nach dem Gl\u00fccke,", "tokens": ["Gunst", ",", "die", "kehrt", "sich", "nach", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geld und Reichtum, das zerst\u00e4ubt,", "tokens": ["Geld", "und", "Reich\u00b7tum", ",", "das", "zer\u00b7st\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nheit l\u00e4\u00dft uns bald zur\u00fccke,", "tokens": ["Sch\u00f6n\u00b7heit", "l\u00e4\u00dft", "uns", "bald", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ein getreues Herze bleibt.", "tokens": ["ein", "ge\u00b7treu\u00b7es", "Her\u00b7ze", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir ist wol bei h\u00f6chstem Schmerze,", "tokens": ["Mir", "ist", "wol", "bei", "h\u00f6chs\u00b7tem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "denn ich wei\u00df ein treues Herze.", "tokens": ["denn", "ich", "wei\u00df", "ein", "treu\u00b7es", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eins ist da sein und geschieden.", "tokens": ["Eins", "ist", "da", "sein", "und", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VAINF", "KON", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein getreues Herze h\u00e4lt,", "tokens": ["Ein", "ge\u00b7treu\u00b7es", "Her\u00b7ze", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "giebt sich allezeit zufrieden,", "tokens": ["giebt", "sich", "al\u00b7le\u00b7zeit", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "steht auf, wenn es niederf\u00e4llt.", "tokens": ["steht", "auf", ",", "wenn", "es", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich bin froh bei h\u00f6chstem Schmerze,", "tokens": ["Ich", "bin", "froh", "bei", "h\u00f6chs\u00b7tem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "denn ich wei\u00df ein treues Herze.", "tokens": ["denn", "ich", "wei\u00df", "ein", "treu\u00b7es", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nichts ist s\u00fc\u00dfers, als zwei Treue,", "tokens": ["Nichts", "ist", "s\u00fc\u00b7\u00dfers", ",", "als", "zwei", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "$,", "KOUS", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn sie eines worden sein.", "tokens": ["wenn", "sie", "ei\u00b7nes", "wor\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VAPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Di\u00df ists, das ich mich erfreue,", "tokens": ["Di\u00df", "ists", ",", "das", "ich", "mich", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und sie giebt ihr ja auch drein.", "tokens": ["und", "sie", "giebt", "ihr", "ja", "auch", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir ist wol bei h\u00f6chstem Schmerze,", "tokens": ["Mir", "ist", "wol", "bei", "h\u00f6chs\u00b7tem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "denn ich wei\u00df ein treues Herze.", "tokens": ["denn", "ich", "wei\u00df", "ein", "treu\u00b7es", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}