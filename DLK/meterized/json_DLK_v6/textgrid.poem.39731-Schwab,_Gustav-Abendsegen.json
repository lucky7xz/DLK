{"textgrid.poem.39731": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Abendsegen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank, Vater! dir f\u00fcr Leid und Lust", "tokens": ["Dank", ",", "Va\u00b7ter", "!", "dir", "f\u00fcr", "Leid", "und", "Lust"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was du mir gegeben.", "tokens": ["Und", "was", "du", "mir", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich, wie dieses liebe Heut,", "tokens": ["La\u00df", "mich", ",", "wie", "die\u00b7ses", "lie\u00b7be", "Heut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Morgen auch erleben.", "tokens": ["Mein", "Mor\u00b7gen", "auch", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erf\u00fcll' mir keinen th\u00f6r'gen Wunsch,", "tokens": ["Er\u00b7f\u00fcll'", "mir", "kei\u00b7nen", "th\u00f6r'\u00b7gen", "Wunsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gute la\u00df nicht s\u00e4umen.", "tokens": ["Das", "Gu\u00b7te", "la\u00df", "nicht", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und was du mir nicht geben kannst,", "tokens": ["Und", "was", "du", "mir", "nicht", "ge\u00b7ben", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ei, davon la\u00df mich tr\u00e4umen!", "tokens": ["Ei", ",", "da\u00b7von", "la\u00df", "mich", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}