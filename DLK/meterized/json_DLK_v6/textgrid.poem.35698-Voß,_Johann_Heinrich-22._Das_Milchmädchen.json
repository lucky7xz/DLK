{"textgrid.poem.35698": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "22. Das Milchm\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, nehmt die Eimer schnell,", "tokens": ["M\u00e4d\u00b7chen", ",", "nehmt", "die", "Ei\u00b7mer", "schnell", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Habt ihr ausgemolken!", "tokens": ["Habt", "ihr", "aus\u00b7ge\u00b7mol\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seht, die Sterne blinken hell,", "tokens": ["Seht", ",", "die", "Ster\u00b7ne", "blin\u00b7ken", "hell", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Vollmond guckt so grell", "tokens": ["Und", "der", "Voll\u00b7mond", "guckt", "so", "grell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Aus den krausen Wolken!", "tokens": ["Aus", "den", "krau\u00b7sen", "Wol\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lieg' und wiederk\u00e4u' in Ruh'", "tokens": ["Lieg'", "und", "wie\u00b7der\u00b7k\u00e4u'", "in", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein gesundes Futter!", "tokens": ["Dein", "ge\u00b7sun\u00b7des", "Fut\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alles, gute fromme Kuh,", "tokens": ["Al\u00b7les", ",", "gu\u00b7te", "from\u00b7me", "Kuh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Milch und K\u00e4se schenkest du,", "tokens": ["Milch", "und", "K\u00e4\u00b7se", "schen\u00b7kest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rahm und s\u00fc\u00dfe Butter!", "tokens": ["Rahm", "und", "s\u00fc\u00b7\u00dfe", "But\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ruhig l\u00e4uten durch das Feld", "tokens": ["Ru\u00b7hig", "l\u00e4u\u00b7ten", "durch", "das", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dumpfe Rinderglocken;", "tokens": ["Dump\u00b7fe", "Rin\u00b7der\u00b7glo\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Hund im Dorfe bellt,", "tokens": ["Und", "der", "Hund", "im", "Dor\u00b7fe", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und der Schlag der Wachtel gellt", "tokens": ["Und", "der", "Schlag", "der", "Wach\u00b7tel", "gellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Im betauten Roggen!", "tokens": ["Im", "be\u00b7tau\u00b7ten", "Rog\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "M\u00e4dchen, singt mit frohem Schall;", "tokens": ["M\u00e4d\u00b7chen", ",", "singt", "mit", "fro\u00b7hem", "Schall", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer nicht singt, den grauet!", "tokens": ["Wer", "nicht", "singt", ",", "den", "grau\u00b7et", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "ART", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6rt den sch\u00f6nen Wiederhall", "tokens": ["H\u00f6rt", "den", "sch\u00f6\u00b7nen", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort im Wald' und Erlenthal,", "tokens": ["Dort", "im", "Wald'", "und", "Er\u00b7len\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Hase brauet!", "tokens": ["Wo", "der", "Ha\u00b7se", "brau\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "T\u00f6chterlein, nimm dich in acht,", "tokens": ["T\u00f6ch\u00b7ter\u00b7lein", ",", "nimm", "dich", "in", "acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "APPR", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm mir bald zu Hause!", "tokens": ["Komm", "mir", "bald", "zu", "Hau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sagt die Mutter: in der Nacht", "tokens": ["Sagt", "die", "Mut\u00b7ter", ":", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schw\u00e4rmt des Teufels wilde Jagd", "tokens": ["Schw\u00e4rmt", "des", "Teu\u00b7fels", "wil\u00b7de", "Jagd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit des Sturms Gesause!", "tokens": ["Mit", "des", "Sturms", "Ge\u00b7sau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ein geh\u00f6rnter schwarzer Mann", "tokens": ["Ein", "ge\u00b7h\u00f6rn\u00b7ter", "schwar\u00b7zer", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt oft hulter pulter!", "tokens": ["Kommt", "oft", "hul\u00b7ter", "pul\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Guckt mit gl\u00fchndem Aug' dich an,", "tokens": ["Guckt", "mit", "gl\u00fchn\u00b7dem", "Aug'", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kneipt dich mit der Krall', und dann", "tokens": ["Kneipt", "dich", "mit", "der", "Krall'", ",", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$,", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hockt er auf die Schulter!", "tokens": ["Hockt", "er", "auf", "die", "Schul\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "M\u00e4dchen, wandelt fr\u00fch und sp\u00e4t,", "tokens": ["M\u00e4d\u00b7chen", ",", "wan\u00b7delt", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz den klugen M\u00fcttern!", "tokens": ["Trotz", "den", "klu\u00b7gen", "M\u00fct\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wer auf guten Wegen geht,", "tokens": ["Wer", "auf", "gu\u00b7ten", "We\u00b7gen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf Kreuze sich versteht,", "tokens": ["Und", "auf", "Kreu\u00b7ze", "sich", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Darf vor Spuk nicht zittern!", "tokens": ["Darf", "vor", "Spuk", "nicht", "zit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Zwar mich fa\u00dft ein B\u00f6sewicht", "tokens": ["Zwar", "mich", "fa\u00dft", "ein", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchmal um den Nacken;", "tokens": ["Manch\u00b7mal", "um", "den", "Na\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber rot ist sein Gesicht,", "tokens": ["A\u00b7ber", "rot", "ist", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Krallen kneipt er nicht", "tokens": ["Und", "mit", "Kral\u00b7len", "kneipt", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freundlich meine Backen!", "tokens": ["Freund\u00b7lich", "mei\u00b7ne", "Ba\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Dieser hei\u00dft, das Ohr gespitzt!", "tokens": ["Die\u00b7ser", "hei\u00dft", ",", "das", "Ohr", "ge\u00b7spitzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wilhelm und so ferner:", "tokens": ["Wil\u00b7helm", "und", "so", "fer\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zwar sein blaues Auge blitzt;", "tokens": ["Zwar", "sein", "blau\u00b7es", "Au\u00b7ge", "blitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber, wenigstens bis itzt,", "tokens": ["A\u00b7ber", ",", "we\u00b7nigs\u00b7tens", "bis", "itzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4gt er keine H\u00f6rner!", "tokens": ["Tr\u00e4gt", "er", "kei\u00b7ne", "H\u00f6r\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}