{"textgrid.poem.53598": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Eink\u00e4ufe", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schenke ich dem kleinen Michel", "tokens": ["Was", "schen\u00b7ke", "ich", "dem", "klei\u00b7nen", "Mi\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "zu diesem kalten Weihnachtsfest?", "tokens": ["zu", "die\u00b7sem", "kal\u00b7ten", "Weih\u00b7nachts\u00b7fest", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kullerball? Den Sabberpichel?", "tokens": ["Den", "Kul\u00b7ler\u00b7ball", "?", "Den", "Sab\u00b7ber\u00b7pi\u00b7chel", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gummikissen, das nicht n\u00e4\u00dft?", "tokens": ["Ein", "Gum\u00b7mi\u00b7kis\u00b7sen", ",", "das", "nicht", "n\u00e4\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein kleines Seifensiederlicht?", "tokens": ["Ein", "klei\u00b7nes", "Sei\u00b7fen\u00b7sie\u00b7der\u00b7licht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er noch nicht. Das hat er noch nicht!", "tokens": ["Das", "hat", "er", "noch", "nicht", ".", "Das", "hat", "er", "noch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$.", "PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "W\u00e4hl ich den Wiederaufbaukasten?", "tokens": ["W\u00e4hl", "ich", "den", "Wie\u00b7der\u00b7auf\u00b7bau\u00b7kas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schenk ich ihm noch mehr Schreibpapier?", "tokens": ["Schenk", "ich", "ihm", "noch", "mehr", "Schreib\u00b7pa\u00b7pier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Ding mit schwarzwei\u00dfroten Tasten;", "tokens": ["Ein", "Ding", "mit", "schwarz\u00b7wei\u00df\u00b7ro\u00b7ten", "Tas\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein patriotisches Klavier?", "tokens": ["ein", "pat\u00b7ri\u00b7o\u00b7ti\u00b7sches", "Kla\u00b7vier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ein objektives Kriegsgericht?", "tokens": ["Ein", "ob\u00b7jek\u00b7ti\u00b7ves", "Kriegs\u00b7ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Das hat er noch nicht. Das hat er noch nicht!", "tokens": ["Das", "hat", "er", "noch", "nicht", ".", "Das", "hat", "er", "noch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$.", "PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schenk ich den Nachttopf ihm auf Rollen?", "tokens": ["Schenk", "ich", "den", "Nacht\u00b7topf", "ihm", "auf", "Rol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schenk ich ein Moratorium?", "tokens": ["Schenk", "ich", "ein", "Mo\u00b7ra\u00b7to\u00b7ri\u00b7um", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sparschwein, kugelig geschwollen?", "tokens": ["Ein", "Spar\u00b7schwein", ",", "ku\u00b7ge\u00b7lig", "ge\u00b7schwol\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Puppenkrematorium?", "tokens": ["Ein", "Pup\u00b7pen\u00b7kre\u00b7ma\u00b7to\u00b7ri\u00b7um", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein neues gescheites Reichsgericht?", "tokens": ["Ein", "neu\u00b7es", "ge\u00b7schei\u00b7tes", "Reichs\u00b7ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das hat er noch nicht. Das hat er noch nicht!", "tokens": ["Das", "hat", "er", "noch", "nicht", ".", "Das", "hat", "er", "noch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$.", "PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach, liebe Basen, Onkels, Tanten \u2013", "tokens": ["Ach", ",", "lie\u00b7be", "Ba\u00b7sen", ",", "On\u00b7kels", ",", "Tan\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "NE", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schenkt ihr ihm was. Ich find es kaum.", "tokens": ["Schenkt", "ihr", "ihm", "was", ".", "Ich", "find", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIS", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr seid die Fixen und Gewandten,", "tokens": ["Ihr", "seid", "die", "Fi\u00b7xen", "und", "Ge\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "h\u00e4ngt ihrs ihm untern Tannenbaum.", "tokens": ["h\u00e4ngt", "ihrs", "ihm", "un\u00b7tern", "Tan\u00b7nen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch schenkt ihm keine Reaktion!", "tokens": ["Doch", "schenkt", "ihm", "kei\u00b7ne", "Re\u00b7ak\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die hat er schon. Die hat er schon!", "tokens": ["Die", "hat", "er", "schon", ".", "Die", "hat", "er", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$.", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}