{"dta.poem.16308": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol manger Mann, wie gro\u00df geworden ist sein Heil,", "tokens": ["Wol", "man\u00b7ger", "Mann", ",", "wie", "gro\u00df", "ge\u00b7wor\u00b7den", "ist", "sein", "Heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PWAV", "ADJD", "VAPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mag meinen, da\u00df ihm viel zu wenig ward zu Theil;", "tokens": ["Mag", "mei\u00b7nen", ",", "da\u00df", "ihm", "viel", "zu", "we\u00b7nig", "ward", "zu", "Theil", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "PTKA", "PIS", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wollt' er sich mit Ernst erpr\u00fcfen, nicht zum Spiel,", "tokens": ["Und", "wollt'", "er", "sich", "mit", "Ernst", "er\u00b7pr\u00fc\u00b7fen", ",", "nicht", "zum", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "NE", "VVINF", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00fcst' er gestehn, da\u00df ihm zu Theil ward viel zu viel.", "tokens": ["M\u00fcst'", "er", "ge\u00b7stehn", ",", "da\u00df", "ihm", "zu", "Theil", "ward", "viel", "zu", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word",