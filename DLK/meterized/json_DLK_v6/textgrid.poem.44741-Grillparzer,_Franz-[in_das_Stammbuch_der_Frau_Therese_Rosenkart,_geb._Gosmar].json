{"textgrid.poem.44741": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in das Stammbuch der Frau Therese Rosenkart, geb. Gosmar]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur selten sah ich dich, ja sprach dich kaum zuvor;", "tokens": ["Nur", "sel\u00b7ten", "sah", "ich", "dich", ",", "ja", "sprach", "dich", "kaum", "zu\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dennoch kenn ich dich, will deinen Wert beschw\u00f6ren.", "tokens": ["Und", "den\u00b7noch", "kenn", "ich", "dich", ",", "will", "dei\u00b7nen", "Wert", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Langweilig ist der Weg von Mund zu Ohr;", "tokens": ["Lang\u00b7wei\u00b7lig", "ist", "der", "Weg", "von", "Mund", "zu", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch was ein Auge spricht, kann auch ein Auge h\u00f6ren.", "tokens": ["Doch", "was", "ein", "Au\u00b7ge", "spricht", ",", "kann", "auch", "ein", "Au\u00b7ge", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}