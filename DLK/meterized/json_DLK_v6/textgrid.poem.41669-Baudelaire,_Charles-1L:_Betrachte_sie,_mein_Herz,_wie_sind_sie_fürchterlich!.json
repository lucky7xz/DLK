{"textgrid.poem.41669": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Betrachte sie, mein Herz, wie sind sie f\u00fcrchterlich!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betrachte sie, mein Herz, wie sind sie f\u00fcrchterlich!", "tokens": ["Be\u00b7trach\u00b7te", "sie", ",", "mein", "Herz", ",", "wie", "sind", "sie", "f\u00fcrch\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Gliederpuppen gleich, fast l\u00e4cherlich zu schauen;", "tokens": ["Den", "Glie\u00b7der\u00b7pup\u00b7pen", "gleich", ",", "fast", "l\u00e4\u00b7cher\u00b7lich", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie Nachtwandelnde erwecken sie uns Grauen.", "tokens": ["Und", "wie", "Nacht\u00b7wan\u00b7deln\u00b7de", "er\u00b7we\u00b7cken", "sie", "uns", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVFIN", "PPER", "PPER", "NN", "$."], "meter": "--++-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Durchs Leere tastet ihr erstorben Auge sich.", "tokens": ["Durchs", "Lee\u00b7re", "tas\u00b7tet", "ihr", "ers\u00b7tor\u00b7ben", "Au\u00b7ge", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Augen, draus entflohn das Licht, das gottgeschenkte,", "tokens": ["Die", "Au\u00b7gen", ",", "draus", "ent\u00b7flohn", "das", "Licht", ",", "das", "gott\u00b7ge\u00b7schenk\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erheben sie, als ob sie in die Ferne s\u00e4hn,", "tokens": ["Er\u00b7he\u00b7ben", "sie", ",", "als", "ob", "sie", "in", "die", "Fer\u00b7ne", "s\u00e4hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Himmel. Niemals noch sahst je du einen gehn,", "tokens": ["Zum", "Him\u00b7mel", ".", "Nie\u00b7mals", "noch", "sahst", "je", "du", "ei\u00b7nen", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "ADV", "VVFIN", "ADV", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der tr\u00e4umerisch sein Haupt zu Boden niedersenkte.", "tokens": ["Der", "tr\u00e4u\u00b7me\u00b7risch", "sein", "Haupt", "zu", "Bo\u00b7den", "nie\u00b7der\u00b7senk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Dunkel unbegrenzt, das sie umfangen hat,", "tokens": ["Das", "Dun\u00b7kel", "un\u00b7be\u00b7grenzt", ",", "das", "sie", "um\u00b7fan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchziehn sie, das verwandt der ewgen Ruh. O Stadt,", "tokens": ["Durch\u00b7ziehn", "sie", ",", "das", "ver\u00b7wandt", "der", "ew\u00b7gen", "Ruh", ".", "O", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VVFIN", "ART", "ADJA", "NN", "$.", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indes du singst und br\u00fcllst, stets neuen Rausch zu finden", "tokens": ["In\u00b7des", "du", "singst", "und", "br\u00fcllst", ",", "stets", "neu\u00b7en", "Rausch", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In grauenhafter Lust, der du schon \u00fcbersatt,", "tokens": ["In", "grau\u00b7en\u00b7haf\u00b7ter", "Lust", ",", "der", "du", "schon", "\u00fc\u00b7ber\u00b7satt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schlepp mich auch, und mehr als sie zerst\u00f6rt und matt,", "tokens": ["Ich", "schlepp", "mich", "auch", ",", "und", "mehr", "als", "sie", "zer\u00b7st\u00f6rt", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "KOUS", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Frag ich: was suchen sie im Himmel, all die Blinden?", "tokens": ["Frag", "ich", ":", "was", "su\u00b7chen", "sie", "im", "Him\u00b7mel", ",", "all", "die", "Blin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PWS", "VVFIN", "PPER", "APPRART", "NN", "$,", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}