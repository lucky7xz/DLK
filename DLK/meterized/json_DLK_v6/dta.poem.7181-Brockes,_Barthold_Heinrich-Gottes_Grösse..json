{"dta.poem.7181": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Verstandes Aug\u2019 erblindet,", "tokens": ["Des", "Ver\u00b7stan\u00b7des", "Aug'", "er\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil bey GOTTES Majest\u00e4t", "tokens": ["Weil", "bey", "GoT\u00b7TES", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles endliche verschwindet,", "tokens": ["Al\u00b7les", "end\u00b7li\u00b7che", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Zahlen Zahl vergeht;", "tokens": ["Al\u00b7ler", "Zah\u00b7len", "Zahl", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle L\u00e4nge, Tieff\u2019 und Weite,", "tokens": ["Al\u00b7le", "L\u00e4n\u00b7ge", ",", "Tief\u00b7f'", "und", "Wei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alle H\u00f6he, Gr\u00f6ss\u2019 und Breite,", "tokens": ["Al\u00b7le", "H\u00f6\u00b7he", ",", "Gr\u00f6ss'", "und", "Brei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird nicht enge, flach noch klein,", "tokens": ["Wird", "nicht", "en\u00b7ge", ",", "flach", "noch", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJA", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6rt gar auf ein Maa\u00df zu seyn.", "tokens": ["H\u00f6rt", "gar", "auf", "ein", "Maa\u00df", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}