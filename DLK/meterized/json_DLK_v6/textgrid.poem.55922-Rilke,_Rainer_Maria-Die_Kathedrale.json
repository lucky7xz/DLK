{"textgrid.poem.55922": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Kathedrale", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jenen kleinen St\u00e4dten, wo herum", "tokens": ["In", "je\u00b7nen", "klei\u00b7nen", "St\u00e4d\u00b7ten", ",", "wo", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PWAV", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die alten H\u00e4user wie ein Jahrmarkt hocken,", "tokens": ["die", "al\u00b7ten", "H\u00e4u\u00b7ser", "wie", "ein", "Jahr\u00b7markt", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der ", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "die Buden zumacht und, ganz zu und stumm,", "tokens": ["die", "Bu\u00b7den", "zu\u00b7macht", "und", ",", "ganz", "zu", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "$,", "ADV", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "die Schreier still, die Trommeln angehalten,", "tokens": ["die", "Schrei\u00b7er", "still", ",", "die", "Trom\u00b7meln", "an\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "zu ihr hinaufhorcht aufgeregten Ohrs \u2013:", "tokens": ["zu", "ihr", "hin\u00b7auf\u00b7horcht", "auf\u00b7ge\u00b7reg\u00b7ten", "Ohrs", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "dieweil sie ruhig immer in dem alten", "tokens": ["die\u00b7weil", "sie", "ru\u00b7hig", "im\u00b7mer", "in", "dem", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Faltenmantel ihrer Contreforts", "tokens": ["Fal\u00b7ten\u00b7man\u00b7tel", "ih\u00b7rer", "Con\u00b7tre\u00b7forts"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "dasteht und von den H\u00e4usern gar nicht wei\u00df:", "tokens": ["das\u00b7teht", "und", "von", "den", "H\u00e4u\u00b7sern", "gar", "nicht", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "APPR", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "in jenen kleinen St\u00e4dten kannst du sehn,", "tokens": ["in", "je\u00b7nen", "klei\u00b7nen", "St\u00e4d\u00b7ten", "kannst", "du", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie sehr entwachsen ihrem Umgangskreis", "tokens": ["wie", "sehr", "ent\u00b7wach\u00b7sen", "ih\u00b7rem", "Um\u00b7gangs\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Kathedralen waren. Ihr Erstehn", "tokens": ["die", "Ka\u00b7thed\u00b7ra\u00b7len", "wa\u00b7ren", ".", "Ihr", "Er\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ging \u00fcber alles fort, so wie den Blick", "tokens": ["ging", "\u00fc\u00b7ber", "al\u00b7les", "fort", ",", "so", "wie", "den", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIS", "PTKVZ", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "des eignen Lebens viel zu gro\u00dfe N\u00e4he", "tokens": ["des", "eig\u00b7nen", "Le\u00b7bens", "viel", "zu", "gro\u00b7\u00dfe", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "fortw\u00e4hrend \u00fcbersteigt, und als gesch\u00e4he", "tokens": ["fort\u00b7w\u00e4h\u00b7rend", "\u00fc\u00b7bers\u00b7teigt", ",", "und", "als", "ge\u00b7sch\u00e4\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "KON", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "nichts anderes; als w\u00e4re Das Geschick,", "tokens": ["nichts", "an\u00b7de\u00b7res", ";", "als", "w\u00e4\u00b7re", "Das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "$.", "KOKOM", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "was sich in ihnen aufh\u00e4uft ohne Ma\u00dfen,", "tokens": ["was", "sich", "in", "ih\u00b7nen", "auf\u00b7h\u00e4uft", "oh\u00b7ne", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "versteinert und zum Dauernden bestimmt,", "tokens": ["ver\u00b7stei\u00b7nert", "und", "zum", "Dau\u00b7ern\u00b7den", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "nicht Das, was unten in den dunkeln Stra\u00dfen", "tokens": ["nicht", "Das", ",", "was", "un\u00b7ten", "in", "den", "dun\u00b7keln", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDS", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "vom Zufall irgendwelche Namen nimmt", "tokens": ["vom", "Zu\u00b7fall", "ir\u00b7gend\u00b7wel\u00b7che", "Na\u00b7men", "nimmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und darin geht, wie Kinder Gr\u00fcn und Rot", "tokens": ["und", "da\u00b7rin", "geht", ",", "wie", "Kin\u00b7der", "Gr\u00fcn", "und", "Rot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "$,", "PWAV", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "und was der Kr\u00e4mer hat als Sch\u00fcrze tragen.", "tokens": ["und", "was", "der", "Kr\u00e4\u00b7mer", "hat", "als", "Sch\u00fcr\u00b7ze", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da war Geburt in diesen Unterlagen,", "tokens": ["Da", "war", "Ge\u00b7burt", "in", "die\u00b7sen", "Un\u00b7ter\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "und Kraft und Andrang war in diesem Ragen", "tokens": ["und", "Kraft", "und", "An\u00b7drang", "war", "in", "die\u00b7sem", "Ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "und Liebe \u00fcberall wie Wein und Brot,", "tokens": ["und", "Lie\u00b7be", "\u00fc\u00b7be\u00b7rall", "wie", "Wein", "und", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "und die Portale voller Liebesklagen.", "tokens": ["und", "die", "Por\u00b7ta\u00b7le", "vol\u00b7ler", "Lie\u00b7bes\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Das Leben z\u00f6gerte im Stundenschlagen,", "tokens": ["Das", "Le\u00b7ben", "z\u00f6\u00b7ger\u00b7te", "im", "Stun\u00b7den\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "und in den T\u00fcrmen, welche voll Entsagen", "tokens": ["und", "in", "den", "T\u00fcr\u00b7men", ",", "wel\u00b7che", "voll", "Ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "auf einmal nicht mehr stiegen, war der Tod.", "tokens": ["auf", "ein\u00b7mal", "nicht", "mehr", "stie\u00b7gen", ",", "war", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}