{"textgrid.poem.51046": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Winterabend", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneebleich lag eine Leiche, und es trank", "tokens": ["Schnee\u00b7bleich", "lag", "ei\u00b7ne", "Lei\u00b7che", ",", "und", "es", "trank"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Bei ihr der Totenw\u00e4chter unverdrossen,", "tokens": ["Bei", "ihr", "der", "To\u00b7ten\u00b7w\u00e4ch\u00b7ter", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis endlich ihm der Himmel aufgeschlossen", "tokens": ["Bis", "end\u00b7lich", "ihm", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und er berauscht zu ihr aufs Lager sank.", "tokens": ["Und", "er", "be\u00b7rauscht", "zu", "ihr", "aufs", "La\u00b7ger", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von rotem Wein den Becher voll und blank", "tokens": ["Von", "ro\u00b7tem", "Wein", "den", "Be\u00b7cher", "voll", "und", "blank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bot er dem Toten; bald war \u00fcbergossen", "tokens": ["Bot", "er", "dem", "To\u00b7ten", ";", "bald", "war", "\u00fc\u00b7ber\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VAFIN", "VVPP"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Das Grabgesicht und purpurn \u00fcberflossen", "tokens": ["Das", "Grab\u00b7ge\u00b7sicht", "und", "pur\u00b7purn", "\u00fc\u00b7berf\u00b7los\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leichenhemd; so trieb er tollen Schwank.", "tokens": ["Das", "Lei\u00b7chen\u00b7hemd", ";", "so", "trieb", "er", "tol\u00b7len", "Schwank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die trunkne rote Sonne \u00fcbergie\u00dft", "tokens": ["Die", "trunk\u00b7ne", "ro\u00b7te", "Son\u00b7ne", "\u00fc\u00b7berg\u00b7ie\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Sinken dieses schneeverh\u00fcllte Land,", "tokens": ["Im", "Sin\u00b7ken", "die\u00b7ses", "schnee\u00b7ver\u00b7h\u00fcll\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Rosenschein von allen H\u00fcgeln flie\u00dft;", "tokens": ["Da\u00df", "Ro\u00b7sen\u00b7schein", "von", "al\u00b7len", "H\u00fc\u00b7geln", "flie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von Purpur trieft der Erde Grabgewand,", "tokens": ["Von", "Pur\u00b7pur", "trieft", "der", "Er\u00b7de", "Grab\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch die verbla\u00dfte Leichenlippe tut", "tokens": ["Doch", "die", "ver\u00b7bla\u00df\u00b7te", "Lei\u00b7chen\u00b7lip\u00b7pe", "tut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erstarrt sich nimmer auf der roten Flut.", "tokens": ["Er\u00b7starrt", "sich", "nim\u00b7mer", "auf", "der", "ro\u00b7ten", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}