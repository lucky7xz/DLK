{"dta.poem.23755": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Furcht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Hier in diesen dunkeln Str\u00e4uchen", "tokens": ["Hier", "in", "die\u00b7sen", "dun\u00b7keln", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich, ganz allein,", "tokens": ["Will", "ich", ",", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Grillen mir verscheuchen,", "tokens": ["Mei\u00b7ne", "Gril\u00b7len", "mir", "ver\u00b7scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich des Fr\u00fchlings freun.", "tokens": ["Mich", "des", "Fr\u00fch\u00b7lings", "freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Philomele soll mich lehren", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", "soll", "mich", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie singen kann;", "tokens": ["Was", "sie", "sin\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich stimm auch ihr zu Ehren", "tokens": ["Und", "ich", "stimm", "auch", "ihr", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "ADV", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl ein Liedgen an! \u2014", "tokens": ["Wohl", "ein", "Lied\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch was h\u00f6r ich sich bewegen?", "tokens": ["Doch", "was", "h\u00f6r", "ich", "sich", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ah! was rauschet dort? \u2014", "tokens": ["Ah", "!", "was", "rau\u00b7schet", "dort", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schrecklich rauscht es mir entgegen, \u2014", "tokens": ["Schreck\u00b7lich", "rauscht", "es", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r ich die\u00dfmal fort! \u2014", "tokens": ["W\u00e4r", "ich", "die\u00df\u00b7mal", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O! ich zittre! ich vergehe,", "tokens": ["O", "!", "ich", "zitt\u00b7re", "!", "ich", "ver\u00b7ge\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh mir Armen! Weh!", "tokens": ["Weh", "mir", "Ar\u00b7men", "!", "Weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "NN", "$.", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jetzund kommt es \u2014 ja, ich sehe \u2014", "tokens": ["Je\u00b7tzund", "kommt", "es", "ja", ",", "ich", "se\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PTKANT", "$,", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! \u2014 ein kleines Reh.", "tokens": ["Ach", "!", "ein", "klei\u00b7nes", "Reh", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}