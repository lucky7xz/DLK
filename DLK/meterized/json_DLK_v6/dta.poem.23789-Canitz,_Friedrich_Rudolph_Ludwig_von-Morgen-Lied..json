{"dta.poem.23789": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh es sind die finstern Stunden/", "tokens": ["Sieh", "es", "sind", "die", "fins\u00b7tern", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abermahls verschwunden/", "tokens": ["A\u00b7ber\u00b7mahls", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwinge dich mit deinen Sinnen/", "tokens": ["Schwin\u00b7ge", "dich", "mit", "dei\u00b7nen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh sie anders was beginnen", "tokens": ["Eh", "sie", "an\u00b7ders", "was", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seele zu den Himmels-Zinnen.", "tokens": ["See\u00b7le", "zu", "den", "Him\u00b7mels\u00b7Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "GoTT/ von dem das Licht entsprungen/", "tokens": ["GoTT", "/", "von", "dem", "das", "Licht", "ent\u00b7sprun\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPR", "PRELS", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir sey ", "tokens": ["Dir", "sey"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df du mich f\u00fcr Noth und Schrecken", "tokens": ["Da\u00df", "du", "mich", "f\u00fcr", "Noth", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen durch dein Heer bedecken/", "tokens": ["Wol\u00b7len", "durch", "dein", "Heer", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und itzt aus dem Schlaff erwecken.", "tokens": ["Und", "itzt", "aus", "dem", "Schlaff", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Meinen Leib und mein Gem\u00fcthe/", "tokens": ["Mei\u00b7nen", "Leib", "und", "mein", "Ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wil ich deiner G\u00fcte/", "tokens": ["Wil", "ich", "dei\u00b7ner", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wohlgemeynt zum Opffer schencken/", "tokens": ["Wohl\u00b7ge\u00b7meynt", "zum", "Opf\u00b7fer", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du magst mein Thun und Dencken/", "tokens": ["Da\u00df", "du", "magst", "mein", "Thun", "und", "Den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie es dir gef\u00e4llig/ lencken.", "tokens": ["Wie", "es", "dir", "ge\u00b7f\u00e4l\u00b7lig", "/", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "$(", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ich sonst als deine Gabe/", "tokens": ["Was", "ich", "sonst", "als", "dei\u00b7ne", "Ga\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "KOKOM", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch auf Erden habe/", "tokens": ["Noch", "auf", "Er\u00b7den", "ha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An Verm\u00f6gen/ Gl\u00fcck und Ehren/", "tokens": ["An", "Ver\u00b7m\u00f6\u00b7gen", "/", "Gl\u00fcck", "und", "Eh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag sich mindern oder mehren/", "tokens": ["Mag", "sich", "min\u00b7dern", "o\u00b7der", "meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJA", "KON", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du wirst mirs zum ", "tokens": ["Du", "wirst", "mirs", "zum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "APPRART"], "meter": "-+-+", "measure": "iambic.di"}}}}}