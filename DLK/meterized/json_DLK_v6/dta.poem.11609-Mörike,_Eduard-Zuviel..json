{"dta.poem.11609": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zuviel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel gl\u00e4nzt vom reinsten Fr\u00fchlingslichte,", "tokens": ["Der", "Him\u00b7mel", "gl\u00e4nzt", "vom", "reins\u00b7ten", "Fr\u00fch\u00b7lings\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm schwillt der H\u00fcgel sehnsuchtsvoll entgegen,", "tokens": ["Ihm", "schwillt", "der", "H\u00fc\u00b7gel", "sehn\u00b7suchts\u00b7voll", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die starre Welt zerflie\u00dft in Liebessegen,", "tokens": ["Die", "star\u00b7re", "Welt", "zer\u00b7flie\u00dft", "in", "Lie\u00b7bes\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schmiegt sich rund zum z\u00e4rtlichsten Gedichte.", "tokens": ["Und", "schmiegt", "sich", "rund", "zum", "z\u00e4rt\u00b7lichs\u00b7ten", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Am Dorfeshang, dort bei der luft'gen Fichte,", "tokens": ["Am", "Dor\u00b7fes\u00b7hang", ",", "dort", "bei", "der", "luft'\u00b7gen", "Fich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist meiner Liebsten kleines Haus gelegen \u2014", "tokens": ["Ist", "mei\u00b7ner", "Liebs\u00b7ten", "klei\u00b7nes", "Haus", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Herz, was hilft dein Wiegen und dein W\u00e4gen,", "tokens": ["O", "Herz", ",", "was", "hilft", "dein", "Wie\u00b7gen", "und", "dein", "W\u00e4\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df all' der Wonne-Streit in dir sich schlichte!", "tokens": ["Da\u00df", "all'", "der", "Won\u00b7ne\u00b7Streit", "in", "dir", "sich", "schlich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, Liebe, hilf den s\u00fc\u00dfen Zauber l\u00f6sen,", "tokens": ["Du", ",", "Lie\u00b7be", ",", "hilf", "den", "s\u00fc\u00b7\u00dfen", "Zau\u00b7ber", "l\u00f6\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit Natur in meinem Innern w\u00fchlet!", "tokens": ["Wo\u00b7mit", "Na\u00b7tur", "in", "mei\u00b7nem", "In\u00b7nern", "w\u00fch\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und du, o Fr\u00fchling, hilf die Liebe beugen!", "tokens": ["Und", "du", ",", "o", "Fr\u00fch\u00b7ling", ",", "hilf", "die", "Lie\u00b7be", "beu\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lisch aus, o Tag! La\u00df mich in Nacht genesen!", "tokens": ["Lisch", "aus", ",", "o", "Tag", "!", "La\u00df", "mich", "in", "Nacht", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM", "NN", "$.", "VVIMP", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Inde\u00df ihr sanften Sterne g\u00f6ttlich k\u00fchlet,", "tokens": ["In\u00b7de\u00df", "ihr", "sanf\u00b7ten", "Ster\u00b7ne", "g\u00f6tt\u00b7lich", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will ich zum Abgrund der Betrachtung steigen.", "tokens": ["Will", "ich", "zum", "Ab\u00b7grund", "der", "Be\u00b7trach\u00b7tung", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}