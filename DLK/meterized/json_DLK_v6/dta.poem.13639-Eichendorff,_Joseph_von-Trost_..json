{"dta.poem.13639": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wo immer m\u00fcde Fechter", "tokens": ["Und", "wo", "im\u00b7mer", "m\u00fc\u00b7de", "Fech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinken im muthigen Strau\u00df,", "tokens": ["Sin\u00b7ken", "im", "mut\u00b7hi\u00b7gen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Es kommen frische Geschlechter", "tokens": ["Es", "kom\u00b7men", "fri\u00b7sche", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und fechten es ehrlich aus.", "tokens": ["Und", "fech\u00b7ten", "es", "ehr\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}