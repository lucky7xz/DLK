{"dta.poem.8646": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Nebel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Du tr\u00fcber Nebel, h\u00fcllest mir", "tokens": ["Du", "tr\u00fc\u00b7ber", "Ne\u00b7bel", ",", "h\u00fcl\u00b7lest", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Thal mit seinem Flu\u00df,", "tokens": ["Das", "Thal", "mit", "sei\u00b7nem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Berg mit seinem Waldrevier,", "tokens": ["Den", "Berg", "mit", "sei\u00b7nem", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeden Sonnengru\u00df.", "tokens": ["Und", "je\u00b7den", "Son\u00b7nen\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nimm fort in deine graue Nacht", "tokens": ["Nimm", "fort", "in", "dei\u00b7ne", "grau\u00b7e", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde weit und breit!", "tokens": ["Die", "Er\u00b7de", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm fort, was mich so traurig macht,", "tokens": ["Nimm", "fort", ",", "was", "mich", "so", "trau\u00b7rig", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch die Vergangenheit!", "tokens": ["Auch", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}