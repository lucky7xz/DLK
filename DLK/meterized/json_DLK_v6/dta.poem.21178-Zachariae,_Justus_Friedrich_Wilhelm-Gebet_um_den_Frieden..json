{"dta.poem.21178": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gebet um den Frieden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! GOtt und Vater deiner Kinder!", "tokens": ["Herr", "!", "Gott", "und", "Va\u00b7ter", "dei\u00b7ner", "Kin\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergi\u00dft du, Sch\u00f6pfer, deiner Welt?", "tokens": ["Ver\u00b7gi\u00dft", "du", ",", "Sch\u00f6p\u00b7fer", ",", "dei\u00b7ner", "Welt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist niemand, welcher f\u00fcr uns S\u00fcnder", "tokens": ["Ist", "nie\u00b7mand", ",", "wel\u00b7cher", "f\u00fcr", "uns", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "APPR", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, Richter, in das Rachschwerdt f\u00e4llt?", "tokens": ["Dir", ",", "Rich\u00b7ter", ",", "in", "das", "Rach\u00b7schwerdt", "f\u00e4llt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch sendest du zum Blutvergiessen", "tokens": ["Noch", "sen\u00b7dest", "du", "zum", "Blut\u00b7ver\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Todesengel vor dir her;", "tokens": ["Den", "To\u00b7de\u00b7sen\u00b7gel", "vor", "dir", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter des Erw\u00fcrgers F\u00fcssen", "tokens": ["Und", "un\u00b7ter", "des", "Er\u00b7w\u00fcr\u00b7gers", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt alles w\u00fcst, entstellt, und leer.", "tokens": ["Liegt", "al\u00b7les", "w\u00fcst", ",", "ent\u00b7stellt", ",", "und", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "$,", "VVFIN", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau doch mit Einem Blick der Gnaden", "tokens": ["Schau", "doch", "mit", "Ei\u00b7nem", "Blick", "der", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf die zerst\u00f6rte Welt herab!", "tokens": ["Auf", "die", "zer\u00b7st\u00f6r\u00b7te", "Welt", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieh, wie ganze Myriaden,", "tokens": ["Und", "sieh", ",", "wie", "gan\u00b7ze", "My\u00b7ria\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Schwerdt fri\u00dft, und das weite Grab.", "tokens": ["Das", "Schwerdt", "fri\u00dft", ",", "und", "das", "wei\u00b7te", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, wie die Fluren \u00f6de liegen;", "tokens": ["Sieh", ",", "wie", "die", "Flu\u00b7ren", "\u00f6\u00b7de", "lie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ohne Trost der Landmann steht,", "tokens": ["Wie", "oh\u00b7ne", "Trost", "der", "Land\u00b7mann", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der unter seiner Herrscher Siegen", "tokens": ["Der", "un\u00b7ter", "sei\u00b7ner", "Herr\u00b7scher", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Mangel schmachtet und vergeht.", "tokens": ["Jm", "Man\u00b7gel", "schmach\u00b7tet", "und", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Leer, und mit Thr\u00e4nenvollen Blicken,", "tokens": ["Leer", ",", "und", "mit", "Thr\u00e4\u00b7nen\u00b7vol\u00b7len", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verl\u00e4\u00dft er sein gepl\u00fcndert Haus;", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "sein", "ge\u00b7pl\u00fcn\u00b7dert", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lodert hinter seinem R\u00fccken,", "tokens": ["Es", "lo\u00b7dert", "hin\u00b7ter", "sei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sinkt, und zerf\u00e4llt in Schutt und Graus.", "tokens": ["Sinkt", ",", "und", "zer\u00b7f\u00e4llt", "in", "Schutt", "und", "Graus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Und seine schwachen Kinder weinen", "tokens": ["Und", "sei\u00b7ne", "schwa\u00b7chen", "Kin\u00b7der", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An seiner Hand umsonst um Brod;", "tokens": ["An", "sei\u00b7ner", "Hand", "um\u00b7sonst", "um", "Brod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder Seufzer von den Seinen", "tokens": ["Und", "je\u00b7der", "Seuf\u00b7zer", "von", "den", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist f\u00fcr sein Herz langsamer Tod.", "tokens": ["Ist", "f\u00fcr", "sein", "Herz", "lang\u00b7sa\u00b7mer", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Von seinem Reichthum, aller Haabe,", "tokens": ["Von", "sei\u00b7nem", "Reicht\u00b7hum", ",", "al\u00b7ler", "Haa\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt ihm zur H\u00fclle kein Gewand.", "tokens": ["Bleibt", "ihm", "zur", "H\u00fcl\u00b7le", "kein", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schleppt er sich am Pilgerstabe", "tokens": ["So", "schleppt", "er", "sich", "am", "Pil\u00b7ger\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fern in ein unbekanntes Land.", "tokens": ["Fern", "in", "ein", "un\u00b7be\u00b7kann\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Rund um umgeben von Gefahren,", "tokens": ["Rund", "um", "um\u00b7ge\u00b7ben", "von", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Entrinnt er so aus Mord und Brand;", "tokens": ["Ent\u00b7rinnt", "er", "so", "aus", "Mord", "und", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ferner V\u00f6lker Kriegesschaaren", "tokens": ["Und", "fer\u00b7ner", "V\u00f6l\u00b7ker", "Krie\u00b7ges\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedecken seiner Fl\u00fcsse Strand.", "tokens": ["Be\u00b7de\u00b7cken", "sei\u00b7ner", "Fl\u00fcs\u00b7se", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Elbe w\u00e4lzt zum Oceane", "tokens": ["Die", "El\u00b7be", "w\u00e4lzt", "zum", "O\u00b7cea\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Fluth, durch Leichen aufgeschwellt.", "tokens": ["Die", "Fluth", ",", "durch", "Lei\u00b7chen", "auf\u00b7ge\u00b7schwellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an der Oder winkt die Fahne", "tokens": ["Und", "an", "der", "O\u00b7der", "winkt", "die", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu wilden Schlachten in das Feld.", "tokens": ["Zu", "wil\u00b7den", "Schlach\u00b7ten", "in", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Spree sieht ihrer Kinder Zagen,", "tokens": ["Die", "Spree", "sieht", "ih\u00b7rer", "Kin\u00b7der", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht ihrer Freuden sich beraubt;", "tokens": ["Sieht", "ih\u00b7rer", "Freu\u00b7den", "sich", "be\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bey der Unterdr\u00fcckten Klagen", "tokens": ["Und", "bey", "der", "Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbirgt der Weserstrom sein Haupt.", "tokens": ["Ver\u00b7birgt", "der", "We\u00b7ser\u00b7strom", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wohin man blickt, sieht man Verheeren;", "tokens": ["Wo\u00b7hin", "man", "blickt", ",", "sieht", "man", "Ver\u00b7hee\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die St\u00e4dte w\u00fcst, das Land in Blut;", "tokens": ["Die", "St\u00e4d\u00b7te", "w\u00fcst", ",", "das", "Land", "in", "Blut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber beyde Hemisph\u00e4ren", "tokens": ["Und", "\u00fc\u00b7ber", "bey\u00b7de", "He\u00b7misp\u00b7h\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbreitet sich des Krieges Wuth.", "tokens": ["Ver\u00b7brei\u00b7tet", "sich", "des", "Krie\u00b7ges", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O sieh darein! Erbarmer, Retter!", "tokens": ["O", "sieh", "da\u00b7rein", "!", "Er\u00b7bar\u00b7mer", ",", "Ret\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst dich uns nicht ganz entziehn;", "tokens": ["Du", "wirst", "dich", "uns", "nicht", "ganz", "ent\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst nicht, verh\u00fcllt in Nacht und Wetter,", "tokens": ["Wirst", "nicht", ",", "ver\u00b7h\u00fcllt", "in", "Nacht", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets wider uns zur Rache ziehn.", "tokens": ["Stets", "wi\u00b7der", "uns", "zur", "Ra\u00b7che", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ruf ab das Schwerdt vom Feld der Todten,", "tokens": ["Ruf", "ab", "das", "Schwerdt", "vom", "Feld", "der", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das uns zum Fluch gesch\u00e4rfet ward!", "tokens": ["Das", "uns", "zum", "Fluch", "ge\u00b7sch\u00e4r\u00b7fet", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sende deinen Friedensboten", "tokens": ["Und", "sen\u00b7de", "dei\u00b7nen", "Frie\u00b7dens\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Erdkreis, welcher auf ihn harrt!", "tokens": ["Dem", "Erd\u00b7kreis", ",", "wel\u00b7cher", "auf", "ihn", "harrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Vernimm das Flehen frommer Bether!", "tokens": ["Ver\u00b7nimm", "das", "Fle\u00b7hen", "from\u00b7mer", "Be\u00b7ther", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Du lenkst der F\u00fcrsten Herz allein;", "tokens": ["Du", "lenkst", "der", "F\u00fcrs\u00b7ten", "Herz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lenk es zum Frieden! La\u00df sie V\u00e4ter,", "tokens": ["Lenk", "es", "zum", "Frie\u00b7den", "!", "La\u00df", "sie", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$.", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Menschen wieder Menschen seyn!", "tokens": ["Und", "Men\u00b7schen", "wie\u00b7der", "Men\u00b7schen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}