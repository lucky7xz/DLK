{"textgrid.poem.34880": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater war ein trockner Taps,", "tokens": ["Mein", "Va\u00b7ter", "war", "ein", "trock\u00b7ner", "Taps", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein n\u00fcchterner Duckm\u00e4user,", "tokens": ["Ein", "n\u00fcch\u00b7ter\u00b7ner", "Duck\u00b7m\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich aber trinke meinen Schnaps", "tokens": ["Ich", "a\u00b7ber", "trin\u00b7ke", "mei\u00b7nen", "Schnaps"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin ein gro\u00dfer Kaiser.", "tokens": ["Und", "bin", "ein", "gro\u00b7\u00dfer", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist ein Zaubertrank! Ich hab's", "tokens": ["Das", "ist", "ein", "Zau\u00b7ber\u00b7trank", "!", "Ich", "hab's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "PPER", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entdeckt in meinem Gem\u00fcte:", "tokens": ["Ent\u00b7deckt", "in", "mei\u00b7nem", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sobald ich getrunken meinen Schnaps,", "tokens": ["So\u00b7bald", "ich", "ge\u00b7trun\u00b7ken", "mei\u00b7nen", "Schnaps", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Steht China ganz in Bl\u00fcte.", "tokens": ["Steht", "Chi\u00b7na", "ganz", "in", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Reich der Mitte verwandelt sich dann", "tokens": ["Das", "Reich", "der", "Mit\u00b7te", "ver\u00b7wan\u00b7delt", "sich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In einen Blumenanger,", "tokens": ["In", "ei\u00b7nen", "Blu\u00b7men\u00b7an\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich selber werde fast ein Mann,", "tokens": ["Ich", "sel\u00b7ber", "wer\u00b7de", "fast", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Frau wird schwanger.", "tokens": ["Und", "mei\u00b7ne", "Frau", "wird", "schwan\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "All\u00fcberall ist \u00dcberflu\u00df,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "ist", "\u00dc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und es gesunden die Kranken;", "tokens": ["Und", "es", "ge\u00b7sun\u00b7den", "die", "Kran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Hofweltweiser Confusius", "tokens": ["Mein", "Hof\u00b7welt\u00b7wei\u00b7ser", "Con\u00b7fu\u00b7si\u00b7us"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bek\u00f6mmt die klarsten Gedanken.", "tokens": ["Be\u00b7k\u00f6mmt", "die", "klars\u00b7ten", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Pumpernickel des Soldats", "tokens": ["Der", "Pum\u00b7per\u00b7ni\u00b7ckel", "des", "Sol\u00b7dats"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Mandelkuchen \u2013 O Freude!", "tokens": ["Wird", "Man\u00b7del\u00b7ku\u00b7chen", "\u2013", "O", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "NE", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und alle Lumpen meines Staats", "tokens": ["Und", "al\u00b7le", "Lum\u00b7pen", "mei\u00b7nes", "Staats"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spazieren in Samt und Seide.", "tokens": ["Spa\u00b7zie\u00b7ren", "in", "Samt", "und", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Mandarinenritterschaft,", "tokens": ["Die", "Man\u00b7da\u00b7ri\u00b7nen\u00b7rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die invaliden K\u00f6pfe,", "tokens": ["Die", "in\u00b7va\u00b7li\u00b7den", "K\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Gewinnen wieder Jugendkraft", "tokens": ["Ge\u00b7win\u00b7nen", "wie\u00b7der", "Ju\u00b7gend\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00fctteln ihre Z\u00f6pfe.", "tokens": ["Und", "sch\u00fct\u00b7teln", "ih\u00b7re", "Z\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die gro\u00dfe Pagode, Symbol und Hort", "tokens": ["Die", "gro\u00b7\u00dfe", "Pa\u00b7go\u00b7de", ",", "Sym\u00b7bol", "und", "Hort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Des Glaubens, ist fertig geworden;", "tokens": ["Des", "Glau\u00b7bens", ",", "ist", "fer\u00b7tig", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADJD", "VAPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die letzten Juden taufen sich dort", "tokens": ["Die", "letz\u00b7ten", "Ju\u00b7den", "tau\u00b7fen", "sich", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und kriegen den Drachenorden.", "tokens": ["Und", "krie\u00b7gen", "den", "Dra\u00b7chen\u00b7or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Es schwindet der Geist der Revolution,", "tokens": ["Es", "schwin\u00b7det", "der", "Geist", "der", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und es rufen die edelsten Mandschu:", "tokens": ["Und", "es", "ru\u00b7fen", "die", "e\u00b7dels\u00b7ten", "Mand\u00b7schu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbwir wollen keine Konstitution,", "tokens": ["\u00bb", "wir", "wol\u00b7len", "kei\u00b7ne", "Kons\u00b7ti\u00b7tu\u00b7ti\u00b7on", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir wollen den Stock, den Kantschu!\u00ab", "tokens": ["Wir", "wol\u00b7len", "den", "Stock", ",", "den", "Kant\u00b7schu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Wohl haben die Sch\u00fcler \u00c4skulaps", "tokens": ["Wohl", "ha\u00b7ben", "die", "Sch\u00fc\u00b7ler", "\u00c4s\u00b7ku\u00b7laps"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Trinken mir widerraten,", "tokens": ["Das", "Trin\u00b7ken", "mir", "wi\u00b7der\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich aber trinke meinen Schnaps", "tokens": ["Ich", "a\u00b7ber", "trin\u00b7ke", "mei\u00b7nen", "Schnaps"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Besten meiner Staaten.", "tokens": ["Zum", "Bes\u00b7ten", "mei\u00b7ner", "Staa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und noch einen Schnaps, und noch einen Schnaps!", "tokens": ["Und", "noch", "ei\u00b7nen", "Schnaps", ",", "und", "noch", "ei\u00b7nen", "Schnaps", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das schmeckt wie lauter Manna!", "tokens": ["Das", "schmeckt", "wie", "lau\u00b7ter", "Man\u00b7na", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mein Volk ist gl\u00fccklich, hat's auch den Raps,", "tokens": ["Mein", "Volk", "ist", "gl\u00fcck\u00b7lich", ",", "hat's", "auch", "den", "Raps", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und jubelt: \u00bbHosianna!\u00ab", "tokens": ["Und", "ju\u00b7belt", ":", "\u00bb", "Ho\u00b7si\u00b7an\u00b7na", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NE", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}