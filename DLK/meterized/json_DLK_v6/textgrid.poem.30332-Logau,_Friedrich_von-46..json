{"textgrid.poem.30332": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeiten sind als wie ein Rad; sie reissen mit sich um,", "tokens": ["Die", "Zei\u00b7ten", "sind", "als", "wie", "ein", "Rad", ";", "sie", "reis\u00b7sen", "mit", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOUS", "KOKOM", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wer sich an sie henckt, machen ihn verdreht, verkehrt, krum, thum.", "tokens": ["Wer", "sich", "an", "sie", "henckt", ",", "ma\u00b7chen", "ihn", "ver\u00b7dreht", ",", "ver\u00b7kehrt", ",", "krum", ",", "thum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,", "VVPP", "$,", "FM.la", "$,", "NN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}