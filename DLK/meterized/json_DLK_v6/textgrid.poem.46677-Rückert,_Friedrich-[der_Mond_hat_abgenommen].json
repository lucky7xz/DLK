{"textgrid.poem.46677": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[der Mond hat abgenommen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond hat abgenommen", "tokens": ["Der", "Mond", "hat", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ist dann verschwunden,", "tokens": ["Und", "ist", "dann", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dann neu hervor gekommen,", "tokens": ["Dann", "neu", "her\u00b7vor", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um voll sich zu runden.", "tokens": ["Um", "voll", "sich", "zu", "run\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Schmerz er hat inzwischen", "tokens": ["Mein", "Schmerz", "er", "hat", "in\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwar ab nicht genommen,", "tokens": ["Zwar", "ab", "nicht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch ist er nun vom Frischen", "tokens": ["Doch", "ist", "er", "nun", "vom", "Fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Herzen erglommen.", "tokens": ["Im", "Her\u00b7zen", "er\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Warum? der Mond will mahnen", "tokens": ["Wa\u00b7rum", "?", "der", "Mond", "will", "mah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wachsen mich immer", "tokens": ["Im", "Wach\u00b7sen", "mich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An dich auf halben Bahnen", "tokens": ["An", "dich", "auf", "hal\u00b7ben", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Erloschenen Schimmer.", "tokens": ["Er\u00b7lo\u00b7sche\u00b7nen", "Schim\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht voll ward deine Scheibe", "tokens": ["Nicht", "voll", "ward", "dei\u00b7ne", "Schei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mu\u00dfte sich schw\u00e4rzen,", "tokens": ["Und", "mu\u00df\u00b7te", "sich", "schw\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Damit mir ewig bleibe", "tokens": ["Da\u00b7mit", "mir", "e\u00b7wig", "blei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Vollmond der Schmerzen.", "tokens": ["Ein", "Voll\u00b7mond", "der", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}