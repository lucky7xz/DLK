{"textgrid.poem.25582": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00e4tt' er ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' er ", "tokens": ["H\u00e4tt'", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Die Hudibras allein verschlang,", "tokens": ["Die", "Hu\u00b7di\u00b7bras", "al\u00b7lein", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zehn tausend Schmeicheleyn verschwendet,", "tokens": ["Zehn", "tau\u00b7send", "Schmei\u00b7che\u00b7leyn", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Statt da\u00df er so viel Verse sang:", "tokens": ["Statt", "da\u00df", "er", "so", "viel", "Ver\u00b7se", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "In Golde h\u00e4tt' er k\u00f6nnen w\u00fchlen,", "tokens": ["In", "Gol\u00b7de", "h\u00e4tt'", "er", "k\u00f6n\u00b7nen", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Titeln prahlen und mit Rang,", "tokens": ["Mit", "Ti\u00b7teln", "prah\u00b7len", "und", "mit", "Rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Warum er aber dennoch sang?", "tokens": ["Wa\u00b7rum", "er", "a\u00b7ber", "den\u00b7noch", "sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das wei\u00df nur der, der gleichen Drang", "tokens": ["Das", "wei\u00df", "nur", "der", ",", "der", "glei\u00b7chen", "Drang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zum Singen, wird im Busen f\u00fchlen.", "tokens": ["Zum", "Sin\u00b7gen", ",", "wird", "im", "Bu\u00b7sen", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}