{"textgrid.poem.38347": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "G g Gimpel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rother, dir gar wohl bekannt, ist sch\u00f6n, doch singt nicht viel,", "tokens": ["Ein", "ro\u00b7ther", ",", "dir", "gar", "wohl", "be\u00b7kannt", ",", "ist", "sch\u00f6n", ",", "doch", "singt", "nicht", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "ADV", "ADV", "ADJD", "$,", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Er k\u00f6mmt aus deinem Vaterland, hei\u00dft Gimpel in der Still,", "tokens": ["Er", "k\u00f6mmt", "aus", "dei\u00b7nem", "Va\u00b7ter\u00b7land", ",", "hei\u00dft", "Gim\u00b7pel", "in", "der", "Still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "All thun sich seiner sch\u00e4men, weil er ein Gimpel ist,", "tokens": ["All", "thun", "sich", "sei\u00b7ner", "sch\u00e4\u00b7men", ",", "weil", "er", "ein", "Gim\u00b7pel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "ADJA", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Thu du ihn zu dir nehmen, weil du sein Landsmann bist.", "tokens": ["Thu", "du", "ihn", "zu", "dir", "neh\u00b7men", ",", "weil", "du", "sein", "Lands\u00b7mann", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}}}}