{"dta.poem.7512": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Finsternis zum Licht steigt eine Stufenleiter,", "tokens": ["Aus", "Fins\u00b7ter\u00b7nis", "zum", "Licht", "steigt", "ei\u00b7ne", "Stu\u00b7fen\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dunkel ist am Fu\u00df und an der Spitze heiter.", "tokens": ["Die", "dun\u00b7kel", "ist", "am", "Fu\u00df", "und", "an", "der", "Spit\u00b7ze", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Schatten siehst du nicht, wie hoch die Leiter du", "tokens": ["Im", "Schat\u00b7ten", "siehst", "du", "nicht", ",", "wie", "hoch", "die", "Lei\u00b7ter", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufklommest, doch du klimmst zum Licht auf, klimm nur zu!", "tokens": ["Auf\u00b7klom\u00b7mest", ",", "doch", "du", "klimmst", "zum", "Licht", "auf", ",", "klimm", "nur", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "VVIMP", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du im Licht erkennst, wie aus dem Licht erstanden", "tokens": ["Wenn", "du", "im", "Licht", "er\u00b7kennst", ",", "wie", "aus", "dem", "Licht", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nothwend'ge Finsternis, dann ist die Welt verstanden.", "tokens": ["Nothwen\u00b7d'\u00b7ge", "Fins\u00b7ter\u00b7nis", ",", "dann", "ist", "die", "Welt", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "War Finsternis einst Licht, so wird sie Licht einst seyn,", "tokens": ["War", "Fins\u00b7ter\u00b7nis", "einst", "Licht", ",", "so", "wird", "sie", "Licht", "einst", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann das Entsprungne geht in seinen Ursprung ein.", "tokens": ["Wann", "das", "Ent\u00b7sprung\u00b7ne", "geht", "in", "sei\u00b7nen", "Ur\u00b7sprung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jedweder Sieg des Lichts im schwachen Geist vollbracht,", "tokens": ["Jed\u00b7we\u00b7der", "Sieg", "des", "Lichts", "im", "schwa\u00b7chen", "Geist", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weissagt den ew'gen Sieg der lichten Geistermacht.", "tokens": ["Weis\u00b7sagt", "den", "ew'\u00b7gen", "Sieg", "der", "lich\u00b7ten", "Geis\u00b7ter\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihn profezeit die Sonn' an jedem Tage tagend,", "tokens": ["Ihn", "pro\u00b7fe\u00b7zeit", "die", "Sonn'", "an", "je\u00b7dem", "Ta\u00b7ge", "ta\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit einem Stral von Licht ein Heer von Schatten schlagend.", "tokens": ["Mit", "ei\u00b7nem", "Stral", "von", "Licht", "ein", "Heer", "von", "Schat\u00b7ten", "schla\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Am Abend wird sie roth vor Scham da\u00df sie erlag,", "tokens": ["Am", "A\u00b7bend", "wird", "sie", "roth", "vor", "Scham", "da\u00df", "sie", "er\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4umt die Nacht hindurch vom gro\u00dfen ew'gen Tag.", "tokens": ["Und", "tr\u00e4umt", "die", "Nacht", "hin\u00b7durch", "vom", "gro\u00b7\u00dfen", "ew'\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}