{"dta.poem.8014": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00e4uleins Wache .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich geh\u2019 all Nacht die Runde", "tokens": ["Ich", "geh'", "all", "Nacht", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Vaters Hof und Hall\u2019.", "tokens": ["Um", "Va\u00b7ters", "Hof", "und", "Hall'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schlafen zu dieser Stunde", "tokens": ["Es", "schla\u00b7fen", "zu", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die tr\u00e4gen W\u00e4chter all.", "tokens": ["Die", "tr\u00e4\u00b7gen", "W\u00e4ch\u00b7ter", "all", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich Fr\u00e4ulein zart mu\u00df streifen,", "tokens": ["Ich", "Fr\u00e4u\u00b7lein", "zart", "mu\u00df", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ohn\u2019 Wehr und Waffen schweifen,", "tokens": ["Ohn'", "Wehr", "und", "Waf\u00b7fen", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Feind der Nacht zu greifen.", "tokens": ["Den", "Feind", "der", "Nacht", "zu", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O weh des schlimmen Gesellen!", "tokens": ["O", "weh", "des", "schlim\u00b7men", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach Argem steht sein Sinn.", "tokens": ["Nach", "Ar\u00b7gem", "steht", "sein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00fcrd\u2019 ich nicht k\u00fchn mich stellen,", "tokens": ["W\u00fcrd'", "ich", "nicht", "k\u00fchn", "mich", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "PPER", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wohl stieg\u2019 er \u00fcber die Zinn\u2019.", "tokens": ["Wohl", "stieg'", "er", "\u00fc\u00b7ber", "die", "Zinn'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wann ich denselben finde,", "tokens": ["Wann", "ich", "den\u00b7sel\u00b7ben", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie er lauert bei der Linde,", "tokens": ["Wie", "er", "lau\u00b7ert", "bei", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich widersag\u2019 ihm geschwinde.", "tokens": ["Ich", "wi\u00b7der\u00b7sag'", "ihm", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da mu\u00df ich mit ihm ringen", "tokens": ["Da", "mu\u00df", "ich", "mit", "ihm", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allein die Nacht entlang;", "tokens": ["Al\u00b7lein", "die", "Nacht", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPO", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er will mich stets umschlingen,", "tokens": ["Er", "will", "mich", "stets", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie eine wilde Schlang\u2019;", "tokens": ["Wie", "ei\u00b7ne", "wil\u00b7de", "Schlang'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er kommt vom H\u00f6llengrunde,", "tokens": ["Er", "kommt", "vom", "H\u00f6l\u00b7len\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie aus eins Drachen Schlunde,", "tokens": ["Wie", "aus", "eins", "Dra\u00b7chen", "Schlun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "CARD", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gehn Flammen aus seinem Munde.", "tokens": ["Gehn", "Flam\u00b7men", "aus", "sei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und hab\u2019 ich ihn \u00fcberwunden,", "tokens": ["Und", "hab'", "ich", "ihn", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Halt\u2019 ihn im Arme dicht:", "tokens": ["Halt'", "ihn", "im", "Ar\u00b7me", "dicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch eh die Sterne geschwunden,", "tokens": ["Doch", "eh", "die", "Ster\u00b7ne", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entschl\u00fcpft mir stets der Wicht.", "tokens": ["Ent\u00b7schl\u00fcpft", "mir", "stets", "der", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann ihn Niemand zeigen,", "tokens": ["Ich", "kann", "ihn", "Nie\u00b7mand", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df meinen Sieg verschweigen", "tokens": ["Mu\u00df", "mei\u00b7nen", "Sieg", "ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mich in Trauer neigen.", "tokens": ["Und", "mich", "in", "Trau\u00b7er", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}