{"textgrid.poem.47341": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Beschwichtigter Zweifel", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber meinen eignen Kopf", "tokens": ["\u00dc\u00b7ber", "mei\u00b7nen", "eig\u00b7nen", "Kopf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich nicht im reinen,", "tokens": ["Bin", "ich", "nicht", "im", "rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hab' ich, wie ein andrer Tropf,", "tokens": ["Hab'", "ich", ",", "wie", "ein", "an\u00b7drer", "Tropf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen oder keinen?", "tokens": ["Ei\u00b7nen", "o\u00b7der", "kei\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "KON", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In der Schenke, wann der Wein", "tokens": ["In", "der", "Schen\u00b7ke", ",", "wann", "der", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu Kopfe steiget,", "tokens": ["Mir", "zu", "Kop\u00b7fe", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchl' ich erst der Kopf ist mein,", "tokens": ["F\u00fchl'", "ich", "erst", "der", "Kopf", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Zweifel schweiget.", "tokens": ["Und", "der", "Zwei\u00b7fel", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}