{"dta.poem.9596": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie suchte ein corallinen hertze/ welches  \n sie verlohren.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aurora/ suchstu was? und hastu was verlohren?", "tokens": ["Au\u00b7ro\u00b7ra", "/", "suchs\u00b7tu", "was", "?", "und", "has\u00b7tu", "was", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PIS", "$.", "KON", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist irgend ohngefehr was liebes dir entwandt?", "tokens": ["Ist", "ir\u00b7gend", "ohn\u00b7ge\u00b7fehr", "was", "lie\u00b7bes", "dir", "ent\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PWS", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hastu zu deiner lust ein etwas au\u00dferkohren/", "tokens": ["Has\u00b7tu", "zu", "dei\u00b7ner", "lust", "ein", "et\u00b7was", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ART", "PIS", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das durch sein abseyn dich itzt setzt in kummer-stand?", "tokens": ["Das", "durch", "sein", "ab\u00b7seyn", "dich", "itzt", "setzt", "in", "kum\u00b7mer\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Suchstu vielleicht ein hertz mit freundschafft angef\u00fcllet/", "tokens": ["Suchs\u00b7tu", "viel\u00b7leicht", "ein", "hertz", "mit", "freund\u00b7schafft", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein hertz/ das lieb und gunst zu deinen f\u00fcssen legt/", "tokens": ["Ein", "hertz", "/", "das", "lieb", "und", "gunst", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "legt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein hertz/ aus welchem selbst die reinste treue quillet/", "tokens": ["Ein", "hertz", "/", "aus", "wel\u00b7chem", "selbst", "die", "reins\u00b7te", "treu\u00b7e", "quil\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "ADV", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dessen adern nichts als keusche demuth regt?", "tokens": ["Und", "des\u00b7sen", "a\u00b7dern", "nichts", "als", "keu\u00b7sche", "de\u00b7muth", "regt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PIS", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht suche/ sch\u00f6nstes bild/ du kanst bey mir schon finden", "tokens": ["Nicht", "su\u00b7che", "/", "sch\u00f6ns\u00b7tes", "bild", "/", "du", "kanst", "bey", "mir", "schon", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "VMFIN", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein hertze/ das dich selbst als seine g\u00f6ttin ehrt/", "tokens": ["Ein", "hert\u00b7ze", "/", "das", "dich", "selbst", "als", "sei\u00b7ne", "g\u00f6t\u00b7tin", "ehrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das sich auff ewig will zu deinem dienst verbinden/", "tokens": ["Das", "sich", "auff", "e\u00b7wig", "will", "zu", "dei\u00b7nem", "dienst", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJD", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und dessen reine glut kein sturm der zeit versehrt.", "tokens": ["Und", "des\u00b7sen", "rei\u00b7ne", "glut", "kein", "sturm", "der", "zeit", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kom\u0303! kom\u0303! demnach mein licht! und nimm was dir geb\u00fchret/", "tokens": ["Kom\u0303", "!", "kom\u0303", "!", "dem\u00b7nach", "mein", "licht", "!", "und", "nimm", "was", "dir", "ge\u00b7b\u00fch\u00b7ret", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVIMP", "$.", "PAV", "PPOSAT", "NN", "$.", "KON", "VVIMP", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Rei\u00df diesen treuen schatz nach deinem willen hin/", "tokens": ["Rei\u00df", "die\u00b7sen", "treu\u00b7en", "schatz", "nach", "dei\u00b7nem", "wil\u00b7len", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und glaube da\u00df ich stets/ so lang mein hertz sich r\u00fchret/", "tokens": ["Und", "glau\u00b7be", "da\u00df", "ich", "stets", "/", "so", "lang", "mein", "hertz", "sich", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "ADV", "$(", "ADV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In unverr\u00fcckter gunst dein treuer diener bin.", "tokens": ["In", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "gunst", "dein", "treu\u00b7er", "die\u00b7ner", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}