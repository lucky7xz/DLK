{"textgrid.poem.35959": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Lenz schwebt hernieder", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz schwebt hernieder", "tokens": ["Der", "Lenz", "schwebt", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Aus schimmernder Luft,", "tokens": ["Aus", "schim\u00b7mern\u00b7der", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bringt Farben und Lieder", "tokens": ["Bringt", "Far\u00b7ben", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und W\u00e4rme und Duft.", "tokens": ["Und", "W\u00e4r\u00b7me", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und willst du mich ziehen? \u2013", "tokens": ["Und", "willst", "du", "mich", "zie\u00b7hen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bin zierlich und fein; \u2013", "tokens": ["Bin", "zier\u00b7lich", "und", "fein", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So lieblich mein Bl\u00fchen,", "tokens": ["So", "lieb\u00b7lich", "mein", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein Duften so rein.", "tokens": ["Mein", "Duf\u00b7ten", "so", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}