, "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wozu?", "tokens": ["Wo\u00b7zu", "?"], "token_info": ["word", "punct"], "pos": ["PWAV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Wir halten uns umfangen", "tokens": ["Wir", "hal\u00b7ten", "uns", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Nacht und Paradies.", "tokens": ["In", "Nacht", "und", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Abendglocken klangen", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus dumpfestem Verlie\u00df.", "tokens": ["Aus", "dum\u00b7pfes\u00b7tem", "Ver\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wir wissen unsrer H\u00e4nde", "tokens": ["Wir", "wis\u00b7sen", "uns\u00b7rer", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Herzen einen Pfad.", "tokens": ["Und", "Her\u00b7zen", "ei\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer wei\u00df, wie bald das Ende", "tokens": ["Wer", "wei\u00df", ",", "wie", "bald", "das", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", 