{"textgrid.poem.35628": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im Osten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den wilden Orgeln des Wintersturms", "tokens": ["Den", "wil\u00b7den", "Or\u00b7geln", "des", "Win\u00b7ter\u00b7sturms"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gleicht des Volkes finstrer Zorn,", "tokens": ["Gleicht", "des", "Vol\u00b7kes", "finst\u00b7rer", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die purpurne Woge der Schlacht,", "tokens": ["Die", "pur\u00b7pur\u00b7ne", "Wo\u00b7ge", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Entlaubter Sterne.", "tokens": ["Ent\u00b7laub\u00b7ter", "Ster\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mit zerbrochnen Brauen, silbernen Armen", "tokens": ["Mit", "zer\u00b7broch\u00b7nen", "Brau\u00b7en", ",", "sil\u00b7ber\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Winkt sterbenden Soldaten die Nacht.", "tokens": ["Winkt", "ster\u00b7ben\u00b7den", "Sol\u00b7da\u00b7ten", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Im Schatten der herbstlichen Esche", "tokens": ["Im", "Schat\u00b7ten", "der", "herbst\u00b7li\u00b7chen", "E\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Seufzen die Geister der Erschlagenen.", "tokens": ["Seuf\u00b7zen", "die", "Geis\u00b7ter", "der", "Er\u00b7schla\u00b7ge\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dornige Wildnis umg\u00fcrtet die Stadt.", "tokens": ["Dor\u00b7ni\u00b7ge", "Wild\u00b7nis", "um\u00b7g\u00fcr\u00b7tet", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Von blutenden Stufen jagt der Mond", "tokens": ["Von", "blu\u00b7ten\u00b7den", "Stu\u00b7fen", "jagt", "der", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die erschrockenen Frauen.", "tokens": ["Die", "er\u00b7schro\u00b7cke\u00b7nen", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wilde W\u00f6lfe brachen durchs Tor.", "tokens": ["Wil\u00b7de", "W\u00f6l\u00b7fe", "bra\u00b7chen", "durchs", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}