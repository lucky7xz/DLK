{"textgrid.poem.35803": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Immer werd' ich's weinend sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer werd' ich's weinend sagen,", "tokens": ["Im\u00b7mer", "werd'", "ich's", "wei\u00b7nend", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich, Jesus! Dich betr\u00fcbt;", "tokens": ["Da\u00df", "ich", ",", "Je\u00b7sus", "!", "Dich", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$.", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer werd' ich's g'nug beklagen,", "tokens": ["Nim\u00b7mer", "werd'", "ich's", "g'\u00b7nug", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df ich Dich so sp\u00e4t geliebt.", "tokens": ["Da\u00df", "ich", "Dich", "so", "sp\u00e4t", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer will ich ringen, beten,", "tokens": ["Im\u00b7mer", "will", "ich", "rin\u00b7gen", ",", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Erbarmen zu erflehn;", "tokens": ["Dein", "Er\u00b7bar\u00b7men", "zu", "er\u00b7flehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber \u2013 ach! mein Uebertreten,", "tokens": ["A\u00b7ber", "\u2013", "ach", "!", "mein", "Ue\u00b7bert\u00b7re\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "ITJ", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer wird es ungeschehn. \u2013", "tokens": ["Nim\u00b7mer", "wird", "es", "un\u00b7ge\u00b7schehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jesus! sei mein F\u00fchrer immer", "tokens": ["Je\u00b7sus", "!", "sei", "mein", "F\u00fch\u00b7rer", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Weg zum Vaterland!", "tokens": ["Auf", "dem", "Weg", "zum", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer sei mein Lieben, nimmer", "tokens": ["Nim\u00b7mer", "sei", "mein", "Lie\u00b7ben", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem Andern zugewandt.", "tokens": ["Ei\u00b7nem", "An\u00b7dern", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}