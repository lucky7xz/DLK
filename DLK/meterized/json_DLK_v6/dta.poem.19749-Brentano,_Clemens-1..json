{"dta.poem.19749": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Urlaub Frau um euren werthen Diestmann               ", "tokens": ["Mit", "Ur\u00b7laub", "Frau", "um", "eu\u00b7ren", "wert\u00b7hen", "Diest\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geheissen war der Bremberger", "tokens": ["Ge\u00b7heis\u00b7sen", "war", "der", "Brem\u00b7ber\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein edler Ritter weise,", "tokens": ["Ein", "ed\u00b7ler", "Rit\u00b7ter", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Ton ich euch wohl singen kann,", "tokens": ["In", "sei\u00b7nem", "Ton", "ich", "euch", "wohl", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darin mir niemand verdenke,", "tokens": ["Da\u00b7rin", "mir", "nie\u00b7mand", "ver\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sein Lob immer preise", "tokens": ["Sein", "Lob", "im\u00b7mer", "prei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Er hat gesungen mannigfalt,", "tokens": ["Er", "hat", "ge\u00b7sun\u00b7gen", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das red ich auf die Treue mein", "tokens": ["Das", "red", "ich", "auf", "die", "Treu\u00b7e", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von einer sch\u00f6nen Frauen.", "tokens": ["Von", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "An ihm geschah grosse Gewalt,", "tokens": ["An", "ihm", "ge\u00b7schah", "gros\u00b7se", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Da\u00df er verlor das Leben sein,", "tokens": ["Da\u00df", "er", "ver\u00b7lor", "das", "Le\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Leib der ward ihm zerhauen.", "tokens": ["Sein", "Leib", "der", "ward", "ihm", "zer\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Der Herr der sprach: \u201eDu hast mir lieb die Fraue", "tokens": ["Der", "Herr", "der", "sprach", ":", "\u201e", "Du", "hast", "mir", "lieb", "die", "Frau\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "mein", "tokens": ["mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "\u201eo Bremberger es geht dir an das Leben dein!\u201c", "tokens": ["\u201e", "o", "Brem\u00b7ber\u00b7ger", "es", "geht", "dir", "an", "das", "Le\u00b7ben", "dein", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sein Haupt das ward ihm abgeschlagen", "tokens": ["Sein", "Haupt", "das", "ward", "ihm", "ab\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zu derselben Stund,", "tokens": ["Zu", "der\u00b7sel\u00b7ben", "Stund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Das Herz er in dem Leibe trug,", "tokens": ["Das", "Herz", "er", "in", "dem", "Lei\u00b7be", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das a\u00df der Fraue rother Mund.", "tokens": ["Das", "a\u00df", "der", "Frau\u00b7e", "ro\u00b7ther", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Herr der sprach: \u201eFrau k\u00f6nnt ihr mich beschei-", "tokens": ["Der", "Herr", "der", "sprach", ":", "\u201e", "Frau", "k\u00f6nnt", "ihr", "mich", "be\u00b7schei"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "$(", "NN", "VVFIN", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den nun,", "tokens": ["den", "nun", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u201ewas ihr jetzund gegessen hand,", "tokens": ["\u201e", "was", "ihr", "je\u00b7tzund", "ge\u00b7ges\u00b7sen", "hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eda\u00df euchs der lieb Gott lohne.\u201c", "tokens": ["\u201e", "da\u00df", "euchs", "der", "lieb", "Gott", "loh\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PIS", "ART", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Frau die sprach: \u201eUnd das wei\u00df ich sicher", "tokens": ["Die", "Frau", "die", "sprach", ":", "\u201e", "Und", "das", "wei\u00df", "ich", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "$(", "KON", "PDS", "VVFIN", "PPER", "PRF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "nicht", "tokens": ["nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "\u201eich wollts also gern wissen ", "tokens": ["\u201e", "ich", "wollts", "al\u00b7so", "gern", "wis\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201ees schmecket mir also sch\u00f6ne.", "tokens": ["\u201e", "es", "schme\u00b7cket", "mir", "al\u00b7so", "sch\u00f6\u00b7ne", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Er sprach: \u201eF\u00fcrwahr glaub du mirs,", "tokens": ["Er", "sprach", ":", "\u201e", "F\u00fcr\u00b7wahr", "glaub", "du", "mirs", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "\u201ees ist gewesen Brembergers Herz,", "tokens": ["\u201e", "es", "ist", "ge\u00b7we\u00b7sen", "Brem\u00b7ber\u00b7gers", "Herz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VAPP", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "\u201eer trugs in seinem Leibe", "tokens": ["\u201e", "er", "trugs", "in", "sei\u00b7nem", "Lei\u00b7be"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "\u201eund bracht dir viel Schimpf und Scherz,", "tokens": ["\u201e", "und", "bracht", "dir", "viel", "Schimpf", "und", "Scherz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "\u201ees konnt dir machen Freuden viel", "tokens": ["\u201e", "es", "konnt", "dir", "ma\u00b7chen", "Freu\u00b7den", "viel"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVINF", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u201eund konnt dir Leid vertreiben.\u201c", "tokens": ["\u201e", "und", "konnt", "dir", "Leid", "ver\u00b7trei\u00b7ben", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die Frau sprach: \u201eHab ich gegessen das mir Leid vertrie-", "tokens": ["Die", "Frau", "sprach", ":", "\u201e", "Hab", "ich", "ge\u00b7ges\u00b7sen", "das", "mir", "Leid", "ver\u00b7trie"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "PPER", "VVPP", "PRELS", "PPER", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "\u201eben hat", "tokens": ["\u201e", "ben", "hat"], "token_info": ["punct", "word", "word"], "pos": ["$(", "VVPP", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "\u201eund sollt meiner armen Seel nimmer werden Rath,", "tokens": ["\u201e", "und", "sollt", "mei\u00b7ner", "ar\u00b7men", "Seel", "nim\u00b7mer", "wer\u00b7den", "Rath", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "\u201eso thu ich einen Trunk darauf zu dieser Stund", "tokens": ["\u201e", "so", "thu", "ich", "ei\u00b7nen", "Trunk", "da\u00b7rauf", "zu", "die\u00b7ser", "Stund"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201evon Essen und von Trinken kommt nimmer mehr in", "tokens": ["\u201e", "von", "Es\u00b7sen", "und", "von", "Trin\u00b7ken", "kommt", "nim\u00b7mer", "mehr", "in"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "ADV", "ADV", "APPR"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.20": {"text": "\u201emeinen Mund.\u201c", "tokens": ["\u201e", "mei\u00b7nen", "Mund", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die Frau stand auf, sie eilet von dem Tische", "tokens": ["Die", "Frau", "stand", "auf", ",", "sie", "ei\u00b7let", "von", "dem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verbarg sich in ihr Gemach,", "tokens": ["Ver\u00b7barg", "sich", "in", "ihr", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und dacht ihrs Herzeus Schwere:", "tokens": ["Und", "dacht", "ihrs", "Her\u00b7zeus", "Schwe\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "NN", "$."], "meter": "-+-+-+-", "mea