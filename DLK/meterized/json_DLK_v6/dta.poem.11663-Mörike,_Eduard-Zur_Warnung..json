{"dta.poem.11663": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zur Warnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Einmal nach einer lustigen Nacht", "tokens": ["Ein\u00b7mal", "nach", "ei\u00b7ner", "lus\u00b7ti\u00b7gen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "War ich am Morgen seltsam aufgewacht:", "tokens": ["War", "ich", "am", "Mor\u00b7gen", "selt\u00b7sam", "auf\u00b7ge\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durst \u2014 Wasserscheu \u2014 ungleich Gebl\u00fct,", "tokens": ["Durst", "Was\u00b7ser\u00b7scheu", "un\u00b7gleich", "Ge\u00b7bl\u00fct", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dabei ger\u00fchrt und weichlich im Gem\u00fcth,", "tokens": ["Da\u00b7bei", "ge\u00b7r\u00fchrt", "und", "weich\u00b7lich", "im", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beinah poetisch, ja, ich bat die Muse um ein Lied;", "tokens": ["Bei\u00b7nah", "po\u00b7e\u00b7tisch", ",", "ja", ",", "ich", "bat", "die", "Mu\u00b7se", "um", "ein", "Lied", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Sie, mit verstelltem Pathos, spottet' mein,", "tokens": ["Sie", ",", "mit", "ver\u00b7stell\u00b7tem", "Pa\u00b7thos", ",", "spot\u00b7tet'", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gab mir den schn\u00f6den Bafel ein:", "tokens": ["Gab", "mir", "den", "schn\u00f6\u00b7den", "Ba\u00b7fel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Am Wasserfall;", "tokens": ["Am", "Was\u00b7ser\u00b7fall", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ein Vogel ebenfalls,", "tokens": ["Und", "ein", "Vo\u00b7gel", "e\u00b7ben\u00b7falls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der schreibt sich Wendehals,", "tokens": ["Der", "schreibt", "sich", "Wen\u00b7de\u00b7hals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Johann Jakob Wendehals;", "tokens": ["Jo\u00b7hann", "Ja\u00b7kob", "Wen\u00b7de\u00b7hals", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der thut tanzen", "tokens": ["Der", "thut", "tan\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Bei den Pflanzen", "tokens": ["Bei", "den", "Pflan\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Obbemeldten Wasserfalls &#8212;&#8220;", "tokens": ["Ob\u00b7be\u00b7meld\u00b7ten", "Was\u00b7ser\u00b7falls", "&#8212;", "&#8220;"], "token_info": ["word", "word", "XML_entity", "XML_entity"], "pos": ["ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So ging es fort: mir wurde immer b\u00e4nger;", "tokens": ["So", "ging", "es", "fort", ":", "mir", "wur\u00b7de", "im\u00b7mer", "b\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jezt sprang ich auf \u2014 zum Wein; der war denn auch", "tokens": ["Jezt", "sprang", "ich", "auf", "zum", "Wein", ";", "der", "war", "denn", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "APPRART", "NN", "$.", "ART", "VAFIN", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u2014 Merkt's euch, ihr thr\u00e4nenreichen S\u00e4nger,", "tokens": ["Merkt's", "euch", ",", "ihr", "thr\u00e4\u00b7nen\u00b7rei\u00b7chen", "S\u00e4n\u00b7ger", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Im Katzenjammer ruft man keine G\u00f6tter!", "tokens": ["Im", "Kat\u00b7zen\u00b7jam\u00b7mer", "ruft", "man", "kei\u00b7ne", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}