{"textgrid.poem.50111": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "67. Auff Attila der Hunnen K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass ich ", "tokens": ["Dass", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Dass ich nur seinen ", "tokens": ["Dass", "ich", "nur", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So dass es scheint, weil mich ein tapfrer Muht entz\u00fcckt,", "tokens": ["So", "dass", "es", "scheint", ",", "weil", "mich", "ein", "tapf\u00b7rer", "Muht", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dass er mir den ", "tokens": ["Dass", "er", "mir", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}}}}