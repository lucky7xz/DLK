{"textgrid.poem.66563": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stille Tage, die ihr leise", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille Tage, die ihr leise", "tokens": ["Stil\u00b7le", "Ta\u00b7ge", ",", "die", "ihr", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Schaffens Ernst beschwingt,", "tokens": ["Von", "des", "Schaf\u00b7fens", "Ernst", "be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir in st\u00f6rungslosem Gleise", "tokens": ["Mir", "in", "st\u00f6\u00b7rungs\u00b7lo\u00b7sem", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum bemerkt vor\u00fcber gingt:", "tokens": ["Kaum", "be\u00b7merkt", "vor\u00b7\u00fc\u00b7ber", "gingt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Th\u00e4tig war't ihr \u00fcberlegen", "tokens": ["Th\u00e4\u00b7tig", "wa\u00b7r't", "ihr", "\u00fc\u00b7berl\u00b7e\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Unruhvoller Gegenwart,", "tokens": ["Un\u00b7ruh\u00b7vol\u00b7ler", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so f\u00fchl' ich euren Segen", "tokens": ["Und", "so", "f\u00fchl'", "ich", "eu\u00b7ren", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir im Tiefsten offenbart.", "tokens": ["Mir", "im", "Tiefs\u00b7ten", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, den Segen zu vollenden,", "tokens": ["Ja", ",", "den", "Se\u00b7gen", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wi\u00dft ihr f\u00fcr des Liedes Ton", "tokens": ["Wi\u00dft", "ihr", "f\u00fcr", "des", "Lie\u00b7des", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch die Stimmung mir zu spenden,", "tokens": ["Noch", "die", "Stim\u00b7mung", "mir", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Arbeit sch\u00f6nsten Lohn.", "tokens": ["Als", "der", "Ar\u00b7beit", "sch\u00f6ns\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}