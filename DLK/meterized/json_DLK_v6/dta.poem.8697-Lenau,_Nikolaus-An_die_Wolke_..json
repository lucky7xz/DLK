{"dta.poem.8697": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Wolke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh nicht so schnell vor\u00fcber", "tokens": ["Zieh", "nicht", "so", "schnell", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An dieser stillen Heide;", "tokens": ["An", "die\u00b7ser", "stil\u00b7len", "Hei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zieh nicht so scheu vor\u00fcber", "tokens": ["Zieh", "nicht", "so", "scheu", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An meinem tiefen Leide;", "tokens": ["An", "mei\u00b7nem", "tie\u00b7fen", "Lei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du Wolke in der H\u00f6h',", "tokens": ["Du", "Wol\u00b7ke", "in", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Steh still bei meinem Weh!", "tokens": ["Steh", "still", "bei", "mei\u00b7nem", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und nimm auf deine Reise", "tokens": ["Und", "nimm", "auf", "dei\u00b7ne", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit fort zu ihr die Kunde:", "tokens": ["Mit", "fort", "zu", "ihr", "die", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz, die arme Waise,", "tokens": ["Mein", "Herz", ",", "die", "ar\u00b7me", "Wai\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verblutet an der Wunde,", "tokens": ["Ver\u00b7blu\u00b7tet", "an", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die mir durch ihren Trug", "tokens": ["Die", "mir", "durch", "ih\u00b7ren", "Trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Ungetreue schlug.", "tokens": ["Die", "Un\u00b7ge\u00b7treu\u00b7e", "schlug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und kommst auf deinen Wegen", "tokens": ["Und", "kommst", "auf", "dei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du an vor ihrem Hause,", "tokens": ["Du", "an", "vor", "ih\u00b7rem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So st\u00fcrze dich als Regen", "tokens": ["So", "st\u00fcr\u00b7ze", "dich", "als", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herunter mit Gebrause,", "tokens": ["Her\u00b7un\u00b7ter", "mit", "Ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie bei dunkler Nacht", "tokens": ["Da\u00df", "sie", "bei", "dunk\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aus ihrem Traum erwacht.", "tokens": ["Aus", "ih\u00b7rem", "Traum", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schlag' an die Fensterscheibe,", "tokens": ["Schlag'", "an", "die", "Fens\u00b7ter\u00b7schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schlag' an ihre Th\u00fcre,", "tokens": ["Und", "schlag'", "an", "ih\u00b7re", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sey dem falschen Weibe", "tokens": ["Und", "sey", "dem", "fal\u00b7schen", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Mahner an die Schw\u00fcre,", "tokens": ["Ein", "Mah\u00b7ner", "an", "die", "Schw\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die sie mir weinend sprach,", "tokens": ["Die", "sie", "mir", "wei\u00b7nend", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und die sie l\u00e4chelnd brach.", "tokens": ["Und", "die", "sie", "l\u00e4\u00b7chelnd", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und will sie das nicht h\u00f6ren", "tokens": ["Und", "will", "sie", "das", "nicht", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PDS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So magst von deinem Sitze", "tokens": ["So", "magst", "von", "dei\u00b7nem", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, Donner, dich emp\u00f6ren,", "tokens": ["Du", ",", "Don\u00b7ner", ",", "dich", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann r\u00fcttelt all' ihr Blitze,", "tokens": ["Dann", "r\u00fct\u00b7telt", "all'", "ihr", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ihr vor\u00fcberzieht,", "tokens": ["Wenn", "ihr", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An ihrem Augenlied!", "tokens": ["An", "ih\u00b7rem", "Au\u00b7gen\u00b7lied", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}