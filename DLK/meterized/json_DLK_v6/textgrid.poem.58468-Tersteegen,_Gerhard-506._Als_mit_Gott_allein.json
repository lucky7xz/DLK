{"textgrid.poem.58468": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "506. Als mit Gott allein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kommt, von Gott f\u00fcr gut annehme,", "tokens": ["Was", "kommt", ",", "von", "Gott", "f\u00fcr", "gut", "an\u00b7neh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Gott dich schmiege und bequeme,", "tokens": ["Nach", "Gott", "dich", "schmie\u00b7ge", "und", "be\u00b7que\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trag dich und alles Gotte auf,", "tokens": ["Trag", "dich", "und", "al\u00b7les", "Got\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denk mehr an Gott als dich im Lauf!", "tokens": ["Denk", "mehr", "an", "Gott", "als", "dich", "im", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NN", "KOUS", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, s\u00fc\u00dfer Gott, wer dich vergn\u00fcget,", "tokens": ["Ach", ",", "s\u00fc\u00b7\u00dfer", "Gott", ",", "wer", "dich", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dir vergn\u00fcgt im Elend lieget.", "tokens": ["Mit", "dir", "ver\u00b7gn\u00fcgt", "im", "E\u00b7lend", "lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}