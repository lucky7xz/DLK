{"dta.poem.19157": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es liegt ein Kl\u00fcmpchen Schnee, da alles rings gethaut,", "tokens": ["Es", "liegt", "ein", "Kl\u00fcmp\u00b7chen", "Schnee", ",", "da", "al\u00b7les", "rings", "ge\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nordwerts in einer Kluft, wo ihn der Blick nicht schaut,", "tokens": ["Nord\u00b7werts", "in", "ei\u00b7ner", "Kluft", ",", "wo", "ihn", "der", "Blick", "nicht", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Der Sonne Lebensblick, der weg das Wei\u00dfe nahm.", "tokens": ["Der", "Son\u00b7ne", "Le\u00b7bens\u00b7blick", ",", "der", "weg", "das", "Wei\u00b7\u00dfe", "nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun wird der wei\u00dfe grau vor Aerger und vor Gram;", "tokens": ["Nun", "wird", "der", "wei\u00b7\u00dfe", "grau", "vor", "A\u00b7er\u00b7ger", "und", "vor", "Gram", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wird vor Jammer schwarz. Schon recht geschieht dem Stolzen;", "tokens": ["Und", "wird", "vor", "Jam\u00b7mer", "schwarz", ".", "Schon", "recht", "ge\u00b7schieht", "dem", "Stol\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADJD", "$.", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum nicht frisch und zart ist er mit weggeschmolzen? \u2014", "tokens": ["Wa\u00b7rum", "nicht", "frisch", "und", "zart", "ist", "er", "mit", "weg\u00b7ge\u00b7schmol\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PTKNEG", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "APPR", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr fragt ob das villeicht auch bildlich sei gemeint?", "tokens": ["Ihr", "fragt", "ob", "das", "vil\u00b7leicht", "auch", "bild\u00b7lich", "sei", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gemeint nicht eigentlich, doch auch gut, wenn es scheint!", "tokens": ["Ge\u00b7meint", "nicht", "ei\u00b7gent\u00b7lich", ",", "doch", "auch", "gut", ",", "wenn", "es", "scheint", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "$,", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Oft wenn ich aus ein Licht und an ein Feuer blie\u00df,", "tokens": ["Oft", "wenn", "ich", "aus", "ein", "Licht", "und", "an", "ein", "Feu\u00b7er", "blie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Merkt' ich, da\u00df das sich auch sinnbildlich deuten lie\u00df.", "tokens": ["Merkt'", "ich", ",", "da\u00df", "das", "sich", "auch", "sinn\u00b7bild\u00b7lich", "deu\u00b7ten", "lie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PDS", "PRF", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}