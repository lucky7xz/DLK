{"textgrid.poem.49131": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Vom Leiden und Sterben Jesu Christi", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme, Jesu, her zu dir", "tokens": ["Ich", "kom\u00b7me", ",", "Je\u00b7su", ",", "her", "zu", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NE", "$,", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringe dir mein' Andacht f\u00fcr", "tokens": ["Und", "brin\u00b7ge", "dir", "mein'", "An\u00b7dacht", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem bittern Kreutz und Leiden;", "tokens": ["Von", "dei\u00b7nem", "bit\u00b7tern", "Kreutz", "und", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe zwar es mit gemacht,", "tokens": ["Ich", "ha\u00b7be", "zwar", "es", "mit", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df du zum Tode bist gebracht,", "tokens": ["Da\u00df", "du", "zum", "To\u00b7de", "bist", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wirstu mich darum nicht meiden,", "tokens": ["Doch", "wirs\u00b7tu", "mich", "da\u00b7rum", "nicht", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weil selbst dein Tod die Ursach ist,", "tokens": ["Weil", "selbst", "dein", "Tod", "die", "Ur\u00b7sach", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df du mein Freund geworden bist.", "tokens": ["Da\u00df", "du", "mein", "Freund", "ge\u00b7wor\u00b7den", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach Heyland, ja, viel mehr, als Freund!", "tokens": ["Ach", "Hey\u00b7land", ",", "ja", ",", "viel", "mehr", ",", "als", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PTKANT", "$,", "ADV", "ADV", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer stirbet doch f\u00fcr seinen Feind,", "tokens": ["Wer", "stir\u00b7bet", "doch", "f\u00fcr", "sei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer dient, der alle Welt regieret,", "tokens": ["Wer", "dient", ",", "der", "al\u00b7le", "Welt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wird ein Mensch, und ist doch Gott,", "tokens": ["Wer", "wird", "ein", "Mensch", ",", "und", "ist", "doch", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer liebet f\u00fcr den Himmel Spott,", "tokens": ["Wer", "lie\u00b7bet", "f\u00fcr", "den", "Him\u00b7mel", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer darbt, von dem das Brodt herr\u00fchret,", "tokens": ["Wer", "darbt", ",", "von", "dem", "das", "Brodt", "her\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Thust du es, liebster Jesu, nicht,", "tokens": ["Thust", "du", "es", ",", "liebs\u00b7ter", "Je\u00b7su", ",", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJA", "NE", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und das um fremder S\u00fcnde Pflicht?", "tokens": ["Und", "das", "um", "frem\u00b7der", "S\u00fcn\u00b7de", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du selber weist von keiner Schuld,", "tokens": ["Du", "sel\u00b7ber", "weist", "von", "kei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was du hie thust, ist lauter Huld,", "tokens": ["Was", "du", "hie", "thust", ",", "ist", "lau\u00b7ter", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du zahlst, was ich verbrochen habe,", "tokens": ["Du", "zahlst", ",", "was", "ich", "ver\u00b7bro\u00b7chen", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du leidst, und das Verdienst ist mein,", "tokens": ["Du", "leidst", ",", "und", "das", "Ver\u00b7dienst", "ist", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich solt' ein Kind des Todes seyn,", "tokens": ["Ich", "solt'", "ein", "Kind", "des", "To\u00b7des", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du gehst f\u00fcr mich hin zum Grabe,", "tokens": ["Und", "du", "gehst", "f\u00fcr", "mich", "hin", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ja, l\u00e4\u00dfest deinen Himmel stehn,", "tokens": ["Ja", ",", "l\u00e4\u00b7\u00dfest", "dei\u00b7nen", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ich zur H\u00f6llen solte gehn.", "tokens": ["Da", "ich", "zur", "H\u00f6l\u00b7len", "sol\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau, blinder Jude, doch recht zu,", "tokens": ["Schau", ",", "blin\u00b7der", "Ju\u00b7de", ",", "doch", "recht", "zu", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wen speystu an, wen schl\u00e4gest du?", "tokens": ["Wen", "speys\u00b7tu", "an", ",", "wen", "schl\u00e4\u00b7gest", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kanst du an deinem Heyl noch zweiffeln,", "tokens": ["Kanst", "du", "an", "dei\u00b7nem", "Heyl", "noch", "zweif\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ehrst du den Held aus Juda so,", "tokens": ["Ehrst", "du", "den", "Held", "aus", "Ju\u00b7da", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "APPR", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bist du um Zions H\u00fclff so froh,", "tokens": ["Bist", "du", "um", "Zi\u00b7ons", "H\u00fclff", "so", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Must hierum nur der Himmel tr\u00e4uffeln,", "tokens": ["Must", "hie\u00b7rum", "nur", "der", "Him\u00b7mel", "tr\u00e4uf\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist so dein K\u00f6nig dir genehm,", "tokens": ["Ist", "so", "dein", "K\u00f6\u00b7nig", "dir", "ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du Tochter von Jerusalem?", "tokens": ["Du", "Toch\u00b7ter", "von", "Je\u00b7ru\u00b7sa\u00b7lem", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mu\u00df ihm es denn ein M\u00f6rder nun", "tokens": ["Mu\u00df", "ihm", "es", "denn", "ein", "M\u00f6r\u00b7der", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zuvor in deiner Liebe thun,", "tokens": ["Zu\u00b7vor", "in", "dei\u00b7ner", "Lie\u00b7be", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdammstu den, der dich befreyet,", "tokens": ["Ver\u00b7damms\u00b7tu", "den", ",", "der", "dich", "be\u00b7fre\u00b7yet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Urtheilstu dem das Leben ab,", "tokens": ["Ur\u00b7theils\u00b7tu", "dem", "das", "Le\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ohn den die gantze Welt ein Grab,", "tokens": ["Ohn", "den", "die", "gant\u00b7ze", "Welt", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kreutzigst, der dich benedeyet?", "tokens": ["Und", "kreut\u00b7zigst", ",", "der", "dich", "be\u00b7ne\u00b7de\u00b7yet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch ach, was klag ich Ander' an,", "tokens": ["Doch", "ach", ",", "was", "klag", "ich", "An\u00b7der'", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "VVFIN", "PPER", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich habe dieses selbst gethan.", "tokens": ["Ich", "ha\u00b7be", "die\u00b7ses", "selbst", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein, mein ist alle Mi\u00dfethat,", "tokens": ["Mein", ",", "mein", "ist", "al\u00b7le", "Mi\u00b7\u00dfet\u00b7hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "PPOSAT", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, Jesu, dich gemartert hat,", "tokens": ["Die", ",", "Je\u00b7su", ",", "dich", "ge\u00b7mar\u00b7tert", "hat", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NE", "$,", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fing dich erstlich zu verklagen,", "tokens": ["Ich", "fing", "dich", "erst\u00b7lich", "zu", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich flochte dir die Dornen-Kron,", "tokens": ["Ich", "floch\u00b7te", "dir", "die", "Dor\u00b7nen\u00b7Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sprach dir mehr als Alle Hohn,", "tokens": ["Ich", "sprach", "dir", "mehr", "als", "Al\u00b7le", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich habe dich ans Kreutz geschlagen,", "tokens": ["Ich", "ha\u00b7be", "dich", "ans", "Kreutz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum denk', in was f\u00fcr Angst ich steh,", "tokens": ["Drum", "denk'", ",", "in", "was", "f\u00fcr", "Angst", "ich", "steh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPR", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Indem ich auff dein Leiden seh.", "tokens": ["In\u00b7dem", "ich", "auff", "dein", "Lei\u00b7den", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, ruff ich dir, mein Heyland, zu,", "tokens": ["Ach", ",", "ruff", "ich", "dir", ",", "mein", "Hey\u00b7land", ",", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, h\u00f6chstes Leben, stirbest du,", "tokens": ["Ach", ",", "h\u00f6chs\u00b7tes", "Le\u00b7ben", ",", "stir\u00b7best", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du A und O, kanst du dich enden,", "tokens": ["Du", "A", "und", "O", ",", "kanst", "du", "dich", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NE", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Neigst du das Haupt, du Haupt der Welt,", "tokens": ["Neigst", "du", "das", "Haupt", ",", "du", "Haupt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter":