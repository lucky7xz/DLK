{"textgrid.poem.51368": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ihr vor dem Spiegel steht,", "tokens": ["Weil", "ihr", "vor", "dem", "Spie\u00b7gel", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit ihm zu Rathe geht,", "tokens": ["Und", "mit", "ihm", "zu", "Ra\u00b7the", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Fra\u00fclein, habe ich euch erschlichen,", "tokens": ["Fra\u00fc\u00b7lein", ",", "ha\u00b7be", "ich", "euch", "er\u00b7schli\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dr\u00fcber etwas ihr verblichen.", "tokens": ["Dr\u00fc\u00b7ber", "et\u00b7was", "ihr", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcber Eurer Achsel hin", "tokens": ["\u00dc\u00b7ber", "Eu\u00b7rer", "Ach\u00b7sel", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht ihr mein Gesichte ziehn", "tokens": ["Seht", "ihr", "mein", "Ge\u00b7sich\u00b7te", "ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Spiegels reinen Pl\u00e4tzen", "tokens": ["In", "des", "Spie\u00b7gels", "rei\u00b7nen", "Pl\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zunechst an Eures setzen.", "tokens": ["Sich", "zu\u00b7nechst", "an", "Eu\u00b7res", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das entsetzen lie\u00df ja nach,", "tokens": ["Das", "ent\u00b7set\u00b7zen", "lie\u00df", "ja", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil mich Euer Mund besprach,", "tokens": ["Weil", "mich", "Eu\u00b7er", "Mund", "be\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df vom Putzen auf der Stellen", "tokens": ["Da\u00df", "vom", "Put\u00b7zen", "auf", "der", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich ein Urtheil solte f\u00e4llen.", "tokens": ["Ich", "ein", "Ur\u00b7theil", "sol\u00b7te", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zier und Antlitz voller Schein,", "tokens": ["Zier", "und", "Ant\u00b7litz", "vol\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fra\u00fclein, sprach ich, treffen ein:", "tokens": ["Fra\u00fc\u00b7lein", ",", "sprach", "ich", ",", "tref\u00b7fen", "ein", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber kehrt der Augen Kertzen", "tokens": ["A\u00b7ber", "kehrt", "der", "Au\u00b7gen", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort in Spiegel, hier zum Hertzen.", "tokens": ["Dort", "in", "Spie\u00b7gel", ",", "hier", "zum", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}