{"dta.poem.9299": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Maedgen sie verzeihe mir/", "tokens": ["Maed\u00b7gen", "sie", "ver\u00b7zei\u00b7he", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich gestern neben ihr", "tokens": ["Da\u00df", "ich", "ge\u00b7stern", "ne\u00b7ben", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Etwas lange sitzen bliebe/", "tokens": ["Et\u00b7was", "lan\u00b7ge", "sit\u00b7zen", "blie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und indem ich abschied nahm/", "tokens": ["Und", "in\u00b7dem", "ich", "ab\u00b7schied", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jmmer gieng und wieder kam/", "tokens": ["Jm\u00b7mer", "gieng", "und", "wie\u00b7der", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Cs geschach aus liebe.", "tokens": ["Cs", "ge\u00b7schach", "aus", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "2. Als ich auch in ihrer hand", "tokens": ["Als", "ich", "auch", "in", "ih\u00b7rer", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein erw\u00fcnschtes lager fand/", "tokens": ["Ein", "er\u00b7w\u00fcnschtes", "la\u00b7ger", "fand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und mich an die finger riebe/", "tokens": ["Und", "mich", "an", "die", "fin\u00b7ger", "rie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Straffte sie das ding an mir/", "tokens": ["Straff\u00b7te", "sie", "das", "ding", "an", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber was kont ich daf\u00fcr?", "tokens": ["A\u00b7ber", "was", "kont", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Es geschach aus liebe.", "tokens": ["Es", "ge\u00b7schach", "aus", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "3. Als ich ihr ein bi\u00dfgen tieff", "tokens": ["Als", "ich", "ihr", "ein", "bi\u00df\u00b7gen", "tieff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter ihre wangen griff", "tokens": ["Un\u00b7ter", "ih\u00b7re", "wan\u00b7gen", "griff"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kriegt ich tausend lungen-hiebe/", "tokens": ["Kriegt", "ich", "tau\u00b7send", "lun\u00b7gen\u00b7hie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich als w\u00e4r ich noch so arg/", "tokens": ["Gleich", "als", "w\u00e4r", "ich", "noch", "so", "arg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch was ich dadurch verbarg/", "tokens": ["Doch", "was", "ich", "da\u00b7durch", "ver\u00b7barg", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PAV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das geschah aus liebe.", "tokens": ["Das", "ge\u00b7schah", "aus", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "4. Als ich lang\u2019 im zweiffel stund/", "tokens": ["Als", "ich", "lang'", "im", "zweif\u00b7fel", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ich endlich auff den mund/", "tokens": ["Kam", "ich", "end\u00b7lich", "auff", "den", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht viel anders als die diebe:", "tokens": ["Nicht", "viel", "an\u00b7ders", "als", "die", "die\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ich stahl ihr einen ku\u00df/", "tokens": ["Denn", "ich", "stahl", "ihr", "ei\u00b7nen", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Halb mit ihrem unverdru\u00df/", "tokens": ["Halb", "mit", "ih\u00b7rem", "un\u00b7ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Es geschah aus liebe.", "tokens": ["Es", "ge\u00b7schah", "aus", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "5. Ach mein Engel/ la\u00df mir zu/", "tokens": ["Ach", "mein", "En\u00b7gel", "/", "la\u00df", "mir", "zu", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$(", "VVIMP", "PPER", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich etwas k\u00fchne thu/", "tokens": ["Da\u00df", "ich", "et\u00b7was", "k\u00fch\u00b7ne", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich in den schertzen \u00fcbe:", "tokens": ["Und", "mich", "in", "den", "schert\u00b7zen", "\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheint der handel etwas frey/", "tokens": ["Scheint", "der", "han\u00b7del", "et\u00b7was", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so bleibt es doch darbey:", "tokens": ["Ach", "so", "bleibt", "es", "doch", "dar\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Es geschieht aus liebe.", "tokens": ["Es", "ge\u00b7schieht", "aus", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "6. Mein gel\u00fccke straffe mich/", "tokens": ["Mein", "ge\u00b7l\u00fc\u00b7cke", "straf\u00b7fe", "mich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich sie ja wissentlich/", "tokens": ["Wenn", "ich", "sie", "ja", "wis\u00b7sent\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder mit bedacht betr\u00fcbe:", "tokens": ["O\u00b7der", "mit", "be\u00b7dacht", "be\u00b7tr\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch da\u00df ich das s\u00fcsse spiel", "tokens": ["Doch", "da\u00df", "ich", "das", "s\u00fcs\u00b7se", "spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den spa\u00df erhalten will/", "tokens": ["Durch", "den", "spa\u00df", "er\u00b7hal\u00b7ten", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das geschieht aus liebe.", "tokens": ["Das", "ge\u00b7schieht", "aus", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "7. Nun sie schau mich freundlich an/", "tokens": ["Nun", "sie", "schau", "mich", "freund\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verbleib ihr unterthan/", "tokens": ["Ich", "ver\u00b7bleib", "ihr", "un\u00b7ter\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich lebe weil ich schniebe/", "tokens": ["Weil", "ich", "le\u00b7be", "weil", "ich", "schnie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wenn sich mein geist bem\u00fcht/",