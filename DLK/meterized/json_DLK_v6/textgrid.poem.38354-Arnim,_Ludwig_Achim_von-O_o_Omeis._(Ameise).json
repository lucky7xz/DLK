{"textgrid.poem.38354": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "O o Omeis. (Ameise)", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fauler Tropf, der m\u00fc\u00dfig ist, die Ameis schau wohl an,", "tokens": ["Du", "fau\u00b7ler", "Tropf", ",", "der", "m\u00fc\u00b7\u00dfig", "ist", ",", "die", "Am\u00b7eis", "schau", "wohl", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Schau wie sie ist ergeben der Arbeit Tag und Nacht,", "tokens": ["Schau", "wie", "sie", "ist", "er\u00b7ge\u00b7ben", "der", "Ar\u00b7beit", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VAFIN", "VVPP", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sch\u00e4m dich, der du dein Leben mit Faulheit zugebracht.", "tokens": ["Sch\u00e4m", "dich", ",", "der", "du", "dein", "Le\u00b7ben", "mit", "Faul\u00b7heit", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}