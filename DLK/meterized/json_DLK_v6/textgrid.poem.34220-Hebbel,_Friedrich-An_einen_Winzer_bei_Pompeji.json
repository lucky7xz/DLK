{"textgrid.poem.34220": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Winzer bei Pompeji", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie sitzen, die Traube, sie ist noch herbe, dir reiften", "tokens": ["La\u00df", "sie", "sit\u00b7zen", ",", "die", "Trau\u00b7be", ",", "sie", "ist", "noch", "her\u00b7be", ",", "dir", "reif\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Tausend and're, nur sie w\u00fcnscht sich des Feuers noch mehr.", "tokens": ["Tau\u00b7send", "an\u00b7d'\u00b7re", ",", "nur", "sie", "w\u00fcnscht", "sich", "des", "Feu\u00b7ers", "noch", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "ADV", "PPER", "VVFIN", "PRF", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dir verdirbt sie den Wein, den herbstlichen Wand'rer erquickt sie,", "tokens": ["Dir", "ver\u00b7dirbt", "sie", "den", "Wein", ",", "den", "herbst\u00b7li\u00b7chen", "Wan\u00b7d'\u00b7rer", "er\u00b7quickt", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und er segnet die Hand, die sie zu brechen verga\u00df.", "tokens": ["Und", "er", "seg\u00b7net", "die", "Hand", ",", "die", "sie", "zu", "bre\u00b7chen", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}