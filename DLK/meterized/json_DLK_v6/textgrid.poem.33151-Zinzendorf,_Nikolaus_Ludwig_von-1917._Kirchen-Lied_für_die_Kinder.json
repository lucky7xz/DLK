{"textgrid.poem.33151": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1917. Kirchen-Lied f\u00fcr die Kinder", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kinder! wo seyd ihr unsehlbar geborgen? Wo kan man so leute am besten versorgen?", "tokens": ["Ihr", "kin\u00b7der", "!", "wo", "seyd", "ihr", "un\u00b7sehl\u00b7bar", "ge\u00b7bor\u00b7gen", "?", "Wo", "kan", "man", "so", "leu\u00b7te", "am", "bes\u00b7ten", "ver\u00b7sor\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "PWAV", "VMFIN", "PIS", "ADV", "VVFIN", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+--+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Geborgen sind wir in dem blutigen Schreine; Versorgt in der Pflege der heilgen Gemeine.", "tokens": ["Ge\u00b7bor\u00b7gen", "sind", "wir", "in", "dem", "blu\u00b7ti\u00b7gen", "Schrei\u00b7ne", ";", "Ver\u00b7sorgt", "in", "der", "Pfle\u00b7ge", "der", "heil\u00b7gen", "Ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Was aber f\u00fcr kinder sich draussen befinden, wo bleiben denn die? Bleibt das alles dahinden?", "tokens": ["Was", "a\u00b7ber", "f\u00fcr", "kin\u00b7der", "sich", "draus\u00b7sen", "be\u00b7fin\u00b7den", ",", "wo", "blei\u00b7ben", "denn", "die", "?", "Bleibt", "das", "al\u00b7les", "da\u00b7hin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "PRF", "ADV", "VVFIN", "$,", "PWAV", "VVFIN", "ADV", "ART", "$.", "VVFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+--+--+--+--+-+-+-+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.4": {"line.1": {"text": "O nein doch! sie kommen geflogen wie tauben. Was t\u00e4ublein ist, l\u00e4\u00dft Er sich nie wieder rauben.", "tokens": ["O", "nein", "doch", "!", "sie", "kom\u00b7men", "ge\u00b7flo\u00b7gen", "wie", "tau\u00b7ben", ".", "Was", "t\u00e4ub\u00b7lein", "ist", ",", "l\u00e4\u00dft", "Er", "sich", "nie", "wie\u00b7der", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "ADV", "$.", "PPER", "VVINF", "VVPP", "KOKOM", "CARD", "$.", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+--+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.5": {"line.1": {"text": "Wird aber f\u00fcr alle die menge der heerden im schreine der seite auch raum gemacht werden?", "tokens": ["Wird", "a\u00b7ber", "f\u00fcr", "al\u00b7le", "die", "men\u00b7ge", "der", "heer\u00b7den", "im", "schrei\u00b7ne", "der", "sei\u00b7te", "auch", "raum", "ge\u00b7macht", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "ART", "NN", "ART", "NN", "APPRART", "VVFIN", "ART", "VVFIN", "ADV", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+-+---+--+--+--+-+--", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Das ist so. Und allenfalls kan noch in ritzen der h\u00e4nde und s\u00fcsse manch st\u00e4ubelein sitzen.", "tokens": ["Das", "ist", "so", ".", "Und", "al\u00b7len\u00b7falls", "kan", "noch", "in", "rit\u00b7zen", "der", "h\u00e4n\u00b7de", "und", "s\u00fcs\u00b7se", "manch", "st\u00e4u\u00b7be\u00b7lein", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$.", "KON", "ADV", "VMFIN", "ADV", "APPR", "VVFIN", "ART", "ADJA", "KON", "VVFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+--+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.7": {"line.1": {"text": "Was h\u00f6rt ihr, was sagt ihr, was singt ihr in stunden?", "tokens": ["Was", "h\u00f6rt", "ihr", ",", "was", "sagt", "ihr", ",", "was", "singt", "ihr", "in", "stun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "So sagt, zum exempel, wies L\u00e4mmlein gestaltet?", "tokens": ["So", "sagt", ",", "zum", "ex\u00b7em\u00b7pel", ",", "wies", "L\u00e4mm\u00b7lein", "ge\u00b7stal\u00b7tet", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN", "$,", "VVFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Wie ist denn das L\u00e4mmlein so blutig und grindig?", "tokens": ["Wie", "ist", "denn", "das", "L\u00e4mm\u00b7lein", "so", "blu\u00b7tig", "und", "grin\u00b7dig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Was ist nun der kinder ihr liebstes auf erden?", "tokens": ["Was", "ist", "nun", "der", "kin\u00b7der", "ihr", "liebs\u00b7tes", "auf", "er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "APPR", "NN", "$."], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Wird Satan und eigenwill nichts dabey st\u00f6ren?", "tokens": ["Wird", "Sa\u00b7tan", "und", "ei\u00b7gen\u00b7will", "nichts", "da\u00b7bey", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJD", "PIS", "PAV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Wenn aber die kinder die Mutter betr\u00fcben?", "tokens": ["Wenn", "a\u00b7ber", "die", "kin\u00b7der", "die", "Mut\u00b7ter", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "Es kommen doch aber die kinder zu jahren; wer kan sie denn immer so niedlich bewahren?", "tokens": ["Es", "kom\u00b7men", "doch", "a\u00b7ber", "die", "kin\u00b7der", "zu", "jah\u00b7ren", ";", "wer", "kan", "sie", "denn", "im\u00b7mer", "so", "nied\u00b7lich", "be\u00b7wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$.", "PWS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+--+--+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.14": {"line.1": {"text": "Je \u00e4lter wir werden, je kleiner wirds herze: wenns gro\u00df wird, so dem\u00fcthigts Jesu sein schmerze.", "tokens": ["Je", "\u00e4l\u00b7ter", "wir", "wer\u00b7den", ",", "je", "klei\u00b7ner", "wirds", "her\u00b7ze", ":", "wenns", "gro\u00df", "wird", ",", "so", "de\u00b7m\u00fct\u00b7higts", "Je\u00b7su", "sein", "schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAINF", "$,", "ADV", "ADJD", "VAFIN", "VVFIN", "$.", "KOUS", "ADJD", "VAFIN", "$,", "ADV", "ART", "NE", "PPOSAT", "ADJA", "$."], "meter": "-+--+--+--+---+--+-+--+-", "measure": "amphibrach.tetra.plus"}}, "stanza.15": {"line.1": {"text": "Auf die art, so h\u00e4ttet ihrs ewige leben?", "tokens": ["Auf", "die", "art", ",", "so", "h\u00e4t\u00b7tet", "ihrs", "e\u00b7wi\u00b7ge", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Das ist wol f\u00fcr euch gut; wo bleibet das Zeugen?", "tokens": ["Das", "ist", "wol", "f\u00fcr", "euch", "gut", ";", "wo", "blei\u00b7bet", "das", "Zeu\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PPER", "ADJD", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.17": {"line.1": {"text": "Wenn aber so kinder die h\u00fctte ablegen?", "tokens": ["Wenn", "a\u00b7ber", "so", "kin\u00b7der", "die", "h\u00fct\u00b7te", "ab\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}