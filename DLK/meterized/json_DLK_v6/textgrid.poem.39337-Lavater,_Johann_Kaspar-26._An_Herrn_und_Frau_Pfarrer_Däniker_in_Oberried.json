{"textgrid.poem.39337": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "26. An Herrn und Frau Pfarrer D\u00e4niker in Oberried", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bescheidenheit ist stets geduldig,", "tokens": ["Be\u00b7schei\u00b7den\u00b7heit", "ist", "stets", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wahre G\u00fcte immer gut!", "tokens": ["Die", "wah\u00b7re", "G\u00fc\u00b7te", "im\u00b7mer", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00e4chte Dankbarkeit f\u00fchlt immerdar sich schuldig", "tokens": ["Und", "\u00e4ch\u00b7te", "Dank\u00b7bar\u00b7keit", "f\u00fchlt", "im\u00b7mer\u00b7dar", "sich", "schul\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Freunde, der ihr Gutes thut.", "tokens": ["Dem", "Freun\u00b7de", ",", "der", "ihr", "Gu\u00b7tes", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von vielen stillen, guten Thaten,", "tokens": ["Von", "vie\u00b7len", "stil\u00b7len", ",", "gu\u00b7ten", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Euer gutes Herz erzeugt,", "tokens": ["Die", "Eu\u00b7er", "gu\u00b7tes", "Herz", "er\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Euer Mund dem Freunde selbst verschweigt,", "tokens": ["Und", "Eu\u00b7er", "Mund", "dem", "Freun\u00b7de", "selbst", "ver\u00b7schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sind die f\u00fcr jene Welt gewi\u00df fruchtreiche Saaten,", "tokens": ["Sind", "die", "f\u00fcr", "je\u00b7ne", "Welt", "ge\u00b7wi\u00df", "fruch\u00b7trei\u00b7che", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "PDAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die kein Gewitter nie, kein Zufall macht misrathen,", "tokens": ["Die", "kein", "Ge\u00b7wit\u00b7ter", "nie", ",", "kein", "Zu\u00b7fall", "macht", "mis\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$,", "PIAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die keine Zeit zerst\u00f6hren kann,", "tokens": ["Die", "kei\u00b7ne", "Zeit", "zer\u00b7st\u00f6h\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Ihr so oft an mir gethan.", "tokens": ["Die", "Ihr", "so", "oft", "an", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "O k\u00f6nnt' ich gro\u00dfe W\u00fcnsch' in kurze Worte fassen,", "tokens": ["O", "k\u00f6nnt'", "ich", "gro\u00b7\u00dfe", "W\u00fcn\u00b7sch'", "in", "kur\u00b7ze", "Wor\u00b7te", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Welch einen Seegen w\u00fcrd' ich Euch zur\u00fcck itzt lassen!", "tokens": ["Welch", "ei\u00b7nen", "See\u00b7gen", "w\u00fcrd'", "ich", "Euch", "zu\u00b7r\u00fcck", "itzt", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er w\u00fcrde herzlich, himmelrein,", "tokens": ["Er", "w\u00fcr\u00b7de", "herz\u00b7lich", ",", "him\u00b7mel\u00b7rein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Voll Einfalt und voll Kraft, wie Gottes Gnade seyn.", "tokens": ["Voll", "Ein\u00b7falt", "und", "voll", "Kraft", ",", "wie", "Got\u00b7tes", "Gna\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,", "PWAV", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}