{"textgrid.poem.25388": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die H\u00fctte lehnt am braunen Rebenh\u00fcgel,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00fctte lehnt am braunen Rebenh\u00fcgel,", "tokens": ["Die", "H\u00fct\u00b7te", "lehnt", "am", "brau\u00b7nen", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der sie Stunden oft ins weite Land geschaut,", "tokens": ["Von", "der", "sie", "Stun\u00b7den", "oft", "ins", "wei\u00b7te", "Land", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Daraus sie eines Tags, auf farbiger D\u00e4mmerung Fl\u00fcgel,", "tokens": ["Da\u00b7raus", "sie", "ei\u00b7nes", "Tags", ",", "auf", "far\u00b7bi\u00b7ger", "D\u00e4m\u00b7me\u00b7rung", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hintrat ins Volk, mit Gr\u00fcn geschm\u00fcckt wie eine Braut.", "tokens": ["Hin\u00b7trat", "ins", "Volk", ",", "mit", "Gr\u00fcn", "ge\u00b7schm\u00fcckt", "wie", "ei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "APPR", "NN", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch ihre Augen irrten blanke Sterne,", "tokens": ["Durch", "ih\u00b7re", "Au\u00b7gen", "irr\u00b7ten", "blan\u00b7ke", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um ihre Kinderwangen Feuer sprang,", "tokens": ["Um", "ih\u00b7re", "Kin\u00b7der\u00b7wan\u00b7gen", "Feu\u00b7er", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Stimme bebte, da ihr Wort zum Volke drang:", "tokens": ["Die", "Stim\u00b7me", "beb\u00b7te", ",", "da", "ihr", "Wort", "zum", "Vol\u00b7ke", "drang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbmich ruft ein hoher Wille in die gro\u00dfe Ferne.", "tokens": ["\u00bb", "mich", "ruft", "ein", "ho\u00b7her", "Wil\u00b7le", "in", "die", "gro\u00b7\u00dfe", "Fer\u00b7ne", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fragt nicht noch sorgt euch, was mir Schicksal werde,", "tokens": ["Fragt", "nicht", "noch", "sorgt", "euch", ",", "was", "mir", "Schick\u00b7sal", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der h\u00e4lt mein Leben, der mir diese Sehnsucht schuf \u2013", "tokens": ["Der", "h\u00e4lt", "mein", "Le\u00b7ben", ",", "der", "mir", "die\u00b7se", "Sehn\u00b7sucht", "schuf", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus stiller Hut rei\u00dft mich ein ungeheurer Ruf", "tokens": ["Aus", "stil\u00b7ler", "Hut", "rei\u00dft", "mich", "ein", "un\u00b7ge\u00b7heu\u00b7rer", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In allen Sturm und Seligkeit der Erde.\u00ab", "tokens": ["In", "al\u00b7len", "Sturm", "und", "Se\u00b7lig\u00b7keit", "der", "Er\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie h\u00f6rte kaum, wie Greise schwach sich m\u00fchten.", "tokens": ["Sie", "h\u00f6r\u00b7te", "kaum", ",", "wie", "Grei\u00b7se", "schwach", "sich", "m\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "NN", "VVFIN", "PRF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie gieng. Im Abend leuchtete wie Weizen gelb ihr Haar.", "tokens": ["Sie", "gieng", ".", "Im", "A\u00b7bend", "leuch\u00b7te\u00b7te", "wie", "Wei\u00b7zen", "gelb", "ihr", "Haar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "KOKOM", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Vor ihrem Fenster die Holunderbl\u00fcten", "tokens": ["Vor", "ih\u00b7rem", "Fens\u00b7ter", "die", "Ho\u00b7lun\u00b7der\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erglommen und verwehten einsam Jahr um Jahr.", "tokens": ["Er\u00b7glom\u00b7men", "und", "ver\u00b7weh\u00b7ten", "ein\u00b7sam", "Jahr", "um", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch eines Morgens, da die sp\u00e4ten Sterne blichen,", "tokens": ["Doch", "ei\u00b7nes", "Mor\u00b7gens", ",", "da", "die", "sp\u00e4\u00b7ten", "Ster\u00b7ne", "bli\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und banges Zwielicht eisig in den Zweigen hieng,", "tokens": ["Und", "ban\u00b7ges", "Zwie\u00b7licht", "ei\u00b7sig", "in", "den", "Zwei\u00b7gen", "hieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sah ein Weib, das Wasser sch\u00f6pfen gieng,", "tokens": ["Da", "sah", "ein", "Weib", ",", "das", "Was\u00b7ser", "sch\u00f6p\u00b7fen", "gieng", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie sich fremd und fr\u00f6stelnd in die T\u00fcr geschlichen.", "tokens": ["Wie", "sie", "sich", "fremd", "und", "fr\u00f6s\u00b7telnd", "in", "die", "T\u00fcr", "ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und seit dem Tage schwebt auf ihren Wegen", "tokens": ["Und", "seit", "dem", "Ta\u00b7ge", "schwebt", "auf", "ih\u00b7ren", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Glorienschein, der Gau und Volk erhellt,", "tokens": ["Ein", "Glo\u00b7ri\u00b7en\u00b7schein", ",", "der", "Gau", "und", "Volk", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und ihre Stimme hat den gro\u00dfen Segen", "tokens": ["Und", "ih\u00b7re", "Stim\u00b7me", "hat", "den", "gro\u00b7\u00dfen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebenden, die Gott zu Mittlern hat bestellt.", "tokens": ["Der", "Lie\u00b7ben\u00b7den", ",", "die", "Gott", "zu", "Mitt\u00b7lern", "hat", "be\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}