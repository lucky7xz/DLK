{"textgrid.poem.67955": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "23. Das Unvergleichbare", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleines Sternenheer der Nacht,", "tokens": ["Du", "klei\u00b7nes", "Ster\u00b7nen\u00b7heer", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das unserm forschenden Gesicht,", "tokens": ["Das", "un\u00b7serm", "for\u00b7schen\u00b7den", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr Zahl, als Schimmer, sichtbar macht,", "tokens": ["Mehr", "Zahl", ",", "als", "Schim\u00b7mer", ",", "sicht\u00b7bar", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Schaaren, denen Raum gebricht;", "tokens": ["Ihr", "Schaa\u00b7ren", ",", "de\u00b7nen", "Raum", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was seid ihr an der Sonne Licht?", "tokens": ["Was", "seid", "ihr", "an", "der", "Son\u00b7ne", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr fr\u00fchen Veilchen auf der Flur", "tokens": ["Ihr", "fr\u00fc\u00b7hen", "Veil\u00b7chen", "auf", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr in sch\u00f6ner Purpurtracht", "tokens": ["Die", "ihr", "in", "sch\u00f6\u00b7ner", "Pur\u00b7pur\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Erstgebohrne der Natur", "tokens": ["Als", "Erst\u00b7ge\u00b7bohr\u00b7ne", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So stolz, so spr\u00f6de um euch lacht;", "tokens": ["So", "stolz", ",", "so", "spr\u00f6\u00b7de", "um", "euch", "lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was seid ihr, wenn die Ros' erwacht?", "tokens": ["Was", "seid", "ihr", ",", "wenn", "die", "Ros'", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr kleinen V\u00f6gel in dem Hain,", "tokens": ["Ihr", "klei\u00b7nen", "V\u00f6\u00b7gel", "in", "dem", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit so reichem, regem Schall,", "tokens": ["Die", "mit", "so", "rei\u00b7chem", ",", "re\u00b7gem", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die S\u00e4nger der Natur zu seyn,", "tokens": ["Die", "S\u00e4n\u00b7ger", "der", "Na\u00b7tur", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Seelchen wirbeln. Allzumal", "tokens": ["Ihr", "Seel\u00b7chen", "wir\u00b7beln", ".", "All\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was seid ihr zu der Nachtigal?", "tokens": ["Was", "seid", "ihr", "zu", "der", "Nach\u00b7ti\u00b7gal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So tritt mein M\u00e4dchen in den Kreis", "tokens": ["So", "tritt", "mein", "M\u00e4d\u00b7chen", "in", "den", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nen, eine K\u00f6nigin.", "tokens": ["Der", "Sch\u00f6\u00b7nen", ",", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sch\u00f6nste gibt ihr gern den Preis", "tokens": ["Die", "Sch\u00f6ns\u00b7te", "gibt", "ihr", "gern", "den", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Lieblichkeit und frohem Sinn;", "tokens": ["An", "Lieb\u00b7lich\u00b7keit", "und", "fro\u00b7hem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe schuf sie K\u00f6nigin.", "tokens": ["Die", "Lie\u00b7be", "schuf", "sie", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}