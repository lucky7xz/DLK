{"textgrid.poem.61279": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Einem Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wenn dahin die erste Jugend,", "tokens": ["O", "wenn", "da\u00b7hin", "die", "ers\u00b7te", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schuldlos noch, noch ohne Tugend", "tokens": ["Die", "schuld\u00b7los", "noch", ",", "noch", "oh\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Tag verschw\u00e4rmt im Sonnenglanz,", "tokens": ["Den", "Tag", "ver\u00b7schw\u00e4rmt", "im", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die unter ahnungsvollen Schauern", "tokens": ["Die", "un\u00b7ter", "ah\u00b7nungs\u00b7vol\u00b7len", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Mondnacht heut verwacht in grundlos s\u00fc\u00dfem Trauern", "tokens": ["Die", "Mond\u00b7nacht", "heut", "ver\u00b7wacht", "in", "grund\u00b7los", "s\u00fc\u00b7\u00dfem", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und morgen sie durchst\u00fcrmt im Tanz;", "tokens": ["Und", "mor\u00b7gen", "sie", "durchs\u00b7t\u00fcrmt", "im", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dieser holde Rausch verflogen,", "tokens": ["Wenn", "die\u00b7ser", "hol\u00b7de", "Rausch", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der an Erkenntnis arm, verschwendrisch im Gef\u00fchl", "tokens": ["Der", "an", "Er\u00b7kennt\u00b7nis", "arm", ",", "ver\u00b7schwend\u00b7risch", "im", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJD", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In unerme\u00dflichem Gew\u00fchl", "tokens": ["In", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7chem", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von Well' in Welle dich gezogen:", "tokens": ["Von", "Well'", "in", "Wel\u00b7le", "dich", "ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie weht so wunderbar dich dann", "tokens": ["Wie", "weht", "so", "wun\u00b7der\u00b7bar", "dich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des Lebens frischer Morgenschauder an!", "tokens": ["Des", "Le\u00b7bens", "fri\u00b7scher", "Mor\u00b7gen\u00b7schau\u00b7der", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, von den Dingen, drin du webtest,", "tokens": ["Ach", ",", "von", "den", "Din\u00b7gen", ",", "drin", "du", "web\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Siehst du dich pl\u00f6tzlich losgetrennt;", "tokens": ["Siehst", "du", "dich", "pl\u00f6tz\u00b7lich", "los\u00b7ge\u00b7trennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00fchlst, da\u00df du in goldnen Tr\u00e4umen lebtest,", "tokens": ["Du", "f\u00fchlst", ",", "da\u00df", "du", "in", "gold\u00b7nen", "Tr\u00e4u\u00b7men", "leb\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und suchest sehnsuchtsvoll dein wahres Element.", "tokens": ["Und", "su\u00b7chest", "sehn\u00b7suchts\u00b7voll", "dein", "wah\u00b7res", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht l\u00e4nger kannst du dich vergeuden,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "kannst", "du", "dich", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des gro\u00dfen Alls bewu\u00dftlos kleiner Teil;", "tokens": ["Des", "gro\u00b7\u00dfen", "Alls", "be\u00b7wu\u00dft\u00b7los", "klei\u00b7ner", "Teil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es strebt dein Geist nach eignen Freuden,", "tokens": ["Es", "strebt", "dein", "Geist", "nach", "eig\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach eignen Schmerzen, eignem Heil.", "tokens": ["Nach", "eig\u00b7nen", "Schmer\u00b7zen", ",", "eig\u00b7nem", "Heil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sieh, in nimmer m\u00fcdem Ringen", "tokens": ["Und", "sieh", ",", "in", "nim\u00b7mer", "m\u00fc\u00b7dem", "Rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erbaust du deine stille Welt;", "tokens": ["Er\u00b7baust", "du", "dei\u00b7ne", "stil\u00b7le", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele strebt mit jungen Schwingen", "tokens": ["Die", "See\u00b7le", "strebt", "mit", "jun\u00b7gen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Zweifeln k\u00fchn zum Himmelszelt.", "tokens": ["Aus", "Zwei\u00b7feln", "k\u00fchn", "zum", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die milde W\u00e4rme, die dein Herz ertauschte", "tokens": ["Die", "mil\u00b7de", "W\u00e4r\u00b7me", ",", "die", "dein", "Herz", "er\u00b7tauschte"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr hast'ge Glut, sie bricht dir standhaft Bahn,", "tokens": ["F\u00fcr", "hast'\u00b7ge", "Glut", ",", "sie", "bricht", "dir", "stand\u00b7haft", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und die Natur, die dich berauschte,", "tokens": ["Und", "die", "Na\u00b7tur", ",", "die", "dich", "be\u00b7rauschte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht dich mit klaren Augen an.", "tokens": ["Sieht", "dich", "mit", "kla\u00b7ren", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, wenn sich's dann wie Traumesh\u00fclle,", "tokens": ["Ach", ",", "wenn", "sich's", "dann", "wie", "Trau\u00b7mes\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PIS", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Nebel dir vom Blicke streift,", "tokens": ["Wie", "Ne\u00b7bel", "dir", "vom", "Bli\u00b7cke", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und himmlischer Gedanken F\u00fclle", "tokens": ["Und", "himm\u00b7li\u00b7scher", "Ge\u00b7dan\u00b7ken", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Haupte wachsend reift;", "tokens": ["In", "dei\u00b7nem", "Haup\u00b7te", "wach\u00b7send", "reift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn aus verworrner Vorzeit wildem Handeln,", "tokens": ["Wenn", "aus", "ver\u00b7worr\u00b7ner", "Vor\u00b7zeit", "wil\u00b7dem", "Han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus jeder Tat, die heute ward,", "tokens": ["Aus", "je\u00b7der", "Tat", ",", "die", "heu\u00b7te", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie aus des Jahres heil'gem Wandeln", "tokens": ["Wie", "aus", "des", "Jah\u00b7res", "heil'\u00b7gem", "Wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein ewig Walten dir sich offenbart;", "tokens": ["Ein", "e\u00b7wig", "Wal\u00b7ten", "dir", "sich", "of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn jene Sterne, die dort oben kreisen,", "tokens": ["Wenn", "je\u00b7ne", "Ster\u00b7ne", ",", "die", "dort", "o\u00b7ben", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Weltgeschlechter Gang, der kleinste Halm am Bach,", "tokens": ["Der", "Welt\u00b7ge\u00b7schlech\u00b7ter", "Gang", ",", "der", "kleins\u00b7te", "Halm", "am", "Bach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein eigen Herz in wundervollen Weisen", "tokens": ["Dein", "ei\u00b7gen", "Herz", "in", "wun\u00b7der\u00b7vol\u00b7len", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dir ", "tokens": ["Dir"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Dann will dein Busen weit sich dehnen,", "tokens": ["Dann", "will", "dein", "Bu\u00b7sen", "weit", "sich", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADJD", "PRF", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dich fa\u00dft ein unaussprechlich Sehnen,", "tokens": ["Dich", "fa\u00dft", "ein", "un\u00b7aus\u00b7sprech\u00b7lich", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Des innern Schatzes los zu sein;", "tokens": ["Des", "in\u00b7nern", "Schat\u00b7zes", "los", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Umsonst, es fehlt die Hand, um ihn zu heben.", "tokens": ["Um\u00b7sonst", ",", "es", "fehlt", "die", "Hand", ",", "um", "ihn", "zu", "he\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Dein Bestes kannst du niemand geben,", "tokens": ["Dein", "Bes\u00b7tes", "kannst", "du", "nie\u00b7mand", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und wie du suchst - du bist allein.", "tokens": ["Und", "wie", "du", "suchst", "du", "bist", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Dann halte fest, dann la\u00df aus deinem Herzen", "tokens": ["Dann", "hal\u00b7te", "fest", ",", "dann", "la\u00df", "aus", "dei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ADV", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Glauben dir hinweg nicht scherzen,", "tokens": ["Den", "Glau\u00b7ben", "dir", "hin\u00b7weg", "nicht", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ertrage still die Wucht der Einsamkeit;", "tokens": ["Er\u00b7tra\u00b7ge", "still", "die", "Wucht", "der", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie toll dich Widerspruch umschwirre,", "tokens": ["Wie", "toll", "dich", "Wi\u00b7der\u00b7spruch", "um\u00b7schwir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Harr' aus in Hoffnung und in Leid", "tokens": ["Harr'", "aus", "in", "Hoff\u00b7nung", "und", "in", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und werd am Gott in deiner Zeit", "tokens": ["Und", "werd", "am", "Gott", "in", "dei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7"