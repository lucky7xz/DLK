{"textgrid.poem.50673": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Clorinde sagt nicht oft, was sie nicht sagen soll;", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Clorinde sagt nicht oft, was sie nicht sagen soll;", "tokens": ["Clo\u00b7rin\u00b7de", "sagt", "nicht", "oft", ",", "was", "sie", "nicht", "sa\u00b7gen", "soll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch was sie sagt und thut, wird durch die Art besch\u00f6net,", "tokens": ["Doch", "was", "sie", "sagt", "und", "thut", ",", "wird", "durch", "die", "Art", "be\u00b7sch\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit der sie Alles thut. Es steht ihr Alles wohl,", "tokens": ["Mit", "der", "sie", "Al\u00b7les", "thut", ".", "Es", "steht", "ihr", "Al\u00b7les", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df die Tugend selbst ihr keinen Zierrath lehnet,", "tokens": ["So", "da\u00df", "die", "Tu\u00b7gend", "selbst", "ihr", "kei\u00b7nen", "Zier\u00b7rath", "leh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "PPOSAT", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den ihre Sch\u00f6nheit nicht der Tugend wiedergiebt:", "tokens": ["Den", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "nicht", "der", "Tu\u00b7gend", "wie\u00b7der\u00b7giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}