{"dta.poem.12021": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der ew'ge Dreiklang, der das irdische Get\u00f6se", "tokens": ["Der", "ew'\u00b7ge", "Drei\u00b7klang", ",", "der", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mit leiser Macht durchgreift, da\u00df ers in Einklang l\u00f6se;", "tokens": ["Mit", "lei\u00b7ser", "Macht", "durch\u00b7greift", ",", "da\u00df", "ers", "in", "Ein\u00b7klang", "l\u00f6\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der heil'ge Dreiklang, den du ewig mu\u00dft erkennen,", "tokens": ["Der", "heil'\u00b7ge", "Drei\u00b7klang", ",", "den", "du", "e\u00b7wig", "mu\u00dft", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie immer du ihn magst mit Wechselnamen nennen;", "tokens": ["Wie", "im\u00b7mer", "du", "ihn", "magst", "mit", "Wech\u00b7sel\u00b7na\u00b7men", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Den: Gott, Gem\u00fct und Welt, am einfachsten genannt,", "tokens": ["Den", ":", "Gott", ",", "Ge\u00b7m\u00fct", "und", "Welt", ",", "am", "ein\u00b7fachs\u00b7ten", "ge\u00b7nannt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer rein das G\u00f6ttliche am menschlichsten erkannt:", "tokens": ["Wer", "rein", "das", "G\u00f6tt\u00b7li\u00b7che", "am", "menschlichs\u00b7ten", "er\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Die drei, die Eines sind, und also sich erg\u00e4nzen,", "tokens": ["Die", "drei", ",", "die", "Ei\u00b7nes", "sind", ",", "und", "al\u00b7so", "sich", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "PRELS", "PIS", "VAFIN", "$,", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie sich gegenseits erf\u00fcllen und begr\u00e4nzen,", "tokens": ["Da\u00df", "sie", "sich", "ge\u00b7gen\u00b7seits", "er\u00b7f\u00fcl\u00b7len", "und", "be\u00b7gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Durchdringen und beziehn, begr\u00fcnden und erkl\u00e4ren,", "tokens": ["Durch\u00b7drin\u00b7gen", "und", "be\u00b7ziehn", ",", "be\u00b7gr\u00fcn\u00b7den", "und", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst nicht w\u00e4ren, wenn sie nicht verbunden w\u00e4ren:", "tokens": ["Und", "selbst", "nicht", "w\u00e4\u00b7ren", ",", "wenn", "sie", "nicht", "ver\u00b7bun\u00b7den", "w\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VAFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Komm la\u00df uns, um in uns den Zwiespalt zu vers\u00f6hnen,", "tokens": ["Komm", "la\u00df", "uns", ",", "um", "in", "uns", "den", "Zwies\u00b7palt", "zu", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVIMP", "PPER", "$,", "KOUI", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dem Dreieinklang ganz durchklingen und durcht\u00f6nen:", "tokens": ["Mit", "dem", "Drei\u00b7ein\u00b7klang", "ganz", "durch\u00b7klin\u00b7gen", "und", "durch\u00b7t\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Welt und dein Gem\u00fct, sie w\u00fcrden sich zerreiben,", "tokens": ["Die", "Welt", "und", "dein", "Ge\u00b7m\u00fct", ",", "sie", "w\u00fcr\u00b7den", "sich", "zer\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nicht vermittelnd Gott sie hie\u00df' in Eintracht bleiben.", "tokens": ["Wenn", "nicht", "ver\u00b7mit\u00b7telnd", "Gott", "sie", "hie\u00df'", "in", "Ein\u00b7tracht", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVPP", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Gott aber und die Welt, sie w\u00e4ren ganz geschieden,", "tokens": ["Gott", "a\u00b7ber", "und", "die", "Welt", ",", "sie", "w\u00e4\u00b7ren", "ganz", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie nicht dein Gem\u00fct geglichen aus in Frieden.", "tokens": ["Wenn", "sie", "nicht", "dein", "Ge\u00b7m\u00fct", "ge\u00b7gli\u00b7chen", "aus", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch Gott und dein Gem\u00fct, sie w\u00fcrden sich vermischen", "tokens": ["Doch", "Gott", "und", "dein", "Ge\u00b7m\u00fct", ",", "sie", "w\u00fcr\u00b7den", "sich", "ver\u00b7mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Innern, st\u00e4nde nicht die \u00e4u\u00dfre Welt dazwischen;", "tokens": ["Im", "In\u00b7nern", ",", "st\u00e4n\u00b7de", "nicht", "die", "\u00e4u\u00df\u00b7re", "Welt", "da\u00b7zwi\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Welt, die dem Gem\u00fct Gott so verbirgt wie zeigt,", "tokens": ["Die", "Welt", ",", "die", "dem", "Ge\u00b7m\u00fct", "Gott", "so", "ver\u00b7birgt", "wie", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die es ewig auf, er ewig nieder steigt.", "tokens": ["Durch", "die", "es", "e\u00b7wig", "auf", ",", "er", "e\u00b7wig", "nie\u00b7der", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PTKVZ", "$,", "PPER", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}