{"textgrid.poem.46856": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du wandelst ober der Sonne]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wandelst ober der Sonne,", "tokens": ["Du", "wan\u00b7delst", "o\u00b7ber", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In einem anderen Licht;", "tokens": ["In", "ei\u00b7nem", "an\u00b7de\u00b7ren", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "F\u00fcr deine h\u00f6here Wonne", "tokens": ["F\u00fcr", "dei\u00b7ne", "h\u00f6\u00b7he\u00b7re", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab' ich die Sinne nicht.", "tokens": ["Hab'", "ich", "die", "Sin\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich freut es hier vom fr\u00fchen", "tokens": ["Mich", "freut", "es", "hier", "vom", "fr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lichtblicke der Sonne mild", "tokens": ["Licht\u00b7bli\u00b7cke", "der", "Son\u00b7ne", "mild"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit L\u00e4cheln \u00fcberspr\u00fchen", "tokens": ["Mit", "L\u00e4\u00b7cheln", "\u00fc\u00b7bers\u00b7pr\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu sehn an der Wand dein Bild;", "tokens": ["Zu", "sehn", "an", "der", "Wand", "dein", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und mehr noch, dich im Herzen", "tokens": ["Und", "mehr", "noch", ",", "dich", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu tragen am Sonnenstrahl", "tokens": ["Zu", "tra\u00b7gen", "am", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unter der L\u00fcfte Scherzen", "tokens": ["Un\u00b7ter", "der", "L\u00fcf\u00b7te", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Durchs bl\u00fchende Fr\u00fchlingsthal.", "tokens": ["Durchs", "bl\u00fc\u00b7hen\u00b7de", "Fr\u00fch\u00b7lings\u00b7thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}