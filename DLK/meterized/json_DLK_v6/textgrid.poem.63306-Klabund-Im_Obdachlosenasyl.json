{"textgrid.poem.63306": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Im Obdachlosenasyl", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war'n junges Ding,", "tokens": ["Ich", "wa\u00b7r'n", "jun\u00b7ges", "Ding", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Man immer frisch und flink,", "tokens": ["Man", "im\u00b7mer", "frisch", "und", "flink", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam von Borsig einer,", "tokens": ["Da", "kam", "von", "Bor\u00b7sig", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hatte Zaster und Grips.", "tokens": ["Der", "hat\u00b7te", "Zas\u00b7ter", "und", "Grips", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So h\u00fcbsch wie er war keiner", "tokens": ["So", "h\u00fcbsch", "wie", "er", "war", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit seinem roten Schlips.", "tokens": ["Mit", "sei\u00b7nem", "ro\u00b7ten", "Schlips", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er kaufte mir 'nen neuen Hut,", "tokens": ["Er", "kauf\u00b7te", "mir", "'nen", "neu\u00b7en", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer wei\u00df, wie Liebe tut.", "tokens": ["Wer", "wei\u00df", ",", "wie", "Lie\u00b7be", "tut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Berlin, o wie s\u00fc\u00df", "tokens": ["Ber\u00b7lin", ",", "o", "wie", "s\u00fc\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ist dein Paradies.", "tokens": ["Ist", "dein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Unsere Vaterstadt", "tokens": ["Un\u00b7se\u00b7re", "Va\u00b7ter\u00b7stadt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Schneidige M\u00e4dchen hat.", "tokens": ["Schnei\u00b7di\u00b7ge", "M\u00e4d\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Schwamm dr\u00fcber. Tralala.", "tokens": ["Schwamm", "dr\u00fc\u00b7ber", ".", "Tra\u00b7la\u00b7la", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PAV", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich immer mit'n mit.", "tokens": ["Ich", "im\u00b7mer", "mit'n", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da ging der Kerl versch\u00fctt.", "tokens": ["Da", "ging", "der", "Kerl", "ver\u00b7sch\u00fctt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich im achten schwanger,", "tokens": ["Als", "ich", "im", "ach\u00b7ten", "schwan\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Nachts bei Wind und Sturm,", "tokens": ["Des", "Nachts", "bei", "Wind", "und", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schleppt ich mich auf'n Anger,", "tokens": ["Schleppt", "ich", "mich", "auf'n", "An\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vergrub das arme Wurm.", "tokens": ["Ver\u00b7grub", "das", "ar\u00b7me", "Wurm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es schrie mein Herz, es brannte mein Blut,", "tokens": ["Es", "schrie", "mein", "Herz", ",", "es", "brann\u00b7te", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wer wei\u00df, wie Liebe tut.", "tokens": ["Wer", "wei\u00df", ",", "wie", "Lie\u00b7be", "tut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Berlin, o wie s\u00fc\u00df", "tokens": ["Ber\u00b7lin", ",", "o", "wie", "s\u00fc\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ist dein Paradies,", "tokens": ["Ist", "dein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Unsere Vaterstadt", "tokens": ["Un\u00b7se\u00b7re", "Va\u00b7ter\u00b7stadt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Schneidige M\u00e4dchen hat,", "tokens": ["Schnei\u00b7di\u00b7ge", "M\u00e4d\u00b7chen", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Schwamm dr\u00fcber. Tralala.", "tokens": ["Schwamm", "dr\u00fc\u00b7ber", ".", "Tra\u00b7la\u00b7la", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PAV", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt schieb ich auf'n Strich.", "tokens": ["Jetzt", "schieb", "ich", "auf'n", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich hab'nen Ludewich.", "tokens": ["Ich", "hab'\u00b7nen", "Lu\u00b7de\u00b7wich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In einem gr\u00fcnen Wagen", "tokens": ["In", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Nachts um halber zwee,", "tokens": ["Des", "Nachts", "um", "hal\u00b7ber", "zwee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "APPO", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ha'm sie mich gefahren", "tokens": ["Da", "ha'm", "sie", "mich", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In die Charit\u00e9.", "tokens": ["In", "die", "Cha\u00b7rit\u00e9", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Verwest mein Herz, verfault mein Blut,", "tokens": ["Ver\u00b7west", "mein", "Herz", ",", "ver\u00b7fault", "mein", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer wei\u00df, wie Liebe tut.", "tokens": ["Wer", "wei\u00df", ",", "wie", "Lie\u00b7be", "tut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Berlin, o wie s\u00fc\u00df", "tokens": ["Ber\u00b7lin", ",", "o", "wie", "s\u00fc\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ist dein Paradies.", "tokens": ["Ist", "dein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Unsere Vaterstadt", "tokens": ["Un\u00b7se\u00b7re", "Va\u00b7ter\u00b7stadt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Schneidige M\u00e4dchen hat,", "tokens": ["Schnei\u00b7di\u00b7ge", "M\u00e4d\u00b7chen", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Schwamm dr\u00fcber. Tralala.", "tokens": ["Schwamm", "dr\u00fc\u00b7ber", ".", "Tra\u00b7la\u00b7la", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PAV", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Krank bin ich allemal.", "tokens": ["Krank", "bin", "ich", "al\u00b7le\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es ist mir allens ejal.", "tokens": ["Es", "ist", "mir", "al\u00b7lens", "e\u00b7jal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Weinstock, der tr\u00e4gt Reben,", "tokens": ["Der", "Wein\u00b7stock", ",", "der", "tr\u00e4gt", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kommt 'n junger Mann,", "tokens": ["Und", "kommt", "'n", "jun\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schenk' ihm was f\u00fcr's Leben,", "tokens": ["Ich", "schenk'", "ihm", "was", "f\u00fcr's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df er an mich denken kann.", "tokens": ["Da\u00df", "er", "an", "mich", "den\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Quecksilber und Absud,", "tokens": ["Queck\u00b7sil\u00b7ber", "und", "Ab\u00b7sud", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wer wei\u00df, wie Liebe tut.", "tokens": ["Wer", "wei\u00df", ",", "wie", "Lie\u00b7be", "tut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Berlin, o wie s\u00fc\u00df", "tokens": ["Ber\u00b7lin", ",", "o", "wie", "s\u00fc\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ist dein Paradies.", "tokens": ["Ist", "dein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Unsere Vaterstadt", "tokens": ["Un\u00b7se\u00b7re", "Va\u00b7ter\u00b7stadt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Schneidige M\u00e4dchen hat.", "tokens": ["Schnei\u00b7di\u00b7ge", "M\u00e4d\u00b7chen", "hat", "."], "token