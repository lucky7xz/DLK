{"textgrid.poem.60613": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Grille musizierte", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Grille musizierte", "tokens": ["Die", "Gril\u00b7le", "mu\u00b7si\u00b7zier\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ganze Sommerzeit \u2013", "tokens": ["Die", "gan\u00b7ze", "Som\u00b7mer\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kam in Not und Leid,", "tokens": ["Und", "kam", "in", "Not", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als nun der Nord regierte.", "tokens": ["Als", "nun", "der", "Nord", "re\u00b7gier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie hatte nicht ein St\u00fcckchen", "tokens": ["Sie", "hat\u00b7te", "nicht", "ein", "St\u00fcck\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von W\u00fcrmchen oder M\u00fcckchen,", "tokens": ["Von", "W\u00fcrm\u00b7chen", "o\u00b7der", "M\u00fcck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Hunger klagend ging sie hin", "tokens": ["Und", "Hun\u00b7ger", "kla\u00b7gend", "ging", "sie", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVPP", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Ameis, ihrer Nachbarin,", "tokens": ["Zur", "Am\u00b7eis", ",", "ih\u00b7rer", "Nach\u00b7ba\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und bat sie voller Sorgen,", "tokens": ["Und", "bat", "sie", "vol\u00b7ler", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ihr etwas Korn zu borgen.", "tokens": ["Ihr", "et\u00b7was", "Korn", "zu", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "\u00bbmir bangt um meine Existenz,\u00ab", "tokens": ["\u00bb", "mir", "bangt", "um", "mei\u00b7ne", "E\u00b7xis\u00b7tenz", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So sprach sie; \u00bbkommt der neue Lenz,", "tokens": ["So", "sprach", "sie", ";", "\u00bb", "kommt", "der", "neu\u00b7e", "Lenz", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dann zahl ich alles dir zur\u00fcck", "tokens": ["Dann", "zahl", "ich", "al\u00b7les", "dir", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und f\u00fcge noch ein gutes St\u00fcck", "tokens": ["Und", "f\u00fc\u00b7ge", "noch", "ein", "gu\u00b7tes", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als Zinsen bei.\u00ab Die Ameis leiht", "tokens": ["Als", "Zin\u00b7sen", "bei", ".", "\u00ab", "Die", "Am\u00b7eis", "leiht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht gern; sie liebt die Sparsamkeit.", "tokens": ["Nicht", "gern", ";", "sie", "liebt", "die", "Spar\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sie sagte zu der Borgerin:", "tokens": ["Sie", "sag\u00b7te", "zu", "der", "Bor\u00b7ge\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbwie brachtest du den Sommer hin?\u00ab", "tokens": ["\u00bb", "wie", "brach\u00b7test", "du", "den", "Som\u00b7mer", "hin", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbich habe Tag und Nacht", "tokens": ["\u00bb", "ich", "ha\u00b7be", "Tag", "und", "Nacht"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Mit Singen mich erg\u00f6tzt.\u00ab", "tokens": ["Mit", "Sin\u00b7gen", "mich", "er\u00b7g\u00f6tzt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "\u00bbdu hast Musik gemacht?", "tokens": ["\u00bb", "du", "hast", "Mu\u00b7sik", "ge\u00b7macht", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Wie h\u00fcbsch! So tanze jetzt!\u00ab", "tokens": ["Wie", "h\u00fcbsch", "!", "So", "tan\u00b7ze", "jetzt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "$.", "ADV", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}