{"textgrid.poem.31636": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "8. Der im Himmel sitzt / lachet ihr / und der Herr spottet ihr /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der den Schickungs-Zaum im H\u00e4nden hat / und wendet wie er will /", "tokens": ["Der", "den", "Schi\u00b7ckungs\u00b7Zaum", "im", "H\u00e4n\u00b7den", "hat", "/", "und", "wen\u00b7det", "wie", "er", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VAFIN", "$(", "KON", "VVFIN", "KOKOM", "PPER", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "lacht der tollen Welt-Beginnen / h\u00e4lt ihr' Anschl\u00e4g vor ein Spiel.", "tokens": ["lacht", "der", "tol\u00b7len", "Welt\u00b7Be\u00b7gin\u00b7nen", "/", "h\u00e4lt", "ih\u00b7r'", "An\u00b7schl\u00e4g", "vor", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Wie der Kinder Karten-Bau / so ist jener Hochmuhts Prallen:", "tokens": ["Wie", "der", "Kin\u00b7der", "Kar\u00b7ten\u00b7Bau", "/", "so", "ist", "je\u00b7ner", "Hoch\u00b7muhts", "Pral\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$(", "ADV", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "bau man es / so hoch man will; bl\u00e4st er drein / so mu\u00df es fallen.", "tokens": ["bau", "man", "es", "/", "so", "hoch", "man", "will", ";", "bl\u00e4st", "er", "drein", "/", "so", "mu\u00df", "es", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "$(", "ADV", "ADJD", "PIS", "VMFIN", "$.", "VVFIN", "PPER", "PTKVZ", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Gott hat noch wol ein Aegypten / da\u00df er Joseph mit ergetzt /", "tokens": ["Gott", "hat", "noch", "wol", "ein", "A\u00b7e\u00b7gyp\u00b7ten", "/", "da\u00df", "er", "Jo\u00b7se\u00b7ph", "mit", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "NE", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "und der Br\u00fcder Bnterdruckung in erhabne Ehr versetzt.", "tokens": ["und", "der", "Br\u00fc\u00b7der", "Bn\u00b7ter\u00b7dru\u00b7ckung", "in", "er\u00b7hab\u00b7ne", "Ehr", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}