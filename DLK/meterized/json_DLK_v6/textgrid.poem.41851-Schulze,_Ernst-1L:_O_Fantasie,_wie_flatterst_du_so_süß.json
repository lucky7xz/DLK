{"textgrid.poem.41851": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Fantasie, wie flatterst du so s\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fantasie, wie flatterst du so s\u00fc\u00df", "tokens": ["O", "Fan\u00b7ta\u00b7sie", ",", "wie", "flat\u00b7terst", "du", "so", "s\u00fc\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um meine Brust mit leisen Fl\u00fcgelschl\u00e4gen", "tokens": ["Um", "mei\u00b7ne", "Brust", "mit", "lei\u00b7sen", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und suchst mir rings ein holdes Paradies", "tokens": ["Und", "suchst", "mir", "rings", "ein", "hol\u00b7des", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll freundlicher Gestalten aufzuregen?", "tokens": ["Voll", "freund\u00b7li\u00b7cher", "Ge\u00b7stal\u00b7ten", "auf\u00b7zu\u00b7re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es wiegt mich hin zu mondbegl\u00e4nzten H\u00f6hn,", "tokens": ["Es", "wiegt", "mich", "hin", "zu", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es leitet mich zum Rande frischer Quellen,", "tokens": ["Es", "lei\u00b7tet", "mich", "zum", "Ran\u00b7de", "fri\u00b7scher", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ums\u00e4uselt mich wie duft'ger Schatten Wehn", "tokens": ["Um\u00b7s\u00e4u\u00b7selt", "mich", "wie", "duft'\u00b7ger", "Schat\u00b7ten", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bettet mich auf weiche Graseswellen.", "tokens": ["Und", "bet\u00b7tet", "mich", "auf", "wei\u00b7che", "Gra\u00b7ses\u00b7wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch fa\u00df' ich's nicht, was mir dein Wink enth\u00fcllt;", "tokens": ["Noch", "fa\u00df'", "ich's", "nicht", ",", "was", "mir", "dein", "Wink", "ent\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch d\u00e4mmernd bl\u00fcht's empor in buntem Leben,", "tokens": ["Doch", "d\u00e4m\u00b7mernd", "bl\u00fcht's", "em\u00b7por", "in", "bun\u00b7tem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4chelnd scheint schon manches liebe Bild", "tokens": ["Und", "l\u00e4\u00b7chelnd", "scheint", "schon", "man\u00b7ches", "lie\u00b7be", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem irren Kampf der Formen zu entschweben.", "tokens": ["Dem", "ir\u00b7ren", "Kampf", "der", "For\u00b7men", "zu", "ent\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Herz ergl\u00fcht in ahnungsvoller Lust,", "tokens": ["Mein", "Herz", "er\u00b7gl\u00fcht", "in", "ah\u00b7nungs\u00b7vol\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "S\u00fc\u00df bebt's in mir gleich stillem Liebeszagen,", "tokens": ["S\u00fc\u00df", "bebt's", "in", "mir", "gleich", "stil\u00b7lem", "Lie\u00b7bes\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Morgenroth umspielt es meine Brust;", "tokens": ["Wie", "Mor\u00b7gen\u00b7roth", "um\u00b7spielt", "es", "mei\u00b7ne", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch will es nie zur lichten Klarheit tagen.", "tokens": ["Doch", "will", "es", "nie", "zur", "lich\u00b7ten", "Klar\u00b7heit", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O la\u00df dich sanft, du holde Gauklerin,", "tokens": ["O", "la\u00df", "dich", "sanft", ",", "du", "hol\u00b7de", "Gauk\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Rosensaum der leichten Schwingen halten,", "tokens": ["Am", "Ro\u00b7sen\u00b7saum", "der", "leich\u00b7ten", "Schwin\u00b7gen", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deute mir den wunderbaren Sinn", "tokens": ["Und", "deu\u00b7te", "mir", "den", "wun\u00b7der\u00b7ba\u00b7ren", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der nahenden, der fliehenden Gestalten!", "tokens": ["Der", "na\u00b7hen\u00b7den", ",", "der", "flie\u00b7hen\u00b7den", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und sieh! da ruht das K\u00e4mpfende vereint,", "tokens": ["Und", "sieh", "!", "da", "ruht", "das", "K\u00e4mp\u00b7fen\u00b7de", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Der irre Duft der D\u00e4mmrung ist entschwunden,", "tokens": ["Der", "ir\u00b7re", "Duft", "der", "D\u00e4mm\u00b7rung", "ist", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gleich dem Strahl des fr\u00fchsten Lichts erscheint", "tokens": ["Und", "gleich", "dem", "Strahl", "des", "fr\u00fchs\u00b7ten", "Lichts", "er\u00b7scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie, die mit ew'gem Zauber mich umwunden.", "tokens": ["Sie", ",", "die", "mit", "ew'\u00b7gem", "Zau\u00b7ber", "mich", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O sey gegr\u00fc\u00dft, du zartes Traumgesicht,", "tokens": ["O", "sey", "ge\u00b7gr\u00fc\u00dft", ",", "du", "zar\u00b7tes", "Traum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie lieblich weht dein luftiges Gefieder,", "tokens": ["Wie", "lieb\u00b7lich", "weht", "dein", "luf\u00b7ti\u00b7ges", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie senkst du hold, ein freundliches Gedicht,", "tokens": ["Wie", "senkst", "du", "hold", ",", "ein", "freund\u00b7li\u00b7ches", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In's \u00f6de Reich der Wahrheit dich hernieder!", "tokens": ["In's", "\u00f6\u00b7de", "Reich", "der", "Wahr\u00b7heit", "dich", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dein L\u00e4cheln ist aus Sonnenschein gewebt,", "tokens": ["Dein", "L\u00e4\u00b7cheln", "ist", "aus", "Son\u00b7nen\u00b7schein", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein milder Ernst aus stillem Mondenglanze,", "tokens": ["Dein", "mil\u00b7der", "Ernst", "aus", "stil\u00b7lem", "Mon\u00b7den\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lust ist das Kleid, das rosig dich umschwebt,", "tokens": ["Lust", "ist", "das", "Kleid", ",", "das", "ro\u00b7sig", "dich", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Ruhe thaut aus deinem duft'gen Kranze.", "tokens": ["Und", "Ru\u00b7he", "thaut", "aus", "dei\u00b7nem", "duft'\u00b7gen", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich nahe dir, du friedliche Gestalt,", "tokens": ["Ich", "na\u00b7he", "dir", ",", "du", "fried\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich hasche dich mit seligem Verlangen", "tokens": ["Ich", "ha\u00b7sche", "dich", "mit", "se\u00b7li\u00b7gem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und halte sanft mit liebender Gewalt", "tokens": ["Und", "hal\u00b7te", "sanft", "mit", "lie\u00b7ben\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An keuscher Brust dich, s\u00fc\u00dfen Traum, umfangen!", "tokens": ["An", "keu\u00b7scher", "Brust", "dich", ",", "s\u00fc\u00b7\u00dfen", "Traum", ",", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "O weile du bey mir im Schattengr\u00fcn,", "tokens": ["O", "wei\u00b7le", "du", "bey", "mir", "im", "Schat\u00b7ten\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df fr\u00f6hlich uns mit luft'gen Bildern spielen,", "tokens": ["La\u00df", "fr\u00f6h\u00b7lich", "uns", "mit", "luft'\u00b7gen", "Bil\u00b7dern", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Befl\u00fcgle mich mit bunten Fantasien.", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "mich", "mit", "bun\u00b7ten", "Fan\u00b7ta\u00b7si\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und kette mich mit heiligen Gef\u00fchlen.", "tokens": ["Und", "ket\u00b7te", "mich", "mit", "hei\u00b7li\u00b7gen", "Ge\u00b7f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Fern sey von uns der Welt verworrner Streit,", "tokens": ["Fern", "sey", "von", "uns", "der", "Welt", "ver\u00b7worr\u00b7ner", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df tr\u00e4umend uns in stillen Lauben wohnen!", "tokens": ["La\u00df", "tr\u00e4u\u00b7mend", "uns", "in", "stil\u00b7len", "Lau\u00b7ben", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVPP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verg\u00e4nglich ist, was uns das Leben beut,", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", "ist", ",", "was", "uns", "das", "Le\u00b7ben", "beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz nur flicht sich ew'ge Bl\u00fcthenkronen.", "tokens": ["Das", "Herz", "nur", "flicht", "sich", "ew'\u00b7ge", "Bl\u00fc\u00b7then\u00b7kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "L\u00e4ngst schwand die Sonn' an meines Himmels Saum,", "tokens": ["L\u00e4ngst", "schwand", "die", "Sonn'", "an", "mei\u00b7nes", "Him\u00b7mels", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erloschen ist mein Leben und mein Lieben,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "mein", "Le\u00b7ben", "und", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du nur allein, du, meiner Liebe Traum,", "tokens": ["Du", "nur", "al\u00b7lein", ",", "du", ",", "mei\u00b7ner", "Lie\u00b7be", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bist tr\u00f6stend mir in kalter Nacht geblieben!", "tokens": ["Bist", "tr\u00f6s\u00b7tend", "mir", "in", "kal\u00b7ter", "Nacht", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}