{"textgrid.poem.30213": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umsonst ist keiner gerne from; wann Tugend nur was tr\u00e4gt,", "tokens": ["Um\u00b7sonst", "ist", "kei\u00b7ner", "ger\u00b7ne", "from", ";", "wann", "Tu\u00b7gend", "nur", "was", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "$.", "PWAV", "NN", "ADV", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "So wird sie, weil sie Fr\u00fcchte bringt, geachtet und gepflegt.", "tokens": ["So", "wird", "sie", ",", "weil", "sie", "Fr\u00fcch\u00b7te", "bringt", ",", "ge\u00b7ach\u00b7tet", "und", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}