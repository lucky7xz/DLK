{"textgrid.poem.59764": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das Leben auch rollt,", "tokens": ["Wie", "das", "Le\u00b7ben", "auch", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ob kreuz oder quer,", "tokens": ["Ob", "kreuz", "o\u00b7der", "quer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was voll du gewollt,", "tokens": ["Was", "voll", "du", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VMPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das streu' nicht umher:", "tokens": ["Das", "streu'", "nicht", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Denn was viele gewu\u00dft,", "tokens": ["Denn", "was", "vie\u00b7le", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Zersplittert sich gleich,", "tokens": ["Zer\u00b7split\u00b7tert", "sich", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "An Macht und an Lust", "tokens": ["An", "Macht", "und", "an", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ist der Stille nur reich.", "tokens": ["Ist", "der", "Stil\u00b7le", "nur", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}