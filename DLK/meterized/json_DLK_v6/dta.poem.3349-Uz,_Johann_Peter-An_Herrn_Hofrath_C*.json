{"dta.poem.3349": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schleiche mich aus einem Hayn,", "tokens": ["Ich", "schlei\u00b7che", "mich", "aus", "ei\u00b7nem", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Myrthen unter Lorbeern rauschen,", "tokens": ["Wo", "Myr\u00b7then", "un\u00b7ter", "Lor\u00b7beern", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Liebesgott und Satyr lauschen,", "tokens": ["Und", "Lie\u00b7bes\u00b7gott", "und", "Sa\u00b7tyr", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einen lichten Tempel ein.", "tokens": ["In", "ei\u00b7nen", "lich\u00b7ten", "Tem\u00b7pel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Musen lachen mir entgegen:", "tokens": ["Die", "Mu\u00b7sen", "la\u00b7chen", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Marmor nachgeahmt, scheint iede sich zu regen,", "tokens": ["In", "Mar\u00b7mor", "nach\u00b7ge\u00b7ahmt", ",", "scheint", "ie\u00b7de", "sich", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PIAT", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mehr, als blo\u00dfer Stein, zu seyn.", "tokens": ["Und", "mehr", ",", "als", "blo\u00b7\u00dfer", "Stein", ",", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADJA", "NN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der weise Marmor scheint beseelet:", "tokens": ["Der", "wei\u00b7se", "Mar\u00b7mor", "scheint", "be\u00b7see\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von keinem neidischen Gewand", "tokens": ["Von", "kei\u00b7nem", "nei\u00b7di\u00b7schen", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wird auch der kleinste Reiz verheelet;", "tokens": ["Wird", "auch", "der", "kleins\u00b7te", "Reiz", "ver\u00b7hee\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und weder sch\u00f6nes Maa\u00df, noch jenes Weiche fehlet,", "tokens": ["Und", "we\u00b7der", "sch\u00f6\u00b7nes", "Maa\u00df", ",", "noch", "je\u00b7nes", "Wei\u00b7che", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ADJA", "NN", "$,", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das alter Griechen leichte Hand,", "tokens": ["Das", "al\u00b7ter", "Grie\u00b7chen", "leich\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von Grazien gef\u00fchrt, mit hartem Stein verband.", "tokens": ["Von", "Gra\u00b7zi\u00b7en", "ge\u00b7f\u00fchrt", ",", "mit", "har\u00b7tem", "Stein", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In Marmor stehn an ihren Seiten", "tokens": ["In", "Mar\u00b7mor", "stehn", "an", "ih\u00b7ren", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die Dichter neuer Zeit, bey Dichtern alter Zeiten:", "tokens": ["Die", "Dich\u00b7ter", "neu\u00b7er", "Zeit", ",", "bey", "Dich\u00b7tern", "al\u00b7ter", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da Lieblichkeit am Griechen lacht,", "tokens": ["Da", "Lieb\u00b7lich\u00b7keit", "am", "Grie\u00b7chen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Ernst voll Majest\u00e4t den R\u00f6mer kenntlich macht,", "tokens": ["Ein", "Ernst", "voll", "Ma\u00b7jes\u00b7t\u00e4t", "den", "R\u00f6\u00b7mer", "kennt\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und manche H\u00e4rte noch und wildere Geberden", "tokens": ["Und", "man\u00b7che", "H\u00e4r\u00b7te", "noch", "und", "wil\u00b7de\u00b7re", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In iedem Bild entdecket werden,", "tokens": ["In", "ie\u00b7dem", "Bild", "ent\u00b7de\u00b7cket", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das j\u00fcngre Kunst hervor gebracht.", "tokens": ["Das", "j\u00fcng\u00b7re", "Kunst", "her\u00b7vor", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mein Auge s\u00e4umt bey iedem St\u00fccke;", "tokens": ["Mein", "Au\u00b7ge", "s\u00e4umt", "bey", "ie\u00b7dem", "St\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Doch Pindar fesselt meine Blicke.", "tokens": ["Doch", "Pin\u00b7dar", "fes\u00b7selt", "mei\u00b7ne", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Sein stolzes Auge rollt, voll ungest\u00fcmer Glut,", "tokens": ["Sein", "stol\u00b7zes", "Au\u00b7ge", "rollt", ",", "voll", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Voll heilger Wuth.", "tokens": ["Voll", "heil\u00b7ger", "Wuth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.25": {"text": "Dem k\u00fchnen Griechen gegen \u00fcber", "tokens": ["Dem", "k\u00fch\u00b7nen", "Grie\u00b7chen", "ge\u00b7gen", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Steht Flaccus, dessen Blick satirisch l\u00e4chelnd blitzt:", "tokens": ["Steht", "Flac\u00b7cus", ",", "des\u00b7sen", "Blick", "sa\u00b7ti\u00b7risch", "l\u00e4\u00b7chelnd", "blitzt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PRELAT", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er singt, von sanftern Gott erhitzt,", "tokens": ["Er", "singt", ",", "von", "sanf\u00b7tern", "Gott", "er\u00b7hitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und ohne Z\u00fcckung, ohne Fieber.", "tokens": ["Und", "oh\u00b7ne", "Z\u00fc\u00b7ckung", ",", "oh\u00b7ne", "Fie\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Oft nachgeahmt und nie erreicht,", "tokens": ["Oft", "nach\u00b7ge\u00b7ahmt", "und", "nie", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Hebt sein gefl\u00fcgelt Lied sich pr\u00e4chtig, hoch, doch leicht.", "tokens": ["Hebt", "sein", "ge\u00b7fl\u00fc\u00b7gelt", "Lied", "sich", "pr\u00e4ch\u00b7tig", ",", "hoch", ",", "doch", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVPP", "NN", "PRF", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}