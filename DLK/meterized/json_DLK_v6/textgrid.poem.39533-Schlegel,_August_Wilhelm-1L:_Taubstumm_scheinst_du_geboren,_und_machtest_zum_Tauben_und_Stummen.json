{"textgrid.poem.39533": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Taubstumm scheinst du geboren, und machtest zum Tauben und Stummen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Taubstumm scheinst du geboren, und machtest zum Tauben und Stummen", "tokens": ["Taub\u00b7stumm", "scheinst", "du", "ge\u00b7bo\u00b7ren", ",", "und", "mach\u00b7test", "zum", "Tau\u00b7ben", "und", "Stum\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "$,", "KON", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mich, vor Bewunderung stumm, taub bei der Anderen Wort.", "tokens": ["Mich", ",", "vor", "Be\u00b7wun\u00b7de\u00b7rung", "stumm", ",", "taub", "bei", "der", "An\u00b7de\u00b7ren", "Wort", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "ADJD", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "H\u00e4tte dir auch das beseelte Gespr\u00e4ch und die liebliche Stimme", "tokens": ["H\u00e4t\u00b7te", "dir", "auch", "das", "be\u00b7seel\u00b7te", "Ge\u00b7spr\u00e4ch", "und", "die", "lieb\u00b7li\u00b7che", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Streng die Natur versagt, dennoch entz\u00fccktest du so.", "tokens": ["Streng", "die", "Na\u00b7tur", "ver\u00b7sagt", ",", "den\u00b7noch", "ent\u00b7z\u00fcck\u00b7test", "du", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "O, wie wollt' ich alsdann mich bem\u00fchn, dich Zeichen zu lehren,", "tokens": ["O", ",", "wie", "wollt'", "ich", "als\u00b7dann", "mich", "be\u00b7m\u00fchn", ",", "dich", "Zei\u00b7chen", "zu", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Kund zu geben das Herz, Regungen zarten Gef\u00fchls!", "tokens": ["Kund", "zu", "ge\u00b7ben", "das", "Herz", ",", "Re\u00b7gun\u00b7gen", "zar\u00b7ten", "Ge\u00b7f\u00fchls", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "O, wie wollt' ich mit Eifer der Blicke Beredsamkeit \u00fcben,", "tokens": ["O", ",", "wie", "wollt'", "ich", "mit", "Ei\u00b7fer", "der", "Bli\u00b7cke", "Be\u00b7red\u00b7sam\u00b7keit", "\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Bis ich verstanden mich s\u00e4h' an dem erwiedernden Blick!", "tokens": ["Bis", "ich", "ver\u00b7stan\u00b7den", "mich", "s\u00e4h'", "an", "dem", "er\u00b7wie\u00b7dern\u00b7den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Doch die h\u00f6ren nicht wollen, das sind die unheilbarsten Tauben,", "tokens": ["Doch", "die", "h\u00f6\u00b7ren", "nicht", "wol\u00b7len", ",", "das", "sind", "die", "un\u00b7heil\u00b7bars\u00b7ten", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PTKNEG", "VMFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-++-+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Sagt man, und also vielleicht h\u00fclfe die Lehre da nicht.", "tokens": ["Sagt", "man", ",", "und", "al\u00b7so", "viel\u00b7leicht", "h\u00fcl\u00b7fe", "die", "Leh\u00b7re", "da", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KON", "ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Bist du doch taub wie das Schicksal, und stumm wie den Fragen die Zukunft,", "tokens": ["Bist", "du", "doch", "taub", "wie", "das", "Schick\u00b7sal", ",", "und", "stumm", "wie", "den", "Fra\u00b7gen", "die", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Redend und h\u00f6rend ja schon: w\u00e4rst du nun stumm noch und taub!", "tokens": ["Re\u00b7dend", "und", "h\u00f6\u00b7rend", "ja", "schon", ":", "w\u00e4rst", "du", "nun", "stumm", "noch", "und", "taub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADV", "ADV", "$.", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "+--+-+-+-++--+", "measure": "iambic.septa.invert"}, "line.13": {"text": "Z\u00fcrnst du schweigend mir jetzt, so wirst du der t\u00fcrkische Stumme,", "tokens": ["Z\u00fcrnst", "du", "schwei\u00b7gend", "mir", "jetzt", ",", "so", "wirst", "du", "der", "t\u00fcr\u00b7ki\u00b7sche", "Stum\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Der den erw\u00fcrgenden Strick, ach dem Verdammten, mir reicht.", "tokens": ["Der", "den", "er\u00b7w\u00fcr\u00b7gen\u00b7den", "Strick", ",", "ach", "dem", "Ver\u00b7damm\u00b7ten", ",", "mir", "reicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}