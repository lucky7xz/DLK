{"textgrid.poem.51122": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ufenau", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier unter diesem Rasengr\u00fcn,", "tokens": ["Hier", "un\u00b7ter", "die\u00b7sem", "Ra\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wir in Jugend stehn,", "tokens": ["Wo", "wir", "in", "Ju\u00b7gend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da liegt ein Ritter frei und k\u00fchn,", "tokens": ["Da", "liegt", "ein", "Rit\u00b7ter", "frei", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie keiner mehr zu sehn!", "tokens": ["Wie", "kei\u00b7ner", "mehr", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er floh herein vom R\u00f6m'schen Reich,", "tokens": ["Er", "floh", "her\u00b7ein", "vom", "R\u00f6m'\u00b7schen", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trug einen Lorbeerkranz,", "tokens": ["Trug", "ei\u00b7nen", "Lor\u00b7beer\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Antlitz zorn- und kummerbleich,", "tokens": ["Das", "Ant\u00b7litz", "zorn", "und", "kum\u00b7mer\u00b7bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Aug voll Sonnenglanz!", "tokens": ["Das", "Aug", "voll", "Son\u00b7nen\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wo die Well' den Blumenstrand", "tokens": ["Und", "wo", "die", "Well'", "den", "Blu\u00b7men\u00b7strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In holder Minne k\u00fc\u00dft,", "tokens": ["In", "hol\u00b7der", "Min\u00b7ne", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warf er sein Schwert auf sichres Land", "tokens": ["Warf", "er", "sein", "Schwert", "auf", "sich\u00b7res", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und rief: Sei mir gegr\u00fc\u00dft!", "tokens": ["Und", "rief", ":", "Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In schwerer Not sank er dahin,", "tokens": ["In", "schwe\u00b7rer", "Not", "sank", "er", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zerbrochen das Gebein;", "tokens": ["Zer\u00b7bro\u00b7chen", "das", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch gl\u00fchte noch sein starker Sinn", "tokens": ["Doch", "gl\u00fch\u00b7te", "noch", "sein", "star\u00b7ker", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Tod wie junger Wein.", "tokens": ["Im", "Tod", "wie", "jun\u00b7ger", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun weht sein Schatten um uns her,", "tokens": ["Nun", "weht", "sein", "Schat\u00b7ten", "um", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ruft sein Geist uns zu:", "tokens": ["Nun", "ruft", "sein", "Geist", "uns", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbich war ein Schiff auf wildem Meer,", "tokens": ["\u00bb", "ich", "war", "ein", "Schiff", "auf", "wil\u00b7dem", "Meer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kannte keine Ruh;", "tokens": ["Ich", "kann\u00b7te", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr wi\u00dft, was ich gestritten hab", "tokens": ["Ihr", "wi\u00dft", ",", "was", "ich", "ge\u00b7strit\u00b7ten", "hab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was gelitten auch;", "tokens": ["Und", "was", "ge\u00b7lit\u00b7ten", "auch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch stieg' ich nochmals aus dem Grab,", "tokens": ["Doch", "stieg'", "ich", "noch\u00b7mals", "aus", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00dcbt' ich den gleichen Brauch!", "tokens": ["\u00dcbt'", "ich", "den", "glei\u00b7chen", "Brauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Qual verfliegt, die Sorg ist klein,", "tokens": ["Die", "Qual", "ver\u00b7fliegt", ",", "die", "Sorg", "ist", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun bin ich unbeschwert;", "tokens": ["Nun", "bin", "ich", "un\u00b7be\u00b7schwert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die besten Freunde nannt ich mein", "tokens": ["Die", "bes\u00b7ten", "Freun\u00b7de", "nannt", "ich", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fand mich ihrer wert!", "tokens": ["Und", "fand", "mich", "ih\u00b7rer", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr lieben Br\u00fcder, wagt es nur", "tokens": ["Ihr", "lie\u00b7ben", "Br\u00fc\u00b7der", ",", "wagt", "es", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und acht't die Not gering!", "tokens": ["Und", "acht't", "die", "Not", "ge\u00b7ring", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Elend zeigt die goldne Spur,", "tokens": ["Das", "E\u00b7lend", "zeigt", "die", "gold\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo sich ein Held erging!\u00ab", "tokens": ["Wo", "sich", "ein", "Held", "er\u00b7ging", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du lichter Schatten, habe Dank!", "tokens": ["Du", "lich\u00b7ter", "Schat\u00b7ten", ",", "ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gut sprach dein k\u00fchner Mund!", "tokens": ["Gut", "sprach", "dein", "k\u00fch\u00b7ner", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und wem der Sinn von Zweifel krank,", "tokens": ["Und", "wem", "der", "Sinn", "von", "Zwei\u00b7fel", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird an dir gesund!", "tokens": ["Der", "wird", "an", "dir", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie diese lustige Silberflut", "tokens": ["Wie", "die\u00b7se", "lus\u00b7ti\u00b7ge", "Sil\u00b7ber\u00b7flut"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein Grab so hell umflie\u00dft,", "tokens": ["Dein", "Grab", "so", "hell", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So uns dein nie geschwundner Mut", "tokens": ["So", "uns", "dein", "nie", "ge\u00b7schwund\u00b7ner", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das frohe Herz erschlie\u00dft!", "tokens": ["Das", "fro\u00b7he", "Herz", "er\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}