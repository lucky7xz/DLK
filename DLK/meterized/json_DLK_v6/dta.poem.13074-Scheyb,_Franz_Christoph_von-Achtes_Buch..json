{"dta.poem.13074": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr langsamer Betrag gab allen abzunehmen,", "tokens": ["Ihr", "lang\u00b7sa\u00b7mer", "Be\u00b7trag", "gab", "al\u00b7len", "ab\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "20Da\u00df sie sich, vor dem Rath frey darzustellen, sch\u00e4men.", "tokens": ["sie", "sich", ",", "vor", "dem", "Rath", "frey", "dar\u00b7zu\u00b7stel\u00b7len", ",", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "$,", "APPR", "ART", "NN", "ADJD", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "So sprach der gute Grei\u00df: \u201eWarum dann so verzagt?", "tokens": ["So", "sprach", "der", "gu\u00b7te", "Grei\u00df", ":", "\u201e", "Wa\u00b7rum", "dann", "so", "ver\u00b7zagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "PWAV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewas ists, wor\u00fcber ihr mit solcher Schwermuth klagt?", "tokens": ["\u201e", "was", "ists", ",", "wo\u00b7r\u00fc\u00b7ber", "ihr", "mit", "sol\u00b7cher", "Schwer\u00b7muth", "klagt", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$,", "PWAV", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eje mehr man euerm Wiz sonst pfleget aufzub\u00fcrden,", "tokens": ["\u201e", "je", "mehr", "man", "eu\u00b7erm", "Wiz", "sonst", "pfle\u00b7get", "auf\u00b7zu\u00b7b\u00fcr\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIS", "PPOSAT", "NN", "ADV", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eje mehr belohnt man euch mit Ehren und mit W\u00fcrden.", "tokens": ["\u201e", "je", "mehr", "be\u00b7lohnt", "man", "euch", "mit", "Eh\u00b7ren", "und", "mit", "W\u00fcr\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "25\u201dAuf auf dann! seht ihr nicht, was diesem Tugend-Saal", "tokens": ["\"", "Auf", "auf", "dann", "!", "seht", "ihr", "nicht", ",", "was", "die\u00b7sem", "Tu\u00b7gen\u00b7dSaal"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "APPR", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ebey dieser Freuden-Zeit der Eifer anbefahl?", "tokens": ["\u201e", "bey", "die\u00b7ser", "Freu\u00b7den\u00b7Zeit", "der", "Ei\u00b7fer", "an\u00b7be\u00b7fahl", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eseyd ihr allein so tr\u00e4g? ihr k\u00f6nnt vernommen haben,", "tokens": ["\u201e", "seyd", "ihr", "al\u00b7lein", "so", "tr\u00e4g", "?", "ihr", "k\u00f6nnt", "ver\u00b7nom\u00b7men", "ha\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "PPER", "VVFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewas wir vor eine Sach ins Werck zu sezen gaben.", "tokens": ["\u201e", "was", "wir", "vor", "ei\u00b7ne", "Sach", "ins", "Werck", "zu", "se\u00b7zen", "ga\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201efa\u00dft Muth! erkl\u00e4ret euch. Hier sprach die ", "tokens": ["\u201e", "fa\u00dft", "Muth", "!", "er\u00b7kl\u00e4\u00b7ret", "euch", ".", "Hier", "sprach", "die"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "NN", "$.", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "30\u201dW\u00e4r ich in diesem Streit erw\u00e4hlte Richterinn,", "tokens": ["\"", "W\u00e4r", "ich", "in", "die\u00b7sem", "Streit", "er\u00b7w\u00e4hl\u00b7te", "Rich\u00b7te\u00b7rinn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eso w\u00fc\u00dfte man den Schlu\u00df; ihr w\u00e4ret schon zu frieden;", "tokens": ["\u201e", "so", "w\u00fc\u00df\u00b7te", "man", "den", "Schlu\u00df", ";", "ihr", "w\u00e4\u00b7ret", "schon", "zu", "frie\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eund alles f\u00e4nde sich durch meinen Spruch entschieden.", "tokens": ["\u201e", "und", "al\u00b7les", "f\u00e4n\u00b7de", "sich", "durch", "mei\u00b7nen", "Spruch", "ent\u00b7schie\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}