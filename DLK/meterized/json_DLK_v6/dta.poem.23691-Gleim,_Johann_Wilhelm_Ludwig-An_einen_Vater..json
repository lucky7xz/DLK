{"dta.poem.23691": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An einen Vater.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alter, denk an deine Jugend!", "tokens": ["Al\u00b7ter", ",", "denk", "an", "dei\u00b7ne", "Ju\u00b7gend", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchle noch einmal die Wollust,", "tokens": ["F\u00fch\u00b7le", "noch", "ein\u00b7mal", "die", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die du in den Adern f\u00fchltest,", "tokens": ["Die", "du", "in", "den", "A\u00b7dern", "f\u00fchl\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Damals, als du Vater wurdest.", "tokens": ["Da\u00b7mals", ",", "als", "du", "Va\u00b7ter", "wur\u00b7dest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh, hier sitzt auf meinem Schoosse", "tokens": ["Sieh", ",", "hier", "sitzt", "auf", "mei\u00b7nem", "Schoos\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Tochter, die mich liebet!", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", ",", "die", "mich", "lie\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh, sie streichelt meine H\u00e4nde!", "tokens": ["Sieh", ",", "sie", "strei\u00b7chelt", "mei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh, sie zupft mich bei der Nase!", "tokens": ["Sieh", ",", "sie", "zupft", "mich", "bei", "der", "Na\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Sieh, sie kneipt mich in die Wangen!", "tokens": ["Sieh", ",", "sie", "kneipt", "mich", "in", "die", "Wan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sieh, sie h\u00fcpft auf meinem Schoosse!", "tokens": ["Sieh", ",", "sie", "h\u00fcpft", "auf", "mei\u00b7nem", "Schoos\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sieh, sie k\u00fctzelt mich und lachet!", "tokens": ["Sieh", ",", "sie", "k\u00fct\u00b7zelt", "mich", "und", "la\u00b7chet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie vergn\u00fcgt ist deine Tochter!", "tokens": ["Wie", "ver\u00b7gn\u00fcgt", "ist", "dei\u00b7ne", "Toch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Kann sie dich nicht fr\u00f6lich machen?", "tokens": ["Kann", "sie", "dich", "nicht", "fr\u00f6\u00b7lich", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Alter, rufst du nicht mit Tr\u00e4nen,", "tokens": ["Al\u00b7ter", ",", "rufst", "du", "nicht", "mit", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Deine Jugendzeit zur\u00fckke?", "tokens": ["Dei\u00b7ne", "Ju\u00b7gend\u00b7zeit", "zu\u00b7r\u00fck\u00b7ke", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sieh, wie sch\u00f6n wir sie gebrauchen!", "tokens": ["Sieh", ",", "wie", "sch\u00f6n", "wir", "sie", "ge\u00b7brau\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Lobe doch, in dem du trinkest,", "tokens": ["Lo\u00b7be", "doch", ",", "in", "dem", "du", "trin\u00b7kest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Unser T\u00e4ndeln, unsre Jugend!", "tokens": ["Un\u00b7ser", "T\u00e4n\u00b7deln", ",", "uns\u00b7re", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Sieh nur, wie vergn\u00fcgt wir t\u00e4ndeln!", "tokens": ["Sieh", "nur", ",", "wie", "ver\u00b7gn\u00fcgt", "wir", "t\u00e4n\u00b7deln", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Doch du kannst mit dunkeln Augen,", "tokens": ["Doch", "du", "kannst", "mit", "dun\u00b7keln", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Unser T\u00e4ndeln nicht mehr sehen.", "tokens": ["Un\u00b7ser", "T\u00e4n\u00b7deln", "nicht", "mehr", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Warte nur, du solst es f\u00fchlen!", "tokens": ["War\u00b7te", "nur", ",", "du", "solst", "es", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}