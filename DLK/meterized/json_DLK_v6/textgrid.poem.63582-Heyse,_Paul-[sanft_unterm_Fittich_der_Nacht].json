{"textgrid.poem.63582": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[sanft unterm Fittich der Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft unterm Fittich der Nacht", "tokens": ["Sanft", "un\u00b7term", "Fit\u00b7tich", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Schl\u00e4ft nun der hastige Wind.", "tokens": ["Schl\u00e4ft", "nun", "der", "has\u00b7ti\u00b7ge", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Komm! la\u00df uns schweigen und lauschen!", "tokens": ["Komm", "!", "la\u00df", "uns", "schwei\u00b7gen", "und", "lau\u00b7schen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVIMP", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "W\u00e4lder und Str\u00f6me, sie rauschen", "tokens": ["W\u00e4l\u00b7der", "und", "Str\u00f6\u00b7me", ",", "sie", "rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Nur wie im Traum noch gelind.", "tokens": ["Nur", "wie", "im", "Traum", "noch", "ge\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "St\u00fcrme, im Busen entfacht,", "tokens": ["St\u00fcr\u00b7me", ",", "im", "Bu\u00b7sen", "ent\u00b7facht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Zitternd veratmet ihr Chor.", "tokens": ["Zit\u00b7ternd", "ver\u00b7at\u00b7met", "ihr", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ruhiger, ohne Gef\u00e4hrde", "tokens": ["Ru\u00b7hi\u00b7ger", ",", "oh\u00b7ne", "Ge\u00b7f\u00e4hr\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUI", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Brennen auf ewigem Herde", "tokens": ["Bren\u00b7nen", "auf", "e\u00b7wi\u00b7gem", "Her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Flammen der Seele empor.", "tokens": ["Flam\u00b7men", "der", "See\u00b7le", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Folgend der himmlischen Macht", "tokens": ["Fol\u00b7gend", "der", "himm\u00b7li\u00b7schen", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Lodern sie herrlich in eins.", "tokens": ["Lo\u00b7dern", "sie", "herr\u00b7lich", "in", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PIS", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mild wie durch Opferged\u00fcfte", "tokens": ["Mild", "wie", "durch", "Op\u00b7fer\u00b7ge\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Blicken die Sterne der L\u00fcfte", "tokens": ["Bli\u00b7cken", "die", "Ster\u00b7ne", "der", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Niederw\u00e4rts segnenden Scheins.", "tokens": ["Nie\u00b7der\u00b7w\u00e4rts", "seg\u00b7nen\u00b7den", "Scheins", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}