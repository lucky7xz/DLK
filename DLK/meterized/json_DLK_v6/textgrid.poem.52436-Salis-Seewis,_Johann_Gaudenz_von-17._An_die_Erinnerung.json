{"textgrid.poem.52436": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "17. An die Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer Wehmut Gef\u00e4hrtin, Erinn'rung,", "tokens": ["S\u00fc\u00b7\u00dfer", "Weh\u00b7mut", "Ge\u00b7f\u00e4hr\u00b7tin", ",", "Er\u00b7inn'\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn jene die Wimper sinnend senkt,", "tokens": ["Wenn", "je\u00b7ne", "die", "Wim\u00b7per", "sin\u00b7nend", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hebst du deinen Schleier und l\u00e4chelst", "tokens": ["Hebst", "du", "dei\u00b7nen", "Schlei\u00b7er", "und", "l\u00e4\u00b7chelst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mit r\u00fcckw\u00e4rts gewandtem Gesicht.", "tokens": ["Mit", "r\u00fcck\u00b7w\u00e4rts", "ge\u00b7wand\u00b7tem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Still und hehr, wie der schweigende Vollmond", "tokens": ["Still", "und", "hehr", ",", "wie", "der", "schwei\u00b7gen\u00b7de", "Voll\u00b7mond"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die Gr\u00e4ber bescheint, betrachtest du", "tokens": ["Die", "Gr\u00e4\u00b7ber", "be\u00b7scheint", ",", "be\u00b7trach\u00b7test", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Vergang'ne, weilenden Blickes,", "tokens": ["Das", "Ver\u00b7gang'\u00b7ne", ",", "wei\u00b7len\u00b7den", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie Br\u00e4ute des Br\u00e4utigams Bild.", "tokens": ["Wie", "Br\u00e4u\u00b7te", "des", "Br\u00e4u\u00b7ti\u00b7gams", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NE", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Deine d\u00e4mmernden Bilder sind lieblich,", "tokens": ["Dei\u00b7ne", "d\u00e4m\u00b7mern\u00b7den", "Bil\u00b7der", "sind", "lieb\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie tauender Duft im Abendrot!", "tokens": ["Wie", "tau\u00b7en\u00b7der", "Duft", "im", "A\u00b7ben\u00b7drot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deine Stimm' ist sanft, wie der Fl\u00f6te", "tokens": ["Dei\u00b7ne", "Stimm'", "ist", "sanft", ",", "wie", "der", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Im Echo entschwindender Hall.", "tokens": ["Im", "E\u00b7cho", "ent\u00b7schwin\u00b7den\u00b7der", "Hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Oftmals zeigst du, in duftiger Ferne,", "tokens": ["Oft\u00b7mals", "zeigst", "du", ",", "in", "duf\u00b7ti\u00b7ger", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Mir freundlich der Jugend Lenzgefild;", "tokens": ["Mir", "freund\u00b7lich", "der", "Ju\u00b7gend", "Lenz\u00b7ge\u00b7fild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Oder reihst in Kr\u00e4nze die Veilchen,", "tokens": ["O\u00b7der", "reihst", "in", "Kr\u00e4n\u00b7ze", "die", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "So Liebe mir, sparsam nur, las.", "tokens": ["So", "Lie\u00b7be", "mir", ",", "spar\u00b7sam", "nur", ",", "las", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "PPER", "$,", "ADJD", "ADV", "$,", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Oft erscheinst du mir, l\u00e4chelnd durch Thr\u00e4nen,", "tokens": ["Oft", "er\u00b7scheinst", "du", "mir", ",", "l\u00e4\u00b7chelnd", "durch", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und kosest mit mir, vertraut und lang,", "tokens": ["Und", "ko\u00b7sest", "mit", "mir", ",", "ver\u00b7traut", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von den toten Lieben, an Gr\u00e4bern,", "tokens": ["Von", "den", "to\u00b7ten", "Lie\u00b7ben", ",", "an", "Gr\u00e4\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "$,", "APPR", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die h\u00f6heres Gras schon umwallt.", "tokens": ["Die", "h\u00f6\u00b7he\u00b7res", "Gras", "schon", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Mir willkommen im Schleier der Trauer!", "tokens": ["Mir", "will\u00b7kom\u00b7men", "im", "Schlei\u00b7er", "der", "Trau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Willkommen im heitern Silberflor!", "tokens": ["Will\u00b7kom\u00b7men", "im", "hei\u00b7tern", "Sil\u00b7ber\u00b7flor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Rasch entfleucht der Gegenwart Freude;", "tokens": ["Rasch", "ent\u00b7fleucht", "der", "Ge\u00b7gen\u00b7wart", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Du, sinnende Tr\u00f6sterin, weilst!", "tokens": ["Du", ",", "sin\u00b7nen\u00b7de", "Tr\u00f6s\u00b7te\u00b7rin", ",", "weilst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}