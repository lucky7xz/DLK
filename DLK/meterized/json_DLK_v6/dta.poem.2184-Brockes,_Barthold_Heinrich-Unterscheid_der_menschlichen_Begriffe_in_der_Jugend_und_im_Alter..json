{"dta.poem.2184": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unterscheid der menschlichen Begriffe  \n in der Jugend und im Alter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend meynt, es sey von ihr die Wahrheit \u00fcber-", "tokens": ["Die", "Ju\u00b7gend", "meynt", ",", "es", "sey", "von", "ihr", "die", "Wahr\u00b7heit", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "all gefunden;", "tokens": ["all", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Das Alter und Erfahrung lehrt: sie sey fast \u00fcberall ver-", "tokens": ["Das", "Al\u00b7ter", "und", "Er\u00b7fah\u00b7rung", "lehrt", ":", "sie", "sey", "fast", "\u00fc\u00b7be\u00b7rall", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "schwunden.", "tokens": ["schwun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hierinn besteht der Unterscheid, womit wir uns behelfen", "tokens": ["Hie\u00b7rinn", "be\u00b7steht", "der", "Un\u00b7ter\u00b7scheid", ",", "wo\u00b7mit", "wir", "uns", "be\u00b7hel\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "m\u00fcssen:", "tokens": ["m\u00fcs\u00b7sen", ":"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die Alten wissen wenig mehr, als da\u00df die Jungen we-", "tokens": ["Die", "Al\u00b7ten", "wis\u00b7sen", "we\u00b7nig", "mehr", ",", "als", "da\u00df", "die", "Jun\u00b7gen", "we"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$,", "KOKOM", "KOUS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "nig wissen.", "tokens": ["nig", "wis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}