{"textgrid.poem.38955": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwer h\u00e4ngen an der Welt-Uhr die Gewichte", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer h\u00e4ngen an der Welt-Uhr die Gewichte", "tokens": ["Schwer", "h\u00e4n\u00b7gen", "an", "der", "Welt\u00b7Uhr", "die", "Ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und treiben sie doch langsam nur zum Gange,", "tokens": ["Und", "trei\u00b7ben", "sie", "doch", "lang\u00b7sam", "nur", "zum", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So manche Tugend geht bei uns im Schwange,", "tokens": ["So", "man\u00b7che", "Tu\u00b7gend", "geht", "bei", "uns", "im", "Schwan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch stehn wir, Freund, uns selber oft im Lichte.", "tokens": ["Doch", "stehn", "wir", ",", "Freund", ",", "uns", "sel\u00b7ber", "oft", "im", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Menschheit schreitet fort und manchem Wichte", "tokens": ["Die", "Menschheit", "schrei\u00b7tet", "fort", "und", "man\u00b7chem", "Wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PIAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird bei den vielen Widerspr\u00fcchen bange,", "tokens": ["Wird", "bei", "den", "vie\u00b7len", "Wi\u00b7der\u00b7spr\u00fc\u00b7chen", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fast jeder fragt, wohin er denn gelange,", "tokens": ["Fast", "je\u00b7der", "fragt", ",", "wo\u00b7hin", "er", "denn", "ge\u00b7lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zweifelt immerdar an dem Berichte.", "tokens": ["Und", "zwei\u00b7felt", "im\u00b7mer\u00b7dar", "an", "dem", "Be\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch lache nur ob diesen ernsten Possen,", "tokens": ["Doch", "la\u00b7che", "nur", "ob", "die\u00b7sen", "erns\u00b7ten", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df nur den Wagen unbek\u00fcmmert fahren,", "tokens": ["La\u00df", "nur", "den", "Wa\u00b7gen", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und glaub', er werde wo die Fracht abladen.", "tokens": ["Und", "glaub'", ",", "er", "wer\u00b7de", "wo", "die", "Fracht", "ab\u00b7la\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch werden wir auf stein'gem Weg zersto\u00dfen,", "tokens": ["Noch", "wer\u00b7den", "wir", "auf", "stein'\u00b7gem", "Weg", "zer\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort seh' ich schon den Sand vor mir, den klaren,", "tokens": ["Dort", "seh'", "ich", "schon", "den", "Sand", "vor", "mir", ",", "den", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieh, der Korb mit Wein nahm keinen Schaden.", "tokens": ["Und", "sieh", ",", "der", "Korb", "mit", "Wein", "nahm", "kei\u00b7nen", "Scha\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}