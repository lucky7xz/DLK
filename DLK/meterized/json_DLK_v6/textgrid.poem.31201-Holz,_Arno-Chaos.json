{"textgrid.poem.31201": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Chaos", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Fluch, der diese Zeit durchzittert,", "tokens": ["Das", "ist", "der", "Fluch", ",", "der", "die\u00b7se", "Zeit", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der uns das Leben und den Tod verbittert:", "tokens": ["Der", "uns", "das", "Le\u00b7ben", "und", "den", "Tod", "ver\u00b7bit\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir legen ewig neu das Fundament", "tokens": ["Wir", "le\u00b7gen", "e\u00b7wig", "neu", "das", "Fun\u00b7da\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und niemals greift der Bau ins Firmament!", "tokens": ["Und", "nie\u00b7mals", "greift", "der", "Bau", "ins", "Fir\u00b7ma\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir h\u00f6ren blutend, wie die V\u00f6lker wimmern,", "tokens": ["Wir", "h\u00f6\u00b7ren", "blu\u00b7tend", ",", "wie", "die", "V\u00f6l\u00b7ker", "wim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und helfen selber ihre Kreuze zimmern!", "tokens": ["Und", "hel\u00b7fen", "sel\u00b7ber", "ih\u00b7re", "Kreu\u00b7ze", "zim\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir flehen br\u00fcnstig um das Weltgenie", "tokens": ["Wir", "fle\u00b7hen", "br\u00fcns\u00b7tig", "um", "das", "Welt\u00b7ge\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sind noch viehisch, viehisch wie das Vieh!", "tokens": ["Und", "sind", "noch", "vie\u00b7hisch", ",", "vie\u00b7hisch", "wie", "das", "Vieh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir speien auf das Kreuz der Kathedrale", "tokens": ["Wir", "spei\u00b7en", "auf", "das", "Kreuz", "der", "Ka\u00b7thed\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dichten nur noch Zukunftsideale!", "tokens": ["Und", "dich\u00b7ten", "nur", "noch", "Zu\u00b7kunft\u00b7si\u00b7de\u00b7a\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir thun die Skepsis feig in Acht und Bann", "tokens": ["Wir", "thun", "die", "Skep\u00b7sis", "feig", "in", "Acht", "und", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "CARD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schliesslich \u2013 glaubt man selber nicht daran!", "tokens": ["Und", "schliess\u00b7lich", "\u2013", "glaubt", "man", "sel\u00b7ber", "nicht", "da\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "VVFIN", "PIS", "ADV", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das ist der Fluch, der diese Zeit durchzittert,", "tokens": ["Das", "ist", "der", "Fluch", ",", "der", "die\u00b7se", "Zeit", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der uns das Leben und den Tod verbittert!", "tokens": ["Der", "uns", "das", "Le\u00b7ben", "und", "den", "Tod", "ver\u00b7bit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}