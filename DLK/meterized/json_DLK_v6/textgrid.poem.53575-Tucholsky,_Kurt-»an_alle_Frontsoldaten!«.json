{"textgrid.poem.53575": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "\u00bban alle Frontsoldaten!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So war es:", "tokens": ["So", "war", "es", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "bekam nicht unter 300 Mark", "tokens": ["be\u00b7kam", "nicht", "un\u00b7ter", "300", "Mark"], "token_info": ["word", "word", "word", "number", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "CARD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gehalt, meist bedeutend mehr", "tokens": ["Ge\u00b7halt", ",", "meist", "be\u00b7deu\u00b7tend", "mehr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "(zulagen, Tagegelder usw.);", "tokens": ["(", "zu\u00b7la\u00b7gen", ",", "Ta\u00b7ge\u00b7gel\u00b7der", "us\u00b7w.", ")", ";"], "token_info": ["punct", "word", "punct", "word", "abbreviation", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "NN", "NE", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": " erhielt ein paar Pfennige und im Bedarfsfalle unzureichende Familienunterst\u00fctzung;", "tokens": ["er\u00b7hielt", "ein", "paar", "Pfen\u00b7ni\u00b7ge", "und", "im", "Be\u00b7darfs\u00b7fal\u00b7le", "un\u00b7zu\u00b7rei\u00b7chen\u00b7de", "Fa\u00b7mi\u00b7li\u00b7en\u00b7un\u00b7ter\u00b7st\u00fct\u00b7zung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "KON", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-+--+-+-+-+-", "measure": "amphibrach.penta.plus"}}, "stanza.3": {"line.1": {"text": "hatte Bewegungsfreiheit, ritt", "tokens": ["hat\u00b7te", "Be\u00b7we\u00b7gungs\u00b7frei\u00b7heit", ",", "ritt"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "NN", "$,", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "aus, besah sich das fremde", "tokens": ["aus", ",", "be\u00b7sah", "sich", "das", "frem\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "VVFIN", "PRF", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Land, in dem er stand, und f\u00fchr-", "tokens": ["Land", ",", "in", "dem", "er", "stand", ",", "und", "f\u00fchr"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KON", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "te ein ziemlich freies Leben;", "tokens": ["te", "ein", "ziem\u00b7lich", "frei\u00b7es", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "hatte die M\u00f6glichkeit, dauernd", "tokens": ["hat\u00b7te", "die", "M\u00f6g\u00b7lich\u00b7keit", ",", "dau\u00b7ernd"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Eink\u00e4ufe zu machen und Einge-", "tokens": ["Ein\u00b7k\u00e4u\u00b7fe", "zu", "ma\u00b7chen", "und", "Ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "KON", "TRUNC"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "kauftes mit nicht immer ein-", "tokens": ["kauf\u00b7tes", "mit", "nicht", "im\u00b7mer", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PTKNEG", "ADV", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wandfreien Transportmitteln", "tokens": ["wand\u00b7frei\u00b7en", "Trans\u00b7port\u00b7mit\u00b7teln"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nach Hause zu schicken;", "tokens": ["nach", "Hau\u00b7se", "zu", "schi\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "a\u00df gut und fast immer aus-", "tokens": ["a\u00df", "gut", "und", "fast", "im\u00b7mer", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADV", "ADV", "TRUNC"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "reichend; in der Etappe", "tokens": ["rei\u00b7chend", ";", "in", "der", "E\u00b7tap\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "ausgezeichnet, dort niemals", "tokens": ["aus\u00b7ge\u00b7zeich\u00b7net", ",", "dort", "nie\u00b7mals"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "aus der Feldk\u00fcche. Sein Essen", "tokens": ["aus", "der", "Feld\u00b7k\u00fc\u00b7che", ".", "Sein", "Es\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "war billig;", "tokens": ["war", "bil\u00b7lig", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "wurde von seinesgleichen in jeder", "tokens": ["wur\u00b7de", "von", "sei\u00b7nes\u00b7glei\u00b7chen", "in", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "APPR", "PIAT"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Weise unterst\u00fctzt: bei Urlaubs-", "tokens": ["Wei\u00b7se", "un\u00b7ter\u00b7st\u00fctzt", ":", "bei", "Ur\u00b7laubs"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$.", "APPR", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sperre, Dienstfahrten, kleinen", "tokens": ["sper\u00b7re", ",", "Dienst\u00b7fahr\u00b7ten", ",", "klei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schiebungen aus der Kammer in", "tokens": ["Schie\u00b7bun\u00b7gen", "aus", "der", "Kam\u00b7mer", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bekleidungsangelegenheiten,", "tokens": ["Be\u00b7klei\u00b7dungs\u00b7an\u00b7ge\u00b7le\u00b7gen\u00b7hei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00e4rztlichen Attesten;", "tokens": ["\u00e4rzt\u00b7li\u00b7chen", "At\u00b7tes\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "wurde in allen kleinen \u00c4u\u00dferlich-", "tokens": ["wur\u00b7de", "in", "al\u00b7len", "klei\u00b7nen", "\u00c4u\u00b7\u00dfe\u00b7rlich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "ADJA", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "keiten, die das Leben so sehr", "tokens": ["kei\u00b7ten", ",", "die", "das", "Le\u00b7ben", "so", "sehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "beeinflussen, unterst\u00fctzt, das", "tokens": ["be\u00b7ein\u00b7flus\u00b7sen", ",", "un\u00b7ter\u00b7st\u00fctzt", ",", "das"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVPP", "$,", "VVPP", "$,", "PRELS"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "ber\u00fcchtigte Schild \u203aNur f\u00fcr", "tokens": ["be\u00b7r\u00fcch\u00b7tig\u00b7te", "Schild", "\u203a", "Nur", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "ADV", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Offiziere\u2039 beherrschte die", "tokens": ["Of\u00b7fi\u00b7zie\u00b7re", "\u2039", "be\u00b7herrschte", "die"], "token_info": ["word", "punct", "word", "word"], "pos": ["FM.la", "$(", "VVFIN", "ART"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Kriegsschaupl\u00e4tze.", "tokens": ["Kriegs\u00b7schau\u00b7pl\u00e4t\u00b7ze", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "sah in den Mond.", "tokens": ["sah", "in", "den", "Mond", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}