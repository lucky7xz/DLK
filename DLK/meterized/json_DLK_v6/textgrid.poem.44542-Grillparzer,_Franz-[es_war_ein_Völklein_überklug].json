{"textgrid.poem.44542": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[es war ein V\u00f6lklein \u00fcberklug]", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein V\u00f6lklein \u00fcberklug,", "tokens": ["Es", "war", "ein", "V\u00f6l\u00b7klein", "\u00fc\u00b7berk\u00b7lug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tat niemals selber sich genug,", "tokens": ["Tat", "nie\u00b7mals", "sel\u00b7ber", "sich", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dacht etwas gro\u00df von seinen Gaben,", "tokens": ["Dacht", "et\u00b7was", "gro\u00df", "von", "sei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wollt immer was Apartes haben,", "tokens": ["Wollt", "im\u00b7mer", "was", "A\u00b7par\u00b7tes", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PWS", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nur was es dachte, was es trieb,", "tokens": ["Nur", "was", "es", "dach\u00b7te", ",", "was", "es", "trieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bockfest in seinem Innern blieb,", "tokens": ["Bock\u00b7fest", "in", "sei\u00b7nem", "In\u00b7nern", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und der Gedanke, gro\u00df und weit,", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ke", ",", "gro\u00df", "und", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kam selten an die Wirklichkeit;", "tokens": ["Kam", "sel\u00b7ten", "an", "die", "Wirk\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das wohnte nun seit manchem Jahr", "tokens": ["Das", "wohn\u00b7te", "nun", "seit", "man\u00b7chem", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In schlichten H\u00e4usern, klein f\u00fcrwahr,", "tokens": ["In", "schlich\u00b7ten", "H\u00e4u\u00b7sern", ",", "klein", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie sies zu bauen prunkentfernt,", "tokens": ["Wie", "sies", "zu", "bau\u00b7en", "prun\u00b7kent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von ihren Nachbarn rings gelernt. usw.", "tokens": ["Von", "ih\u00b7ren", "Nach\u00b7barn", "rings", "ge\u00b7lernt", ".", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$.", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}