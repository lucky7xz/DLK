{"textgrid.poem.35037": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Werdet frei!", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werdet frei! Ihr windet euch in Ketten,", "tokens": ["Wer\u00b7det", "frei", "!", "Ihr", "win\u00b7det", "euch", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Glaube nur kann euch befrein.", "tokens": ["Und", "der", "Glau\u00b7be", "nur", "kann", "euch", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Werdet frei! Gott m\u00f6chte gern euch retten,", "tokens": ["Wer\u00b7det", "frei", "!", "Gott", "m\u00f6ch\u00b7te", "gern", "euch", "ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber grad durch ihn wollt ihrs nicht sein.", "tokens": ["A\u00b7ber", "grad", "durch", "ihn", "wollt", "ihrs", "nicht", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VAINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ists so schwer, Verehrung dem zu zollen,", "tokens": ["Ists", "so", "schwer", ",", "Ver\u00b7eh\u00b7rung", "dem", "zu", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "NN", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der da war und ist in Ewigkeit?", "tokens": ["Der", "da", "war", "und", "ist", "in", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "KON", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Werdet frei! Ihr braucht es nur zu wollen;", "tokens": ["Wer\u00b7det", "frei", "!", "Ihr", "braucht", "es", "nur", "zu", "wol\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Werdet frei, die ihr jetzt Sklaven seid!", "tokens": ["Wer\u00b7det", "frei", ",", "die", "ihr", "jetzt", "Skla\u00b7ven", "seid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Kam der Hauch des Herrn zur Erde nieder,", "tokens": ["Kam", "der", "Hauch", "des", "Herrn", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df des Fleisches Ackerknecht er sei?", "tokens": ["Da\u00df", "des", "Flei\u00b7sches", "A\u00b7cker\u00b7knecht", "er", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oeffnet ihm die Heimatspforte wieder;", "tokens": ["Oeff\u00b7net", "ihm", "die", "Hei\u00b7mats\u00b7pfor\u00b7te", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Macht ihn vom Gesindedienste frei!", "tokens": ["Macht", "ihn", "vom", "Ge\u00b7sin\u00b7de\u00b7diens\u00b7te", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "L\u00e4ngst schon ist des Himmels Ruf erschollen;", "tokens": ["L\u00e4ngst", "schon", "ist", "des", "Him\u00b7mels", "Ruf", "er\u00b7schol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihn zu h\u00f6ren, ists nun h\u00f6chste Zeit.", "tokens": ["Ihn", "zu", "h\u00f6\u00b7ren", ",", "ists", "nun", "h\u00f6chs\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Werdet frei! Ihr braucht es nur zu wollen;", "tokens": ["Wer\u00b7det", "frei", "!", "Ihr", "braucht", "es", "nur", "zu", "wol\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Werdet frei, die ihr jetzt Sklaven seid!", "tokens": ["Wer\u00b7det", "frei", ",", "die", "ihr", "jetzt", "Skla\u00b7ven", "seid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "H\u00e4lt die Fremde euch denn so gefangen,", "tokens": ["H\u00e4lt", "die", "Frem\u00b7de", "euch", "denn", "so", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ihr eure Heimat nicht mehr kennt?", "tokens": ["Da\u00df", "ihr", "eu\u00b7re", "Hei\u00b7mat", "nicht", "mehr", "kennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00f6nnt ihr nicht mehr zu dem Wort gelangen,", "tokens": ["K\u00f6nnt", "ihr", "nicht", "mehr", "zu", "dem", "Wort", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Welches euch beim rechten Namen nennt?", "tokens": ["Wel\u00b7ches", "euch", "beim", "rech\u00b7ten", "Na\u00b7men", "nennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn sie es euch offenbaren sollen,", "tokens": ["Wenn", "sie", "es", "euch", "of\u00b7fen\u00b7ba\u00b7ren", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sind viel heil'ge Stimmen gern bereit.", "tokens": ["Sind", "viel", "heil'\u00b7ge", "Stim\u00b7men", "gern", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Werdet frei! Ihr braucht es nur zu wollen;", "tokens": ["Wer\u00b7det", "frei", "!", "Ihr", "braucht", "es", "nur", "zu", "wol\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Werdet frei, die ihr jetzt Sklaven seid!", "tokens": ["Wer\u00b7det", "frei", ",", "die", "ihr", "jetzt", "Skla\u00b7ven", "seid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}