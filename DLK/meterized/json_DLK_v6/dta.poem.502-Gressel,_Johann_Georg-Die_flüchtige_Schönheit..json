{"dta.poem.502": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die fl\u00fcchtige Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe offt gesagt/ wir grauen und veralten/", "tokens": ["Ich", "ha\u00b7be", "offt", "ge\u00b7sagt", "/", "wir", "grau\u00b7en", "und", "ver\u00b7al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "PPER", "ADJA", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebrauche deiner Schoo\u00df/ dieweil du brauchbahr bist/", "tokens": ["Ge\u00b7brau\u00b7che", "dei\u00b7ner", "Schoo\u00df", "/", "die\u00b7weil", "du", "brauch\u00b7bahr", "bist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein es war umsonst/ inzwischen zog in Falten", "tokens": ["Al\u00b7lein", "es", "war", "um\u00b7sonst", "/", "in\u00b7zwi\u00b7schen", "zog", "in", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$(", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Zeit dein Angesicht/ die nicht zu halten ist.", "tokens": ["Die", "Zeit", "dein", "An\u00b7ge\u00b7sicht", "/", "die", "nicht", "zu", "hal\u00b7ten", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "PRELS", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jetzund gereut es dich/ weil du nicht diesen Willen/", "tokens": ["Je\u00b7tzund", "ge\u00b7reut", "es", "dich", "/", "weil", "du", "nicht", "die\u00b7sen", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$(", "KOUS", "PPER", "PTKNEG", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch jetzige Gestalt zu jener Zeit gehabt.", "tokens": ["Noch", "jet\u00b7zi\u00b7ge", "Ge\u00b7stalt", "zu", "je\u00b7ner", "Zeit", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PDAT", "NN", "VAPP", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Doch h\u00f6r/ umarme mich die alte Lust zu stillen/", "tokens": ["Doch", "h\u00f6r", "/", "um\u00b7ar\u00b7me", "mich", "die", "al\u00b7te", "Lust", "zu", "stil\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Werd\u2019 ich gleich nicht mit dem/ was ich gewolt/ begabt.", "tokens": ["Werd'", "ich", "gleich", "nicht", "mit", "dem", "/", "was", "ich", "ge\u00b7wolt", "/", "be\u00b7gabt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "$(", "PWS", "PPER", "VVPP", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}