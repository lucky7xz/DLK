{"textgrid.poem.28411": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie da\u00df der Herre Christ den Esel wil beschreiten,", "tokens": ["Wie", "da\u00df", "der", "Her\u00b7re", "Christ", "den", "E\u00b7sel", "wil", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Grosse dieser Welt wolln sch\u00f6ne Hengste reiten?", "tokens": ["Und", "Gros\u00b7se", "die\u00b7ser", "Welt", "wolln", "sch\u00f6\u00b7ne", "Hengs\u00b7te", "rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PDAT", "NN", "VMFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein sanfftes Thier geh\u00f6rt auff einen engen Steg;", "tokens": ["Ein", "sanff\u00b7tes", "Thier", "ge\u00b7h\u00f6rt", "auff", "ei\u00b7nen", "en\u00b7gen", "Steg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein tummelhafftig Gaul auff einen breiten Weg.", "tokens": ["Ein", "tum\u00b7mel\u00b7haff\u00b7tig", "Gaul", "auff", "ei\u00b7nen", "brei\u00b7ten", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}