{"textgrid.poem.49446": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Die Besiegten I", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben triumphiert, das Ideal ist tot,", "tokens": ["Das", "Le\u00b7ben", "tri\u00b7um\u00b7phiert", ",", "das", "I\u00b7deal", "ist", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und wiehernd in den Wind, von j\u00e4her Freude trunken", "tokens": ["Und", "wie\u00b7hernd", "in", "den", "Wind", ",", "von", "j\u00e4\u00b7her", "Freu\u00b7de", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zerknirscht des Siegers Pferd und stampft in blut'gen Kot", "tokens": ["Zer\u00b7knirscht", "des", "Sie\u00b7gers", "Pferd", "und", "stampft", "in", "blut'\u00b7gen", "Kot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Br\u00fcder, die mit stolzem Anstand hingesunken.", "tokens": ["Die", "Br\u00fc\u00b7der", ",", "die", "mit", "stol\u00b7zem", "An\u00b7stand", "hin\u00b7ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wir, die \u00fcberlebt die Niederlage, ach!", "tokens": ["Und", "wir", ",", "die", "\u00fc\u00b7ber\u00b7lebt", "die", "Nie\u00b7der\u00b7la\u00b7ge", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "VVFIN", "ART", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die F\u00fcsse wund, den Blick verweint, das Haupt zerschlagen,", "tokens": ["Die", "F\u00fcs\u00b7se", "wund", ",", "den", "Blick", "ver\u00b7weint", ",", "das", "Haupt", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beschmutzt, ermattet, blutend, ehrberaubt und schwach", "tokens": ["Be\u00b7schmutzt", ",", "er\u00b7mat\u00b7tet", ",", "blu\u00b7tend", ",", "ehr\u00b7be\u00b7raubt", "und", "schwach"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gehn wir, und kaum ersticken wir die dumpfen Klagen.", "tokens": ["Gehn", "wir", ",", "und", "kaum", "er\u00b7sti\u00b7cken", "wir", "die", "dum\u00b7pfen", "Kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir gehen immerfort den Weg, der d\u00fcster droht,", "tokens": ["Wir", "ge\u00b7hen", "im\u00b7mer\u00b7fort", "den", "Weg", ",", "der", "d\u00fcs\u00b7ter", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie M\u00f6rder und Ge\u00e4chtete sind wir entflohen,", "tokens": ["Wie", "M\u00f6r\u00b7der", "und", "Ge\u00e4ch\u00b7te\u00b7te", "sind", "wir", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verwaist, verwitwet, ohne Kind noch Morgenrot", "tokens": ["Ver\u00b7waist", ",", "ver\u00b7wit\u00b7wet", ",", "oh\u00b7ne", "Kind", "noch", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "KOUI", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beim Schein der lieben W\u00e4lder, die in Flammen lohen.", "tokens": ["Beim", "Schein", "der", "lie\u00b7ben", "W\u00e4l\u00b7der", ",", "die", "in", "Flam\u00b7men", "lo\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun, weil sich unser Los vollendet, weil ihr wisst,", "tokens": ["Nun", ",", "weil", "sich", "un\u00b7ser", "Los", "voll\u00b7en\u00b7det", ",", "weil", "ihr", "wisst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dass alle Hoffnung aus und g\u00e4nzlich wir verlassen,", "tokens": ["Dass", "al\u00b7le", "Hoff\u00b7nung", "aus", "und", "g\u00e4nz\u00b7lich", "wir", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKVZ", "KON", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dass auch die gr\u00f6\u00dfte M\u00fchsal leer und eitel ist,", "tokens": ["Dass", "auch", "die", "gr\u00f6\u00df\u00b7te", "M\u00fch\u00b7sal", "leer", "und", "ei\u00b7tel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dass es am Ende ist mit allem unsern Hassen,", "tokens": ["Dass", "es", "am", "En\u00b7de", "ist", "mit", "al\u00b7lem", "un\u00b7sern", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So brauchen wir zur Stunde, da es nachten will,", "tokens": ["So", "brau\u00b7chen", "wir", "zur", "Stun\u00b7de", ",", "da", "es", "nach\u00b7ten", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wir die eitle Hoffnung auf ein Grab verachten,", "tokens": ["Die", "wir", "die", "eit\u00b7le", "Hoff\u00b7nung", "auf", "ein", "Grab", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur noch im Dunkel sterben, ungesehn und still,", "tokens": ["Nur", "noch", "im", "Dun\u00b7kel", "ster\u00b7ben", ",", "un\u00b7ge\u00b7sehn", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie es Besiegten ziemt nach so gewalt'gen Schlachten.", "tokens": ["Wie", "es", "Be\u00b7sieg\u00b7ten", "ziemt", "nach", "so", "ge\u00b7walt'\u00b7gen", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}