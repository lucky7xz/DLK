{"textgrid.poem.60360": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die L\u00fcfte rasten auf der weiten Heide,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00fcfte rasten auf der weiten Heide,", "tokens": ["Die", "L\u00fcf\u00b7te", "ras\u00b7ten", "auf", "der", "wei\u00b7ten", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Disteln sind so regungslos zu schauen,", "tokens": ["Die", "Dis\u00b7teln", "sind", "so", "re\u00b7gungs\u00b7los", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So starr, als w\u00e4ren sie aus Stein gehauen,", "tokens": ["So", "starr", ",", "als", "w\u00e4\u00b7ren", "sie", "aus", "Stein", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sie der Wandrer streift mit seinem Kleide.", "tokens": ["Bis", "sie", "der", "Wand\u00b7rer", "streift", "mit", "sei\u00b7nem", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Erd und Himmel haben keine Scheide,", "tokens": ["Und", "Erd", "und", "Him\u00b7mel", "ha\u00b7ben", "kei\u00b7ne", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In eins gefallen sind die nebelgrauen,", "tokens": ["In", "eins", "ge\u00b7fal\u00b7len", "sind", "die", "ne\u00b7bel\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zwei Freunden gleich, die sich ihr Leid vertrauen,", "tokens": ["Zwei", "Freun\u00b7den", "gleich", ",", "die", "sich", "ihr", "Leid", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Mein und Dein vergessen traurig beide.", "tokens": ["Und", "Mein", "und", "Dein", "ver\u00b7ges\u00b7sen", "trau\u00b7rig", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "KON", "PPOSAT", "ADJA", "ADJD", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun pl\u00f6tzlich wankt die Distel hin und wider,", "tokens": ["Nun", "pl\u00f6tz\u00b7lich", "wankt", "die", "Dis\u00b7tel", "hin", "und", "wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und heftig rauschend bricht der Regen nieder,", "tokens": ["Und", "hef\u00b7tig", "rau\u00b7schend", "bricht", "der", "Re\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie laute Antwort auf ein stummes Fragen.", "tokens": ["Wie", "lau\u00b7te", "Ant\u00b7wort", "auf", "ein", "stum\u00b7mes", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Wandrer h\u00f6rt den Regen niederbrausen,", "tokens": ["Der", "Wand\u00b7rer", "h\u00f6rt", "den", "Re\u00b7gen", "nie\u00b7der\u00b7brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00f6rt die windgepeitschte Distel sausen,", "tokens": ["Er", "h\u00f6rt", "die", "wind\u00b7ge\u00b7peitschte", "Dis\u00b7tel", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eine Wehmut f\u00fchlt er, nicht zu sagen.", "tokens": ["Und", "ei\u00b7ne", "Weh\u00b7mut", "f\u00fchlt", "er", ",", "nicht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}