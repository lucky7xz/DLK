{"textgrid.poem.58264": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "302. Wer nichts hat, hat Gott", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir gleich mangelt Trost und Licht,", "tokens": ["Wenn", "dir", "gleich", "man\u00b7gelt", "Trost", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mangelt dir drum Gott noch nicht;", "tokens": ["So", "man\u00b7gelt", "dir", "drum", "Gott", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei still in dir und halt dein Nichts", "tokens": ["Sei", "still", "in", "dir", "und", "halt", "dein", "Nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "APPR", "PPER", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anstatt des Trostes und des Lichts!", "tokens": ["An\u00b7statt", "des", "Tros\u00b7tes", "und", "des", "Lichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seel', glaub es, wer ", "tokens": ["Seel'", ",", "glaub", "es", ",", "wer"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PWS"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Hat Gott und alles in der Tat!", "tokens": ["Hat", "Gott", "und", "al\u00b7les", "in", "der", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}