{"textgrid.poem.55106": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im weiten Mantel bis ans Kinn verh\u00fcllet,", "tokens": ["Im", "wei\u00b7ten", "Man\u00b7tel", "bis", "ans", "Kinn", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging ich den Felsenweg, den schroffen, grauen,", "tokens": ["Ging", "ich", "den", "Fel\u00b7sen\u00b7weg", ",", "den", "schrof\u00b7fen", ",", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hernieder dann zu winterhaften Auen,", "tokens": ["Her\u00b7nie\u00b7der", "dann", "zu", "win\u00b7ter\u00b7haf\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unruh'gen Sinns, zur nahen Flucht gewillet.", "tokens": ["Un\u00b7ruh'\u00b7gen", "Sinns", ",", "zur", "na\u00b7hen", "Flucht", "ge\u00b7wil\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Auf einmal schien der neue Tag enth\u00fcllet:", "tokens": ["Auf", "ein\u00b7mal", "schien", "der", "neu\u00b7e", "Tag", "ent\u00b7h\u00fcl\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein M\u00e4dchen kam, ein Himmel anzuschauen,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "kam", ",", "ein", "Him\u00b7mel", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So musterhaft wie jene lieben Frauen", "tokens": ["So", "mus\u00b7ter\u00b7haft", "wie", "je\u00b7ne", "lie\u00b7ben", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Dichterwelt. Mein Sehnen war gestillet.", "tokens": ["Der", "Dich\u00b7ter\u00b7welt", ".", "Mein", "Seh\u00b7nen", "war", "ge\u00b7stil\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wandt ich mich hinweg und lie\u00df sie gehen!", "tokens": ["Doch", "wandt", "ich", "mich", "hin\u00b7weg", "und", "lie\u00df", "sie", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APZR", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wickelte mich enger in die Falten,", "tokens": ["Und", "wi\u00b7ckel\u00b7te", "mich", "en\u00b7ger", "in", "die", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wollt ich trutzend in mir selbst erwarmen;", "tokens": ["Als", "wollt", "ich", "trut\u00b7zend", "in", "mir", "selbst", "er\u00b7war\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und folgt ihr doch. Sie stand. Da war's geschehen", "tokens": ["Und", "folgt", "ihr", "doch", ".", "Sie", "stand", ".", "Da", "wa\u00b7r's", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In meiner H\u00fclle konnt ich mich nicht halten,", "tokens": ["In", "mei\u00b7ner", "H\u00fcl\u00b7le", "konnt", "ich", "mich", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die warf ich weg, ", "tokens": ["Die", "warf", "ich", "weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}}}}}