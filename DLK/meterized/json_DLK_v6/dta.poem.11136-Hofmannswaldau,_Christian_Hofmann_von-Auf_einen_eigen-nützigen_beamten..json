{"dta.poem.11136": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen eigen-n\u00fctzigen beamten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00e4fer lieget hier. Soll dichs nicht wunder haben?", "tokens": ["Ein", "sch\u00e4\u00b7fer", "lie\u00b7get", "hier", ".", "Soll", "dichs", "nicht", "wun\u00b7der", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "$.", "VMFIN", "PIS", "PTKNEG", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre schur, wird doch mit gantzer haut begraben.", "tokens": ["Der", "and\u00b7re", "schur", ",", "wird", "doch", "mit", "gant\u00b7zer", "haut", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}