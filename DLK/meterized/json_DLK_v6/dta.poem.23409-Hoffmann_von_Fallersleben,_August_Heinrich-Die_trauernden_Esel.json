{"dta.poem.23409": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die trauernden Esel", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Esel gingen im Leide,", "tokens": ["Die", "E\u00b7sel", "gin\u00b7gen", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drob staunte die ganze Welt,", "tokens": ["Drob", "staun\u00b7te", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weil gr\u00fcn noch war die Heide,", "tokens": ["Weil", "gr\u00fcn", "noch", "war", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Disteln noch trug das Feld.", "tokens": ["Und", "Dis\u00b7teln", "noch", "trug", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie gingen tief geb\u00fccket", "tokens": ["Sie", "gin\u00b7gen", "tief", "ge\u00b7b\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lie\u00dfen hangen das Ohr,", "tokens": ["Und", "lie\u00b7\u00dfen", "han\u00b7gen", "das", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und hatten den Schwanz geschm\u00fccket", "tokens": ["Und", "hat\u00b7ten", "den", "Schwanz", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit einem langen Flor.", "tokens": ["Mit", "ei\u00b7nem", "lan\u00b7gen", "Flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was hat sich denn zugetragen?", "tokens": ["Was", "hat", "sich", "denn", "zu\u00b7ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wir staunen und schweigen still,", "tokens": ["Wir", "stau\u00b7nen", "und", "schwei\u00b7gen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und niemand wei\u00df zu sagen,", "tokens": ["Und", "nie\u00b7mand", "wei\u00df", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was diese Trauer will.", "tokens": ["Was", "die\u00b7se", "Trau\u00b7er", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer meldet uns jetzunder,", "tokens": ["Wer", "mel\u00b7det", "uns", "je\u00b7tzun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was diese Trauer soll?", "tokens": ["Was", "die\u00b7se", "Trau\u00b7er", "soll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer deutet uns das Wunder?", "tokens": ["Wer", "deu\u00b7tet", "uns", "das", "Wun\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt zu, ich wei\u00df es wol.", "tokens": ["H\u00f6rt", "zu", ",", "ich", "wei\u00df", "es", "wol", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es starb im spanischen Lande,", "tokens": ["Es", "starb", "im", "spa\u00b7ni\u00b7schen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch eh' ein Jahr verflo\u00df,", "tokens": ["Noch", "eh'", "ein", "Jahr", "ver\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Heilandseselstande", "tokens": ["Vom", "Hei\u00b7land\u00b7se\u00b7sel\u00b7stan\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon wieder ein echter Spro\u00df.", "tokens": ["Schon", "wie\u00b7der", "ein", "ech\u00b7ter", "Spro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Drum gehn die Esel im Leide,", "tokens": ["Drum", "gehn", "die", "E\u00b7sel", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als ob verdorben das Feld,", "tokens": ["Als", "ob", "ver\u00b7dor\u00b7ben", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als ob versenget die Heide", "tokens": ["Als", "ob", "ver\u00b7sen\u00b7get", "die", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und gestorben w\u00e4re die Welt.", "tokens": ["Und", "ge\u00b7stor\u00b7ben", "w\u00e4\u00b7re", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}