{"dta.poem.2174": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Meynung hat, als K\u00f6niginn, sich auf den", "tokens": ["Die", "Mey\u00b7nung", "hat", ",", "als", "K\u00f6\u00b7ni\u00b7ginn", ",", "sich", "auf", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "NN", "$,", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thron der Welt geschwungen,", "tokens": ["Thron", "der", "Welt", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sie herrschet und tyrannisiret. Doch wird", "tokens": ["Sie", "herr\u00b7schet", "und", "ty\u00b7ran\u00b7ni\u00b7si\u00b7ret", ".", "Doch", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "KON", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ihr Wesen nicht erkannt.", "tokens": ["ihr", "We\u00b7sen", "nicht", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie wird vermuthlich auch so leicht von ihrem Sitze", "tokens": ["Sie", "wird", "ver\u00b7muth\u00b7lich", "auch", "so", "leicht", "von", "ih\u00b7rem", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nicht verdrungen,", "tokens": ["nicht", "ver\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Seitdem sie meynt, und wir mit ihr: sie hei\u00dfe und sie", "tokens": ["Seit\u00b7dem", "sie", "meynt", ",", "und", "wir", "mit", "ihr", ":", "sie", "hei\u00b7\u00dfe", "und", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "$,", "KON", "PPER", "APPR", "PPOSAT", "$.", "PPER", "VVFIN", "KON", "PPER"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "sey Verstand.", "tokens": ["sey", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}