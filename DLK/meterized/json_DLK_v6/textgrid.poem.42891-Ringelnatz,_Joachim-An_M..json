{"textgrid.poem.42891": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An M.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du meine Wege mit mir gehst,", "tokens": ["Der", "du", "mei\u00b7ne", "We\u00b7ge", "mit", "mir", "gehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jede Laune meiner Wimper sp\u00fcrst,", "tokens": ["Je\u00b7de", "Lau\u00b7ne", "mei\u00b7ner", "Wim\u00b7per", "sp\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Schlechtigkeiten duldest und verstehst \u2013 \u2013.", "tokens": ["Mei\u00b7ne", "Schlech\u00b7tig\u00b7kei\u00b7ten", "dul\u00b7dest", "und", "ver\u00b7stehst", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$(", "$(", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wei\u00dft du wohl, wie hei\u00df du oft mich r\u00fchrst?", "tokens": ["Wei\u00dft", "du", "wohl", ",", "wie", "hei\u00df", "du", "oft", "mich", "r\u00fchrst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich tot bin, darfst du gar nicht trauern.", "tokens": ["Wenn", "ich", "tot", "bin", ",", "darfst", "du", "gar", "nicht", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Liebe wird mich \u00fcberdauern", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "wird", "mich", "\u00fc\u00b7berd\u00b7au\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und in fremden Kleidern dir begegnen", "tokens": ["Und", "in", "frem\u00b7den", "Klei\u00b7dern", "dir", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dich segnen.", "tokens": ["Und", "dich", "seg\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Lebe, lache gut!", "tokens": ["Le\u00b7be", ",", "la\u00b7che", "gut", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mache deine Sache gut!", "tokens": ["Ma\u00b7che", "dei\u00b7ne", "Sa\u00b7che", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}