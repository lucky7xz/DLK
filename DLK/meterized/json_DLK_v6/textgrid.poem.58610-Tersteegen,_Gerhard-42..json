{"textgrid.poem.58610": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mancher denkt, er stehe wohl, wenn ihn Gottes Gnad' erquicket;", "tokens": ["Man\u00b7cher", "denkt", ",", "er", "ste\u00b7he", "wohl", ",", "wenn", "ihn", "Got\u00b7tes", "Gnad'", "er\u00b7quic\u00b7ket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Mancher, der im Leiden ist,", "tokens": ["Man\u00b7cher", ",", "der", "im", "Lei\u00b7den", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinet, da\u00df Gott sein vergi\u00dft.", "tokens": ["Mei\u00b7net", ",", "da\u00df", "Gott", "sein", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NN", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcrres Zion, sei vergn\u00fcgt, was Gott nimmt und was er schicket;", "tokens": ["D\u00fcr\u00b7res", "Zi\u00b7on", ",", "sei", "ver\u00b7gn\u00fcgt", ",", "was", "Gott", "nimmt", "und", "was", "er", "schi\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "VVPP", "$,", "PRELS", "NN", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Du mu\u00dft lernen lieben rein,", "tokens": ["Du", "mu\u00dft", "ler\u00b7nen", "lie\u00b7ben", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht die Gaben, Gott allein!", "tokens": ["Nicht", "die", "Ga\u00b7ben", ",", "Gott", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er, Gott selbst, verl\u00e4\u00dft dich nicht, er kann deiner nie vergessen;", "tokens": ["Er", ",", "Gott", "selbst", ",", "ver\u00b7l\u00e4\u00dft", "dich", "nicht", ",", "er", "kann", "dei\u00b7ner", "nie", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ADV", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VMFIN", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Deine Seel' in seiner Hand", "tokens": ["Dei\u00b7ne", "Seel'", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist ihm gar zu wohl bekannt.", "tokens": ["Ist", "ihm", "gar", "zu", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Kreuzeskinder liebt sein Herz mit Erbarmen unermessen;", "tokens": ["Kreu\u00b7zes\u00b7kin\u00b7der", "liebt", "sein", "Herz", "mit", "Er\u00b7bar\u00b7men", "un\u00b7er\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Denk nur, du, an Gott allein,", "tokens": ["Denk", "nur", ",", "du", ",", "an", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "$,", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Er wird nicht vergessen dein!", "tokens": ["Er", "wird", "nicht", "ver\u00b7ges\u00b7sen", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVFIN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}