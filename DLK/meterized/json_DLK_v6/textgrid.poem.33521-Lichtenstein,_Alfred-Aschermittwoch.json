{"textgrid.poem.33521": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Aschermittwoch", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesten noch ging ich gepudert und s\u00fcchtig", "tokens": ["Ges\u00b7ten", "noch", "ging", "ich", "ge\u00b7pu\u00b7dert", "und", "s\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "VVPP", "KON", "ADJD"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "In der vielbunten t\u00f6nenden Welt.", "tokens": ["In", "der", "viel\u00b7bun\u00b7ten", "t\u00f6\u00b7nen\u00b7den", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Heute ist alles schon lange ersoffen.", "tokens": ["Heu\u00b7te", "ist", "al\u00b7les", "schon", "lan\u00b7ge", "er\u00b7sof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier ist ein Ding.", "tokens": ["Hier", "ist", "ein", "Ding", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dort ist ein Ding.", "tokens": ["Dort", "ist", "ein", "Ding", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Etwas sieht so aus.", "tokens": ["Et\u00b7was", "sieht", "so", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Etwas sieht anders aus.", "tokens": ["Et\u00b7was", "sieht", "an\u00b7ders", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wie leicht pustet einer die ganze", "tokens": ["Wie", "leicht", "pus\u00b7tet", "ei\u00b7ner", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Bl\u00fchende Erde aus.", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Er\u00b7de", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der Himmel ist kalt und blau.", "tokens": ["Der", "Him\u00b7mel", "ist", "kalt", "und", "blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Oder der Mond ist gelb und platt.", "tokens": ["O\u00b7der", "der", "Mond", "ist", "gelb", "und", "platt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Wald hat viele einzelne B\u00e4ume.", "tokens": ["Ein", "Wald", "hat", "vie\u00b7le", "ein\u00b7zel\u00b7ne", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ist nichts mehr zum Weinen.", "tokens": ["Ist", "nichts", "mehr", "zum", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist nichts mehr zum Schreien.", "tokens": ["Ist", "nichts", "mehr", "zum", "Schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wo bin ich \u2013", "tokens": ["Wo", "bin", "ich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$("], "meter": "+--", "measure": "dactylic.init"}}}}}