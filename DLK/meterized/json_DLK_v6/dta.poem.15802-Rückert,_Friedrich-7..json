{"dta.poem.15802": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken kalt und grau, die dich am Tag geh\u00e4rmt,", "tokens": ["Die", "Wol\u00b7ken", "kalt", "und", "grau", ",", "die", "dich", "am", "Tag", "ge\u00b7h\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Haben am Abend dich mit farb'gem Trost erw\u00e4rmt.", "tokens": ["Ha\u00b7ben", "am", "A\u00b7bend", "dich", "mit", "fa\u00b7rb'\u00b7gem", "Trost", "er\u00b7w\u00e4rmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Wolken graukalt sind nun rosig angegl\u00fcht;", "tokens": ["Die", "Wol\u00b7ken", "grau\u00b7kalt", "sind", "nun", "ro\u00b7sig", "an\u00b7ge\u00b7gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sch\u00f6n w\u00e4r' ohne sie kein Abendrot erbl\u00fcht.", "tokens": ["So", "sch\u00f6n", "w\u00e4r'", "oh\u00b7ne", "sie", "kein", "A\u00b7ben\u00b7drot", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-