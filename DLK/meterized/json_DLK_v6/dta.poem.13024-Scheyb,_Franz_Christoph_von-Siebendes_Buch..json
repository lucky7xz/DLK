{"dta.poem.13024": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Staats-Kunst ruhte nicht, sie hob ein Buch empor,", "tokens": ["Die", "Staats\u00b7Kunst", "ruh\u00b7te", "nicht", ",", "sie", "hob", "ein", "Buch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kam uns, ihren Zorn mit List verbergend, vor.", "tokens": ["Und", "kam", "uns", ",", "ih\u00b7ren", "Zorn", "mit", "List", "ver\u00b7ber\u00b7gend", ",", "vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem sie theils entflammt, theils freundlich wiedersezte:", "tokens": ["In\u00b7dem", "sie", "theils", "ent\u00b7flammt", ",", "theils", "freund\u00b7lich", "wie\u00b7der\u00b7sez\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie\u00df ist das Buch, worein ich meinen Rathschlag ezte.", "tokens": ["\u201e", "die\u00df", "ist", "das", "Buch", ",", "wo\u00b7rein", "ich", "mei\u00b7nen", "Rath\u00b7schlag", "ez\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "145(Sie schlug, indem sie sprach, ein, zwey Mahl auf das Buch)", "tokens": ["(", "Sie", "schlug", ",", "in\u00b7dem", "sie", "sprach", ",", "ein", ",", "zwey", "Mahl", "auf", "das", "Buch", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKVZ", "$,", "CARD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "\u201ehierinnen findet ihr der Frage Schlu\u00df und Spruch!", "tokens": ["\u201e", "hie\u00b7rin\u00b7nen", "fin\u00b7det", "ihr", "der", "Fra\u00b7ge", "Schlu\u00df", "und", "Spruch", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ehierinnen steht die Macht, das Trieb-Werck aufgeschrieben,", "tokens": ["\u201e", "hie\u00b7rin\u00b7nen", "steht", "die", "Macht", ",", "das", "Trie\u00b7b\u00b7Werck", "auf\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "\u201edurch welches man des Feinds Bestreben aufgerieben.", "tokens": ["\u201e", "durch", "wel\u00b7ches", "man", "des", "Feinds", "Be\u00b7stre\u00b7ben", "auf\u00b7ge\u00b7rie\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PIS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edie\u00df ist das Staats-Gesez, der Kriegs- und Friedens-Schild,", "tokens": ["\u201e", "die\u00df", "ist", "das", "Staats\u00b7Ge\u00b7sez", ",", "der", "Kriegs", "und", "Frie\u00b7dens\u00b7Schild", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "150\u201dDen ich der K\u00f6niginn im Streit vor Augen hielt.", "tokens": ["\"", "Den", "ich", "der", "K\u00f6\u00b7ni\u00b7ginn", "im", "Streit", "vor", "Au\u00b7gen", "hielt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "PPER", "ART", "NN", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}