{"dta.poem.2790": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Passions-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach JEsu! soll mein Hertz nicht brennen/", "tokens": ["Ach", "Je\u00b7su", "!", "soll", "mein", "Hertz", "nicht", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und voll erhitzter Andacht gluhn?", "tokens": ["Und", "voll", "er\u00b7hitz\u00b7ter", "An\u00b7dacht", "gluhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geist zu deinem Leiden rennen/", "tokens": ["Der", "Geist", "zu", "dei\u00b7nem", "Lei\u00b7den", "ren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sein verwe\u00dflich Kleid ausziehn/", "tokens": ["Und", "sein", "ver\u00b7we\u00df\u00b7lich", "Kleid", "aus\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein unaussprechlich Angst und Marter zu erw\u00e4gen?", "tokens": ["Dein", "un\u00b7aus\u00b7sprech\u00b7lich", "Angst", "und", "Mar\u00b7ter", "zu", "er\u00b7w\u00e4\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach ja ich bin bereit/", "tokens": ["Ach", "ja", "ich", "bin", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In tieffer Traurigkeit/", "tokens": ["In", "tief\u00b7fer", "Trau\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mich f\u00fcr dein heilig Creutz/ mein Heyland/ hinzulegen.", "tokens": ["Mich", "f\u00fcr", "dein", "hei\u00b7lig", "Creutz", "/", "mein", "Hey\u00b7land", "/", "hin\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJD", "NN", "$(", "PPOSAT", "NN", "$(", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den alle Himmel nicht umbfassen/", "tokens": ["Den", "al\u00b7le", "Him\u00b7mel", "nicht", "umb\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Schlie\u00df ich in Glaubens-Armen ein/", "tokens": ["Schlie\u00df", "ich", "in", "Glau\u00b7bens\u00b7Ar\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie k\u00f6nt ich diesen Schatz verlassen/", "tokens": ["Wie", "k\u00f6nt", "ich", "die\u00b7sen", "Schatz", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der mich befreyt der H\u00f6llen-Pein.", "tokens": ["Der", "mich", "be\u00b7freyt", "der", "H\u00f6l\u00b7len\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich flieh in meiner Angst gedruckt von schweren S\u00fcnden/", "tokens": ["Ich", "flieh", "in", "mei\u00b7ner", "Angst", "ge\u00b7druckt", "von", "schwe\u00b7ren", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In deine Seiten H\u00f6l/", "tokens": ["In", "dei\u00b7ne", "Sei\u00b7ten", "H\u00f6l", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "La\u00df/ JEsu/ meine Seel", "tokens": ["La\u00df", "/", "Je\u00b7su", "/", "mei\u00b7ne", "Seel"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "$(", "NE", "$(", "PPOSAT", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Errettung von dem Tod und s\u00fcsse Labsal finden.", "tokens": ["Er\u00b7ret\u00b7tung", "von", "dem", "Tod", "und", "s\u00fcs\u00b7se", "Lab\u00b7sal", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich zittre zwar/ wenn ich bedencke", "tokens": ["Ich", "zitt\u00b7re", "zwar", "/", "wenn", "ich", "be\u00b7den\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der bittren Marter Grimmigkeit.", "tokens": ["Der", "bit\u00b7tren", "Mar\u00b7ter", "Grim\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Du stirbst/ und gibst mir zum Geschencke", "tokens": ["Du", "stirbst", "/", "und", "gibst", "mir", "zum", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das Heyl der wahren Ewigkeit.", "tokens": ["Das", "Heyl", "der", "wah\u00b7ren", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Man kr\u00f6nt mit Dornen dich/ da\u00df ich kan Blumen tragen.", "tokens": ["Man", "kr\u00f6nt", "mit", "Dor\u00b7nen", "dich", "/", "da\u00df", "ich", "kan", "Blu\u00b7men", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PPER", "$(", "KOUS", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du f\u00e4llst/ ich ward erh\u00f6ht.", "tokens": ["Du", "f\u00e4llst", "/", "ich", "ward", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Auf da\u00df der Mensch besteht/", "tokens": ["Auf", "da\u00df", "der", "Mensch", "be\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "So l\u00e4st du Dich f\u00fcr ihn mit Ruth\u2019 und Geisseln schlagen.", "tokens": ["So", "l\u00e4st", "du", "Dich", "f\u00fcr", "ihn", "mit", "Ruth'", "und", "Geis\u00b7seln", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie milde flo\u00df die Purpur-Quelle/", "tokens": ["Wie", "mil\u00b7de", "flo\u00df", "die", "Pur\u00b7pur\u00b7Quel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zu tilgen meiner Flecken Mahl.", "tokens": ["Zu", "til\u00b7gen", "mei\u00b7ner", "Fle\u00b7cken", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nun f\u00fcrcht ich weder Tod noch H\u00f6lle/", "tokens": ["Nun", "f\u00fcrcht", "ich", "we\u00b7der", "Tod", "noch", "H\u00f6l\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Noch aller Marter Angst und Quaal.", "tokens": ["Noch", "al\u00b7ler", "Mar\u00b7ter", "Angst", "und", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die Liebe zwischen mir und dir ist nicht zu trennen.", "tokens": ["Die", "Lie\u00b7be", "zwi\u00b7schen", "mir", "und", "dir", "ist", "nicht", "zu", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "KON", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du bist mein h\u00f6chstes Gut/", "tokens": ["Du", "bist", "mein", "h\u00f6chs\u00b7tes", "Gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Und deine Liebes-Glut", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7bes\u00b7Glut"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Soll stets in meiner Seel als eine Kertze brennen.", "tokens": ["Soll", "stets", "in", "mei\u00b7ner", "Seel", "als", "ei\u00b7ne", "Kert\u00b7ze", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wenn meine Augen einst verfallen/", "tokens": ["Wenn", "mei\u00b7ne", "Au\u00b7gen", "einst", "ver\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ach JEsu/ so erscheine mir!", "tokens": ["Ach", "Je\u00b7su", "/", "so", "er\u00b7schei\u00b7ne", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "So lang ich auf der Welt mu\u00df wallen/", "tokens": ["So", "lang", "ich", "auf", "der", "Welt", "mu\u00df", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "So leb ich gleichfals eintzig dir.", "tokens": ["So", "leb", "ich", "gleich\u00b7fals", "eint\u00b7zig", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ich \u00fcberliefer auch den Geist in deine H\u00e4nde.", "tokens": ["Ich", "\u00fc\u00b7berl\u00b7ie\u00b7fer", "auch", "den", "Geist", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Den Leib bedeckt ein Klo\u00df.", "tokens": ["Den", "Leib", "be\u00b7deckt", "ein", "Klo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Ich sitz in deiner Schos/", "tokens": ["Ich", "sitz", "in", "dei\u00b7ner", "Schos", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Und preise Gottes Treu und G\u00fcte sonder Ende.", "tokens": ["Und", "prei\u00b7se", "Got\u00b7tes", "Treu", "und", "G\u00fc\u00b7te", "son\u00b7der", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}