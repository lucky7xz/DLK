{"textgrid.poem.43689": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir V\u00f6gel haben's wahrlich gut,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir V\u00f6gel haben's wahrlich gut,", "tokens": ["Wir", "V\u00f6\u00b7gel", "ha\u00b7ben's", "wahr\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir fliegen, h\u00fcpfen, singen.", "tokens": ["Wir", "flie\u00b7gen", ",", "h\u00fcp\u00b7fen", ",", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir singen frisch und wohlgemuth,", "tokens": ["Wir", "sin\u00b7gen", "frisch", "und", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wald und Feld erklingen.", "tokens": ["Das", "Wald", "und", "Feld", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sind gesund und sorgenfrei,", "tokens": ["Wir", "sind", "ge\u00b7sund", "und", "sor\u00b7gen\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und finden, was uns schmecket;", "tokens": ["Und", "fin\u00b7den", ",", "was", "uns", "schme\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohin wir fliegen, wo's auch sei,", "tokens": ["Wo\u00b7hin", "wir", "flie\u00b7gen", ",", "wo's", "auch", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist unser Tisch gedecket.", "tokens": ["Ist", "un\u00b7ser", "Tisch", "ge\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ist unser Tagewerk vollbracht,", "tokens": ["Ist", "un\u00b7ser", "Ta\u00b7ge\u00b7werk", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann zieh'n wir in die B\u00e4ume,", "tokens": ["Dann", "zieh'n", "wir", "in", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir ruhen still und sanft die Nacht", "tokens": ["Wir", "ru\u00b7hen", "still", "und", "sanft", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und haben s\u00fc\u00dfe Tr\u00e4ume.", "tokens": ["Und", "ha\u00b7ben", "s\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und weckt uns fr\u00fch der Sonnenschein,", "tokens": ["Und", "weckt", "uns", "fr\u00fch", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann schwingen wir's Gefieder,", "tokens": ["Dann", "schwin\u00b7gen", "wir's", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir fliegen in die Welt hinein", "tokens": ["Wir", "flie\u00b7gen", "in", "die", "Welt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singen unsre Lieder.", "tokens": ["Und", "sin\u00b7gen", "uns\u00b7re", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}