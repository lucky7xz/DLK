{"dta.poem.10173": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der schimmernde Schnee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war die starre Welt im Eis' annoch verstecket;", "tokens": ["Es", "war", "die", "star\u00b7re", "Welt", "im", "Eis'", "an\u00b7noch", "ver\u00b7ste\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es hatte sie annoch der weisse Schnee bedecket,", "tokens": ["Es", "hat\u00b7te", "sie", "an\u00b7noch", "der", "weis\u00b7se", "Schnee", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Als welcher \u00fcberall noch lag;", "tokens": ["Als", "wel\u00b7cher", "\u00fc\u00b7be\u00b7rall", "noch", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich an einem heitern Tag\u2019,", "tokens": ["Da", "ich", "an", "ei\u00b7nem", "hei\u00b7tern", "Tag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um auch, zu GOTTES Ruhm, an stat des gr\u00fcnen,", "tokens": ["Um", "auch", ",", "zu", "GoT\u00b7TES", "Ruhm", ",", "an", "stat", "des", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "$,", "APPR", "NE", "NN", "$,", "APPR", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des weissen Schmucks der Welt mich fr\u00f6hlich zu bedienen.", "tokens": ["Des", "weis\u00b7sen", "Schmucks", "der", "Welt", "mich", "fr\u00f6h\u00b7lich", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich auf das Land verf\u00fcgte:", "tokens": ["Mich", "auf", "das", "Land", "ver\u00b7f\u00fcg\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Woselbst ich tausend Dinge fand,", "tokens": ["Wo\u00b7selbst", "ich", "tau\u00b7send", "Din\u00b7ge", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "An welchen ich mich sehr vergn\u00fcgte.", "tokens": ["An", "wel\u00b7chen", "ich", "mich", "sehr", "ver\u00b7gn\u00fcg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es zeigt der weisse Schnee, der duncklen Aeste,", "tokens": ["Es", "zeigt", "der", "weis\u00b7se", "Schnee", ",", "der", "dunck\u00b7len", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verworrne, starre, krumme Kn\u00e4ste,", "tokens": ["Ver\u00b7worr\u00b7ne", ",", "star\u00b7re", ",", "krum\u00b7me", "Kn\u00e4s\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Viel deutlicher, als sonst. Das aller kleinste Reis", "tokens": ["Viel", "deut\u00b7li\u00b7cher", ",", "als", "sonst", ".", "Das", "al\u00b7ler", "kleins\u00b7te", "Reis"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "$.", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erscheinet ietzt, da sonsten alles wei\u00df,", "tokens": ["Er\u00b7schei\u00b7net", "ietzt", ",", "da", "sons\u00b7ten", "al\u00b7les", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch seine braune Dunckelheit,", "tokens": ["Durch", "sei\u00b7ne", "brau\u00b7ne", "Dun\u00b7ckel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da es nicht gantz bedeckt, in solcher Deutlichkeit,", "tokens": ["Da", "es", "nicht", "gantz", "be\u00b7deckt", ",", "in", "sol\u00b7cher", "Deut\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df alle B\u00e4ume ietzt an Zweigen reicher scheinen:", "tokens": ["Da\u00df", "al\u00b7le", "B\u00e4u\u00b7me", "ietzt", "an", "Zwei\u00b7gen", "rei\u00b7cher", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dadurch sieht alles rauch und kraus,", "tokens": ["Da\u00b7durch", "sieht", "al\u00b7les", "rauch", "und", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verwirrt und wild, und dennoch lieblich aus.", "tokens": ["Ver\u00b7wirrt", "und", "wild", ",", "und", "den\u00b7noch", "lieb\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "KON", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn in der falben d\u00fcstern Lufft", "tokens": ["Denn", "in", "der", "fal\u00b7ben", "d\u00fcs\u00b7tern", "Lufft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint ieder Wipfel ietzt", "tokens": ["Scheint", "ie\u00b7der", "Wip\u00b7fel", "ietzt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Silber-wei\u00df Gew\u00fclck, ein heller Dufft.", "tokens": ["Ein", "Sil\u00b7ber\u00b7wei\u00df", "Ge\u00b7w\u00fclck", ",", "ein", "hel\u00b7ler", "Dufft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Taxus, der bereifft, scheint nicht nur ieder Ast,", "tokens": ["Am", "Ta\u00b7xus", ",", "der", "be\u00b7re\u00b7ifft", ",", "scheint", "nicht", "nur", "ie\u00b7der", "Ast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So gar ein iedes Blat, in Silber eingefasst.", "tokens": ["So", "gar", "ein", "ie\u00b7des", "Blat", ",", "in", "Sil\u00b7ber", "ein\u00b7ge\u00b7fasst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er gleicht dadurch, da\u00df gr\u00fcn und wei\u00df so s\u00fc\u00df sich mischen,", "tokens": ["Er", "gleicht", "da\u00b7durch", ",", "da\u00df", "gr\u00fcn", "und", "wei\u00df", "so", "s\u00fc\u00df", "sich", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von weitem, gr\u00fcn-und weissen Feder-B\u00fcschen.", "tokens": ["Von", "wei\u00b7tem", ",", "gr\u00fcn\u00b7\u00b7und", "weis\u00b7sen", "Fe\u00b7der\u00b7B\u00fc\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier sencket sich das Licht in rauhen Schnee hinein,", "tokens": ["Hier", "sen\u00b7cket", "sich", "das", "Licht", "in", "rau\u00b7hen", "Schnee", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeugt ein reines wei\u00df; dort sieht mans r\u00fcckw\u00e4rts prallen,", "tokens": ["Und", "zeugt", "ein", "rei\u00b7nes", "wei\u00df", ";", "dort", "sieht", "mans", "r\u00fcck\u00b7w\u00e4rts", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, weil die Fl\u00e4che glatt, mit einem hellen Schein,", "tokens": ["Und", ",", "weil", "die", "Fl\u00e4\u00b7che", "glatt", ",", "mit", "ei\u00b7nem", "hel\u00b7len", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wie von klaren Berg-Crystallen,", "tokens": ["Als", "wie", "von", "kla\u00b7ren", "Ber\u00b7gCry\u00b7stal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns blendend in die Augen fallen:", "tokens": ["Uns", "blen\u00b7dend", "in", "die", "Au\u00b7gen", "fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wozwischen \u00f6ffters kleine H\u00f6hen,", "tokens": ["Woz\u00b7wi\u00b7schen", "\u00f6ff\u00b7ters", "klei\u00b7ne", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die, weil der Schnee von ihnen abgeleckt", "tokens": ["Die", ",", "weil", "der", "Schnee", "von", "ih\u00b7nen", "ab\u00b7ge\u00b7leckt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und abgeschmoltzen war, entdeckt,", "tokens": ["Und", "ab\u00b7ge\u00b7schmolt\u00b7zen", "war", ",", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bald schwartz, bald gr\u00fcnlich braun zu sehen,", "tokens": ["Bald", "schwartz", ",", "bald", "gr\u00fcn\u00b7lich", "braun", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von welchem Kraut und Gras, so noch auf ihnen stund.", "tokens": ["Von", "wel\u00b7chem", "Kraut", "und", "Gras", ",", "so", "noch", "auf", "ih\u00b7nen", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da alles denn, in einer wilden Pracht,", "tokens": ["Da", "al\u00b7les", "denn", ",", "in", "ei\u00b7ner", "wil\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Durch die Ver\u00e4nderung, die Landschaft dennoch bunt,", "tokens": ["Durch", "die", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", ",", "die", "Land\u00b7schaft", "den\u00b7noch", "bunt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und, in dem Wechsel, lieblich macht.", "tokens": ["Und", ",", "in", "dem", "Wech\u00b7sel", ",", "lieb\u00b7lich", "macht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich gieng im Garten auf und nieder,", "tokens": ["Ich", "gieng", "im", "Gar\u00b7ten", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00fcckt auf hartem Schnee die Fu\u00df-Spur hin und wieder,", "tokens": ["Und", "dr\u00fcckt", "auf", "har\u00b7tem", "Schnee", "die", "Fu\u00df\u00b7Spur", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit sanftem knirschen, ein.", "tokens": ["Mit", "sanf\u00b7tem", "knir\u00b7schen", ",", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ferne zwar annoch, doch unbew\u00f6lckte Sonne", "tokens": ["Die", "fer\u00b7ne", "zwar", "an\u00b7noch", ",", "doch", "un\u00b7be\u00b7w\u00f6lck\u00b7te", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADV", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bestrahlte Schnee und Eis mit einem hellen Schein,", "tokens": ["Be\u00b7strahl\u00b7te", "Schnee", "und", "Eis", "mit", "ei\u00b7nem", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meinen Geist mit ungemeiner Wonne.", "tokens": ["Und", "mei\u00b7nen", "Geist", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie gl\u00e4ntzte, blitzt\u2019 und funckelte", "tokens": ["Wie", "gl\u00e4ntz\u00b7te", ",", "blitzt'", "und", "fun\u00b7ckel\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der angestrahlte Schnee!", "tokens": ["Der", "an\u00b7ge\u00b7strahl\u00b7te", "Schnee", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum siehet man so klar, so rein,", "tokens": ["Kaum", "sie\u00b7het", "man", "so"