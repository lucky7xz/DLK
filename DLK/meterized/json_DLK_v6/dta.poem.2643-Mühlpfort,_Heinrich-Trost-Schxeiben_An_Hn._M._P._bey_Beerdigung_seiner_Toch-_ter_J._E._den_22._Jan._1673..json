{"dta.poem.2643": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Schxeiben  \n An Hn. M. P. bey Beerdigung seiner Toch-  \n ter J. E. den 22. Jan. 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich/ geehrter Freund/ sein Schreiben nur erblicket/", "tokens": ["Als", "ich", "/", "ge\u00b7ehr\u00b7ter", "Freund", "/", "sein", "Schrei\u00b7ben", "nur", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ADJA", "NN", "$(", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah ich das schwartze Wachs/ den Boten grosser Pein/", "tokens": ["Sah", "ich", "das", "schwart\u00b7ze", "Wachs", "/", "den", "Bo\u00b7ten", "gros\u00b7ser", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie ich weiter noch im lesen fortger\u00fccket/", "tokens": ["Und", "wie", "ich", "wei\u00b7ter", "noch", "im", "le\u00b7sen", "fort\u00b7ge\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "APPRART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Traff mein Gedencken auch mit dem Verh\u00e4ngn\u00fc\u00df ein.", "tokens": ["Traff", "mein", "Ge\u00b7den\u00b7cken", "auch", "mit", "dem", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Briefes Inhalt war: Mein Leitstern ist verblichen/", "tokens": ["Des", "Brie\u00b7fes", "In\u00b7halt", "war", ":", "Mein", "Leits\u00b7tern", "ist", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$.", "PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Eltern liebster Trost und Hoffnung f\u00e4llt dahin/", "tokens": ["Der", "El\u00b7tern", "liebs\u00b7ter", "Trost", "und", "Hoff\u00b7nung", "f\u00e4llt", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun unser Tochter Treu aus unsrem Haus entwichen/", "tokens": ["Nun", "un\u00b7ser", "Toch\u00b7ter", "Treu", "aus", "uns\u00b7rem", "Haus", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird mit des Traurens Nacht umbzogen Hertz und Sinn.", "tokens": ["Wird", "mit", "des", "Trau\u00b7rens", "Nacht", "um\u00b7bzo\u00b7gen", "Hertz", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Schmertz saugt Safft und Krafft aus Adern und Gebeinen/", "tokens": ["Der", "Schmertz", "saugt", "Safft", "und", "Krafft", "aus", "A\u00b7dern", "und", "Ge\u00b7bei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df ich vor Leid ihr kaum die letzte Pflicht kan thun;", "tokens": ["Da\u00df", "ich", "vor", "Leid", "ihr", "kaum", "die", "letz\u00b7te", "Pflicht", "kan", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich bitte mir mit Trost gewierig zu erscheinen/", "tokens": ["Ich", "bit\u00b7te", "mir", "mit", "Trost", "ge\u00b7wie\u00b7rig", "zu", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der meinen Jammer stillt/ und heist die Thr\u00e4nen ruhn.", "tokens": ["Der", "mei\u00b7nen", "Jam\u00b7mer", "stillt", "/", "und", "heist", "die", "Thr\u00e4\u00b7nen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja freilich ", "tokens": ["Ja", "frei\u00b7lich"], "token_info": ["word", "word"], "pos": ["PTKANT", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Von einem solchen Brunn der aus dem Hertzen quillt/", "tokens": ["Von", "ei\u00b7nem", "sol\u00b7chen", "Brunn", "der", "aus", "dem", "Hert\u00b7zen", "quillt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die nicht allein den Leib die Seele selbst beschweren/", "tokens": ["Die", "nicht", "al\u00b7lein", "den", "Leib", "die", "See\u00b7le", "selbst", "be\u00b7schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die auch kein Barbar nicht/ wie hart er sonsten/ schillt.", "tokens": ["Die", "auch", "kein", "Bar\u00b7bar", "nicht", "/", "wie", "hart", "er", "sons\u00b7ten", "/", "schillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "PTKNEG", "$(", "PWAV", "ADJD", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn/ seiner Kinder Tod mit trocknen Augen schauen/", "tokens": ["Denn", "/", "sei\u00b7ner", "Kin\u00b7der", "Tod", "mit", "trock\u00b7nen", "Au\u00b7gen", "schau\u00b7en", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und umb derselben Bahr\u2019 ohn\u2019 ein\u2019 ge Regung stehn/", "tokens": ["Und", "umb", "der\u00b7sel\u00b7ben", "Bahr'", "ohn'", "ein'", "ge", "Re\u00b7gung", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist fast der Menschligkeit unm\u00f6glich zuzutrauen/", "tokens": ["Ist", "fast", "der", "Mensc\u00b7hlig\u00b7keit", "un\u00b7m\u00f6g\u00b7lich", "zu\u00b7zu\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es w\u00fcrde der Natur nur stracks zu wieder gehn.", "tokens": ["Es", "w\u00fcr\u00b7de", "der", "Na\u00b7tur", "nur", "stracks", "zu", "wie\u00b7der", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "PTKA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hingegen nutzt auch nicht ein unauffh\u00f6rlich Weinen/", "tokens": ["Hin\u00b7ge\u00b7gen", "nutzt", "auch", "nicht", "ein", "un\u00b7auff\u00b7h\u00f6r\u00b7lich", "Wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ART", "ADJD", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Noch da\u00df man vor der Zeit im Kummer sich vergr\u00e4bt;", "tokens": ["Noch", "da\u00df", "man", "vor", "der", "Zeit", "im", "Kum\u00b7mer", "sich", "ver\u00b7gr\u00e4bt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "APPR", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hier mu\u00df das Christenthum vor allem Leid erscheinen/", "tokens": ["Hier", "mu\u00df", "das", "Chris\u00b7ten\u00b7thum", "vor", "al\u00b7lem", "Leid", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PIS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So uns versichert macht/ da\u00df einst der Todte lebt.", "tokens": ["So", "uns", "ver\u00b7si\u00b7chert", "macht", "/", "da\u00df", "einst", "der", "Tod\u00b7te", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man pflegt mit h\u00f6chstem Flei\u00df was sch\u00e4tzbar zu verwahren.", "tokens": ["Man", "pflegt", "mit", "h\u00f6chs\u00b7tem", "Flei\u00df", "was", "sch\u00e4tz\u00b7bar", "zu", "ver\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "PWS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie sicher hebt der Tod die m\u00fcrben ", "tokens": ["Wie", "si\u00b7cher", "hebt", "der", "Tod", "die", "m\u00fcr\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wir leben hier umschrenckt mit Aengsten und Gefahren/", "tokens": ["Wir", "le\u00b7ben", "hier", "um\u00b7schrenckt", "mit", "A\u00b7engs\u00b7ten", "und", "Ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Der/ in der Erde ruht/ f\u00fchlt nicht der Zeiten Lauff.", "tokens": ["Der", "/", "in", "der", "Er\u00b7de", "ruht", "/", "f\u00fchlt", "nicht", "der", "Zei\u00b7ten", "Lauff", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und t\u00e4glich sehen wir von Kr\u00e4fften was verschwinden/", "tokens": ["Und", "t\u00e4g\u00b7lich", "se\u00b7hen", "wir", "von", "Kr\u00e4ff\u00b7ten", "was", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Gleichwie ein altes Kleid der Wurm und Moder frist/", "tokens": ["Gleich\u00b7wie", "ein", "al\u00b7tes", "Kleid", "der", "Wurm", "und", "Mo\u00b7der", "frist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bald mangelts uns am Blut/ bald ists zu viel zu finden/", "tokens": ["Bald", "man\u00b7gelts", "uns", "am", "Blut", "/", "bald", "ists", "zu", "viel", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$(", "ADV", "VAFIN", "PTKA", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bald wird am Haupte St\u00e4rck\u2019/ umbs Hertze Lufft vermist.", "tokens": ["Bald", "wird", "am", "Haup\u00b7te", "St\u00e4rck", "/", "umbs", "Hert\u00b7ze", "Lufft", "ver\u00b7mist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "NE", "$(", "NE", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und di\u00df nicht nur allein. Ach wie viel tausend F\u00e4llen", "tokens": ["Und", "di\u00df", "nicht", "nur", "al\u00b7lein", ".", "Ach", "wie", "viel", "tau\u00b7send", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PTKNEG", "ADV", "ADV", "$.", "NN", "KOKOM", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mu\u00df hier der arme Mensch stets unterworffen seyn!", "tokens": ["Mu\u00df", "hier", "der", "ar\u00b7me", "Mensch", "stets", "un\u00b7ter\u00b7worf\u00b7fen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der stirbt in Feuers-Noth/ ein ander in den Wellen/", "tokens": ["Der", "stirbt", "in", "Feu\u00b7er\u00b7sNoth", "/", "ein", "an\u00b7der", "in", "den", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$(", "ART", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und mit dem Athem zeucht man offt den Tod auch ein.", "tokens": ["Und", "mit", "dem", "A\u00b7them", "zeucht", "man", "offt", "den", "Tod", "auch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Weysen m\u00fchen sich das Leben abzubilden/", "tokens": ["Die", "Wey\u00b7sen", "m\u00fc\u00b7hen", "sich", "das", "Le\u00b7ben", "ab\u00b7zu\u00b7bil\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Varro stelt es uns in Wasser-Blasen f\u00fcr.", "tokens": ["Der", "Var\u00b7ro", "stelt", "es", "uns", "in", "Was\u00b7ser\u00b7Bla\u00b7sen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Viel durch die Hiacinth in lustigen Gefilden", "tokens": ["Viel", "durch", "die", "Hia\u00b7cin\u00b7th", "in", "lus\u00b7ti\u00b7gen", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie sch\u00f6n er immer bl\u00fcht/ so fl\u00fcchtig ist die Zier.", "tokens": ["Wie", "sch\u00f6n", "er", "im\u00b7mer", "bl\u00fcht", "/", "so", "fl\u00fcch\u00b7tig", "ist", "die", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es nennets Epictet ein Licht vom Wind umbgeben/", "tokens": ["Es", "nen\u00b7nets", "E\u00b7pic\u00