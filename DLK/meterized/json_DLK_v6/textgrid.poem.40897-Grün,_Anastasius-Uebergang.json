{"textgrid.poem.40897": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Uebergang", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf eines Berges R\u00fccken hoch steht ein Cederbaum,", "tokens": ["Auf", "ei\u00b7nes", "Ber\u00b7ges", "R\u00fc\u00b7cken", "hoch", "steht", "ein", "Ce\u00b7der\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein zweiter zeigt den Blicken weitum sich in dem Raum,", "tokens": ["Kein", "zwei\u00b7ter", "zeigt", "den", "Bli\u00b7cken", "wei\u00b7tum", "sich", "in", "dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es schaut fern in die Lande des Riesen Kraftgestalt,", "tokens": ["Es", "schaut", "fern", "in", "die", "Lan\u00b7de", "des", "Rie\u00b7sen", "Kraft\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein Stamm: ein Berg am Berge, sein Laub: ein ganzer Wald.", "tokens": ["Sein", "Stamm", ":", "ein", "Berg", "am", "Ber\u00b7ge", ",", "sein", "Laub", ":", "ein", "gan\u00b7zer", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Tief in den Aether greift er mit gr\u00fcnen Armen empor,", "tokens": ["Tief", "in", "den", "A\u00b7e\u00b7ther", "greift", "er", "mit", "gr\u00fc\u00b7nen", "Ar\u00b7men", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Als wagt' er's anzupochen k\u00fchn an des Himmels Thor,", "tokens": ["Als", "wagt'", "er's", "an\u00b7zu\u00b7po\u00b7chen", "k\u00fchn", "an", "des", "Him\u00b7mels", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "VVIZU", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als Schleier nimmt er Wolken, die er im Flug geraubt,", "tokens": ["Als", "Schlei\u00b7er", "nimmt", "er", "Wol\u00b7ken", ",", "die", "er", "im", "Flug", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und setzt die goldne Sonne als Krone sich aufs Haupt.", "tokens": ["Und", "setzt", "die", "gold\u00b7ne", "Son\u00b7ne", "als", "Kro\u00b7ne", "sich", "aufs", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "KOUS", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Fr\u00fchroth, seinen Diener, sieht man zuerst ihm nahn,", "tokens": ["Das", "Fr\u00fch\u00b7roth", ",", "sei\u00b7nen", "Die\u00b7ner", ",", "sieht", "man", "zu\u00b7erst", "ihm", "nahn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "ADV", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Um mit dem Purpurmantel den Leib ihn zu umfahn;", "tokens": ["Um", "mit", "dem", "Pur\u00b7pur\u00b7man\u00b7tel", "den", "Leib", "ihn", "zu", "um\u00b7fahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und erst, wenn's mild beim Scheiden den letzten Gru\u00df ihm bot,", "tokens": ["Und", "erst", ",", "wenn's", "mild", "beim", "Schei\u00b7den", "den", "letz\u00b7ten", "Gru\u00df", "ihm", "bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Purpurs ihn zu entkleiden beginnt das Abendroth.", "tokens": ["Des", "Pur\u00b7purs", "ihn", "zu", "ent\u00b7klei\u00b7den", "be\u00b7ginnt", "das", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-++-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "So stehst auf deinem Berge, du stolzer Cederbaum,", "tokens": ["So", "stehst", "auf", "dei\u00b7nem", "Ber\u00b7ge", ",", "du", "stol\u00b7zer", "Ce\u00b7der\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gewaltig, herrlich, aber \u2013 allein im weiten Raum!", "tokens": ["Ge\u00b7wal\u00b7tig", ",", "herr\u00b7lich", ",", "a\u00b7ber", "\u2013", "al\u00b7lein", "im", "wei\u00b7ten", "Raum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADV", "$(", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So, F\u00fcrst, aufragst im Leben du kronumgl\u00e4nzter Mann'", "tokens": ["So", ",", "F\u00fcrst", ",", "auf\u00b7ragst", "im", "Le\u00b7ben", "du", "kro\u00b7num\u00b7gl\u00e4nz\u00b7ter", "Mann'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So standst auch du im Leben, Held Maximilian!", "tokens": ["So", "standst", "auch", "du", "im", "Le\u00b7ben", ",", "Held", "Ma\u00b7xi\u00b7mi\u00b7li\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Horch, majest\u00e4tisch rauschen der Ceder Zweig' empor,", "tokens": ["Horch", ",", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "rau\u00b7schen", "der", "Ce\u00b7der", "Zweig'", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So hehr ist's zu belauschen, wie ernster Geisterchor,", "tokens": ["So", "hehr", "ist's", "zu", "be\u00b7lau\u00b7schen", ",", "wie", "erns\u00b7ter", "Geis\u00b7ter\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PTKZU", "VVINF", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie eines Jahrhunderts Kunde so m\u00e4chtig rauscht's weitum,", "tokens": ["Wie", "ei\u00b7nes", "Jahr\u00b7hun\u00b7derts", "Kun\u00b7de", "so", "m\u00e4ch\u00b7tig", "rauscht's", "wei\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df feierlich in der Runde jed' andres Lied nun stumm.", "tokens": ["Da\u00df", "fei\u00b7er\u00b7lich", "in", "der", "Run\u00b7de", "jed'", "and\u00b7res", "Lied", "nun", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "So m\u00f6ge nun auch schweigen, o Max, mein Lied davon,", "tokens": ["So", "m\u00f6\u00b7ge", "nun", "auch", "schwei\u00b7gen", ",", "o", "Max", ",", "mein", "Lied", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "VVINF", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie du dein Volk gelenket von deinem Kaiserthron.", "tokens": ["Wie", "du", "dein", "Volk", "ge\u00b7len\u00b7ket", "von", "dei\u00b7nem", "Kai\u00b7ser\u00b7thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn wer auch wollte lauschen dem sch\u00fcchternen Gedicht,", "tokens": ["Denn", "wer", "auch", "woll\u00b7te", "lau\u00b7schen", "dem", "sch\u00fcch\u00b7ter\u00b7nen", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn eines ganzen Volkes Gejubel jauchzend spricht?", "tokens": ["Wenn", "ei\u00b7nes", "gan\u00b7zen", "Vol\u00b7kes", "Ge\u00b7ju\u00b7bel", "jauch\u00b7zend", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Saiten m\u00f6gen verschweigen manch hohe Herrscherthat,", "tokens": ["Die", "Sai\u00b7ten", "m\u00f6\u00b7gen", "ver\u00b7schwei\u00b7gen", "manch", "ho\u00b7he", "Herr\u00b7scher\u00b7that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie sich die V\u00f6lker neigen vor dir im V\u00f6lkerrath,", "tokens": ["Wie", "sich", "die", "V\u00f6l\u00b7ker", "nei\u00b7gen", "vor", "dir", "im", "V\u00f6l\u00b7ker\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie herrlich stolz du gl\u00e4nztest in der Juwelenkron',", "tokens": ["Wie", "herr\u00b7lich", "stolz", "du", "gl\u00e4nz\u00b7test", "in", "der", "Ju\u00b7we\u00b7len\u00b7kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wie doch stille Demut das sch\u00f6nste Juwel davon;", "tokens": ["Und", "wie", "doch", "stil\u00b7le", "De\u00b7mut", "das", "sch\u00f6ns\u00b7te", "Ju\u00b7wel", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und wie auch ", "tokens": ["Und", "wie", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Und doch kein Bl\u00e4ttlein kr\u00fcmmte vom Kranz, der es umschlo\u00df;", "tokens": ["Und", "doch", "kein", "Bl\u00e4tt\u00b7lein", "kr\u00fcmm\u00b7te", "vom", "Kranz", ",", "der", "es", "um\u00b7schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn oft mag mehr es wiegen, erworbne Lorbeern wahren,", "tokens": ["Denn", "oft", "mag", "mehr", "es", "wie\u00b7gen", ",", "er\u00b7worb\u00b7ne", "Lor\u00b7beern", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "PPER", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als zu den alten Siegen noch neue Kr\u00e4nze paaren.", "tokens": ["Als", "zu", "den", "al\u00b7ten", "Sie\u00b7gen", "noch", "neu\u00b7e", "Kr\u00e4n\u00b7ze", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Nicht bat'st du um die Krone zu Rom nach altem Brauch,", "tokens": ["Nicht", "bat'st", "du", "um", "die", "Kro\u00b7ne", "zu", "Rom", "nach", "al\u00b7tem", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u2013 Ha, sollte Cedern st\u00fctzen der Pfaffenk\u00e4ppleinstrauch? \u2013", "tokens": ["\u2013", "Ha", ",", "soll\u00b7te", "Ce\u00b7dern", "st\u00fct\u00b7zen", "der", "Pfaf\u00b7fen\u00b7k\u00e4pp\u00b7lein\u00b7strauch", "?", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VMFIN", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Du hast, den Blick nach oben, sie selbst aufs Haupt gepre\u00dft,", "tokens": ["Du", "hast", ",", "den", "Blick", "nach", "o\u00b7ben", ",", "sie", "selbst", "aufs", "Haupt", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "APPR", "ADV", "$,", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Himmel sprach den Segen, und sie stand sch\u00f6n und fest.", "tokens": ["Der", "Him\u00b7mel", "sprach", "den", "Se\u00b7gen", ",", "und", "sie", "stand", "sch\u00f6n", "und", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.10": {"line.1": {"text": "Doch, Bischof Roms! fest halte die eigne Tiar' am Haupt,", "tokens": ["Doch", ",", "Bi\u00b7schof", "Roms", "!", "fest", "hal\u00b7te", "die", "eig\u00b7ne", "Ti\u00b7a\u00b7r'", "am", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "NE", "$.", "ADJD", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Denn sieh, schon tos't der Windsto\u00df, der sie gar leicht dir raubt,", "tokens": ["Denn", "sieh", ",", "schon", "to\u00b7s't", "der", "Wind\u00b7sto\u00df", ",", "der", "sie", "gar", "leicht", "dir", "raubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Im Staub wird er sie rollen, nicht fern ist mehr die Zeit,", "tokens": ["Im", "Staub", "wird", "er", "sie", "rol\u00b7len", ",", "nicht", "fern", "ist", "mehr", "die", "Zeit", ","], "token_info": ["word", "wo