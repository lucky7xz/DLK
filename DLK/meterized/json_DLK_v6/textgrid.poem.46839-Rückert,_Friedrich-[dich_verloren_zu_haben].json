{"textgrid.poem.46839": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[dich verloren zu haben]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich verloren zu haben", "tokens": ["Dich", "ver\u00b7lo\u00b7ren", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "PTKZU", "VAINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ist wohl ein Verlust;", "tokens": ["Ist", "wohl", "ein", "Ver\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch gehabt dich zu haben,", "tokens": ["Doch", "ge\u00b7habt", "dich", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAPP", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Ist auch eine Lust.", "tokens": ["Ist", "auch", "ei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht gehabt dich zu haben,", "tokens": ["Nicht", "ge\u00b7habt", "dich", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAPP", "PPER", "PTKZU", "VAINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das w\u00e4r' ein Verlust.", "tokens": ["Das", "w\u00e4r'", "ein", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dich verloren zu haben", "tokens": ["Dich", "ver\u00b7lo\u00b7ren", "zu", "h