{"textgrid.poem.60866": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Odyssee", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich zum Begleiter empfehl ich dem Reisenden; aber vor Allem,", "tokens": ["Dich", "zum", "Be\u00b7glei\u00b7ter", "emp\u00b7fehl", "ich", "dem", "Rei\u00b7sen\u00b7den", ";", "a\u00b7ber", "vor", "Al\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "APPR", "PIS", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wann des italischen Meers hohes Gestad er umschifft:", "tokens": ["Wann", "des", "i\u00b7ta\u00b7li\u00b7schen", "Meers", "ho\u00b7hes", "Ge\u00b7stad", "er", "um\u00b7schifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wunder und doch Wahrheit, Ehrfurcht vor dem G\u00f6ttlichen lern er,", "tokens": ["Wun\u00b7der", "und", "doch", "Wahr\u00b7heit", ",", "Ehr\u00b7furcht", "vor", "dem", "G\u00f6tt\u00b7li\u00b7chen", "lern", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "NN", "$,", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Lerne das Menschengem\u00fct kennen und Menschengeschick.", "tokens": ["Ler\u00b7ne", "das", "Men\u00b7schen\u00b7ge\u00b7m\u00fct", "ken\u00b7nen", "und", "Men\u00b7schen\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "KON", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Sch\u00f6nstes Gedicht! Nichts kommt dir Gleich an Behagen und Anmut:", "tokens": ["Sch\u00f6ns\u00b7tes", "Ge\u00b7dicht", "!", "Nichts", "kommt", "dir", "Gleich", "an", "Be\u00b7ha\u00b7gen", "und", "An\u00b7mut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PIS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Unter den Neuen erschuf \u00c4hnliches blo\u00df Ariost.", "tokens": ["Un\u00b7ter", "den", "Neu\u00b7en", "er\u00b7schuf", "\u00c4hn\u00b7li\u00b7ches", "blo\u00df", "A\u00b7riost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "ADV", "NN", "$."], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}}}}}