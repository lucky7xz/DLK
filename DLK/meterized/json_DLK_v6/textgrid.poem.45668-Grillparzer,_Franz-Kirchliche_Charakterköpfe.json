{"textgrid.poem.45668": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Kirchliche Charakterk\u00f6pfe", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der heilige Aloysius und der selige Sarkander,", "tokens": ["Der", "hei\u00b7li\u00b7ge", "A\u00b7loy\u00b7si\u00b7us", "und", "der", "se\u00b7li\u00b7ge", "Sar\u00b7kan\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-----+--", "measure": "dactylic.init"}, "line.2": {"text": "Dabei der Apostel Judas \u2013", "tokens": ["Da\u00b7bei", "der", "A\u00b7pos\u00b7tel", "Ju\u00b7das", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NE", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Judas? rufen sie untereinander,", "tokens": ["Ju\u00b7das", "?", "ru\u00b7fen", "sie", "un\u00b7ter\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ist der das? bist du das?", "tokens": ["Ist", "der", "das", "?", "bist", "du", "das", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PDS", "$.", "VAFIN", "PPER", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}