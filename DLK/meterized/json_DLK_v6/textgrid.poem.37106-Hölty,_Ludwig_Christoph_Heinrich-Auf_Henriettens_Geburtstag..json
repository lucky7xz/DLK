{"textgrid.poem.37106": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Henriettens Geburtstag.", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosen und Nelkenblumen gl\u00e4nzet lichter,", "tokens": ["Ro\u00b7sen", "und", "Nel\u00b7ken\u00b7blu\u00b7men", "gl\u00e4n\u00b7zet", "lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wann das beste der M\u00e4dchen euch besuchet,", "tokens": ["Wann", "das", "bes\u00b7te", "der", "M\u00e4d\u00b7chen", "euch", "be\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dank gen Himmel l\u00e4chelt, und Wonnethr\u00e4nen", "tokens": ["Dank", "gen", "Him\u00b7mel", "l\u00e4\u00b7chelt", ",", "und", "Won\u00b7net\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPR", "NN", "VVFIN", "$,", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Auf euch herabweint.", "tokens": ["Auf", "euch", "her\u00b7ab\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "-+-++", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Thr\u00e4nen des Danks, da\u00df ihre Jugendtage", "tokens": ["Thr\u00e4\u00b7nen", "des", "Danks", ",", "da\u00df", "ih\u00b7re", "Ju\u00b7gend\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gleich dem S\u00e4useln des Mayn vor\u00fcberflohen,", "tokens": ["Gleich", "dem", "S\u00e4u\u00b7seln", "des", "Mayn", "vor\u00b7\u00fc\u00b7berf\u00b7lo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und den frohen Reigen ein neues ihrer", "tokens": ["Und", "den", "fro\u00b7hen", "Rei\u00b7gen", "ein", "neu\u00b7es", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jahre begonnen.", "tokens": ["Jah\u00b7re", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nstes der M\u00e4dchen! Spiel auf Veilchenauen,", "tokens": ["Sch\u00f6ns\u00b7tes", "der", "M\u00e4d\u00b7chen", "!", "Spiel", "auf", "Veil\u00b7chen\u00b7au\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "APPR", "NE", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Tanz im Nachtigallw\u00e4ldchen sey dein Leben,", "tokens": ["Tanz", "im", "Nach\u00b7ti\u00b7gall\u00b7w\u00e4ld\u00b7chen", "sey", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gleich dem Lorbeer bl\u00fchend, der deine finstre", "tokens": ["Gleich", "dem", "Lor\u00b7beer", "bl\u00fc\u00b7hend", ",", "der", "dei\u00b7ne", "finst\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Locke beschattet.", "tokens": ["Lo\u00b7cke", "be\u00b7schat\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Rosen und Nelkenblumen gl\u00e4nzet lichter,", "tokens": ["Ro\u00b7sen", "und", "Nel\u00b7ken\u00b7blu\u00b7men", "gl\u00e4n\u00b7zet", "lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gleicht Elysiums Blumen, wann sie meiner", "tokens": ["Gleicht", "E\u00b7ly\u00b7si\u00b7ums", "Blu\u00b7men", ",", "wann", "sie", "mei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "NN", "$,", "PWAV", "PPER", "PPOSAT"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Denkt, dann komm' ein L\u00fcftchen, und fl\u00fcstr' ihr tausend", "tokens": ["Denkt", ",", "dann", "komm'", "ein", "L\u00fcft\u00b7chen", ",", "und", "fl\u00fcstr'", "ihr", "tau\u00b7send"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "CARD"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Seufzer entgegen.", "tokens": ["Seuf\u00b7zer", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}