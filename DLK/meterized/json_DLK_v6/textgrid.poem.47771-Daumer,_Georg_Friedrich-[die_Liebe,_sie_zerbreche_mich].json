{"textgrid.poem.47771": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Liebe, sie zerbreche mich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe, sie zerbreche mich;", "tokens": ["Die", "Lie\u00b7be", ",", "sie", "zer\u00b7bre\u00b7che", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es raffe meine Kraft der Wein!", "tokens": ["Es", "raf\u00b7fe", "mei\u00b7ne", "Kraft", "der", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht stark will ich und tapfer sein;", "tokens": ["Nicht", "stark", "will", "ich", "und", "tap\u00b7fer", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich freue meiner Schw\u00e4che mich.", "tokens": ["Ich", "freu\u00b7e", "mei\u00b7ner", "Schw\u00e4\u00b7che", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}