{"dta.poem.23423": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aria  \n  eines deutschen aus Frankreich heimkehrenden  \n  Aristokaten Aristokraten   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl mir, da\u00df ich ", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Freiheit und Gleichheit \u2014 weg damit!", "tokens": ["Frei\u00b7heit", "und", "Gleich\u00b7heit", "weg", "da\u00b7mit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "ADV", "PAV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich wei\u00df, was ich in Frankreich litt.", "tokens": ["Ich", "wei\u00df", ",", "was", "ich", "in", "Fran\u00b7kreich", "litt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Soviel Regie und Polizei!", "tokens": ["So\u00b7viel", "Re\u00b7gie", "und", "Po\u00b7li\u00b7zei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O la\u00dft uns unsre Sklaverei", "tokens": ["O", "la\u00dft", "uns", "uns\u00b7re", "Skla\u00b7ve\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und d\u00fcnkt euch gl\u00fccklich, gro\u00df und frei!", "tokens": ["Und", "d\u00fcnkt", "euch", "gl\u00fcck\u00b7lich", ",", "gro\u00df", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Freiheit ist nur dummer Schnack,", "tokens": ["Die", "Frei\u00b7heit", "ist", "nur", "dum\u00b7mer", "Schnack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang man raucht mit Hack und Pack", "tokens": ["So", "lang", "man", "raucht", "mit", "Hack", "und", "Pack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Einen schlechten Rauchtabak.", "tokens": ["Nur", "Ei\u00b7nen", "schlech\u00b7ten", "Rauch\u00b7ta\u00b7bak", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}