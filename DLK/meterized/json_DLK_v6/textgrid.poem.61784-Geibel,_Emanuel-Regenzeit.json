{"textgrid.poem.61784": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Regenzeit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh' ich nach dem ew'gen Regen", "tokens": ["Geh'", "ich", "nach", "dem", "ew'\u00b7gen", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Wald bei fr\u00fcher Zeit,", "tokens": ["Durch", "den", "Wald", "bei", "fr\u00fc\u00b7her", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, wie macht auf allen Wegen", "tokens": ["Ei", ",", "wie", "macht", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich das Volk der Pilze breit!", "tokens": ["Sich", "das", "Volk", "der", "Pil\u00b7ze", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwischen Dorn und Hagebutte", "tokens": ["Zwi\u00b7schen", "Dorn", "und", "Ha\u00b7ge\u00b7but\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Truppweis auf des Pfades Rand", "tokens": ["Trupp\u00b7weis", "auf", "des", "Pfa\u00b7des", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehn sie hier in wei\u00dfer Kutte,", "tokens": ["Stehn", "sie", "hier", "in", "wei\u00b7\u00dfer", "Kut\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort im braunen M\u00f6nchsgewand.", "tokens": ["Dort", "im", "brau\u00b7nen", "M\u00f6n\u00b7chs\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Andre bl\u00e4hn gleich Kardin\u00e4len", "tokens": ["And\u00b7re", "bl\u00e4hn", "gleich", "Kar\u00b7di\u00b7n\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im flachen Scharlachhut,", "tokens": ["Sich", "im", "fla\u00b7chen", "Schar\u00b7lach\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, und vollends nicht zu z\u00e4hlen", "tokens": ["Ach", ",", "und", "vol\u00b7lends", "nicht", "zu", "z\u00e4h\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die schwarzgefleckte Brut.", "tokens": ["Ist", "die", "schwarz\u00b7ge\u00b7fleck\u00b7te", "Brut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dicht geschart und immer dichter", "tokens": ["Dicht", "ge\u00b7schart", "und", "im\u00b7mer", "dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durchs Revier von Ort zu Ort", "tokens": ["Durchs", "Re\u00b7vier", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "W\u00e4lzt das schwammige Gelichter", "tokens": ["W\u00e4lzt", "das", "schwam\u00b7mi\u00b7ge", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Propagande fort;", "tokens": ["Sei\u00b7ne", "Pro\u00b7pa\u00b7gan\u00b7de", "fort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Klimmt mit unheimlicher Schnelle", "tokens": ["Klimmt", "mit", "un\u00b7heim\u00b7li\u00b7cher", "Schnel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "H\u00fcgelan aus jeder Schluft,", "tokens": ["H\u00fc\u00b7ge\u00b7lan", "aus", "je\u00b7der", "Schluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haucht von jeder sumpf'gen Stelle", "tokens": ["Haucht", "von", "je\u00b7der", "sumpf'\u00b7gen", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Brodem in die Luft.", "tokens": ["Sei\u00b7nen", "Bro\u00b7dem", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Frischen Sonnenatem sende,", "tokens": ["Fri\u00b7schen", "Son\u00b7ne\u00b7na\u00b7tem", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00fct'ger Himmel, send' ihn bald!", "tokens": ["G\u00fct'\u00b7ger", "Him\u00b7mel", ",", "send'", "ihn", "bald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonst verdumpft uns noch am Ende", "tokens": ["Sonst", "ver\u00b7dumpft", "uns", "noch", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies Gez\u00fccht den ganzen Wald.", "tokens": ["Dies", "Ge\u00b7z\u00fccht", "den", "gan\u00b7zen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}