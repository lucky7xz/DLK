{"textgrid.poem.49302": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An einem sch\u00f6nen Fr\u00fchlingstage", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sich Himmel und Erd' erfreut", "tokens": ["Nun", "sich", "Him\u00b7mel", "und", "Erd'", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "NN", "KON", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In der lieblichen Fr\u00fchlingszeit,", "tokens": ["In", "der", "lieb\u00b7li\u00b7chen", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nun die V\u00f6gelein stimmen an", "tokens": ["Nun", "die", "V\u00f6\u00b7ge\u00b7lein", "stim\u00b7men", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das die Menschen ergetzen kan;", "tokens": ["Das", "die", "Men\u00b7schen", "er\u00b7get\u00b7zen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nun die Fl\u00fcsse so sanft und fein", "tokens": ["Nun", "die", "Fl\u00fcs\u00b7se", "so", "sanft", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wiedrum schleichen ins Meer hinein,", "tokens": ["Wie\u00b7drum", "schlei\u00b7chen", "ins", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Nun der Winter sich gibt zur Ruh'", "tokens": ["Nun", "der", "Win\u00b7ter", "sich", "gibt", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PRF", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und die W\u00e4rme nimmt t\u00e4glich zu;", "tokens": ["Und", "die", "W\u00e4r\u00b7me", "nimmt", "t\u00e4g\u00b7lich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nun die B\u00e4ume gleich schwanger stehn,", "tokens": ["Nun", "die", "B\u00e4u\u00b7me", "gleich", "schwan\u00b7ger", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und die Blumen sich lassen sehn,", "tokens": ["Und", "die", "Blu\u00b7men", "sich", "las\u00b7sen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Nun die fl\u00fcchtigen Thier im Wald", "tokens": ["Nun", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Thier", "im", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Artig springen und tanzen bald;", "tokens": ["Ar\u00b7tig", "sprin\u00b7gen", "und", "tan\u00b7zen", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVFIN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Ist der Mangel an denen doch,", "tokens": ["Ist", "der", "Man\u00b7gel", "an", "de\u00b7nen", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PRELS", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Die nur lieben des Krieges Joch", "tokens": ["Die", "nur", "lie\u00b7ben", "des", "Krie\u00b7ges", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Und nicht suchen des Friedens Ziel;", "tokens": ["Und", "nicht", "su\u00b7chen", "des", "Frie\u00b7dens", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Menschen halten das Widerspiel.", "tokens": ["Men\u00b7schen", "hal\u00b7ten", "das", "Wi\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}