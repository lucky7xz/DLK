{"textgrid.poem.35131": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich f\u00fchren; la\u00df dich f\u00fchren,", "tokens": ["La\u00df", "dich", "f\u00fch\u00b7ren", ";", "la\u00df", "dich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob du redest, ob du handelst;", "tokens": ["Ob", "du", "re\u00b7dest", ",", "ob", "du", "han\u00b7delst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Thust du das, so wirst du sp\u00fcren,", "tokens": ["Thust", "du", "das", ",", "so", "wirst", "du", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du unter Leitung wandelst.", "tokens": ["Da\u00df", "du", "un\u00b7ter", "Lei\u00b7tung", "wan\u00b7delst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df dich leiten; la\u00df dich leiten;", "tokens": ["La\u00df", "dich", "lei\u00b7ten", ";", "la\u00df", "dich", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du allein kannst nichts erringen.", "tokens": ["Du", "al\u00b7lein", "kannst", "nichts", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Berg der Seligkeiten", "tokens": ["Auf", "den", "Berg", "der", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tragen dich nur Engelsschwingen.", "tokens": ["Tra\u00b7gen", "dich", "nur", "En\u00b7gels\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df dich tragen; la\u00df dich tragen", "tokens": ["La\u00df", "dich", "tra\u00b7gen", ";", "la\u00df", "dich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Wehr und Widerstreben;", "tokens": ["Oh\u00b7ne", "Wehr", "und", "Wi\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann wird dir ein Himmel tagen,", "tokens": ["Dann", "wird", "dir", "ein", "Him\u00b7mel", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den kein Mensch vermag zu geben.", "tokens": ["Den", "kein", "Mensch", "ver\u00b7mag", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}