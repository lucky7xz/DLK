{"dta.poem.19445": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fclle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Genug ist nicht genug! Gepriesen werde", "tokens": ["Ge\u00b7nug", "ist", "nicht", "ge\u00b7nug", "!", "Ge\u00b7prie\u00b7sen", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "$.", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herbst! Kein Ast, der seiner Frucht entbehrte!", "tokens": ["Der", "Herbst", "!", "Kein", "Ast", ",", "der", "sei\u00b7ner", "Frucht", "ent\u00b7behr\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tief beugt sich mancher allzureich beschwerte,", "tokens": ["Tief", "beugt", "sich", "man\u00b7cher", "all\u00b7zu\u00b7reich", "be\u00b7schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Apfel f\u00e4llt mit dumpfem Laut zur Erde.", "tokens": ["Der", "Ap\u00b7fel", "f\u00e4llt", "mit", "dum\u00b7pfem", "Laut", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Genug ist nicht genug! Es lacht im Laube!", "tokens": ["Ge\u00b7nug", "ist", "nicht", "ge\u00b7nug", "!", "Es", "lacht", "im", "Lau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Pfirsche hat dem Munde zugewunken!", "tokens": ["Die", "Pfir\u00b7sche", "hat", "dem", "Mun\u00b7de", "zu\u00b7ge\u00b7wun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein helles Zechlied summt die Wespe trunken \u2014", "tokens": ["Ein", "hel\u00b7les", "Zech\u00b7lied", "summt", "die", "Wes\u00b7pe", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genug ist nicht genug! \u2014 um eine Traube.", "tokens": ["Ge\u00b7nug", "ist", "nicht", "ge\u00b7nug", "!", "um", "ei\u00b7ne", "Trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "$.", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Genug ist nicht genug! Mit vollen Z\u00fcgen", "tokens": ["Ge\u00b7nug", "ist", "nicht", "ge\u00b7nug", "!", "Mit", "vol\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schl\u00fcrft Dichtergeist am Borne des Genu\u00dfes,", "tokens": ["Schl\u00fcrft", "Dich\u00b7ter\u00b7geist", "am", "Bor\u00b7ne", "des", "Ge\u00b7nu\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz, auch es bedarf des Ueberflu\u00dfes,", "tokens": ["Das", "Herz", ",", "auch", "es", "be\u00b7darf", "des", "Ue\u00b7berf\u00b7lu\u00b7\u00dfes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genug kann nie und nimmermehr gen\u00fcgen!", "tokens": ["Ge\u00b7nug", "kann", "nie", "und", "nim\u00b7mer\u00b7mehr", "ge\u00b7n\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}