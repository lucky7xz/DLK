{"dta.poem.19199": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, die soviel ist gr\u00f6\u00dfer als die Erde,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "so\u00b7viel", "ist", "gr\u00f6\u00b7\u00dfer", "als", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist sie die Hirtin, und die Erd' ein Lamm der Herde?", "tokens": ["Ist", "sie", "die", "Hir\u00b7tin", ",", "und", "die", "Erd'", "ein", "Lamm", "der", "Her\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist sie die goldne Trift, mit Flammengras bekleidet,", "tokens": ["Ist", "sie", "die", "gold\u00b7ne", "Trift", ",", "mit", "Flam\u00b7men\u00b7gras", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauf die Erde mit den andern L\u00e4mmern weidet?", "tokens": ["Wo\u00b7rauf", "die", "Er\u00b7de", "mit", "den", "an\u00b7dern", "L\u00e4m\u00b7mern", "wei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist sie der Bronnen, der mit Glanz die Herde tr\u00e4nkt?", "tokens": ["Ist", "sie", "der", "Bron\u00b7nen", ",", "der", "mit", "Glanz", "die", "Her\u00b7de", "tr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die H\u00fcrd', in welche sie wird Abends eingelenkt?", "tokens": ["Die", "H\u00fcrd'", ",", "in", "wel\u00b7che", "sie", "wird", "A\u00b7bends", "ein\u00b7ge\u00b7lenkt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob Hirtin oder Trift, ob Bronnen oder H\u00fcrde,", "tokens": ["Ob", "Hir\u00b7tin", "o\u00b7der", "Trift", ",", "ob", "Bron\u00b7nen", "o\u00b7der", "H\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie hat ein sch\u00f6nes Amt und eine hohe W\u00fcrde.", "tokens": ["Sie", "hat", "ein", "sch\u00f6\u00b7nes", "Amt", "und", "ei\u00b7ne", "ho\u00b7he", "W\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn Hirtin, h\u00fcte sie mit treuem Blick die ihren;", "tokens": ["Wenn", "Hir\u00b7tin", ",", "h\u00fc\u00b7te", "sie", "mit", "treu\u00b7em", "Blick", "die", "ih\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wol aus den Augen wird sie leicht kein St\u00fcck verlieren.", "tokens": ["Wol", "aus", "den", "Au\u00b7gen", "wird", "sie", "leicht", "kein", "St\u00fcck", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Wenn Trift, so treibe sie mit ew'gen Fr\u00fchlingstrieben,", "tokens": ["Wenn", "Trift", ",", "so", "trei\u00b7be", "sie", "mit", "ew'\u00b7gen", "Fr\u00fch\u00b7ling\u00b7strie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lustgetrieben gehn die L\u00e4mmer nach Belieben.", "tokens": ["Und", "lust\u00b7ge\u00b7trie\u00b7ben", "gehn", "die", "L\u00e4m\u00b7mer", "nach", "Be\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn Bronnen, sei sie uns voll stets vom Thau der Gnaden;", "tokens": ["Wenn", "Bron\u00b7nen", ",", "sei", "sie", "uns", "voll", "stets", "vom", "Thau", "der", "Gna\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VAFIN", "PPER", "PRF", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn H\u00fcrde, sammle sie die Herd' ein ohne Schaden.", "tokens": ["Wenn", "H\u00fcr\u00b7de", ",", "samm\u00b7le", "sie", "die", "Herd'", "ein", "oh\u00b7ne", "Scha\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein sch\u00f6ner Sommertag, den ausgesprungen habend,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Som\u00b7mer\u00b7tag", ",", "den", "aus\u00b7ge\u00b7sprun\u00b7gen", "ha\u00b7bend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die m\u00fcde Herde sucht den warmen Stall am Abend.", "tokens": ["Die", "m\u00fc\u00b7de", "Her\u00b7de", "sucht", "den", "war\u00b7men", "Stall", "am", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}