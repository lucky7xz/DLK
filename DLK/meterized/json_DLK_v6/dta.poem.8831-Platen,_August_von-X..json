{"dta.poem.8831": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Es kehrt an das, was Kranke qu\u00e4lt, sich ewig der Ge-", "tokens": ["Es", "kehrt", "an", "das", ",", "was", "Kran\u00b7ke", "qu\u00e4lt", ",", "sich", "e\u00b7wig", "der", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDS", "$,", "PWS", "NN", "VVFIN", "$,", "PRF", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00e4re nicht das Leben kurz, das stets der Mensch", "tokens": ["Und", "w\u00e4\u00b7re", "nicht", "das", "Le\u00b7ben", "kurz", ",", "das", "stets", "der", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So g\u00e4b's Beklagenswertheres auf diesem weiten Runde", "tokens": ["So", "g\u00e4b's", "Be\u00b7kla\u00b7gens\u00b7wert\u00b7he\u00b7res", "auf", "die\u00b7sem", "wei\u00b7ten", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Einf\u00f6rmig stellt Natur sich her, doch tausendf\u00f6rmig ist", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "stellt", "Na\u00b7tur", "sich", "her", ",", "doch", "tau\u00b7send\u00b7f\u00f6r\u00b7mig", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN", "PRF", "PTKVZ", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Es fragt die Welt nach meinem Ziel, nach deiner lezten", "tokens": ["Es", "fragt", "die", "Welt", "nach", "mei\u00b7nem", "Ziel", ",", "nach", "dei\u00b7ner", "lez\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wer sich willig nicht ergiebt dem ehrnen Loose, das", "tokens": ["Und", "wer", "sich", "wil\u00b7lig", "nicht", "er\u00b7giebt", "dem", "ehr\u00b7nen", "Loo\u00b7se", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PRF", "ADJD", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Der z\u00fcrnt in's Grab sich rettungslos, und f\u00fchlt in", "tokens": ["Der", "z\u00fcrnt", "in's", "Grab", "sich", "ret\u00b7tungs\u00b7los", ",", "und", "f\u00fchlt", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PRF", "ADJD", "$,", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dies wissen Alle, doch vergi\u00dft es Jeder gerne jeden Tag,", "tokens": ["Dies", "wis\u00b7sen", "Al\u00b7le", ",", "doch", "ver\u00b7gi\u00dft", "es", "Je\u00b7der", "ger\u00b7ne", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PPER", "PIS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "So komme denn, in diesem Sinn, hinfort aus meinem", "tokens": ["So", "kom\u00b7me", "denn", ",", "in", "die\u00b7sem", "Sinn", ",", "hin\u00b7fort", "aus", "mei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "APPR", "PDAT", "NN", "$,", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verge\u00dft, da\u00df auch die Welt betr\u00fcgt, und da\u00df ihr Wunsch", "tokens": ["Ver\u00b7ge\u00dft", ",", "da\u00df", "auch", "die", "Welt", "be\u00b7tr\u00fcgt", ",", "und", "da\u00df", "ihr", "Wunsch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "La\u00dft eurer Liebe nichts entgehn, entschl\u00fcpfen eurer", "tokens": ["La\u00dft", "eu\u00b7rer", "Lie\u00b7be", "nichts", "ent\u00b7gehn", ",", "ent\u00b7schl\u00fcp\u00b7fen", "eu\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PIS", "VVINF", "$,", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es hoffe Jeder, da\u00df die Zeit ihm gebe was sie Keinem", "tokens": ["Es", "hof\u00b7fe", "Je\u00b7der", ",", "da\u00df", "die", "Zeit", "ihm", "ge\u00b7be", "was", "sie", "Kei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "PWS", "PPER", "PIS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Denn Jeder sucht ein All zu seyn, und Jeder ist im", "tokens": ["Denn", "Je\u00b7der", "sucht", "ein", "All", "zu", "seyn", ",", "und", "Je\u00b7der", "ist", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "$,", "KON", "PIS", "VAFIN", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}