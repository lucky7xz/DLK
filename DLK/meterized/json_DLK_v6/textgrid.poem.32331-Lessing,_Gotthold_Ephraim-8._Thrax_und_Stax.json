{"textgrid.poem.32331": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "8. Thrax und Stax", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stax. Thrax! eine taube Frau zu nehmen!", "tokens": ["Stax", ".", "Thrax", "!", "ei\u00b7ne", "tau\u00b7be", "Frau", "zu", "neh\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O Thrax, das nenn' ich dumm.", "tokens": ["O", "Thrax", ",", "das", "nenn'", "ich", "dumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thrax. Ja freilich, Stax! ich mu\u00df mich sch\u00e4men.", "tokens": ["Thrax", ".", "Ja", "frei\u00b7lich", ",", "Stax", "!", "ich", "mu\u00df", "mich", "sch\u00e4\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKANT", "ADV", "$,", "NE", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch sieh, ich hielt sie auch f\u00fcr stumm.", "tokens": ["Doch", "sieh", ",", "ich", "hielt", "sie", "auch", "f\u00fcr", "stumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}