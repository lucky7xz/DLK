{"dta.poem.638": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Pompejens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es zitterte die Welt wenn ich den Fu\u00dfbewegte/", "tokens": ["Es", "zit\u00b7ter\u00b7te", "die", "Welt", "wenn", "ich", "den", "Fu\u00df\u00b7be\u00b7weg\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den grossen ", "tokens": ["Den", "gros\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch als ich Rom auf Rom zum B\u00fcrger-Krieg erregte/", "tokens": ["Doch", "als", "ich", "Rom", "auf", "Rom", "zum", "B\u00fcr\u00b7ger\u00b7Krieg", "er\u00b7reg\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "APPR", "NE", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So fiel ich/ und wurd bald verlassen umgebracht.", "tokens": ["So", "fiel", "ich", "/", "und", "wurd", "bald", "ver\u00b7las\u00b7sen", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "VAFIN", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}