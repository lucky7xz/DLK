{"textgrid.poem.38740": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber mir das hellgestirnte Himmelsdach,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber mir das hellgestirnte Himmelsdach,", "tokens": ["Ue\u00b7ber", "mir", "das", "hell\u00b7ges\u00b7tirn\u00b7te", "Him\u00b7mels\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Alle Menschen dem Schlaf ergeben,", "tokens": ["Al\u00b7le", "Men\u00b7schen", "dem", "Schlaf", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ruhend von dem m\u00fchevollen Leben,", "tokens": ["Ru\u00b7hend", "von", "dem", "m\u00fc\u00b7he\u00b7vol\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ich allein, allein im Hause wach.", "tokens": ["Ich", "al\u00b7lein", ",", "al\u00b7lein", "im", "Hau\u00b7se", "wach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tr\u00fcbe brennt das Licht herunter;", "tokens": ["Tr\u00fc\u00b7be", "brennt", "das", "Licht", "her\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich aus dem Fenster schauen", "tokens": ["Soll", "ich", "aus", "dem", "Fens\u00b7ter", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "'n\u00fcber nach den fernen Auen?", "tokens": ["'n\u00b7\u00fcber", "nach", "den", "fer\u00b7nen", "Au\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Augen bleiben munter.", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "blei\u00b7ben", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll ich mich im Strahl ergehen", "tokens": ["Soll", "ich", "mich", "im", "Strahl", "er\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Mondes Aufgang suchen?", "tokens": ["Und", "des", "Mon\u00b7des", "Auf\u00b7gang", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', es flimmert durch die Buchen,", "tokens": ["Sieh'", ",", "es", "flim\u00b7mert", "durch", "die", "Bu\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiden am Bach im Golde stehen.", "tokens": ["Wei\u00b7den", "am", "Bach", "im", "Gol\u00b7de", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ist es nicht, als k\u00e4me aus den Weiden", "tokens": ["Ist", "es", "nicht", ",", "als", "k\u00e4\u00b7me", "aus", "den", "Wei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KOKOM", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach! ein Freund, den ich lange nicht gesehn,", "tokens": ["Ach", "!", "ein", "Freund", ",", "den", "ich", "lan\u00b7ge", "nicht", "ge\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach, wie viel ist schon seither geschehn,", "tokens": ["Ach", ",", "wie", "viel", "ist", "schon", "sei\u00b7ther", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PIS", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Seit dem quaalenvollen, bittern Scheiden!", "tokens": ["Seit", "dem", "qua\u00b7a\u00b7len\u00b7vol\u00b7len", ",", "bit\u00b7tern", "Schei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "An den Busen will ich ihn m\u00e4chtig dr\u00fccken,", "tokens": ["An", "den", "Bu\u00b7sen", "will", "ich", "ihn", "m\u00e4ch\u00b7tig", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Sagen, was so ofte mir gebangt,", "tokens": ["Sa\u00b7gen", ",", "was", "so", "of\u00b7te", "mir", "ge\u00b7bangt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie mich inniglich nach ihm verlangt,", "tokens": ["Wie", "mich", "in\u00b7nig\u00b7lich", "nach", "ihm", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und ihm in die s\u00fc\u00dfen Augen blicken.", "tokens": ["Und", "ihm", "in", "die", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber der Schatten bleibt dort unter den Zweigen,", "tokens": ["A\u00b7ber", "der", "Schat\u00b7ten", "bleibt", "dort", "un\u00b7ter", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ist nur Mondenschein,", "tokens": ["Ist", "nur", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6mmt nicht zu mir herein,", "tokens": ["K\u00f6mmt", "nicht", "zu", "mir", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich als Freund zu zeigen.", "tokens": ["Sich", "als", "Freund", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Ist auch schon gestorben und begraben,", "tokens": ["Ist", "auch", "schon", "ge\u00b7stor\u00b7ben", "und", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und verge\u00df es jeden Tag,", "tokens": ["Und", "ver\u00b7ge\u00df", "es", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich's so \u00fcbergern vergessen mag;", "tokens": ["Weil", "ich's", "so", "\u00fc\u00b7ber\u00b7gern", "ver\u00b7ges\u00b7sen", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie kann ich ihn denn in den Armen haben?", "tokens": ["Wie", "kann", "ich", "ihn", "denn", "in", "den", "Ar\u00b7men", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Geht der Flu\u00df murmelnd durch die Kl\u00fcfte,", "tokens": ["Geht", "der", "Flu\u00df", "mur\u00b7melnd", "durch", "die", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sucht die Ferne nach eigner Melodie,", "tokens": ["Sucht", "die", "Fer\u00b7ne", "nach", "eig\u00b7ner", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Unerm\u00fcdet sprechend spat und fr\u00fch:", "tokens": ["Un\u00b7er\u00b7m\u00fc\u00b7det", "spre\u00b7chend", "spat", "und", "fr\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wehn vom Berge schon Septemberl\u00fcfte.", "tokens": ["Wehn", "vom", "Ber\u00b7ge", "schon", "Sep\u00b7tem\u00b7ber\u00b7l\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "T\u00f6ne fallen von oben in die Welt,", "tokens": ["T\u00f6\u00b7ne", "fal\u00b7len", "von", "o\u00b7ben", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lust'ge Pfeifen, fr\u00f6hliche Schallmey'n,", "tokens": ["Lust'\u00b7ge", "Pfei\u00b7fen", ",", "fr\u00f6h\u00b7li\u00b7che", "Schall\u00b7mey'n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ach! sollten es Bekannte sein?", "tokens": ["Ach", "!", "soll\u00b7ten", "es", "Be\u00b7kann\u00b7te", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wandern zu mir \u00fcber's Feld.", "tokens": ["Sie", "wan\u00b7dern", "zu", "mir", "\u00fc\u00b7ber's", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Fernab erklingt es, keiner wei\u00df von mir,", "tokens": ["Fern\u00b7ab", "er\u00b7klingt", "es", ",", "kei\u00b7ner", "wei\u00df", "von", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Alle meine Freunde mich verlassen,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Freun\u00b7de", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die mich liebten, jetzt mich hassen,", "tokens": ["Die", "mich", "lieb\u00b7ten", ",", "jetzt", "mich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcmmert sich keiner, da\u00df ich wohne hier.", "tokens": ["K\u00fcm\u00b7mert", "sich", "kei\u00b7ner", ",", "da\u00df", "ich", "woh\u00b7ne", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "Ziehn mit Netzen oft lustig zum See,", "tokens": ["Ziehn", "mit", "Net\u00b7zen", "oft", "lus\u00b7tig", "zum", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "H\u00f6re dann das ferne Gelach;", "tokens": ["H\u00f6\u00b7re", "dann", "das", "fer\u00b7ne", "Ge\u00b7lach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Seufze mein kummervoll Ach!", "tokens": ["Seuf\u00b7ze", "mein", "kum\u00b7mer\u00b7voll", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "ITJ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Thut mir der Busen so weh.", "tokens": ["Thut", "mir", "der", "Bu\u00b7sen", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Ach! wo bist du Bild geblieben,", "tokens": ["Ach", "!", "wo", "bist", "du", "Bild", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engelbild vom sch\u00f6nsten Kind?", "tokens": ["En\u00b7gel\u00b7bild", "vom", "sch\u00f6ns\u00b7ten", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Freuden \u00fcbrig sind,", "tokens": ["Kei\u00b7ne", "Freu\u00b7den", "\u00fcb\u00b7rig", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterstund mich, dich zu lieben.", "tokens": ["Un\u00b7ter\u00b7stund", "mich", ",", "dich", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hast den Gatten l\u00e4ngst gefunden,", "tokens": ["Hast", "den", "Gat\u00b7ten", "l\u00e4ngst", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der fernste Schimmerschein,", "tokens": ["Wie", "der", "ferns\u00b7te", "Schim\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4llt mein Name dir nicht ein,", "tokens": ["F\u00e4llt", "mein", "Na\u00b7me", "dir", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie in deinen guten Stunden.", "tokens": ["Nie", "in", "dei\u00b7nen", "gu\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und das Licht ist ausgegangen,", "tokens": ["Und", "das", "Licht", "ist", "aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitze in der Dunkelheit,", "tokens": ["Sit\u00b7ze", "in", "der", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denke, was mich sonst erfreut,", "tokens": ["Den\u00b7ke", ",", "was", "mich", "sonst", "er\u00b7freut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als noch Nachtigallen sangen.", "tokens": ["Als", "noch", "Nach\u00b7ti\u00b7gal\u00b7len", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ach! und warst nicht einsam immer?", "tokens": ["Ach", "!", "und", "warst", "nicht", "ein\u00b7sam", "im\u00b7mer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VAFIN", "PTKNEG", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner der dein Herz verstand,", "tokens": ["Kei\u00b7ner", "der", "dein", "Herz", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner sich zu dir verband. \u2013", "tokens": ["Kei\u00b7ner", "sich", "zu", "dir", "ver\u00b7band", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "PRF", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geh auch unter Mondesschimmer.", "tokens": ["Geh", "auch", "un\u00b7ter", "Mon\u00b7des\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "L\u00f6sche, l\u00f6sche letztes Licht!", "tokens": ["L\u00f6\u00b7sche", ",", "l\u00f6\u00b7sche", "letz\u00b7tes", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch wenn Freunde mich umgeben,", "tokens": ["Auch", "wenn", "Freun\u00b7de", "mich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchr' ich doch einsames Leben:", "tokens": ["F\u00fchr'", "ich", "doch", "ein\u00b7sa\u00b7mes", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00f6sche, l\u00f6sche, letztes Licht,", "tokens": ["L\u00f6\u00b7sche", ",", "l\u00f6\u00b7sche", ",", "letz\u00b7tes", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Ungl\u00fcckliche braucht dich nicht!", "tokens": ["Der", "Un\u00b7gl\u00fcck\u00b7li\u00b7che", "braucht", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}}}}