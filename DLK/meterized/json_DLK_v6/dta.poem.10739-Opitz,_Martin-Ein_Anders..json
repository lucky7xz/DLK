{"dta.poem.10739": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ein Anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Morgens eh's tages schein anbricht", "tokens": ["Mor\u00b7gens", "eh's", "ta\u00b7ges", "schein", "an\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit Purpurfarbem hellem licht/", "tokens": ["Mit", "Pur\u00b7purf\u00b7ar\u00b7bem", "hel\u00b7lem", "licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So glentzt herf\u00fcr deins mundes r\u00f6t/", "tokens": ["So", "glentzt", "her\u00b7f\u00fcr", "deins", "mun\u00b7des", "r\u00f6t", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie vor der Sonn die Morgenr\u00f6t/", "tokens": ["Wie", "vor", "der", "Sonn", "die", "Mor\u00b7gen\u00b7r\u00f6t", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Rosinfarben Wangen", "tokens": ["Mit", "Ro\u00b7sin\u00b7far\u00b7ben", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00fcbsch/ lustig klar auffgangen.", "tokens": ["H\u00fcbsch", "/", "lus\u00b7tig", "klar", "auff\u