 "word", "punct"], "pos": ["ADV", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Freundligkeit vnd sch\u00f6n geberd", "tokens": ["Dein", "Freund\u00b7lig\u00b7keit", "vnd", "sch\u00f6n", "ge\u00b7berd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mehr dan alle Perlen werth:", "tokens": ["Ist", "mehr", "dan", "al\u00b7le", "Per\u00b7len", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Es gilt dein zucht vil reine", "tokens": ["Es", "gilt", "dein", "zucht", "vil", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mehr dan all\u2019 Edle Steine.", "tokens": ["Mehr", "dan", "all'", "Ed\u00b7le", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Trewhertzig sein ohn arge list/", "tokens": ["Trew\u00b7hert\u00b7zig", "sein", "ohn", "ar\u00b7ge", "list", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rechtenlieb warzeichen ist:", "tokens": ["Der", "Rech\u00b7ten\u00b7lieb", "war\u00b7zei\u00b7chen", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wan gleicher ma\u00df nit liebest mich/", "tokens": ["Wan", "glei\u00