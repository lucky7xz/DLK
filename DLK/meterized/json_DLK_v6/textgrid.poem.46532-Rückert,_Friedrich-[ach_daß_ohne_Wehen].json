{"textgrid.poem.46532": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach da\u00df ohne Wehen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach da\u00df ohne Wehen", "tokens": ["Ach", "da\u00df", "oh\u00b7ne", "We\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie ein Bl\u00fcthenstrauch", "tokens": ["Wie", "ein", "Bl\u00fc\u00b7then\u00b7strauch"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kinder k\u00f6nnten gehen", "tokens": ["Kin\u00b7der", "k\u00f6nn\u00b7ten", "ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Aus dem Boden auch!", "tokens": ["Aus", "dem", "Bo\u00b7den", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df sie ohne Leiden", "tokens": ["Da\u00df", "sie", "oh\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sinken in den Staub", "tokens": ["Sin\u00b7ken", "in", "den", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00fcrften und verscheiden", "tokens": ["D\u00fcrf\u00b7ten", "und", "ver\u00b7schei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie ein Rosenlaub!", "tokens": ["Wie", "ein", "Ro\u00b7sen\u00b7laub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Mutter unterm Herzen", "tokens": ["Mut\u00b7ter", "un\u00b7term", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Trug sie schmerzenvoll,", "tokens": ["Trug", "sie", "schmer\u00b7zen\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die sie unter Schmerzen", "tokens": ["Die", "sie", "un\u00b7ter", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sterben sehen soll.", "tokens": ["Ster\u00b7ben", "se\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}