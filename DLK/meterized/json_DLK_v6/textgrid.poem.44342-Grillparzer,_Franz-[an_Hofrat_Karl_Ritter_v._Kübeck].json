{"textgrid.poem.44342": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[an Hofrat Karl Ritter v. K\u00fcbeck]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von seiner ewigen Berge Spitzen", "tokens": ["Von", "sei\u00b7ner", "e\u00b7wi\u00b7gen", "Ber\u00b7ge", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hebt sich Tirols gef\u00fcrchteter Aar,", "tokens": ["Hebt", "sich", "Ti\u00b7rols", "ge\u00b7f\u00fcrch\u00b7te\u00b7ter", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hoch ob der Menschen niedrigen Sitzen", "tokens": ["Hoch", "ob", "der", "Men\u00b7schen", "nied\u00b7ri\u00b7gen", "Sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "L\u00e4\u00dft er die m\u00e4chtigen Fl\u00fcgel blitzen,", "tokens": ["L\u00e4\u00dft", "er", "die", "m\u00e4ch\u00b7ti\u00b7gen", "Fl\u00fc\u00b7gel", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Stellet ein G\u00f6tterbote sich dar.", "tokens": ["Stel\u00b7let", "ein", "G\u00f6t\u00b7ter\u00b7bo\u00b7te", "sich", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Einen Kranz in den m\u00e4chtigen Krallen,", "tokens": ["Ei\u00b7nen", "Kranz", "in", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schwebt er daher zu der Donau Strand.", "tokens": ["Schwebt", "er", "da\u00b7her", "zu", "der", "Do\u00b7nau", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ART", "NE", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Welchem Gl\u00fccklichen, welchem vor allen,", "tokens": ["Wel\u00b7chem", "Gl\u00fcck\u00b7li\u00b7chen", ",", "wel\u00b7chem", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PRELS", "APPR", "PIAT", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ist das herrliche Los gefallen,", "tokens": ["Ist", "das", "herr\u00b7li\u00b7che", "Los", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wem ward solcher Bote gesandt?", "tokens": ["Wem", "ward", "sol\u00b7cher", "Bo\u00b7te", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und er senket das stolze Gefieder.", "tokens": ["Und", "er", "sen\u00b7ket", "das", "stol\u00b7ze", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Auf ein wertes, ein w\u00fcrdiges Haupt", "tokens": ["Auf", "ein", "wer\u00b7tes", ",", "ein", "w\u00fcr\u00b7di\u00b7ges", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Legt er die k\u00f6stliche Spende nieder;", "tokens": ["Legt", "er", "die", "k\u00f6st\u00b7li\u00b7che", "Spen\u00b7de", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "O, ihr kennt es, ihr kennet es, Br\u00fcder!", "tokens": ["O", ",", "ihr", "kennt", "es", ",", "ihr", "ken\u00b7net", "es", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ists gleich dem Sang nicht zu nennen erlaubt.", "tokens": ["Ists", "gleich", "dem", "Sang", "nicht", "zu", "nen\u00b7nen", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Trefflicher! Weise den Schmuck nicht von dannen!", "tokens": ["Treff\u00b7li\u00b7cher", "!", "Wei\u00b7se", "den", "Schmuck", "nicht", "von", "dan\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ART", "NN", "PTKNEG", "APPR", "ADV", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ging gleich nach Schmuck dein Begehren nie;", "tokens": ["Ging", "gleich", "nach", "Schmuck", "dein", "Be\u00b7geh\u00b7ren", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Reihe dich zu den kr\u00e4ftigen Mannen,", "tokens": ["Rei\u00b7he", "dich", "zu", "den", "kr\u00e4f\u00b7ti\u00b7gen", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die das Werk der Freiheit begannen,", "tokens": ["Die", "das", "Werk", "der", "Frei\u00b7heit", "be\u00b7gan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du, ein Befreier, so wie sie!", "tokens": ["Du", ",", "ein", "Be\u00b7frei\u00b7er", ",", "so", "wie", "sie", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Befreier von st\u00e4rkeren Banden,", "tokens": ["Ein", "Be\u00b7frei\u00b7er", "von", "st\u00e4r\u00b7ke\u00b7ren", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als Tyrannei sie jemals gest\u00e4hlt.", "tokens": ["Als", "Ty\u00b7ran\u00b7nei", "sie", "je\u00b7mals", "ge\u00b7st\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ketten, die trotzende Kr\u00e4fte wanden,", "tokens": ["Ket\u00b7ten", ",", "die", "trot\u00b7zen\u00b7de", "Kr\u00e4f\u00b7te", "wan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Haben noch nie der Kraft widerstanden,", "tokens": ["Ha\u00b7ben", "noch", "nie", "der", "Kraft", "wi\u00b7der\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Nie hat dem Zwingherrn ein Gegner gefehlt.", "tokens": ["Nie", "hat", "dem", "Zwing\u00b7herrn", "ein", "Geg\u00b7ner", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Offen stellt die Gefahr sich entgegen,", "tokens": ["Of\u00b7fen", "stellt", "die", "Ge\u00b7fahr", "sich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wecket den Gegner mit st\u00fcrmender Hand,", "tokens": ["We\u00b7cket", "den", "Geg\u00b7ner", "mit", "st\u00fcr\u00b7men\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Pocht an den Busen mit donnernden Schl\u00e4gen,", "tokens": ["Pocht", "an", "den", "Bu\u00b7sen", "mit", "don\u00b7nern\u00b7den", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Da\u00df die schlummernden Kr\u00e4fte sich regen,", "tokens": ["Da\u00df", "die", "schlum\u00b7mern\u00b7den", "Kr\u00e4f\u00b7te", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Eilig sich r\u00fcsten zum Widerstand.", "tokens": ["Ei\u00b7lig", "sich", "r\u00fcs\u00b7ten", "zum", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "La\u00dft uns die Kraft und den Mut und den Willen,", "tokens": ["La\u00dft", "uns", "die", "Kraft", "und", "den", "Mut", "und", "den", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wo ist Gefahr? Sie komme nur an!", "tokens": ["Wo", "ist", "Ge\u00b7fahr", "?", "Sie", "kom\u00b7me", "nur", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch was tief unter schmeichelnden H\u00fcllen", "tokens": ["Doch", "was", "tief", "un\u00b7ter", "schmei\u00b7cheln\u00b7den", "H\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADJD", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Heimlich naget und gr\u00e4bt im Stillen,", "tokens": ["Heim\u00b7lich", "na\u00b7get", "und", "gr\u00e4bt", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Da gilts zu zittern; da bebt auch der Mann!", "tokens": ["Da", "gilts", "zu", "zit\u00b7tern", ";", "da", "bebt", "auch", "der", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Was in dem eigenen Busen geboren,", "tokens": ["Was", "in", "dem", "ei\u00b7ge\u00b7nen", "Bu\u00b7sen", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Krieg dem eigenen Busen erregt,", "tokens": ["Krieg", "dem", "ei\u00b7ge\u00b7nen", "Bu\u00b7sen", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das sein Ich zum Gott sich erkoren", "tokens": ["Das", "sein", "Ich", "zum", "Gott", "sich", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "APPRART", "NN", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und dem Moloch, dem es geschworen,", "tokens": ["Und", "dem", "Mo\u00b7loch", ",", "dem", "es", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das eigene Kind in die Arme legt;", "tokens": ["Das", "ei\u00b7ge\u00b7ne", "Kind", "in", "die", "Ar\u00b7me", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Eigennutz, die gefr\u00e4\u00df'ge Hy\u00e4ne,", "tokens": ["Ei\u00b7gen\u00b7nutz", ",", "die", "ge\u00b7fr\u00e4\u00df'\u00b7ge", "Hy\u00b7\u00e4\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Eigenliebe, sich Gott und Altar,", "tokens": ["Ei\u00b7gen\u00b7lie\u00b7be", ",", "sich", "Gott", "und", "Al\u00b7tar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Selbstsucht, wetzend die gierigen Z\u00e4hne,", "tokens": ["Selbst\u00b7sucht", ",", "wet\u00b7zend", "die", "gie\u00b7ri\u00b7gen", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "L\u00fcstern schl\u00fcrfend des Bruders Tr\u00e4ne \u2013", "tokens": ["L\u00fcs\u00b7tern", "schl\u00fcr\u00b7fend", "des", "Bru\u00b7ders", "Tr\u00e4\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Austria! ", "tokens": ["Aust\u00b7ria", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Ob dieser Brut von zischenden Schlangen", "tokens": ["Ob", "die\u00b7ser", "Brut", "von", "zi\u00b7schen\u00b7den", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hast du, Starker, den Fu\u00df gesetzt.", "tokens": ["Hast", "du", ",", "Star\u00b7ker", ",", "den", "Fu\u00df", "ge\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ende das Werk, das du angefangen,", "tokens": ["En\u00b7de", "das", "Werk", ",", "das", "du", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und dein Bild soll ewig uns prangen,", "tokens": ["Und", "dein", "Bild", "soll", "e\u00b7wig", "uns", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "In der Zukunft Hallen wie jetzt.", "tokens": ["In", "der", "Zu\u00b7kunft", "Hal\u00b7len", "wie", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KOKOM", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Auf! du Starker! es mu\u00df gelingen!", "tokens":