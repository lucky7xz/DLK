{"textgrid.poem.34216": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine r\u00f6mische Courtisane und die Polizei", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4na, ich finde dich tragisch gestellt in der heiligen Roma:", "tokens": ["N\u00e4\u00b7na", ",", "ich", "fin\u00b7de", "dich", "tra\u00b7gisch", "ge\u00b7stellt", "in", "der", "hei\u00b7li\u00b7gen", "Ro\u00b7ma", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Alle sollen dich seh'n, die nur, die Alles seh'n, nicht.", "tokens": ["Al\u00b7le", "sol\u00b7len", "dich", "seh'n", ",", "die", "nur", ",", "die", "Al\u00b7les", "seh'n", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$,", "PRELS", "ADV", "$,", "PRELS", "PIS", "VVINF", "$,", "PTKNEG", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}