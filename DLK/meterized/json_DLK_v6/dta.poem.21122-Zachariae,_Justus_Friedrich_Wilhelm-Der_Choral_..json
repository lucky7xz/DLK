{"dta.poem.21122": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Choral .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlummer und schimmernder Reif, und stille ver-\ntrauliche Wolken", "tokens": ["Schlum\u00b7mer", "und", "schim\u00b7mern\u00b7der", "Reif", ",", "und", "stil\u00b7le", "ver", "trau\u00b7li\u00b7che", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "KON", "ADJA", "TRUNC", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00e4ngen schon \u00fcber der schlafenden Welt.", "tokens": ["H\u00e4n\u00b7gen", "schon", "\u00fc\u00b7ber", "der", "schla\u00b7fen\u00b7den", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Breite dich, einsame Nacht, mit sanfteinwiegenden Fl\u00fc-", "tokens": ["Brei\u00b7te", "dich", ",", "ein\u00b7sa\u00b7me", "Nacht", ",", "mit", "sanf\u00b7tein\u00b7wie\u00b7gen\u00b7den", "Fl\u00fc"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Ueber die ruhige H\u00e4lfte der Welt.", "tokens": ["Ue\u00b7ber", "die", "ru\u00b7hi\u00b7ge", "H\u00e4lf\u00b7te", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Traurig versinkt die Natur in einen heiligen Schauer,", "tokens": ["Trau\u00b7rig", "ver\u00b7sinkt", "die", "Na\u00b7tur", "in", "ei\u00b7nen", "hei\u00b7li\u00b7gen", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wie er in W\u00e4ldern der Barden gewohnt;", "tokens": ["Wie", "er", "in", "W\u00e4l\u00b7dern", "der", "Bar\u00b7den", "ge\u00b7wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Oder auch, wie er vordem auf menschenfeindliche Grot-", "tokens": ["O\u00b7der", "auch", ",", "wie", "er", "vor\u00b7dem", "auf", "men\u00b7schen\u00b7feind\u00b7li\u00b7che", "Grot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Frommer veralteter Einsiedler fiel.", "tokens": ["From\u00b7mer", "ver\u00b7al\u00b7te\u00b7ter", "Ein\u00b7sied\u00b7ler", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Singe der Mitternacht ietzt, du S\u00e4nger auf silbernen", "tokens": ["Sin\u00b7ge", "der", "Mit\u00b7ter\u00b7nacht", "ietzt", ",", "du", "S\u00e4n\u00b7ger", "auf", "sil\u00b7ber\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PPER", "NN", "APPR", "NN"], "meter": "+--+-+--+--+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "Heilig, der Nacht gleich, sey heilig dein Lied.", "tokens": ["Hei\u00b7lig", ",", "der", "Nacht", "gleich", ",", "sey", "hei\u00b7lig", "dein", "Lied", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ART", "NN", "ADV", "$,", "VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Singe den hohen Choral mit Bachs ehrw\u00fcrdigen T\u00f6nen;", "tokens": ["Sin\u00b7ge", "den", "ho\u00b7hen", "Cho\u00b7ral", "mit", "Bachs", "ehr\u00b7w\u00fcr\u00b7di\u00b7gen", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "F\u00fclle mit Andacht das zitternde Herz!", "tokens": ["F\u00fcl\u00b7le", "mit", "An\u00b7dacht", "das", "zit\u00b7tern\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch ein erhabner Gesang! Die Seele f\u00fchlt ihn; und", "tokens": ["Welch", "ein", "er\u00b7hab\u00b7ner", "Ge\u00b7sang", "!", "Die", "See\u00b7le", "f\u00fchlt", "ihn", ";", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$.", "KON"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schwingt sie sich \u00fcber die Himmel hinauf.", "tokens": ["Schwingt", "sie", "sich", "\u00fc\u00b7ber", "die", "Him\u00b7mel", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "So, aber r\u00fchrender noch, ert\u00f6nten die Ch\u00f6re des Him-", "tokens": ["So", ",", "a\u00b7ber", "r\u00fch\u00b7ren\u00b7der", "noch", ",", "er\u00b7t\u00f6n\u00b7ten", "die", "Ch\u00f6\u00b7re", "des", "Him"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "ADV", "$,", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "M\u00e4rtyrer! als ihr, mit Blute bedeckt,", "tokens": ["M\u00e4r\u00b7ty\u00b7rer", "!", "als", "ihr", ",", "mit", "Blu\u00b7te", "be\u00b7deckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Eure gefaltete Hand zum Ewigen aufhubt, und ruhig", "tokens": ["Eu\u00b7re", "ge\u00b7fal\u00b7te\u00b7te", "Hand", "zum", "E\u00b7wi\u00b7gen", "auf\u00b7hubt", ",", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "VVPP", "$,", "KON", "ADJD"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Unter den Qualen den Feinden vergabt;", "tokens": ["Un\u00b7ter", "den", "Qua\u00b7len", "den", "Fein\u00b7den", "ver\u00b7gabt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Als euch der Seraph erschien, und triumphirend die", "tokens": ["Als", "euch", "der", "Se\u00b7raph", "er\u00b7schien", ",", "und", "tri\u00b7um\u00b7phi\u00b7rend", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "ADJD", "ART"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ueber des Todesthals Schrecknisse hob.", "tokens": ["Ue\u00b7ber", "des", "To\u00b7dest\u00b7hals", "Schreck\u00b7nis\u00b7se", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Die du den S\u00e4nger gelehrt, o Tonkunst unter den En-", "tokens": ["Die", "du", "den", "S\u00e4n\u00b7ger", "ge\u00b7lehrt", ",", "o", "Ton\u00b7kunst", "un\u00b7ter", "den", "En"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "FM", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sing ihm, du heilige S\u00e4ngerin, auch,", "tokens": ["Sing", "ihm", ",", "du", "hei\u00b7li\u00b7ge", "S\u00e4n\u00b7ge\u00b7rin", ",", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wenn er die Stunde nun sieht, die f\u00fcrchterlichste der", "tokens": ["Wenn", "er", "die", "Stun\u00b7de", "nun", "sieht", ",", "die", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ART"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Welche den Christen oft selber erschreckt.", "tokens": ["Wel\u00b7che", "den", "Chris\u00b7ten", "oft", "sel\u00b7ber", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Leit ihn mit sicherem Schritt dann \u00fcber die B\u00e4che des", "tokens": ["Leit", "ihn", "mit", "si\u00b7che\u00b7rem", "Schritt", "dann", "\u00fc\u00b7ber", "die", "B\u00e4\u00b7che", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "ART"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sing ihm den hohen Gesang des Olymps!", "tokens": ["Sing", "ihm", "den", "ho\u00b7hen", "Ge\u00b7sang", "des", "O\u00b7lymps", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Stimmen des heiligen Chors und Stimmen der g\u00f6tt-", "tokens": ["Stim\u00b7men", "des", "hei\u00b7li\u00b7gen", "Chors", "und", "Stim\u00b7men", "der", "g\u00f6t\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "NN", "ART", "TRUNC"], "meter": "+--+--+-+--++", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jauchzen ihm unter Unsterblichen zu!", "tokens": ["Jauch\u00b7zen", "ihm", "un\u00b7ter", "U\u00b7nsterb\u00b7li\u00b7chen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}