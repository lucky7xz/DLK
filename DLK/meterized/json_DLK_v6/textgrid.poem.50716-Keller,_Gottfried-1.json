{"textgrid.poem.50716": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Gold und Purpur eingeh\u00fcllt,", "tokens": ["In", "Gold", "und", "Pur\u00b7pur", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sonne! willst du scheiden?", "tokens": ["O", "Son\u00b7ne", "!", "willst", "du", "schei\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ich, von Freuden \u00fcberf\u00fcllt,", "tokens": ["Und", "ich", ",", "von", "Freu\u00b7den", "\u00fc\u00b7berf\u00b7\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll pl\u00f6tzlich dich nun meiden?", "tokens": ["Soll", "pl\u00f6tz\u00b7lich", "dich", "nun", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du hast mein Herz mit Lust entz\u00fcndet,", "tokens": ["Du", "hast", "mein", "Herz", "mit", "Lust", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du allersch\u00f6nste K\u00f6nigin!", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn mir dein strahlend Antlitz schwindet,", "tokens": ["Wenn", "mir", "dein", "strah\u00b7lend", "Ant\u00b7litz", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nicht das Feuer tot und hin?", "tokens": ["Ist", "nicht", "das", "Feu\u00b7er", "tot", "und", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O reiche mir noch ", "tokens": ["O", "rei\u00b7che", "mir", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der bindend, helfend auf mich falle,", "tokens": ["Der", "bin\u00b7dend", ",", "hel\u00b7fend", "auf", "mich", "fal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich aus diesem D\u00e4mmertal", "tokens": ["Da\u00df", "ich", "aus", "die\u00b7sem", "D\u00e4m\u00b7mer\u00b7tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deiner Hand hin\u00fcberwalle!", "tokens": ["An", "dei\u00b7ner", "Hand", "hin\u00b7\u00fc\u00b7berw\u00b7al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will dein treuer Page bleiben,", "tokens": ["Ich", "will", "dein", "treu\u00b7er", "Pa\u00b7ge", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Spiegel, wie das klare Meer,", "tokens": ["Dein", "Spie\u00b7gel", ",", "wie", "das", "kla\u00b7re", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Sch\u00e4fer deine L\u00e4mmer treiben,", "tokens": ["Als", "Sch\u00e4\u00b7fer", "dei\u00b7ne", "L\u00e4m\u00b7mer", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Morgenwolken, vor dir her.", "tokens": ["Die", "Mor\u00b7gen\u00b7wol\u00b7ken", ",", "vor", "dir", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als leichte, leichte Wolke nur", "tokens": ["Als", "leich\u00b7te", ",", "leich\u00b7te", "Wol\u00b7ke", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "$,", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich an deinem Hofe weilen,", "tokens": ["La\u00df", "mich", "an", "dei\u00b7nem", "Ho\u00b7fe", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als deines Glanzes schw\u00e4chste Spur", "tokens": ["Als", "dei\u00b7nes", "Glan\u00b7zes", "schw\u00e4chs\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor deinem Zuge k\u00fcndend eilen!", "tokens": ["Vor", "dei\u00b7nem", "Zu\u00b7ge", "k\u00fcn\u00b7dend", "ei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich pr\u00e4g als Lehrer neue Lieder", "tokens": ["Ich", "pr\u00e4g", "als", "Leh\u00b7rer", "neu\u00b7e", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KOKOM", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Lerchen, deinen Kindern, ein.", "tokens": ["Den", "Ler\u00b7chen", ",", "dei\u00b7nen", "Kin\u00b7dern", ",", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du willst mich nicht? du tauchest nieder?", "tokens": ["Du", "willst", "mich", "nicht", "?", "du", "tau\u00b7chest", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich bin im Schatten und allein!", "tokens": ["Ich", "bin", "im", "Schat\u00b7ten", "und", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wende mich verlassen ab,", "tokens": ["Ich", "wen\u00b7de", "mich", "ver\u00b7las\u00b7sen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist eine tote Kohle!", "tokens": ["Die", "Welt", "ist", "ei\u00b7ne", "to\u00b7te", "Koh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was sonst die Klarheit wiedergab,", "tokens": ["Was", "sonst", "die", "Klar\u00b7heit", "wie\u00b7der\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00e4ubt, Asche, unter meiner Sohle.", "tokens": ["St\u00e4ubt", ",", "A\u00b7sche", ",", "un\u00b7ter", "mei\u00b7ner", "Soh\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie ich mich gen Osten kehre,", "tokens": ["Doch", "wie", "ich", "mich", "gen", "Os\u00b7ten", "keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Steigt mir ein Wunder zaubrisch auf:", "tokens": ["Steigt", "mir", "ein", "Wun\u00b7der", "zaub\u00b7risch", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einem weichen Rosenmeere", "tokens": ["In", "ei\u00b7nem", "wei\u00b7chen", "Ro\u00b7sen\u00b7mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beginnt der Mond den Elfenlauf.", "tokens": ["Be\u00b7ginnt", "der", "Mond", "den", "El\u00b7fen\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gro\u00df, magisch, wie ein Riesenstern,", "tokens": ["Gro\u00df", ",", "ma\u00b7gisch", ",", "wie", "ein", "Rie\u00b7sens\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Seh ich ihn \u00fcberm Gr\u00fcnen hangen:", "tokens": ["Seh", "ich", "ihn", "\u00fc\u00b7berm", "Gr\u00fc\u00b7nen", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist nicht kalt, er ist nicht fern,", "tokens": ["Er", "ist", "nicht", "kalt", ",", "er", "ist", "nicht", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, warm und nah, wie zum Erlangen!", "tokens": ["Nein", ",", "warm", "und", "nah", ",", "wie", "zum", "Er\u00b7lan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist er der Sonne \u00c4hrenleser,", "tokens": ["Ist", "er", "der", "Son\u00b7ne", "\u00c4h\u00b7ren\u00b7le\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der nach verlornen Strahlen jagt?", "tokens": ["Der", "nach", "ver\u00b7lor\u00b7nen", "Strah\u00b7len", "jagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist er der Sonne Reichsverweser,", "tokens": ["Ist", "er", "der", "Son\u00b7ne", "Reichs\u00b7ver\u00b7we\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis wieder sie im Osten tagt?", "tokens": ["Bis", "wie\u00b7der", "sie", "im", "Os\u00b7ten", "tagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es webt von Silber und Kristall", "tokens": ["Es", "webt", "von", "Sil\u00b7ber", "und", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Netz sich \u00fcber alle R\u00e4ume,", "tokens": ["Ein", "Netz", "sich", "\u00fc\u00b7ber", "al\u00b7le", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie feiner Elfengl\u00f6cklein Schall", "tokens": ["Wie", "fei\u00b7ner", "El\u00b7fen\u00b7gl\u00f6c\u00b7klein", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erklingt das spr\u00f6de Laub der B\u00e4ume:", "tokens": ["Er\u00b7klingt", "das", "spr\u00f6\u00b7de", "Laub", "der", "B\u00e4u\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das sind des Mondes Zitterstrahlen,", "tokens": ["Das", "sind", "des", "Mon\u00b7des", "Zit\u00b7ter\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die schlagen an die Bl\u00e4tter an \u2013", "tokens": ["Die", "schla\u00b7gen", "an", "die", "Bl\u00e4t\u00b7ter", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf all dem Meer von Schein und Strahlen", "tokens": ["Auf", "all", "dem", "Meer", "von", "Schein", "und", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Auge schifft als leichter Kahn.", "tokens": ["Mein", "Au\u00b7ge", "schifft", "als", "leich\u00b7ter", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und weil mein Herz am Westen noch,", "tokens": ["Und", "weil", "mein", "Herz", "am", "Wes\u00b7ten", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abendrote h\u00e4ngt mit Sehnen,", "tokens": ["Am", "A\u00b7ben\u00b7dro\u00b7te", "h\u00e4ngt", "mit", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchl ich ein neues s\u00fc\u00dfes Joch", "tokens": ["F\u00fchl", "ich", "ein", "neu\u00b7es", "s\u00fc\u00b7\u00dfes", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Osten \u00fcber mich sich dehnen.", "tokens": ["Von", "Os\u00b7ten", "\u00fc\u00b7ber", "mich", "sich", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schwebe zwischen toten Wonnen", "tokens": ["Ich", "schwe\u00b7be", "zwi\u00b7schen", "to\u00b7ten", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und neuem Hoffen wunderbar", "tokens": ["Und", "neu\u00b7em", "Hof\u00b7fen", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie eine Knospe eingesponnen,", "tokens": ["Wie", "ei\u00b7ne", "Knos\u00b7pe", "ein\u00b7ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die erst noch am Ersterben war!", "tokens": ["Die", "erst", "noch", "am", "E\u00b7rster\u00b7ben", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es ist auf Erden keine Nacht,", "tokens": ["Es", "ist", "auf", "Er\u00b7den", "kei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht noch ihren Schimmer h\u00e4tte!", "tokens": ["Die", "nicht", "noch", "ih\u00b7ren", "Schim\u00b7mer", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So gro\u00df ist keines Ungl\u00fccks Macht:", "tokens": ["So", "gro\u00df", "ist", "kei\u00b7nes", "Un\u00b7gl\u00fccks", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bl\u00fcmlein h\u00e4ngt in seiner Kette.", "tokens": ["Ein", "Bl\u00fcm\u00b7lein", "h\u00e4ngt", "in", "sei\u00b7ner", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist das Herz vom rechten Schlage,", "tokens": ["Und", "ist", "das", "Herz", "vom", "rech\u00b7ten", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So baut es sich ein Sternenhaus", "tokens": ["So", "baut", "es", "sich", "ein", "Ster\u00b7nen\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und macht die Nacht zum hellen Tage,", "tokens": ["Und", "macht", "die", "Nacht", "zum", "hel\u00b7len", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo sonst nur Asche, Schutt und Graus!", "tokens": ["Wo", "sonst", "nur", "A\u00b7sche", ",", "Schutt", "und", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der du im Unsichtbaren schwebst,", "tokens": ["Der", "du", "im", "Un\u00b7sicht\u00b7ba\u00b7ren", "schwebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch immer in mir widerklingend!", "tokens": ["Doch", "im\u00b7mer", "in", "mir", "wi\u00b7der\u00b7klin\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der du die goldnen Br\u00fccken webst,", "tokens": ["Der", "du", "die", "gold\u00b7nen", "Br\u00fc\u00b7cken", "webst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Welten sie zu Welten schwingend,", "tokens": ["Von", "Wel\u00b7ten", "sie", "zu", "Wel\u00b7ten", "schwin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast ein Liebesband gewoben", "tokens": ["Du", "hast", "ein", "Lie\u00b7bes\u00b7band", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir um das Herz so alt und krank,", "tokens": ["Mir", "um", "das", "Herz", "so", "alt", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast mich aus dem Staub erhoben \u2013", "tokens": ["Du", "hast", "mich", "aus", "dem", "Staub", "er\u00b7ho\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du gro\u00dfer Weber, habe Dank!", "tokens": ["Du", "gro\u00b7\u00dfer", "We\u00b7ber", ",", "ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}