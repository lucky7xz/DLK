{"textgrid.poem.59621": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gericht", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wirbeln die Trommeln: Heraus! Heraus!", "tokens": ["Es", "wir\u00b7beln", "die", "Trom\u00b7meln", ":", "He\u00b7raus", "!", "He\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Feind ist vergangen mit Mann und Maus,", "tokens": ["Der", "Feind", "ist", "ver\u00b7gan\u00b7gen", "mit", "Mann", "und", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da liegt er zerschmettert durch Gottes Hand,", "tokens": ["Da", "liegt", "er", "zer\u00b7schmet\u00b7tert", "durch", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und seine Gebeine bedeckt kein Sand.", "tokens": ["Und", "sei\u00b7ne", "Ge\u00b7bei\u00b7ne", "be\u00b7deckt", "kein", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die Raben, sie kr\u00e4chzen, die Kr\u00e4hen sind laut,", "tokens": ["Die", "Ra\u00b7ben", ",", "sie", "kr\u00e4ch\u00b7zen", ",", "die", "Kr\u00e4\u00b7hen", "sind", "laut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Winter hat Br\u00fccken von Eis gebaut,", "tokens": ["Der", "Win\u00b7ter", "hat", "Br\u00fc\u00b7cken", "von", "Eis", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Hunger ist bitter und tief der Schnee \u2013", "tokens": ["Der", "Hun\u00b7ger", "ist", "bit\u00b7ter", "und", "tief", "der", "Schnee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was will das bedeuten? O weh! O weh!", "tokens": ["Was", "will", "das", "be\u00b7deu\u00b7ten", "?", "O", "weh", "!", "O", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VVINF", "$.", "NE", "PTKVZ", "$.", "NE", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Vierhundertmaltausend zu Ro\u00df und Fu\u00df", "tokens": ["Vier\u00b7hun\u00b7dert\u00b7mal\u00b7tau\u00b7send", "zu", "Ro\u00df", "und", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gingen \u00fcber die Weichsel, den Niemenflu\u00df,", "tokens": ["Gin\u00b7gen", "\u00fc\u00b7ber", "die", "Weich\u00b7sel", ",", "den", "Nie\u00b7men\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vierhundertmaltausend \u2013 o welch ein Heer!", "tokens": ["Vier\u00b7hun\u00b7dert\u00b7mal\u00b7tau\u00b7send", "\u2013", "o", "welch", "ein", "Heer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "FM", "PWAT", "ART", "NN", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gott hat sie zerst\u00f6rt und der Russen Wehr.", "tokens": ["Gott", "hat", "sie", "zer\u00b7st\u00f6rt", "und", "der", "Rus\u00b7sen", "Wehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "KON", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Napoleon hat auf den Teufel gebaut,", "tokens": ["Na\u00b7po\u00b7le\u00b7on", "hat", "auf", "den", "Teu\u00b7fel", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Alexander der Kaiser hat Gott vertraut,", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "der", "Kai\u00b7ser", "hat", "Gott", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "NN", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Franzen verehrten Wollust und Geiz,", "tokens": ["Die", "Fran\u00b7zen", "ver\u00b7ehr\u00b7ten", "Wol\u00b7lust", "und", "Geiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Russen verehrten das heilige Kreuz.", "tokens": ["Die", "Rus\u00b7sen", "ver\u00b7ehr\u00b7ten", "das", "hei\u00b7li\u00b7ge", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Die Franzen, sie prahlten im trotzigen Mut,", "tokens": ["Die", "Fran\u00b7zen", ",", "sie", "prahl\u00b7ten", "im", "trot\u00b7zi\u00b7gen", "Mut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Russen, sie brannten von frommer Glut,", "tokens": ["Die", "Rus\u00b7sen", ",", "sie", "brann\u00b7ten", "von", "from\u00b7mer", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Franzen, sie stritten f\u00fcr Lug und Wind,", "tokens": ["Die", "Fran\u00b7zen", ",", "sie", "strit\u00b7ten", "f\u00fcr", "Lug", "und", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Russen, sie stritten f\u00fcr Weib und Kind.", "tokens": ["Die", "Rus\u00b7sen", ",", "sie", "strit\u00b7ten", "f\u00fcr", "Weib", "und", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Drum wurden die Franzen wie Sand verweht,", "tokens": ["Drum", "wur\u00b7den", "die", "Fran\u00b7zen", "wie", "Sand", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wann Sturmwind hinunter-, hin\u00fcbergeht;", "tokens": ["Wann", "Sturm\u00b7wind", "hin\u00b7un\u00b7ter", ",", "hin\u00b7\u00fc\u00b7ber\u00b7geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "TRUNC", "$,", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ihr Grab ist das Wasser, der Schnee ihr Bett,", "tokens": ["Ihr", "Grab", "ist", "das", "Was\u00b7ser", ",", "der", "Schnee", "ihr", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie machen die W\u00f6lfe und Raben fett.", "tokens": ["Sie", "ma\u00b7chen", "die", "W\u00f6l\u00b7fe", "und", "Ra\u00b7ben", "fett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Es wirbeln die Trommeln: Heraus! Heraus!", "tokens": ["Es", "wir\u00b7beln", "die", "Trom\u00b7meln", ":", "He\u00b7raus", "!", "He\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zum Kriege der Rache ins Feld hinaus!", "tokens": ["Zum", "Krie\u00b7ge", "der", "Ra\u00b7che", "ins", "Feld", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Franzen sie fliehen! Frisch hinterdrein!", "tokens": ["Die", "Fran\u00b7zen", "sie", "flie\u00b7hen", "!", "Frisch", "hin\u00b7ter\u00b7drein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$.", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wer wollte wohl heute der letzte sein?", "tokens": ["Wer", "woll\u00b7te", "wohl", "heu\u00b7te", "der", "letz\u00b7te", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Auf! Frische Gesellen! Und greift die Wehr!", "tokens": ["Auf", "!", "Fri\u00b7sche", "Ge\u00b7sel\u00b7len", "!", "Und", "greift", "die", "Wehr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ADJA", "NN", "$.", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf! \u00dcber die schn\u00f6den Banditen her!", "tokens": ["Auf", "!", "\u00dc\u00b7ber", "die", "schn\u00f6\u00b7den", "Ban\u00b7di\u00b7ten", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00e4rbt Lanzen und Schwerter im Blute rot", "tokens": ["F\u00e4rbt", "Lan\u00b7zen", "und", "Schwer\u00b7ter", "im", "Blu\u00b7te", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "APPRART", "NN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schlaget die Schelmenfranzosen tot.", "tokens": ["Und", "schla\u00b7get", "die", "Schel\u00b7men\u00b7fran\u00b7zo\u00b7sen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "So hetzet sie munter zum Rhein, zum Rhein,", "tokens": ["So", "het\u00b7zet", "sie", "mun\u00b7ter", "zum", "Rhein", ",", "zum", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NE", "$,", "APPRART", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und \u00fcber den Rhein und \u00fcber den Rhein;", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Rhein", "und", "\u00fc\u00b7ber", "den", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "KON", "APPR", "ART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo Worte der Treue der Deutsche spricht,", "tokens": ["Wo", "Wor\u00b7te", "der", "Treu\u00b7e", "der", "Deut\u00b7sche", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da wohne franz\u00f6sische L\u00fcge nicht.", "tokens": ["Da", "woh\u00b7ne", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "L\u00fc\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Raben sie kr\u00e4chzen, die Kr\u00e4hen sind laut,", "tokens": ["Die", "Ra\u00b7ben", "sie", "kr\u00e4ch\u00b7zen", ",", "die", "Kr\u00e4\u00b7hen", "sind", "laut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Winter hat Br\u00fccken von Eis gebaut,", "tokens": ["Der", "Win\u00b7ter", "hat", "Br\u00fc\u00b7cken", "von", "Eis", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Hunger ist bitter und tief der Schnee \u2013", "tokens": ["Der", "Hun\u00b7ger", "ist", "bit\u00b7ter", "und", "tief", "der", "Schnee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was will das bedeuten? O weh! O weh!", "tokens": ["Was", "will", "das", "be\u00b7deu\u00b7ten", "?", "O", "weh", "!", "O", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VVINF", "$.", "NE", "PTKVZ", "$.", "NE", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}