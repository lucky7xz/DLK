{"dta.poem.13213": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia fragte mich, ob ich die Lebens-Zeit", "tokens": ["Tha\u00b7lia", "frag\u00b7te", "mich", ",", "ob", "ich", "die", "Le\u00b7bens\u00b7Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dergleichen Lust gesehn, und solche Z\u00e4rtlichkeit?", "tokens": ["Derg\u00b7lei\u00b7chen", "Lust", "ge\u00b7sehn", ",", "und", "sol\u00b7che", "Z\u00e4rt\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man h\u00f6rt\u2019 und sahe nichts als Schmeicheln, Spielen, Herzen,", "tokens": ["Man", "h\u00f6rt'", "und", "sa\u00b7he", "nichts", "als", "Schmei\u00b7cheln", ",", "Spie\u00b7len", ",", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PIS", "KOKOM", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ganze Saal empfand die Reizung mit zu scherzen.", "tokens": ["Der", "gan\u00b7ze", "Saal", "emp\u00b7fand", "die", "Rei\u00b7zung", "mit", "zu", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "365Welch ungemeines Gl\u00fcck erf\u00fcllte nicht den Saal", "tokens": ["un\u00b7ge\u00b7mei\u00b7nes", "Gl\u00fcck", "er\u00b7f\u00fcll\u00b7te", "nicht", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Bey diesem Kinder-Chor, bey diesem Freuden-Mahl!", "tokens": ["Bey", "die\u00b7sem", "Kin\u00b7der\u00b7Chor", ",", "bey", "die\u00b7sem", "Freu\u00b7den\u00b7Mahl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "War irgendswo ein Kind hier oder dort umrungen,", "tokens": ["War", "ir\u00b7gends\u00b7wo", "ein", "Kind", "hier", "o\u00b7der", "dort", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So merckten wir da\u00df sich dahin die meisten drungen;", "tokens": ["So", "merck\u00b7ten", "wir", "da\u00df", "sich", "da\u00b7hin", "die", "meis\u00b7ten", "drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PRF", "PAV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bi\u00df endlich eine die\u00df, die andre jenes hielt;", "tokens": ["Bi\u00df", "end\u00b7lich", "ei\u00b7ne", "die\u00df", ",", "die", "and\u00b7re", "je\u00b7nes", "hielt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "PDS", "$,", "PRELS", "PIS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "370Das machte, da\u00df der Streit sich allgemach gestillt.", "tokens": ["mach\u00b7te", ",", "da\u00df", "der", "Streit", "sich", "all\u00b7ge\u00b7mach", "ge\u00b7stillt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Nur bey dem guten Rath gab es noch Artigkeiten,", "tokens": ["Nur", "bey", "dem", "gu\u00b7ten", "Rath", "gab", "es", "noch", "Ar\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Die seines Prinzens Aug\u2019 und Herz und Hand erfreuten.", "tokens": ["Die", "sei\u00b7nes", "Prin\u00b7zens", "Aug'", "und", "Herz", "und", "Hand", "er\u00b7freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein ganzes Leben war in solcher Freud\u2019 und Lust,", "tokens": ["Sein", "gan\u00b7zes", "Le\u00b7ben", "war", "in", "sol\u00b7cher", "Freud'", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ihn der treue Grei\u00df fast nicht zu halten wu\u00dft.", "tokens": ["Da\u00df", "ihn", "der", "treu\u00b7e", "Grei\u00df", "fast", "nicht", "zu", "hal\u00b7ten", "wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "375So lebhaft und erweckt, so geistreich sein Gebl\u00fcthe;", "tokens": ["leb\u00b7haft", "und", "er\u00b7weckt", ",", "so", "geist\u00b7reich", "sein", "Ge\u00b7bl\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "So Kummer-voll und schwach schien dieses Manns Gem\u00fcthe.", "tokens": ["So", "Kum\u00b7mer\u00b7voll", "und", "schwach", "schien", "die\u00b7ses", "Manns", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJD", "VVFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}