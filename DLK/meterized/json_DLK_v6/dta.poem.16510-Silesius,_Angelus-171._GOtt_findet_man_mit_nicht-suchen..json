{"dta.poem.16510": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "171.  GOtt findet man mit nicht-suchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist nicht hier noch da: wer jhn begehrt zufinden", "tokens": ["Gott", "ist", "nicht", "hier", "noch", "da", ":", "wer", "jhn", "be\u00b7gehrt", "zu\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "ADV", "ADV", "$.", "PWS", "PPER", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der la\u00df\u2019 jhm H\u00e4nd\u2019 und F\u00fc\u00df\u2019/ und Leib un\u0304 Seele binden.", "tokens": ["Der", "la\u00df'", "jhm", "H\u00e4nd'", "und", "F\u00fc\u00df'", "/", "und", "Leib", "un\u0304", "See\u00b7le", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}