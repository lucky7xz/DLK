{"textgrid.poem.30516": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Puta kan die Kunst zu tadeln; alles wird von ihr veracht;", "tokens": ["Pu\u00b7ta", "kan", "die", "Kunst", "zu", "ta\u00b7deln", ";", "al\u00b7les", "wird", "von", "ihr", "ver\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PTKZU", "VVINF", "$.", "PIS", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Andre k\u00fcnnen Kunst zum spotten, und ihr tadeln wird verlacht.", "tokens": ["And\u00b7re", "k\u00fcn\u00b7nen", "Kunst", "zum", "spot\u00b7ten", ",", "und", "ihr", "ta\u00b7deln", "wird", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "APPRART", "VVFIN", "$,", "KON", "PPER", "VVINF", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}