{"textgrid.poem.63063": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Wind schritt wild", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wind schritt wild von des Flusses Ramft", "tokens": ["Der", "Wind", "schritt", "wild", "von", "des", "Flus\u00b7ses", "Ramft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Empor und hat die Getreidefelder wie ein Riese niedergestampft.", "tokens": ["Em\u00b7por", "und", "hat", "die", "Ge\u00b7trei\u00b7de\u00b7fel\u00b7der", "wie", "ein", "Rie\u00b7se", "nie\u00b7der\u00b7ge\u00b7stampft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Dann strich der Regen nieder, Regenb\u00e4che sprangen wie silberne Hunde", "tokens": ["Dann", "strich", "der", "Re\u00b7gen", "nie\u00b7der", ",", "Re\u00b7gen\u00b7b\u00e4\u00b7che", "spran\u00b7gen", "wie", "sil\u00b7ber\u00b7ne", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKVZ", "$,", "NN", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Vor mir im schwarzen Erdreich auf \u2013 ich sah auf ihrem Grunde", "tokens": ["Vor", "mir", "im", "schwar\u00b7zen", "Er\u00b7dreich", "auf", "\u2013", "ich", "sah", "auf", "ih\u00b7rem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "APPR", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Den Himmel: wolkig, zerfetzt, leuchtend zerrissen \u2013 und ein Augenpaar,", "tokens": ["Den", "Him\u00b7mel", ":", "wol\u00b7kig", ",", "zer\u00b7fetzt", ",", "leuch\u00b7tend", "zer\u00b7ris\u00b7sen", "\u2013", "und", "ein", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "$,", "VVPP", "$,", "ADJD", "VVPP", "$(", "KON", "ART", "NN", "$,"], "meter": "-+--+-++--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Das wie der Himmel: wolkig, zerfetzt, leuchtend zerrissen war.", "tokens": ["Das", "wie", "der", "Him\u00b7mel", ":", "wol\u00b7kig", ",", "zer\u00b7fetzt", ",", "leuch\u00b7tend", "zer\u00b7ris\u00b7sen", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "$.", "ADJD", "$,", "VVPP", "$,", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-++--+-+", "measure": "iambic.septa.relaxed"}}}}}