{"dta.poem.3873": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "3) Als er ihm seinen Zustand melden muste.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mir hat des Vaters Bild gezeigt", "tokens": ["Mir", "hat", "des", "Va\u00b7ters", "Bild", "ge\u00b7zeigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man zum Ehren-Gipffel steigt,", "tokens": ["Wie", "man", "zum", "Eh\u00b7ren\u00b7Gipf\u00b7fel", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ihn der Wei\u00dfheit Sanfftmuths-Proben,", "tokens": ["Da", "ihn", "der", "Wei\u00df\u00b7heit", "Sanf\u00b7ft\u00b7muths\u00b7Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Gott Lob! bis itzo hoch erhoben,", "tokens": ["Gott", "Lob", "!", "bis", "it\u00b7zo", "hoch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "APPR", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du frommer Priester deiner Stadt,", "tokens": ["Du", "from\u00b7mer", "Pries\u00b7ter", "dei\u00b7ner", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du redlicher aus Gilead,", "tokens": ["Du", "red\u00b7li\u00b7cher", "aus", "Gi\u00b7le\u00b7ad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Beyspiel soll mich k\u00fcnfftig lehren", "tokens": ["Dein", "Bey\u00b7spiel", "soll", "mich", "k\u00fcnff\u00b7tig", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Tugend ernstlich zu verehren.", "tokens": ["Die", "Tu\u00b7gend", "ernst\u00b7lich", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich folge willig deinem Rath,", "tokens": ["Ich", "fol\u00b7ge", "wil\u00b7lig", "dei\u00b7nem", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den dein Befehl geheissen hat.", "tokens": ["Den", "dein", "Be\u00b7fehl", "ge\u00b7heis\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich suche stets in wahren Gr\u00fcnden", "tokens": ["Ich", "su\u00b7che", "stets", "in", "wah\u00b7ren", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Wei\u00dfheit s\u00fcsse Kost zu finden,", "tokens": ["Der", "Wei\u00df\u00b7heit", "s\u00fcs\u00b7se", "Kost", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Di\u00df lehren die ", "tokens": ["Di\u00df", "leh\u00b7ren", "die"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ist denn der sp\u00e4te Abend da,", "tokens": ["Ist", "denn", "der", "sp\u00e4\u00b7te", "A\u00b7bend", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So such ich, wie du mir befohlen,", "tokens": ["So", "such", "ich", ",", "wie", "du", "mir", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit Ernst und Flei\u00df zu wiederholen,", "tokens": ["Mit", "Ernst", "und", "Flei\u00df", "zu", "wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Di\u00df ist mein eintzig Augenmerck,", "tokens": ["Di\u00df", "ist", "mein", "eint\u00b7zig", "Au\u00b7gen\u00b7merck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Dichtkunst bleibt ein Nebenwerck,", "tokens": ["Die", "Dicht\u00b7kunst", "bleibt", "ein", "Ne\u00b7ben\u00b7werck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die legt die Ruhe zum Vergn\u00fcgen,", "tokens": ["Die", "legt", "die", "Ru\u00b7he", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Eh ich mich kan ins Bett verf\u00fcgen.", "tokens": ["Eh", "ich", "mich", "kan", "ins", "Bett", "ver\u00b7f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "In meinem Thun bin ich getrost,", "tokens": ["In", "mei\u00b7nem", "Thun", "bin", "ich", "ge\u00b7trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich liebe keine reiche Kost,", "tokens": ["Ich", "lie\u00b7be", "kei\u00b7ne", "rei\u00b7che", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mich reitzen keine Zauber-Th\u00f6ne,", "tokens": ["Mich", "reit\u00b7zen", "kei\u00b7ne", "Zau\u00b7ber\u00b7\u00b7T\u00b7h\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Gelassenheit ist meine Sch\u00f6ne.", "tokens": ["Ge\u00b7las\u00b7sen\u00b7heit", "ist", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Di\u00df alles, Vater! ist die Frucht", "tokens": ["Di\u00df", "al\u00b7les", ",", "Va\u00b7ter", "!", "ist", "die", "Frucht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PIS", "$,", "NN", "$.", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Von deiner treuen Kinder-Zucht.", "tokens": ["Von", "dei\u00b7ner", "treu\u00b7en", "Kin\u00b7der\u00b7Zucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Da hast du nun gantz ungeheuchelt,", "tokens": ["Da", "hast", "du", "nun", "gantz", "un\u00b7ge\u00b7heu\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da ich mir warrlich nicht geschmeichelt,", "tokens": ["Da", "ich", "mir", "warr\u00b7lich", "nicht", "ge\u00b7schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die Nachricht, wie dein Brief befohl,", "tokens": ["Die", "Nach\u00b7richt", ",", "wie", "dein", "Brief", "be\u00b7fohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Geliebter Vater, lebe wohl!", "tokens": ["Ge\u00b7lieb\u00b7ter", "Va\u00b7ter", ",", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}