{"textgrid.poem.65368": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Muse liebt das Reisen,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Muse liebt das Reisen,", "tokens": ["Mei\u00b7ne", "Mu\u00b7se", "liebt", "das", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehret gern bei Freunden ein:", "tokens": ["Keh\u00b7ret", "gern", "bei", "Freun\u00b7den", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neue Wirthe, neue Weisen,", "tokens": ["Neu\u00b7e", "Wirt\u00b7he", ",", "neu\u00b7e", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die neuesten sind dein.", "tokens": ["Und", "die", "neu\u00b7es\u00b7ten", "sind", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "PPOSAT", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "In dem gr\u00fcnen Felsenthale", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Fel\u00b7sen\u00b7tha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter dem Forellenbach", "tokens": ["Hin\u00b7ter", "dem", "Fo\u00b7rel\u00b7len\u00b7bach"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00df sie j\u00fcngst an deinem Mahle,", "tokens": ["Sa\u00df", "sie", "j\u00fcngst", "an", "dei\u00b7nem", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter deinem treuen Dach.", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "treu\u00b7en", "Dach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Fr\u00fchling streute nieder", "tokens": ["Und", "der", "Fr\u00fch\u00b7ling", "streu\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Gaben in das Gras.", "tokens": ["Sei\u00b7ne", "Ga\u00b7ben", "in", "das", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Muse suchte Lieder,", "tokens": ["Mei\u00b7ne", "Mu\u00b7se", "such\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie Maienblumen las.", "tokens": ["Wenn", "sie", "Mai\u00b7en\u00b7blu\u00b7men", "las", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, der Kranz, den sie gewunden", "tokens": ["Sieh", ",", "der", "Kranz", ",", "den", "sie", "ge\u00b7wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den liebsten, die sie fand,", "tokens": ["Von", "den", "liebs\u00b7ten", ",", "die", "sie", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dankbar ist er angebunden", "tokens": ["Dank\u00b7bar", "ist", "er", "an\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Wirthes Giebelwand.", "tokens": ["An", "des", "Wirt\u00b7hes", "Gie\u00b7bel\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}