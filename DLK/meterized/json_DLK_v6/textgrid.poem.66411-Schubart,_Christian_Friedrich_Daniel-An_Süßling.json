{"textgrid.poem.66411": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An S\u00fc\u00dfling", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mit dem sch\u00f6ngekr\u00e4usten Haare", "tokens": ["Du", "mit", "dem", "sch\u00f6n\u00b7ge\u00b7kr\u00e4us\u00b7ten", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00f6n bestrumpften Fu\u00df,", "tokens": ["Und", "sch\u00f6n", "be\u00b7strumpf\u00b7ten", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00fcnsche dir zum neuen Jahre,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "zum", "neu\u00b7en", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was man dir w\u00fcnschen mu\u00df.", "tokens": ["Was", "man", "dir", "w\u00fcn\u00b7schen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Stutzern pfleg' ich nicht zu scherzen;", "tokens": ["Mit", "Stut\u00b7zern", "pfleg'", "ich", "nicht", "zu", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Ernst, \u2013 hier hast du meine Hand, \u2013", "tokens": ["Im", "Ernst", ",", "\u2013", "hier", "hast", "du", "mei\u00b7ne", "Hand", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "$(", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich w\u00fcnsche dir \u2013 es geht mir recht von Herzen \u2013", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "\u2013", "es", "geht", "mir", "recht", "von", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich w\u00fcnsche dir \u2013 Verstand!", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "\u2013", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}