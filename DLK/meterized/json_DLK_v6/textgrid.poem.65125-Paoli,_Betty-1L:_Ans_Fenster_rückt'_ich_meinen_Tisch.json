{"textgrid.poem.65125": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ans Fenster r\u00fcckt' ich meinen Tisch", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ans Fenster r\u00fcckt' ich meinen Tisch", "tokens": ["Ans", "Fens\u00b7ter", "r\u00fcckt'", "ich", "mei\u00b7nen", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollte weise Dinge schreiben,", "tokens": ["Und", "woll\u00b7te", "wei\u00b7se", "Din\u00b7ge", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, eh' ichs dachte, sah ich frisch", "tokens": ["Doch", ",", "eh'", "ichs", "dach\u00b7te", ",", "sah", "ich", "frisch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Blatt im Morgenwinde treiben.", "tokens": ["Mein", "Blatt", "im", "Mor\u00b7gen\u00b7win\u00b7de", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was liegt an einem Blatt Papier?", "tokens": ["Was", "liegt", "an", "ei\u00b7nem", "Blatt", "Pa\u00b7pier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leicht ist's ein zweites zu bereiten!", "tokens": ["Leicht", "ist's", "ein", "zwei\u00b7tes", "zu", "be\u00b7rei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nun aber lie\u00df die Sonne mir", "tokens": ["Nun", "a\u00b7ber", "lie\u00df", "die", "Son\u00b7ne", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Streiflichter blendend dr\u00fcber gleiten.", "tokens": ["Streif\u00b7lich\u00b7ter", "blen\u00b7dend", "dr\u00fc\u00b7ber", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wie flogen sie so lustig hell", "tokens": ["Wie", "flo\u00b7gen", "sie", "so", "lus\u00b7tig", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pfeile von dem gold'nen Bogen!", "tokens": ["Die", "Pfei\u00b7le", "von", "dem", "gold'\u00b7nen", "Bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich einem Schilde lie\u00df ich schnell", "tokens": ["Gleich", "ei\u00b7nem", "Schil\u00b7de", "lie\u00df", "ich", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den gr\u00fcnen Vorhang niederwogen.", "tokens": ["Den", "gr\u00fc\u00b7nen", "Vor\u00b7hang", "nie\u00b7der\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt, meint' ich, jetzt wird Ruhe sein!", "tokens": ["Jetzt", ",", "meint'", "ich", ",", "jetzt", "wird", "Ru\u00b7he", "sein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Flei\u00dfes ernste Zeit beginne!", "tokens": ["Des", "Flei\u00b7\u00dfes", "erns\u00b7te", "Zeit", "be\u00b7gin\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So dacht' ich, still vergn\u00fcgt, allein", "tokens": ["So", "dacht'", "ich", ",", "still", "ver\u00b7gn\u00fcgt", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald ward ich meines Irrthums inne.", "tokens": ["Bald", "ward", "ich", "mei\u00b7nes", "Irr\u00b7thums", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn schmeichelnd und verlockend drang", "tokens": ["Denn", "schmei\u00b7chelnd", "und", "ver\u00b7lo\u00b7ckend", "drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Bl\u00e4ttergr\u00fcn und gr\u00fcnen Schleier", "tokens": ["Durch", "Bl\u00e4t\u00b7ter\u00b7gr\u00fcn", "und", "gr\u00fc\u00b7nen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der V\u00f6gel Lied wie Festgesang,", "tokens": ["Der", "V\u00f6\u00b7gel", "Lied", "wie", "Fest\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine freud'ge Liebesfeier.", "tokens": ["Wie", "ei\u00b7ne", "freu\u00b7d'\u00b7ge", "Lie\u00b7be\u00b7sfei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Was half es mir, da\u00df ich mein Ohr", "tokens": ["Was", "half", "es", "mir", ",", "da\u00df", "ich", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Lauschen suchte zu entw\u00f6hnen?", "tokens": ["Vom", "Lau\u00b7schen", "such\u00b7te", "zu", "ent\u00b7w\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Geiste h\u00f6rte ich den Chor", "tokens": ["Im", "Geis\u00b7te", "h\u00f6r\u00b7te", "ich", "den", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der s\u00fc\u00dfen Stimmen doch ert\u00f6nen.", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Stim\u00b7men", "doch", "er\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vergeblich sorgt' ich, da\u00df sich nicht", "tokens": ["Ver\u00b7geb\u00b7lich", "sorgt'", "ich", ",", "da\u00df", "sich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "PRF", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonne Schimmer zu mir stehle;", "tokens": ["Der", "Son\u00b7ne", "Schim\u00b7mer", "zu", "mir", "steh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ich von mir gebannt, das Licht,", "tokens": ["Das", "ich", "von", "mir", "ge\u00b7bannt", ",", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schaut' es doch in meiner Seele.", "tokens": ["Ich", "schaut'", "es", "doch", "in", "mei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da warf ich meine Feder hin!", "tokens": ["Da", "warf", "ich", "mei\u00b7ne", "Fe\u00b7der", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht l\u00e4nger konnt' ich widerstreben,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "konnt'", "ich", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gefangen war mir Herz und Sinn \u2013", "tokens": ["Ge\u00b7fan\u00b7gen", "war", "mir", "Herz", "und", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00dfte mich dem Lenz ergeben.", "tokens": ["Ich", "mu\u00df\u00b7te", "mich", "dem", "Lenz", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Aus meinem Hause trieb mich's fort", "tokens": ["Aus", "mei\u00b7nem", "Hau\u00b7se", "trieb", "mich's", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf waldgekr\u00f6nte Vergesh\u00f6hen,", "tokens": ["Auf", "wald\u00b7ge\u00b7kr\u00f6n\u00b7te", "Ver\u00b7ges\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo, wie ein mildes Segenswort,", "tokens": ["Wo", ",", "wie", "ein", "mil\u00b7des", "Se\u00b7gens\u00b7wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ahnungsvollen L\u00fcfte wehen.", "tokens": ["Die", "ah\u00b7nungs\u00b7vol\u00b7len", "L\u00fcf\u00b7te", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Den heil'gen Stimmen horchend, sa\u00df", "tokens": ["Den", "heil'\u00b7gen", "Stim\u00b7men", "hor\u00b7chend", ",", "sa\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dort bis sp\u00e4t zum Abendlichte,", "tokens": ["Ich", "dort", "bis", "sp\u00e4t", "zum", "A\u00b7bend\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine trunk'ne Seele las", "tokens": ["Und", "mei\u00b7ne", "trunk'\u00b7ne", "See\u00b7le", "las"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Gottes ewigem Gedichte!", "tokens": ["In", "Got\u00b7tes", "e\u00b7wi\u00b7gem", "Ge\u00b7dich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}