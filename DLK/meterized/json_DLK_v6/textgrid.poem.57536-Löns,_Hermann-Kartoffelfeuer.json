{"textgrid.poem.57536": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Kartoffelfeuer", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Ende September Kartoffelfeuer", "tokens": ["Wenn", "En\u00b7de", "Sep\u00b7tem\u00b7ber", "Kar\u00b7tof\u00b7fel\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit wei\u00dfem Schleier bedecken das Land,", "tokens": ["Mit", "wei\u00b7\u00dfem", "Schlei\u00b7er", "be\u00b7de\u00b7cken", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann denk' ich an manches, was ich als teuer", "tokens": ["Dann", "denk'", "ich", "an", "man\u00b7ches", ",", "was", "ich", "als", "teu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIS", "$,", "PWS", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meiner Erinnerung halte gebannt.", "tokens": ["In", "mei\u00b7ner", "E\u00b7rin\u00b7ne\u00b7rung", "hal\u00b7te", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Verflossene Zeiten, verflogene Tage,", "tokens": ["Ver\u00b7flos\u00b7se\u00b7ne", "Zei\u00b7ten", ",", "ver\u00b7flo\u00b7ge\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In rosigen Wolken die ganze Welt,", "tokens": ["In", "ro\u00b7si\u00b7gen", "Wol\u00b7ken", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Als noch nicht das Leben die h\u00e4\u00dfliche Frage", "tokens": ["Als", "noch", "nicht", "das", "Le\u00b7ben", "die", "h\u00e4\u00df\u00b7li\u00b7che", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbberuf und Brot?\u00ab an uns hatte gestellt.", "tokens": ["\u00bb", "be\u00b7ruf", "und", "Brot", "?", "\u00ab", "an", "uns", "hat\u00b7te", "ge\u00b7stellt", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "$.", "$(", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O Hannes mit knallroten Spitzbubenhaaren,", "tokens": ["O", "Han\u00b7nes", "mit", "knall\u00b7ro\u00b7ten", "Spitz\u00b7bu\u00b7ben\u00b7haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Wolf mit dem pechschwarzen Lockenkopf,", "tokens": ["O", "Wolf", "mit", "dem", "pech\u00b7schwar\u00b7zen", "Lo\u00b7cken\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich selber, ein Nichtsnutz von dreizehnhalb Jahren,", "tokens": ["Ich", "sel\u00b7ber", ",", "ein", "Nichts\u00b7nutz", "von", "drei\u00b7zehn\u00b7halb", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit Kletten und Disteln im flachsblonden Schopf.", "tokens": ["Mit", "Klet\u00b7ten", "und", "Dis\u00b7teln", "im", "flachs\u00b7blon\u00b7den", "Schopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Barf\u00fc\u00dfig, bark\u00f6pfig, zerrissene Hosen,", "tokens": ["Bar\u00b7f\u00fc\u00b7\u00dfig", ",", "bar\u00b7k\u00f6p\u00b7fig", ",", "zer\u00b7ris\u00b7se\u00b7ne", "Ho\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Knie schimmert durch die br\u00e4unliche Haut \u2013", "tokens": ["Am", "Knie", "schim\u00b7mert", "durch", "die", "br\u00e4un\u00b7li\u00b7che", "Haut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "O herrliche Zeit, wo mit sorgenlosen", "tokens": ["O", "herr\u00b7li\u00b7che", "Zeit", ",", "wo", "mit", "sor\u00b7gen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Blauaugen ich keck in die Stunden geschaut.", "tokens": ["Blau\u00b7au\u00b7gen", "ich", "keck", "in", "die", "Stun\u00b7den", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Kein Wasser zu tief, zu hoch keine H\u00f6he,", "tokens": ["Kein", "Was\u00b7ser", "zu", "tief", ",", "zu", "hoch", "kei\u00b7ne", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "PIAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Apfel zu sauer, kein Vogel zu flink \u2013", "tokens": ["Kein", "Ap\u00b7fel", "zu", "sau\u00b7er", ",", "kein", "Vo\u00b7gel", "zu", "flink", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKA", "ADJD", "$,", "PIAT", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In unserm frechfrohen Raubk\u00f6nigreiche", "tokens": ["In", "un\u00b7serm", "frech\u00b7fro\u00b7hen", "Raub\u00b7k\u00f6\u00b7nig\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wurde geknechtet, was mit uns nicht ging.", "tokens": ["Da", "wur\u00b7de", "ge\u00b7knech\u00b7tet", ",", "was", "mit", "uns", "nicht", "ging", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PRELS", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Katzenjagd stand bei uns m\u00e4chtig in Bl\u00fcte,", "tokens": ["Die", "Kat\u00b7zen\u00b7jagd", "stand", "bei", "uns", "m\u00e4ch\u00b7tig", "in", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es mieden die Hunde sehr schnell uns're N\u00e4h,", "tokens": ["Es", "mie\u00b7den", "die", "Hun\u00b7de", "sehr", "schnell", "un\u00b7s'\u00b7re", "N\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Flursch\u00fctzen war'n wir ein Dorn im Gem\u00fcte,", "tokens": ["Dem", "Flur\u00b7sch\u00fct\u00b7zen", "wa\u00b7r'n", "wir", "ein", "Dorn", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-++--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dem Obstbaumbesitzer ein fressendes Weh.", "tokens": ["Dem", "Obst\u00b7baum\u00b7be\u00b7sit\u00b7zer", "ein", "fres\u00b7sen\u00b7des", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Im Buchwald am Seerand, da war eine Ecke,", "tokens": ["Im", "Buch\u00b7wald", "am", "See\u00b7rand", ",", "da", "war", "ei\u00b7ne", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Von Weiden umwuchert, von Dornen gesch\u00fctzt.", "tokens": ["Von", "Wei\u00b7den", "um\u00b7wu\u00b7chert", ",", "von", "Dor\u00b7nen", "ge\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wir brieten in sicherem R\u00e4uberverstecke", "tokens": ["Wir", "brie\u00b7ten", "in", "si\u00b7che\u00b7rem", "R\u00e4u\u00b7ber\u00b7ver\u00b7ste\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Uns dort die Kartoffeln, die wir uns stibitzt.", "tokens": ["Uns", "dort", "die", "Kar\u00b7tof\u00b7feln", ",", "die", "wir", "uns", "sti\u00b7bitzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Wir rauchten getrocknete Wallnu\u00dfbaumbl\u00e4tter", "tokens": ["Wir", "rauch\u00b7ten", "ge\u00b7trock\u00b7ne\u00b7te", "Wall\u00b7nu\u00df\u00b7baum\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aus Pfeifen, geschnitzelt aus Ellernholz,", "tokens": ["Aus", "Pfei\u00b7fen", ",", "ge\u00b7schnit\u00b7zelt", "aus", "El\u00b7lern\u00b7holz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und f\u00fchlten uns selig, wie Helden und G\u00f6tter,", "tokens": ["Und", "f\u00fchl\u00b7ten", "uns", "se\u00b7lig", ",", "wie", "Hel\u00b7den", "und", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir F\u00fcrsten der Wildnis, verwegen und stolz.", "tokens": ["Wir", "F\u00fcrs\u00b7ten", "der", "Wild\u00b7nis", ",", "ver\u00b7we\u00b7gen", "und", "stolz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Wir hauten uns auch, da\u00df die Haare so flogen", "tokens": ["Wir", "hau\u00b7ten", "uns", "auch", ",", "da\u00df", "die", "Haa\u00b7re", "so", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und blaubeulig wurden Kopf und Gesicht,", "tokens": ["Und", "blau\u00b7beu\u00b7lig", "wur\u00b7den", "Kopf", "und", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wurde daf\u00fcr dann auch Wichse bezogen", "tokens": ["Und", "wur\u00b7de", "da\u00b7f\u00fcr", "dann", "auch", "Wich\u00b7se", "be\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "ADV", "ADV", "NN", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zu Haus' vom Papa, das genierte uns nicht.", "tokens": ["Zu", "Haus'", "vom", "Pa\u00b7pa", ",", "das", "ge\u00b7nier\u00b7te", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Jetzt gehn wir geputzt nach der neuesten Mode", "tokens": ["Jetzt", "gehn", "wir", "ge\u00b7putzt", "nach", "der", "neu\u00b7es\u00b7ten", "Mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit schneewei\u00dfem Kragen und blitzblankem Hut,", "tokens": ["Mit", "schnee\u00b7wei\u00b7\u00dfem", "Kra\u00b7gen", "und", "blitz\u00b7blan\u00b7kem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wir kommen vor H\u00f6flichkeit fast noch zu Tode", "tokens": ["Wir", "kom\u00b7men", "vor", "H\u00f6f\u00b7lich\u00b7keit", "fast", "noch", "zu", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ADV", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und tuen getreu, was ein jedermann tut.", "tokens": ["Und", "tu\u00b7en", "ge\u00b7treu", ",", "was", "ein", "je\u00b7der\u00b7mann", "tut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PRELS", "ART", "PIS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Du wirbelnder Rauch der Kartoffelfeuer,", "tokens": ["Du", "wir\u00b7beln\u00b7der", "Rauch", "der", "Kar\u00b7tof\u00b7fel\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Erinn'rer an alte, verflossene Zeit,", "tokens": ["Er\u00b7inn'\u00b7rer", "an", "al\u00b7te", ",", "ver\u00b7flos\u00b7se\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie ist mir dein herber Geruch doch so teuer,", "tokens": ["Wie", "ist", "mir", "dein", "her\u00b7ber", "Ge\u00b7ruch", "doch", "so", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du bleibst mir als Jugenderinn'rung geweiht.", "tokens": ["Du", "bleibst", "mir", "als", "Ju\u00b7gend\u00b7e\u00b7rinn'\u00b7rung", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}