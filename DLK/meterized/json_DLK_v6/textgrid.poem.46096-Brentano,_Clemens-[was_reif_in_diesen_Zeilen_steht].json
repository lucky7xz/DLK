{"textgrid.poem.46096": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[was reif in diesen Zeilen steht]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was reif in diesen Zeilen steht,", "tokens": ["Was", "reif", "in", "die\u00b7sen", "Zei\u00b7len", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was l\u00e4chelnd winkt und sinnend fleht,", "tokens": ["Was", "l\u00e4\u00b7chelnd", "winkt", "und", "sin\u00b7nend", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das soll kein Kind betr\u00fcben,", "tokens": ["Das", "soll", "kein", "Kind", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Einfalt hat es ausges\u00e4et,", "tokens": ["Die", "Ein\u00b7falt", "hat", "es", "aus\u00b7ge\u00b7s\u00e4et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schwermut hat hindurchgeweht,", "tokens": ["Die", "Schwer\u00b7mut", "hat", "hin\u00b7durch\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sehnsucht hat's getrieben;", "tokens": ["Die", "Sehn\u00b7sucht", "hat's", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ist das Feld einst abgem\u00e4ht,", "tokens": ["Und", "ist", "das", "Feld", "einst", "ab\u00b7ge\u00b7m\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Armut durch die Stoppeln geht,", "tokens": ["Die", "Ar\u00b7mut", "durch", "die", "Stop\u00b7peln", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sucht \u00c4hren, die geblieben,", "tokens": ["Sucht", "\u00c4h\u00b7ren", ",", "die", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sucht Lieb', die f\u00fcr sie untergeht,", "tokens": ["Sucht", "Lieb'", ",", "die", "f\u00fcr", "sie", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sucht Lieb', die mit ihr aufersteht,", "tokens": ["Sucht", "Lieb'", ",", "die", "mit", "ihr", "auf\u00b7er\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sucht Lieb', die sie kann lieben,", "tokens": ["Sucht", "Lieb'", ",", "die", "sie", "kann", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und hat sie einsam und verschm\u00e4ht", "tokens": ["Und", "hat", "sie", "ein\u00b7sam", "und", "ver\u00b7schm\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Nacht durch dankend in Gebet", "tokens": ["Die", "Nacht", "durch", "dan\u00b7kend", "in", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJD", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Die K\u00f6rner ausgerieben,", "tokens": ["Die", "K\u00f6r\u00b7ner", "aus\u00b7ge\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Liest sie, als fr\u00fch der Hahn gekr\u00e4ht,", "tokens": ["Liest", "sie", ",", "als", "fr\u00fch", "der", "Hahn", "ge\u00b7kr\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Was Lieb' erhielt, was Leid verweht,", "tokens": ["Was", "Lieb'", "er\u00b7hielt", ",", "was", "Leid", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ans Feldkreuz angeschrieben,", "tokens": ["Ans", "Feld\u00b7kreuz", "an\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "O Stern und Blume, Geist und Kleid,", "tokens": ["O", "Stern", "und", "Blu\u00b7me", ",", "Geist", "und", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Lieb', Leid und Zeit und Ewigkeit!", "tokens": ["Lieb'", ",", "Leid", "und", "Zeit", "und", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}