{"textgrid.poem.41037": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Dulderin", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du legtest einst in deines Herzens Schrein", "tokens": ["Du", "leg\u00b7test", "einst", "in", "dei\u00b7nes", "Her\u00b7zens", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(indessen Dornen dir das Haupt umschlangen,", "tokens": ["(", "in\u00b7des\u00b7sen", "Dor\u00b7nen", "dir", "das", "Haupt", "um\u00b7schlan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die du vom Engel deiner Wieg' empfangen,)", "tokens": ["Die", "du", "vom", "En\u00b7gel", "dei\u00b7ner", "Wieg'", "emp\u00b7fan\u00b7gen", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kleinodien gar viele still hinein.", "tokens": ["Klein\u00b7o\u00b7di\u00b7en", "gar", "vie\u00b7le", "still", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du mochtest sie den Menschen niemals nennen,", "tokens": ["Du", "moch\u00b7test", "sie", "den", "Men\u00b7schen", "nie\u00b7mals", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie h\u00e4tten sie ja doch nicht fassen k\u00f6nnen;", "tokens": ["Sie", "h\u00e4t\u00b7ten", "sie", "ja", "doch", "nicht", "fas\u00b7sen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie hei\u00dfen: Lieben, Glauben, Stillesein.", "tokens": ["Sie", "hei\u00b7\u00dfen", ":", "Lie\u00b7ben", ",", "Glau\u00b7ben", ",", "Stil\u00b7le\u00b7sein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ADJA", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch Gott, als er dich rief zur Sternenreise,", "tokens": ["Doch", "Gott", ",", "als", "er", "dich", "rief", "zur", "Ster\u00b7nen\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nahm die Kleinodien und legte leise", "tokens": ["Nahm", "die", "Klein\u00b7o\u00b7di\u00b7en", "und", "leg\u00b7te", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Sie in die Dornen deines Kranzes ein.", "tokens": ["Sie", "in", "die", "Dor\u00b7nen", "dei\u00b7nes", "Kran\u00b7zes", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aufschweben sah ich dich im Morgenrot,", "tokens": ["Auf\u00b7schwe\u00b7ben", "sah", "ich", "dich", "im", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Erkannte dich an deinem Dornenkranze", "tokens": ["Er\u00b7kann\u00b7te", "dich", "an", "dei\u00b7nem", "Dor\u00b7nen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wurde irre nicht, da\u00df er an Glanze", "tokens": ["Und", "wur\u00b7de", "ir\u00b7re", "nicht", ",", "da\u00df", "er", "an", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den Glanz des jungen Morgens \u00fcberbot.", "tokens": ["Den", "Glanz", "des", "jun\u00b7gen", "Mor\u00b7gens", "\u00fc\u00b7ber\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}