{"dta.poem.23798": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Kampff wider die S\u00fcnde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Emp\u00f6re dich mein Geist/ es mu\u00df gewaget seyn/", "tokens": ["Em\u00b7p\u00f6\u00b7re", "dich", "mein", "Geist", "/", "es", "mu\u00df", "ge\u00b7wa\u00b7get", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf! setze dich dem Schwarm der L\u00fcste frisch entge-", "tokens": ["Auf", "!", "set\u00b7ze", "dich", "dem", "Schwarm", "der", "L\u00fcs\u00b7te", "frisch", "ent\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Greiff an das grosse Werck/ dran alles ist gelegen/", "tokens": ["Greiff", "an", "das", "gros\u00b7se", "Werck", "/", "dran", "al\u00b7les", "ist", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$(", "PAV", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und r\u00e4ume deinem Feind nicht allen Vortheil ein.", "tokens": ["Und", "r\u00e4u\u00b7me", "dei\u00b7nem", "Feind", "nicht", "al\u00b7len", "Vor\u00b7theil", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Versuch obs besser sey/ wenn du den Sch\u00f6pffer ehrest/", "tokens": ["Ver\u00b7such", "obs", "bes\u00b7ser", "sey", "/", "wenn", "du", "den", "Sch\u00f6pf\u00b7fer", "eh\u00b7rest", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ADJD", "VAFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von dessen starcken Hand du \u00fcberzeuget bist/", "tokens": ["Von", "des\u00b7sen", "star\u00b7cken", "Hand", "du", "\u00fc\u00b7berz\u00b7eu\u00b7get", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als wenn du immerhin/ das Maa\u00df der S\u00fcnden mehrest/", "tokens": ["Als", "wenn", "du", "im\u00b7mer\u00b7hin", "/", "das", "Maa\u00df", "der", "S\u00fcn\u00b7den", "meh\u00b7rest", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "$(", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die deinen C\u00f6rper schw\u00e4cht/ und deine Kr\u00e4ffte fri\u00dft.", "tokens": ["Die", "dei\u00b7nen", "C\u00f6r\u00b7per", "schw\u00e4cht", "/", "und", "dei\u00b7ne", "Kr\u00e4ff\u00b7te", "fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denck was in schn\u00f6der Lust f\u00fcr Stacheln sind versteckt/", "tokens": ["Denck", "was", "in", "schn\u00f6\u00b7der", "Lust", "f\u00fcr", "Sta\u00b7cheln", "sind", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Was oft ein Augenblick macht f\u00fcr betr\u00fcbte Stunden/", "tokens": ["Was", "oft", "ein", "Au\u00b7gen\u00b7blick", "macht", "f\u00fcr", "be\u00b7tr\u00fcb\u00b7te", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie gnau da\u00df der Genu\u00df und Eckel sind verbunden/", "tokens": ["Wie", "gnau", "da\u00df", "der", "Ge\u00b7nu\u00df", "und", "E\u00b7ckel", "sind", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KOUS", "ART", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie in der Freude selbst dich was verborgnes schreckt;", "tokens": ["Wie", "in", "der", "Freu\u00b7de", "selbst", "dich", "was", "ver\u00b7borg\u00b7nes", "schreckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "PPER", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie du/ als Cain dort/ vor GOttes Antlitz fliehest/", "tokens": ["Wie", "du", "/", "als", "Cain", "dort", "/", "vor", "Got\u00b7tes", "Ant\u00b7litz", "flie\u00b7hest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "KOUS", "NN", "ADV", "$(", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wie oft dich in dem Schlaaf des Satans Larve st\u00f6hrt/", "tokens": ["Wie", "oft", "dich", "in", "dem", "Schlaaf", "des", "Sa\u00b7tans", "Lar\u00b7ve", "st\u00f6hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "APPR", "ART", "NN", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie du des Himmels Grim\u0303 auf dein Geschlechte ziehest/", "tokens": ["Wie", "du", "des", "Him\u00b7mels", "Grim\u0303", "auf", "dein", "Ge\u00b7schlech\u00b7te", "zie\u00b7hest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie der MenschenGunst sich endlich von dir kehrt.", "tokens": ["Und", "wie", "der", "Men\u00b7schen", "Gunst", "sich", "end\u00b7lich", "von", "dir", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bedencke da\u00df der Tod/ der alles zu sich rei\u00dft/", "tokens": ["Be\u00b7den\u00b7cke", "da\u00df", "der", "Tod", "/", "der", "al\u00b7les", "zu", "sich", "rei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "$(", "ART", "PIS", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich f\u00fchret bey der Hand und \u00fcber jede Schwelle/", "tokens": ["Dich", "f\u00fch\u00b7ret", "bey", "der", "Hand", "und", "\u00fc\u00b7ber", "je\u00b7de", "Schwel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und immer unvermerckt zur finstern Grabes-Stelle/", "tokens": ["Und", "im\u00b7mer", "un\u00b7ver\u00b7merckt", "zur", "fins\u00b7tern", "Gra\u00b7bes\u00b7Stel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du weist nicht ob er dich nicht heut zu Boden schmei\u00dft/", "tokens": ["Du", "weist", "nicht", "ob", "er", "dich", "nicht", "heut", "zu", "Bo\u00b7den", "schmei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df aber weist du wohl: Solt\u2019 itzt das Band zerspringen/", "tokens": ["Di\u00df", "a\u00b7ber", "weist", "du", "wohl", ":", "Solt'", "itzt", "das", "Band", "zer\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADV", "$.", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df dich und diesen Leib/ O Geist/ zusammen h\u00e4lt/", "tokens": ["Da\u00df", "dich", "und", "die\u00b7sen", "Leib", "/", "O", "Geist", "/", "zu\u00b7sam\u00b7men", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PDAT", "NN", "$(", "NE", "NN", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du w\u00fcrdest schlechten Zeug vor deinen Richter bringen/", "tokens": ["Du", "w\u00fcr\u00b7dest", "schlech\u00b7ten", "Zeug", "vor", "dei\u00b7nen", "Rich\u00b7ter", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erwege nur den Spruch/ den das Gewissen f\u00e4llt.", "tokens": ["Er\u00b7we\u00b7ge", "nur", "den", "Spruch", "/", "den", "das", "Ge\u00b7wis\u00b7sen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was dein verderbtes Blut beweget und ergetzt/", "tokens": ["Was", "dein", "ver\u00b7derb\u00b7tes", "Blut", "be\u00b7we\u00b7get", "und", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du von Jugend an am eyffrigsten getrieben/", "tokens": ["Hast", "du", "von", "Ju\u00b7gend", "an", "am", "eyf\u00b7frigs\u00b7ten", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPR", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hingegen in der Furcht des HErren dich zu \u00fcben/", "tokens": ["Hin\u00b7ge\u00b7gen", "in", "der", "Furcht", "des", "Her\u00b7ren", "dich", "zu", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleibt als ein Neben-Werck auf k\u00fcnfftig ausgesetzt.", "tokens": ["Bleibt", "als", "ein", "Ne\u00b7ben\u00b7\u00b7Werck", "auf", "k\u00fcnff\u00b7tig", "aus\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Worin dein Gottesdienst besteht/ ist da\u00df zu weilen", "tokens": ["Wo\u00b7rin", "dein", "Got\u00b7tes\u00b7dienst", "be\u00b7steht", "/", "ist", "da\u00df", "zu", "wei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$(", "VAFIN", "KOUS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Seufzer ohngefehr aus lauher Andacht fliegt/", "tokens": ["Ein", "Seuf\u00b7zer", "ohn\u00b7ge\u00b7fehr", "aus", "lau\u00b7her", "An\u00b7dacht", "fliegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn du pflegst dergestalt dein Leben einzutheilen/", "tokens": ["Denn", "du", "pflegst", "der\u00b7ge\u00b7stalt", "dein", "Le\u00b7ben", "ein\u00b7zu\u00b7thei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df dessen Kern die Welt/ und GOtt die H\u00fclsen kriegt.", "tokens": ["Da\u00df", "des\u00b7sen", "Kern", "die", "Welt", "/", "und", "Gott", "die", "H\u00fcl\u00b7sen", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ART", "NN", "$(", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dein Christenthum ist nichts als Dunst und Sicherheit/", "tokens": ["Dein", "Chris\u00b7ten\u00b7thum", "ist", "nichts", "als", "Dunst", "und", "Si\u00b7cher\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum/ du machest GOtt zum G\u00f6tzen deiner Sinnen/", "tokens": ["Wa\u00b7rum", "/", "du", "ma\u00b7chest", "Gott", "zum", "G\u00f6t\u00b7zen", "dei\u00b7ner", "Sin\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "PPER", "VVFIN", "NN", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dessen Gegenwart du Dinge darffst beginnen/", "tokens": ["In", "des\u00b7sen", "Ge\u00b7gen\u00b7wart", "du", "Din\u00b7ge", "darffst", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "NN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die ein frecher Mensch sich f\u00fcr den andern scheut.", "tokens": ["Und", "die", "ein", "fre\u00b7cher", "Mensch", "sich", "f\u00fcr", "den", "an\u00b7dern", "scheut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein alter Adam pflegt den Moses auszudeuten/", "tokens": ["Dein", "al\u00b7ter", "A\u00b7dam", "pflegt", "den", "Mo\u00b7ses", "aus\u00b7zu\u00b7deu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Un\u0303 macht des Heylands Wort zu deine\u0303 Fleisch beqvem.", "tokens": ["U\u00f1", "macht", "des", "Hey\u00b7lands", "Wort", "zu", "dein\u1ebd", "Fleisch", "be\u00b7qvem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und wenn zwey Lehrer sich um eine Meynung streiten/", "tokens": ["Und", "wenn", "zwey", "Leh\u00b7rer", "sich", "um", "ei\u00b7ne", "Mey\u00b7nung", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "CARD", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist der/ so deinen Trieb entfesselt/ angenehm.", "tokens": ["Ist", "der", "/", "so", "dei\u00b7nen", "Trieb", "ent\u00b7fes\u00b7selt", "/", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word"