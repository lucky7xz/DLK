{"textgrid.poem.65985": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rechts die Fabrik mit ragendem Schlot,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rechts die Fabrik mit ragendem Schlot,", "tokens": ["Rechts", "die", "Fab\u00b7rik", "mit", "ra\u00b7gen\u00b7dem", "Schlot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und der Bahnhof, wie tot,", "tokens": ["Und", "der", "Bahn\u00b7hof", ",", "wie", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit hartem, kaltem Beamtengesicht.", "tokens": ["Mit", "har\u00b7tem", ",", "kal\u00b7tem", "Be\u00b7am\u00b7ten\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Links, nur auf sandigem Wege erreichbar,", "tokens": ["Links", ",", "nur", "auf", "san\u00b7di\u00b7gem", "We\u00b7ge", "er\u00b7reich\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Einem Schmutzfleck vergleichbar", "tokens": ["Ei\u00b7nem", "Schmutz\u00b7fleck", "ver\u00b7gleich\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Im Landschaftsbild,", "tokens": ["Im", "Land\u00b7schafts\u00b7bild", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Glash\u00fctte. \u2013 Wild", "tokens": ["Die", "Glas\u00b7h\u00fct\u00b7te", ".", "\u2013", "Wild"], "token_info": ["word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "$.", "$(", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und w\u00fcst umher: Schutt, Scherben und Schlacken.", "tokens": ["Und", "w\u00fcst", "um\u00b7her", ":", "Schutt", ",", "Scher\u00b7ben", "und", "Schla\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "ADJD", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein M\u00e4nnerstiefel, zerlocht, ohne Hacken,", "tokens": ["Ein", "M\u00e4n\u00b7ner\u00b7stie\u00b7fel", ",", "zer\u00b7locht", ",", "oh\u00b7ne", "Ha\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und ein rostiger, zerbeulter Kessel", "tokens": ["Und", "ein", "ros\u00b7ti\u00b7ger", ",", "zer\u00b7beul\u00b7ter", "Kes\u00b7sel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Feiern in Klee und Nessel", "tokens": ["Fei\u00b7ern", "in", "Klee", "und", "Nes\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Unterm Heckengehege", "tokens": ["Un\u00b7term", "He\u00b7cken\u00b7ge\u00b7he\u00b7ge"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Am Wege.", "tokens": ["Am", "We\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Arbeiterwohnungen, langgestreckt", "tokens": ["Ar\u00b7bei\u00b7ter\u00b7woh\u00b7nun\u00b7gen", ",", "lang\u00b7ge\u00b7streckt"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unter ein Dach gesteckt,", "tokens": ["Un\u00b7ter", "ein", "Dach", "ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wei\u00df get\u00fcncht, doch sauber nicht,", "tokens": ["Wei\u00df", "ge\u00b7t\u00fcncht", ",", "doch", "sau\u00b7ber", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Verfreundlicht von vollem Sonnenlicht.", "tokens": ["Ver\u00b7freund\u00b7licht", "von", "vol\u00b7lem", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Vor allen Th\u00fcren Kinder und Weiber.", "tokens": ["Vor", "al\u00b7len", "Th\u00fc\u00b7ren", "Kin\u00b7der", "und", "Wei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die M\u00e4nner sitzen beim Zeitvertreiber,", "tokens": ["Die", "M\u00e4n\u00b7ner", "sit\u00b7zen", "beim", "Zeit\u00b7ver\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Beim Bierskat, oder die Kegelbahn", "tokens": ["Beim", "Bier\u00b7skat", ",", "o\u00b7der", "die", "Ke\u00b7gel\u00b7bahn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat's ihnen angethan.", "tokens": ["Hat's", "ih\u00b7nen", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist Sonntag heute. Nach Wochenplag'", "tokens": ["Es", "ist", "Sonn\u00b7tag", "heu\u00b7te", ".", "Nach", "Wo\u00b7chen\u00b7plag'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ADV", "$.", "APPR", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Will der Mann einen frohen Tag.", "tokens": ["Will", "der", "Mann", "ei\u00b7nen", "fro\u00b7hen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Die Weiber tragen immer ihr Pack,", "tokens": ["Die", "Wei\u00b7ber", "tra\u00b7gen", "im\u00b7mer", "ihr", "Pack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Feiern zu Hause bei Kaffee und Schnack,", "tokens": ["Fei\u00b7ern", "zu", "Hau\u00b7se", "bei", "Kaf\u00b7fee", "und", "Schnack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Haben immer zu thun,", "tokens": ["Ha\u00b7ben", "im\u00b7mer", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "K\u00f6nnen selten ruhn.", "tokens": ["K\u00f6n\u00b7nen", "sel\u00b7ten", "ruhn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Hahn, H\u00fchner und Hennen", "tokens": ["Hahn", ",", "H\u00fch\u00b7ner", "und", "Hen\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Mit piepsendem V\u00f6lkchen scharren und rennen.", "tokens": ["Mit", "piep\u00b7sen\u00b7dem", "V\u00f6lk\u00b7chen", "schar\u00b7ren", "und", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Unterm Zaun die gro\u00dfe graue Katz'", "tokens": ["Un\u00b7term", "Zaun", "die", "gro\u00b7\u00dfe", "grau\u00b7e", "Katz'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "R\u00fcckt nicht vom Platz", "tokens": ["R\u00fcckt", "nicht", "vom", "Platz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und blinzt nach den K\u00fccken.", "tokens": ["Und", "blinzt", "nach", "den", "K\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Welch' Trippeln, Picken und Pfl\u00fccken.", "tokens": ["Welch'", "Trip\u00b7peln", ",", "Pi\u00b7cken", "und", "Pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Auf dem Schutt, am Graben, am Weg, \u00fcberall.", "tokens": ["Auf", "dem", "Schutt", ",", "am", "Gra\u00b7ben", ",", "am", "Weg", ",", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "ADV", "$."], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.18": {"text": "Bei jedem Haus fast ein H\u00fchnerstall.", "tokens": ["Bei", "je\u00b7dem", "Haus", "fast", "ein", "H\u00fch\u00b7ner\u00b7stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Auch Kaninchen mit weichen Fellen", "tokens": ["Auch", "Ka\u00b7nin\u00b7chen", "mit", "wei\u00b7chen", "Fel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Entschl\u00fcpfen Verschl\u00e4gen, dummschlaue Gesellen,", "tokens": ["Ent\u00b7schl\u00fcp\u00b7fen", "Ver\u00b7schl\u00e4\u00b7gen", ",", "dumm\u00b7schlau\u00b7e", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.21": {"text": "An den Ohren zur\u00fcckgetragen,", "tokens": ["An", "den", "Oh\u00b7ren", "zu\u00b7r\u00fcck\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Wenn sie zu weit davon sich wagen.", "tokens": ["Wenn", "sie", "zu", "weit", "da\u00b7von", "sich", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "PAV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Scherbengeflirr und -gefunkel,", "tokens": ["Scher\u00b7ben\u00b7ge\u00b7flirr", "und", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weibergeplausch und -gemunkel,", "tokens": ["Wei\u00b7ber\u00b7ge\u00b7plausch", "und", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kinderspektakel", "tokens": ["Kin\u00b7der\u00b7spek\u00b7ta\u00b7kel"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und H\u00fchnergegakel", "tokens": ["Und", "H\u00fch\u00b7ner\u00b7ge\u00b7ga\u00b7kel"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "\u00dcberall.", "tokens": ["\u00dc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Zwischen Fabrik und Fabrik der Wall,", "tokens": ["Zwi\u00b7schen", "Fab\u00b7rik", "und", "Fab\u00b7rik", "der", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NE", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der Bahndamm mit blitzenden Eisenstr\u00e4ngen,", "tokens": ["Der", "Bahn\u00b7damm", "mit", "blit\u00b7zen\u00b7den", "Ei\u00b7sen\u00b7str\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bekleidet mit bl\u00fchenden Seitengeh\u00e4ngen:", "tokens": ["Be\u00b7klei\u00b7det", "mit", "bl\u00fc\u00b7hen\u00b7den", "Sei\u00b7ten\u00b7ge\u00b7h\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Haidekraut, L\u00f6wenzahn und kriechender Wicke.", "tokens": ["Hai\u00b7de\u00b7kraut", ",", "L\u00f6\u00b7wen\u00b7zahn", "und", "krie\u00b7chen\u00b7der", "Wi\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Abseits im Knicke", "tokens": ["Ab\u00b7seits", "im", "Kni\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Leuchten abbl\u00fchender Dorn und Syringen.", "tokens": ["Leuch\u00b7ten", "ab\u00b7bl\u00fc\u00b7hen\u00b7der", "Dorn", "und", "Sy\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Aus dem G\u00e4rtchen dringen,", "tokens": ["Aus", "dem", "G\u00e4rt\u00b7chen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Des Bahnwarts G\u00e4rtchen, Jasmind\u00fcfte.", "tokens": ["Des", "Bahn\u00b7warts", "G\u00e4rt\u00b7chen", ",", "Jas\u00b7min\u00b7d\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So still die L\u00fcfte,", "tokens": ["So", "still", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Keine Regung, kein Hauch,", "tokens": ["Kei\u00b7ne", "Re\u00b7gung", ",", "kein", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Als w\u00fcssten sie auch,", "tokens": ["Als", "w\u00fcss\u00b7ten", "sie", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Dass Sonntag heute,", "tokens": ["Dass", "Sonn\u00b7tag", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ruhtag. \u2013 \u2013 \u2013", "tokens": ["Ruh\u00b7tag", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$.", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "\u2013 \u2013 \u2013 Gel\u00e4ute!", "tokens": ["\u2013", "\u2013", "\u2013", "Ge\u00b7l\u00e4u\u00b7te", "!"], "token_info": ["punct", "punct", "punct", "word", "punct"], "pos": ["$(", "$(", "$(", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Ein Bahnzug donnert heran und h\u00e4lt,", "tokens": ["Ein", "Bahn\u00b7zug", "don\u00b7nert", "he\u00b7ran", "und"