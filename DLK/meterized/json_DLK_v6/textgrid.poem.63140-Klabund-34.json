{"textgrid.poem.63140": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "34", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dies das Ende w\u00e4re von allen Dingen,", "tokens": ["Wenn", "dies", "das", "En\u00b7de", "w\u00e4\u00b7re", "von", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich s\u00e4nge hell die S\u00fc\u00dfe aller Zonen.", "tokens": ["Ich", "s\u00e4n\u00b7ge", "hell", "die", "S\u00fc\u00b7\u00dfe", "al\u00b7ler", "Zo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00fcrde gern bei alten Frauen wohnen", "tokens": ["Ich", "w\u00fcr\u00b7de", "gern", "bei", "al\u00b7ten", "Frau\u00b7en", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich im Tanz der Feuerl\u00e4nder schwingen.", "tokens": ["Und", "mich", "im", "Tanz", "der", "Feu\u00b7er\u00b7l\u00e4n\u00b7der", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So aber bleibt ein Letztes ungesagt.", "tokens": ["So", "a\u00b7ber", "bleibt", "ein", "Letz\u00b7tes", "un\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus allen Totenm\u00fcndern schreit es: gestern.", "tokens": ["Aus", "al\u00b7len", "To\u00b7ten\u00b7m\u00fcn\u00b7dern", "schreit", "es", ":", "ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$.", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und: morgen! weinen Kinder, unbeklagt.", "tokens": ["Und", ":", "mor\u00b7gen", "!", "wei\u00b7nen", "Kin\u00b7der", ",", "un\u00b7be\u00b7klagt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$.", "ADV", "$.", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O meine armen Br\u00fcder, meine Schwestern!", "tokens": ["O", "mei\u00b7ne", "ar\u00b7men", "Br\u00fc\u00b7der", ",", "mei\u00b7ne", "Schwes\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}