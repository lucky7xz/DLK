{"textgrid.poem.65079": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erinnerung nennt Ihr ein Gl\u00fcck?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinnerung nennt Ihr ein Gl\u00fcck?", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "nennt", "Ihr", "ein", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sie, die nach Eurem tiefen Falle", "tokens": ["Sie", ",", "die", "nach", "Eu\u00b7rem", "tie\u00b7fen", "Fal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die hingeschwund'nen Freuden alle", "tokens": ["Die", "hin\u00b7ge\u00b7schwun\u00b7d'\u00b7nen", "Freu\u00b7den", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Euch h\u00f6hnend vorz\u00e4hlt St\u00fcck f\u00fcr St\u00fcck?", "tokens": ["Euch", "h\u00f6h\u00b7nend", "vor\u00b7z\u00e4hlt", "St\u00fcck", "f\u00fcr", "St\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wi\u00dft Ihr, was mir Erinnerung?", "tokens": ["Wi\u00dft", "Ihr", ",", "was", "mir", "E\u00b7rin\u00b7ne\u00b7rung", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein Tiger, der aus finst'rer H\u00f6hle", "tokens": ["Ein", "Ti\u00b7ger", ",", "der", "aus", "finst'\u00b7rer", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Grimm auf den Frieden meiner Seele", "tokens": ["Grimm", "auf", "den", "Frie\u00b7den", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hinst\u00fcrzt, mit m\u00f6rderischem Sprung!", "tokens": ["Hin\u00b7st\u00fcrzt", ",", "mit", "m\u00f6r\u00b7de\u00b7ri\u00b7schem", "Sprung", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Upasbaum auf \u00f6der Trift,", "tokens": ["Ein", "U\u00b7pas\u00b7baum", "auf", "\u00f6\u00b7der", "Trift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, wag' ich es in eitlem Sehnen,", "tokens": ["Der", ",", "wag'", "ich", "es", "in", "eit\u00b7lem", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Haupt an seinen Stamm zu lehnen,", "tokens": ["Mein", "Haupt", "an", "sei\u00b7nen", "Stamm", "zu", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich rasch durchdringt mit gl\u00fch'ndem Gift!", "tokens": ["Mich", "rasch", "durch\u00b7dringt", "mit", "gl\u00fch'n\u00b7dem", "Gift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Ungl\u00fcck trag' ich wie ich kann,", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "trag'", "ich", "wie", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch soll der Muth mir nicht entgleiten.", "tokens": ["Doch", "soll", "der", "Muth", "mir", "nicht", "ent\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ihr verlor'nen Seligkeiten,", "tokens": ["Dann", "ihr", "ver\u00b7lor'\u00b7nen", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O tretet weinend nicht heran!", "tokens": ["O", "tre\u00b7tet", "wei\u00b7nend", "nicht", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn auf meiner Stirne Rand", "tokens": ["Und", "wenn", "auf", "mei\u00b7ner", "Stir\u00b7ne", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich kalt jetzt deine Lippen pressen,", "tokens": ["Sich", "kalt", "jetzt", "dei\u00b7ne", "Lip\u00b7pen", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lass' auf ewig mich vergessen,", "tokens": ["Dann", "lass'", "auf", "e\u00b7wig", "mich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hei\u00df vordem ihr Ku\u00df gebrannt!", "tokens": ["Wie", "hei\u00df", "vor\u00b7dem", "ihr", "Ku\u00df", "ge\u00b7brannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O still! soll nicht mein eig'nes Wort", "tokens": ["O", "still", "!", "soll", "nicht", "mein", "eig'\u00b7nes", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "VMFIN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufrufen der D\u00e4monen W\u00fcthen,", "tokens": ["Auf\u00b7ru\u00b7fen", "der", "D\u00e4\u00b7mo\u00b7nen", "W\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die tief in meinem Innern h\u00fcthen", "tokens": ["Die", "tief", "in", "mei\u00b7nem", "In\u00b7nern", "h\u00fct\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schmerzen Nibelungenhort.", "tokens": ["Der", "Schmer\u00b7zen", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7hort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es ist genug! mehr als genug!", "tokens": ["Es", "ist", "ge\u00b7nug", "!", "mehr", "als", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Abgrund kann kein Blick ermessen.", "tokens": ["Den", "Ab\u00b7grund", "kann", "kein", "Blick", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will nichts weiter als vergessen", "tokens": ["Ich", "will", "nichts", "wei\u00b7ter", "als", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "KOKOM", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich geno\u00df, was ich ertrug!", "tokens": ["Was", "ich", "ge\u00b7no\u00df", ",", "was", "ich", "er\u00b7trug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}