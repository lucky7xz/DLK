{"textgrid.poem.63546": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Tiefer Brunnen", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschlie\u00df dich nur, du sch\u00f6ner Mund,", "tokens": ["Ver\u00b7schlie\u00df", "dich", "nur", ",", "du", "sch\u00f6\u00b7ner", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbirg dich, tiefes Herz, mit Flei\u00df:", "tokens": ["Ver\u00b7birg", "dich", ",", "tie\u00b7fes", "Herz", ",", "mit", "Flei\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Rechte kommt zur rechten Stund',", "tokens": ["Der", "Rech\u00b7te", "kommt", "zur", "rech\u00b7ten", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mund und Herz zu l\u00f6sen wei\u00df.", "tokens": ["Der", "Mund", "und", "Herz", "zu", "l\u00f6\u00b7sen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gedenk' ich dein, kommt mir zu Sinn", "tokens": ["Ge\u00b7denk'", "ich", "dein", ",", "kommt", "mir", "zu", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die Sage von der alten Stadt.", "tokens": ["Die", "Sa\u00b7ge", "von", "der", "al\u00b7ten", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein tiefer Brunnen lag darin,", "tokens": ["Ein", "tie\u00b7fer", "Brun\u00b7nen", "lag", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drau\u00df keiner noch getrunken hatt'.", "tokens": ["Drau\u00df", "kei\u00b7ner", "noch", "ge\u00b7trun\u00b7ken", "hatt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er war so tief, so wundertief,", "tokens": ["Er", "war", "so", "tief", ",", "so", "wun\u00b7der\u00b7tief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df man ein Becherlein hinab,", "tokens": ["Lie\u00df", "man", "ein", "Be\u00b7cher\u00b7lein", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Faden viele Stunden lief", "tokens": ["Der", "Fa\u00b7den", "vie\u00b7le", "Stun\u00b7den", "lief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reichte doch den Grund nicht ab.", "tokens": ["Und", "reich\u00b7te", "doch", "den", "Grund", "nicht", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da kam des Wegs ein Musikant,", "tokens": ["Da", "kam", "des", "Wegs", "ein", "Mu\u00b7si\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sah den Brunn und trat herzu", "tokens": ["Der", "sah", "den", "Brunn", "und", "trat", "her\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nahm sein Geigenspiel zur Hand", "tokens": ["Und", "nahm", "sein", "Gei\u00b7gen\u00b7spiel", "zur", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spielt' ein St\u00fcck und sang dazu.", "tokens": ["Und", "spielt'", "ein", "St\u00fcck", "und", "sang", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und horch, da rauscht' es tief und voll", "tokens": ["Und", "horch", ",", "da", "rauscht'", "es", "tief", "und", "voll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wogt' herauf und sprudelt' klar,", "tokens": ["Und", "wogt'", "her\u00b7auf", "und", "spru\u00b7delt'", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lieblich k\u00fchl Gew\u00e4sser schwoll", "tokens": ["Und", "lieb\u00b7lich", "k\u00fchl", "Ge\u00b7w\u00e4s\u00b7ser", "schwoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Empor zum Rande wunderbar.", "tokens": ["Em\u00b7por", "zum", "Ran\u00b7de", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Spielmann trank nach Herzgel\u00fcst,", "tokens": ["Der", "Spiel\u00b7mann", "trank", "nach", "Herz\u00b7ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war gel\u00f6st der dunkle Bann.", "tokens": ["Da", "war", "ge\u00b7l\u00f6st", "der", "dunk\u00b7le", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ach, w\u00e4re wohl ein sel'ger Mann!", "tokens": ["Ach", ",", "w\u00e4\u00b7re", "wohl", "ein", "sel'\u00b7ger", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}