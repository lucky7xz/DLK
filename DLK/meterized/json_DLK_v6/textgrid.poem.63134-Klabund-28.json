{"textgrid.poem.63134": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "28", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du immer h\u00e4ltst in H\u00e4nden,", "tokens": ["Was", "du", "im\u00b7mer", "h\u00e4ltst", "in", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen oder Buch.", "tokens": ["M\u00e4d\u00b7chen", "o\u00b7der", "Buch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, wie bald wird es sich wenden,", "tokens": ["Ach", ",", "wie", "bald", "wird", "es", "sich", "wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und die wei\u00dfen Frauenlenden", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "Frau\u00b7en\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deckt ein schwarzes Tuch.", "tokens": ["Deckt", "ein", "schwar\u00b7zes", "Tuch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Asche wird die s\u00fc\u00dfe Zofe,", "tokens": ["A\u00b7sche", "wird", "die", "s\u00fc\u00b7\u00dfe", "Zo\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lippe ist versteint.", "tokens": ["Lip\u00b7pe", "ist", "ver\u00b7steint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sto\u00df das Fenster auf: im Hofe", "tokens": ["Sto\u00df", "das", "Fens\u00b7ter", "auf", ":", "im", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnattern G\u00e4nse um die Kofe,", "tokens": ["Schnat\u00b7tern", "G\u00e4n\u00b7se", "um", "die", "Ko\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Bettler weint.", "tokens": ["Und", "ein", "Bett\u00b7ler", "weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Deine Verse sind Gesaber", "tokens": ["Dei\u00b7ne", "Ver\u00b7se", "sind", "Ge\u00b7sa\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Eines hohlen Herrn.", "tokens": ["Ei\u00b7nes", "hoh\u00b7len", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts als wennschon oder aber \u2013", "tokens": ["Nichts", "als", "wenn\u00b7schon", "o\u00b7der", "a\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "KON", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ng dich an den Kandelaber", "tokens": ["H\u00e4ng", "dich", "an", "den", "Kan\u00b7de\u00b7la\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Sturm und Stern.", "tokens": ["Un\u00b7ter", "Sturm", "und", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Deine Beine m\u00f6gen baumeln,", "tokens": ["Dei\u00b7ne", "Bei\u00b7ne", "m\u00f6\u00b7gen", "bau\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein Haupt benickt", "tokens": ["Und", "dein", "Haupt", "be\u00b7nickt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Welche weinw\u00e4rts singend taumeln,", "tokens": ["Wel\u00b7che", "wein\u00b7w\u00e4rts", "sin\u00b7gend", "tau\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich von dem grellen traumhelln", "tokens": ["Pl\u00f6tz\u00b7lich", "von", "dem", "grel\u00b7len", "traum\u00b7helln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Eulenschrei zerdr\u00fcckt.", "tokens": ["Eu\u00b7len\u00b7schrei", "zer\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}