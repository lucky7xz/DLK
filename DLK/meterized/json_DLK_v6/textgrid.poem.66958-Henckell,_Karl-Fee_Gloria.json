{"textgrid.poem.66958": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Fee Gloria", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hold knospt der Ruhm. Schon seh ich voll Entz\u00fccken", "tokens": ["Hold", "knospt", "der", "Ruhm", ".", "Schon", "seh", "ich", "voll", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den gr\u00fcnen Busch mit Bl\u00fctenspur sich schm\u00fccken.", "tokens": ["Den", "gr\u00fc\u00b7nen", "Busch", "mit", "Bl\u00fc\u00b7ten\u00b7spur", "sich", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit rauhem Frost gef\u00e4hrdet' ihn der Neid,", "tokens": ["Mit", "rau\u00b7hem", "Frost", "ge\u00b7f\u00e4hr\u00b7det'", "ihn", "der", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "B\u00f6s blies der Nordwind der Geh\u00e4ssigkeit.", "tokens": ["B\u00f6s", "blies", "der", "Nord\u00b7wind", "der", "Ge\u00b7h\u00e4s\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Gl\u00fcckauf! Des B\u00e4umchens Mark hat nicht gelitten ...", "tokens": ["Gl\u00fcc\u00b7kauf", "!", "Des", "B\u00e4um\u00b7chens", "Mark", "hat", "nicht", "ge\u00b7lit\u00b7ten", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fee Gloria kommt auf Maienhauch geritten.", "tokens": ["Fee", "Glo\u00b7ria", "kommt", "auf", "Mai\u00b7en\u00b7hauch", "ge\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}