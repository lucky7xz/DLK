{"textgrid.poem.38078": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6llisches Recht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein Hirt gar fr\u00fch austreiben,", "tokens": ["Es", "ging", "ein", "Hirt", "gar", "fr\u00fch", "aus\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er h\u00f6rt' ein kleines Kindlein schreien.", "tokens": ["Er", "h\u00f6rt'", "ein", "klei\u00b7nes", "Kin\u00b7dlein", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kindelein ich h\u00f6r' dich und seh dich nicht.", "tokens": ["Kin\u00b7de\u00b7lein", "ich", "h\u00f6r'", "dich", "und", "seh", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbich bin in einem hohlen Baum", "tokens": ["\u00bb", "ich", "bin", "in", "ei\u00b7nem", "hoh\u00b7len", "Baum"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit eichenen R\u00fcthlein g'deckt.", "tokens": ["Und", "mit", "ei\u00b7che\u00b7nen", "R\u00fcth\u00b7lein", "g'\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ach Alter nimm mich mit zu Haus,", "tokens": ["Ach", "Al\u00b7ter", "nimm", "mich", "mit", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein' Mutter hat Hochzeit zu Haus.\u00ab", "tokens": ["Mein'", "Mut\u00b7ter", "hat", "Hoch\u00b7zeit", "zu", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Als er das Kind zur Th\u00fcr nein bracht:", "tokens": ["Als", "er", "das", "Kind", "zur", "Th\u00fcr", "nein", "bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "PTKANT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbgr\u00fc\u00df euch Gott ihr Hochzeitg\u00e4st,", "tokens": ["\u00bb", "gr\u00fc\u00df", "euch", "Gott", "ihr", "Hoch\u00b7zeit\u00b7g\u00e4st", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dieweil die Braut mein Mutter ist.\u00ab", "tokens": ["Die\u00b7weil", "die", "Braut", "mein", "Mut\u00b7ter", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie soll ich denn dein Mutter sein,", "tokens": ["Wie", "soll", "ich", "denn", "dein", "Mut\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich trage ja ein Kr\u00e4nzelein?", "tokens": ["Ich", "tra\u00b7ge", "ja", "ein", "Kr\u00e4n\u00b7zel\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbtragst du ein Kr\u00e4nzelein rosenroth,", "tokens": ["\u00bb", "tragst", "du", "ein", "Kr\u00e4n\u00b7zel\u00b7ein", "ro\u00b7sen\u00b7roth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Du hast schon drei Kinder todt.", "tokens": ["Du", "hast", "schon", "drei", "Kin\u00b7der", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "'s erst hast ins Wasser geschmissen,", "tokens": ["'s", "erst", "hast", "ins", "Was\u00b7ser", "ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "'s ander hast in Mist vergraben,", "tokens": ["'s", "an\u00b7der", "hast", "in", "Mist", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "'s dritt' in einen holen Baum,", "tokens": ["'", "s", "dritt", "'", "in", "ei\u00b7nen", "ho\u00b7len", "Baum", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und mit eichenen R\u00fcthlein zugedeckt.\u00ab", "tokens": ["Und", "mit", "ei\u00b7che\u00b7nen", "R\u00fcth\u00b7lein", "zu\u00b7ge\u00b7deckt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Ach wie kann das m\u00f6glich seyn!", "tokens": ["Ach", "wie", "kann", "das", "m\u00f6g\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VMFIN", "PDS", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Kam der Teuffel zum Fenster hinein,", "tokens": ["Kam", "der", "Teuf\u00b7fel", "zum", "Fens\u00b7ter", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Und nahm sie bei ihrer schneeweissen Hand,", "tokens": ["Und", "nahm", "sie", "bei", "ih\u00b7rer", "schnee\u00b7weis\u00b7sen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Thut mit ihr den Ehrentanz", "tokens": ["Thut", "mit", "ihr", "den", "Eh\u00b7ren\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und f\u00fchrt sie in die h\u00f6llische Pein.", "tokens": ["Und", "f\u00fchrt", "sie", "in", "die", "h\u00f6l\u00b7li\u00b7sche", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}