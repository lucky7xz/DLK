{"dta.poem.15933": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "138.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit deinem Lernen ists im Augenblick vorbei,", "tokens": ["Mit", "dei\u00b7nem", "Ler\u00b7nen", "ists", "im", "Au\u00b7gen\u00b7blick", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du dich selber fragst, wozu's ersprie\u00dflich sei.", "tokens": ["Wo", "du", "dich", "sel\u00b7ber", "fragst", ",", "wo\u00b7zu's", "er\u00b7sprie\u00df\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es dient nicht deiner Seel' und nicht der Welt zum Heil;", "tokens": ["Es", "dient", "nicht", "dei\u00b7ner", "Seel'", "und", "nicht", "der", "Welt", "zum", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "KON", "PTKNEG", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wendest du daran des Lebens einen Theil?", "tokens": ["Was", "wen\u00b7dest", "du", "da\u00b7ran", "des", "Le\u00b7bens", "ei\u00b7nen", "Theil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measu