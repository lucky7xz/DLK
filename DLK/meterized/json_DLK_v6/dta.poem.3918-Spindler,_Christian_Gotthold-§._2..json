 "durch", "Mu\u00b7sic", "ein", "St\u00e4nd\u00b7gen", "vor", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hau\u00df.", "tokens": ["Hau\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}