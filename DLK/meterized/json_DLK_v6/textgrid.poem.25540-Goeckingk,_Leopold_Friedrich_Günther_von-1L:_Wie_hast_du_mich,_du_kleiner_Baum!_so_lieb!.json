{"textgrid.poem.25540": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie hast du mich, du kleiner Baum! so lieb!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hast du mich, du kleiner Baum! so lieb!", "tokens": ["Wie", "hast", "du", "mich", ",", "du", "klei\u00b7ner", "Baum", "!", "so", "lieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie so gelinde spielest", "tokens": ["Wie", "so", "ge\u00b7lin\u00b7de", "spie\u00b7lest"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du mit den runden Bl\u00e4ttern um mich hin!", "tokens": ["Du", "mit", "den", "run\u00b7den", "Bl\u00e4t\u00b7tern", "um", "mich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ob du vielleicht es f\u00fchlest,", "tokens": ["Ob", "du", "viel\u00b7leicht", "es", "f\u00fch\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich in deinem Schatten bin?", "tokens": ["Da\u00df", "ich", "in", "dei\u00b7nem", "Schat\u00b7ten", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie lieb' ich dich! Hier ist's, wo ich zuerst", "tokens": ["Wie", "lieb'", "ich", "dich", "!", "Hier", "ist's", ",", "wo", "ich", "zu\u00b7erst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "ADV", "VAFIN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(wie brachten da die Winde", "tokens": ["(", "wie", "brach\u00b7ten", "da", "die", "Win\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns deinen Duft!) mein Nantchen sah;", "tokens": ["Uns", "dei\u00b7nen", "Duft", "!", ")", "mein", "Nant\u00b7chen", "sah", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner wei\u00dfen Rinde,", "tokens": ["In", "dei\u00b7ner", "wei\u00b7\u00dfen", "Rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Steht noch dein Stolz, ihr Name, da.", "tokens": ["Steht", "noch", "dein", "Stolz", ",", "ihr", "Na\u00b7me", ",", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als die Natur den rosenfarbnen May,", "tokens": ["Als", "die", "Na\u00b7tur", "den", "ro\u00b7sen\u00b7farb\u00b7nen", "May", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Schoo\u00dfkind, niedlich schm\u00fcckte,", "tokens": ["Ihr", "Schoo\u00df\u00b7kind", ",", "nied\u00b7lich", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Veilchen ihn bekr\u00e4nzt in dieses Thal", "tokens": ["Mit", "Veil\u00b7chen", "ihn", "be\u00b7kr\u00e4nzt", "in", "die\u00b7ses", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu frommen Hirten schickte,", "tokens": ["Zu", "from\u00b7men", "Hir\u00b7ten", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da k\u00fc\u00dfte sie mich hier zum ersten mal.", "tokens": ["Da", "k\u00fc\u00df\u00b7te", "sie", "mich", "hier", "zum", "ers\u00b7ten", "mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ach! du sollst nicht mehr, geliebter Baum,", "tokens": ["Doch", "ach", "!", "du", "sollst", "nicht", "mehr", ",", "ge\u00b7lieb\u00b7ter", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr uns Arme k\u00fchlen,", "tokens": ["Nicht", "mehr", "uns", "Ar\u00b7me", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von dir bedeckt, wird hier der Liebe Scherz", "tokens": ["Von", "dir", "be\u00b7deckt", ",", "wird", "hier", "der", "Lie\u00b7be", "Scherz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht mehr um Pf\u00e4nder spielen;", "tokens": ["Nicht", "mehr", "um", "Pf\u00e4n\u00b7der", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O wenn du kannst, empfinde meinen Schmerz.", "tokens": ["O", "wenn", "du", "kannst", ",", "emp\u00b7fin\u00b7de", "mei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VMFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von dir, o Lind'! und meinem ", "tokens": ["Von", "dir", ",", "o", "Lind'", "!", "und", "mei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$.", "KON", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll auch die Fl\u00f6te schweigen;", "tokens": ["Soll", "auch", "die", "Fl\u00f6\u00b7te", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier hange sie so lang' unanger\u00fchrt", "tokens": ["Hier", "han\u00b7ge", "sie", "so", "lang'", "un\u00b7an\u00b7ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An deinen h\u00f6chsten Zweigen,", "tokens": ["An", "dei\u00b7nen", "h\u00f6chs\u00b7ten", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis mich zur\u00fcck der Himmel f\u00fchrt.", "tokens": ["Bis", "mich", "zu\u00b7r\u00fcck", "der", "Him\u00b7mel", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch bringt der Schmerz mein ", "tokens": ["Doch", "bringt", "der", "Schmerz", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So la\u00df den Zweig hernieder,", "tokens": ["So", "la\u00df", "den", "Zweig", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Reich' ihr die Fl\u00f6te hin, und spielet sie", "tokens": ["Reich'", "ihr", "die", "Fl\u00f6\u00b7te", "hin", ",", "und", "spie\u00b7let", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe, Klagelieder,", "tokens": ["Der", "Lie\u00b7be", ",", "Kla\u00b7ge\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So rausche nicht in ihre Melodie.", "tokens": ["So", "rau\u00b7sche", "nicht", "in", "ih\u00b7re", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}