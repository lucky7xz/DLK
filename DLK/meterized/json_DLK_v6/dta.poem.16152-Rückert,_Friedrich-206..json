{"dta.poem.16152": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "206.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer viele Diener hat, hat viele zu bedienen;", "tokens": ["Wer", "vie\u00b7le", "Die\u00b7ner", "hat", ",", "hat", "vie\u00b7le", "zu", "be\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "$,", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn alle dienen ihm nur weil er dienet ihnen.", "tokens": ["Denn", "al\u00b7le", "die\u00b7nen", "ihm", "nur", "weil", "er", "die\u00b7net", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bedienen mu\u00df er sie mit Unterhalt und Lohn;", "tokens": ["Be\u00b7die\u00b7nen", "mu\u00df", "er", "sie", "mit", "Un\u00b7ter\u00b7halt", "und", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt das sie nicht im Dienst, so laufen sie davon.", "tokens": ["H\u00e4lt", "das", "sie", "nicht", "im", "Dienst", ",", "so", "lau\u00b7fen", "sie", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "PTKNEG", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie dienen mit dem Leib, ihr Geist ist sorgenfrei,", "tokens": ["Sie", "die\u00b7nen", "mit", "dem", "Leib", ",", "ihr", "Geist", "ist", "sor\u00b7gen\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie lassen ihrem Herrn der Sorgen Sklaverei.", "tokens": ["Sie", "las\u00b7sen", "ih\u00b7rem", "Herrn", "der", "Sor\u00b7gen", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}