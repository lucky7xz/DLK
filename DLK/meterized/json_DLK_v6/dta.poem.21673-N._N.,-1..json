{"dta.poem.21673": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Damascener-Stahl ist eingepr\u00e4gt", "tokens": ["Auf", "Da\u00b7ma\u00b7sce\u00b7ner\u00b7Stahl", "ist", "ein\u00b7ge\u00b7pr\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVPP"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ein Koran-Vers, den Lebenszweck zu zeigen,", "tokens": ["Ein", "Ko\u00b7ran\u00b7Vers", ",", "den", "Le\u00b7bens\u00b7zweck", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dient als Amulet dem, der ihn tr\u00e4gt.", "tokens": ["Und", "dient", "als", "A\u00b7mu\u00b7let", "dem", ",", "der", "ihn", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NE", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwertfeger, dieser Vers fiel mir zu eigen:", "tokens": ["Schwert\u00b7fe\u00b7ger", ",", "die\u00b7ser", "Vers", "fiel", "mir", "zu", "ei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Mein Lieben ist f\u00fcr mich mein ganzes Leben,", "tokens": ["Mein", "Lie\u00b7ben", "ist", "f\u00fcr", "mich", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Lied mein ganzer Ruhm; und wenn gegeben", "tokens": ["Mein", "Lied", "mein", "gan\u00b7zer", "Ruhm", ";", "und", "wenn", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$.", "KON", "KOUS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mir eine Seele, die unsterblich, dann", "tokens": ["Mir", "ei\u00b7ne", "See\u00b7le", ",", "die", "uns\u00b7terb\u00b7lich", ",", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zuleika\u2019s Name nimmer sterben kann.", "tokens": ["Zu\u00b7lei\u00b7ka's", "Na\u00b7me", "nim\u00b7mer", "ster\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn \u201emeine Seele\u201c nannte ich sie immer \u2014", "tokens": ["Denn", "\u201e", "mei\u00b7ne", "See\u00b7le", "\u201c", "nann\u00b7te", "ich", "sie", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "PPER", "ADV", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Sie lebt in meinem Lied und das stirbt nimmer.", "tokens": ["Sie", "lebt", "in", "mei\u00b7nem", "Lied", "und", "das", "stirbt", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "PDS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}