{"textgrid.poem.49211": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich gleiche nicht mit dir de\u00df weissen Mondens Liecht;", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gleiche nicht mit dir de\u00df weissen Mondens Liecht;", "tokens": ["Ich", "glei\u00b7che", "nicht", "mit", "dir", "de\u00df", "weis\u00b7sen", "Mon\u00b7dens", "Liecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Monde fellt und steigt, du bleibst in einem Scheine;", "tokens": ["Der", "Mon\u00b7de", "fellt", "und", "steigt", ",", "du", "bleibst", "in", "ei\u00b7nem", "Schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja nicht die Sonne selbst, die Sonn' ist gantz gemeine,", "tokens": ["Ja", "nicht", "die", "Son\u00b7ne", "selbst", ",", "die", "Sonn'", "ist", "gantz", "ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "ART", "NN", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gemein' auch ist ihr Glantz, du bist gemeine nicht,", "tokens": ["Ge\u00b7mein'", "auch", "ist", "ihr", "Glantz", ",", "du", "bist", "ge\u00b7mei\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du zwingst durch Zucht den Neid, wie sehr er auff dich sticht.", "tokens": ["Du", "zwingst", "durch", "Zucht", "den", "Neid", ",", "wie", "sehr", "er", "auff", "dich", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ART", "NN", "$,", "PWAV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich mach kein Heuchler seyn, der bey mir selbst verneine", "tokens": ["Ich", "mach", "kein", "Heuch\u00b7ler", "seyn", ",", "der", "bey", "mir", "selbst", "ver\u00b7nei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VAINF", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das, was ich jetzt gesagt; es gleichet sich dir keine,", "tokens": ["Das", ",", "was", "ich", "jetzt", "ge\u00b7sagt", ";", "es", "glei\u00b7chet", "sich", "dir", "kei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PRF", "PPER", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist dir \u00e4hnlich selbst; ein ander Bild gebricht,", "tokens": ["Du", "bist", "dir", "\u00e4hn\u00b7lich", "selbst", ";", "ein", "an\u00b7der", "Bild", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "$.", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das dir dich zeigen kan, du bist dein eigen Gl\u00fccke,", "tokens": ["Das", "dir", "dich", "zei\u00b7gen", "kan", ",", "du", "bist", "dein", "ei\u00b7gen", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein eigenes Gestirn, der Sch\u00f6nheit Meisterst\u00fccke.", "tokens": ["Dein", "ei\u00b7ge\u00b7nes", "Ge\u00b7stirn", ",", "der", "Sch\u00f6n\u00b7heit", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hettest sollen seyn, wie noch die Tugend war", "tokens": ["Du", "het\u00b7test", "sol\u00b7len", "seyn", ",", "wie", "noch", "die", "Tu\u00b7gend", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VMFIN", "VAINF", "$,", "PWAV", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geehret als ein Gott, in der Welt ersten Jugend,", "tokens": ["Ge\u00b7eh\u00b7ret", "als", "ein", "Gott", ",", "in", "der", "Welt", "ers\u00b7ten", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So were wol gewi\u00df gewesen deine Tugend", "tokens": ["So", "we\u00b7re", "wol", "ge\u00b7wi\u00df", "ge\u00b7we\u00b7sen", "dei\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "VAPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Kirch' und Opfferung, der Weyrauch und Altar.", "tokens": ["Die", "Kirch'", "und", "Opf\u00b7fe\u00b7rung", ",", "der", "Wey\u00b7rauch", "und", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}