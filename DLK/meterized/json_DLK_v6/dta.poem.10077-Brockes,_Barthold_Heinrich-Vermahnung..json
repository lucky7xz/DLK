{"dta.poem.10077": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vermahnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebste Menschen, braucht die Sinnen besser, als ihr\nsie gebraucht!", "tokens": ["Liebs\u00b7te", "Men\u00b7schen", ",", "braucht", "die", "Sin\u00b7nen", "bes\u00b7ser", ",", "als", "ihr", "sie", "ge\u00b7braucht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sucht von eurer Augen Spiegel, den die Leidenschafft be-", "tokens": ["Sucht", "von", "eu\u00b7rer", "Au\u00b7gen", "Spie\u00b7gel", ",", "den", "die", "Lei\u00b7den\u00b7schafft", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Der Gewohnheit Dufft zu wischen. Gleich wird durchs", "tokens": ["Der", "Ge\u00b7wohn\u00b7heit", "Dufft", "zu", "wi\u00b7schen", ".", "Gleich", "wird", "durchs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$.", "ADV", "VAFIN", "APPRART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Der Gesch\u00f6pfe Wunder-Pracht strahlend in die Seele", "tokens": ["Der", "Ge\u00b7sch\u00f6p\u00b7fe", "Wun\u00b7der\u00b7Pracht", "strah\u00b7lend", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und sie wird in einer Menge GOTT-gef\u00e4lliger Jdeen,", "tokens": ["Und", "sie", "wird", "in", "ei\u00b7ner", "Men\u00b7ge", "GoT\u00b7T\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7ger", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "Weisheit, Lieb\u2019 und Macht des Sch\u00f6pfers, ja den Sch\u00f6pfer", "tokens": ["Weis\u00b7heit", ",", "Lieb'", "und", "Macht", "des", "Sch\u00f6p\u00b7fers", ",", "ja", "den", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}