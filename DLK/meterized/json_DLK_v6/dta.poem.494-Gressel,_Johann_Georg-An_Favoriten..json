{"dta.poem.494": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Favoriten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hat dein erster Blick so gleich in Brand gebracht/", "tokens": ["Mich", "hat", "dein", "ers\u00b7ter", "Blick", "so", "gleich", "in", "Brand", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich dich/ sch\u00f6nstes Kind/ von ferne nur betracht/", "tokens": ["Als", "ich", "dich", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "von", "fer\u00b7ne", "nur", "be\u00b7tracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$(", "ADJA", "NN", "$(", "APPR", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie aber dich das Gl\u00fcck lie\u00df in der N\u00e4he sehen/", "tokens": ["Wie", "a\u00b7ber", "dich", "das", "Gl\u00fcck", "lie\u00df", "in", "der", "N\u00e4\u00b7he", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da must mein Hertzens-Bau in lichten Flammen stehen.", "tokens": ["Da", "must", "mein", "Hert\u00b7zens\u00b7Bau", "in", "lich\u00b7ten", "Flam\u00b7men", "ste\u