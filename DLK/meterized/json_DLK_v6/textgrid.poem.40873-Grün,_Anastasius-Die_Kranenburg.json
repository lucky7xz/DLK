{"textgrid.poem.40873": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Kranenburg", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Koppenoll im Rathhaus schrieb sich die Finger matt,", "tokens": ["Herr", "Kop\u00b7pe\u00b7noll", "im", "Rath\u00b7haus", "schrieb", "sich", "die", "Fin\u00b7ger", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann rief er seinen Buben: \u00bbKomm', Bursche, nimm die\u00df Blatt,", "tokens": ["Dann", "rief", "er", "sei\u00b7nen", "Bu\u00b7ben", ":", "\u00bb", "Komm'", ",", "Bur\u00b7sche", ",", "nimm", "die\u00df", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "VVIMP", "$,", "NN", "$,", "VVIMP", "PDS", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zu Herren Maxen trag' es, dazu noch meinen Gru\u00df.\u00ab", "tokens": ["Zu", "Her\u00b7ren", "Ma\u00b7xen", "trag'", "es", ",", "da\u00b7zu", "noch", "mei\u00b7nen", "Gru\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "$,", "PAV", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Knabe neigt sich in Ehrfurcht und eilt auf schnellem Fu\u00df.", "tokens": ["Der", "Kna\u00b7be", "neigt", "sich", "in", "Ehr\u00b7furcht", "und", "eilt", "auf", "schnel\u00b7lem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "\u00bbdieweil zwei Sonnen am Himmel zugleich nicht k\u00f6nnen stehn,", "tokens": ["\u00bb", "die\u00b7weil", "zwei", "Son\u00b7nen", "am", "Him\u00b7mel", "zu\u00b7gleich", "nicht", "k\u00f6n\u00b7nen", "stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "CARD", "NN", "APPRART", "NN", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mu\u00df, weil jetzt steigt die eine, die andre untergehn;", "tokens": ["Mu\u00df", ",", "weil", "jetzt", "steigt", "die", "ei\u00b7ne", ",", "die", "and\u00b7re", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "ADV", "VVFIN", "ART", "ART", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und ist der Aar in Freiheit, raubt er nach Adlerbrauch,", "tokens": ["Und", "ist", "der", "Aar", "in", "Frei\u00b7heit", ",", "raubt", "er", "nach", "Ad\u00b7ler\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum sorgten wir Volk von Br\u00fcgge f\u00fcr einen K\u00e4fig auch.\u00ab", "tokens": ["Drum", "sorg\u00b7ten", "wir", "Volk", "von", "Br\u00fcg\u00b7ge", "f\u00fcr", "ei\u00b7nen", "K\u00e4\u00b7fig", "auch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "APPR", "NN", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "So stand es in dem Blatte, das Max nun l\u00e4chelnd las,", "tokens": ["So", "stand", "es", "in", "dem", "Blat\u00b7te", ",", "das", "Max", "nun", "l\u00e4\u00b7chelnd", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "NE", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drauf, wie ein rother Meerkrebs, der Z\u00fcnfte Siegel sa\u00df,", "tokens": ["Drauf", ",", "wie", "ein", "ro\u00b7ther", "Meer\u00b7krebs", ",", "der", "Z\u00fcnf\u00b7te", "Sie\u00b7gel", "sa\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Darunter Kopp'nolls Name in krummen Schn\u00f6rkeln stand,", "tokens": ["Da\u00b7run\u00b7ter", "Kopp'\u00b7nolls", "Na\u00b7me", "in", "krum\u00b7men", "Schn\u00f6r\u00b7keln", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Umkr\u00e4nzt von schwarzen Wolken, den Spuren der Schusterhand.", "tokens": ["Um\u00b7kr\u00e4nzt", "von", "schwar\u00b7zen", "Wol\u00b7ken", ",", "den", "Spu\u00b7ren", "der", "Schus\u00b7ter\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbes ist just Carnevalszeit, drum freut eu'r Schw\u00e4nkchen mich.\u00ab", "tokens": ["\u00bb", "es", "ist", "just", "Car\u00b7ne\u00b7vals\u00b7zeit", ",", "drum", "freut", "eu'r", "Schw\u00e4nk\u00b7chen", "mich", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "NN", "$,", "PAV", "VVFIN", "PPOSAT", "NN", "PPER", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So sprach der F\u00fcrst zum Boten, doch still spricht er f\u00fcr sich:", "tokens": ["So", "sprach", "der", "F\u00fcrst", "zum", "Bo\u00b7ten", ",", "doch", "still", "spricht", "er", "f\u00fcr", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "APPR", "PRF", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbstellt nur an euren Himmel kein thranig Lampenlicht!", "tokens": ["\u00bb", "stellt", "nur", "an", "eu\u00b7ren", "Him\u00b7mel", "kein", "thra\u00b7nig", "Lam\u00b7pen\u00b7licht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PIAT", "ADJD", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Gebt Acht, da\u00df nicht der Adler des K\u00e4figs St\u00e4be bricht!\u00ab", "tokens": ["Gebt", "Acht", ",", "da\u00df", "nicht", "der", "Ad\u00b7ler", "des", "K\u00e4\u00b7figs", "St\u00e4\u00b7be", "bricht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Drauf zog er in seinen Kerker, die Kranenburg hei\u00dft das Haus,", "tokens": ["Drauf", "zog", "er", "in", "sei\u00b7nen", "Ker\u00b7ker", ",", "die", "Kra\u00b7nen\u00b7burg", "hei\u00dft", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Ostens Gew\u00fcrze feilschte sonst hier ein Kr\u00e4mer aus.", "tokens": ["Des", "Os\u00b7tens", "Ge\u00b7w\u00fcr\u00b7ze", "feilschte", "sonst", "hier", "ein", "Kr\u00e4\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von Balsam, Myrrh' und Ambra qualmt Kammer noch und Luft;", "tokens": ["Von", "Bal\u00b7sam", ",", "Myr\u00b7rh'", "und", "A\u00b7mbra", "qualmt", "Kam\u00b7mer", "noch", "und", "Luft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hier lernte der junge K\u00f6nig zu w\u00fcrd'gen den Weihrauchduft.", "tokens": ["Hier", "lern\u00b7te", "der", "jun\u00b7ge", "K\u00f6\u00b7nig", "zu", "w\u00fcrd'\u00b7gen", "den", "Weih\u00b7rauch\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Jetzt herrscht das Volk zu Br\u00fcgge. Still stehen die Gewerbe,", "tokens": ["Jetzt", "herrscht", "das", "Volk", "zu", "Br\u00fcg\u00b7ge", ".", "Still", "ste\u00b7hen", "die", "Ge\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Gerber mu\u00df regieren. Wann bleibt ihm Zeit, da\u00df er gerbe?", "tokens": ["Der", "Ger\u00b7ber", "mu\u00df", "re\u00b7gie\u00b7ren", ".", "Wann", "bleibt", "ihm", "Zeit", ",", "da\u00df", "er", "ger\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$.", "PWAV", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie soll der Kr\u00e4mer vereinen den Zepter und Ellenstab?", "tokens": ["Wie", "soll", "der", "Kr\u00e4\u00b7mer", "ver\u00b7ei\u00b7nen", "den", "Zep\u00b7ter", "und", "El\u00b7len\u00b7stab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVFIN", "ART", "NN", "KON", "NE", "$."], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.4": {"text": "Der Todtengr\u00e4ber nur wetzet, wie sonst, den Spaten am Grab.", "tokens": ["Der", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "nur", "wet\u00b7zet", ",", "wie", "sonst", ",", "den", "Spa\u00b7ten", "am", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}}}}}