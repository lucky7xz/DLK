{"textgrid.poem.33593": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Lied im S\u00fcden", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.71", "id:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonnenuntergang!", "tokens": ["Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Lautlos ruhen S\u00e4ulengang", "tokens": ["Laut\u00b7los", "ru\u00b7hen", "S\u00e4u\u00b7len\u00b7gang"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verlassne Marmorb\u00e4der,", "tokens": ["Und", "ver\u00b7lass\u00b7ne", "Mar\u00b7mor\u00b7b\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo den stillen Weg entlang", "tokens": ["Wo", "den", "stil\u00b7len", "Weg", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPO"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch antiker Wagenr\u00e4der", "tokens": ["Noch", "an\u00b7ti\u00b7ker", "Wa\u00b7gen\u00b7r\u00e4\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Furchen tr\u00e4gt der Lavastein.", "tokens": ["Fur\u00b7chen", "tr\u00e4gt", "der", "La\u00b7vas\u00b7tein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rot im Abendschein", "tokens": ["Rot", "im", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wirft der \u00d6lwald l\u00e4ngre Schatten", "tokens": ["Wirft", "der", "\u00d6l\u00b7wald", "l\u00e4ng\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "L\u00e4ngs der braunen Felsenplatten", "tokens": ["L\u00e4ngs", "der", "brau\u00b7nen", "Fel\u00b7sen\u00b7plat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Um den Bergabhang \u2013", "tokens": ["Um", "den", "Ber\u00b7gab\u00b7hang", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Sonnenuntergang.", "tokens": ["Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Abendd\u00e4mmerung!", "tokens": ["A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7rung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Blumen atmen wieder jung,", "tokens": ["Blu\u00b7men", "at\u00b7men", "wie\u00b7der", "jung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in uns erbl\u00fchn die wei\u00dfen", "tokens": ["Und", "in", "uns", "er\u00b7bl\u00fchn", "die", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Rosen der Erinnerung.", "tokens": ["Ro\u00b7sen", "der", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "K\u00f6nnt' ich sie verwelken hei\u00dfen?", "tokens": ["K\u00f6nnt'", "ich", "sie", "ver\u00b7wel\u00b7ken", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schnell im S\u00fcden kommt die Nacht,", "tokens": ["Schnell", "im", "S\u00fc\u00b7den", "kommt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fl\u00fcchtig ist die Macht", "tokens": ["Fl\u00fcch\u00b7tig", "ist", "die", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Deines schw\u00e4rmerischen Gl\u00fcckes,", "tokens": ["Dei\u00b7nes", "schw\u00e4r\u00b7me\u00b7ri\u00b7schen", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie die Flammen eines Blickes", "tokens": ["Wie", "die", "Flam\u00b7men", "ei\u00b7nes", "Bli\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Voll Begeisterung,", "tokens": ["Voll", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Abendd\u00e4mmerung.", "tokens": ["A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Sommermitternacht!", "tokens": ["Som\u00b7mer\u00b7mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nur noch die Cicade wacht;", "tokens": ["Nur", "noch", "die", "Ci\u00b7ca\u00b7de", "wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ringsum ruhn die dunkeln T\u00e4ler.", "tokens": ["Ring\u00b7sum", "ruhn", "die", "dun\u00b7keln", "T\u00e4\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter alter Tempelpracht,", "tokens": ["Un\u00b7ter", "al\u00b7ter", "Tem\u00b7pel\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo gest\u00fcrzte Kapit\u00e4ler", "tokens": ["Wo", "ge\u00b7st\u00fcrz\u00b7te", "Ka\u00b7pi\u00b7t\u00e4\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Kissen, wo mein Haupt", "tokens": ["Mei\u00b7ne", "Kis\u00b7sen", ",", "wo", "mein", "Haupt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lorbeer selbst umlaubt,", "tokens": ["Lor\u00b7beer", "selbst", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sollt' ich's nicht gestehn im Liede,", "tokens": ["Sollt'", "ich's", "nicht", "ge\u00b7stehn", "im", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie dein tiefer, stiller Friede", "tokens": ["Wie", "dein", "tie\u00b7fer", ",", "stil\u00b7ler", "Frie\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ganz mich gl\u00fccklich macht,", "tokens": ["Ganz", "mich", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Sommermitternacht?", "tokens": ["Som\u00b7mer\u00b7mit\u00b7ter\u00b7nacht", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}