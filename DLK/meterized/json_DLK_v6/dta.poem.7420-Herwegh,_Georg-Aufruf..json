{"dta.poem.7420": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rei\u00dft die Kreuze aus der Erden!", "tokens": ["Rei\u00dft", "die", "Kreu\u00b7ze", "aus", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle sollen Schwerter werden,", "tokens": ["Al\u00b7le", "sol\u00b7len", "Schwer\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott im Himmel wird's verzeih'n.", "tokens": ["Gott", "im", "Him\u00b7mel", "wird's", "ver\u00b7zeih'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "APPRART", "NN", "VAFIN", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft, o la\u00dft das Verseschwei\u00dfen!", "tokens": ["La\u00dft", ",", "o", "la\u00dft", "das", "Ver\u00b7se\u00b7schwei\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den Ambos legt das Eisen!", "tokens": ["Auf", "den", "Am\u00b7bos", "legt", "das", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heiland soll das Eisen sein.", "tokens": ["Hei\u00b7land", "soll", "das", "Ei\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eure Tannen, eure Eichen \u2014", "tokens": ["Eu\u00b7re", "Tan\u00b7nen", ",", "eu\u00b7re", "Ei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Habt die gr\u00fcnen Fragezeichen", "tokens": ["Habt", "die", "gr\u00fc\u00b7nen", "Fra\u00b7ge\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutscher Freiheit ihr gewahrt?", "tokens": ["Deut\u00b7scher", "Frei\u00b7heit", "ihr", "ge\u00b7wahrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, sie soll nicht untergehen!", "tokens": ["Nein", ",", "sie", "soll", "nicht", "un\u00b7ter\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ihr fr\u00f6hlich Auferstehen", "tokens": ["Doch", "ihr", "fr\u00f6h\u00b7lich", "Auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kostet eine H\u00f6llenfahrt.", "tokens": ["Kos\u00b7tet", "ei\u00b7ne", "H\u00f6l\u00b7len\u00b7fahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deutsche, glaubet euren Sehern,", "tokens": ["Deut\u00b7sche", ",", "glau\u00b7bet", "eu\u00b7ren", "Se\u00b7hern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Tage werden ehern,", "tokens": ["Uns\u00b7re", "Ta\u00b7ge", "wer\u00b7den", "e\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Zukunft klirrt in Erz;", "tokens": ["Uns\u00b7re", "Zu\u00b7kunft", "klirrt", "in", "Erz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwarzer Tod ist unser Sold nur,", "tokens": ["Schwar\u00b7zer", "Tod", "ist", "un\u00b7ser", "Sold", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Gold ein Abendgold nur,", "tokens": ["Un\u00b7ser", "Gold", "ein", "A\u00b7bend\u00b7gold", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser Rot ein blutend Herz!", "tokens": ["Un\u00b7ser", "Rot", "ein", "blu\u00b7tend", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rei\u00dft die Kreuze aus der Erden!", "tokens": ["Rei\u00dft", "die", "Kreu\u00b7ze", "aus", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle sollen Schwerter werden,", "tokens": ["Al\u00b7le", "sol\u00b7len", "Schwer\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott im Himmel wird's verzeihn.", "tokens": ["Gott", "im", "Him\u00b7mel", "wird's", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt er unsre Feuer brausen", "tokens": ["H\u00f6rt", "er", "uns\u00b7re", "Feu\u00b7er", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein heilig Eisen sausen,", "tokens": ["Und", "sein", "hei\u00b7lig", "Ei\u00b7sen", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spricht er wohl den Segen drein.", "tokens": ["Spricht", "er", "wohl", "den", "Se\u00b7gen", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor der Freiheit sei kein Frieden,", "tokens": ["Vor", "der", "Frei\u00b7heit", "sei", "kein", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei dem Mann kein Weib beschieden", "tokens": ["Sei", "dem", "Mann", "kein", "Weib", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein golden Korn dem Feld;", "tokens": ["Und", "kein", "gol\u00b7den", "Korn", "dem", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der Freiheit, vor dem Siege", "tokens": ["Vor", "der", "Frei\u00b7heit", ",", "vor", "dem", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh' kein S\u00e4ugling aus der Wiege", "tokens": ["Seh'", "kein", "S\u00e4ug\u00b7ling", "aus", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frohen Blickes in die Welt!", "tokens": ["Fro\u00b7hen", "Bli\u00b7ckes", "in", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In den St\u00e4dten sei nur Trauern,", "tokens": ["In", "den", "St\u00e4d\u00b7ten", "sei", "nur", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die Freiheit von den Mauern", "tokens": ["Bis", "die", "Frei\u00b7heit", "von", "den", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwingt die Fahnen in das Land;", "tokens": ["Schwingt", "die", "Fah\u00b7nen", "in", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis du, Rhein, durch freie Bogen", "tokens": ["Bis", "du", ",", "Rhein", ",", "durch", "frei\u00b7e", "Bo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Donnerst, la\u00df die letzten Wogen", "tokens": ["Don\u00b7nerst", ",", "la\u00df", "die", "letz\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fluchend knirschen in den Sand.", "tokens": ["Flu\u00b7chend", "knir\u00b7schen", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Rei\u00dft die Kreuze aus der Erden!", "tokens": ["Rei\u00dft", "die", "Kreu\u00b7ze", "aus", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle sollen Schwerter werden,", "tokens": ["Al\u00b7le", "sol\u00b7len", "Schwer\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott im Himmel wird's verzeih'n.", "tokens": ["Gott", "im", "Him\u00b7mel", "wird's", "ver\u00b7zeih'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "APPRART", "NN", "VAFIN", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gen Tyrannen und Philister!", "tokens": ["Gen", "Ty\u00b7ran\u00b7nen", "und", "Phi\u00b7lis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch das Schwert hat seine Priester,", "tokens": ["Auch", "das", "Schwert", "hat", "sei\u00b7ne", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wir wollen Priester sein!", "tokens": ["Und", "wir", "wol\u00b7len", "Pries\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}