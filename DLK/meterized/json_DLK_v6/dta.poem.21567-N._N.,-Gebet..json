{"dta.poem.21567": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geschick ruht ganz in Deinen H\u00e4nden,", "tokens": ["Mein", "Ge\u00b7schick", "ruht", "ganz", "in", "Dei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sternenherrscher, und nach Deinem Wink", "tokens": ["Ster\u00b7nen\u00b7herr\u00b7scher", ",", "und", "nach", "Dei\u00b7nem", "Wink"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird die Nacht mein lichtes Leben enden", "tokens": ["Wird", "die", "Nacht", "mein", "lich\u00b7tes", "Le\u00b7ben", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und sich schlie\u00dfen meiner Tage Ring.", "tokens": ["Und", "sich", "schlie\u00b7\u00dfen", "mei\u00b7ner", "Ta\u00b7ge", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber gn\u00e4dig wandelst Du Dein Wollen,", "tokens": ["A\u00b7ber", "gn\u00e4\u00b7dig", "wan\u00b7delst", "Du", "Dein", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deinen Rathschlu\u00df: wenn ein hei\u00df Gebet", "tokens": ["Dei\u00b7nen", "Rath\u00b7schlu\u00df", ":", "wenn", "ein", "hei\u00df", "Ge\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "ART", "ADJD", "NN"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aus dem glaubenssel\u2019gen inbrunstvollen", "tokens": ["Aus", "dem", "glau\u00b7bens\u00b7sel'\u00b7gen", "in\u00b7brunst\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Menschenherzen zu Dir aufw\u00e4rts fleht.", "tokens": ["Men\u00b7schen\u00b7her\u00b7zen", "zu", "Dir", "auf\u00b7w\u00e4rts", "fleht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und so bitt ich heut mit heiligem Werben", "tokens": ["Und", "so", "bitt", "ich", "heut", "mit", "hei\u00b7li\u00b7gem", "Wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "La\u00df mich, Vater, nicht im Fr\u00fchling sterben.", "tokens": ["La\u00df", "mich", ",", "Va\u00b7ter", ",", "nicht", "im", "Fr\u00fch\u00b7ling", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn der Wiesen bunte Blumen blinken,", "tokens": ["Wenn", "der", "Wie\u00b7sen", "bun\u00b7te", "Blu\u00b7men", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Falter gaukeln in der lauen Luft,", "tokens": ["Fal\u00b7ter", "gau\u00b7keln", "in", "der", "lau\u00b7en", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Frei des Waldbachs helle Wellen winken", "tokens": ["Frei", "des", "Wald\u00b7bachs", "hel\u00b7le", "Wel\u00b7len", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Forstung trinkt den Maienduft,", "tokens": ["Und", "die", "For\u00b7stung", "trinkt", "den", "Mai\u00b7en\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Im Gelaub sich froh die Finken wiegen,", "tokens": ["Im", "Ge\u00b7laub", "sich", "froh", "die", "Fin\u00b7ken", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Drosseln schlagen, Lerchen jubelnd fliegen.", "tokens": ["Dros\u00b7seln", "schla\u00b7gen", ",", "Ler\u00b7chen", "ju\u00b7belnd", "flie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, dann strahlt die Welt, die lenzgek\u00fc\u00dfte,", "tokens": ["Ach", ",", "dann", "strahlt", "die", "Welt", ",", "die", "lenz\u00b7ge\u00b7k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wunderherrlich wie ein Paradies,", "tokens": ["Wun\u00b7der\u00b7herr\u00b7lich", "wie", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das ich trauern nur und weinen m\u00fc\u00dfte,", "tokens": ["Das", "ich", "trau\u00b7ern", "nur", "und", "wei\u00b7nen", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "KON", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn das Schicksal mich daraus verstie\u00df,", "tokens": ["Wenn", "das", "Schick\u00b7sal", "mich", "da\u00b7raus", "ver\u00b7stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und ich k\u00f6nnte noch im letzten Ringen,", "tokens": ["Und", "ich", "k\u00f6nn\u00b7te", "noch", "im", "letz\u00b7ten", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Welt, zu Dir die Liebe nicht bezwingen.", "tokens": ["Welt", ",", "zu", "Dir", "die", "Lie\u00b7be", "nicht", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "La\u00df mein m\u00fcdes Auge sich umflirren,", "tokens": ["La\u00df", "mein", "m\u00fc\u00b7des", "Au\u00b7ge", "sich", "um\u00b7flir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn der Winter durch die Tannen saust", "tokens": ["Wenn", "der", "Win\u00b7ter", "durch", "die", "Tan\u00b7nen", "saust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der wilde Forstwind durch die d\u00fcrren,", "tokens": ["Und", "der", "wil\u00b7de", "Forst\u00b7wind", "durch", "die", "d\u00fcr\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bl\u00e4tterlosen Buchenwipfel braust,", "tokens": ["Bl\u00e4t\u00b7ter\u00b7lo\u00b7sen", "Bu\u00b7chen\u00b7wip\u00b7fel", "braust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Eisige Wolken sich am Himmel ballen", "tokens": ["Ei\u00b7si\u00b7ge", "Wol\u00b7ken", "sich", "am", "Him\u00b7mel", "bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PRF", "APPRART", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und in Schnee und Hagel niederfallen.", "tokens": ["Und", "in", "Schnee", "und", "Ha\u00b7gel", "nie\u00b7der\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Gern und freudig werd\u2019 ich Deinem Winken", "tokens": ["Gern", "und", "freu\u00b7dig", "werd'", "ich", "Dei\u00b7nem", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dann mich weihen und mit voller Brust", "tokens": ["Dann", "mich", "wei\u00b7hen", "und", "mit", "vol\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVINF", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meines Daseins letzten Athem trinken,", "tokens": ["Mei\u00b7nes", "Da\u00b7seins", "letz\u00b7ten", "A\u00b7them", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jener sel\u2019gen Hoffnung froh bewu\u00dft:", "tokens": ["Je\u00b7ner", "sel'\u00b7gen", "Hoff\u00b7nung", "froh", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df ich aus der Erde Winterwehe", "tokens": ["Da\u00df", "ich", "aus", "der", "Er\u00b7de", "Win\u00b7ter\u00b7we\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In den ew\u2019gen Sternenfr\u00fchling gehe.", "tokens": ["In", "den", "ew'\u00b7gen", "Ster\u00b7nen\u00b7fr\u00fch\u00b7ling", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}