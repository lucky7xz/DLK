{"textgrid.poem.39917": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Psalm 104, 4", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anklopft das Wetter unter Sturm", "tokens": ["An\u00b7klopft", "das", "Wet\u00b7ter", "un\u00b7ter", "Sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Biberach am S\u00fcnderthurm.", "tokens": ["Zu", "Bi\u00b7be\u00b7rach", "am", "S\u00fcn\u00b7der\u00b7thurm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die W\u00f6lbung bebt vom Widerhall,", "tokens": ["Die", "W\u00f6l\u00b7bung", "bebt", "vom", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Eisenst\u00e4be zittern all.", "tokens": ["Die", "Ei\u00b7sen\u00b7st\u00e4\u00b7be", "zit\u00b7tern", "all", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es blitzt so hell, es kracht so schnell:", "tokens": ["Es", "blitzt", "so", "hell", ",", "es", "kracht", "so", "schnell", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegt auf Stroh kein Diebsgesell,", "tokens": ["Da", "liegt", "auf", "Stroh", "kein", "Diebs\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem in der schwarzen Feuernacht", "tokens": ["Dem", "in", "der", "schwar\u00b7zen", "Feu\u00b7er\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht das Gewissen lodernd wacht.", "tokens": ["Nicht", "das", "Ge\u00b7wis\u00b7sen", "lo\u00b7dernd", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein jeder Blitz weckt eine T\u00fcck',", "tokens": ["Ein", "je\u00b7der", "Blitz", "weckt", "ei\u00b7ne", "T\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jeder Knall ein Bubenst\u00fcck.", "tokens": ["Ein", "je\u00b7der", "Knall", "ein", "Bu\u00b7ben\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie werfen auf die Kniee sich,", "tokens": ["Sie", "wer\u00b7fen", "auf", "die", "Kni\u00b7ee", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und flehn und weinen bitterlich.", "tokens": ["Und", "flehn", "und", "wei\u00b7nen", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein M\u00f6rder nur ohn' all's Gebet", "tokens": ["Ein", "M\u00f6r\u00b7der", "nur", "ohn'", "all's", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "In Ketten angeschmiedet steht,", "tokens": ["In", "Ket\u00b7ten", "an\u00b7ge\u00b7schmie\u00b7det", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein eisern Band den Leib umflicht,", "tokens": ["Ein", "ei\u00b7sern", "Band", "den", "Leib", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er kann nicht knie'n, er th\u00e4t's auch nicht.", "tokens": ["Er", "kann", "nicht", "knie'n", ",", "er", "th\u00e4t's", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er rasselt an der Wand vor Wut,", "tokens": ["Er", "ras\u00b7selt", "an", "der", "Wand", "vor", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wohl ein Wolf im K\u00e4fig thut;", "tokens": ["Wie", "wohl", "ein", "Wolf", "im", "K\u00e4\u00b7fig", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er fl\u00fcstert: \u00bbBald bin ich befreit!", "tokens": ["Er", "fl\u00fcs\u00b7tert", ":", "\u00bb", "Bald", "bin", "ich", "be\u00b7freit", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blitz Element, jetzt ist es Zeit!\u00ab", "tokens": ["Blitz", "E\u00b7le\u00b7ment", ",", "jetzt", "ist", "es", "Zeit", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Aus einer Falte seiner Haut", "tokens": ["Aus", "ei\u00b7ner", "Fal\u00b7te", "sei\u00b7ner", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00fcpft eine Feil', eh's Einer schaut.", "tokens": ["Schl\u00fcpft", "ei\u00b7ne", "Feil'", ",", "eh's", "Ei\u00b7ner", "schaut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbjetzt feil' ich in der dunkeln Nacht,", "tokens": ["\u00bb", "jetzt", "feil'", "ich", "in", "der", "dun\u00b7keln", "Nacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich feile, weil das Wetter kracht!", "tokens": ["Ich", "fei\u00b7le", ",", "weil", "das", "Wet\u00b7ter", "kracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr Narren, betet nur und heult", "tokens": ["Ihr", "Nar\u00b7ren", ",", "be\u00b7tet", "nur", "und", "heult"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Derweil mein Ring wird durchgefeilt!", "tokens": ["Der\u00b7weil", "mein", "Ring", "wird", "durch\u00b7ge\u00b7feilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eu'r Winseln bittet euch nicht los,", "tokens": ["Eu'r", "Win\u00b7seln", "bit\u00b7tet", "euch", "nicht", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ich, bald wandl' ich kettenblo\u00df.", "tokens": ["Doch", "ich", ",", "bald", "wandl'", "ich", "ket\u00b7ten\u00b7blo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dem Richter, dem Gesetz zu Spott!", "tokens": ["Dem", "Rich\u00b7ter", ",", "dem", "Ge\u00b7setz", "zu", "Spott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einen Strich \u2013 dann Trotz dir, Gott!", "tokens": ["Noch", "ei\u00b7nen", "Strich", "\u2013", "dann", "Trotz", "dir", ",", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ADV", "APPR", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja wettre nur, ich feil', ich feil'!\u00ab \u2013", "tokens": ["Ja", "wett\u00b7re", "nur", ",", "ich", "feil'", ",", "ich", "feil'", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "$,", "PPER", "PTKVZ", "$,", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da fliegt der Blitz, der Flammenpfeil.", "tokens": ["Da", "fliegt", "der", "Blitz", ",", "der", "Flam\u00b7men\u00b7pfeil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da feilt der Stral den Ring durchein,", "tokens": ["Da", "feilt", "der", "Stral", "den", "Ring", "durc\u00b7hein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er feilt bis in das Herz hinein,", "tokens": ["Er", "feilt", "bis", "in", "das", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der M\u00f6rder kr\u00fcmmt sich wie ein Wurm,", "tokens": ["Der", "M\u00f6r\u00b7der", "kr\u00fcmmt", "sich", "wie", "ein", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Donner sch\u00fcttelt an dem Thurm.", "tokens": ["Der", "Don\u00b7ner", "sch\u00fct\u00b7telt", "an", "dem", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Andern hat verschont der Schlag,", "tokens": ["Die", "An\u00b7dern", "hat", "ver\u00b7schont", "der", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nur als schwarze Schlacke lag,", "tokens": ["Und", "nur", "als", "schwar\u00b7ze", "Schla\u00b7cke", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Ketten und mit Eisenband", "tokens": ["Mit", "Ket\u00b7ten", "und", "mit", "Ei\u00b7sen\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschmolzen, Einer an der Wand.", "tokens": ["Ver\u00b7schmol\u00b7zen", ",", "Ei\u00b7ner", "an", "der", "Wand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}