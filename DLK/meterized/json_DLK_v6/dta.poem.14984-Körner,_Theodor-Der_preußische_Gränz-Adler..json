{"dta.poem.14984": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der preu\u00dfische Gr\u00e4nz-Adler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Das Herz verhei\u00dft mir Sieg in deinem Zeichen.", "tokens": ["Das", "Herz", "ver\u00b7hei\u00dft", "mir", "Sieg", "in", "dei\u00b7nem", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch! edler Aar! die Wolke mu\u00df dir weichen,", "tokens": ["Durch", "!", "ed\u00b7ler", "Aar", "!", "die", "Wol\u00b7ke", "mu\u00df", "dir", "wei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ADJA", "NN", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fleuch r\u00e4chend auf von deiner Todten H\u00fcgel. \u2014", "tokens": ["Fleuch", "r\u00e4\u00b7chend", "auf", "von", "dei\u00b7ner", "Tod\u00b7ten", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "APPR", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das freie Ro\u00df gehorcht dem Sklavenz\u00fcgel,", "tokens": ["Das", "frei\u00b7e", "Ro\u00df", "ge\u00b7horcht", "dem", "Skla\u00b7ven\u00b7z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Glanz der Raute seh' ich welk verbleichen,", "tokens": ["Den", "Glanz", "der", "Rau\u00b7te", "seh'", "ich", "welk", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der L\u00f6we kr\u00fcmmt sich unter fremden Streichen,", "tokens": ["Der", "L\u00f6\u00b7we", "kr\u00fcmmt", "sich", "un\u00b7ter", "frem\u00b7den", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du nur erhebst mit neuem Muth die Fl\u00fcgel.", "tokens": ["Du", "nur", "er\u00b7hebst", "mit", "neu\u00b7em", "Muth", "die", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bald werd' ich unter deinen S\u00f6hnen stehen,", "tokens": ["Bald", "werd'", "ich", "un\u00b7ter", "dei\u00b7nen", "S\u00f6h\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bald werd' ich dich im Kampfe wiedersehen,", "tokens": ["Bald", "werd'", "ich", "dich", "im", "Kamp\u00b7fe", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du wirst voran zum Sieg, zur Freiheit wehen!", "tokens": ["Du", "wirst", "vo\u00b7ran", "zum", "Sieg", ",", "zur", "Frei\u00b7heit", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Was dann auch immer aus dem S\u00e4nger werde,", "tokens": ["Was", "dann", "auch", "im\u00b7mer", "aus", "dem", "S\u00e4n\u00b7ger", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Heil ihm, erk\u00e4mpft er auch mit seinem Schwerdte", "tokens": ["Heil", "ihm", ",", "er\u00b7k\u00e4mpft", "er", "auch", "mit", "sei\u00b7nem", "Schwerd\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Nichts als ein Grab in einer freien Erde.", "tokens": ["Nichts", "als", "ein", "Grab", "in", "ei\u00b7ner", "frei\u00b7en", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}