{"textgrid.poem.26201": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Im Zimmer deine zarte Brust sich atmend auf und ab bewegt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blaue Tag liegt in dem Fensterrahmen unerregt.", "tokens": ["Der", "blau\u00b7e", "Tag", "liegt", "in", "dem", "Fens\u00b7ter\u00b7rah\u00b7men", "un\u00b7er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Im Zimmer deine zarte Brust sich atmend auf und ab bewegt,", "tokens": ["Im", "Zim\u00b7mer", "dei\u00b7ne", "zar\u00b7te", "Brust", "sich", "at\u00b7mend", "auf", "und", "ab", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "PRF", "ADJD", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Als ist zu atmen schon allein des Lebens allerh\u00f6chste Lust.", "tokens": ["Als", "ist", "zu", "at\u00b7men", "schon", "al\u00b7lein", "des", "Le\u00b7bens", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PTKZU", "VVINF", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Windstille liegt am Berge drau\u00dfen unbewu\u00dft,", "tokens": ["Wind\u00b7stil\u00b7le", "liegt", "am", "Ber\u00b7ge", "drau\u00b7\u00dfen", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als hat die Erde weite Wege heut zur\u00fcckgelegt.", "tokens": ["Als", "hat", "die", "Er\u00b7de", "wei\u00b7te", "We\u00b7ge", "heut", "zu\u00b7r\u00fcck\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und durch die Fenster sieht der Berg auf deine Brust,", "tokens": ["Und", "durch", "die", "Fens\u00b7ter", "sieht", "der", "Berg", "auf", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die keinen Augenblick erm\u00fcdet stille liegt.", "tokens": ["Die", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "er\u00b7m\u00fc\u00b7det", "stil\u00b7le", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Vogel hoch hin \u00fcberm Himmel einsam fliegt", "tokens": ["Ein", "Vo\u00b7gel", "hoch", "hin", "\u00fc\u00b7berm", "Him\u00b7mel", "ein\u00b7sam", "fliegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADJD", "ADV", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wie dein Atem ohne Pausen sich \u00fcber allen Bergen wiegt.", "tokens": ["Und", "wie", "dein", "A\u00b7tem", "oh\u00b7ne", "Pau\u00b7sen", "sich", "\u00fc\u00b7ber", "al\u00b7len", "Ber\u00b7gen", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "APPR", "NN", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}