{"dta.poem.19241": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt: der Donner rollt, wann auf unreine Geister", "tokens": ["Man", "sagt", ":", "der", "Don\u00b7ner", "rollt", ",", "wann", "auf", "un\u00b7rei\u00b7ne", "Geis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der L\u00fcfte reiner grollt und wird im Kampf ihr Meister.", "tokens": ["Der", "L\u00fcf\u00b7te", "rei\u00b7ner", "grollt", "und", "wird", "im", "Kampf", "ihr", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "KON", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Blitz ist sein Gescho\u00df, geschleudert aufs Gez\u00fcchte,", "tokens": ["Der", "Blitz", "ist", "sein", "Ge\u00b7scho\u00df", ",", "ge\u00b7schleu\u00b7dert", "aufs", "Ge\u00b7z\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zitternd sucht, wohin vor seinem Grimm es fl\u00fcchte.", "tokens": ["Das", "zit\u00b7ternd", "sucht", ",", "wo\u00b7hin", "vor", "sei\u00b7nem", "Grimm", "es", "fl\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VVFIN", "$,", "PWAV", "APPR", "PPOSAT", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wohin es nehmen mag die Flucht zu Schlucht und Schluft,", "tokens": ["Wo\u00b7hin", "es", "neh\u00b7men", "mag", "die", "Flucht", "zu", "Schlucht", "und", "Schluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Schlupf erreichts der schnelle Geist der Luft.", "tokens": ["In", "je\u00b7dem", "Schlupf", "er\u00b7reichts", "der", "schnel\u00b7le", "Geist", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum, wenn du bist im Haus, steh nicht am Fenster offen;", "tokens": ["Drum", ",", "wenn", "du", "bist", "im", "Haus", ",", "steh", "nicht", "am", "Fens\u00b7ter", "of\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$,", "VVFIN", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie k\u00f6nnten sich herein vor ihm zu retten hoffen.", "tokens": ["Sie", "k\u00f6nn\u00b7ten", "sich", "her\u00b7ein", "vor", "ihm", "zu", "ret\u00b7ten", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKVZ", "APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Und wenn du bist im Feld, steh auch nicht unterm Baum;", "tokens": ["Und", "wenn", "du", "bist", "im", "Feld", ",", "steh", "auch", "nicht", "un\u00b7term", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort bergen k\u00f6nnten die Verfolgten sich im Raum.", "tokens": ["Dort", "ber\u00b7gen", "k\u00f6nn\u00b7ten", "die", "Ver\u00b7folg\u00b7ten", "sich", "im", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Nur, stille wo du bist, bleib, bis sie ausgekriegt;", "tokens": ["Nur", ",", "stil\u00b7le", "wo", "du", "bist", ",", "bleib", ",", "bis", "sie", "aus\u00b7ge\u00b7kriegt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PWAV", "PPER", "VAFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald die unreinen hat der reine Geist besiegt.", "tokens": ["Bald", "die", "un\u00b7rei\u00b7nen", "hat", "der", "rei\u00b7ne", "Geist", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}