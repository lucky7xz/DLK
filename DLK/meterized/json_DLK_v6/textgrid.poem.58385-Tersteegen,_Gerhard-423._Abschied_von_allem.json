{"textgrid.poem.58385": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "423. Abschied von allem", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun fahr hin, o Kreatur,", "tokens": ["Nun", "fahr", "hin", ",", "o", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Deinen Sch\u00f6pfer such' ich nur;", "tokens": ["Dei\u00b7nen", "Sch\u00f6p\u00b7fer", "such'", "ich", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig weg, ihr Eigenheiten,", "tokens": ["E\u00b7wig", "weg", ",", "ihr", "Ei\u00b7gen\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will ganz von euch mich scheiden;", "tokens": ["Ich", "will", "ganz", "von", "euch", "mich", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott, der l\u00e4utre, bis ich rein", "tokens": ["Gott", ",", "der", "l\u00e4ut\u00b7re", ",", "bis", "ich", "rein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd' in seinen Augen sein!", "tokens": ["Werd'", "in", "sei\u00b7nen", "Au\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihm will ich mich ewig lassen", "tokens": ["Ihm", "will", "ich", "mich", "e\u00b7wig", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ihn blo\u00df im Geist umfassen.", "tokens": ["Und", "ihn", "blo\u00df", "im", "Geist", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}