{"textgrid.poem.39272": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[die alte Zeit mit ihrer Kraft, der reinen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alte Zeit mit ihrer Kraft, der reinen,", "tokens": ["Die", "al\u00b7te", "Zeit", "mit", "ih\u00b7rer", "Kraft", ",", "der", "rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihrem Leid aus tiefen Herzenstrieben,", "tokens": ["Mit", "ih\u00b7rem", "Leid", "aus", "tie\u00b7fen", "Her\u00b7zen\u00b7strie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihrer Treue, ihrem Feuerlieben,", "tokens": ["Mit", "ih\u00b7rer", "Treu\u00b7e", ",", "ih\u00b7rem", "Feu\u00b7er\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mag mir oft als liebes Bild erscheinen.", "tokens": ["Die", "mag", "mir", "oft", "als", "lie\u00b7bes", "Bild", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und hei\u00dfe Tr\u00e4nen m\u00f6cht' ich klagend weinen", "tokens": ["Und", "hei\u00b7\u00dfe", "Tr\u00e4\u00b7nen", "m\u00f6cht'", "ich", "kla\u00b7gend", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei alter M\u00e4r' von deutschen Schwerteshieben,", "tokens": ["Bei", "al\u00b7ter", "M\u00e4r'", "von", "deut\u00b7schen", "Schwer\u00b7tes\u00b7hie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Minnehuld, die fest im Tod geblieben,", "tokens": ["Von", "Min\u00b7ne\u00b7huld", ",", "die", "fest", "im", "Tod", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Blut besiegelnd ihres Bunds Vereinen.", "tokens": ["Mit", "Blut", "be\u00b7sie\u00b7gelnd", "ih\u00b7res", "Bunds", "Ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "PPOSAT", "NN", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Treue schwand, die Tod nicht trennen konnte,", "tokens": ["Die", "Treu\u00b7e", "schwand", ",", "die", "Tod", "nicht", "tren\u00b7nen", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie floh hinweg vom Zeitensturm verschlagen,", "tokens": ["Sie", "floh", "hin\u00b7weg", "vom", "Zei\u00b7ten\u00b7sturm", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sie nicht mehr im Rittermut sich sonnte.", "tokens": ["Als", "sie", "nicht", "mehr", "im", "Rit\u00b7ter\u00b7mut", "sich", "sonn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Minne starb im Hauch der feigen L\u00fcste,", "tokens": ["Die", "Min\u00b7ne", "starb", "im", "Hauch", "der", "fei\u00b7gen", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der S\u00e4nger blieb mit seiner Lieder Klagen", "tokens": ["Der", "S\u00e4n\u00b7ger", "blieb", "mit", "sei\u00b7ner", "Lie\u00b7der", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein gr\u00fcnes Eiland in der Sandesw\u00fcste.", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Ei\u00b7land", "in", "der", "San\u00b7des\u00b7w\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}