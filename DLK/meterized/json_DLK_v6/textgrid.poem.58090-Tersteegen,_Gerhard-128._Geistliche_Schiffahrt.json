{"textgrid.poem.58090": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "128. Geistliche Schiffahrt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stroman geht meine Reis', wie soll ich's machen dann,", "tokens": ["Stro\u00b7man", "geht", "mei\u00b7ne", "Reis'", ",", "wie", "soll", "ich's", "ma\u00b7chen", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PIS", "VVFIN", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ich bin so matt und m\u00fcd', da\u00df ich nicht rudern kann?", "tokens": ["Ich", "bin", "so", "matt", "und", "m\u00fcd'", ",", "da\u00df", "ich", "nicht", "ru\u00b7dern", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier sitz' ich still und wart', die Segel haltend auf,", "tokens": ["Hier", "sitz'", "ich", "still", "und", "wart'", ",", "die", "Se\u00b7gel", "hal\u00b7tend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis da\u00df ein guter Wind bef\u00f6rdre meinen Lauf.", "tokens": ["Bis", "da\u00df", "ein", "gu\u00b7ter", "Wind", "be\u00b7f\u00f6rd\u00b7re", "mei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}