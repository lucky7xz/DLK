{"textgrid.poem.51151": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herr gab dir ein gutes Augenpaar,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr gab dir ein gutes Augenpaar,", "tokens": ["Der", "Herr", "gab", "dir", "ein", "gu\u00b7tes", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wei\u00dft damit zu blicken lieb und klar.", "tokens": ["Du", "wei\u00dft", "da\u00b7mit", "zu", "bli\u00b7cken", "lieb", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit feiner Hand h\u00e4ltst du in sch\u00f6nen Banden,", "tokens": ["Mit", "fei\u00b7ner", "Hand", "h\u00e4ltst", "du", "in", "sch\u00f6\u00b7nen", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das er dir gab, dein anmutreiches Haar.", "tokens": ["Das", "er", "dir", "gab", ",", "dein", "an\u00b7mu\u00b7trei\u00b7ches", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gleich einer Palme aus den Morgenlanden", "tokens": ["Gleich", "ei\u00b7ner", "Pal\u00b7me", "aus", "den", "Mor\u00b7gen\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lie\u00df er dich wachsen, der im Anfang war;", "tokens": ["Lie\u00df", "er", "dich", "wach\u00b7sen", ",", "der", "im", "An\u00b7fang", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Du aber wei\u00dft dich k\u00f6stlich zu gewanden,", "tokens": ["Du", "a\u00b7ber", "wei\u00dft", "dich", "k\u00f6st\u00b7lich", "zu", "ge\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sich verdunkelt deiner Schwestern Schar.", "tokens": ["Da\u00df", "sich", "ver\u00b7dun\u00b7kelt", "dei\u00b7ner", "Schwes\u00b7tern", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie dankbar du des Sch\u00f6pfers Sinn verstanden,", "tokens": ["Wie", "dank\u00b7bar", "du", "des", "Sch\u00f6p\u00b7fers", "Sinn", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als seine Interpretin legst du dar!", "tokens": ["Als", "sei\u00b7ne", "In\u00b7ter\u00b7pre\u00b7tin", "legst", "du", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}