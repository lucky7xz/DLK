{"textgrid.poem.41372": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirsch, der Hund und der Wolf", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder Frommer thut, was man in Hamburg thut:", "tokens": ["Ein", "je\u00b7der", "From\u00b7mer", "thut", ",", "was", "man", "in", "Ham\u00b7burg", "thut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PIS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gute glaubt er oft, allein das B\u00f6se selten.", "tokens": ["Das", "Gu\u00b7te", "glaubt", "er", "oft", ",", "al\u00b7lein", "das", "B\u00f6\u00b7se", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihn lehrt der Lauf der Welt, da\u00df Neid und Frevelmuth", "tokens": ["Ihn", "lehrt", "der", "Lauf", "der", "Welt", ",", "da\u00df", "Neid", "und", "Fre\u00b7vel\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tugend Henker sind, und auch die Fr\u00f6mmsten schelten.", "tokens": ["Der", "Tu\u00b7gend", "Hen\u00b7ker", "sind", ",", "und", "auch", "die", "Fr\u00f6mms\u00b7ten", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sonst ist's ein blo\u00dfes Gl\u00fcck, wenn einen B\u00f6sewicht", "tokens": ["Sonst", "ist's", "ein", "blo\u00b7\u00dfes", "Gl\u00fcck", ",", "wenn", "ei\u00b7nen", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Unschuld und das Recht, trotz seiner Kunst! besch\u00e4men.", "tokens": ["Die", "Un\u00b7schuld", "und", "das", "Recht", ",", "trotz", "sei\u00b7ner", "Kunst", "!", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Wolf jagt' einen Hund. Der bat, aus Zuversicht,", "tokens": ["Ein", "Wolf", "jagt'", "ei\u00b7nen", "Hund", ".", "Der", "bat", ",", "aus", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$.", "ART", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Hirsch, ihn unges\u00e4umt in seinen Schutz zu nehmen.", "tokens": ["Den", "Hirsch", ",", "ihn", "un\u00b7ge\u00b7s\u00e4umt", "in", "sei\u00b7nen", "Schutz", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Fl\u00fcchtling wird erh\u00f6rt; doch ihn verfolgt sein Feind,", "tokens": ["Der", "Fl\u00fccht\u00b7ling", "wird", "er\u00b7h\u00f6rt", ";", "doch", "ihn", "ver\u00b7folgt", "sein", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und spricht: Ich komm', o Hirsch, dein einzig Kalb zu r\u00e4chen.", "tokens": ["Und", "spricht", ":", "Ich", "komm'", ",", "o", "Hirsch", ",", "dein", "ein\u00b7zig", "Kalb", "zu", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Schnapphan hat's erw\u00fcrgt; ich sah es, ich, dein Freund,", "tokens": ["Der", "Schnap\u00b7phan", "hat's", "er\u00b7w\u00fcrgt", ";", "ich", "sah", "es", ",", "ich", ",", "dein", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und den verwirkten Hals soll ihm kein andrer brechen.", "tokens": ["Und", "den", "ver\u00b7wirk\u00b7ten", "Hals", "soll", "ihm", "kein", "an\u00b7drer", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PPER", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Hund verneint die That. Er fleht, und schw\u00f6rt dabei:", "tokens": ["Der", "Hund", "ver\u00b7neint", "die", "That", ".", "Er", "fleht", ",", "und", "schw\u00f6rt", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es sei ihm, von Natur, das Wildpret recht zuwider.", "tokens": ["Es", "sei", "ihm", ",", "von", "Na\u00b7tur", ",", "das", "Wild\u00b7pret", "recht", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "APPR", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihm zeigt der strenge Hirsch sein f\u00fcrchterlich Geweih.", "tokens": ["Ihm", "zeigt", "der", "stren\u00b7ge", "Hirsch", "sein", "f\u00fcrch\u00b7ter\u00b7lich", "Ge\u00b7weih", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Beklagter seufzt und heult, und wirft sich vor ihm nieder.", "tokens": ["Be\u00b7klag\u00b7ter", "seufzt", "und", "heult", ",", "und", "wirft", "sich", "vor", "ihm", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als drauf sein Kl\u00e4ger ihm mit neuen Zeugen droht,", "tokens": ["Als", "drauf", "sein", "Kl\u00e4\u00b7ger", "ihm", "mit", "neu\u00b7en", "Zeu\u00b7gen", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PPOSAT", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "K\u00f6mmt, gleich zu rechter Zeit, das Hirschkalb hergesprungen.", "tokens": ["K\u00f6mmt", ",", "gleich", "zu", "rech\u00b7ter", "Zeit", ",", "das", "Hirschkalb", "her\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Den frechen L\u00fcgner trifft Verwirrung, Furcht und Tod;", "tokens": ["Den", "fre\u00b7chen", "L\u00fcg\u00b7ner", "trifft", "Ver\u00b7wir\u00b7rung", ",", "Furcht", "und", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch dieses Beispiel schreckt nur wenig L\u00e4sterzungen.", "tokens": ["Doch", "die\u00b7ses", "Bei\u00b7spiel", "schreckt", "nur", "we\u00b7nig", "L\u00e4s\u00b7ter\u00b7zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}