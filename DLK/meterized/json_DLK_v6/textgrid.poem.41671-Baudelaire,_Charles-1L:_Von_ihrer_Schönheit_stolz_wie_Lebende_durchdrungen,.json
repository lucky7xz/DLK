{"textgrid.poem.41671": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von ihrer Sch\u00f6nheit stolz wie Lebende durchdrungen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ihrer Sch\u00f6nheit stolz wie Lebende durchdrungen,", "tokens": ["Von", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "stolz", "wie", "Le\u00b7ben\u00b7de", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Prunkt sie mit Taschentuch, mit Handschuh und mit Strauss;", "tokens": ["Prunkt", "sie", "mit", "Ta\u00b7schen\u00b7tuch", ",", "mit", "Hand\u00b7schuh", "und", "mit", "Strauss", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In k\u00fchner L\u00e4ssigkeit zeigt sie sich ungezwungen \u2013", "tokens": ["In", "k\u00fch\u00b7ner", "L\u00e4s\u00b7sig\u00b7keit", "zeigt", "sie", "sich", "un\u00b7ge\u00b7zwun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie eine magere Kokette sieht sie aus.", "tokens": ["Wie", "ei\u00b7ne", "ma\u00b7ge\u00b7re", "Ko\u00b7ket\u00b7te", "sieht", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat je auf einem Ball man schlankren Wuchs gesehen?", "tokens": ["Hat", "je", "auf", "ei\u00b7nem", "Ball", "man", "schlan\u00b7kren", "Wuchs", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PIS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schaust ihr grelles Kleid, an weiten Falten reich,", "tokens": ["Du", "schaust", "ihr", "grel\u00b7les", "Kleid", ",", "an", "wei\u00b7ten", "Fal\u00b7ten", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf einen Knochenfu\u00df in Wellen niedergehen,", "tokens": ["Auf", "ei\u00b7nen", "Kno\u00b7chen\u00b7fu\u00df", "in", "Wel\u00b7len", "nie\u00b7der\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von buntem Schuh geschm\u00fcckt, der zieren Blumen gleich.", "tokens": ["Von", "bun\u00b7tem", "Schuh", "ge\u00b7schm\u00fcckt", ",", "der", "zie\u00b7ren", "Blu\u00b7men", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr magres Schl\u00fcsselbein umschmiegen leichte Spitzen,", "tokens": ["Ihr", "mag\u00b7res", "Schl\u00fcs\u00b7sel\u00b7bein", "um\u00b7schmie\u00b7gen", "leich\u00b7te", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich einem \u00fcppgen Bach, der sich am Felsen reibt,", "tokens": ["Gleich", "ei\u00b7nem", "\u00fcpp\u00b7gen", "Bach", ",", "der", "sich", "am", "Fel\u00b7sen", "reibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sittsam bergen sie vor possenhaften Witzen", "tokens": ["Und", "sitt\u00b7sam", "ber\u00b7gen", "sie", "vor", "pos\u00b7sen\u00b7haf\u00b7ten", "Wit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den unheilvollen Reiz, der tief verborgen bleibt.", "tokens": ["Den", "un\u00b7heil\u00b7vol\u00b7len", "Reiz", ",", "der", "tief", "ver\u00b7bor\u00b7gen", "bleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die hohlen Augen sind erloschen und verwittert,", "tokens": ["Die", "hoh\u00b7len", "Au\u00b7gen", "sind", "er\u00b7lo\u00b7schen", "und", "ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es nickt der Blumenschmuck vom Sch\u00e4del grauenvoll,", "tokens": ["Es", "nickt", "der", "Blu\u00b7men\u00b7schmuck", "vom", "Sch\u00e4\u00b7del", "grau\u00b7en\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schwank sich wiegend auf den d\u00fcnnen Wirbeln zittert \u2013", "tokens": ["Der", "schwank", "sich", "wie\u00b7gend", "auf", "den", "d\u00fcn\u00b7nen", "Wir\u00b7beln", "zit\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVPP", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Zauber eines Nichts, das aufgeputzt und toll!", "tokens": ["O", "Zau\u00b7ber", "ei\u00b7nes", "Nichts", ",", "das", "auf\u00b7ge\u00b7putzt", "und", "toll", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gar manche m\u00f6chten dich ein n\u00e4chtig Zerrbild nennen,", "tokens": ["Gar", "man\u00b7che", "m\u00f6ch\u00b7ten", "dich", "ein", "n\u00e4ch\u00b7tig", "Zerr\u00b7bild", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PRF", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die von der Trunkenheit des Fleisches nur gewu\u00dft,", "tokens": ["Die", "von", "der", "Trun\u00b7ken\u00b7heit", "des", "Flei\u00b7sches", "nur", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die nicht der menschlichen Gebeine Feinheit kennen:", "tokens": ["Die", "nicht", "der", "menschli\u00b7chen", "Ge\u00b7bei\u00b7ne", "Fein\u00b7heit", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du m\u00e4chtiges Skelett stillst meine h\u00f6chste Lust!", "tokens": ["Du", "m\u00e4ch\u00b7ti\u00b7ges", "Ske\u00b7lett", "stillst", "mei\u00b7ne", "h\u00f6chs\u00b7te", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Kommst du zu st\u00f6ren mit erschreckender Grimasse", "tokens": ["Kommst", "du", "zu", "st\u00f6\u00b7ren", "mit", "er\u00b7schre\u00b7cken\u00b7der", "Gri\u00b7mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Fest des Lebens, als ob l\u00fcsterne Begehr,", "tokens": ["Das", "Fest", "des", "Le\u00b7bens", ",", "als", "ob", "l\u00fcs\u00b7ter\u00b7ne", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOKOM", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Leichtgl\u00e4ubge, dein Gebein im Grab nicht ruhen lasse,", "tokens": ["Leicht\u00b7gl\u00e4ub\u00b7ge", ",", "dein", "Ge\u00b7bein", "im", "Grab", "nicht", "ru\u00b7hen", "las\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum wilden Taumeltanz des Freudensabbats her?", "tokens": ["Zum", "wil\u00b7den", "Tau\u00b7mel\u00b7tanz", "des", "Freu\u00b7dens\u00b7ab\u00b7bats", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Beim Sang der Geigen, bei der Kerzen lichtem Prangen", "tokens": ["Beim", "Sang", "der", "Gei\u00b7gen", ",", "bei", "der", "Ker\u00b7zen", "lich\u00b7tem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoffst zu verscheuchen du der finstren Tr\u00e4ume Not?", "tokens": ["Hoffst", "zu", "ver\u00b7scheu\u00b7chen", "du", "der", "finst\u00b7ren", "Tr\u00e4u\u00b7me", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Willst du vom wilden Strom der Orgien erlangen,", "tokens": ["Willst", "du", "vom", "wil\u00b7den", "Strom", "der", "Or\u00b7gi\u00b7en", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er die H\u00f6lle k\u00fchlt, die dir im Herzen loht?", "tokens": ["Da\u00df", "er", "die", "H\u00f6l\u00b7le", "k\u00fchlt", ",", "die", "dir", "im", "Her\u00b7zen", "loht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Unausgesch\u00f6pfter Quell von Wahn und Seltsamkeiten,", "tokens": ["Un\u00b7aus\u00b7ge\u00b7sch\u00f6pf\u00b7ter", "Quell", "von", "Wahn", "und", "Selt\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem der Menschheit Schmerz seit alter Zeit geforscht,", "tokens": ["Nach", "dem", "der", "Menschheit", "Schmerz", "seit", "al\u00b7ter", "Zeit", "ge\u00b7forscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ich sehe durchs Gewand, gesch\u00fcrzt an deinen Seiten,", "tokens": ["Ich", "se\u00b7he", "durchs", "Ge\u00b7wand", ",", "ge\u00b7sch\u00fcrzt", "an", "dei\u00b7nen", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gierge Schlange, die dir das Gebein zermorscht.", "tokens": ["Die", "gier\u00b7ge", "Schlan\u00b7ge", ",", "die", "dir", "das", "Ge\u00b7bein", "zer\u00b7morscht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Zwar f\u00fcrchte wahrlich ich, da\u00df deine Reize scheitern,", "tokens": ["Zwar", "f\u00fcrch\u00b7te", "wahr\u00b7lich", "ich", ",", "da\u00df", "dei\u00b7ne", "Rei\u00b7ze", "schei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df kein Preis dich kr\u00f6nt, der w\u00fcrdig deiner M\u00fchn;", "tokens": ["Und", "da\u00df", "kein", "Preis", "dich", "kr\u00f6nt", ",", "der", "w\u00fcr\u00b7dig", "dei\u00b7ner", "M\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wen dieser Sterblichen wird solcher Spott erheitern?", "tokens": ["Wen", "die\u00b7ser", "Sterb\u00b7li\u00b7chen", "wird", "sol\u00b7cher", "Spott", "er\u00b7hei\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Graun kann Starke nur mit seiner Lust durchgl\u00fchn.", "tokens": ["Das", "Graun", "kann", "Star\u00b7ke", "nur", "mit", "sei\u00b7ner", "Lust", "durch\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der Augen H\u00f6hlung, drin des Grabes Schauer nachtet,", "tokens": ["Der", "Au\u00b7gen", "H\u00f6h\u00b7lung", ",", "drin", "des", "Gra\u00b7bes", "Schau\u00b7er", "nach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Enthaucht den Schwindel, und es wird kein T\u00e4nzer sein,", "tokens": ["Ent\u00b7haucht", "den", "Schwin\u00b7del", ",", "und", "es", "wird", "kein", "T\u00e4n\u00b7zer", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ohne Ekel und Beklemmung je betrachtet", "tokens": ["Der", "oh\u00b7ne", "E\u00b7kel", "und", "Be\u00b7klem\u00b7mung", "je", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das L\u00e4cheln, das uns grinst aus deiner Z\u00e4hne Reihn.", "tokens": ["Das", "L\u00e4\u00b7cheln", ",", "das", "uns", "grinst", "aus", "dei\u00b7ner", "Z\u00e4h\u00b7ne", "Reihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Doch welches Menschen Arm umfing nicht schon Skelette?", "tokens": ["Doch", "wel\u00b7ches", "Men\u00b7schen", "Arm", "um\u00b7fing", "nicht", "schon", "Ske\u00b7let\u00b7te", "?"], "token_inf