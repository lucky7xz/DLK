{"dta.poem.5455": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vermuthliche Beschaffenheit der Seelen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unwiedersprechlich ist es ja, da\u00df wir aus Seel' und", "tokens": ["Un\u00b7wie\u00b7der\u00b7sprech\u00b7lich", "ist", "es", "ja", ",", "da\u00df", "wir", "aus", "Seel'", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leib bestehen;", "tokens": ["Leib", "be\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der Leib aus immer neuen Theilen; die Seele zeuget stets", "tokens": ["Der", "Leib", "aus", "im\u00b7mer", "neu\u00b7en", "Thei\u00b7len", ";", "die", "See\u00b7le", "zeu\u00b7get", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Jdeen.", "tokens": ["Jdeen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wie nun der Leib von seinem Wesen durch Ausdunst im-", "tokens": ["Wie", "nun", "der", "Leib", "von", "sei\u00b7nem", "We\u00b7sen", "durch", "Aus\u00b7dunst", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mer was verliehrt,", "tokens": ["mer", "was", "ver\u00b7liehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Theilchen aber nicht vergehen, verwesen oder sich", "tokens": ["Die", "Theil\u00b7chen", "a\u00b7ber", "nicht", "ver\u00b7ge\u00b7hen", ",", "ver\u00b7we\u00b7sen", "o\u00b7der", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$,", "VVPP", "KON", "PRF"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "zerreiben,", "tokens": ["zer\u00b7rei\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und stets dem allgemeinen Stoff sich, so zu reden, einver-", "tokens": ["Und", "stets", "dem", "all\u00b7ge\u00b7mei\u00b7nen", "Stoff", "sich", ",", "so", "zu", "re\u00b7den", ",", "ein\u00b7ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PRF", "$,", "ADV", "PTKZU", "VVINF", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "leiben;", "tokens": ["lei\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "So scheinet auch, da\u00df unsre Seele best\u00e4ndig einen Abgang", "tokens": ["So", "schei\u00b7net", "auch", ",", "da\u00df", "uns\u00b7re", "See\u00b7le", "be\u00b7st\u00e4n\u00b7dig", "ei\u00b7nen", "Ab\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "sp\u00fchrt,", "tokens": ["sp\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Durch ihrer Kinder, der Gedancken, verfliegende Ver-", "tokens": ["Durch", "ih\u00b7rer", "Kin\u00b7der", ",", "der", "Ge\u00b7dan\u00b7cken", ",", "ver\u00b7flie\u00b7gen\u00b7de", "Ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ADJA", "TRUNC"], "meter": "++-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "gessenheit,", "tokens": ["ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Die doch kein v\u00f6lliger Beweis von v\u00f6lliger Verg\u00e4nglichkeit;", "tokens": ["Die", "doch", "kein", "v\u00f6l\u00b7li\u00b7ger", "Be\u00b7weis", "von", "v\u00f6l\u00b7li\u00b7ger", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.16": {"text": "Allein es d\u00fcncket mich, ob hier mit Recht nicht diese Frag\u2019", "tokens": ["Al\u00b7lein", "es", "d\u00fcn\u00b7cket", "mich", ",", "ob", "hier", "mit", "Recht", "nicht", "die\u00b7se", "Frag'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "NN", "PTKNEG", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "entstehe?", "tokens": ["ent\u00b7ste\u00b7he", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Ob nicht der allgemeine Stoff durch Kleinheit der Materie,", "tokens": ["Ob", "nicht", "der", "all\u00b7ge\u00b7mei\u00b7ne", "Stoff", "durch", "Klein\u00b7heit", "der", "Ma\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "Die immer feiner zu ihm fliesset, in sich sich nicht verbessere?", "tokens": ["Die", "im\u00b7mer", "fei\u00b7ner", "zu", "ihm", "flies\u00b7set", ",", "in", "sich", "sich", "nicht", "ver\u00b7bes\u00b7se\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,", "APPR", "PRF", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Jmgleichen, ob auf gleiche Weise in der Natur das Geistige", "tokens": ["Jm\u00b7glei\u00b7chen", ",", "ob", "auf", "glei\u00b7che", "Wei\u00b7se", "in", "der", "Na\u00b7tur", "das", "Geis\u00b7ti\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.21": {"text": "Sich, durch entstandene Gedancken (von welchen, da\u00df sie", "tokens": ["Sich", ",", "durch", "ent\u00b7stan\u00b7de\u00b7ne", "Ge\u00b7dan\u00b7cken", "(", "von", "wel\u00b7chen", ",", "da\u00df", "sie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "$,", "APPR", "ADJA", "NN", "$(", "APPR", "PWAT", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "nicht vergehen", "tokens": ["nicht", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Und wir sie zu erhalten f\u00e4hig, in Schriften wir ein Bey-", "tokens": ["Und", "wir", "sie", "zu", "er\u00b7hal\u00b7ten", "f\u00e4\u00b7hig", ",", "in", "Schrif\u00b7ten", "wir", "ein", "Bey"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "PTKZU", "VVINF", "ADJD", "$,", "APPR", "NN", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "spiel sehen)", "tokens": ["spiel", "se\u00b7hen", ")"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Auch, durch derselben steten Zuflu\u00df, wo nicht verbessre,", "tokens": ["Auch", ",", "durch", "der\u00b7sel\u00b7ben", "ste\u00b7ten", "Zu\u00b7flu\u00df", ",", "wo", "nicht", "ver\u00b7bess\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,", "PWAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "doch sich mehre?", "tokens": ["doch", "sich", "meh\u00b7re", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Bey unsrer Einfalt scheint es meistens, als ob es nicht un-", "tokens": ["Bey", "uns\u00b7rer", "Ein\u00b7falt", "scheint", "es", "meis\u00b7tens", ",", "als", "ob", "es", "nicht", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "PPER", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "m\u00f6glich w\u00e4re.", "tokens": ["m\u00f6g\u00b7lich", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}