{"textgrid.poem.60841": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An Vasari", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklicher, der du Italien sahst in der h\u00f6chsten Verkl\u00e4rung,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", ",", "der", "du", "I\u00b7ta\u00b7li\u00b7en", "sahst", "in", "der", "h\u00f6chs\u00b7ten", "Ver\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-----+--+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Ehe der pf\u00e4ffischen Zeit plumper Geschmack es entehrt,", "tokens": ["E\u00b7he", "der", "pf\u00e4f\u00b7fi\u00b7schen", "Zeit", "plum\u00b7per", "Ge\u00b7schmack", "es", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der du die Werke der Kunst vollst\u00e4ndig und Gl\u00e4nzend und neu sahst,", "tokens": ["Der", "du", "die", "Wer\u00b7ke", "der", "Kunst", "voll\u00b7st\u00e4n\u00b7dig", "und", "Gl\u00e4n\u00b7zend", "und", "neu", "sahst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "ADJD", "KON", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Deren die H\u00e4lfte zerst\u00f6rt nun, und die H\u00e4lfte zerstreut:", "tokens": ["De\u00b7ren", "die", "H\u00e4lf\u00b7te", "zer\u00b7st\u00f6rt", "nun", ",", "und", "die", "H\u00e4lf\u00b7te", "zer\u00b7streut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "ADV", "$,", "KON", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Selbst die gebliebenen hat nachhelfender Pfuscher Verkehrtheit,", "tokens": ["Selbst", "die", "ge\u00b7blie\u00b7be\u00b7nen", "hat", "nach\u00b7hel\u00b7fen\u00b7der", "Pfu\u00b7scher", "Ver\u00b7kehrt\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "APPR", "NN", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Tempel und Bilder zugleich, \u00fcber die Ma\u00dfen entstellt!", "tokens": ["Tem\u00b7pel", "und", "Bil\u00b7der", "zu\u00b7gleich", ",", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", "ent\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}