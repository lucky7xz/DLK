{"dta.poem.220": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Jm Lieben mu\u00df Maasse seyn.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Allzuviel ist ungesund.", "tokens": ["All\u00b7zu\u00b7viel", "ist", "un\u00b7ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Schertzen/", "tokens": ["In", "den", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In den Lachen", "tokens": ["In", "den", "La\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "In den s\u00fcssen Lippen hertzen", "tokens": ["In", "den", "s\u00fcs\u00b7sen", "Lip\u00b7pen", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in den betr\u00fcbten Sachen", "tokens": ["Und", "in", "den", "be\u00b7tr\u00fcb\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Mu\u00df ein Ziel und Maasse seyn:", "tokens": ["Mu\u00df", "ein", "Ziel", "und", "Maas\u00b7se", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn es trifft bey allen ein", "tokens": ["Denn", "es", "trifft", "bey", "al\u00b7len", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser vestgesetzte Grund/", "tokens": ["Die\u00b7ser", "vest\u00b7ge\u00b7setz\u00b7te", "Grund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Allzuviel ist ungesund.", "tokens": ["All\u00b7zu\u00b7viel", "ist", "un\u00b7ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Drum ist es gut", "tokens": ["Drum", "ist", "es", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wenn einer Maasse halten kan/", "tokens": ["Wenn", "ei\u00b7ner", "Maas\u00b7se", "hal\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und alles/ was er thut/", "tokens": ["Und", "al\u00b7les", "/", "was", "er", "thut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "F\u00e4ngt mit Bedachte an.", "tokens": ["F\u00e4ngt", "mit", "Be\u00b7dach\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Liebe schreibet ihren L\u00fcsten", "tokens": ["Die", "Lie\u00b7be", "schrei\u00b7bet", "ih\u00b7ren", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ebenfals Gesetze f\u00fcr.", "tokens": ["Auch", "e\u00b7ben\u00b7fals", "Ge\u00b7set\u00b7ze", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schliesset Regung und Gedancken/", "tokens": ["Sie", "schlies\u00b7set", "Re\u00b7gung", "und", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwar nicht in allzuenge Schrancken/", "tokens": ["Zwar", "nicht", "in", "all\u00b7zu\u00b7en\u00b7ge", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch hats auch Maa\u00df und Ziel mit ihr.", "tokens": ["Doch", "hats", "auch", "Maa\u00df", "und", "Ziel", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe schreibet ihren L\u00fcsten", "tokens": ["Die", "Lie\u00b7be", "schrei\u00b7bet", "ih\u00b7ren", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch ebenfals Gesetze f\u00fcr.", "tokens": ["Auch", "e\u00b7ben\u00b7fals", "Ge\u00b7set\u00b7ze", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}