{"textgrid.poem.40541": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warnend sprechen Muselmanen:", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warnend sprechen Muselmanen:", "tokens": ["War\u00b7nend", "spre\u00b7chen", "Mu\u00b7sel\u00b7ma\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Maler, malt kein Menschenbild,", "tokens": ["Ma\u00b7ler", ",", "malt", "kein", "Men\u00b7schen\u00b7bild", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da in ihm, eh' ihr's m\u00f6gt ahnen,", "tokens": ["Da", "in", "ihm", ",", "eh'", "ih\u00b7r's", "m\u00f6gt", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich Seel' und Leben quillt!", "tokens": ["Pl\u00f6tz\u00b7lich", "Seel'", "und", "Le\u00b7ben", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weh, als unberuf'ne V\u00e4ter", "tokens": ["Weh", ",", "als", "un\u00b7be\u00b7ruf'\u00b7ne", "V\u00e4\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagt einst das Gebild euch an;", "tokens": ["Klagt", "einst", "das", "Ge\u00b7bild", "euch", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00f6rdern gleich, als Misseth\u00e4ter,", "tokens": ["M\u00f6r\u00b7dern", "gleich", ",", "als", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht vor Allahs Thron ihr dann!", "tokens": ["Steht", "vor", "Al\u00b7lahs", "Thron", "ihr", "dann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Anders mag der Spruch auch klingen:", "tokens": ["An\u00b7ders", "mag", "der", "Spruch", "auch", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dichter, schaffet kein Gebild,", "tokens": ["Dich\u00b7ter", ",", "schaf\u00b7fet", "kein", "Ge\u00b7bild", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem ihr Seele nicht k\u00f6nnt bringen,", "tokens": ["Dem", "ihr", "See\u00b7le", "nicht", "k\u00f6nnt", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das nicht ganz von Leben quillt!", "tokens": ["Das", "nicht", "ganz", "von", "Le\u00b7ben", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weh, als unberuf'ne V\u00e4ter", "tokens": ["Weh", ",", "als", "un\u00b7be\u00b7ruf'\u00b7ne", "V\u00e4\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagt einst das Gebild euch an,", "tokens": ["Klagt", "einst", "das", "Ge\u00b7bild", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ihr steht als Uebelth\u00e4ter", "tokens": ["Und", "ihr", "steht", "als", "Ue\u00b7belt\u00b7h\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Thron der Muse dann!", "tokens": ["Vor", "dem", "Thron", "der", "Mu\u00b7se", "dann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum la\u00df nie die Ros' entschweben", "tokens": ["Drum", "la\u00df", "nie", "die", "Ros'", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Nichtseins stiller Gruft,", "tokens": ["Aus", "des", "Nich\u00b7tseins", "stil\u00b7ler", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannst du ihrem Kelch nicht geben", "tokens": ["Kannst", "du", "ih\u00b7rem", "Kelch", "nicht", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Seele: Gluth und Duft!", "tokens": ["Sei\u00b7ne", "See\u00b7le", ":", "Gluth", "und", "Duft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Soll sich Nachtigall aufschwingen,", "tokens": ["Soll", "sich", "Nach\u00b7ti\u00b7gall", "auf\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frag' erst: ob dein Hauch vermag", "tokens": ["Frag'", "erst", ":", "ob", "dein", "Hauch", "ver\u00b7mag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Kehle zu durchdringen", "tokens": ["Ih\u00b7re", "Keh\u00b7le", "zu", "durch\u00b7drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz mit Nachtigallenschlag?", "tokens": ["Ganz", "mit", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Banne zu der Himmel Wonne", "tokens": ["Ban\u00b7ne", "zu", "der", "Him\u00b7mel", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen neuen Stern uns nicht,", "tokens": ["Ei\u00b7nen", "neu\u00b7en", "Stern", "uns", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ihn nicht dein Herz als Sonne", "tokens": ["Kann", "ihn", "nicht", "dein", "Herz", "als", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllen ganz mit Sternenlicht!", "tokens": ["F\u00fcl\u00b7len", "ganz", "mit", "Ster\u00b7nen\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}