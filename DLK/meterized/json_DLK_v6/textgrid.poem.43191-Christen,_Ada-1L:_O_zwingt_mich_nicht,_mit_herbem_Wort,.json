{"textgrid.poem.43191": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: O zwingt mich nicht, mit herbem Wort,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O zwingt mich nicht, mit herbem Wort,", "tokens": ["O", "zwingt", "mich", "nicht", ",", "mit", "her\u00b7bem", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit hartem, euch zu nennen,", "tokens": ["Mit", "har\u00b7tem", ",", "euch", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn solche Worte fort und fort", "tokens": ["Denn", "sol\u00b7che", "Wor\u00b7te", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meinem Herzen brennen.", "tokens": ["Auf", "mei\u00b7nem", "Her\u00b7zen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat solch' Wort in dunkler Stund'", "tokens": ["Es", "hat", "solch'", "Wort", "in", "dunk\u00b7ler", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Kraft und Muth gebrochen,", "tokens": ["Mir", "Kraft", "und", "Muth", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als einst ein b\u00f6ser Menschenmund", "tokens": ["Als", "einst", "ein", "b\u00f6\u00b7ser", "Men\u00b7schen\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es z\u00fcrnend ausgesprochen.", "tokens": ["Es", "z\u00fcr\u00b7nend", "aus\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich ein herbes Wort euch sag'", "tokens": ["Wenn", "ich", "ein", "her\u00b7bes", "Wort", "euch", "sag'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ungez\u00e4hmtem Grimme,", "tokens": ["In", "un\u00b7ge\u00b7z\u00e4hm\u00b7tem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Trifft wie ein blut'ger Gei\u00dfelschlag", "tokens": ["Trifft", "wie", "ein", "blut'\u00b7ger", "Gei\u00b7\u00dfel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz die eigne Stimme.", "tokens": ["Mein", "Herz", "die", "eig\u00b7ne", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}