{"textgrid.poem.35051": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ade", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gehe fort, und dennoch geh ich nicht.", "tokens": ["Ich", "ge\u00b7he", "fort", ",", "und", "den\u00b7noch", "geh", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ade, mein Heim, und doch auch nicht ade!", "tokens": ["A\u00b7de", ",", "mein", "Heim", ",", "und", "doch", "auch", "nicht", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ich scheide zwar, doch leist ich nicht Verzicht,", "tokens": ["Ich", "schei\u00b7de", "zwar", ",", "doch", "leist", "ich", "nicht", "Ver\u00b7zicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich dich einst nach Jahren wiederseh.", "tokens": ["Da\u00df", "ich", "dich", "einst", "nach", "Jah\u00b7ren", "wie\u00b7der\u00b7seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob dich mein Fu\u00df f\u00fcr eine Zeit verl\u00e4\u00dft,", "tokens": ["Ob", "dich", "mein", "Fu\u00df", "f\u00fcr", "ei\u00b7ne", "Zeit", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du h\u00e4ltst doch meine ganze Seele fest.", "tokens": ["Du", "h\u00e4ltst", "doch", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ans Land, wo meine Wiege einst gestanden,", "tokens": ["Ans", "Land", ",", "wo", "mei\u00b7ne", "Wie\u00b7ge", "einst", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bleib ich gekettet mit geheimen Banden.", "tokens": ["Bleib", "ich", "ge\u00b7ket\u00b7tet", "mit", "ge\u00b7hei\u00b7men", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich gehe fort, und dennoch geh ich nicht.", "tokens": ["Ich", "ge\u00b7he", "fort", ",", "und", "den\u00b7noch", "geh", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ade, lieb M\u00fctterlein, und nicht ade!", "tokens": ["A\u00b7de", ",", "lieb", "M\u00fct\u00b7ter\u00b7lein", ",", "und", "nicht", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "NN", "$,", "KON", "PTKNEG", "ADV", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ob auch der Mund das Wort des Abschieds spricht,", "tokens": ["Ob", "auch", "der", "Mund", "das", "Wort", "des", "Ab\u00b7schieds", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz wei\u00df doch, da\u00df ich nicht von dir geh.", "tokens": ["Das", "Herz", "wei\u00df", "doch", ",", "da\u00df", "ich", "nicht", "von", "dir", "geh."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Treibt das Geschick mich in die Welt hinaus,", "tokens": ["Treibt", "das", "Ge\u00b7schick", "mich", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "APZR", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Es scheint nur so; ich bleib bei dir zu Haus.", "tokens": ["Es", "scheint", "nur", "so", ";", "ich", "bleib", "bei", "dir", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wohl mag der Gram mein Gehn ein Scheiden nennen,", "tokens": ["Wohl", "mag", "der", "Gram", "mein", "Gehn", "ein", "Schei\u00b7den", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Ferne kann nie Sohn und Mutter trennen.", "tokens": ["Die", "Fer\u00b7ne", "kann", "nie", "Sohn", "und", "Mut\u00b7ter", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich gehe fort, und dennoch geh ich nicht.", "tokens": ["Ich", "ge\u00b7he", "fort", ",", "und", "den\u00b7noch", "geh", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ade, ihr Lieben, und doch nicht ade!", "tokens": ["A\u00b7de", ",", "ihr", "Lie\u00b7ben", ",", "und", "doch", "nicht", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Tr\u00e4gt mich der Tod jetzt auf zum ewgen Licht,", "tokens": ["Tr\u00e4gt", "mich", "der", "Tod", "jetzt", "auf", "zum", "ew\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wi\u00dft, da\u00df unsichtbar stets ich bei euch steh.", "tokens": ["Wi\u00dft", ",", "da\u00df", "un\u00b7sicht\u00b7bar", "stets", "ich", "bei", "euch", "steh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Von Gott zu eurem Schutz herabgesandt,", "tokens": ["Von", "Gott", "zu", "eu\u00b7rem", "Schutz", "her\u00b7ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Halt \u00fcber euch ich meine treue Hand.", "tokens": ["Halt", "\u00fc\u00b7ber", "euch", "ich", "mei\u00b7ne", "treu\u00b7e", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es stirbt der K\u00f6rper nur, und nach dem Tode", "tokens": ["Es", "stirbt", "der", "K\u00f6r\u00b7per", "nur", ",", "und", "nach", "dem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird mein Gebet f\u00fcr euch ein Himmelsbote.", "tokens": ["Wird", "mein", "Ge\u00b7bet", "f\u00fcr", "euch", "ein", "Him\u00b7mels\u00b7bo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}