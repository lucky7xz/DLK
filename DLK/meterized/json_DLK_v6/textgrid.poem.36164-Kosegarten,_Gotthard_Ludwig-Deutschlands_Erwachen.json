{"textgrid.poem.36164": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Deutschlands Erwachen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwachst du endlich aus dem Todesschlummer,", "tokens": ["Er\u00b7wachst", "du", "end\u00b7lich", "aus", "dem", "To\u00b7des\u00b7schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heimath des Hermann, Marbod, Wittekind?", "tokens": ["Hei\u00b7math", "des", "Her\u00b7mann", ",", "Mar\u00b7bod", ",", "Wit\u00b7te\u00b7kind", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NE", "$,", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wird endlich Grimm das Gr\u00e4men, Zorn der Kummer?", "tokens": ["Wird", "end\u00b7lich", "Grimm", "das", "Gr\u00e4\u00b7men", ",", "Zorn", "der", "Kum\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verr\u00e4th die Zunge, wie die Brust gesinnt?", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "die", "Zun\u00b7ge", ",", "wie", "die", "Brust", "ge\u00b7sinnt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ja, du erwachst! du sprengst die Kette,", "tokens": ["Ja", ",", "du", "er\u00b7wachst", "!", "du", "sprengst", "die", "Ket\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du raffst dich auf voll Scham und Schmerz;", "tokens": ["Du", "raffst", "dich", "auf", "voll", "Scham", "und", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Kinder Angstruf: Rette! Rette!", "tokens": ["Der", "Kin\u00b7der", "Angs\u00b7truf", ":", "Ret\u00b7te", "!", "Ret\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zerrei\u00dft dein Ohr und schwellt dein Herz.", "tokens": ["Zer\u00b7rei\u00dft", "dein", "Ohr", "und", "schwellt", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}