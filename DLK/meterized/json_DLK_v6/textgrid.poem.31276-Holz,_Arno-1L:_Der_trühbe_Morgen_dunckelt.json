{"textgrid.poem.31276": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der tr\u00fchbe Morgen dunckelt/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der tr\u00fchbe Morgen dunckelt/", "tokens": ["Der", "tr\u00fch\u00b7be", "Mor\u00b7gen", "dun\u00b7ckelt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der Dag bricht kaum schon f\u00fcr/", "tokens": ["der", "Dag", "bricht", "kaum", "schon", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mein L\u00e4mpgen spr\u00fcht und funckelt/", "tokens": ["mein", "L\u00e4mp\u00b7gen", "spr\u00fcht", "und", "fun\u00b7ckelt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ich f\u00fchls/ noch horcht wer vor der Dh\u00fcr.", "tokens": ["ich", "f\u00fchls", "/", "noch", "horcht", "wer", "vor", "der", "Dh\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADV", "VVFIN", "PWS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch ist er nicht verwichen/", "tokens": ["Noch", "ist", "er", "nicht", "ver\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ich schlieff/ er hat gewacht/", "tokens": ["ich", "schlieff", "/", "er", "hat", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "mit Augen lengst verblichen", "tokens": ["mit", "Au\u00b7gen", "lengst", "ver\u00b7bli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "stund er die gantze Nacht.", "tokens": ["stund", "er", "die", "gant\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Sein Seiger saust/ die Stunden rinnen/", "tokens": ["Sein", "Sei\u00b7ger", "saust", "/", "die", "Stun\u00b7den", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "sey wer du seyst/ du mu\u00dft von hinnen!", "tokens": ["sey", "wer", "du", "seyst", "/", "du", "mu\u00dft", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWS", "PPER", "VAFIN", "$(", "PPER", "VMFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich soff und hab gefr\u00f6ssen/", "tokens": ["Ich", "soff", "und", "hab", "ge\u00b7fr\u00f6s\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gehurt mit nichts al\u00df Pakk/", "tokens": ["ge\u00b7hurt", "mit", "nichts", "al\u00df", "Pakk", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "KOKOM", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Truddeln und mit Tr\u00f6ssen", "tokens": ["mit", "Trud\u00b7deln", "und", "mit", "Tr\u00f6s\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "behing ich dihsen Maden-Sakk.", "tokens": ["be\u00b7hing", "ich", "dih\u00b7sen", "Ma\u00b7den\u00b7Sakk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wein/ Weibrichins und Karten/", "tokens": ["Wein", "/", "Wei\u00b7bri\u00b7chins", "und", "Kar\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "$("], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "nichts war mir ji zu bundt/", "tokens": ["nichts", "war", "mir", "ji", "zu", "bundt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "mein Hieber hieb sich Scharten", "tokens": ["mein", "Hie\u00b7ber", "hieb", "sich", "Schar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "in manchen Lumpen-Hund.", "tokens": ["in", "man\u00b7chen", "Lum\u00b7pen\u00b7Hund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Noch Keinen hat man so bedroffen/", "tokens": ["Noch", "Kei\u00b7nen", "hat", "man", "so", "be\u00b7drof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "allein \u2013 wie ist da\u00df abgeloffen?", "tokens": ["al\u00b7lein", "\u2013", "wie", "ist", "da\u00df", "ab\u00b7ge\u00b7lof\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "VAFIN", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlohwei\u00df sind meine Brauen/", "tokens": ["Schloh\u00b7wei\u00df", "sind", "mei\u00b7ne", "Brau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mein vor so froher Mund", "tokens": ["mein", "vor", "so", "fro\u00b7her", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ward for mir s\u00e4lbst zum Grauen", "tokens": ["ward", "for", "mir", "s\u00e4lbst", "zum", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein zubed\u00e4kkter Abgrunds-Schlund.", "tokens": ["ein", "zu\u00b7be\u00b7d\u00e4kk\u00b7ter", "Ab\u00b7grunds\u00b7Sch\u00b7lund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein R\u00fckken h\u00e4ngkt gebogen/", "tokens": ["Mein", "R\u00fck\u00b7ken", "h\u00e4ngkt", "ge\u00b7bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ich krige kaum mehr Lufft/", "tokens": ["ich", "kri\u00b7ge", "kaum", "mehr", "Lufft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "mein Mercks f\u00e4ult au\u00dfgesogen/", "tokens": ["mein", "Mercks", "f\u00e4ult", "au\u00df\u00b7ge\u00b7so\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "mein Fleisch r\u00e4ucht nach der Grufft.", "tokens": ["mein", "Fleisch", "r\u00e4ucht", "nach", "der", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich k\u00e4nne w\u00fcrcklich nicht mehr wihder", "tokens": ["Ich", "k\u00e4n\u00b7ne", "w\u00fcrck\u00b7lich", "nicht", "mehr", "wih\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "mein f\u00fcrmahls stoltzes Pfau-Gefihder!", "tokens": ["mein", "f\u00fcr\u00b7mahls", "stolt\u00b7zes", "Pfau\u00b7Ge\u00b7fih\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00fcmbringeln mir mein Stroh/", "tokens": ["\u00fcm\u00b7brin\u00b7geln", "mir", "mein", "Stroh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "kaum ist so ergangen", "tokens": ["kaum", "ist", "so", "er\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "s\u00e4lbst jenem armen ", 