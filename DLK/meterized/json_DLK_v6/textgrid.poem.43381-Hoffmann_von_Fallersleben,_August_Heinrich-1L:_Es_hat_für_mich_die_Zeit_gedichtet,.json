{"textgrid.poem.43381": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es hat f\u00fcr mich die Zeit gedichtet,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat f\u00fcr mich die Zeit gedichtet,", "tokens": ["Es", "hat", "f\u00fcr", "mich", "die", "Zeit", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr aber habt nur ", "tokens": ["Ihr", "a\u00b7ber", "habt", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ihr habt mit euerer Gemeinheit", "tokens": ["Ihr", "habt", "mit", "eu\u00b7e\u00b7rer", "Ge\u00b7mein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Besudelt meines Strebens Reinheit,", "tokens": ["Be\u00b7su\u00b7delt", "mei\u00b7nes", "Stre\u00b7bens", "Rein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich als Vogelscheuch' ins Feld", "tokens": ["Und", "mich", "als", "Vo\u00b7gel\u00b7scheuch'", "ins", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor's Angesicht der Welt gestellt.", "tokens": ["Vor's", "An\u00b7ge\u00b7sicht", "der", "Welt", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Zeit wird euren Frevel r\u00e4chen,", "tokens": ["Die", "Zeit", "wird", "eu\u00b7ren", "Fre\u00b7vel", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird euch bald ein Urtheil sprechen;", "tokens": ["Sie", "wird", "euch", "bald", "ein", "Ur\u00b7theil", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man wird von euch dereinst nicht sagen", "tokens": ["Man", "wird", "von", "euch", "de\u00b7reinst", "nicht", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's jetzt noch hei\u00dft in unsern Tagen:", "tokens": ["Wie's", "jetzt", "noch", "hei\u00dft", "in", "un\u00b7sern", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Je gr\u00f6\u00dfer Lump je gr\u00f6\u00dfer Gunst,", "tokens": ["Je", "gr\u00f6\u00b7\u00dfer", "Lump", "je", "gr\u00f6\u00b7\u00dfer", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Schweigen ist die beste Kunst.", "tokens": ["Und", "Schwei\u00b7gen", "ist", "die", "bes\u00b7te", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}