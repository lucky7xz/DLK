{"textgrid.poem.62396": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Spaziergang", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Leinpfad geht sie gern", "tokens": ["Auf", "dem", "Lein\u00b7pfad", "geht", "sie", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ngs dem holden Rheine,", "tokens": ["L\u00e4ngs", "dem", "hol\u00b7den", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Angeblickt vom Abendstern", "tokens": ["An\u00b7ge\u00b7blickt", "vom", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Einsam und alleine.", "tokens": ["Ein\u00b7sam", "und", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie der Blick sich hebt und senkt,", "tokens": ["Wie", "der", "Blick", "sich", "hebt", "und", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die W\u00fcnsche schweifen,", "tokens": ["Wie", "die", "W\u00fcn\u00b7sche", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was sie dichtet, was sie denkt,", "tokens": ["Was", "sie", "dich\u00b7tet", ",", "was", "sie", "denkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann's ein Mensch begreifen?", "tokens": ["Kann's", "ein", "Mensch", "be\u00b7grei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auf dem Leinpfad geht sie gern", "tokens": ["Auf", "dem", "Lein\u00b7pfad", "geht", "sie", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ngs dem holden Rheine,", "tokens": ["L\u00e4ngs", "dem", "hol\u00b7den", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denket r\u00fcckw\u00e4rts froh und fern", "tokens": ["Den\u00b7ket", "r\u00fcck\u00b7w\u00e4rts", "froh", "und", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Thal der Leine.", "tokens": ["In", "das", "Thal", "der", "Lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Morgenglanz, o Jugendlust,", "tokens": ["Mor\u00b7gen\u00b7glanz", ",", "o", "Ju\u00b7gend\u00b7lust", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterne, Blumen, B\u00e4ume,", "tokens": ["Ster\u00b7ne", ",", "Blu\u00b7men", ",", "B\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erster Hauch der jungen Brust,", "tokens": ["Ers\u00b7ter", "Hauch", "der", "jun\u00b7gen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All' ihr fr\u00fchen Tr\u00e4ume!", "tokens": ["All'", "ihr", "fr\u00fc\u00b7hen", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wollet stets das liebe Kind", "tokens": ["Wol\u00b7let", "stets", "das", "lie\u00b7be", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Pfad geleiten,", "tokens": ["Auf", "dem", "Pfad", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Spiel' um sie, du Abendwind,", "tokens": ["Spiel'", "um", "sie", ",", "du", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie um Harfensaiten!", "tokens": ["Wie", "um", "Har\u00b7fen\u00b7sai\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Erste Lieb' und erster Gru\u00df", "tokens": ["Ers\u00b7te", "Lieb'", "und", "ers\u00b7ter", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Thal der Leine,", "tokens": ["Aus", "dem", "Thal", "der", "Lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weht sie an wie Geisterku\u00df", "tokens": ["Weht", "sie", "an", "wie", "Geis\u00b7ter\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KOKOM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Pfad am Rheine!", "tokens": ["Auf", "dem", "Pfad", "am", "Rhei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}