{"dta.poem.2415": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als ich zu L\u00fcbekk im 1643sten Jahre/  \n den 12. Aprill zu Schiffe/ \u00fcm nach K\u00f6-  \n nigsberg auff die  U niversitet zu reisen/  \n gieng.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Neptunus sol zwar seyn ein Gott der stoltzen Wellen", "tokens": ["Nep\u00b7tu\u00b7nus", "sol", "zwar", "seyn", "ein", "Gott", "der", "stolt\u00b7zen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "PPOSAT", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Eolus der Wind\u2019 im fall dasselb\u2019 ist war", "tokens": ["Und", "E\u00b7o\u00b7lus", "der", "Wind'", "im", "fall", "das\u00b7selb'", "ist", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "NN", "APPRART", "NN", "PDS", "VAFIN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was uns zusagen pflegt die kluge Tichterschar;", "tokens": ["Was", "uns", "zu\u00b7sa\u00b7gen", "pflegt", "die", "klu\u00b7ge", "Tich\u00b7ter\u00b7schar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gleichwol k\u00f6nnen sie noch niemals nicht erhellen", "tokens": ["Und", "gleich\u00b7wol", "k\u00f6n\u00b7nen", "sie", "noch", "nie\u00b7mals", "nicht", "er\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sie bes\u00e4nfftigen der Winde starkes Bellen.", "tokens": ["Da\u00df", "sie", "be\u00b7s\u00e4nff\u00b7ti\u00b7gen", "der", "Win\u00b7de", "star\u00b7kes", "Bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier gilt Neptunus nichts auch Eolus kein Haar!", "tokens": ["Hier", "gilt", "Nep\u00b7tu\u00b7nus", "nichts", "auch", "E\u00b7o\u00b7lus", "kein", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PIS", "ADV", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn wir armselige gerahten in Gefahr/", "tokens": ["Wenn", "wir", "arm\u00b7se\u00b7li\u00b7ge", "ge\u00b7rah\u00b7ten", "in", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "So mu\u00df uns Gott allein in solchen Ungl\u00fcksf\u00e4llen", "tokens": ["So", "mu\u00df", "uns", "Gott", "al\u00b7lein", "in", "sol\u00b7chen", "Un\u00b7gl\u00fcks\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erretten aus der Noht/ wenn der nicht bey uns ist/", "tokens": ["Er\u00b7ret\u00b7ten", "aus", "der", "Noht", "/", "wenn", "der", "nicht", "bey", "uns", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KOUS", "ART", "PTKNEG", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So hilfft kein Ankerseil des Schiffers Kunst und List", "tokens": ["So", "hilfft", "kein", "An\u00b7ker\u00b7seil", "des", "Schif\u00b7fers", "Kunst", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie gro\u00df sie auch mag seyn/ kan nichts allhier verfangen.", "tokens": ["Wie", "gro\u00df", "sie", "auch", "mag", "seyn", "/", "kan", "nichts", "all\u00b7hier", "ver\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VMFIN", "VAINF", "$(", "VMFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dr\u00fcm hilff du treuer GOtt/ denn du bist HErr der", "tokens": ["Dr\u00fcm", "hilff", "du", "treu\u00b7er", "Gott", "/", "denn", "du", "bist", "Herr", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$(", "KON", "PPER", "VAFIN", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "See/", "tokens": ["See", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Da\u00df ich frisch und gesund ohn alles Ach und Weh\u2019", "tokens": ["Da\u00df", "ich", "frisch", "und", "ge\u00b7sund", "ohn", "al\u00b7les", "Ach", "und", "Weh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "An den gew\u00fcnschten Port des Pregels an- mag-langen.", "tokens": ["An", "den", "ge\u00b7w\u00fcnschten", "Port", "des", "Pre\u00b7gels", "an", "mag\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "TRUNC", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}