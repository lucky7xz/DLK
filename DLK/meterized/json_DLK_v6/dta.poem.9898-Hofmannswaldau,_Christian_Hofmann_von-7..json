{"dta.poem.9898": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ohn bewegung kom\u0303 ich fort/ wei\u00df was da und dort geschehen/", "tokens": ["Ohn", "be\u00b7we\u00b7gung", "kom\u0303", "ich", "fort", "/", "wei\u00df", "was", "da", "und", "dort", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "PTKVZ", "$(", "VVFIN", "PWS", "ADV", "KON", "ADV", "VVPP", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bin ohn augen und kan doch sch\u00e4rffer als kein luchs nicht sehe\u0303/", "tokens": ["Bin", "ohn", "au\u00b7gen", "und", "kan", "doch", "sch\u00e4rf\u00b7fer", "als", "kein", "luchs", "nicht", "seh\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "VMFIN", "ADV", "ADJD", "KOKOM", "PIAT", "NN", "PTKNEG", "ADV", "$("], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Habe meine freye willk\u00fchr/ und wehl offt f\u00fcr freude leid/", "tokens": ["Ha\u00b7be", "mei\u00b7ne", "frey\u00b7e", "will\u00b7k\u00fchr", "/", "und", "wehl", "offt", "f\u00fcr", "freu\u00b7de", "leid", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "KON", "NN", "ADV", "APPR", "NN", "ADJD", "$("], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Bin von anfang bey den menschen bi\u00df zu ihrer sterbens-zeit.", "tokens": ["Bin", "von", "an\u