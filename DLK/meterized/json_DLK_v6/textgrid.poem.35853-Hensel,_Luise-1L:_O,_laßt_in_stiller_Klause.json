{"textgrid.poem.35853": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, la\u00dft in stiller Klause", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, la\u00dft in stiller Klause", "tokens": ["O", ",", "la\u00dft", "in", "stil\u00b7ler", "Klau\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Auge einsam weinen;", "tokens": ["Mein", "Au\u00b7ge", "ein\u00b7sam", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich will nicht im Gebrause", "tokens": ["Ich", "will", "nicht", "im", "Ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Welt fortan erscheinen.", "tokens": ["Der", "Welt", "for\u00b7tan", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herz, ihr Lieben, sehnet", "tokens": ["Mein", "Herz", ",", "ihr", "Lie\u00b7ben", ",", "seh\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich nicht nach ihren Freuden;", "tokens": ["Sich", "nicht", "nach", "ih\u00b7ren", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein d\u00fcstres Auge thr\u00e4net", "tokens": ["Mein", "d\u00fcst\u00b7res", "Au\u00b7ge", "thr\u00e4\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch nicht ob ihrer Leiden.", "tokens": ["Auch", "nicht", "ob", "ih\u00b7rer", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie bot mir ihre Freuden \u2013", "tokens": ["Sie", "bot", "mir", "ih\u00b7re", "Freu\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe sie verlassen.", "tokens": ["Ich", "ha\u00b7be", "sie", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl kenn' ich ihre Leiden", "tokens": ["Wohl", "kenn'", "ich", "ih\u00b7re", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mu\u00df sie ewig hassen.", "tokens": ["Und", "mu\u00df", "sie", "e\u00b7wig", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df nun in stiller Zelle", "tokens": ["La\u00df", "nun", "in", "stil\u00b7ler", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herz zu Dem sich heben,", "tokens": ["Mein", "Herz", "zu", "Dem", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der einzig ist die Quelle", "tokens": ["Der", "ein\u00b7zig", "ist", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Liebe, Licht und Leben. \u2013", "tokens": ["Von", "Lie\u00b7be", ",", "Licht", "und", "Le\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er l\u00e4\u00dft im stillen Grabe", "tokens": ["Er", "l\u00e4\u00dft", "im", "stil\u00b7len", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl bald mich Ruhe finden.", "tokens": ["Wohl", "bald", "mich", "Ru\u00b7he", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich gelitten habe,", "tokens": ["Was", "ich", "ge\u00b7lit\u00b7ten", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kann Er allein ergr\u00fcnden. \u2013", "tokens": ["Kann", "Er", "al\u00b7lein", "er\u00b7gr\u00fcn\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr! la\u00df in ew'gem Frieden", "tokens": ["Herr", "!", "la\u00df", "in", "ew'\u00b7gem", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich dann in Dir erfreuen.", "tokens": ["Mich", "dann", "in", "Dir", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich gefehlt hienieden,", "tokens": ["Was", "ich", "ge\u00b7fehlt", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr! Du wirst es verzeihen.", "tokens": ["Herr", "!", "Du", "wirst", "es", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}