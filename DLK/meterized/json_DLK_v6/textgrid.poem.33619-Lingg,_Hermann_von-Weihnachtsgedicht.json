{"textgrid.poem.33619": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsgedicht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr euch, o Kinder, bl\u00fcht das Fest der Feste,", "tokens": ["F\u00fcr", "euch", ",", "o", "Kin\u00b7der", ",", "bl\u00fcht", "das", "Fest", "der", "Fes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was bringt's wohl diesmal? Welch ein Meer von Licht?", "tokens": ["Was", "bringt's", "wohl", "dies\u00b7mal", "?", "Welch", "ein", "Meer", "von", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$.", "PIAT", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6nnt ihr's erwarten? Wi\u00dft, das Allerbeste,", "tokens": ["K\u00f6nnt", "ih\u00b7r's", "er\u00b7war\u00b7ten", "?", "Wi\u00dft", ",", "das", "Al\u00b7ler\u00b7bes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$.", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Das habt ihr schon. Das ist's: ihr wi\u00dft's noch nicht.", "tokens": ["Das", "habt", "ihr", "schon", ".", "Das", "ist's", ":", "ihr", "wi\u00dft's", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$.", "PDS", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was wir zum Spiel, was wir zum Ernst euch geben,", "tokens": ["Was", "wir", "zum", "Spiel", ",", "was", "wir", "zum", "Ernst", "euch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als reine Freude gebt ihr's uns zur\u00fcck.", "tokens": ["Als", "rei\u00b7ne", "Freu\u00b7de", "gebt", "ih\u00b7r's", "uns", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das ist das Beste, da\u00df es eurem Leben", "tokens": ["Das", "ist", "das", "Bes\u00b7te", ",", "da\u00df", "es", "eu\u00b7rem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch Wahrheit ist und ungetr\u00fcbtes Gl\u00fcck.", "tokens": ["Noch", "Wahr\u00b7heit", "ist", "und", "un\u00b7ge\u00b7tr\u00fcb\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch goldne Fr\u00fcchte tr\u00e4gt an seinen Zweigen", "tokens": ["Noch", "gold\u00b7ne", "Fr\u00fcch\u00b7te", "tr\u00e4gt", "an", "sei\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr euch der Tannbaum, der im Wintergraun", "tokens": ["F\u00fcr", "euch", "der", "Tann\u00b7baum", ",", "der", "im", "Win\u00b7ter\u00b7graun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und einsam steht im Wald mit ernstem Schweigen,", "tokens": ["Und", "ein\u00b7sam", "steht", "im", "Wald", "mit", "erns\u00b7tem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf den die goldnen Sterne niederschaun.", "tokens": ["Auf", "den", "die", "gold\u00b7nen", "Ster\u00b7ne", "nie\u00b7der\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein ganzes Jahr mit vielen, vielen Tagen", "tokens": ["Ein", "gan\u00b7zes", "Jahr", "mit", "vie\u00b7len", ",", "vie\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergl\u00e4nzt an dieses Tages Widerschein.", "tokens": ["Er\u00b7gl\u00e4nzt", "an", "die\u00b7ses", "Ta\u00b7ges", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6g' jeder Ernst euch goldne Fr\u00fcchte tragen", "tokens": ["M\u00f6g'", "je\u00b7der", "Ernst", "euch", "gold\u00b7ne", "Fr\u00fcch\u00b7te", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes Spiel euch lehren, froh zu sein.", "tokens": ["Und", "je\u00b7des", "Spiel", "euch", "leh\u00b7ren", ",", "froh", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVINF", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}