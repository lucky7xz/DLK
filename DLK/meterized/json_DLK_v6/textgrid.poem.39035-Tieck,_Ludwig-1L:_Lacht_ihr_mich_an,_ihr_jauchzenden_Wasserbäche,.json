{"textgrid.poem.39035": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lacht ihr mich an, ihr jauchzenden Wasserb\u00e4che,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lacht ihr mich an, ihr jauchzenden Wasserb\u00e4che,", "tokens": ["Lacht", "ihr", "mich", "an", ",", "ihr", "jauch\u00b7zen\u00b7den", "Was\u00b7ser\u00b7b\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie ihr klingend zwischen Oelbaum und Blumen niederjubelt?", "tokens": ["Wie", "ihr", "klin\u00b7gend", "zwi\u00b7schen", "O\u00b7el\u00b7baum", "und", "Blu\u00b7men", "nie\u00b7der\u00b7ju\u00b7belt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Springt und sprudelt, ihr Ueberm\u00fcthigen,", "tokens": ["Springt", "und", "spru\u00b7delt", ",", "ihr", "Ue\u00b7ber\u00b7m\u00fct\u00b7hi\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-++-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der duftende Wald hallt eure T\u00f6ne nach,", "tokens": ["Der", "duf\u00b7ten\u00b7de", "Wald", "hallt", "eu\u00b7re", "T\u00f6\u00b7ne", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und Baum und Fels, und Himmel und Strom", "tokens": ["Und", "Baum", "und", "Fels", ",", "und", "Him\u00b7mel", "und", "Strom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ein Freudengesang.", "tokens": ["Ein", "Freu\u00b7den\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Gegr\u00fc\u00dft sei mir, du du M\u00e4cenas t\u00f6nendes Haus:", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "sei", "mir", ",", "du", "du", "M\u00e4\u00b7ce\u00b7nas", "t\u00f6\u00b7nen\u00b7des", "Haus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "PPER", "PPER", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Grotte Neptuns, voll Wellenmusik und Regenbogen:", "tokens": ["Grot\u00b7te", "Nep\u00b7tuns", ",", "voll", "Wel\u00b7len\u00b7mu\u00b7sik", "und", "Re\u00b7gen\u00b7bo\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Wie ich hier in Blumen wandle, ruhend liege,", "tokens": ["Wie", "ich", "hier", "in", "Blu\u00b7men", "wand\u00b7le", ",", "ru\u00b7hend", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Mich entz\u00fccke, und wieder Welt und Natur vergesse", "tokens": ["Mich", "ent\u00b7z\u00fc\u00b7cke", ",", "und", "wie\u00b7der", "Welt", "und", "Na\u00b7tur", "ver\u00b7ges\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Im s\u00fcssesten Traum \u2013", "tokens": ["Im", "s\u00fcs\u00b7ses\u00b7ten", "Traum", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Saht ihr schon je, ihr klingenden Gestade", "tokens": ["Saht", "ihr", "schon", "je", ",", "ihr", "klin\u00b7gen\u00b7den", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Einen so gl\u00fccklichen Wandersmann?", "tokens": ["Ei\u00b7nen", "so", "gl\u00fcck\u00b7li\u00b7chen", "Wan\u00b7ders\u00b7mann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Doch schon winkt mir", "tokens": ["Doch", "schon", "winkt", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Roma's erhabene Kuppel", "tokens": ["Ro\u00b7ma's", "er\u00b7ha\u00b7be\u00b7ne", "Kup\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Zur\u00fcckzukehren,", "tokens": ["Zu\u00b7r\u00fcck\u00b7zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und bald, in wenigen Tagen,", "tokens": ["Und", "bald", ",", "in", "we\u00b7ni\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schon steht der Abschied an der Th\u00fcr,", "tokens": ["Schon", "steht", "der", "Ab\u00b7schied", "an", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entflieh ich diesem Himmel.", "tokens": ["Ent\u00b7flieh", "ich", "die\u00b7sem", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}