{"dta.poem.3311": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Tobacksraucher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich stets die trunknen Reben,", "tokens": ["Soll", "ich", "stets", "die", "trunk\u00b7nen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich nur den Gott erheben,", "tokens": ["Soll", "ich", "nur", "den", "Gott", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der aus holden Augen blitzt?", "tokens": ["Der", "aus", "hol\u00b7den", "Au\u00b7gen", "blitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ich nie zu deinem Preise,", "tokens": ["Werd", "ich", "nie", "zu", "dei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pflanze, meine Lust! erhitzt,", "tokens": ["Pflan\u00b7ze", ",", "mei\u00b7ne", "Lust", "!", "er\u00b7hitzt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unterde\u00df der Thor und Weise", "tokens": ["Un\u00b7ter\u00b7de\u00df", "der", "Thor", "und", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Beym verblasnen Rauche sitzt?", "tokens": ["Beym", "ver\u00b7blas\u00b7nen", "Rau\u00b7che", "sitzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie viele g\u00fcldne Stunden", "tokens": ["O", "wie", "vie\u00b7le", "g\u00fcld\u00b7ne", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind mir unbereut verschwunden,", "tokens": ["Sind", "mir", "un\u00b7be\u00b7reut", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey geliebter Bl\u00e4tter Glut!", "tokens": ["Bey", "ge\u00b7lieb\u00b7ter", "Bl\u00e4t\u00b7ter", "Glut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da emp\u00f6rt mein rascher Wille", "tokens": ["Da", "em\u00b7p\u00f6rt", "mein", "ra\u00b7scher", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich f\u00fcr kein verderblich Gut:", "tokens": ["Sich", "f\u00fcr", "kein", "ver\u00b7derb\u00b7lich", "Gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich genie\u00dfe s\u00fcsser Stille;", "tokens": ["Ich", "ge\u00b7nie\u00b7\u00dfe", "s\u00fcs\u00b7ser", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine ganze Seele ruht.", "tokens": ["Mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weg mit l\u00e4rmendem Gepr\u00e4nge!", "tokens": ["Weg", "mit", "l\u00e4r\u00b7men\u00b7dem", "Ge\u00b7pr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich mich durch Narren dr\u00e4nge,", "tokens": ["Wo", "ich", "mich", "durch", "Nar\u00b7ren", "dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4hn\u2019 ich bey dem besten Wein.", "tokens": ["G\u00e4hn'", "ich", "bey", "dem", "bes\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chle, Venus! unter Thr\u00e4nen;", "tokens": ["L\u00e4ch\u00b7le", ",", "Ve\u00b7nus", "!", "un\u00b7ter", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sey die Mutter s\u00fcsser Pein!", "tokens": ["Sey", "die", "Mut\u00b7ter", "s\u00fcs\u00b7ser", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber zeuch mit deinen Schw\u00e4nen,", "tokens": ["A\u00b7ber", "zeuch", "mit", "dei\u00b7nen", "Schw\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeuch bey mir nicht sieghaft ein.", "tokens": ["Zeuch", "bey", "mir", "nicht", "sieg\u00b7haft", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich beneide keine Krone,", "tokens": ["Ich", "be\u00b7nei\u00b7de", "kei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann aus wei\u00dfgebranntem Thone", "tokens": ["Wann", "aus", "wei\u00df\u00b7ge\u00b7brann\u00b7tem", "Tho\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Manch balsamisch W\u00f6lkchen dringt;", "tokens": ["Manch", "bal\u00b7sa\u00b7misch", "W\u00f6lk\u00b7chen", "dringt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und in meiner Muse H\u00e4nden", "tokens": ["Und", "in", "mei\u00b7ner", "Mu\u00b7se", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhrer Leyer Scherz erklingt;", "tokens": ["Ih\u00b7rer", "Le\u00b7yer", "Scherz", "er\u00b7klingt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder h\u00f6hern Gegenst\u00e4nden", "tokens": ["O\u00b7der", "h\u00f6\u00b7hern", "Ge\u00b7gen\u00b7st\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich mein Geist entgegen schwingt.", "tokens": ["Sich", "mein", "Geist", "ent\u00b7ge\u00b7gen", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die gefl\u00fcgelten Gedanken", "tokens": ["Die", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehn des Wahnes enge Schranken:", "tokens": ["Fliehn", "des", "Wah\u00b7nes", "en\u00b7ge", "Schran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Weise scheint mir gro\u00df.", "tokens": ["Nur", "der", "Wei\u00b7se", "scheint", "mir", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur des Gl\u00fcckes falsches Lachen", "tokens": ["Nur", "des", "Gl\u00fc\u00b7ckes", "fal\u00b7sches", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein oft entweihter Schoos,", "tokens": ["Und", "sein", "oft", "ent\u00b7weih\u00b7ter", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reichthum, Hoheit, (schlechte Sachen!)", "tokens": ["Reicht\u00b7hum", ",", "Ho\u00b7heit", ",", "(", "schlech\u00b7te", "Sa\u00b7chen", "!", ")"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind betrogner Thorheit Loos.", "tokens": ["Sind", "be\u00b7trog\u00b7ner", "Thor\u00b7heit", "Loos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}