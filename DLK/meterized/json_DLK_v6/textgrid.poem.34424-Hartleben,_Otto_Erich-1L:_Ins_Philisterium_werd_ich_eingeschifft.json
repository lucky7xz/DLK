{"textgrid.poem.34424": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ins Philisterium werd ich eingeschifft", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Philisterium werd ich eingeschifft", "tokens": ["Ins", "Phi\u00b7lis\u00b7te\u00b7ri\u00b7um", "werd", "ich", "ein\u00b7ge\u00b7schifft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "als Material f\u00fcr k\u00fcnftige Schwiegerv\u00e4ter,", "tokens": ["als", "Ma\u00b7te\u00b7ri\u00b7al", "f\u00fcr", "k\u00fcnf\u00b7ti\u00b7ge", "Schwie\u00b7ger\u00b7v\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und meid ich nicht die Poesie wie Gift,", "tokens": ["und", "meid", "ich", "nicht", "die", "Poe\u00b7sie", "wie", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "so ernt ich ein Familiengezeter.", "tokens": ["so", "ernt", "ich", "ein", "Fa\u00b7mi\u00b7li\u00b7en\u00b7ge\u00b7ze\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "O Lore! Kind! \u2013 Es rauschen die Pandekten \u2013", "tokens": ["O", "Lo\u00b7re", "!", "Kind", "!", "\u2013", "Es", "rau\u00b7schen", "die", "Pan\u00b7dek\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und du in deiner Sophaecke lachst?", "tokens": ["und", "du", "in", "dei\u00b7ner", "Sop\u00b7ha\u00b7e\u00b7cke", "lachst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Gott, wenn sie zu Hause das entdeckten!", "tokens": ["O", "Gott", ",", "wenn", "sie", "zu", "Hau\u00b7se", "das", "ent\u00b7deck\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kind, sei doch ernst! Du weisst nicht, was du machst!", "tokens": ["Kind", ",", "sei", "doch", "ernst", "!", "Du", "weisst", "nicht", ",", "was", "du", "machst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fchlst du denn nicht den tiefen Ernst der Lage?", "tokens": ["F\u00fchlst", "du", "denn", "nicht", "den", "tie\u00b7fen", "Ernst", "der", "La\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Pflichten, Lebens Jus und Muss? \u2013", "tokens": ["Des", "Le\u00b7bens", "Pflich\u00b7ten", ",", "Le\u00b7bens", "Jus", "und", "Muss", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass ich mit frischer Kraft ans Werk mich wage,", "tokens": ["Dass", "ich", "mit", "fri\u00b7scher", "Kraft", "ans", "Werk", "mich", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gieb mir \u2013 dann aber still! \u2013 noch ", "tokens": ["gieb", "mir", "\u2013", "dann", "a\u00b7ber", "still", "!", "\u2013", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["VVIMP", "PPER", "$(", "ADV", "ADV", "PTKVZ", "$.", "$(", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}