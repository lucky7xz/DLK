{"dta.poem.11431": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gelehrten bettler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grillirus! deine wissenschafft,", "tokens": ["Gril\u00b7li\u00b7rus", "!", "dei\u00b7ne", "wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In die du dich so sehr vergafft,", "tokens": ["In", "die", "du", "dich", "so", "sehr", "ver\u00b7gafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nur ein dunst in meinen augen:", "tokens": ["Ist", "nur", "ein", "dunst", "in", "mei\u00b7nen", "au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie dein geld und dich verzehrt.", "tokens": ["Weil", "sie", "dein", "geld", "und", "dich", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was solt auch wohl ein handwerck taugen,", "tokens": ["Was", "solt", "auch", "wohl", "ein", "hand\u00b7werck", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das seinen meister nicht ern\u00e4hrt?", "tokens": ["Das", "sei\u00b7nen", "meis\u00b7ter", "nicht", "er\u00b7n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}