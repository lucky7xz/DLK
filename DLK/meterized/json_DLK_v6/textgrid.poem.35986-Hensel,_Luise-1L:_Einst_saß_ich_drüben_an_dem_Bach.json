{"textgrid.poem.35986": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst sa\u00df ich dr\u00fcben an dem Bach", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst sa\u00df ich dr\u00fcben an dem Bach", "tokens": ["Einst", "sa\u00df", "ich", "dr\u00fc\u00b7ben", "an", "dem", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah den kleinen Wellen nach.", "tokens": ["Und", "sah", "den", "klei\u00b7nen", "Wel\u00b7len", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sang ich einen frohen Sang,", "tokens": ["Da", "sang", "ich", "ei\u00b7nen", "fro\u00b7hen", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's damals mir im Sinn noch klang.", "tokens": ["Wie's", "da\u00b7mals", "mir", "im", "Sinn", "noch", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sang, k\u00f6nnt' ich die Liebe mein", "tokens": ["Ich", "sang", ",", "k\u00f6nnt'", "ich", "die", "Lie\u00b7be", "mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ganz in Blumen bergen ein.", "tokens": ["Doch", "ganz", "in", "Blu\u00b7men", "ber\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun armes Kind, ach nun ist gar", "tokens": ["Nun", "ar\u00b7mes", "Kind", ",", "ach", "nun", "ist", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ITJ", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein seltsam kindisch Tr\u00e4umen wahr.", "tokens": ["Dein", "selt\u00b7sam", "kin\u00b7disch", "Tr\u00e4u\u00b7men", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlaf sanft in deiner Laube nun,", "tokens": ["Schlaf", "sanft", "in", "dei\u00b7ner", "Lau\u00b7be", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie gerne m\u00f6cht' ich bei dir ruhn!", "tokens": ["Wie", "ger\u00b7ne", "m\u00f6cht'", "ich", "bei", "dir", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}