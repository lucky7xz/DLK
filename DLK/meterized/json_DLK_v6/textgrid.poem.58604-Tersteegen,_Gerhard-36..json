{"textgrid.poem.58604": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seligkeit, o Seel', ist leicht zu kriegen;", "tokens": ["Die", "Se\u00b7lig\u00b7keit", ",", "o", "Seel'", ",", "ist", "leicht", "zu", "krie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Herz und Liebe sollst du wenden nur", "tokens": ["Dein", "Herz", "und", "Lie\u00b7be", "sollst", "du", "wen\u00b7den", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ganz von dir selbst und aller Kreatur", "tokens": ["Ganz", "von", "dir", "selbst", "und", "al\u00b7ler", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu Gott allein, so wird er dich vergn\u00fcgen.", "tokens": ["Zu", "Gott", "al\u00b7lein", ",", "so", "wird", "er", "dich", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er sei dein Gott, dein Gut, und keiner mehr,", "tokens": ["Er", "sei", "dein", "Gott", ",", "dein", "Gut", ",", "und", "kei\u00b7ner", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Beug deine Knie, bet an, gib ihm die Ehr',", "tokens": ["Beug", "dei\u00b7ne", "Knie", ",", "bet", "an", ",", "gib", "ihm", "die", "Ehr'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vergi\u00df dich selbst, schau nicht auf deine Werke,", "tokens": ["Ver\u00b7gi\u00df", "dich", "selbst", ",", "schau", "nicht", "auf", "dei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADJD", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "In Gott ist nur Gerechtigkeit und St\u00e4rke!", "tokens": ["In", "Gott", "ist", "nur", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "St\u00e4r\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}