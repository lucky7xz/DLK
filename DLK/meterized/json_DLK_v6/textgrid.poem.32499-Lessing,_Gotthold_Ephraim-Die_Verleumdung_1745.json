{"textgrid.poem.32499": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Verleumdung 1745", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu nennst mich vom gestrigen Rausche noch trunken?", "tokens": ["\u00bb", "du", "nennst", "mich", "vom", "ge\u00b7stri\u00b7gen", "Rau\u00b7sche", "noch", "trun\u00b7ken", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom gestrigen Rausche? das spricht", "tokens": ["Vom", "ge\u00b7stri\u00b7gen", "Rau\u00b7sche", "?", "das", "spricht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PDS", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ein\u00ab \u2013 \u2013 Fasse dich, schimpfe nur nicht!", "tokens": ["Ein", "\u00ab", "\u2013", "\u2013", "Fas\u00b7se", "dich", ",", "schimp\u00b7fe", "nur", "nicht", "!"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "$(", "$(", "NN", "PPER", "$,", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich wei\u00df wohl, du hast bis am Morgen getrunken.", "tokens": ["Ich", "wei\u00df", "wohl", ",", "du", "hast", "bis", "am", "Mor\u00b7gen", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}