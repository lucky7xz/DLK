{"textgrid.poem.29273": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fromes liegt ins Grabes Nacht;", "tokens": ["Fro\u00b7mes", "liegt", "ins", "Gra\u00b7bes", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00f6ses, hat es umgebracht;", "tokens": ["B\u00f6\u00b7ses", ",", "hat", "es", "um\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frevel erbte seine Habe,", "tokens": ["Fre\u00b7vel", "erb\u00b7te", "sei\u00b7ne", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tantzt daf\u00fcr ihm auff dem Grabe.", "tokens": ["Tantzt", "da\u00b7f\u00fcr", "ihm", "auff", "dem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}