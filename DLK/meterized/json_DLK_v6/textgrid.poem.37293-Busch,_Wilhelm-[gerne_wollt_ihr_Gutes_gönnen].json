["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei es Ehre, sei es Liebe;", "tokens": ["Sei", "es", "Eh\u00b7re", ",", "sei", "es", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dahinter stecken Weiber.", "tokens": ["Doch", "da\u00b7hin\u00b7ter", "ste\u00b7cken", "Wei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVINF", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}