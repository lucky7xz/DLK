{"textgrid.poem.33714": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "5. St. Jago in Chili", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bang ist der Tag, die L\u00fcfte welk und trocken,", "tokens": ["Bang", "ist", "der", "Tag", ",", "die", "L\u00fcf\u00b7te", "welk", "und", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In allen Kirchen wogt's von frommen Bittern", "tokens": ["In", "al\u00b7len", "Kir\u00b7chen", "wogt's", "von", "from\u00b7men", "Bit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um Regen \u2013 horch, was war das f\u00fcr ein Zittern?", "tokens": ["Um", "Re\u00b7gen", "\u2013", "horch", ",", "was", "war", "das", "f\u00fcr", "ein", "Zit\u00b7tern", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$(", "VVIMP", "$,", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wieder \u2013 wieder \u2013 alle Pulse stocken.", "tokens": ["Und", "wie\u00b7der", "\u2013", "wie\u00b7der", "\u2013", "al\u00b7le", "Pul\u00b7se", "sto\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "$(", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Erde bebt \u2013 ein Gott bewegt die Glocken \u2013", "tokens": ["Die", "Er\u00b7de", "bebt", "\u2013", "ein", "Gott", "be\u00b7wegt", "die", "Glo\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaus, hinaus! Von tausend Ungewittern", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", "!", "Von", "tau\u00b7send", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PTKVZ", "$.", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erbebt es unter uns, die Mauern splittern,", "tokens": ["Er\u00b7bebt", "es", "un\u00b7ter", "uns", ",", "die", "Mau\u00b7ern", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde g\u00e4hnt, es regnet Feuerflocken.", "tokens": ["Die", "Er\u00b7de", "g\u00e4hnt", ",", "es", "reg\u00b7net", "Feu\u00b7er\u00b7flo\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Sturz auf Sturz \u2013 auf aus den dumpfen Kammern", "tokens": ["Und", "Sturz", "auf", "Sturz", "\u2013", "auf", "aus", "den", "dum\u00b7pfen", "Kam\u00b7mern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "$(", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerborstner Kirchen, Kerker, Hospit\u00e4ler", "tokens": ["Zer\u00b7borst\u00b7ner", "Kir\u00b7chen", ",", "Ker\u00b7ker", ",", "Hos\u00b7pi\u00b7t\u00e4\u00b7ler"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "St\u00f6hnt Hilferufen, \u00c4chzen, Todesjammern.", "tokens": ["St\u00f6hnt", "Hil\u00b7fe\u00b7ru\u00b7fen", ",", "\u00c4ch\u00b7zen", ",", "To\u00b7des\u00b7jam\u00b7mern", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort aber vor der Stadt durch Hain und T\u00e4ler", "tokens": ["Dort", "a\u00b7ber", "vor", "der", "Stadt", "durch", "Hain", "und", "T\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fliehn Frauen, die ihr lachend Kind umklammern,", "tokens": ["Fliehn", "Frau\u00b7en", ",", "die", "ihr", "la\u00b7chend", "Kind", "um\u00b7klam\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Schwarzen, die gerettet ihre Qu\u00e4ler.", "tokens": ["Mit", "Schwar\u00b7zen", ",", "die", "ge\u00b7ret\u00b7tet", "ih\u00b7re", "Qu\u00e4\u00b7ler", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}