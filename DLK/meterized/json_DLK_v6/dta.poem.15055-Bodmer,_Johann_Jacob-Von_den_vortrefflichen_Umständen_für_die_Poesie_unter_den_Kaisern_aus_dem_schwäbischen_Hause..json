{"dta.poem.15055": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man liset von ainem Hunde", "tokens": ["Man", "li\u00b7set", "von", "ai\u00b7nem", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der trug in seinem Munde", "tokens": ["Der", "trug", "in", "sei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ain st&#250;k flaisch das was gro\u00df", "tokens": ["Ain", "st", "&#250;", "k", "flaisch", "das", "was", "gro\u00df"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "VVFIN", "ADJD", "PDS", "PRELS", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Des sein geschlechte nie verdro\u00df", "tokens": ["Des", "sein", "ge\u00b7schlech\u00b7te", "nie", "ver\u00b7dro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An ainen bach trug in sin weg", "tokens": ["An", "ai\u00b7nen", "bach", "trug", "in", "sin", "weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Do vand er weder brugg noch steg", "tokens": ["Do", "vand", "er", "we\u00b7der", "brugg", "noch", "steg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Do was weder schiff noch man", "tokens": ["Do", "was", "we\u00b7der", "schiff", "noch", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "KON", "VVFIN", "ADV", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ze fu\u00df so must er &#250;ber gan", "tokens": ["Ze", "fu\u00df", "so", "must", "er", "&#250;", "ber", "gan"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NE", "PTKVZ", "ADV", "VMFIN", "PPER", "$(", "ADV", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Do er kam fer in den bach", "tokens": ["Do", "er", "kam", "fer", "in", "den", "bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Den schatten er des flaisches sach", "tokens": ["Den", "schat\u00b7ten", "er", "des", "flai\u00b7sches", "sach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das er in seinem munde trug", "tokens": ["Das", "er", "in", "sei\u00b7nem", "mun\u00b7de", "trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er sprach ich hette wol genug", "tokens": ["Er", "sprach", "ich", "het\u00b7te", "wol", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Moecht ich das Stuck zu diesem han", "tokens": ["Moecht", "ich", "das", "Stuck", "zu", "die\u00b7sem", "han"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vil schier er ginen began", "tokens": ["Vil", "schier", "er", "gi\u00b7nen", "be\u00b7gan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Vnd wolt das stuck begriffen", "tokens": ["Vnd", "wolt", "das", "stuck", "be\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Do muost im das entschliffen,", "tokens": ["Do", "mu\u00b7ost", "im", "das", "ent\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Das er in dem munde hatt.", "tokens": ["Das", "er", "in", "dem", "mun\u00b7de", "hatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Do stund er ledig und matt", "tokens": ["Do", "stund", "er", "le\u00b7dig", "und", "matt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Das er sin stucki hatt verlorn", "tokens": ["Das", "er", "sin", "stu\u00b7cki", "hatt", "ver\u00b7lorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Durch gitigkait das was im zorn.", "tokens": ["Durch", "gi\u00b7tig\u00b7kait", "das", "was", "im", "zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PDS", "PRELS", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}