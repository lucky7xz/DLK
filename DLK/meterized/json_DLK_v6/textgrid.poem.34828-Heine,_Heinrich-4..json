{"textgrid.poem.34828": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge Franziskaner sitzt", "tokens": ["Der", "jun\u00b7ge", "Fran\u00b7zis\u00b7ka\u00b7ner", "sitzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einsam in der Klosterzelle,", "tokens": ["Ein\u00b7sam", "in", "der", "Klos\u00b7ter\u00b7zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er liest im alten Zauberbuch,", "tokens": ["Er", "liest", "im", "al\u00b7ten", "Zau\u00b7ber\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genannt der Zwang der H\u00f6lle.", "tokens": ["Ge\u00b7nannt", "der", "Zwang", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als die Mitternachtstunde schlug,", "tokens": ["Und", "als", "die", "Mit\u00b7ter\u00b7nachts\u00b7tun\u00b7de", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da konnt er nicht l\u00e4nger sich halten,", "tokens": ["Da", "konnt", "er", "nicht", "l\u00e4n\u00b7ger", "sich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit bleichen Lippen ruft er an", "tokens": ["Mit", "blei\u00b7chen", "Lip\u00b7pen", "ruft", "er", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Unterweltsgewalten.", "tokens": ["Die", "Un\u00b7ter\u00b7welts\u00b7ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbihr Geister! holt mir aus dem Grab", "tokens": ["\u00bb", "ihr", "Geis\u00b7ter", "!", "holt", "mir", "aus", "dem", "Grab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leiche der sch\u00f6nsten Frauen,", "tokens": ["Die", "Lei\u00b7che", "der", "sch\u00f6ns\u00b7ten", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Belebt sie mir f\u00fcr diese Nacht,", "tokens": ["Be\u00b7lebt", "sie", "mir", "f\u00fcr", "die\u00b7se", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will mich dran erbauen.\u00ab", "tokens": ["Ich", "will", "mich", "dran", "er\u00b7bau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PAV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er spricht das grause Beschw\u00f6rungswort,", "tokens": ["Er", "spricht", "das", "grau\u00b7se", "Be\u00b7schw\u00f6\u00b7rungs\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da wird sein Wunsch erf\u00fcllet,", "tokens": ["Da", "wird", "sein", "Wunsch", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die arme verstorbene Sch\u00f6nheit kommt,", "tokens": ["Die", "ar\u00b7me", "ver\u00b7stor\u00b7be\u00b7ne", "Sch\u00f6n\u00b7heit", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In wei\u00dfen Laken geh\u00fcllet.", "tokens": ["In", "wei\u00b7\u00dfen", "La\u00b7ken", "ge\u00b7h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihr Blick ist traurig. Aus kalter Brust", "tokens": ["Ihr", "Blick", "ist", "trau\u00b7rig", ".", "Aus", "kal\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die schmerzlichen Seufzer steigen.", "tokens": ["Die", "schmerz\u00b7li\u00b7chen", "Seuf\u00b7zer", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Tote setzt sich zu dem M\u00f6nch,", "tokens": ["Die", "To\u00b7te", "setzt", "sich", "zu", "dem", "M\u00f6nch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schauen sich an und schweigen.", "tokens": ["Sie", "schau\u00b7en", "sich", "an", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}