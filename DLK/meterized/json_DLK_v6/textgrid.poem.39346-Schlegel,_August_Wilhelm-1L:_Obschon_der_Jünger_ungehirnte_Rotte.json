{"textgrid.poem.39346": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Obschon der J\u00fcnger ungehirnte Rotte", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Obschon der J\u00fcnger ungehirnte Rotte", "tokens": ["Ob\u00b7schon", "der", "J\u00fcn\u00b7ger", "un\u00b7ge\u00b7hirn\u00b7te", "Rot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So frech entweiht des S\u00e4ngers hohes Amt,", "tokens": ["So", "frech", "ent\u00b7weiht", "des", "S\u00e4n\u00b7gers", "ho\u00b7hes", "Amt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Obschon das Volk zu schlaffem Lob' und Spotte", "tokens": ["Ob\u00b7schon", "das", "Volk", "zu", "schlaf\u00b7fem", "Lob'", "und", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Manch halbverstandnes G\u00f6tterlied verdammt:", "tokens": ["Manch", "halb\u00b7ver\u00b7stand\u00b7nes", "G\u00f6t\u00b7ter\u00b7lied", "ver\u00b7dammt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch schw\u00f6r' ich Huldigung dem Musengotte,", "tokens": ["Doch", "schw\u00f6r'", "ich", "Hul\u00b7di\u00b7gung", "dem", "Mu\u00b7sen\u00b7got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wahr ein Funk' in mir vom Himmel stammt.", "tokens": ["So", "wahr", "ein", "Funk'", "in", "mir", "vom", "Him\u00b7mel", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oft hat mir, einsam, in der Weihung Grotte", "tokens": ["Oft", "hat", "mir", ",", "ein\u00b7sam", ",", "in", "der", "Wei\u00b7hung", "Grot\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein wunderbares Wort den Geist entflammt.", "tokens": ["Sein", "wun\u00b7der\u00b7ba\u00b7res", "Wort", "den", "Geist", "ent\u00b7flammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich werbe nicht um Ruhm, um Lorbeerkronen;", "tokens": ["Ich", "wer\u00b7be", "nicht", "um", "Ruhm", ",", "um", "Lor\u00b7beer\u00b7kro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer nicht um ihretwillen Ph\u00f6bus Kunst", "tokens": ["Wer", "nicht", "um", "ih\u00b7ret\u00b7wil\u00b7len", "Ph\u00f6\u00b7bus", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Liebe pflegt, erbuhlt nicht Ph\u00f6bus Gunst.", "tokens": ["Mit", "Lie\u00b7be", "pflegt", ",", "er\u00b7buhlt", "nicht", "Ph\u00f6\u00b7bus", "Gunst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVFIN", "PTKNEG", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Dichters Werk soll seinem Sch\u00f6pfer lohnen.", "tokens": ["Des", "Dich\u00b7ters", "Werk", "soll", "sei\u00b7nem", "Sch\u00f6p\u00b7fer", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein goldner Pfeil ereilet rasch das Ziel,", "tokens": ["Sein", "gold\u00b7ner", "Pfeil", "er\u00b7ei\u00b7let", "rasch", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und still gen\u00fcgt ihm seiner That Gef\u00fchl.", "tokens": ["Und", "still", "ge\u00b7n\u00fcgt", "ihm", "sei\u00b7ner", "That", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}