{"textgrid.poem.55876": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch drohen gro\u00dfe V\u00f6gel allenthalben,", "tokens": ["Noch", "dro\u00b7hen", "gro\u00b7\u00dfe", "V\u00f6\u00b7gel", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und Drachen gl\u00fchn und h\u00fcten \u00fcberall", "tokens": ["und", "Dra\u00b7chen", "gl\u00fchn", "und", "h\u00fc\u00b7ten", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der W\u00e4lder Wunder und der Schluchten Fall;", "tokens": ["der", "W\u00e4l\u00b7der", "Wun\u00b7der", "und", "der", "Schluch\u00b7ten", "Fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und Knaben wachsen an, und M\u00e4nner salben", "tokens": ["und", "Kna\u00b7ben", "wach\u00b7sen", "an", ",", "und", "M\u00e4n\u00b7ner", "sal\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "sich zu dem Kampfe mit der Nachtigall,", "tokens": ["sich", "zu", "dem", "Kamp\u00b7fe", "mit", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "die oben in den Kronen von neun Eichen", "tokens": ["die", "o\u00b7ben", "in", "den", "Kro\u00b7nen", "von", "neun", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich lagert wie ein tausendfaches Tier,", "tokens": ["sich", "la\u00b7gert", "wie", "ein", "tau\u00b7send\u00b7fa\u00b7ches", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und abends geht ein Schreien ohnegleichen,", "tokens": ["und", "a\u00b7bends", "geht", "ein", "Schrei\u00b7en", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein schreiendes Bis-an-das-Ende-Reichen,", "tokens": ["ein", "schrei\u00b7en\u00b7des", "Bis\u00b7an\u00b7das\u00b7En\u00b7de\u00b7Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und geht die ganze Nacht lang aus von ihr;", "tokens": ["und", "geht", "die", "gan\u00b7ze", "Nacht", "lang", "aus", "von", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "die Fr\u00fchlingsnacht, die schrecklicher als alles", "tokens": ["die", "Fr\u00fch\u00b7lings\u00b7nacht", ",", "die", "schreck\u00b7li\u00b7cher", "als", "al\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KOKOM", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schwerer war und banger zu bestehn:", "tokens": ["und", "schwe\u00b7rer", "war", "und", "ban\u00b7ger", "zu", "be\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ringsum kein Zeichen eines \u00dcberfalles", "tokens": ["ring\u00b7sum", "kein", "Zei\u00b7chen", "ei\u00b7nes", "\u00dc\u00b7berf\u00b7al\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und dennoch alles voller \u00dcbergehn,", "tokens": ["und", "den\u00b7noch", "al\u00b7les", "vol\u00b7ler", "\u00dc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "hinwerfend sich und St\u00fcck f\u00fcr St\u00fcck sich gebend,", "tokens": ["hin\u00b7wer\u00b7fend", "sich", "und", "St\u00fcck", "f\u00fcr", "St\u00fcck", "sich", "ge\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "KON", "NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ja jenes Etwas, welches um sich griff;", "tokens": ["ja", "je\u00b7nes", "Et\u00b7was", ",", "wel\u00b7ches", "um", "sich", "griff", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ADV", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "anrufend noch, am ganzen Leibe bebend", "tokens": ["an\u00b7ru\u00b7fend", "noch", ",", "am", "gan\u00b7zen", "Lei\u00b7be", "be\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und darin untergehend wie ein Schiff.", "tokens": ["und", "da\u00b7rin", "un\u00b7ter\u00b7ge\u00b7hend", "wie", "ein", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das waren \u00dcberstarke, die da blieben,", "tokens": ["Das", "wa\u00b7ren", "\u00dc\u00b7bers\u00b7tar\u00b7ke", ",", "die", "da", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von diesem Riesigen nicht aufgerieben,", "tokens": ["von", "die\u00b7sem", "Rie\u00b7si\u00b7gen", "nicht", "auf\u00b7ge\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das aus den Kehlen wie aus Kratern brach;", "tokens": ["das", "aus", "den", "Keh\u00b7len", "wie", "aus", "Kra\u00b7tern", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "KOKOM", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sie dauerten, und alternd nach und nach", "tokens": ["sie", "dau\u00b7er\u00b7ten", ",", "und", "al\u00b7ternd", "nach", "und", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "begriffen sie die Bangnis der Aprile,", "tokens": ["be\u00b7grif\u00b7fen", "sie", "die", "Bang\u00b7nis", "der", "Ap\u00b7ri\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und ihre ruhigen H\u00e4nde hielten viele", "tokens": ["und", "ih\u00b7re", "ru\u00b7hi\u00b7gen", "H\u00e4n\u00b7de", "hiel\u00b7ten", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIS"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "und f\u00fchrten sie durch Furcht und Ungemach", "tokens": ["und", "f\u00fchr\u00b7ten", "sie", "durch", "Furcht", "und", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "zu Tagen, da sie froher und ges\u00fcnder", "tokens": ["zu", "Ta\u00b7gen", ",", "da", "sie", "fro\u00b7her", "und", "ge\u00b7s\u00fcn\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "die Mauern bauten um die St\u00e4dtegr\u00fcnder,", "tokens": ["die", "Mau\u00b7ern", "bau\u00b7ten", "um", "die", "St\u00e4d\u00b7te\u00b7gr\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "die \u00fcber allem gut und kundig sa\u00dfen.", "tokens": ["die", "\u00fc\u00b7ber", "al\u00b7lem", "gut", "und", "kun\u00b7dig", "sa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und schlie\u00dflich kamen auf den ersten Stra\u00dfen", "tokens": ["Und", "schlie\u00df\u00b7lich", "ka\u00b7men", "auf", "den", "ers\u00b7ten", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus H\u00f6hlen und verha\u00dften Hinterhalten", "tokens": ["aus", "H\u00f6h\u00b7len", "und", "ver\u00b7ha\u00df\u00b7ten", "Hin\u00b7ter\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Tiere, die f\u00fcr unerbittlich galten.", "tokens": ["die", "Tie\u00b7re", ",", "die", "f\u00fcr", "un\u00b7er\u00b7bitt\u00b7lich", "gal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie stiegen still aus ihren \u00dcberma\u00dfen", "tokens": ["Sie", "stie\u00b7gen", "still", "aus", "ih\u00b7ren", "\u00dc\u00b7berm\u00b7a\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(besch\u00e4mte und veraltete Gewalten)", "tokens": ["(", "be\u00b7sch\u00e4m\u00b7te", "und", "ver\u00b7al\u00b7te\u00b7te", "Ge\u00b7wal\u00b7ten", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und legten sich gehorsam vor die Alten.", "tokens": ["und", "leg\u00b7ten", "sich", "ge\u00b7hor\u00b7sam", "vor", "die", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}