{"textgrid.poem.60116": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herrlich ist der Fr\u00fchlingsmond,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlich ist der Fr\u00fchlingsmond,", "tokens": ["Herr\u00b7lich", "ist", "der", "Fr\u00fch\u00b7lings\u00b7mond", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn, umschwebt von Balsamd\u00fcften,", "tokens": ["Wenn", ",", "um\u00b7schwebt", "von", "Bal\u00b7sam\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Th\u00e4lern, in den L\u00fcften", "tokens": ["In", "den", "Th\u00e4\u00b7lern", ",", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sichtbar Gottes Segen wohnt;", "tokens": ["Sicht\u00b7bar", "Got\u00b7tes", "Se\u00b7gen", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn, einander liebzukosen,", "tokens": ["Wenn", ",", "ein\u00b7an\u00b7der", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Blumenschaar aus tausend Knospen dringt,", "tokens": ["Ei\u00b7ne", "Blu\u00b7men\u00b7schaar", "aus", "tau\u00b7send", "Knos\u00b7pen", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und die Nachtigall den Rosen", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr Geburtslied singt!", "tokens": ["Ihr", "Ge\u00b7burts\u00b7lied", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Herrlicher ist noch die Stunde,", "tokens": ["Herr\u00b7li\u00b7cher", "ist", "noch", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Noch in einem s\u00fc\u00dfern Bunde", "tokens": ["Noch", "in", "ei\u00b7nem", "s\u00fc\u00b7\u00dfern", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fc\u00dfen Erd' und Himmel stehn,", "tokens": ["M\u00fc\u00b7\u00dfen", "Erd'", "und", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ein Engel, ungesehn,", "tokens": ["Wenn", "ein", "En\u00b7gel", ",", "un\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterm Nachruf seiner Br\u00fcder,", "tokens": ["Un\u00b7term", "Nach\u00b7ruf", "sei\u00b7ner", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine sch\u00f6ne Menschenseel' hernieder", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Men\u00b7schen\u00b7seel'", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Aus der Seelen Vaterlande tr\u00e4gt,", "tokens": ["Aus", "der", "See\u00b7len", "Va\u00b7ter\u00b7lan\u00b7de", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df, von Lieb' und Unschuld gro\u00df gepflegt,", "tokens": ["Da\u00df", ",", "von", "Lieb'", "und", "Un\u00b7schuld", "gro\u00df", "ge\u00b7pflegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wallend durch des Lebens Dorngewinde,", "tokens": ["Wal\u00b7lend", "durch", "des", "Le\u00b7bens", "Dorn\u00b7ge\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Sie das Paradies verk\u00fcnde!", "tokens": ["Sie", "das", "Pa\u00b7ra\u00b7dies", "ver\u00b7k\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}