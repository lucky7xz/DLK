{"textgrid.poem.26443": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: An die Staaten, oder an irgend einen von ihnen, oder an irgend eine Stadt der Staaten:", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An die Staaten, oder an irgend einen von ihnen, oder an irgend eine Stadt der Staaten:", "tokens": ["An", "die", "Staa\u00b7ten", ",", "o\u00b7der", "an", "ir\u00b7gend", "ei\u00b7nen", "von", "ih\u00b7nen", ",", "o\u00b7der", "an", "ir\u00b7gend", "ei\u00b7ne", "Stadt", "der", "Staa\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "ADV", "ART", "APPR", "PPER", "$,", "KON", "APPR", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Einmal fragloser Gehorsam, einmal v\u00f6llig unterjocht, einmal ganz zum Sklaven gemacht,", "tokens": ["Ein\u00b7mal", "frag\u00b7lo\u00b7ser", "Ge\u00b7hor\u00b7sam", ",", "ein\u00b7mal", "v\u00f6l\u00b7lig", "un\u00b7ter\u00b7jocht", ",", "ein\u00b7mal", "ganz", "zum", "Skla\u00b7ven", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$,", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+--+-+--+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Erlangt keine Nation, kein Staat, keine Stadt dieser Erde je ihre Freiheit zur\u00fcck!", "tokens": ["Er\u00b7langt", "kei\u00b7ne", "Na\u00b7tion", ",", "kein", "Staat", ",", "kei\u00b7ne", "Stadt", "die\u00b7ser", "Er\u00b7de", "je", "ih\u00b