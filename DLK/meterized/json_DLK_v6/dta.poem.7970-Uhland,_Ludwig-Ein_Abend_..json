{"dta.poem.7970": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Als w\u00e4re nichts geschehen, wird es stille,", "tokens": ["Als", "w\u00e4\u00b7re", "nichts", "ge\u00b7sche\u00b7hen", ",", "wird", "es", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "VVPP", "$,", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Glocken hallen aus, die Lieder enden.", "tokens": ["Die", "Glo\u00b7cken", "hal\u00b7len", "aus", ",", "die", "Lie\u00b7der", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und leichter ward mir in der Thr\u00e4nen F\u00fclle,", "tokens": ["Und", "leich\u00b7ter", "ward", "mir", "in", "der", "Thr\u00e4\u00b7nen", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit Sie versenket war von frommen H\u00e4nden.", "tokens": ["Seit", "Sie", "ver\u00b7sen\u00b7ket", "war", "von", "from\u00b7men", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als noch im Hause lag die bleiche H\u00fclle,", "tokens": ["Als", "noch", "im", "Hau\u00b7se", "lag", "die", "blei\u00b7che", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da wu\u00dft\u2019 ich nicht, wohin nach Ihr mich wenden;", "tokens": ["Da", "wu\u00dft'", "ich", "nicht", ",", "wo\u00b7hin", "nach", "Ihr", "mich", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie schien mir, heimathlos, mit Klaggeb\u00e4rde,", "tokens": ["Sie", "schien", "mir", ",", "hei\u00b7math\u00b7los", ",", "mit", "Klag\u00b7ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu schweben zwischen Himmel hin und Erde.", "tokens": ["Zu", "schwe\u00b7ben", "zwi\u00b7schen", "Him\u00b7mel", "hin", "und", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Abendsonne stralt\u2019, ich sa\u00df im K\u00fchlen", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "stralt'", ",", "ich", "sa\u00df", "im", "K\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und blickte tief in\u2019s lichte Gr\u00fcn der Matten;", "tokens": ["Und", "blick\u00b7te", "tief", "in's", "lich\u00b7te", "Gr\u00fcn", "der", "Mat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir d\u00fcnkte bald, zwei Kinder s\u00e4h\u2019 ich spielen,", "tokens": ["Mir", "d\u00fcnk\u00b7te", "bald", ",", "zwei", "Kin\u00b7der", "s\u00e4h'", "ich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "CARD", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So bl\u00fchend, wie einst wir gebl\u00fchet hatten.", "tokens": ["So", "bl\u00fc\u00b7hend", ",", "wie", "einst", "wir", "ge\u00b7bl\u00fc\u00b7het", "hat\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWAV", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da sank die Sonne, graue Schleier fielen,", "tokens": ["Da", "sank", "die", "Son\u00b7ne", ",", "grau\u00b7e", "Schlei\u00b7er", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Bilder fliehn, die Erde liegt im Schatten;", "tokens": ["Die", "Bil\u00b7der", "fliehn", ",", "die", "Er\u00b7de", "liegt", "im", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich blick\u2019 empor, und hoch in Aethers Auen", "tokens": ["Ich", "blick'", "em\u00b7por", ",", "und", "hoch", "in", "A\u00b7e\u00b7thers", "Au\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ist Abendroth und all mein Gl\u00fcck zu schauen.", "tokens": ["Ist", "A\u00b7ben\u00b7droth", "und", "all", "mein", "Gl\u00fcck", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PIAT", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}