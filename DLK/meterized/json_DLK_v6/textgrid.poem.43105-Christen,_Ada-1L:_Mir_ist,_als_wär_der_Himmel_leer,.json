{"textgrid.poem.43105": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist, als w\u00e4r der Himmel leer,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist, als w\u00e4r der Himmel leer,", "tokens": ["Mir", "ist", ",", "als", "w\u00e4r", "der", "Him\u00b7mel", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde nur ein weites Grab,", "tokens": ["Die", "Er\u00b7de", "nur", "ein", "wei\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder Stern rings ausgegl\u00fcht,", "tokens": ["Und", "je\u00b7der", "Stern", "rings", "aus\u00b7ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herzen gleich, das Alles gab.", "tokens": ["Dem", "Her\u00b7zen", "gleich", ",", "das", "Al\u00b7les", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich, das Grabmal meines Ich's,", "tokens": ["Und", "ich", ",", "das", "Grab\u00b7mal", "mei\u00b7nes", "Ich's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steh' \u00f6d' und still und ganz allein;", "tokens": ["Steh'", "\u00f6d'", "und", "still", "und", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es braust der Wind, der Regen weint", "tokens": ["Es", "braust", "der", "Wind", ",", "der", "Re\u00b7gen", "weint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kalte Thr\u00e4nen auf kalten Stein.", "tokens": ["Kal\u00b7te", "Thr\u00e4\u00b7nen", "auf", "kal\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}