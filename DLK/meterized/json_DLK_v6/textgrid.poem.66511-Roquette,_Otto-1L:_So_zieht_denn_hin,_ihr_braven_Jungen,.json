{"textgrid.poem.66511": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: So zieht denn hin, ihr braven Jungen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So zieht denn hin, ihr braven Jungen,", "tokens": ["So", "zieht", "denn", "hin", ",", "ihr", "bra\u00b7ven", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In's Feld, von heil'ger Gluth entbrannt;", "tokens": ["In's", "Feld", ",", "von", "heil'\u00b7ger", "Gluth", "ent\u00b7brannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus deutscher Kraft seid ihr entsprungen,", "tokens": ["Aus", "deut\u00b7scher", "Kraft", "seid", "ihr", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heil euch, und Heil dem Vaterland!", "tokens": ["Heil", "euch", ",", "und", "Heil", "dem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die uns bisher vereint, die Stunden,", "tokens": ["Die", "uns", "bis\u00b7her", "ver\u00b7eint", ",", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie weichen einer gr\u00f6\u00dfren Pflicht,", "tokens": ["Sie", "wei\u00b7chen", "ei\u00b7ner", "gr\u00f6\u00df\u00b7ren", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ihr wie Morgenruf empfunden", "tokens": ["Die", "ihr", "wie", "Mor\u00b7gen\u00b7ruf", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit freudeleuchtendem Gesicht.", "tokens": ["Mit", "freu\u00b7de\u00b7leuch\u00b7ten\u00b7dem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr wi\u00dft, da\u00df nicht zu leichter F\u00fchrung", "tokens": ["Ihr", "wi\u00dft", ",", "da\u00df", "nicht", "zu", "leich\u00b7ter", "F\u00fch\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "PTKA", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr jugendlich nach Waffen greift,", "tokens": ["Ihr", "ju\u00b7gend\u00b7lich", "nach", "Waf\u00b7fen", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df unerh\u00f6rte Frevelsch\u00fcrung", "tokens": ["Da\u00df", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Fre\u00b7vel\u00b7sch\u00fc\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr schweren Kampf herangereift.", "tokens": ["F\u00fcr", "schwe\u00b7ren", "Kampf", "her\u00b7an\u00b7ge\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Feind, der wahnsinnstrunken heute", "tokens": ["Der", "Feind", ",", "der", "wahn\u00b7sinns\u00b7trun\u00b7ken", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die deutschen Grenzen hart bedroht,", "tokens": ["Die", "deut\u00b7schen", "Gren\u00b7zen", "hart", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchrt seiner Horden ganze Meute", "tokens": ["F\u00fchrt", "sei\u00b7ner", "Hor\u00b7den", "gan\u00b7ze", "Meu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Spiel um Leben oder Tod.", "tokens": ["Zum", "Spiel", "um", "Le\u00b7ben", "o\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob morsch und faul auch seine Sache,", "tokens": ["Ob", "morsch", "und", "faul", "auch", "sei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er kennt des Trugs, der L\u00fcge Kleid,", "tokens": ["Er", "kennt", "des", "Trugs", ",", "der", "L\u00fc\u00b7ge", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kennt den Weg, der Immerwache,", "tokens": ["Und", "kennt", "den", "Weg", ",", "der", "Im\u00b7mer\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Volkesgunst, um die er freit.", "tokens": ["Zur", "Vol\u00b7kes\u00b7gunst", ",", "um", "die", "er", "freit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Schmeichelwort vom Ruhmespfade", "tokens": ["Sein", "Schmei\u00b7chel\u00b7wort", "vom", "Ruh\u00b7mes\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Saugt Spahi, Zuav und Turko ein;", "tokens": ["Saugt", "Spa\u00b7hi", ",", "Zu\u00b7av", "und", "Tur\u00b7ko", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "So f\u00fchrt die H\u00f6llenmaskerade", "tokens": ["So", "f\u00fchrt", "die", "H\u00f6l\u00b7len\u00b7mas\u00b7ke\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der blutigen Macht er an den Rhein.", "tokens": ["Der", "blu\u00b7ti\u00b7gen", "Macht", "er", "an", "den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "F\u00fcr ein Phantom nur fechten jene.", "tokens": ["F\u00fcr", "ein", "Phan\u00b7tom", "nur", "fech\u00b7ten", "je\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PDS", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr fechtet f\u00fcr das h\u00f6chste Gut,", "tokens": ["Ihr", "fech\u00b7tet", "f\u00fcr", "das", "h\u00f6chs\u00b7te", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch spannt der Freiheit Ruf die Sehne,", "tokens": ["Euch", "spannt", "der", "Frei\u00b7heit", "Ruf", "die", "Seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch st\u00e4rkt des ganzen Volkes Muth.", "tokens": ["Euch", "st\u00e4rkt", "des", "gan\u00b7zen", "Vol\u00b7kes", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr zieht in's Feld f\u00fcr Deutschlands Marken,", "tokens": ["Ihr", "zieht", "in's", "Feld", "f\u00fcr", "Deutschlands", "Mar\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "F\u00fcr eurer M\u00fctter reinen Herd,", "tokens": ["F\u00fcr", "eu\u00b7rer", "M\u00fct\u00b7ter", "rei\u00b7nen", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr f\u00fchlt die junge Kraft erstarken", "tokens": ["Ihr", "f\u00fchlt", "die", "jun\u00b7ge", "Kraft", "er\u00b7star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr Alles was uns hoch und werth.", "tokens": ["F\u00fcr", "Al\u00b7les", "was", "uns", "hoch", "und", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es kann nicht, kann sich nicht mehr wenden,", "tokens": ["Es", "kann", "nicht", ",", "kann", "sich", "nicht", "mehr", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was Deutschland heilig sich gelobt,", "tokens": ["Was", "Deutschland", "hei\u00b7lig", "sich", "ge\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ob die Welt an allen Enden", "tokens": ["Und", "ob", "die", "Welt", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Feindesha\u00df dagegen tobt.", "tokens": ["In", "Fein\u00b7des\u00b7ha\u00df", "da\u00b7ge\u00b7gen", "tobt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch ihr seid dazu auserkoren,", "tokens": ["Auch", "ihr", "seid", "da\u00b7zu", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Freiwillig zieht ihr in den Krieg,", "tokens": ["Frei\u00b7wil\u00b7lig", "zieht", "ihr", "in", "den", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu dem wir All' uns zugeschworen", "tokens": ["Zu", "dem", "wir", "All'", "uns", "zu\u00b7ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PIS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unwandelbaren deutschen Sieg!", "tokens": ["Un\u00b7wan\u00b7del\u00b7ba\u00b7ren", "deut\u00b7schen", "Sieg", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So folgt gesegnet euren Fahnen,", "tokens": ["So", "folgt", "ge\u00b7seg\u00b7net", "eu\u00b7ren", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des h\u00f6chsten Segens seid ihr werth!", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Se\u00b7gens", "seid", "ihr", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willkommen, wer von Siegesbahnen", "tokens": ["Will\u00b7kom\u00b7men", ",", "wer", "von", "Sie\u00b7ges\u00b7bah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur alten Schulbank wiederkehrt!", "tokens": ["Zur", "al\u00b7ten", "Schul\u00b7bank", "wie\u00b7der\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Willkommen, wer aus Waffenz\u00fcgen", "tokens": ["Will\u00b7kom\u00b7men", ",", "wer", "aus", "Waf\u00b7fen\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein J\u00fcnglingsherz mir rein bewahrt,", "tokens": ["Sein", "J\u00fcng\u00b7lings\u00b7herz", "mir", "rein", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Tag, da freudiges Gen\u00fcgen", "tokens": ["Am", "Tag", ",", "da", "freu\u00b7di\u00b7ges", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Friedenswerk uns wieder schaart.", "tokens": ["Zum", "Frie\u00b7dens\u00b7werk", "uns", "wie\u00b7der", "schaart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}