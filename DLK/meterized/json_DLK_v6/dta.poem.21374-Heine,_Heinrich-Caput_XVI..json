{"dta.poem.21374": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als solcherma\u00dfen in Eifer gerieth", "tokens": ["Als", "sol\u00b7cher\u00b7ma\u00b7\u00dfen", "in", "Ei\u00b7fer", "ge\u00b7rieth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Alte und sonder Schranken", "tokens": ["Der", "Al\u00b7te", "und", "son\u00b7der", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Schonung mich anschnob, da platzten heraus", "tokens": ["Und", "Scho\u00b7nung", "mich", "an\u00b7schnob", ",", "da", "platz\u00b7ten", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auch mir die geheimsten Gedanken.", "tokens": ["Auch", "mir", "die", "ge\u00b7heims\u00b7ten", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Herr Rothbart \u2013 rief ich laut \u2013 du bist", "tokens": ["Herr", "Roth\u00b7bart", "\u2013", "rief", "ich", "laut", "\u2013", "du", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$(", "VVFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein altes Fabelwesen,", "tokens": ["Ein", "al\u00b7tes", "Fa\u00b7bel\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geh\u2019, leg\u2019 dich schlafen, wir werden uns", "tokens": ["Geh'", ",", "leg'", "dich", "schla\u00b7fen", ",", "wir", "wer\u00b7den", "uns"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch ohne dich erl\u00f6sen.", "tokens": ["Auch", "oh\u00b7ne", "dich", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Republikaner lachen uns aus,", "tokens": ["Die", "Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", "la\u00b7chen", "uns", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sehn sie an unserer Spitze", "tokens": ["Sehn", "sie", "an", "un\u00b7se\u00b7rer", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "So ein Gespenst mit Zepter und Kron\u2019;", "tokens": ["So", "ein", "Ge\u00b7spenst", "mit", "Zep\u00b7ter", "und", "Kron'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie rissen schlechte Witze.", "tokens": ["Sie", "ris\u00b7sen", "schlech\u00b7te", "Wit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch deine Fahne gef\u00e4llt mir nicht mehr,", "tokens": ["Auch", "dei\u00b7ne", "Fah\u00b7ne", "ge\u00b7f\u00e4llt", "mir", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die altdeutschen Narren verdarben", "tokens": ["Die", "alt\u00b7deut\u00b7schen", "Nar\u00b7ren", "ver\u00b7dar\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mir schon in der Burschenschaft die Lust", "tokens": ["Mir", "schon", "in", "der", "Bur\u00b7schen\u00b7schaft", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "An den schwarz-roth-goldnen Farben.", "tokens": ["An", "den", "schwa\u00b7rz\u00b7ro\u00b7th\u00b7gold\u00b7nen", "Far\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}