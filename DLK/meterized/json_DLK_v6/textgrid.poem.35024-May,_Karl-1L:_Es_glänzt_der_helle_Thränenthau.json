{"textgrid.poem.35024": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es gl\u00e4nzt der helle Thr\u00e4nenthau", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00e4nzt der helle Thr\u00e4nenthau", "tokens": ["Es", "gl\u00e4nzt", "der", "hel\u00b7le", "Thr\u00e4\u00b7nent\u00b7hau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Deinem Kelch, dem todesmatten;", "tokens": ["In", "Dei\u00b7nem", "Kelch", ",", "dem", "to\u00b7des\u00b7mat\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sehnst Dich nach des Himmels Blau", "tokens": ["Du", "sehnst", "Dich", "nach", "des", "Him\u00b7mels", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus aus d\u00fcstrem Waldesschatten.", "tokens": ["Hin\u00b7aus", "aus", "d\u00fcst\u00b7rem", "Wal\u00b7des\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es rauscht der Bach am Felsenspalt", "tokens": ["Es", "rauscht", "der", "Bach", "am", "Fel\u00b7sen\u00b7spalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein melancholisch Lied.", "tokens": ["Sein", "me\u00b7lan\u00b7cho\u00b7lisch", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Hier ists so eng, hier ists so kalt,", "tokens": ["Hier", "ists", "so", "eng", ",", "hier", "ists", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo nie der Nebel flieht.", "tokens": ["Wo", "nie", "der", "Ne\u00b7bel", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du meine s\u00fc\u00dfe Himmelslust,", "tokens": ["Du", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Him\u00b7mels\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O traure nicht und la\u00df das Weinen!", "tokens": ["O", "trau\u00b7re", "nicht", "und", "la\u00df", "das", "Wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "KON", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir soll ja stets an treuer Brust", "tokens": ["Dir", "soll", "ja", "stets", "an", "treu\u00b7er", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne meiner Liebe scheinen.", "tokens": ["Die", "Son\u00b7ne", "mei\u00b7ner", "Lie\u00b7be", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum schlie\u00dfe Deine Augen zu,", "tokens": ["Drum", "schlie\u00b7\u00dfe", "Dei\u00b7ne", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Worin die Thr\u00e4nen gl\u00fchn.", "tokens": ["Wo\u00b7rin", "die", "Thr\u00e4\u00b7nen", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja, meine wilde Rose, Du", "tokens": ["Ja", ",", "mei\u00b7ne", "wil\u00b7de", "Ro\u00b7se", ",", "Du"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sollst nicht im Wald verbl\u00fchn!", "tokens": ["Sollst", "nicht", "im", "Wald", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}