{"textgrid.poem.48623": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "35. Als Einer von seiner Liebsten verreisete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! Ich mu\u00df nunmehr von hinnen;", "tokens": ["Nein", "!", "Ich", "mu\u00df", "nun\u00b7mehr", "von", "hin\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "es, es mu\u00df geschieden sein.", "tokens": ["es", ",", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stellt das kl\u00e4gliche Beginnen,", "tokens": ["Stellt", "das", "kl\u00e4g\u00b7li\u00b7che", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebste, stellt das Weinen ein!", "tokens": ["Liebs\u00b7te", ",", "stellt", "das", "Wei\u00b7nen", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wol dem, wer beherzt nimmt an,", "tokens": ["Wol", "dem", ",", "wer", "be\u00b7herzt", "nimmt", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PWS", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "was er doch nicht \u00e4ndern kan!", "tokens": ["was", "er", "doch", "nicht", "\u00e4n\u00b7dern", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Verh\u00e4ngn\u00fc\u00df will ertragen,", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "will", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "allzeit nicht gewendet sein.", "tokens": ["all\u00b7zeit", "nicht", "ge\u00b7wen\u00b7det", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollet ihr an dem verzagen,", "tokens": ["Wol\u00b7let", "ihr", "an", "dem", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "was doch Gott nur wei\u00df allein?", "tokens": ["was", "doch", "Gott", "nur", "wei\u00df", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkt, es k\u00f6nne nichts geschehn,", "tokens": ["Denkt", ",", "es", "k\u00f6n\u00b7ne", "nichts", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "was er nicht zuvor versehn!", "tokens": ["was", "er", "nicht", "zu\u00b7vor", "ver\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ich mu\u00df es selbst bekennen,", "tokens": ["Zwar", "ich", "mu\u00df", "es", "selbst", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "es ist ein sehr fernes Land,", "tokens": ["es", "ist", "ein", "sehr", "fer\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "das mich seinen Gast wird nennen.", "tokens": ["das", "mich", "sei\u00b7nen", "Gast", "wird", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch, wie weit es abgewandt,", "tokens": ["Doch", ",", "wie", "weit", "es", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "so ist doch nur eine Welt,", "tokens": ["so", "ist", "doch", "nur", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "die uns Beiden doch beh\u00e4lt.", "tokens": ["die", "uns", "Bei\u00b7den", "doch", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der, der euch kan unterhalten", "tokens": ["Der", ",", "der", "euch", "kan", "un\u00b7ter\u00b7hal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hier in unsrer s\u00fc\u00dfen Stadt,", "tokens": ["hier", "in", "uns\u00b7rer", "s\u00fc\u00b7\u00dfen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "eben der wird meiner walten,", "tokens": ["e\u00b7ben", "der", "wird", "mei\u00b7ner", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wo er auch zu herrschen hat.", "tokens": ["wo", "er", "auch", "zu", "herr\u00b7schen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Gott, der hilft ohne Wahl", "tokens": ["Ein", "Gott", ",", "der", "hilft", "oh\u00b7ne", "Wahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "hier und da und \u00fcberall.", "tokens": ["hier", "und", "da", "und", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eine Gnad' ist schon ergangen,", "tokens": ["Ei\u00b7ne", "Gnad'", "ist", "schon", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df er euch mir wiedergab.", "tokens": ["da\u00df", "er", "euch", "mir", "wie\u00b7der\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich euch noch kan umfangen,", "tokens": ["Weil", "ich", "euch", "noch", "kan", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "desto lieber scheid' ich ab;", "tokens": ["des\u00b7to", "lie\u00b7ber", "scheid'", "ich", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "desto freier zieh' ich hin,", "tokens": ["des\u00b7to", "frei\u00b7er", "zieh'", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "weil ich stets doch bei euch bin.", "tokens": ["weil", "ich", "stets", "doch", "bei", "euch", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dieses Pfand, mein treues Herze", "tokens": ["Die\u00b7ses", "Pfand", ",", "mein", "treu\u00b7es", "Her\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nehmet hin, wie eures ich!", "tokens": ["neh\u00b7met", "hin", ",", "wie", "eu\u00b7res", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPOSAT", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was uns itzund zwingt zu Schmerze,", "tokens": ["Was", "uns", "it\u00b7zund", "zwingt", "zu", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "soll ergetzen euch und mich!", "tokens": ["soll", "er\u00b7get\u00b7zen", "euch", "und", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freude folgt auf Angst und Pein,", "tokens": ["Freu\u00b7de", "folgt", "auf", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie auf Regen Sonnenschein.", "tokens": ["wie", "auf", "Re\u00b7gen", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gute Nacht, o liebe Seele,", "tokens": ["Gu\u00b7te", "Nacht", ",", "o", "lie\u00b7be", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "o Gem\u00fcte voller Treu',", "tokens": ["o", "Ge\u00b7m\u00fc\u00b7te", "vol\u00b7ler", "Treu'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das ich durch mein Absein qu\u00e4le", "tokens": ["das", "ich", "durch", "mein", "Ab\u00b7sein", "qu\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "und durch Wiederkunft erfreu'!", "tokens": ["und", "durch", "Wie\u00b7der\u00b7kunft", "er\u00b7freu'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Itzt schon n\u00e4hert sich die Zeit,", "tokens": ["Itzt", "schon", "n\u00e4\u00b7hert", "sich", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "die uns setzt in Fr\u00f6ligkeit.", "tokens": ["die", "uns", "setzt", "in", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}