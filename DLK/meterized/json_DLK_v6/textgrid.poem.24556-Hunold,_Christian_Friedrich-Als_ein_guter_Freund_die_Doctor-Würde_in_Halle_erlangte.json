{"textgrid.poem.24556": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als ein guter Freund die Doctor-W\u00fcrde in Halle erlangte", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hoch in ihrem Werth die ", "tokens": ["Wie", "hoch", "in", "ih\u00b7rem", "Werth", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Klugen Welt-bekandt. Di\u00df Friederichs-", "tokens": ["Ist", "Klu\u00b7gen", "Welt\u00b7be\u00b7kandt", ".", "Di\u00df", "Frie\u00b7de\u00b7richs"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$.", "PDS", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat ihr unsterblich Lob am wenigsten verschwiegen:", "tokens": ["Hat", "ihr", "uns\u00b7terb\u00b7lich", "Lob", "am", "we\u00b7nigs\u00b7ten", "ver\u00b7schwie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ihr Prei\u00df wird ohne mich und meinen Ruhm bestehn.", "tokens": ["Ihr", "Prei\u00df", "wird", "oh\u00b7ne", "mich", "und", "mei\u00b7nen", "Ruhm", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey denen nun dein Geist die Lehren eingesogen/", "tokens": ["Bey", "de\u00b7nen", "nun", "dein", "Geist", "die", "Leh\u00b7ren", "ein\u00b7ge\u00b7so\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die k\u00f6nnen sich auch selbst ein Ehren-", "tokens": ["Die", "k\u00f6n\u00b7nen", "sich", "auch", "selbst", "ein", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den an Vortrefflichkeit noch keiner \u00fcberwogen/", "tokens": ["Den", "an", "Vor\u00b7treff\u00b7lich\u00b7keit", "noch", "kei\u00b7ner", "\u00fc\u00b7ber\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und der dich heute kr\u00f6nt/ der prangt durch sich allein.", "tokens": ["Und", "der", "dich", "heu\u00b7te", "kr\u00f6nt", "/", "der", "prangt", "durch", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "VVFIN", "$(", "ART", "VVFIN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hiernechst so r\u00fchmen dich so viel gelehrte Proben/", "tokens": ["Hier\u00b7nechst", "so", "r\u00fch\u00b7men", "dich", "so", "viel", "ge\u00b7lehr\u00b7te", "Pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So da\u00df ich/ Wehrter Freund/ nichts weiter schreiben kan/", "tokens": ["So", "da\u00df", "ich", "/", "Wehr\u00b7ter", "Freund", "/", "nichts", "wei\u00b7ter", "schrei\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$(", "NN", "NN", "$(", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als: den sein eigner Flei\u00df/ die theuren Lehrer loben/", "tokens": ["Als", ":", "den", "sein", "eig\u00b7ner", "Flei\u00df", "/", "die", "theu\u00b7ren", "Leh\u00b7rer", "lo\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "ART", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Ort/ der Weltber\u00fchmt/ wo Er sich vorgethan/", "tokens": ["Der", "Ort", "/", "der", "Welt\u00b7be\u00b7r\u00fchmt", "/", "wo", "Er", "sich", "vor\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "PWAV", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den mu\u00df ein Ehren-Amt statt aller Ver\u00dfe schm\u00fccken/", "tokens": ["Den", "mu\u00df", "ein", "Eh\u00b7ren\u00b7Amt", "statt", "al\u00b7ler", "Ver\u00b7\u00dfe", "schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Damit der Himmel dich in kurtzen wird begl\u00fccken.", "tokens": ["Da\u00b7mit", "der", "Him\u00b7mel", "dich", "in", "kurt\u00b7zen", "wird", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "APPR", "ADJA", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}