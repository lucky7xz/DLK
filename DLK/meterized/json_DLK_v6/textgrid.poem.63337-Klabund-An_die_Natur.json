{"textgrid.poem.63337": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "An die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur! Natur! Du G\u00f6tterwelt!", "tokens": ["Na\u00b7tur", "!", "Na\u00b7tur", "!", "Du", "G\u00f6t\u00b7ter\u00b7welt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bist du pr\u00e4chtig aufgestellt", "tokens": ["Wie", "bist", "du", "pr\u00e4ch\u00b7tig", "auf\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Bergen gro\u00df und T\u00e4lern klein,", "tokens": ["Mit", "Ber\u00b7gen", "gro\u00df", "und", "T\u00e4\u00b7lern", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es hat wohl m\u00fcssen also sein.", "tokens": ["Es", "hat", "wohl", "m\u00fcs\u00b7sen", "al\u00b7so", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und mittendrin in der Natur", "tokens": ["Und", "mit\u00b7ten\u00b7drin", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dehnt sich die gr\u00fcne Wiesenflur,", "tokens": ["Dehnt", "sich", "die", "gr\u00fc\u00b7ne", "Wie\u00b7sen\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im Winter ist sie wei\u00df beschneit,", "tokens": ["Im", "Win\u00b7ter", "ist", "sie", "wei\u00df", "be\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hat ein Jedes seine Zeit.", "tokens": ["So", "hat", "ein", "Je\u00b7des", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch du, auch du, o Menschenkind,", "tokens": ["Auch", "du", ",", "auch", "du", ",", "o", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedenke, wie die Zeit verrinnt.", "tokens": ["Be\u00b7den\u00b7ke", ",", "wie", "die", "Zeit", "ver\u00b7rinnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heut rauscht sie m\u00e4chtig noch daher", "tokens": ["Heut", "rauscht", "sie", "m\u00e4ch\u00b7tig", "noch", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und morgen sieht man sie nicht mehr.", "tokens": ["Und", "mor\u00b7gen", "sieht", "man", "sie", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Frisch auf, frisch auf, mit H\u00f6rnerklang", "tokens": ["Frisch", "auf", ",", "frisch", "auf", ",", "mit", "H\u00f6r\u00b7ner\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch das verschneite Tal entlang,", "tokens": ["Durch", "das", "ver\u00b7schnei\u00b7te", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die Gl\u00f6ckchen klingeln am Gel\u00e4ut:", "tokens": ["Die", "Gl\u00f6ck\u00b7chen", "klin\u00b7geln", "am", "Ge\u00b7l\u00e4ut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gestern war gestern, morgen wird morgen sein, heute ist heut.", "tokens": ["Ge\u00b7stern", "war", "ge\u00b7stern", ",", "mor\u00b7gen", "wird", "mor\u00b7gen", "sein", ",", "heu\u00b7te", "ist", "heut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "ADV", "VAINF", "$,", "ADV", "VAFIN", "ADV", "$."], "meter": "-+--+--++-+---+", "measure": "iambic.hexa.relaxed"}}}}}