{"textgrid.poem.39943": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fridolin, der fromme Schotte,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fridolin, der fromme Schotte,", "tokens": ["Fri\u00b7do\u00b7lin", ",", "der", "from\u00b7me", "Schot\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trat vor Landolph hin, den Grafen:", "tokens": ["Trat", "vor", "Lan\u00b7dolph", "hin", ",", "den", "Gra\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach: \u00bbWas Gottes ist, gieb Gotte!", "tokens": ["Sprach", ":", "\u00bb", "Was", "Got\u00b7tes", "ist", ",", "gieb", "Got\u00b7te", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PWS", "NN", "VAFIN", "$,", "VVIMP", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dein Bruder nicht entschlafen?", "tokens": ["Ist", "dein", "Bru\u00b7der", "nicht", "ent\u00b7schla\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der zu seiner Seele Frieden", "tokens": ["Der", "zu", "sei\u00b7ner", "See\u00b7le", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinem heil'gen Gotteshause", "tokens": ["Mei\u00b7nem", "heil'\u00b7gen", "Got\u00b7tes\u00b7hau\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gut und Habe zubeschieden,", "tokens": ["Gut", "und", "Ha\u00b7be", "zu\u00b7be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liegt zu Glaris in der Klause.", "tokens": ["Liegt", "zu", "Gla\u00b7ris", "in", "der", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Warum erntest du die Felder,", "tokens": ["Wa\u00b7rum", "ern\u00b7test", "du", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Die dem Herrn zu schneiden w\u00e4ren,", "tokens": ["Die", "dem", "Herrn", "zu", "schnei\u00b7den", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Warum f\u00e4llest du die W\u00e4lder,", "tokens": ["Wa\u00b7rum", "f\u00e4l\u00b7lest", "du", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die dem Kirchenbau geh\u00f6ren?", "tokens": ["Die", "dem", "Kir\u00b7chen\u00b7bau", "ge\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wagest du's, den Rausch zu trinken", "tokens": ["Wa\u00b7gest", "du's", ",", "den", "Rausch", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Von dem roten Ehrenweine,", "tokens": ["Von", "dem", "ro\u00b7ten", "Eh\u00b7ren\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Der im heil'gen Kelch soll blinken?", "tokens": ["Der", "im", "heil'\u00b7gen", "Kelch", "soll", "blin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Kirchengut, ist es das deine?", "tokens": ["Kir\u00b7chen\u00b7gut", ",", "ist", "es", "das", "dei\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ART", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "La\u00df von deines Bruders Gabe,", "tokens": ["La\u00df", "von", "dei\u00b7nes", "Bru\u00b7ders", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wald und Feld und Garten r\u00e4ume,", "tokens": ["Wald", "und", "Feld", "und", "Gar\u00b7ten", "r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df der Bruder in dem Grabe", "tokens": ["Da\u00df", "der", "Bru\u00b7der", "in", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sanfter lieg' und besser tr\u00e4ume.\u00ab", "tokens": ["Sanf\u00b7ter", "lieg'", "und", "bes\u00b7ser", "tr\u00e4u\u00b7me", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aber Landolph sprach mit Lachen:", "tokens": ["A\u00b7ber", "Lan\u00b7dolph", "sprach", "mit", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "\u00bbsoll ich deinem Spruch mich beugen,", "tokens": ["\u00bb", "soll", "ich", "dei\u00b7nem", "Spruch", "mich", "beu\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Mu\u00df der Bruder erst erwachen,", "tokens": ["Mu\u00df", "der", "Bru\u00b7der", "erst", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Deine Worte selbst bezeugen!", "tokens": ["Dei\u00b7ne", "Wor\u00b7te", "selbst", "be\u00b7zeu\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Kannst du ihn herauf beschw\u00f6ren", "tokens": ["Kannst", "du", "ihn", "her\u00b7auf", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wenn zu Rangkwil wird gerichtet,", "tokens": ["Wenn", "zu", "Rangk\u00b7wil", "wird", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wohl, dann m\u00f6gen wir dich h\u00f6ren,", "tokens": ["Wohl", ",", "dann", "m\u00f6\u00b7gen", "wir", "dich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Sonst ist's Lug, den du erdichtet!\u00ab", "tokens": ["Sonst", "ist's", "Lug", ",", "den", "du", "er\u00b7dich\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Fridolin auf solche T\u00fccke", "tokens": ["Fri\u00b7do\u00b7lin", "auf", "sol\u00b7che", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "W\u00fcrdiget kein Wort zu sprechen,", "tokens": ["W\u00fcr\u00b7di\u00b7get", "kein", "Wort", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Sieht ihn an mit einem Blicke,", "tokens": ["Sieht", "ihn", "an", "mit", "ei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Der durch Gr\u00e4ber k\u00f6nnte brechen,", "tokens": ["Der", "durch", "Gr\u00e4\u00b7ber", "k\u00f6nn\u00b7te", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und von Seckingen am Rheine", "tokens": ["Und", "von", "Se\u00b7ckin\u00b7gen", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPRART", "NE"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Aus dem Kloster, an dem Stabe", "tokens": ["Aus", "dem", "Klos\u00b7ter", ",", "an", "dem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Zog der Greis durch's Waldgesteine", "tokens": ["Zog", "der", "Greis", "durch's", "Wald\u00b7ge\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Bis gen Glaris zu dem Grabe.", "tokens": ["Bis", "gen", "Gla\u00b7ris", "zu", "dem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und er trat bei'm Abendschauer", "tokens": ["Und", "er", "trat", "bei'm", "A\u00b7bend\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "In die d\u00fcstre Waldkapelle,", "tokens": ["In", "die", "d\u00fcst\u00b7re", "Wald\u00b7ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Er durchbricht des Grabes Mauer,", "tokens": ["Er", "durch\u00b7bricht", "des", "Gra\u00b7bes", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.40": {"text": "Stellt sich auf die kalte Schwelle.", "tokens": ["Stellt", "sich", "auf", "die", "kal\u00b7te", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "\u00bbauf, erwach in Gottes Namen,\u00ab", "tokens": ["\u00bb", "auf", ",", "er\u00b7wach", "in", "Got\u00b7tes", "Na\u00b7men", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Ruft er, \u00bbUrso, wehr' den T\u00fccken:", "tokens": ["Ruft", "er", ",", "\u00bb", "Ur\u00b7so", ",", "wehr'", "den", "T\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Sieh! und aus der Grube kamen", "tokens": ["Sieh", "!", "und", "aus", "der", "Gru\u00b7be", "ka\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Wei\u00dfe H\u00e4nd' und Haupt und R\u00fccken.", "tokens": ["Wei\u00b7\u00dfe", "H\u00e4nd'", "und", "Haupt", "und", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Und als ob des Herrn Posaunen", "tokens": ["Und", "als", "ob", "des", "Herrn", "Po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.46": {"text": "Zum Gerichte schon gerufen,", "tokens": ["Zum", "Ge\u00b7rich\u00b7te", "schon", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Steigt der Leichnam sonder Staunen", "tokens": ["Steigt", "der", "Leich\u00b7nam", "son\u00b7der", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Starr empor des Grabes Stufen.", "tokens": ["Starr", "em\u00b7por", "des", "Gra\u00b7bes", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Und es fa\u00dft die kalten H\u00e4nde", "tokens": ["Und", "es", "fa\u00dft", "die", "kal\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Fridolin ihm, frei von Schrecken,", "tokens": ["Fri\u00b7do\u00b7lin", "ihm", ",", "frei", "von", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Steigt mit ihm die Felsenw\u00e4nde", "tokens": ["Steigt", "mit", "ihm", "die", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Auf, bis an der Gletscher Decken.", "tokens": ["Auf", ",", "bis", "an", "der", "Glet\u00b7scher", "De\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Durch das Hochgebirge schreitet", "tokens": ["Durch", "das", "Hoch\u00b7ge\u00b7bir\u00b7ge", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Der Lebend'ge mit der Leiche,", "tokens": ["Der", "Le\u00b7ben\u00b7d'\u00b7ge", "mit", "der", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Und die Nacht den Mantel spreitet", "tokens": ["Und", "die", "Nacht", "den", "Man\u00b7tel", "sprei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Um das Paar, das geistergleiche.", "tokens": ["Um", "das", "Paar", ",", "das", "geis\u00b7ter\u00b7glei\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Wie der Morgen schon sich wittert,", "tokens": ["Wie", "der", "Mor\u00b7gen", "schon", "sich", "wit\u