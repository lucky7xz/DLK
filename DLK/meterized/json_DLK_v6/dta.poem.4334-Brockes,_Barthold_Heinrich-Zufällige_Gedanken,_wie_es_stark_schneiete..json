{"dta.poem.4334": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4llige Gedanken, wie es stark schneiete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich das schw\u00e4rmende Gewirre von nicht zu", "tokens": ["Wenn", "ich", "das", "schw\u00e4r\u00b7men\u00b7de", "Ge\u00b7wir\u00b7re", "von", "nicht", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PTKNEG", "PTKZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zehl'nden Theilen Schnee,", "tokens": ["zehl'n\u00b7den", "Thei\u00b7len", "Schnee", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Getrieben durch verschiedne Winde, wild durch einander", "tokens": ["Ge\u00b7trie\u00b7ben", "durch", "ver\u00b7schied\u00b7ne", "Win\u00b7de", ",", "wild", "durch", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "PRF"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "fliegen seh,", "tokens": ["flie\u00b7gen", "seh", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und, mit unordentlichem Drang, von Millionen Orten", "tokens": ["Und", ",", "mit", "un\u00b7or\u00b7dent\u00b7li\u00b7chem", "Drang", ",", "von", "Mil\u00b7lion\u00b7en", "Or\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "NN"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "eilen,", "tokens": ["ei\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Doch all' herabwerts fallen schau; f\u00e4llt mir bey ihren", "tokens": ["Doch", "all'", "her\u00b7ab\u00b7werts", "fal\u00b7len", "schau", ";", "f\u00e4llt", "mir", "bey", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "VVFIN", "PTKVZ", "$.", "VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wirbeln ein:", "tokens": ["Wir\u00b7beln", "ein", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Welch ein entsetzliches Gemisch mu\u00df von des ersten Chaos", "tokens": ["Welch", "ein", "ent\u00b7setz\u00b7li\u00b7ches", "Ge\u00b7misch", "mu\u00df", "von", "des", "ers\u00b7ten", "Chaos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Theilen,", "tokens": ["Thei\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Wie aller Meer' und Erden Stoff sich durch einander", "tokens": ["Wie", "al\u00b7ler", "Meer'", "und", "Er\u00b7den", "Stoff", "sich", "durch", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN", "NN", "PRF", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "hergelenkt,", "tokens": ["her\u00b7ge\u00b7lenkt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und zum gemeinen Mittelpunct sich \u00fcberall herabgesenkt,", "tokens": ["Und", "zum", "ge\u00b7mei\u00b7nen", "Mit\u00b7tel\u00b7punct", "sich", "\u00fc\u00b7be\u00b7rall", "her\u00b7ab\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.14": {"text": "Auf ihres Sch\u00f6pfers Allmacht-Wort, im Augenblick ent-", "tokens": ["Auf", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "All\u00b7macht\u00b7Wort", ",", "im", "Au\u00b7gen\u00b7blick", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "standen seyn!", "tokens": ["stan\u00b7den", "seyn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es schwindelt der beherztste Geist, er stutzt, er wei\u00df sich", "tokens": ["Es", "schwin\u00b7delt", "der", "be\u00b7herzts\u00b7te", "Geist", ",", "er", "stutzt", ",", "er", "wei\u00df", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "kaum zu lassen,", "tokens": ["kaum", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Noch von den recht entsetzlichen Bewegungs-Kr\u00e4ften was", "tokens": ["Noch", "von", "den", "recht", "ent\u00b7setz\u00b7li\u00b7chen", "Be\u00b7we\u00b7gungs\u00b7Kr\u00e4f\u00b7ten", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "PWS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "zu fassen,", "tokens": ["zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Erstaunet ob der Theile Menge, doch bleibet ihm die Kraft", "tokens": ["Er\u00b7stau\u00b7net", "ob", "der", "Thei\u00b7le", "Men\u00b7ge", ",", "doch", "blei\u00b7bet", "ihm", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+---+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "allein,", "tokens": ["al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die Allmacht-Kr\u00e4fte zu bewundern, die sich in nichts so", "tokens": ["Die", "All\u00b7macht\u00b7Kr\u00e4f\u00b7te", "zu", "be\u00b7wun\u00b7dern", ",", "die", "sich", "in", "nichts", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PRF", "APPR", "PIS", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "deutlich weisen,", "tokens": ["deut\u00b7lich", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Als, in so greulicher Verwirrung, dennoch ein\u2019 Ordnung", "tokens": ["Als", ",", "in", "so", "greu\u00b7li\u00b7cher", "Ver\u00b7wir\u00b7rung", ",", "den\u00b7noch", "ein'", "Ord\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.10": {"text": "auszufinden,", "tokens": ["aus\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Und das, was nicht verbindlich scheint, so wunderbarlich", "tokens": ["Und", "das", ",", "was", "nicht", "ver\u00b7bind\u00b7lich", "scheint", ",", "so", "wun\u00b7der\u00b7bar\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "PTKNEG", "ADJD", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "zu verbinden,", "tokens": ["zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wahrhaftig wohl ein w\u00fcrd\u2019ger Vorwurf, in tiefer Ehr-", "tokens": ["Wahr\u00b7haf\u00b7tig", "wohl", "ein", "w\u00fcrd'\u00b7ger", "Vor\u00b7wurf", ",", "in", "tie\u00b7fer", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "furcht, GOtt zu preisen!", "tokens": ["furcht", ",", "Gott", "zu", "prei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Damit nun dieses \u00f6fterer, zu GOttes Ruhm, von mir", "tokens": ["Da\u00b7mit", "nun", "die\u00b7ses", "\u00f6f\u00b7te\u00b7rer", ",", "zu", "Got\u00b7tes", "Ruhm", ",", "von", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "ADV", "PDAT", "ADJA", "$,", "APPR", "NN", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "gescheh,", "tokens": ["ge\u00b7scheh", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Soll mich, so oft ich die Verwirrung so ungez\u00e4hlter Theil\u2019", "tokens": ["Soll", "mich", ",", "so", "oft", "ich", "die", "Ver\u00b7wir\u00b7rung", "so", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Theil'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ADV", "ADV", "PPER", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "im Schnee,", "tokens": ["im", "Schnee", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "In wild- und fl\u00fcchtiger Bewegung, von oben abwerts", "tokens": ["In", "wild", "und", "fl\u00fcch\u00b7ti\u00b7ger", "Be\u00b7we\u00b7gung", ",", "von", "o\u00b7ben", "ab\u00b7werts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "fallen seh,", "tokens": ["fal\u00b7len", "seh", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Es mir zu einem Lehrbild dienen: So oft sie schnell herab", "tokens": ["Es", "mir", "zu", "ei\u00b7nem", "Lehr\u00b7bild", "die\u00b7nen", ":", "So", "oft", "sie", "schnell", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "ADV", "ADV", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "sich senken,", "tokens": ["sich", "sen\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Will ich an GOttes weise Macht, im ", "tokens": ["Will", "ich", "an", "Got\u00b7tes", "wei\u00b7se", "Macht", ",", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADJA", "NN", "$,", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sch\u00f6pfung, denken.", "tokens": ["Sch\u00f6p\u00b7fung", ",", "den\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}