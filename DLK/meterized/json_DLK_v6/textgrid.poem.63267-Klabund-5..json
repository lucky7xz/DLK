{"textgrid.poem.63267": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe das heiligste Herz verloren.", "tokens": ["Ich", "ha\u00b7be", "das", "hei\u00b7ligs\u00b7te", "Herz", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe allen Schmerz der Welt getragen.", "tokens": ["Ich", "ha\u00b7be", "al\u00b7len", "Schmerz", "der", "Welt", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sechs Monate lag ich \u00fcber einem Grabe", "tokens": ["Sechs", "Mo\u00b7na\u00b7te", "lag", "ich", "\u00fc\u00b7ber", "ei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und jaulte wie ein Hund.", "tokens": ["Und", "jaul\u00b7te", "wie", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe in die Sonne gebellt,", "tokens": ["Ich", "ha\u00b7be", "in", "die", "Son\u00b7ne", "ge\u00b7bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ich habe in den Mond gebellt,", "tokens": ["Ich", "ha\u00b7be", "in", "den", "Mond", "ge\u00b7bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Einsamer war ich wie der Dipplodocus.", "tokens": ["Ein\u00b7sa\u00b7mer", "war", "ich", "wie", "der", "Dipp\u00b7lo\u00b7do\u00b7cus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Aber nun reisst es mich empor,", "tokens": ["A\u00b7ber", "nun", "reisst", "es", "mich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Jemand biegt meinen Kopf zur\u00fcck, dass meine Nackenmuskeln knacken,", "tokens": ["Je\u00b7mand", "biegt", "mei\u00b7nen", "Kopf", "zu\u00b7r\u00fcck", ",", "dass", "mei\u00b7ne", "Na\u00b7cken\u00b7mus\u00b7keln", "kna\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.10": {"text": "Und ein b\u00e4rtiger Mann, mit einem Ziegenfell bekleidet,", "tokens": ["Und", "ein", "b\u00e4r\u00b7ti\u00b7ger", "Mann", ",", "mit", "ei\u00b7nem", "Zie\u00b7gen\u00b7fell", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "L\u00e4chelt wie der Himmel \u00fcber mir,", "tokens": ["L\u00e4\u00b7chelt", "wie", "der", "Him\u00b7mel", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Donnert wie der Himmel \u00fcber mir:", "tokens": ["Don\u00b7nert", "wie", "der", "Him\u00b7mel", "\u00fc\u00b7ber", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Lebe!", "tokens": ["Le\u00b7be", "!"], "token_info": ["word", "punct"], "pos": ["VVIMP", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}