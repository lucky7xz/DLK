{"textgrid.poem.26312": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Vollmond", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vollmond macht die Nacht so weit,", "tokens": ["Der", "Voll\u00b7mond", "macht", "die", "Nacht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume wachsen dunkel breit,", "tokens": ["Die", "B\u00e4u\u00b7me", "wach\u00b7sen", "dun\u00b7kel", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die Bl\u00e4tter springt Gefunkel.", "tokens": ["Und", "durch", "die", "Bl\u00e4t\u00b7ter", "springt", "Ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie eine reiche goldne Last", "tokens": ["Wie", "ei\u00b7ne", "rei\u00b7che", "gold\u00b7ne", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4ngt er dort blendend auf dem Ast,", "tokens": ["H\u00e4ngt", "er", "dort", "blen\u00b7dend", "auf", "dem", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sein Glei\u00dfen hell verschwendend.", "tokens": ["Sein", "Glei\u00b7\u00dfen", "hell", "ver\u00b7schwen\u00b7dend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schutzspendend gl\u00e4nzt er wie ein Schild,", "tokens": ["Schutz\u00b7spen\u00b7dend", "gl\u00e4nzt", "er", "wie", "ein", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ruhe und der Milde Bild", "tokens": ["Der", "Ru\u00b7he", "und", "der", "Mil\u00b7de", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf himmlischem Gefilde.", "tokens": ["Auf", "himm\u00b7li\u00b7schem", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}