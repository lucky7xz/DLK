{"textgrid.poem.24966": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine Franz", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern noch im muntern Spiel", "tokens": ["Ge\u00b7stern", "noch", "im", "mun\u00b7tern", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit den Seifenblasen", "tokens": ["Mit", "den", "Sei\u00b7fen\u00b7bla\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sprang er viel und lachte viel", "tokens": ["Sprang", "er", "viel", "und", "lach\u00b7te", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem gr\u00fcnen Rasen;", "tokens": ["Auf", "dem", "gr\u00fc\u00b7nen", "Ra\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Abends drauf von meinen Knien", "tokens": ["A\u00b7bends", "drauf", "von", "mei\u00b7nen", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sp\u00e4ht' er nach den Sternen;", "tokens": ["Sp\u00e4ht'", "er", "nach", "den", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jeden, der am Himmel schien,", "tokens": ["Je\u00b7den", ",", "der", "am", "Him\u00b7mel", "schien", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollt' er kennen lernen.", "tokens": ["Wollt'", "er", "ken\u00b7nen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbgute Nacht nun! Morgen dann", "tokens": ["\u00bb", "gu\u00b7te", "Nacht", "nun", "!", "Mor\u00b7gen", "dann"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "ADV", "$.", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir erz\u00e4hlst du weiter!\u00ab", "tokens": ["Mir", "er\u00b7z\u00e4hlst", "du", "wei\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und er l\u00e4chelte mich an,", "tokens": ["Und", "er", "l\u00e4\u00b7chel\u00b7te", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "H\u00fcpfte fort so heiter;", "tokens": ["H\u00fcpf\u00b7te", "fort", "so", "hei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gestern noch so frisch im Glanz", "tokens": ["Ge\u00b7stern", "noch", "so", "frisch", "im", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Seiner sieben Jahre,", "tokens": ["Sei\u00b7ner", "sie\u00b7ben", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Liegt er heut, der kleine Franz,", "tokens": ["Liegt", "er", "heut", ",", "der", "klei\u00b7ne", "Franz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der Totenbahre.", "tokens": ["Auf", "der", "To\u00b7ten\u00b7bah\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zarter Knabe, der du bang", "tokens": ["Zar\u00b7ter", "Kna\u00b7be", ",", "der", "du", "bang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst im Finstern zagtest,", "tokens": ["Sonst", "im", "Fins\u00b7tern", "zag\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sprich, wie du den gro\u00dfen Gang", "tokens": ["Sprich", ",", "wie", "du", "den", "gro\u00b7\u00dfen", "Gang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das Dunkel wagtest?", "tokens": ["Durch", "das", "Dun\u00b7kel", "wag\u00b7test", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wagtest, in den Schlund, davor", "tokens": ["Wag\u00b7test", ",", "in", "den", "Schlund", ",", "da\u00b7vor"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "$,", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle zitternd stehen,", "tokens": ["Al\u00b7le", "zit\u00b7ternd", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Durch das schwarzverh\u00e4ngte Thor", "tokens": ["Durch", "das", "schwarz\u00b7ver\u00b7h\u00e4ng\u00b7te", "Thor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "So allein zu gehen?", "tokens": ["So", "al\u00b7lein", "zu", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Seit dem letzten Sonnenstrahl", "tokens": ["Seit", "dem", "letz\u00b7ten", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie weit die Reise!", "tokens": ["O", "wie", "weit", "die", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weiter, weiter tausendmal,", "tokens": ["Wei\u00b7ter", ",", "wei\u00b7ter", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als vom Kind zum Greise!", "tokens": ["Als", "vom", "Kind", "zum", "Grei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "J\u00fcngst erst auf der Mutter Scho\u00df,", "tokens": ["J\u00fcngst", "erst", "auf", "der", "Mut\u00b7ter", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr am Busen lagst du,", "tokens": ["Ihr", "am", "Bu\u00b7sen", "lagst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nun die Gr\u00f6\u00dften riesengro\u00df", "tokens": ["Nun", "die", "Gr\u00f6\u00df\u00b7ten", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Pl\u00f6tzlich \u00fcberragst du.", "tokens": ["Pl\u00f6tz\u00b7lich", "\u00fc\u00b7berr\u00b7agst", "du", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und mit allem, was ich kann,", "tokens": ["Und", "mit", "al\u00b7lem", ",", "was", "ich", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich bin und habe,", "tokens": ["Was", "ich", "bin", "und", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts vermag ich dir fortan", "tokens": ["Nichts", "ver\u00b7mag", "ich", "dir", "for\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr zu lehren, Knabe;", "tokens": ["Mehr", "zu", "leh\u00b7ren", ",", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Weiser du als Sokrates,", "tokens": ["Wei\u00b7ser", "du", "als", "Sok\u00b7ra\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich an Geist erblindet,", "tokens": ["Ich", "an", "Geist", "er\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Alles, alles wei\u00dft du es,", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "wei\u00dft", "du", "es", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was wir nie ergr\u00fcndet.", "tokens": ["Was", "wir", "nie", "er\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "L\u00e4chelnd blickst auf uns du nun,", "tokens": ["L\u00e4\u00b7chelnd", "blickst", "auf", "uns", "du", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denen du entrissen;", "tokens": ["De\u00b7nen", "du", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kindisch d\u00fcnkt dich unser Thun,", "tokens": ["Kin\u00b7disch", "d\u00fcnkt", "dich", "un\u00b7ser", "Thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Sein und Wissen.", "tokens": ["Un\u00b7ser", "Sein", "und", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seit du \u00fcber mich so hoch", "tokens": ["Seit", "du", "\u00fc\u00b7ber", "mich", "so", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bist erh\u00f6ht, o Kleiner,", "tokens": ["Bist", "er\u00b7h\u00f6ht", ",", "o", "Klei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "FM", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nur mit heil'gem Schauer noch", "tokens": ["Nur", "mit", "heil'\u00b7gem", "Schau\u00b7er", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denken kann ich deiner.", "tokens": ["Den\u00b7ken", "kann", "ich", "dei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}