{"textgrid.poem.56457": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Palmyras Tempelstaub bl\u00e4st auf der Wind,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmyras Tempelstaub bl\u00e4st auf der Wind,", "tokens": ["Pal\u00b7my\u00b7ras", "Tem\u00b7pel\u00b7staub", "bl\u00e4st", "auf", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der durch die Hallen s\u00e4uselt in der Zeit", "tokens": ["Der", "durch", "die", "Hal\u00b7len", "s\u00e4u\u00b7selt", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des leeren Mittags, wo die Sonne weit", "tokens": ["Des", "lee\u00b7ren", "Mit\u00b7tags", ",", "wo", "die", "Son\u00b7ne", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Blauen rast. Der goldene Atem spinnt,", "tokens": ["Im", "Blau\u00b7en", "rast", ".", "Der", "gol\u00b7de\u00b7ne", "A\u00b7tem", "spinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der goldene Staub des Mittags sich wie Rauch", "tokens": ["Der", "gol\u00b7de\u00b7ne", "Staub", "des", "Mit\u00b7tags", "sich", "wie", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "KOKOM", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Glanz der W\u00fcste, wie ein seidenes Zelt", "tokens": ["Im", "Glanz", "der", "W\u00fcs\u00b7te", ",", "wie", "ein", "sei\u00b7de\u00b7nes", "Zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PWAV", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der ungeheuren Fl\u00e4che. Dach der Welt.", "tokens": ["Der", "un\u00b7ge\u00b7heu\u00b7ren", "Fl\u00e4\u00b7che", ".", "Dach", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ferne Fl\u00f6ten t\u00f6nt des Zephirs Hauch,", "tokens": ["Wie", "fer\u00b7ne", "Fl\u00f6\u00b7ten", "t\u00f6nt", "des", "Ze\u00b7phirs", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und leise singt der Sand. Doch unverweilt", "tokens": ["Und", "lei\u00b7se", "singt", "der", "Sand", ".", "Doch", "un\u00b7ver\u00b7weilt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jagt hoch das Licht. Damaskus' Rosenduft", "tokens": ["Jagt", "hoch", "das", "Licht", ".", "Da\u00b7mas\u00b7kus'", "Ro\u00b7sen\u00b7duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schl\u00e4gt auf wie eine Woge in die Luft,", "tokens": ["Schl\u00e4gt", "auf", "wie", "ei\u00b7ne", "Wo\u00b7ge", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Flamme, die den \u00c4ther teilt.", "tokens": ["Wie", "ei\u00b7ne", "Flam\u00b7me", ",", "die", "den", "\u00c4\u00b7ther", "teilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der wei\u00dfen Stiere roter Blutsaft sch\u00e4umt", "tokens": ["Der", "wei\u00b7\u00dfen", "Stie\u00b7re", "ro\u00b7ter", "Blu\u00b7tsaft", "sch\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Tempelh\u00f6fen, wo das Volk im Kranz", "tokens": ["Auf", "Tem\u00b7pel\u00b7h\u00f6\u00b7fen", ",", "wo", "das", "Volk", "im", "Kranz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Blutes Regen f\u00fchlt, und seinen Glanz,", "tokens": ["Des", "Blu\u00b7tes", "Re\u00b7gen", "f\u00fchlt", ",", "und", "sei\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der mit Rubinen ihre Togen s\u00e4umt.", "tokens": ["Der", "mit", "Ru\u00b7bi\u00b7nen", "ih\u00b7re", "To\u00b7gen", "s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein T\u00e4nzer tanzt im blauen Mittagsrot", "tokens": ["Ein", "T\u00e4n\u00b7zer", "tanzt", "im", "blau\u00b7en", "Mit\u00b7tags\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf wei\u00dfer Platte, der vom Strahle trank. \u2013", "tokens": ["Auf", "wei\u00b7\u00dfer", "Plat\u00b7te", ",", "der", "vom", "Strah\u00b7le", "trank", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Licht entflieht. Der Libanon versank,", "tokens": ["Das", "Licht", "ent\u00b7flieht", ".", "Der", "Li\u00b7ba\u00b7non", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Zedern Haus, das sich dem Gotte bot.", "tokens": ["Der", "Ze\u00b7dern", "Haus", ",", "das", "sich", "dem", "Got\u00b7te", "bot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und westw\u00e4rts eilt der Tag. Mit tiefem Gold", "tokens": ["Und", "west\u00b7w\u00e4rts", "eilt", "der", "Tag", ".", "Mit", "tie\u00b7fem", "Gold"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist weit des Westens W\u00f6lbung angef\u00fcllt:", "tokens": ["Ist", "weit", "des", "Wes\u00b7tens", "W\u00f6l\u00b7bung", "an\u00b7ge\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Gottes Rundschild, der die Schultern h\u00fcllt", "tokens": ["Des", "Got\u00b7tes", "Rund\u00b7schild", ",", "der", "die", "Schul\u00b7tern", "h\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Fl\u00fcchtigen. Sein blauer Helmbusch rollt", "tokens": ["Des", "Fl\u00fcch\u00b7ti\u00b7gen", ".", "Sein", "blau\u00b7er", "Helm\u00b7busch", "rollt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Darob im Sturme weit am Horizont,", "tokens": ["Da\u00b7rob", "im", "Stur\u00b7me", "weit", "am", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Meer, und seiner Inseln Perlenseil.", "tokens": ["Am", "Meer", ",", "und", "sei\u00b7ner", "In\u00b7seln", "Per\u00b7len\u00b7seil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er eilt dahin, wo schon der Ida steil", "tokens": ["Er", "eilt", "da\u00b7hin", ",", "wo", "schon", "der", "I\u00b7da", "steil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PWAV", "ADV", "ART", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Eichen tost und dr\u00f6hnt der Hellespont.", "tokens": ["Mit", "Ei\u00b7chen", "tost", "und", "dr\u00f6hnt", "der", "Hel\u00b7les\u00b7pont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Stromland fort, dem gr\u00fcnen Abend zu.", "tokens": ["Das", "Strom\u00b7land", "fort", ",", "dem", "gr\u00fc\u00b7nen", "A\u00b7bend", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie der Drommete Ton erschallt sein Gang", "tokens": ["Wie", "der", "Drom\u00b7me\u00b7te", "Ton", "er\u00b7schallt", "sein", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "An Ossas Echo. Troas Schilf entlang,", "tokens": ["An", "Os\u00b7sas", "E\u00b7cho", ".", "Troas", "Schilf", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$.", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In rote W\u00e4lder tritt sein Purpurschuh,", "tokens": ["In", "ro\u00b7te", "W\u00e4l\u00b7der", "tritt", "sein", "Pur\u00b7pur\u00b7schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "In Sammetwiesen weich. Dem Feuer nach,", "tokens": ["In", "Sam\u00b7met\u00b7wie\u00b7sen", "weich", ".", "Dem", "Feu\u00b7er", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$.", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das einst gen Argos flog, tritt machtvoll er", "tokens": ["Das", "einst", "gen", "Ar\u00b7gos", "flog", ",", "tritt", "macht\u00b7voll", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NE", "VVFIN", "$,", "VVFIN", "VVFIN", "PPER"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf Chalkis hin. Darunter rauscht das Meer", "tokens": ["Auf", "Chal\u00b7kis", "hin", ".", "Da\u00b7run\u00b7ter", "rauscht", "das", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "PTKVZ", "$.", "PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hervor aus gr\u00fcner Grotten Steingemach.", "tokens": ["Her\u00b7vor", "aus", "gr\u00fc\u00b7ner", "Grot\u00b7ten", "Stein\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sein Arm, den er auf Meer und Lande streckt,", "tokens": ["Sein", "Arm", ",", "den", "er", "auf", "Meer", "und", "Lan\u00b7de", "streckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ragt dunkel auf wie eine Feuersbrunst.", "tokens": ["Ragt", "dun\u00b7kel", "auf", "wie", "ei\u00b7ne", "Feu\u00b7ers\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Atem f\u00fcllt das Meer mit schwarzem Dunst,", "tokens": ["Sein", "A\u00b7tem", "f\u00fcllt", "das", "Meer", "mit", "schwar\u00b7zem", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des wei\u00dfes Maul die roten Sohlen leckt.", "tokens": ["Des", "wei\u00b7\u00dfes", "Maul", "die", "ro\u00b7ten", "Soh\u00b7len", "leckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Auf Marathon schleppt seines Mantels Saum,", "tokens": ["Auf", "Ma\u00b7ra\u00b7thon", "schleppt", "sei\u00b7nes", "Man\u00b7tels", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein violetter Streif, wo schon das Horn", "tokens": ["Ein", "vi\u00b7o\u00b7let\u00b7ter", "Streif", ",", "wo", "schon", "das", "Horn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Muschel stimmt am Strand der Toten vorn", "tokens": ["Der", "Mu\u00b7schel", "stimmt", "am", "Strand", "der", "To\u00b7ten", "vorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Sturmgott laut aus wei\u00dfer Brandung Schaum.", "tokens": ["Der", "Sturm\u00b7gott", "laut", "aus", "wei\u00b7\u00dfer", "Bran\u00b7dung", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Des Rohres rote Fahnen r\u00fchrt der Wind", "tokens": ["Des", "Roh\u00b7res", "ro\u00b7te", "Fah\u00b7nen", "r\u00fchrt", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von seines Fu\u00dfes Fittich um am Strand", "tokens": ["Von", "sei\u00b7nes", "Fu\u00b7\u00dfes", "Fit\u00b7tich", "um", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der fernen Elis, da der Nacht Trabant,", "tokens": ["Der", "fer\u00b7nen", "E\u00b7lis", ",", "da", "der", "Nacht", "Tra\u00b7bant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schildknappe Mond, den dunklen Pfad beginnt.", "tokens": ["Schild\u00b7knap\u00b7pe", "Mond", ",", "den", "dunk\u00b7len", "Pfad", "be\u00b7ginnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}