{"textgrid.poem.53159": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer auff Gottes Wegen wandelt", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer auff Gottes Wegen wandelt", "tokens": ["Wer", "auff", "Got\u00b7tes", "We\u00b7gen", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd in seiner Furcht sich h\u00e4lt,", "tokens": ["Vnd", "in", "sei\u00b7ner", "Furcht", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles, was er sinnt vnd handelt,", "tokens": ["Al\u00b7les", ",", "was", "er", "sinnt", "vnd", "han\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff den Grund der Vnschuld stellt,", "tokens": ["Auff", "den", "Grund", "der", "Vn\u00b7schuld", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der ist warlich wol daran", "tokens": ["Der", "ist", "war\u00b7lich", "wol", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd ein Segen-reicher Mann.", "tokens": ["Vnd", "ein", "Se\u00b7gen\u00b7rei\u00b7cher", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer du bist, du wirst dich nehren", "tokens": ["Wer", "du", "bist", ",", "du", "wirst", "dich", "neh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Arbeit deiner Hand,", "tokens": ["Von", "der", "Ar\u00b7beit", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie wird reichlich dir gewehren", "tokens": ["Sie", "wird", "reich\u00b7lich", "dir", "ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beydes Gut vnd Ehren-Stand,", "tokens": ["Bey\u00b7des", "Gut", "vnd", "Eh\u00b7ren\u00b7Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wol dir, was dein Vorsatz thut,", "tokens": ["Wol", "dir", ",", "was", "dein", "Vor\u00b7satz", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vberall hast du es gut!", "tokens": ["Vbe\u00b7rall", "hast", "du", "es", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Dein geliebtes Weib wird eben", "tokens": ["Dein", "ge\u00b7lieb\u00b7tes", "Weib", "wird", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vmb dein reiches Haus her seyn", "tokens": ["Vmb", "dein", "rei\u00b7ches", "Haus", "her", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "APZR", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Stock mit fruchtbarn Reben,", "tokens": ["Wie", "ein", "Stock", "mit", "frucht\u00b7barn", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Kinder gro\u00df vnd klein", "tokens": ["Dei\u00b7ne", "Kin\u00b7der", "gro\u00df", "vnd", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Oelzweig' ohn gefehr", "tokens": ["Wie", "die", "O\u00b7el\u00b7zweig'", "ohn", "ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Deinen vollen Tisch umbher.", "tokens": ["Dei\u00b7nen", "vol\u00b7len", "Tisch", "um\u00b7bher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Siehe, mit so grossem Segen", "tokens": ["Sie\u00b7he", ",", "mit", "so", "gros\u00b7sem", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird begabt ein solcher Mann,", "tokens": ["Wird", "be\u00b7gabt", "ein", "sol\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmt er nur sich aller wegen", "tokens": ["Nimmt", "er", "nur", "sich", "al\u00b7ler", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "PIAT", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fein der Furcht des H\u00f6chsten an,", "tokens": ["Fein", "der", "Furcht", "des", "H\u00f6chs\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott, der Frommen Schild vnd Lohn,", "tokens": ["Gott", ",", "der", "From\u00b7men", "Schild", "vnd", "Lohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird dich segnen aus Sion.", "tokens": ["Wird", "dich", "seg\u00b7nen", "aus", "Si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Was Jerusalem wird bawen", "tokens": ["Was", "Je\u00b7ru\u00b7sa\u00b7lem", "wird", "ba\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIS", "VAFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihren Schmuck, jhr Gl\u00fcck vnd Ruh", "tokens": ["Ih\u00b7ren", "Schmuck", ",", "jhr", "Gl\u00fcck", "vnd", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst du, weil du lebest, schawen,", "tokens": ["Wirst", "du", ",", "weil", "du", "le\u00b7best", ",", "scha\u00b7wen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kindes-Kinder auch darzu:", "tokens": ["Kin\u00b7des\u00b7Kin\u00b7der", "auch", "dar\u00b7zu", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es wird schweben Lust vnd Zier", "tokens": ["Es", "wird", "schwe\u00b7ben", "Lust", "vnd", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vber Israel vnd Dir.", "tokens": ["Vber", "Is\u00b7rael", "vnd", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}