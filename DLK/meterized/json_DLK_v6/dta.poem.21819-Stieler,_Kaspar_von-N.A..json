{"dta.poem.21819": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Und h\u00e4tte dir di\u00df Werk/ Geehrter/ nicht ge-", "tokens": ["Und", "h\u00e4t\u00b7te", "dir", "di\u00df", "Werk", "/", "Ge\u00b7ehr\u00b7ter", "/", "nicht", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PDS", "NN", "$(", "NN", "$(", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "fallen/", "tokens": ["fal\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "so st\u00fcnd\u2019 es nicht befel\u00dft in seiner Zierligkeit.", "tokens": ["so", "st\u00fcnd'", "es", "nicht", "be\u00b7fel\u00dft", "in", "sei\u00b7ner", "Zier\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir dankt es nur allein sein\u2019 erstgebohrne", "tokens": ["Dir", "dankt", "es", "nur", "al\u00b7lein", "sein'", "erst\u00b7ge\u00b7bohr\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zeit/", "tokens": ["Zeit", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "auf-fort-wachs/ ia sich ganz. Dir h\u00e4tt\u2019 au\u00df an-", "tokens": ["auf\u00b7for\u00b7twachs", "/", "i\u00b7a", "sich", "ganz", ".", "Dir", "h\u00e4tt'", "au\u00df", "an"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "NE", "PRF", "ADV", "$.", "PPER", "VAFIN", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "dern allen", "tokens": ["dern", "al\u00b7len"], "token_info": ["word", "word"], "pos": ["ADV", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "der Ober-plaz geb\u00fchrt: doch nim es an f\u00fcr", "tokens": ["der", "O\u00b7ber\u00b7plaz", "ge\u00b7b\u00fchrt", ":", "doch", "nim", "es", "an", "f\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "ADV", "VVIMP", "PPER", "APPR", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Willen", "tokens": ["Wil\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "(zu.", "tokens": ["(", "zu", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "da\u00df ich den sechsten Ort dir dienstlich neige", "tokens": ["da\u00df", "ich", "den", "sechs\u00b7ten", "Ort", "dir", "dienst\u00b7lich", "nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das ganze Buch ist dein. Was ich hier-", "tokens": ["Das", "gan\u00b7ze", "Buch", "ist", "dein", ".", "Was", "ich", "hier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$.", "PWS", "PPER", "TRUNC"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "unter tuh'", "tokens": ["un\u00b7ter", "tuh'"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "ist/ meiner Sch\u00fcldigkeit Bezw\u00e4ngn\u00fc\u00df zu er-", "tokens": ["ist", "/", "mei\u00b7ner", "Sch\u00fcl\u00b7dig\u00b7keit", "Be\u00b7zw\u00e4n\u00b7gn\u00fc\u00df", "zu", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$(", "PPOSAT", "NN", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "f\u00fcllen/", "tokens": ["f\u00fcl\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "(gelten/", "tokens": ["(", "gel\u00b7ten", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Nim meine Neigung an/ und la\u00df mich ferner", "tokens": ["Nim", "mei\u00b7ne", "Nei\u00b7gung", "an", "/", "und", "la\u00df", "mich", "fer\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPR", "$(", "KON", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "die Venus wird nicht nur von mir besun-", "tokens": ["die", "Ve\u00b7nus", "wird", "nicht", "nur", "von", "mir", "be\u00b7sun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "gen sein/", "tokens": ["gen", "sein", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$("], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "(ein/", "tokens": ["(", "ein", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ART", "$("], "meter": "-", "measure": "single.down"}, "line.21": {"text": "iezt schwazzt Minerve mir ein anders Treibe\u0303", "tokens": ["iezt", "schwazzt", "Mi\u00b7ner\u00b7ve", "mir", "ein", "an\u00b7ders", "Treib\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ART", "ADV", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "forthin entzieh\u2019 ich mich des Zypripors Ge-", "tokens": ["for\u00b7thin", "ent\u00b7zieh'", "ich", "mich", "des", "Zyp\u00b7ri\u00b7pors", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "w\u00e4lten.", "tokens": ["w\u00e4l\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "(leiden/", "tokens": ["(", "lei\u00b7den", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Ist mir die Fame stumm: hier wil ichs gerne", "tokens": ["Ist", "mir", "die", "Fa\u00b7me", "stumm", ":", "hier", "wil", "ichs", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$.", "ADV", "VMFIN", "PIS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "ich hei\u00df\u2019 auch allzeit nicht der Hirte Filidor.", "tokens": ["ich", "hei\u00df'", "auch", "all\u00b7zeit", "nicht", "der", "Hir\u00b7te", "Fi\u00b7li\u00b7dor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Nahm\u2019 ist sonst bekant. Wenn mich", "tokens": ["Mein", "Nahm'", "ist", "sonst", "be\u00b7kant", ".", "Wenn", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "der Musen Chor", "tokens": ["der", "Mu\u00b7sen", "Chor"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "bey Zirren reiner Fluht lie\u00df um Parnassen", "tokens": ["bey", "Zir\u00b7ren", "rei\u00b7ner", "Fluht", "lie\u00df", "um", "Par\u00b7nas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "weiden.", "tokens": ["wei\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Hier such\u2019 ich keinen Ruhm. Doch/ wen\u0303 auch", "tokens": ["Hier", "such'", "ich", "kei\u00b7nen", "Ruhm", ".", "Doch", "/", "we\u00f1", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "KON", "$(", "KOUS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Marzialens", "tokens": ["Mar\u00b7zi\u00b7a\u00b7lens"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "wird bey der Welt gedacht: so steifft die", "tokens": ["wird", "bey", "der", "Welt", "ge\u00b7dacht", ":", "so", "steifft", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Hoffnung mich/", "tokens": ["Hoff\u00b7nung", "mich", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "es werde mich nicht ganz erstechen Morten", "tokens": ["es", "wer\u00b7de", "mich", "nicht", "ganz", "er\u00b7ste\u00b7chen", "Mor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Stich/", "tokens": ["Stich", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.37": {"text": "st\u00fcrb\u2019 ich schon diesen Tag. Die Wahre darff", "tokens": ["st\u00fcrb'", "ich", "schon", "die\u00b7sen", "Tag", ".", "Die", "Wah\u00b7re", "darff"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$.", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "des Prahlens", "tokens": ["des", "Prah\u00b7lens"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "die Wehrt und selten ist. Gemeine Hempel-", "tokens": ["die", "Wehrt", "und", "sel\u00b7ten", "ist", ".", "Ge\u00b7mei\u00b7ne", "Hem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "ADJD", "VAFIN", "$.", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "M\u00e4nner", "tokens": ["M\u00e4n\u00b7ner"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "das kleine Dokken-Werk wird offters ehr", "tokens": ["das", "klei\u00b7ne", "Dok\u00b7ken\u00b7Werk", "wird", "off\u00b7ters", "ehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "verkaufft/", "tokens": ["ver\u00b7kaufft", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "als das/ wornach man auch in beydes In-", "tokens": ["als", "das", "/", "wor\u00b7nach", "man", "auch", "in", "bey\u00b7des", "In"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "$(", "PWAV", "PIS", "ADV", "APPR", "PIS", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "dien laufft.", "tokens": ["di\u00b7en", "laufft", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Wer schollt\u2019 ie Persen aus/ da\u00df ihn der Fl\u00fc-", "tokens": ["Wer", "schollt'", "ie", "Per\u00b7sen", "aus", "/", "da\u00df", "ihn", "der", "Fl\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "NN", "PTKVZ", "$(", "KOUS", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "gel-renner", "tokens": ["gel\u00b7ren\u00b7ner"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "nach Zefeus Tochter trug. Wir sch\u00e4rfen uns", "tokens": ["nach", "Ze\u00b7feus", "Toch\u00b7ter", "trug", ".", "Wir", "sch\u00e4r\u00b7fen", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "im Lieben.", "tokens": ["im", "Lie\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "ADJA", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.49": {"text": "Das Narr- und Fobel-werkbereitet unserm", "tokens": ["Das", "Narr", "und", "Fo\u00b7bel\u00b7werk\u00b7be\u00b7rei\u00b7tet", "un\u00b7serm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Sinn'", "tokens": ["Sinn'"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.51": {"text": "aus diesen stiegen wir auff grosse Sachen", "tokens": ["aus", "die\u00b7sen", "stie\u00b7gen", "wir", "auff", "gros\u00b7se", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "hin:", "tokens": ["hin", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.53": {"text": "hat Opiz/ Flemming doch und Rist erst so ge-", "tokens": ["hat", "O\u00b7piz", "/", "Flem\u00b7ming", "doch", "und", "Rist", "erst", "so", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "$(", "NE", "ADV", "KON", "NN", "ADV", "ADV", "TRUNC"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.54": {"text": "schrieben/", "tokens": ["schrie\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.55": {"text": "Da\u00df diese M\u00e4nner sich im Dichten mehr ge-", "tokens": ["Da\u00df", "die\u00b7se", "M\u00e4n\u00b7ner", "sich", "im", "Dich\u00b7ten", "mehr", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "PRF", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.56": {"text": "zwungen/", "tokens": ["zwun\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.57": {"text": "gesteh\u2019 ich gern. Mir ist das Urtheil all zu", "tokens": ["ge\u00b7steh'", "ich", "gern", ".", "Mir", "ist", "das", "Ur\u00b7theil", "all", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "PIAT", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.58": {"text": "schwach/", "tokens": ["schwach", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.59": {"text": "so bald der Eyfer wird in meiner Feder", "tokens": ["so", "bald", "der", "Ey\u00b7fer", "wird", "in", "mei\u00b7ner", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.60": {"text": "wach/", "tokens": ["wach", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.61": {"text": "denn wei\u00df ich keinen Halt. Katull hat so ge-", "tokens": ["denn", "wei\u00df", "ich", "kei\u00b7nen", "Halt", ".", "Ka\u00b7tull", "hat", "so", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$.", "NE", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.62": {"text": "sungen,", "tokens": ["sun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.63": {"text": "sein Leben ward gelobt/ das Lied vor hoch ge-", "tokens": ["sein", "Le\u00b7ben", "ward", "ge\u00b7lobt", "/", "das", "Lied", "vor", "hoch", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "APPR", "ADJD