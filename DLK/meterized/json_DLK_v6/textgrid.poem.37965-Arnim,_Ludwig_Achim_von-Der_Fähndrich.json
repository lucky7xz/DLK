{"textgrid.poem.37965": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der F\u00e4hndrich", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marschiert ihr Regiment", "tokens": ["Mar\u00b7schiert", "ihr", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nun in das Feld,", "tokens": ["Nun", "in", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In aller Welt", "tokens": ["In", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Viel Krieg ist heuer zu finden.", "tokens": ["Viel", "Krieg", "ist", "heu\u00b7er", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bey der Frau Wirthin Nachts,", "tokens": ["Bey", "der", "Frau", "Wirt\u00b7hin", "Nachts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sie kehrten ein:", "tokens": ["Sie", "kehr\u00b7ten", "ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbwollen lustig seyn,", "tokens": ["\u00bb", "wol\u00b7len", "lus\u00b7tig", "seyn", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Das M\u00e4dchen schl\u00e4ft allein.\u00ab", "tokens": ["Das", "M\u00e4d\u00b7chen", "schl\u00e4ft", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als das M\u00e4dchen nun", "tokens": ["Und", "als", "das", "M\u00e4d\u00b7chen", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom Schlaf erwacht,", "tokens": ["Vom", "Schlaf", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sich bedacht,", "tokens": ["Und", "sich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Da fing sie an zu weinen.", "tokens": ["Da", "fing", "sie", "an", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbey schwarzbraun M\u00e4dchen sagt,", "tokens": ["\u00bb", "ey", "schwarz\u00b7braun", "M\u00e4d\u00b7chen", "sagt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was weint ihr hier?\u00ab", "tokens": ["Was", "weint", "ihr", "hier", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbein sch\u00f6ner Offizier,", "tokens": ["\u00bb", "ein", "sch\u00f6\u00b7ner", "Of\u00b7fi\u00b7zier", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat mir genommen mein Ehr!\u00ab", "tokens": ["Hat", "mir", "ge\u00b7nom\u00b7men", "mein", "Ehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Der Hauptmann ein braver Mann,", "tokens": ["Der", "Haupt\u00b7mann", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Trommeln r\u00fchrt,", "tokens": ["Die", "Trom\u00b7meln", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Trommeln r\u00fchrt,", "tokens": ["Die", "Trom\u00b7meln", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Feldmarsch l\u00e4st er schlagen.", "tokens": ["Den", "Feld\u00b7marsch", "l\u00e4st", "er", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er lie\u00df marschieren sie,", "tokens": ["Er", "lie\u00df", "mar\u00b7schie\u00b7ren", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu zwey und drey,", "tokens": ["Zu", "zwey", "und", "drey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu drey und zwey,", "tokens": ["Zu", "drey", "und", "zwey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auf da\u00df sie ihn erkenne.", "tokens": ["Auf", "da\u00df", "sie", "ihn", "er\u00b7ken\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbmamsell erkennt ihr ihn?\u00ab", "tokens": ["\u00bb", "mam\u00b7sell", "er\u00b7kennt", "ihr", "ihn", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbich kenn ihn wohl", "tokens": ["\u00bb", "ich", "kenn", "ihn", "wohl"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So sch\u00f6n und voll,", "tokens": ["So", "sch\u00f6n", "und", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Er thut die Fahne schwenken.\u00ab", "tokens": ["Er", "thut", "die", "Fah\u00b7ne", "schwen\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Hauptmann, ein solcher Mann,", "tokens": ["Der", "Haupt\u00b7mann", ",", "ein", "sol\u00b7cher", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Galgen baut,", "tokens": ["Den", "Gal\u00b7gen", "baut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den ihr weit schaut,", "tokens": ["Den", "ihr", "weit", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Den F\u00e4hndrich dran zu h\u00e4ngen.", "tokens": ["Den", "F\u00e4hn\u00b7drich", "dran", "zu", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbo liebster Kammerad,", "tokens": ["\u00bb", "o", "liebs\u00b7ter", "Kam\u00b7me\u00b7rad", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn einer fragt,", "tokens": ["Wenn", "ei\u00b7ner", "fragt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr ihm doch sagt,", "tokens": ["Ihr", "ihm", "doch", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich w\u00e4r mit Ehrn erschossen.\u00ab", "tokens": ["Ich", "w\u00e4r", "mit", "Ehrn", "er\u00b7schos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Des andern Tages kam", "tokens": ["Des", "an\u00b7dern", "Ta\u00b7ges", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des F\u00e4hndrichs Frau:", "tokens": ["Des", "F\u00e4hn\u00b7drichs", "Frau", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbmein Mann nicht schau,", "tokens": ["\u00bb", "mein", "Mann", "nicht", "schau", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wo ist er denn geblieben?\u00ab", "tokens": ["Wo", "ist", "er", "denn", "ge\u00b7blie\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbdort draussen vor dem Thor,\u00ab", "tokens": ["\u00bb", "dort", "draus\u00b7sen", "vor", "dem", "Thor", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie sagten an,", "tokens": ["Sie", "sag\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbden armen Mann,", "tokens": ["\u00bb", "den", "ar\u00b7men", "Mann", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zwey J\u00e4ger ihn erschossen.\u00ab", "tokens": ["Zwey", "J\u00e4\u00b7ger", "ihn", "er\u00b7schos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So geht es in der Welt,", "tokens": ["So", "geht", "es", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn man verliebt,", "tokens": ["Wenn", "man", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn man verliebt,", "tokens": ["Wenn", "man", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mu\u00df man sein Leben lassen.", "tokens": ["Mu\u00df", "man", "sein", "Le\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}