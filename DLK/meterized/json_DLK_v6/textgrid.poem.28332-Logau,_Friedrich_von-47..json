{"textgrid.poem.28332": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey Hofe kan ein guter Koch auch seyn ein guter Rath;", "tokens": ["Bey", "Ho\u00b7fe", "kan", "ein", "gu\u00b7ter", "Koch", "auch", "seyn", "ein", "gu\u00b7ter", "Rath", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "ADJA", "NE", "ADV", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Er wei\u00df, was seinem Herren schmeckt, und was er gerne hat;", "tokens": ["Er", "wei\u00df", ",", "was", "sei\u00b7nem", "Her\u00b7ren", "schmeckt", ",", "und", "was", "er", "ger\u00b7ne", "hat", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PWS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Er tr\u00e4gt verdecktes Essen auff und Essen nur zu schau,", "tokens": ["Er", "tr\u00e4gt", "ver\u00b7deck\u00b7tes", "Es\u00b7sen", "auff", "und", "Es\u00b7sen", "nur", "zu", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "KON", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Geust S\u00f6der auff und Senff daran, die dienlich f\u00fcr den grau;", "tokens": ["Geust", "S\u00f6\u00b7der", "auff", "und", "Senff", "da\u00b7ran", ",", "die", "dien\u00b7lich", "f\u00fcr", "den", "grau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "KON", "NN", "PAV", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Auffs bittre streut er Zucker her; das magre w\u00fcrtzt er wol;", "tokens": ["Auffs", "bitt\u00b7re", "streut", "er", "Zu\u00b7cker", "her", ";", "das", "mag\u00b7re", "w\u00fcrtzt", "er", "wol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "ART", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Dem Herren werden Ohren satt und ihm der Beutel voll;", "tokens": ["Dem", "Her\u00b7ren", "wer\u00b7den", "Oh\u00b7ren", "satt", "und", "ihm", "der", "Beu\u00b7tel", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADJD", "KON", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Die Kammer geht zur K\u00fcche zu, die Wirthschafft in das Fa\u00df,", "tokens": ["Die", "Kam\u00b7mer", "geht", "zur", "K\u00fc\u00b7che", "zu", ",", "die", "Wirth\u00b7schafft", "in", "das", "Fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Die Cantzeley h\u00e4lt Fasten-Zeit, der lechzend Untersa\u00df", "tokens": ["Die", "Cant\u00b7ze\u00b7ley", "h\u00e4lt", "Fas\u00b7ten\u00b7Zeit", ",", "der", "lech\u00b7zend", "Un\u00b7ter\u00b7sa\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Mag lauffen, kan er sitzen nicht: Die gantze Policey", "tokens": ["Mag", "lauf\u00b7fen", ",", "kan", "er", "sit\u00b7zen", "nicht", ":", "Die", "gant\u00b7ze", "Po\u00b7li\u00b7cey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Wird Heucheley, Betriegerey und K\u00fcchen-Meisterey.", "tokens": ["Wird", "Heu\u00b7che\u00b7ley", ",", "Be\u00b7trie\u00b7ge\u00b7rey", "und", "K\u00fc\u00b7chen\u00b7Meis\u00b7te\u00b7rey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}