{"textgrid.poem.42354": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wandre in der grossen Stadt. Ein tr\u00fcber", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandre in der grossen Stadt. Ein tr\u00fcber", "tokens": ["Ich", "wand\u00b7re", "in", "der", "gros\u00b7sen", "Stadt", ".", "Ein", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herbstnebelschleier flattert um die Zinnen,", "tokens": ["Herbst\u00b7ne\u00b7bel\u00b7schlei\u00b7er", "flat\u00b7tert", "um", "die", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Tagwerk schwirrt und braust vor meinen Sinnen,", "tokens": ["Das", "Tag\u00b7werk", "schwirrt", "und", "braust", "vor", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tausend Menschen gehn an mir vor\u00fcber.", "tokens": ["Und", "tau\u00b7send", "Men\u00b7schen", "gehn", "an", "mir", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich kenn sie nicht. Wer sind die Vielen? Tragen", "tokens": ["Ich", "kenn", "sie", "nicht", ".", "Wer", "sind", "die", "Vie\u00b7len", "?", "Tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PWS", "VAFIN", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie in der Brust ein Los wie meins? Und blutet", "tokens": ["Sie", "in", "der", "Brust", "ein", "Los", "wie", "meins", "?", "Und", "blu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "KOKOM", "NE", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Herz vielleicht, von mir so unvermutet,", "tokens": ["Ihr", "Herz", "viel\u00b7leicht", ",", "von", "mir", "so", "un\u00b7ver\u00b7mu\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ihnen fremd ist meines Herzens Schlagen?", "tokens": ["Als", "ih\u00b7nen", "fremd", "ist", "mei\u00b7nes", "Her\u00b7zens", "Schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Nebel tropft. Wir alle wandern, wandern.", "tokens": ["Der", "Ne\u00b7bel", "tropft", ".", "Wir", "al\u00b7le", "wan\u00b7dern", ",", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "PIS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dir zu mir erhellt kein Blitz die Tiefen.", "tokens": ["Von", "dir", "zu", "mir", "er\u00b7hellt", "kein", "Blitz", "die", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn wir uns das Wort entgegenriefen \u2013", "tokens": ["Und", "wenn", "wir", "uns", "das", "Wort", "ent\u00b7ge\u00b7gen\u00b7rie\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es stirbt im Wind und keiner weiss vom andern.", "tokens": ["Es", "stirbt", "im", "Wind", "und", "kei\u00b7ner", "weiss", "vom", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "PIS", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}