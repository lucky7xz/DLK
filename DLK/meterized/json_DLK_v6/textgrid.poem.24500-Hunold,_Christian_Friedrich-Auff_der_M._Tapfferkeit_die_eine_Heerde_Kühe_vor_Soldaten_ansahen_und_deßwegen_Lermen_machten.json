{"textgrid.poem.24500": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auff der M. Tapfferkeit die eine Heerde K\u00fche vor Soldaten ansahen und de\u00dfwegen Lermen machten", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bald nur das Ger\u00fccht von Wand: \u2013 \u2013 kam/", "tokens": ["So", "bald", "nur", "das", "Ge\u00b7r\u00fccht", "von", "Wand", ":", "\u2013", "\u2013", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPR", "NN", "$.", "$(", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df etwas Volck daraus auf M \u2013 \u2013 wolte ziehen/", "tokens": ["Da\u00df", "et\u00b7was", "Volck", "da\u00b7raus", "auf", "M", "\u2013", "\u2013", "wol\u00b7te", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PAV", "APPR", "NE", "$(", "$(", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So sahe man bereits ein tapfferes bem\u00fchen/", "tokens": ["So", "sa\u00b7he", "man", "be\u00b7reits", "ein", "tapf\u00b7fe\u00b7res", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie die Helden Zahl dasselbst die Waffen nahm.", "tokens": ["Und", "wie", "die", "Hel\u00b7den", "Zahl", "das\u00b7selbst", "die", "Waf\u00b7fen", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Wache ward das Schlo\u00df/ das feste/ wohl besetzt/", "tokens": ["Mit", "Wa\u00b7che", "ward", "das", "Schlo\u00df", "/", "das", "fes\u00b7te", "/", "wohl", "be\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$(", "ART", "ADJA", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und als dieselbige ein rothes Heer erblickte/", "tokens": ["Und", "als", "die\u00b7sel\u00b7bi\u00b7ge", "ein", "ro\u00b7thes", "Heer", "er\u00b7blick\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von Lachen Berge her/ das immer n\u00e4her r\u00fcckte/", "tokens": ["Von", "La\u00b7chen", "Ber\u00b7ge", "her", "/", "das", "im\u00b7mer", "n\u00e4\u00b7her", "r\u00fcck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$(", "PDS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kuallt ein Losungs Schu\u00df/ der Degen ward gewetzt.", "tokens": ["So", "ku\u00b7allt", "ein", "Lo\u00b7sungs", "Schu\u00df", "/", "der", "De\u00b7gen", "ward", "ge\u00b7wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die Trommel ward ger\u00fchrt/ ", "tokens": ["Die", "Trom\u00b7mel", "ward", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Man schwur: dem ", "tokens": ["Man", "schwur", ":", "dem"], "token_info": ["word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "$.", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Und endlich kam der Feind der Vestung ziemlich nah.", "tokens": ["Und", "end\u00b7lich", "kam", "der", "Feind", "der", "Ves\u00b7tung", "ziem\u00b7lich", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er schlug sein Lager gleich auff einer Wiese hin.", "tokens": ["Er", "schlug", "sein", "La\u00b7ger", "gleich", "auff", "ei\u00b7ner", "Wie\u00b7se", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch schlagen wolt er nicht/ er gieng nur", "tokens": ["Doch", "schla\u00b7gen", "wolt", "er", "nicht", "/", "er", "gieng", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$(", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und weil der ", "tokens": ["Und", "weil", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "So rieff er: M \u2013 \u2013 hat was tapfferes im Sinn.", "tokens": ["So", "rieff", "er", ":", "M", "\u2013", "\u2013", "hat", "was", "tapf\u00b7fe\u00b7res", "im", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "$(", "$(", "VAFIN", "PIS", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Denn einer Schlacht verlohnt sich jetzo wohl die M\u00fche.", "tokens": ["Denn", "ei\u00b7ner", "Schlacht", "ver\u00b7lohnt", "sich", "jet\u00b7zo", "wohl", "die", "M\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wer aber war der Feind? die Wand \u2013 \u2013 K\u00fche.", "tokens": ["Wer", "a\u00b7ber", "war", "der", "Feind", "?", "die", "Wand", "\u2013", "\u2013", "K\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$(", "$(", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}