{"textgrid.poem.54237": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hochwohlgebohrne Frau,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochwohlgebohrne Frau,", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ne", "Frau", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erlaube da\u00df ich schreibe,", "tokens": ["Er\u00b7lau\u00b7be", "da\u00df", "ich", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warum ich diesesmahl so ungehorsam bin.", "tokens": ["Wa\u00b7rum", "ich", "die\u00b7ses\u00b7mahl", "so", "un\u00b7ge\u00b7hor\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach, glaube, da\u00df ich mir betr\u00fcbt die Zeit vertreibe,", "tokens": ["Ach", ",", "glau\u00b7be", ",", "da\u00df", "ich", "mir", "be\u00b7tr\u00fcbt", "die", "Zeit", "ver\u00b7trei\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich denck ohn Unterla\u00df nach deinen N-- hin.", "tokens": ["Ich", "denck", "ohn", "Un\u00b7ter\u00b7la\u00df", "nach", "dei\u00b7nen", "N\u00b7", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "TRUNC", "PTKVZ", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.6": {"text": "Mein Hertz ist mi\u00dfvergn\u00fcgt, das Schicksal will nicht leiden,", "tokens": ["Mein", "Hertz", "ist", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", ",", "das", "Schick\u00b7sal", "will", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df ich von N-- geh; ein unverhoffter Streich,", "tokens": ["Da\u00df", "ich", "von", "N\u00b7", "geh", ";", "ein", "un\u00b7ver\u00b7hoff\u00b7ter", "Streich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "TRUNC", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.8": {"text": "Scheint leider! meine Lust und Gl\u00fccke zu beneiden,", "tokens": ["Scheint", "lei\u00b7der", "!", "mei\u00b7ne", "Lust", "und", "Gl\u00fc\u00b7cke", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und nichts ist dem Verdru\u00df, den ich erdulte, gleich.", "tokens": ["Und", "nichts", "ist", "dem", "Ver\u00b7dru\u00df", ",", "den", "ich", "er\u00b7dul\u00b7te", ",", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zwar war ich schon geschickt und auch bereit zum reissen,", "tokens": ["Zwar", "war", "ich", "schon", "ge\u00b7schickt", "und", "auch", "be\u00b7reit", "zum", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "KON", "ADV", "ADJD", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein mein Vorsatz ward gehemmet und gest\u00f6hrt;", "tokens": ["Al\u00b7lein", "mein", "Vor\u00b7satz", "ward", "ge\u00b7hem\u00b7met", "und", "ge\u00b7st\u00f6hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch wirst du mich darum nicht eigensinnig heissen,", "tokens": ["Doch", "wirst", "du", "mich", "da\u00b7rum", "nicht", "ei\u00b7gen\u00b7sin\u00b7nig", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "PAV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weil dieses Laster mir mit recht nicht zugeh\u00f6rt.", "tokens": ["Weil", "die\u00b7ses", "Las\u00b7ter", "mir", "mit", "recht", "nicht", "zu\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "APPR", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht meyne, da\u00df ich mich mit falschen Federn schm\u00fccke,", "tokens": ["Nicht", "mey\u00b7ne", ",", "da\u00df", "ich", "mich", "mit", "fal\u00b7schen", "Fe\u00b7dern", "schm\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du kennest schon mein Hertz und seine Redlichkeit,", "tokens": ["Du", "ken\u00b7nest", "schon", "mein", "Hertz", "und", "sei\u00b7ne", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Schuld ist meine nicht, das widrige Geschicke", "tokens": ["Die", "Schuld", "ist", "mei\u00b7ne", "nicht", ",", "das", "wid\u00b7ri\u00b7ge", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "PTKNEG", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Macht, da\u00df mich nicht von dir die Gegenwart erfreut.", "tokens": ["Macht", ",", "da\u00df", "mich", "nicht", "von", "dir", "die", "Ge\u00b7gen\u00b7wart", "er\u00b7freut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Wie gerne wolt ich dich in deinen Mauren k\u00fcssen!", "tokens": ["Wie", "ger\u00b7ne", "wolt", "ich", "dich", "in", "dei\u00b7nen", "Mau\u00b7ren", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie sanffte w\u00fcrd ich nicht an deiner Seite ruhn!", "tokens": ["Wie", "sanff\u00b7te", "w\u00fcrd", "ich", "nicht", "an", "dei\u00b7ner", "Sei\u00b7te", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch weil die Mi\u00dfgunst mich heist di\u00df Vergn\u00fcgen missen,", "tokens": ["Doch", "weil", "die", "Mi\u00df\u00b7gunst", "mich", "heist", "di\u00df", "Ver\u00b7gn\u00fc\u00b7gen", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VAFIN", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "So mu\u00df ich selbiges nur in Gedancken thun.", "tokens": ["So", "mu\u00df", "ich", "sel\u00b7bi\u00b7ges", "nur", "in", "Ge\u00b7dan\u00b7cken", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Indessen will ich dich noch um Verzeihung bitten,", "tokens": ["In\u00b7des\u00b7sen", "will", "ich", "dich", "noch", "um", "Ver\u00b7zei\u00b7hung", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "La\u00df deine Freundschafft nicht aus Zorn ver\u00e4ndert seyn.", "tokens": ["La\u00df", "dei\u00b7ne", "Freund\u00b7schafft", "nicht", "aus", "Zorn", "ver\u00b7\u00e4n\u00b7dert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hab ich gleich den Befehl gezwungen \u00fcberschritten,", "tokens": ["Hab", "ich", "gleich", "den", "Be\u00b7fehl", "ge\u00b7zwun\u00b7gen", "\u00fc\u00b7bersc\u00b7hrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "So stell ich mich bey dir vielleicht in kurtzen ein.", "tokens": ["So", "stell", "ich", "mich", "bey", "dir", "viel\u00b7leicht", "in", "kurt\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "APPR", "PPER", "ADV", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Mein Kiel und auch mein Hertz bleibt ewig dir ergeben,", "tokens": ["Mein", "Kiel", "und", "auch", "mein", "Hertz", "bleibt", "e\u00b7wig", "dir", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "VVFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Drum k\u00fc\u00dft er, eh ich ihn noch v\u00f6llig ausgespr\u00fctzt,", "tokens": ["Drum", "k\u00fc\u00dft", "er", ",", "eh", "ich", "ihn", "noch", "v\u00f6l\u00b7lig", "aus\u00b7ge\u00b7spr\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dir deine liebste Hand, und auch zugleich darneben", "tokens": ["Dir", "dei\u00b7ne", "liebs\u00b7te", "Hand", ",", "und", "auch", "zu\u00b7gleich", "dar\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "KON", "ADV", "ADV", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Demjenigen, der dir in Schoo\u00df und Armen sitzt.", "tokens": ["Dem\u00b7je\u00b7ni\u00b7gen", ",", "der", "dir", "in", "Schoo\u00df", "und", "Ar\u00b7men", "sitzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}