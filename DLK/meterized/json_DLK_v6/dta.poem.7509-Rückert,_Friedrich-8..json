{"dta.poem.7509": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn das Erhabne staunt die junge Menschheit an,", "tokens": ["Wenn", "das", "Er\u00b7hab\u00b7ne", "staunt", "die", "jun\u00b7ge", "Menschheit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spricht sie im hellen Traum: das hat der Gott gethan.", "tokens": ["Spricht", "sie", "im", "hel\u00b7len", "Traum", ":", "das", "hat", "der", "Gott", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn sie zum Gef\u00fchl des Sch\u00f6nen dann erwacht,", "tokens": ["Und", "wenn", "sie", "zum", "Ge\u00b7f\u00fchl", "des", "Sch\u00f6\u00b7nen", "dann", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekennt sie freudig stolz: Es hats der Mensch vollbracht.", "tokens": ["Be\u00b7kennt", "sie", "freu\u00b7dig", "stolz", ":", "Es", "hats", "der", "Mensch", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn zum Wahren einst sie reift, wird sie erkennen,", "tokens": ["Und", "wenn", "zum", "Wah\u00b7ren", "einst", "sie", "reift", ",", "wird", "sie", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es thuts im Menschen Gott, der nicht von ihm zu trennen.", "tokens": ["Es", "thuts", "im", "Men\u00b7schen", "Gott", ",", "der", "nicht", "von", "ihm", "zu", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "$,", "PRELS", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}