{"textgrid.poem.41653": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer irgend wert, ein Mensch zu sein,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer irgend wert, ein Mensch zu sein,", "tokens": ["Wer", "ir\u00b7gend", "wert", ",", "ein", "Mensch", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat eine Natter in der Seele,", "tokens": ["Hat", "ei\u00b7ne", "Nat\u00b7ter", "in", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie gibt ihm wie ein F\u00fcrst Befehle,", "tokens": ["Sie", "gibt", "ihm", "wie", "ein", "F\u00fcrst", "Be\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagt er: \u00bbJa\u00ab, so spricht sie: \u00bbNein!\u00ab", "tokens": ["Und", "sagt", "er", ":", "\u00bb", "Ja", "\u00ab", ",", "so", "spricht", "sie", ":", "\u00bb", "Nein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$(", "$,", "ADV", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Willst du in starre Augen schauen", "tokens": ["Willst", "du", "in", "star\u00b7re", "Au\u00b7gen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Nymphen und den Wasserfrauen,", "tokens": ["Den", "Nym\u00b7phen", "und", "den", "Was\u00b7ser\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zahn sagt: \u00bbDeiner Pflicht hab acht!\u00ab", "tokens": ["Der", "Zahn", "sagt", ":", "\u00bb", "Dei\u00b7ner", "Pflicht", "hab", "acht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VAFIN", "CARD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Pflanz B\u00e4ume oder zeuge S\u00f6hne,", "tokens": ["Pflanz", "B\u00e4u\u00b7me", "o\u00b7der", "zeu\u00b7ge", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gib Vers und Marmor Form und Sch\u00f6ne,", "tokens": ["Gib", "Vers", "und", "Mar\u00b7mor", "Form", "und", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zahn sagt: \u00bbLebst du diese Nacht?\u00ab", "tokens": ["Der", "Zahn", "sagt", ":", "\u00bb", "Lebst", "du", "die\u00b7se", "Nacht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVIMP", "PPER", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was er auch plane und verlange,", "tokens": ["Was", "er", "auch", "pla\u00b7ne", "und", "ver\u00b7lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jedem Augenblicke trifft", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7bli\u00b7cke", "trifft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Menschen warnend all das Gift", "tokens": ["Den", "Men\u00b7schen", "war\u00b7nend", "all", "das", "Gift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der unertr\u00e4glich-argen Schlange.", "tokens": ["Der", "un\u00b7er\u00b7tr\u00e4g\u00b7lich\u00b7ar\u00b7gen", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}