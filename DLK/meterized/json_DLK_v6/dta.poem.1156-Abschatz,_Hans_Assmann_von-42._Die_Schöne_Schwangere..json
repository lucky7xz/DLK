{"dta.poem.1156": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "42. Die Sch\u00f6ne Schwangere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Vor f\u00fchrtestu ein Hertz allein in deinem Leibe/ ", "tokens": ["Vor", "f\u00fchr\u00b7te\u00b7stu", "ein", "Hertz", "al\u00b7lein", "in", "dei\u00b7nem", "Lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bl\u00fchtest Rosen-gleich in Anmutts-voller Zier;", "tokens": ["Und", "bl\u00fch\u00b7test", "Ro\u00b7sen\u00b7gleich", "in", "An\u00b7mutts\u00b7vol\u00b7ler", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Izt tr\u00e4gstu deren zwey/ und macht Verdru\u00df bey dir/", "tokens": ["Izt", "tr\u00e4gs\u00b7tu", "de\u00b7ren", "zwey", "/", "und", "macht", "Ver\u00b7dru\u00df", "bey", "dir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "CARD", "$(", "KON", "VVFIN", "NN", "APPR", "PPER", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da\u00df deine R\u00f6the nicht in altem Stande bleibe.", "tokens": ["Da\u00df", "dei\u00b7ne", "R\u00f6\u00b7the", "nicht", "in", "al\u00b7tem", "Stan\u00b7de", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht klage/ da\u00df die Frucht der Bl\u00fctte Glantz vertreibe:", "tokens": ["Nicht", "kla\u00b7ge", "/", "da\u00df", "die", "Frucht", "der", "Bl\u00fct\u00b7te", "Glantz", "ver\u00b7trei\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geht nicht der volle Mond dem halben Lichte f\u00fcr/", "tokens": ["Geht", "nicht", "der", "vol\u00b7le", "Mond", "dem", "hal\u00b7ben", "Lich\u00b7te", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und w\u00e4r er noch so bla\u00df? zeigt sich ein Mangel hier/", "tokens": ["Und", "w\u00e4r", "er", "noch", "so", "bla\u00df", "?", "zeigt", "sich", "ein", "Man\u00b7gel", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "VVFIN", "PRF", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So dencke/ da\u00df man ihn der Schuld der Zeit zuschreibe.", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "man", "ihn", "der", "Schuld", "der", "Zeit", "zu\u00b7schrei\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PIS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dir steht wohl an und frey/ was andern ist verwehrt.", "tokens": ["Dir", "steht", "wohl", "an", "und", "frey", "/", "was", "an\u00b7dern", "ist", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "ADJD", "$(", "PWS", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das schwangre Schiff im See tr\u00e4gt Last/ doch unbeschwert/", "tokens": ["Das", "schwang\u00b7re", "Schiff", "im", "See", "tr\u00e4gt", "Last", "/", "doch", "un\u00b7be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "NE", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dieweil es an dem Port sich zu entladen sucht.", "tokens": ["Die\u00b7weil", "es", "an", "dem", "Port", "sich", "zu", "ent\u00b7la\u00b7den", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NE", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ulysses keusche Frau beschlo\u00df ihr langes Weben/", "tokens": ["U\u00b7lys\u00b7ses", "keu\u00b7sche", "Frau", "be\u00b7schlo\u00df", "ihr", "lan\u00b7ges", "We\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Da\u00df sie nach gleicher Kost der bitter-s\u00fcssen Frucht/", "tokens": ["Da\u00df", "sie", "nach", "glei\u00b7cher", "Kost", "der", "bit\u00b7ter\u00b7s\u00fcs\u00b7sen", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was H\u00e4nd und F\u00fcsse hat/ der Nachwelt konte geben.", "tokens": ["Was", "H\u00e4nd", "und", "F\u00fcs\u00b7se", "hat", "/", "der", "Nach\u00b7welt", "kon\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VAFIN", "$(", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}