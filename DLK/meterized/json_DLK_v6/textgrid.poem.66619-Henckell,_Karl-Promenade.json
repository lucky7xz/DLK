{"textgrid.poem.66619": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Promenade", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieses gr\u00fcnen Parks Revieren", "tokens": ["In", "die\u00b7ses", "gr\u00fc\u00b7nen", "Parks", "Re\u00b7vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flie\u00dft milder Hauch von Baum zu Baum,", "tokens": ["Flie\u00dft", "mil\u00b7der", "Hauch", "von", "Baum", "zu", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die jungen M\u00e4dchen gehn spazieren,", "tokens": ["Die", "jun\u00b7gen", "M\u00e4d\u00b7chen", "gehn", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben ist ein Liebestraum.", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Lie\u00b7best\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Tante Marlitt just erg\u00f6tzt sich", "tokens": ["An", "Tan\u00b7te", "Mar\u00b7litt", "just", "er\u00b7g\u00f6tzt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die breite Bonne neben mir,", "tokens": ["Die", "brei\u00b7te", "Bon\u00b7ne", "ne\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Greis in braunem Schurzfell setzt sich:", "tokens": ["Ein", "Greis", "in", "brau\u00b7nem", "Schurz\u00b7fell", "setzt", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit sorgenhaft vergrilltem Blicke", "tokens": ["Mit", "sor\u00b7gen\u00b7haft", "ver\u00b7grill\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spazierstockt ein Rentier daher:", "tokens": ["Spa\u00b7zier\u00b7stockt", "ein", "Ren\u00b7tier", "da\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbauf nichts Verla\u00df! Die Welt voll T\u00fccke!", "tokens": ["\u00bb", "auf", "nichts", "Ver\u00b7la\u00df", "!", "Die", "Welt", "voll", "T\u00fc\u00b7cke", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIS", "NN", "$.", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kurse sinken immer mehr.\u00ab", "tokens": ["Die", "Kur\u00b7se", "sin\u00b7ken", "im\u00b7mer", "mehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Dutzend Kinder schlingt den Reigen,", "tokens": ["Ein", "Dut\u00b7zend", "Kin\u00b7der", "schlingt", "den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Springbrunn silberne Funken speit,", "tokens": ["Der", "Spring\u00b7brunn", "sil\u00b7ber\u00b7ne", "Fun\u00b7ken", "speit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Strahlen sprudeln, springen, steigen \u2013", "tokens": ["Die", "Strah\u00b7len", "spru\u00b7deln", ",", "sprin\u00b7gen", ",", "stei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O wundersch\u00f6ne Jugendzeit.", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Ju\u00b7gend\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Br\u00fcckenpfeiler dort zerschellen", "tokens": ["Am", "Br\u00fc\u00b7cken\u00b7pfei\u00b7ler", "dort", "zer\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fluten, gurgelnd rauscht es hohl,", "tokens": ["Die", "Flu\u00b7ten", ",", "gur\u00b7gelnd", "rauscht", "es", "hohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Weib starrt trostlos in die Wellen", "tokens": ["Ein", "Weib", "starrt", "trost\u00b7los", "in", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seufzt: \u00bbWie w\u00e4r' mir drunten wohl!\u00ab", "tokens": ["Und", "seufzt", ":", "\u00bb", "Wie", "w\u00e4r'", "mir", "drun\u00b7ten", "wohl", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie flieht den Strom mit leisem St\u00f6hnen,", "tokens": ["Sie", "flieht", "den", "Strom", "mit", "lei\u00b7sem", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Frech gafft ein Geck ihr ins Gesicht,", "tokens": ["Frech", "gafft", "ein", "Geck", "ihr", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Eisenh\u00e4mmer dr\u00fcben dr\u00f6hnen,", "tokens": ["Die", "Ei\u00b7sen\u00b7h\u00e4m\u00b7mer", "dr\u00fc\u00b7ben", "dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Qualm verschlingt das Sonnenlicht.", "tokens": ["Der", "Qualm", "ver\u00b7schlingt", "das", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}