{"textgrid.poem.41055": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Willkomm der Heilbronner Turner an ihre Turnbr\u00fcder bei dem Turnfeste in Heilbronn", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Turnerbr\u00fcder! seid willkommen,", "tokens": ["Tur\u00b7ner\u00b7br\u00fc\u00b7der", "!", "seid", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid mit Jubel aufgenommen", "tokens": ["Seid", "mit", "Ju\u00b7bel", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der alten Neckarstadt,", "tokens": ["In", "der", "al\u00b7ten", "Ne\u00b7ckar\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ein Turner, nah dem Blitze,", "tokens": ["Wo", "ein", "Tur\u00b7ner", ",", "nah", "dem", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stehend auf des Kirchturms Spitze", "tokens": ["Ste\u00b7hend", "auf", "des", "Kirch\u00b7turms", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4ngst schon euch erwartet hat.", "tokens": ["L\u00e4ngst", "schon", "euch", "er\u00b7war\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Folget seiner Fahne, Br\u00fcder!", "tokens": ["Fol\u00b7get", "sei\u00b7ner", "Fah\u00b7ne", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht heran! \u2013 Frisch, frei und bieder", "tokens": ["Zieht", "he\u00b7ran", "!", "\u2013", "Frisch", ",", "frei", "und", "bie\u00b7der"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "$(", "NE", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollet ihr empfangen sein!", "tokens": ["Sol\u00b7let", "ihr", "emp\u00b7fan\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt auf Neckars blauen Wogen,", "tokens": ["Kommt", "auf", "Ne\u00b7ckars", "blau\u00b7en", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt zu Fu\u00df durchs Tal gezogen", "tokens": ["Kommt", "zu", "Fu\u00df", "durchs", "Tal", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die Stadt voll Sang und Wein.", "tokens": ["In", "die", "Stadt", "voll", "Sang", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df schon in der Vorzeit Jahren", "tokens": ["Da\u00df", "schon", "in", "der", "Vor\u00b7zeit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrttemberger Turner waren,", "tokens": ["W\u00fcrt\u00b7tem\u00b7ber\u00b7ger", "Tur\u00b7ner", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zeigt der Hirsch im Wappen klar;", "tokens": ["Zeigt", "der", "Hirsch", "im", "Wap\u00b7pen", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hirt aus alten Tagen,", "tokens": ["Und", "der", "Hirt", "aus", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der den Eberhard getragen,", "tokens": ["Der", "den", "E\u00b7ber\u00b7hard", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon ein t\u00fccht'ger Turner war.", "tokens": ["Schon", "ein", "t\u00fccht'\u00b7ger", "Tur\u00b7ner", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Schaut den Aar in blauen L\u00fcften,", "tokens": ["Schaut", "den", "Aar", "in", "blau\u00b7en", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut die Gemse \u00fcber Kl\u00fcften,", "tokens": ["Schaut", "die", "Gem\u00b7se", "\u00fc\u00b7ber", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Forelle in dem Flu\u00df,", "tokens": ["Die", "Fo\u00b7rel\u00b7le", "in", "dem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaut des Dampfes m\u00e4cht'ges Ringen,", "tokens": ["Schaut", "des", "Damp\u00b7fes", "m\u00e4cht'\u00b7ges", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcberall schl\u00e4gt seine Schwingen", "tokens": ["\u00dc\u00b7be\u00b7rall", "schl\u00e4gt", "sei\u00b7ne", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Bewegung Genius!", "tokens": ["Der", "Be\u00b7we\u00b7gung", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Nur der Mensch sollt' tr\u00e4ge liegen?", "tokens": ["Nur", "der", "Mensch", "sollt'", "tr\u00e4\u00b7ge", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein! herbei in bunten Z\u00fcgen,", "tokens": ["Nein", "!", "her\u00b7bei", "in", "bun\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Turnerbr\u00fcder! zu der Stadt,", "tokens": ["Tur\u00b7ner\u00b7br\u00fc\u00b7der", "!", "zu", "der", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ein Turner, nah dem Blitze,", "tokens": ["Wo", "ein", "Tur\u00b7ner", ",", "nah", "dem", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stehend auf des Kirchturms Spitze", "tokens": ["Ste\u00b7hend", "auf", "des", "Kirch\u00b7turms", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4ngst schon euch erwartet hat.", "tokens": ["L\u00e4ngst", "schon", "euch", "er\u00b7war\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}