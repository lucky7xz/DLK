{"textgrid.poem.60482": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Rezept", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie des Fr\u00fchlings Sonnenblick", "tokens": ["Gleich\u00b7wie", "des", "Fr\u00fch\u00b7lings", "Son\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Busen der Natur durchgl\u00fcht,", "tokens": ["Den", "Bu\u00b7sen", "der", "Na\u00b7tur", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem neues Leben nun entbl\u00fcht,", "tokens": ["Dem", "neu\u00b7es", "Le\u00b7ben", "nun", "ent\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So traf auch mich dein Engelblick.", "tokens": ["So", "traf", "auch", "mich", "dein", "En\u00b7gel\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er kr\u00f6net all die stillen Stunden,", "tokens": ["Er", "kr\u00f6\u00b7net", "all", "die", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In denen ich dem Ideal gelebt;", "tokens": ["In", "de\u00b7nen", "ich", "dem", "I\u00b7deal", "ge\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn nun hat sich das Zauberbild gefunden,", "tokens": ["Denn", "nun", "hat", "sich", "das", "Zau\u00b7ber\u00b7bild", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Wesen selbst die Brust entgegenbebt.", "tokens": ["Dem", "We\u00b7sen", "selbst", "die", "Brust", "ent\u00b7ge\u00b7gen\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn mich des Waldes d\u00fcstre Nacht umschlingt,", "tokens": ["Wenn", "mich", "des", "Wal\u00b7des", "d\u00fcst\u00b7re", "Nacht", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Zephir seufzend durch Geb\u00fcsche schleicht,", "tokens": ["Wo", "Ze\u00b7phir", "seuf\u00b7zend", "durch", "Ge\u00b7b\u00fc\u00b7sche", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo sich die Seel ins Land der Tr\u00e4ume schwingt", "tokens": ["Wo", "sich", "die", "Seel", "ins", "Land", "der", "Tr\u00e4u\u00b7me", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kaltes Denken der Empfindung weicht:", "tokens": ["Und", "kal\u00b7tes", "Den\u00b7ken", "der", "Emp\u00b7fin\u00b7dung", "weicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da schwebt dein Bild vor meinen trunknen Blicken,", "tokens": ["Da", "schwebt", "dein", "Bild", "vor", "mei\u00b7nen", "trunk\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ich verliere mich in himmlisches Entz\u00fccken.", "tokens": ["Und", "ich", "ver\u00b7lie\u00b7re", "mich", "in", "himm\u00b7li\u00b7sches", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}