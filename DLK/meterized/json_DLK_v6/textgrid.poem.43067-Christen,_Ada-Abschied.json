{"textgrid.poem.43067": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als ich fortgezogen,", "tokens": ["Und", "als", "ich", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich in der letzten Nacht", "tokens": ["Hab'", "ich", "in", "der", "letz\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Stra\u00dfe, wo er wohnte,", "tokens": ["Der", "Stra\u00b7\u00dfe", ",", "wo", "er", "wohn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eine Abschiedsvisite gemacht.", "tokens": ["Ei\u00b7ne", "Ab\u00b7schieds\u00b7vi\u00b7si\u00b7te", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hab' angesehen die Steine,", "tokens": ["Hab'", "an\u00b7ge\u00b7se\u00b7hen", "die", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die oft sein Fu\u00df betritt,", "tokens": ["Die", "oft", "sein", "Fu\u00df", "be\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dachte, w\u00e4r' ich reich,", "tokens": ["Und", "dach\u00b7te", ",", "w\u00e4r'", "ich", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich n\u00e4hme sie alle mit.", "tokens": ["Ich", "n\u00e4h\u00b7me", "sie", "al\u00b7le", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich kam zu seinem Hause", "tokens": ["Ich", "kam", "zu", "sei\u00b7nem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wu\u00dfte selbst nicht wie,", "tokens": ["Und", "wu\u00df\u00b7te", "selbst", "nicht", "wie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hin bis an das Thor \u2013", "tokens": ["Und", "hin", "bis", "an", "das", "Thor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dort sank ich auf die Knie'.", "tokens": ["Dort", "sank", "ich", "auf", "die", "Knie'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sah empor zum Fenster", "tokens": ["Ich", "sah", "em\u00b7por", "zum", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hab' es schmerzlich gegr\u00fc\u00dft;", "tokens": ["Und", "hab'", "es", "schmerz\u00b7lich", "ge\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich habe mit hei\u00dfer Lippe", "tokens": ["Ich", "ha\u00b7be", "mit", "hei\u00b7\u00dfer", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Stufen am Thore gek\u00fc\u00dft.", "tokens": ["Die", "Stu\u00b7fen", "am", "Tho\u00b7re", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Ja selbst die kalte Mauer", "tokens": ["Ja", "selbst", "die", "kal\u00b7te", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ber\u00fchrte mein brennender Mund;", "tokens": ["Be\u00b7r\u00fchr\u00b7te", "mein", "bren\u00b7nen\u00b7der", "Mund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch hielt ich zitternd inne,", "tokens": ["Doch", "hielt", "ich", "zit\u00b7ternd", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn an mich hinan sprang sein Hund.", "tokens": ["Denn", "an", "mich", "hi\u00b7nan", "sprang", "sein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ich sah ihn schweigend an.", "tokens": ["Ich", "sah", "ihn", "schwei\u00b7gend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da fragte er mich l\u00e4chelnd,", "tokens": ["Da", "frag\u00b7te", "er", "mich", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ich denn hier gethan?", "tokens": ["Was", "ich", "denn", "hier", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dies L\u00e4cheln war vernichtend,", "tokens": ["Dies", "L\u00e4\u00b7cheln", "war", "ver\u00b7nich\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich rang nach einem Wort;", "tokens": ["Ich", "rang", "nach", "ei\u00b7nem", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sagte ich kaum h\u00f6rbar:", "tokens": ["Dann", "sag\u00b7te", "ich", "kaum", "h\u00f6r\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "\u00bbherr, morgen geh' ich fort.\u00ab", "tokens": ["\u00bb", "herr", ",", "mor\u00b7gen", "geh'", "ich", "fort", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Und abermals dies L\u00e4cheln,", "tokens": ["Und", "a\u00b7ber\u00b7mals", "dies", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mich so elend gemacht:", "tokens": ["Das", "mich", "so", "e\u00b7lend", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbich w\u00fcnsche gl\u00fcckliche Reise \u2013", "tokens": ["\u00bb", "ich", "w\u00fcn\u00b7sche", "gl\u00fcck\u00b7li\u00b7che", "Rei\u00b7se", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und mithin gute Nacht.\u00ab", "tokens": ["Und", "mi\u00b7thin", "gu\u00b7te", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}