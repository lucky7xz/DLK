{"textgrid.poem.63599": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Kr\u00e4nzen, wie kein Br\u00e4utigam, geschm\u00fcckt,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", ",", "wie", "kein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PIAT", "NE", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Feierkleidern angetan aufs beste,", "tokens": ["Mit", "Fei\u00b7er\u00b7klei\u00b7dern", "an\u00b7ge\u00b7tan", "aufs", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch deine gro\u00dfen Augen zugedr\u00fcckt,", "tokens": ["Doch", "dei\u00b7ne", "gro\u00b7\u00dfen", "Au\u00b7gen", "zu\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So fuhrst du weg zu deinem letzten Feste,", "tokens": ["So", "fuhrst", "du", "weg", "zu", "dei\u00b7nem", "letz\u00b7ten", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Langsam, im Schritt. Warum dich \u00fcbereilen?", "tokens": ["Lang\u00b7sam", ",", "im", "Schritt", ".", "Wa\u00b7rum", "dich", "\u00fc\u00b7be\u00b7rei\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "$.", "PWAV", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Gern wartet jener Wirt auf seine G\u00e4ste.", "tokens": ["Gern", "war\u00b7tet", "je\u00b7ner", "Wirt", "auf", "sei\u00b7ne", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dort hinter langen, stillen H\u00fcgelzeilen \u2013", "tokens": ["Dort", "hin\u00b7ter", "lan\u00b7gen", ",", "stil\u00b7len", "H\u00fc\u00b7gel\u00b7zei\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Siehst du das Haus? Es brennen viele Lichter,", "tokens": ["Siehst", "du", "das", "Haus", "?", "Es", "bren\u00b7nen", "vie\u00b7le", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch denen nicht zur Lust, die dort verweilen.", "tokens": ["Doch", "de\u00b7nen", "nicht", "zur", "Lust", ",", "die", "dort", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKNEG", "APPRART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Seltsam! Die hagern, br\u00e4unlichen Gesichter,", "tokens": ["Selt\u00b7sam", "!", "Die", "ha\u00b7gern", ",", "br\u00e4un\u00b7li\u00b7chen", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Wie sie bek\u00fcmmert in die Kerzen starren.", "tokens": ["Wie", "sie", "be\u00b7k\u00fcm\u00b7mert", "in", "die", "Ker\u00b7zen", "star\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Manch einer grinst, doch nicht das Schweigen bricht er.", "tokens": ["Manch", "ei\u00b7ner", "grinst", ",", "doch", "nicht", "das", "Schwei\u00b7gen", "bricht", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "$,", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es scheint, es finden's all die armen Narren", "tokens": ["Es", "scheint", ",", "es", "fin\u00b7den's", "all", "die", "ar\u00b7men", "Nar\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gar unbequem, hier aufgeputzt der Stunde,", "tokens": ["Gar", "un\u00b7be\u00b7quem", ",", "hier", "auf\u00b7ge\u00b7putzt", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Da man zur Ruh' sie bringen wird, zu harren.", "tokens": ["Da", "man", "zur", "Ruh'", "sie", "brin\u00b7gen", "wird", ",", "zu", "har\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "VVINF", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die alte Dame dort, mit gutem Grunde", "tokens": ["Die", "al\u00b7te", "Da\u00b7me", "dort", ",", "mit", "gu\u00b7tem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Verstimmt es sie, da\u00df man sie hergebracht", "tokens": ["Ver\u00b7stimmt", "es", "sie", ",", "da\u00df", "man", "sie", "her\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PIS", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "So ungeschminkt, mit zahnlos offnem Munde.", "tokens": ["So", "un\u00b7ge\u00b7schminkt", ",", "mit", "zahn\u00b7los", "off\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und dort das Fr\u00e4ulein, das so gern gelacht,", "tokens": ["Und", "dort", "das", "Fr\u00e4u\u00b7lein", ",", "das", "so", "gern", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Get\u00e4nzelt und das s\u00fc\u00dfe Kind gespielt,", "tokens": ["Ge\u00b7t\u00e4n\u00b7zelt", "und", "das", "s\u00fc\u00b7\u00dfe", "Kind", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Was hat auf einmal sie so ernst gemacht?", "tokens": ["Was", "hat", "auf", "ein\u00b7mal", "sie", "so", "ernst", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der alte Dandy zwar, der nach ihr schielt", "tokens": ["Der", "al\u00b7te", "Dan\u00b7dy", "zwar", ",", "der", "nach", "ihr", "schielt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und ihren Kranz und Schleier scheint zu loben,", "tokens": ["Und", "ih\u00b7ren", "Kranz", "und", "Schlei\u00b7er", "scheint", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ist ein Galan, der sich nur schlecht empfiehlt.", "tokens": ["Ist", "ein", "Ga\u00b7lan", ",", "der", "sich", "nur", "schlecht", "emp\u00b7fiehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die blonde Haartour hat sich ihm verschoben;", "tokens": ["Die", "blon\u00b7de", "Haar\u00b7tour", "hat", "sich", "ihm", "ver\u00b7scho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Sein Kammerdiener hielt der M\u00fch' vielleicht,", "tokens": ["Sein", "Kam\u00b7mer\u00b7die\u00b7ner", "hielt", "der", "M\u00fch'", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Obwohl er ihn beerbt, sich \u00fcberhoben.", "tokens": ["Ob\u00b7wohl", "er", "ihn", "be\u00b7erbt", ",", "sich", "\u00fc\u00b7berh\u00b7o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und dort, zum Alabasterbild gebleicht,", "tokens": ["Und", "dort", ",", "zum", "A\u00b7la\u00b7bas\u00b7ter\u00b7bild", "ge\u00b7bleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die junge Mutter, hier zum Fest geladen,", "tokens": ["Die", "jun\u00b7ge", "Mut\u00b7ter", ",", "hier", "zum", "Fest", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Eh sie dem S\u00e4ugling noch die Brust gereicht.", "tokens": ["Eh", "sie", "dem", "S\u00e4ug\u00b7ling", "noch", "die", "Brust", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Ihr Nachbar auch scheint tr\u00fcb und grambeladen,", "tokens": ["Ihr", "Nach\u00b7bar", "auch", "scheint", "tr\u00fcb", "und", "gram\u00b7be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Nicht drei\u00dfig alt, ein schmucker Offizier,", "tokens": ["Nicht", "drei\u00b7\u00dfig", "alt", ",", "ein", "schmu\u00b7cker", "Of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und schon getrennt von allen Kameraden!", "tokens": ["Und", "schon", "ge\u00b7trennt", "von", "al\u00b7len", "Ka\u00b7me\u00b7ra\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "F\u00fcrwahr, es ist nicht eben lustig hier;", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "ist", "nicht", "e\u00b7ben", "lus\u00b7tig", "hier", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Ein jeder Gast hat nur mit sich zu tun,", "tokens": ["Ein", "je\u00b7der", "Gast", "hat", "nur", "mit", "sich", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Die Kerzen knistern wie ge\u00e4ngstet schier.", "tokens": ["Die", "Ker\u00b7zen", "knis\u00b7tern", "wie", "ge\u00b7\u00e4ngs\u00b7tet", "schier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "O kommst auch du, mein lieber Knabe, nun", "tokens": ["O", "kommst", "auch", "du", ",", "mein", "lie\u00b7ber", "Kna\u00b7be", ",", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Und suchst dir bei den stillen fremden Leuten", "tokens": ["Und", "suchst", "dir", "bei", "den", "stil\u00b7len", "frem\u00b7den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Bescheiden einen Platz, um auszuruhn?", "tokens": ["Be\u00b7schei\u00b7den", "ei\u00b7nen", "Platz", ",", "um", "aus\u00b7zu\u00b7ruhn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUI", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Hier spielt man nicht die Spiele, die dich freuten.", "tokens": ["Hier", "spielt", "man", "nicht", "die", "Spie\u00b7le", ",", "die", "dich", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Soll Jugend, die den Ernst des Lebens kaum", "tokens": ["Soll", "Ju\u00b7gend", ",", "die", "den", "Ernst", "des", "Le\u00b7bens", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Von fern geahnt, den ernsten Tod sich deuten?", "tokens": ["Von", "fern", "ge\u00b7ahnt", ",", "den", "erns\u00b7ten", "Tod", "sich", "deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Es geht ein heimlich Regen durch den Raum,", "tokens": ["Es", "geht", "ein", "heim\u00b7lich", "Re\u00b7gen", "durch", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Als wollten sie den Ank\u00f6mmling beschauen", "tokens": ["Als", "woll\u00b7ten", "sie", "den", "An\u00b7k\u00f6mm\u00b7ling", "be\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Durch eingesunkner Wimpern schmalen Saum.", "tokens": ["Durch", "ein\u00b7ge\u00b7sun\u00b7kner", "Wim\u00b7pern", "schma\u00b7len", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Er aber achtet nicht der Herrn und Frauen.", "tokens": ["Er", "a\u00b7ber", "ach\u00b7tet", "nicht", "der", "Herrn", "und", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Er ruht wie \u00fcber Lieb' und Graun erhaben", "tokens": ["Er", "ruht", "wie", "\u00fc\u00b