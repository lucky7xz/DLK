{"textgrid.poem.39112": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die erste Saat", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist die St\u00e4tte, seht! Hier f\u00e4llten", "tokens": ["Hier", "ist", "die", "St\u00e4t\u00b7te", ",", "seht", "!", "Hier", "f\u00e4ll\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den unbetretnen Urwald wir,", "tokens": ["Den", "un\u00b7be\u00b7tret\u00b7nen", "Ur\u00b7wald", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit unserm Schwei\u00dfe hier bestellten", "tokens": ["Mit", "un\u00b7serm", "Schwei\u00b7\u00dfe", "hier", "be\u00b7stell\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir das jungfr\u00e4uliche Revier!", "tokens": ["Wir", "das", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Re\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier soll die Heimat sich erneuen,", "tokens": ["Hier", "soll", "die", "Hei\u00b7mat", "sich", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier, von Europa wir verbannt,", "tokens": ["Hier", ",", "von", "Eu\u00b7ro\u00b7pa", "wir", "ver\u00b7bannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wolln wir den ersten Samen streuen", "tokens": ["Wolln", "wir", "den", "ers\u00b7ten", "Sa\u00b7men", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dieses neue, fremde Land.", "tokens": ["In", "die\u00b7ses", "neu\u00b7e", ",", "frem\u00b7de", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Reicht her das Korn \u2013 o sei willkommen,", "tokens": ["Reicht", "her", "das", "Korn", "\u2013", "o", "sei", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "ART", "NN", "$(", "FM", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du unsrer Heimat teure Frucht,", "tokens": ["Du", "uns\u00b7rer", "Hei\u00b7mat", "teu\u00b7re", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wir als Erbschaft mitgenommen,", "tokens": ["Die", "wir", "als", "Erb\u00b7schaft", "mit\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Pfand der Zukunft auf der Flucht!", "tokens": ["Als", "Pfand", "der", "Zu\u00b7kunft", "auf", "der", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als w\u00e4r's ein Kind, das wir versenken,", "tokens": ["Als", "w\u00e4r's", "ein", "Kind", ",", "das", "wir", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So streut dich z\u00f6gernd unsre Hand,", "tokens": ["So", "streut", "dich", "z\u00f6\u00b7gernd", "uns\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unsre tiefsten Herzen denken", "tokens": ["Und", "uns\u00b7re", "tiefs\u00b7ten", "Her\u00b7zen", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An das geliebte Vaterland.", "tokens": ["An", "das", "ge\u00b7lieb\u00b7te", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als du zuerst emporgewachsen,", "tokens": ["Als", "du", "zu\u00b7erst", "em\u00b7por\u00b7ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gr\u00fcner Halm aus dunkler Gruft,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Halm", "aus", "dunk\u00b7ler", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Elbestrand, im sch\u00f6nen Sachsen,", "tokens": ["Am", "El\u00b7be\u00b7strand", ",", "im", "sch\u00f6\u00b7nen", "Sach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da k\u00fc\u00dfte dich die deutsche Luft;", "tokens": ["Da", "k\u00fc\u00df\u00b7te", "dich", "die", "deut\u00b7sche", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schien auf dich, da flo\u00df hernieder", "tokens": ["Da", "schien", "auf", "dich", ",", "da", "flo\u00df", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die deutsche Sonne, deutscher Tau,", "tokens": ["Die", "deut\u00b7sche", "Son\u00b7ne", ",", "deut\u00b7scher", "Tau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und deutscher Lerchen s\u00fc\u00dfe Lieder", "tokens": ["Und", "deut\u00b7scher", "Ler\u00b7chen", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Begr\u00fc\u00dften die geschm\u00fcckte Au.", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7ten", "die", "ge\u00b7schm\u00fcck\u00b7te", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drauf als die Halme h\u00f6her rauschten,", "tokens": ["Drauf", "als", "die", "Hal\u00b7me", "h\u00f6\u00b7her", "rauschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als schon die Frucht im Keime schwoll,", "tokens": ["Als", "schon", "die", "Frucht", "im", "Kei\u00b7me", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Gott, da standen wir und lauschten", "tokens": ["O", "Gott", ",", "da", "stan\u00b7den", "wir", "und", "lauschten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wehm\u00fctiger Erwartung voll.", "tokens": ["Weh\u00b7m\u00fc\u00b7ti\u00b7ger", "Er\u00b7war\u00b7tung", "voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und als sich wiegten deine \u00c4hren,", "tokens": ["Und", "als", "sich", "wieg\u00b7ten", "dei\u00b7ne", "\u00c4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gekleidet all in lautres Gold,", "tokens": ["Ge\u00b7klei\u00b7det", "all", "in", "laut\u00b7res", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O damals, damals wieviel Z\u00e4hren", "tokens": ["O", "da\u00b7mals", ",", "da\u00b7mals", "wie\u00b7viel", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sind abw\u00e4rts in den Sand gerollt!", "tokens": ["Sind", "ab\u00b7w\u00e4rts", "in", "den", "Sand", "ge\u00b7rollt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn ach! schon suchten die Gedanken", "tokens": ["Denn", "ach", "!", "schon", "such\u00b7ten", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fern \u00fcberm Meer ein neues Ziel,", "tokens": ["Fern", "\u00fc\u00b7berm", "Meer", "ein", "neu\u00b7es", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Geiste schon sahn wir uns schwanken", "tokens": ["Im", "Geis\u00b7te", "schon", "sahn", "wir", "uns", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Fernhin auf ungewissem Kiel:", "tokens": ["Fern\u00b7hin", "auf", "un\u00b7ge\u00b7wis\u00b7sem", "Kiel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was n\u00fctzt es, da\u00df geerntet werde,", "tokens": ["Was", "n\u00fctzt", "es", ",", "da\u00df", "ge\u00b7ern\u00b7tet", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was wogt das Korn, was bl\u00fcht der Wein,", "tokens": ["Was", "wogt", "das", "Korn", ",", "was", "bl\u00fcht", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll nimmer doch auf deutscher Erde", "tokens": ["Soll", "nim\u00b7mer", "doch", "auf", "deut\u00b7scher", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freiheit teure Saat gedeihn?", "tokens": ["Der", "Frei\u00b7heit", "teu\u00b7re", "Saat", "ge\u00b7deihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und als man unter Spiel und Scherzen", "tokens": ["Und", "als", "man", "un\u00b7ter", "Spiel", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das reife Korn in Garben flocht,", "tokens": ["Das", "rei\u00b7fe", "Korn", "in", "Gar\u00b7ben", "flocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat da schon in Abschiedsschmerzen", "tokens": ["Wie", "hat", "da", "schon", "in", "Ab\u00b7schieds\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Busen \u00e4ngstlich uns gepocht!", "tokens": ["Der", "Bu\u00b7sen", "\u00e4ngst\u00b7lich", "uns", "ge\u00b7pocht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die andern schwangen sich im Tanze,", "tokens": ["Die", "an\u00b7dern", "schwan\u00b7gen", "sich", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da schrie die Fiedel, klang das Horn:", "tokens": ["Da", "schrie", "die", "Fie\u00b7del", ",", "klang", "das", "Horn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wir, im letzten Abendglanze,", "tokens": ["Doch", "wir", ",", "im", "letz\u00b7ten", "A\u00b7bend\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir banden schweigend unser Korn. \u2013", "tokens": ["Wir", "ban\u00b7den", "schwei\u00b7gend", "un\u00b7ser", "Korn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht eine Hand voll Erde nahmen", "tokens": ["Nicht", "ei\u00b7ne", "Hand", "voll", "Er\u00b7de", "nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir zum Valet von unsrer Flur:", "tokens": ["Wir", "zum", "Va\u00b7let", "von", "uns\u00b7rer", "Flur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nur deutsche Frucht, nur deutschen Samen!", "tokens": ["Nur", "deut\u00b7sche", "Frucht", ",", "nur", "deut\u00b7schen", "Sa\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Leben bringt Lebend'ges nur.", "tokens": ["Denn", "Le\u00b7ben", "bringt", "Le\u00b7ben\u00b7d'\u00b7ges", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wie ein F\u00e4hnrich seine Fahne", "tokens": ["Und", "wie", "ein", "F\u00e4hn\u00b7rich", "sei\u00b7ne", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Pflanzt auf des letzten Walles Rand,", "tokens": ["Pflanzt", "auf", "des", "letz\u00b7ten", "Wal\u00b7les", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So, jenseits nun dem Ozeane,", "tokens": ["So", ",", "jen\u00b7seits", "nun", 