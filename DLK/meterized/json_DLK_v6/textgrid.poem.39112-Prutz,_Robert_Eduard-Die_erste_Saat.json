ord", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie du sie einst vernommen hast,", "tokens": ["Wie", "du", "sie", "einst", "ver\u00b7nom\u00b7men", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Kranz von Rosen wird vers\u00fc\u00dfen", "tokens": ["Kein", "Kranz", "von", "Ro\u00b7sen", "wird", "ver\u00b7s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NE", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des hei\u00dfen Erntetages Last.", "tokens": ["Des", "hei\u00b7\u00dfen", "Ern\u00b7te\u00b7ta\u00b7ges", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und doch, will's Gott, so sollst du sprie\u00dfen", "tokens": ["Und", "doch", ",", "will's", "Gott", ",", "so", "sollst", "du", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "VMFIN", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In stolzen Halmen, frei und stark,", "tokens": ["In", "stol\u00b7zen", "Hal\u00b7men", ",", "frei", "und", "stark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und freie M\u00e4nner solln genie\u00dfen", "tokens": ["Und", "frei\u00b7e", "M\u00e4n\u00b7ner", "solln", "ge\u00b7nie\u00b7\u00dfen"], "token