{"textgrid.poem.35850": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Verbirg Dein Antlitz nicht vor mir", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! hab' ich Dir nicht Alles hingegeben,", "tokens": ["Herr", "!", "hab'", "ich", "Dir", "nicht", "Al\u00b7les", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PPER", "PTKNEG", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Lieben, Wollen, Denken und mein Sein?", "tokens": ["Mein", "Lie\u00b7ben", ",", "Wol\u00b7len", ",", "Den\u00b7ken", "und", "mein", "Sein", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "$,", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Dich, mit Dir, in Dir nur will ich leben,", "tokens": ["F\u00fcr", "Dich", ",", "mit", "Dir", ",", "in", "Dir", "nur", "will", "ich", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Dir allein soll treu die Seele streben;", "tokens": ["Nach", "Dir", "al\u00b7lein", "soll", "treu", "die", "See\u00b7le", "stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo Du ihr fehlst, kennt sie nur Nacht und Pein.", "tokens": ["Wo", "Du", "ihr", "fehlst", ",", "kennt", "sie", "nur", "Nacht", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Um Dich hab' ich ein Mutterherz verlassen", "tokens": ["Um", "Dich", "hab'", "ich", "ein", "Mut\u00b7ter\u00b7herz", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Bruder, Schwester, Freund und Vaterland.", "tokens": ["Und", "Bru\u00b7der", ",", "Schwes\u00b7ter", ",", "Freund", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich lie\u00df den vollen Freudenkranz erblassen,", "tokens": ["Ich", "lie\u00df", "den", "vol\u00b7len", "Freu\u00b7den\u00b7kranz", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um Deine Dornenkrone zu erfassen.", "tokens": ["Um", "Dei\u00b7ne", "Dor\u00b7nen\u00b7kro\u00b7ne", "zu", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, la\u00df mich nicht! O, gieb mir Deine Hand! \u2013", "tokens": ["O", ",", "la\u00df", "mich", "nicht", "!", "O", ",", "gieb", "mir", "Dei\u00b7ne", "Hand", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "PTKNEG", "$.", "NE", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du spielst ein grausam Spiel mit meinem Herzen,", "tokens": ["Du", "spielst", "ein", "grau\u00b7sam", "Spiel", "mit", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verbirgst mir Deines reinen Lichtes Schein,", "tokens": ["Ver\u00b7birgst", "mir", "Dei\u00b7nes", "rei\u00b7nen", "Lich\u00b7tes", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und um mich her seh' ich das Leben scherzen,", "tokens": ["Und", "um", "mich", "her", "seh'", "ich", "das", "Le\u00b7ben", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir bietend tausend bunte Freudenkerzen \u2013", "tokens": ["Mir", "bie\u00b7tend", "tau\u00b7send", "bun\u00b7te", "Freu\u00b7den\u00b7ker\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, la\u00df mich nicht! In Dir ist Licht allein! \u2013", "tokens": ["O", ",", "la\u00df", "mich", "nicht", "!", "In", "Dir", "ist", "Licht", "al\u00b7lein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "PTKNEG", "$.", "APPR", "PPER", "VAFIN", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}