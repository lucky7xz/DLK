{"dta.poem.858": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlV.  Auff den Sontag de\u00df Hertz erkennen-  \n den Prophetens/ oder  VIII.  Sontag nach  \n dem Fest der H. Dreyeinigkeit Matth. 7.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht grosser Bl\u00e4tter art: nicht weiter \u00e4ste sprossen: ", "tokens": ["Nicht", "gros\u00b7ser", "Bl\u00e4t\u00b7ter", "art", ":", "nicht", "wei\u00b7ter", "\u00e4s\u00b7te", "spros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "$.", "PTKNEG", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht hoher St\u00e4mme Macht/ nicht zartter Bl\u00fcten", "tokens": ["Nicht", "ho\u00b7her", "St\u00e4m\u00b7me", "Macht", "/", "nicht", "zart\u00b7ter", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$(", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "licht. ", "tokens": ["licht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ist was den Bawm bewehrt/ Mann suchet nur die Fr\u00fccht", "tokens": ["Ist", "was", "den", "Bawm", "be\u00b7wehrt", "/", "Mann", "su\u00b7chet", "nur", "die", "Fr\u00fccht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN", "VVPP", "$(", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Al\u00df bald die reiffe zeit de\u00df Sommers ist verflossen/", "tokens": ["Al\u00df", "bald", "die", "reif\u00b7fe", "zeit", "de\u00df", "Som\u00b7mers", "ist", "ver\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Zweig verraucht von dem nie Jemand was genossen:", "tokens": ["Der", "Zweig", "ver\u00b7raucht", "von", "dem", "nie", "Je\u00b7mand", "was", "ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADV", "PIS", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nutzen sch\u00f6ne Wort vnd kluge Reden nicht", "tokens": ["So", "nut\u00b7zen", "sch\u00f6\u00b7ne", "Wort", "vnd", "klu\u00b7ge", "Re\u00b7den", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Gott den schlimmen Wolff nach seinen thaten richt", "tokens": ["Wenn", "Gott", "den", "schlim\u00b7men", "Wolff", "nach", "sei\u00b7nen", "tha\u00b7ten", "richt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Christum in den Mund/ nie in das Hertz verschlossen.", "tokens": ["Der", "Chris\u00b7tum", "in", "den", "Mund", "/", "nie", "in", "das", "Hertz", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drumb pr\u00fcfe Mensch die Werck/ schaw nicht die Klei-", "tokens": ["Drumb", "pr\u00fc\u00b7fe", "Mensch", "die", "Werck", "/", "schaw", "nicht", "die", "Klei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "ART", "NN", "$(", "VVFIN", "PTKNEG", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der an: ", "tokens": ["der", "an", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es ist kein Distelstrauch der Feigen bringen kan.", "tokens": ["Es", "ist", "kein", "Dis\u00b7tel\u00b7strauch", "der", "Fei\u00b7gen", "brin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob dessen bl\u00fct\u2019 auch schon von fernen Rosen gleichet/", "tokens": ["Ob", "des\u00b7sen", "bl\u00fct'", "auch", "schon", "von", "fer\u00b7nen", "Ro\u00b7sen", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob schon manch Mord Prophet/ Herr ohn auffh\u00f6ren", "tokens": ["Ob", "schon", "manch", "Mord", "Pro\u00b7phet", "/", "Herr", "ohn", "auff\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "NN", "$(", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schrey't ", "tokens": ["schrey't"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vnd wol den Teufel zwingt: kom\u2019t doch die hartte zeit", "tokens": ["Vnd", "wol", "den", "Teu\u00b7fel", "zwingt", ":", "kom't", "doch", "die", "hart\u00b7te", "zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In welcher ", "tokens": ["In", "wel\u00b7cher"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}