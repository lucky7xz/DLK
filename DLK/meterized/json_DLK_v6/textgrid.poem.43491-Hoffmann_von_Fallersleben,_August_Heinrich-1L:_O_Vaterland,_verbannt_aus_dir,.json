{"textgrid.poem.43491": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Vaterland, verbannt aus dir,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vaterland, verbannt aus dir,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "ver\u00b7bannt", "aus", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was w\u00e4re doch das Leben mir!", "tokens": ["Was", "w\u00e4\u00b7re", "doch", "das", "Le\u00b7ben", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll ich verbannet sein,", "tokens": ["Soll", "ich", "ver\u00b7ban\u00b7net", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So bin ich \u00fcberall allein.", "tokens": ["So", "bin", "ich", "\u00fc\u00b7be\u00b7rall", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verbannt mich aus dem Lande nicht!", "tokens": ["Ver\u00b7bannt", "mich", "aus", "dem", "Lan\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O haltet nicht so streng Gericht!", "tokens": ["O", "hal\u00b7tet", "nicht", "so", "streng", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll ich verbannet sein,", "tokens": ["Soll", "ich", "ver\u00b7ban\u00b7net", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So \u00fcbet Gnad' und sperrt mich ein!", "tokens": ["So", "\u00fc\u00b7bet", "Gnad'", "und", "sperrt", "mich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O werft mich nicht in Kerkers Nacht,", "tokens": ["O", "werft", "mich", "nicht", "in", "Ker\u00b7kers", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Scherg und B\u00fcttel streng bewacht!", "tokens": ["Von", "Scherg", "und", "B\u00fct\u00b7tel", "streng", "be\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll ich begraben sein,", "tokens": ["Soll", "ich", "be\u00b7gra\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So senkt mich in das Grab hinein!", "tokens": ["So", "senkt", "mich", "in", "das", "Grab", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}