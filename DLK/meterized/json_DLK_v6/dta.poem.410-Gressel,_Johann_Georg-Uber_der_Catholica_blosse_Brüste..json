{"dta.poem.410": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber der  Catholica  blosse Br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du tr\u00e4gst Catholica die Br\u00fcste ziemlich blo\u00df/", "tokens": ["Du", "tr\u00e4gst", "Cat\u00b7ho\u00b7li\u00b7ca", "die", "Br\u00fcs\u00b7te", "ziem\u00b7lich", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und reichst der l\u00fcstern Welt die scharff verbohtne Frucht", "tokens": ["Und", "reichst", "der", "l\u00fcs\u00b7tern", "Welt", "die", "scharff", "ver\u00b7boht\u00b7ne", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als eine ", "tokens": ["Als", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Weil sie ein reitzend Feur der Liebe in sich hegt/", "tokens": ["Weil", "sie", "ein", "reit\u00b7zend", "Feur", "der", "Lie\u00b7be", "in", "sich", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das da den ", "tokens": ["Das", "da", "den"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Allein sie ist mit Gifft und Wermuth angew\u00fcrtzt/", "tokens": ["Al\u00b7lein", "sie", "ist", "mit", "Gifft", "und", "Wer\u00b7muth", "an\u00b7ge\u00b7w\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man wird/ indem man i\u00dft/ ins schwartze Grab gest\u00fcrtzt/", "tokens": ["Man", "wird", "/", "in\u00b7dem", "man", "i\u00dft", "/", "ins", "schwart\u00b7ze", "Grab", "ge\u00b7st\u00fcrtzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "KOUS", "PIS", "VVFIN", "$(", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn ist die Wollust klein/ und das Verderben gro\u00df.", "tokens": ["Denn", "ist", "die", "Wol\u00b7lust", "klein", "/", "und", "das", "Ver\u00b7der\u00b7ben", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "$(", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}