{"textgrid.poem.45898": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[aus K\u00f6llen war ein Edelknecht]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus K\u00f6llen war ein Edelknecht,", "tokens": ["Aus", "K\u00f6l\u00b7len", "war", "ein", "E\u00b7del\u00b7knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Botschaft ausgegangen", "tokens": ["Um", "Bot\u00b7schaft", "aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Vater hielt ihm Engelbrecht", "tokens": ["Den", "Va\u00b7ter", "hielt", "ihm", "En\u00b7gel\u00b7brecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bischof hart gefangen.", "tokens": ["Der", "Bi\u00b7schof", "hart", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er gieng gen Arle manchen Tag", "tokens": ["Er", "gieng", "gen", "Ar\u00b7le", "man\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er gieng in schweren Sorgen,", "tokens": ["Er", "gieng", "in", "schwe\u00b7ren", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Liebchen ihm im Sinne lag,", "tokens": ["Sein", "Lieb\u00b7chen", "ihm", "im", "Sin\u00b7ne", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der h\u00e4tt' er es verborgen,", "tokens": ["Der", "h\u00e4tt'", "er", "es", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ganz traurig er am Brunnen lag,", "tokens": ["Ganz", "trau\u00b7rig", "er", "am", "Brun\u00b7nen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Busch und gr\u00fcnen Hecken,", "tokens": ["In", "Busch", "und", "gr\u00fc\u00b7nen", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt er schallen Hufesschlag,", "tokens": ["Da", "h\u00f6rt", "er", "schal\u00b7len", "Hu\u00b7fes\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gieng sich zu verstecken.", "tokens": ["Und", "gieng", "sich", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sah da einen frohen Mann", "tokens": ["Er", "sah", "da", "ei\u00b7nen", "fro\u00b7hen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Ro\u00df zur Quelle lenken,", "tokens": ["Sein", "Ro\u00df", "zur", "Quel\u00b7le", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein andrer ritt betr\u00fcbt heran,", "tokens": ["Ein", "an\u00b7drer", "ritt", "be\u00b7tr\u00fcbt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Pferd am Born zu tr\u00e4nken.", "tokens": ["Sein", "Pferd", "am", "Born", "zu", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Betr\u00fcbter Mann! der frohe sprach", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "Mann", "!", "der", "fro\u00b7he", "sprach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott woll' dir Trost verleihen!", "tokens": ["Gott", "woll'", "dir", "Trost", "ver\u00b7lei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O froher Mann! der andre sprach", "tokens": ["O", "fro\u00b7her", "Mann", "!", "der", "and\u00b7re", "sprach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was mag dich so erfreuen!", "tokens": ["Was", "mag", "dich", "so", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr Gottschalk sprach der frohe Mann", "tokens": ["Herr", "Gott\u00b7schalk", "sprach", "der", "fro\u00b7he", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht frei aus seinen Banden,", "tokens": ["Geht", "frei", "aus", "sei\u00b7nen", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch ein Mirakel er entrann", "tokens": ["Durch", "ein", "Mi\u00b7ra\u00b7kel", "er", "ent\u00b7rann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen den Verbannten.", "tokens": ["Mit", "al\u00b7len", "den", "Ver\u00b7bann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er hatte eine kleine Maus", "tokens": ["Er", "hat\u00b7te", "ei\u00b7ne", "klei\u00b7ne", "Maus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kerker zahm erzogen,", "tokens": ["Im", "Ker\u00b7ker", "zahm", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die gieng da freundlich ein und aus,", "tokens": ["Die", "gieng", "da", "freund\u00b7lich", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und war ihm gar gewogen.", "tokens": ["Und", "war", "ihm", "gar", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch einst sein kleiner Freund entlief,", "tokens": ["Doch", "einst", "sein", "klei\u00b7ner", "Freund", "ent\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollte nicht mehr kehren,", "tokens": ["Und", "woll\u00b7te", "nicht", "mehr", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr Gottschalk ihr gar traurig rief", "tokens": ["Herr", "Gott\u00b7schalk", "ihr", "gar", "trau\u00b7rig", "rief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das M\u00e4uslein wollt' nicht h\u00f6ren.", "tokens": ["Das", "M\u00e4us\u00b7lein", "wollt'", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das schmerzte den getreuen Mann,", "tokens": ["Das", "schmerz\u00b7te", "den", "ge\u00b7treu\u00b7en", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein M\u00e4uslein wollt' er haben,", "tokens": ["Sein", "M\u00e4us\u00b7lein", "wollt'", "er", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinen Freunden er begann,", "tokens": ["Mit", "sei\u00b7nen", "Freun\u00b7den", "er", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach ihrem Freund zu graben.", "tokens": ["Nach", "ih\u00b7rem", "Freund", "zu", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und in der Erde eingescharrt", "tokens": ["Und", "in", "der", "Er\u00b7de", "ein\u00b7ge\u00b7scharrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fand Mei\u00dfel er und Feilen,", "tokens": ["Fand", "Mei\u00b7\u00dfel", "er", "und", "Fei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KON", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Womit er ihre Bande hart,", "tokens": ["Wo\u00b7mit", "er", "ih\u00b7re", "Ban\u00b7de", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar leichtlich konnt' zerteilen.", "tokens": ["Gar", "leicht\u00b7lich", "konnt'", "zer\u00b7tei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der andre sprach, mein Schwesterlein", "tokens": ["Der", "and\u00b7re", "sprach", ",", "mein", "Schwes\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es liegt gar schwer gefangen,", "tokens": ["Es", "liegt", "gar", "schwer", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und selbst das treue M\u00e4uslein dein", "tokens": ["Und", "selbst", "das", "treu\u00b7e", "M\u00e4us\u00b7lein", "dein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nnt' nicht zu ihr gelangen.", "tokens": ["K\u00f6nnt'", "nicht", "zu", "ihr", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Des Schlosses Dach ist Himmelblau,", "tokens": ["Des", "Schlos\u00b7ses", "Dach", "ist", "Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mauren gr\u00fcne Wellen,", "tokens": ["Die", "Mau\u00b7ren", "gr\u00fc\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Graben rings sind Flur und Au,", "tokens": ["Die", "Gra\u00b7ben", "rings", "sind", "Flur", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fenster Flu\u00df und Quellen.", "tokens": ["Die", "Fens\u00b7ter", "Flu\u00df", "und", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der s\u00fc\u00dfe Knecht die Liebe brach", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Knecht", "die", "Lie\u00b7be", "brach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihres Herzens Kammer,", "tokens": ["In", "ih\u00b7res", "Her\u00b7zens", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm st\u00fcrzten die Gesellen nach,", "tokens": ["Ihm", "st\u00fcrz\u00b7ten", "die", "Ge\u00b7sel\u00b7len", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schmerz und b\u00f6ser Jammer.", "tokens": ["Der", "Schmerz", "und", "b\u00f6\u00b7ser", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Liebe blies das L\u00e4mpchen aus", "tokens": ["Die", "Lie\u00b7be", "blies", "das", "L\u00e4mp\u00b7chen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schmerzen sie bezwangen,", "tokens": ["Die", "Schmer\u00b7zen", "sie", "be\u00b7zwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und legten sie ins k\u00fchle Haus", "tokens": ["Und", "leg\u00b7ten", "sie", "ins", "k\u00fch\u00b7le", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl auf den Tod gefangen.", "tokens": ["Wohl", "auf", "den", "Tod", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Am Fels wo wild der Rhein zerschellt,", "tokens": ["Am", "Fels", "wo", "wild", "der", "Rhein", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWAV", "ADJD", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo b\u00f6s die Schiffe stranden,", "tokens": ["Wo", "b\u00f6s", "die", "Schif\u00b7fe", "stran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort ewig Sie gefangen h\u00e4lt,", "tokens": ["Dort", "e\u00b7wig", "Sie", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "VVFIN", "$,"], "meter