{"textgrid.poem.44093": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Cr\u00f6nt der Ausgang die Gedult,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cr\u00f6nt der Ausgang die Gedult,", "tokens": ["Cr\u00f6nt", "der", "Aus\u00b7gang", "die", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht die Hofnung endlich Rosen,", "tokens": ["Bricht", "die", "Hof\u00b7nung", "end\u00b7lich", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So gedenck ich stets der Huld", "tokens": ["So", "ge\u00b7denck", "ich", "stets", "der", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Gottes liebzukosen,", "tokens": ["Mei\u00b7nes", "Got\u00b7tes", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob sein Trost gleich oft verzieht", "tokens": ["Ob", "sein", "Trost", "gleich", "oft", "ver\u00b7zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Gl\u00fccke langsam bl\u00fcht.", "tokens": ["Und", "das", "Gl\u00fc\u00b7cke", "lang\u00b7sam", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Christen sind darzu versehn,", "tokens": ["Chris\u00b7ten", "sind", "dar\u00b7zu", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie sich im Warthen \u00fcben", "tokens": ["Da\u00df", "sie", "sich", "im", "Wart\u00b7hen", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch unter Creuz und Flehn", "tokens": ["Und", "auch", "un\u00b7ter", "Creuz", "und", "Flehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die getreue Pr\u00fcfung lieben,", "tokens": ["Die", "ge\u00b7treu\u00b7e", "Pr\u00fc\u00b7fung", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die nach mancher finstern Nacht", "tokens": ["Die", "nach", "man\u00b7cher", "fins\u00b7tern", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihren Morgen heller macht.", "tokens": ["Ih\u00b7ren", "Mor\u00b7gen", "hel\u00b7ler", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Geht man doch nach Canaan", "tokens": ["Geht", "man", "doch", "nach", "Ca\u00b7naan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NE"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Erst durch Umschweif zum Vergn\u00fcgen;", "tokens": ["Erst", "durch", "Um\u00b7schweif", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wer mit Gro\u00dfmuth streiten kan,", "tokens": ["Wer", "mit", "Gro\u00df\u00b7muth", "strei\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mit gr\u00f6\u00dfrer Ehre siegen,", "tokens": ["Wird", "mit", "gr\u00f6\u00df\u00b7rer", "Eh\u00b7re", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zum Tempel s\u00fc\u00dfer Ruh", "tokens": ["Und", "zum", "Tem\u00b7pel", "s\u00fc\u00b7\u00dfer", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht man stets geradezu.", "tokens": ["Geht", "man", "stets", "ge\u00b7ra\u00b7de\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vor dem Winter bl\u00fcht kein May,", "tokens": ["Vor", "dem", "Win\u00b7ter", "bl\u00fcht", "kein", "May", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Kummer keine Freude;", "tokens": ["Oh\u00b7ne", "Kum\u00b7mer", "kei\u00b7ne", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gela\u00dfenheit macht frey", "tokens": ["Die", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "macht", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erquickt uns nach dem Leide,", "tokens": ["Und", "er\u00b7quickt", "uns", "nach", "dem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Regen und der West", "tokens": ["Wie", "der", "Re\u00b7gen", "und", "der", "West"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00fcrre Saaten wachsen l\u00e4st.", "tokens": ["D\u00fcr\u00b7re", "Saa\u00b7ten", "wach\u00b7sen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seele schweig und murre nicht", "tokens": ["See\u00b7le", "schweig", "und", "mur\u00b7re", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "VVFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die V\u00f6lcker in der W\u00fcsten,", "tokens": ["Wie", "die", "V\u00f6l\u00b7cker", "in", "der", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor Moses Angesicht", "tokens": ["Die", "vor", "Mo\u00b7ses", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich ihren Frevel b\u00fc\u00dften;", "tokens": ["End\u00b7lich", "ih\u00b7ren", "Fre\u00b7vel", "b\u00fc\u00df\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn durch solchen Unverstand", "tokens": ["Denn", "durch", "sol\u00b7chen", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt man ums gelobte Land.", "tokens": ["Kommt", "man", "ums", "ge\u00b7lob\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Traurig vor der tummen Welt", "tokens": ["Trau\u00b7rig", "vor", "der", "tum\u00b7men", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und doch fr\u00f6hlich im Gem\u00fcthe!", "tokens": ["Und", "doch", "fr\u00f6h\u00b7lich", "im", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer den Glauben fest beh\u00e4lt,", "tokens": ["Wer", "den", "Glau\u00b7ben", "fest", "be\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Merckt im Schlagen Gottes G\u00fcte", "tokens": ["Merckt", "im", "Schla\u00b7gen", "Got\u00b7tes", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erg\u00f6zt sich fort und fort", "tokens": ["Und", "er\u00b7g\u00f6zt", "sich", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch des H\u00f6chsten wahres Wort.", "tokens": ["Durch", "des", "H\u00f6chs\u00b7ten", "wah\u00b7res", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unser Leben bringt's so mit:", "tokens": ["Un\u00b7ser", "Le\u00b7ben", "bringt's", "so", "mit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abends Gram und morgens Lachen;", "tokens": ["A\u00b7bends", "Gram", "und", "mor\u00b7gens", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, so gestern schmerzlich lidt,", "tokens": ["Der", ",", "so", "ge\u00b7stern", "schmerz\u00b7lich", "lidt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kan sich heute lustig machen.", "tokens": ["Kan", "sich", "heu\u00b7te", "lus\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird nicht jeder Wuntsch erf\u00fcllt,", "tokens": ["Wird", "nicht", "je\u00b7der", "Wunt\u00b7sch", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gnug, wenn nur der beste gilt.", "tokens": ["Gnug", ",", "wenn", "nur", "der", "bes\u00b7te", "gilt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df der beste gelten mu\u00df,", "tokens": ["Da\u00df", "der", "bes\u00b7te", "gel\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan Vernunft und Schrift beweisen;", "tokens": ["Kan", "Ver\u00b7nunft", "und", "Schrift", "be\u00b7wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum will ich stets den Schlu\u00df", "tokens": ["Da\u00b7rum", "will", "ich", "stets", "den", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines h\u00f6chsten Vaters preisen,", "tokens": ["Mei\u00b7nes", "h\u00f6chs\u00b7ten", "Va\u00b7ters", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der bey allem, was er schickt,", "tokens": ["Der", "bey", "al\u00b7lem", ",", "was", "er", "schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Kinder Wohlseyn blickt.", "tokens": ["Auf", "der", "Kin\u00b7der", "Wohl\u00b7seyn", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Niemand lebt wohl ohne Feind,", "tokens": ["Nie\u00b7mand", "lebt", "wohl", "oh\u00b7ne", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner kan der Misgunst weichen;", "tokens": ["Kei\u00b7ner", "kan", "der", "Mis\u00b7gunst", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer uns noch so redlich scheint,", "tokens": ["Wer", "uns", "noch", "so", "red\u00b7lich", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kan uns oft mit List beschleichen,", "tokens": ["Kan", "uns", "oft", "mit", "List", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Undancks Spott und Hohn", "tokens": ["Und", "des", "Un\u00b7dancks", "Spott", "und", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Giebt vor Wohlthat schlimmen Lohn.", "tokens": ["Giebt", "vor", "Wohlt\u00b7hat", "schlim\u00b7men", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Immerhin, ich leid es gern", "tokens": ["Im\u00b7mer\u00b7hin", ",", "ich", "leid", "es", "gern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bin in mir selbst zufrieden;", "tokens": ["Und", "bin", "in", "mir", "selbst", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Untreu schl\u00e4gt den eignen Herrn.", "tokens": ["Un\u00b7treu", "schl\u00e4gt", "den", "eig\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mir Gl\u00fcck und Gott beschieden,", "tokens": ["Was", "mir", "Gl\u00fcck", "und", "Gott", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt mir dennoch nie zu sp\u00e4t,", "tokens": ["Kommt", "mir", "den\u00b7noch", "nie", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob sich's noch so artig dreht.", "tokens": ["Ob", "sich's", "noch", "so", "ar\u00b7tig", "dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Thoren mehren sich die Last", "tokens": ["Tho\u00b7ren", "meh\u00b7ren", "sich", "die", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch ein unruhvolles Sehnen;", "tokens": ["Durch", "ein", "un\u00b7ruh\u00b7vol\u00b7les", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer den Hofnungsancker fast,", "tokens": ["Wer", "den", "Hof\u00b7nungs\u00b7an\u00b7cker", "fast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der lacht allzeit auch durch Thr\u00e4nen", "tokens": ["Der", "lacht", "all\u00b7zeit", "auch", "durch", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und erlangt durch Meer und Nord", "tokens": ["Und", "er\u00b7langt", "durch", "Meer", "und", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Fluch den Seegensport.", "tokens": ["Oh\u00b7ne", "Fluch", "den", "See\u00b7gen\u00b7sport", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos"