{"textgrid.poem.46073": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[\u00fcber Berg und Tal getragen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Berg und Tal getragen,", "tokens": ["\u00dc\u00b7ber", "Berg", "und", "Tal", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder in den See getaucht,", "tokens": ["Nie\u00b7der", "in", "den", "See", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist das Licht von meinen Tagen", "tokens": ["Ist", "das", "Licht", "von", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Klagen", "tokens": ["Und", "in", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Bebend meine Seele haucht.", "tokens": ["Be\u00b7bend", "mei\u00b7ne", "See\u00b7le", "haucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sonne magst nur niedersinken,", "tokens": ["Son\u00b7ne", "magst", "nur", "nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh nur auf, du roter Mond", "tokens": ["Geh", "nur", "auf", ",", "du", "ro\u00b7ter", "Mond"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Sterne m\u00f6gt nur blinken,", "tokens": ["Und", "ihr", "Ster\u00b7ne", "m\u00f6gt", "nur", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' mein Flehen nicht belohnt", "tokens": ["Hab'", "mein", "Fle\u00b7hen", "nicht", "be\u00b7lohnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Euer Winken", "tokens": ["Eu\u00b7er", "Win\u00b7ken"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sagt mir nicht, wo Lindi wohnt.", "tokens": ["Sagt", "mir", "nicht", ",", "wo", "Lin\u00b7di", "wohnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Jetzt mag wohl die Lilie drehen,", "tokens": ["Jetzt", "mag", "wohl", "die", "Li\u00b7lie", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihren Kelch zu diesem Stern", "tokens": ["Ih\u00b7ren", "Kelch", "zu", "die\u00b7sem", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Licht und k\u00fchlem Wehen", "tokens": ["Und", "mit", "Licht", "und", "k\u00fch\u00b7lem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllen, was ich k\u00fc\u00dfte gern", "tokens": ["F\u00fcl\u00b7len", ",", "was", "ich", "k\u00fc\u00df\u00b7te", "gern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hei\u00dfes Flehen!", "tokens": ["Hei\u00b7\u00dfes", "Fle\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Zieh zu ihr, zu ihr so fern!", "tokens": ["Zieh", "zu", "ihr", ",", "zu", "ihr", "so", "fern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "War ich doch die kleine M\u00fccke,", "tokens": ["War", "ich", "doch", "die", "klei\u00b7ne", "M\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf ihren Nacken flog", "tokens": ["Die", "auf", "ih\u00b7ren", "Na\u00b7cken", "flog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Tr\u00f6pfchen meinem Gl\u00fccke,", "tokens": ["Und", "ein", "Tr\u00f6pf\u00b7chen", "mei\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den blauen Adern flog", "tokens": ["Aus", "den", "blau\u00b7en", "A\u00b7dern", "flog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df zur\u00fccke", "tokens": ["Da\u00df", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sie nach mir sich z\u00fcrnend bog.", "tokens": ["Sie", "nach", "mir", "sich", "z\u00fcr\u00b7nend", "bog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "War ich doch das Regentr\u00f6pfchen,", "tokens": ["War", "ich", "doch", "das", "Re\u00b7gen\u00b7tr\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ihr auf die Wange fiel", "tokens": ["Das", "ihr", "auf", "die", "Wan\u00b7ge", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie sch\u00fcttelte das K\u00f6pfchen", "tokens": ["Und", "sie", "sch\u00fct\u00b7tel\u00b7te", "das", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es rann zu s\u00fc\u00dferm Ziel.", "tokens": ["Und", "es", "rann", "zu", "s\u00fc\u00b7\u00dferm", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00df Gesch\u00f6pfchen!", "tokens": ["S\u00fc\u00df", "Ge\u00b7sch\u00f6pf\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "O das war zu viel, zu viel!", "tokens": ["O", "das", "war", "zu", "viel", ",", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "PTKA", "PIS", "$,", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Alles war ich, war das Sehnen,", "tokens": ["Al\u00b7les", "war", "ich", ",", "war", "das", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sie gr\u00fc\u00dft im Abendrot,", "tokens": ["Das", "sie", "gr\u00fc\u00dft", "im", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War der Herdenglocke T\u00f6nen,", "tokens": ["War", "der", "Her\u00b7den\u00b7glo\u00b7cke", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ihr Gru\u00df der Heimat bot,", "tokens": ["Das", "ihr", "Gru\u00df", "der", "Hei\u00b7mat", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War die Tr\u00e4nen,", "tokens": ["War", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "War die unbestimmte Not", "tokens": ["War", "die", "un\u00b7be\u00b7stimm\u00b7te", "Not"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "O du durst'ger Strahl der Augen,", "tokens": ["O", "du", "dur\u00b7st'\u00b7ger", "Strahl", "der", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der zum Monde trinken geht,", "tokens": ["Der", "zum", "Mon\u00b7de", "trin\u00b7ken", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig f\u00fchlte ich dein Saugen!", "tokens": ["Se\u00b7lig", "f\u00fchl\u00b7te", "ich", "dein", "Sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Licht und Luft, die mich umweht", "tokens": ["Licht", "und", "Luft", ",", "die", "mich", "um\u00b7weht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War das Hauchen", "tokens": ["War", "das", "Hau\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ihrer Lippen im Gebet.", "tokens": ["Ih\u00b7rer", "Lip\u00b7pen", "im", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Gute Ruh', gute Ruh',", "tokens": ["Gu\u00b7te", "Ruh'", ",", "gu\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O s\u00fc\u00dfe Turtel wie marterst du!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Tur\u00b7tel", "wie", "mar\u00b7terst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Eines wei\u00df ich, eine Decke", "tokens": ["Ei\u00b7nes", "wei\u00df", "ich", ",", "ei\u00b7ne", "De\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchl und lind, auf der sie ruht,", "tokens": ["K\u00fchl", "und", "lind", ",", "auf", "der", "sie", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die von Ecke ich zu Ecke", "tokens": ["Die", "von", "E\u00b7cke", "ich", "zu", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckte mit der K\u00fcsse Glut. \u2013", "tokens": ["Deck\u00b7te", "mit", "der", "K\u00fcs\u00b7se", "Glut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht erschrecke", "tokens": ["Nicht", "er\u00b7schre\u00b7cke"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "S\u00fc\u00dfes Lieb, o schla