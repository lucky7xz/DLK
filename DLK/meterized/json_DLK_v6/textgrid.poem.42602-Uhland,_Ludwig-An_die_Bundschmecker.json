{"textgrid.poem.42602": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Bundschmecker", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr mit scharfen Nasen ausgewittert", "tokens": ["Die", "ihr", "mit", "schar\u00b7fen", "Na\u00b7sen", "aus\u00b7ge\u00b7wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel h\u00f6chst gef\u00e4hrlicher, geheimer B\u00fcnde,", "tokens": ["Viel", "h\u00f6chst", "ge\u00b7f\u00e4hr\u00b7li\u00b7cher", ",", "ge\u00b7hei\u00b7mer", "B\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verg\u00f6nnt mir, da\u00df ich einen euch verk\u00fcnde,", "tokens": ["Ver\u00b7g\u00f6nnt", "mir", ",", "da\u00df", "ich", "ei\u00b7nen", "euch", "ver\u00b7k\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dem ihr wohl bis heute nicht gezittert!", "tokens": ["Vor", "dem", "ihr", "wohl", "bis", "heu\u00b7te", "nicht", "ge\u00b7zit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich kenne, was das Leben euch verbittert,", "tokens": ["Ich", "ken\u00b7ne", ",", "was", "das", "Le\u00b7ben", "euch", "ver\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die arge Pest, die weitvererbte S\u00fcnde:", "tokens": ["Die", "ar\u00b7ge", "Pest", ",", "die", "weit\u00b7ve\u00b7rerb\u00b7te", "S\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sehnsucht, da\u00df ein Deutschland sich begr\u00fcnde,", "tokens": ["Die", "Sehn\u00b7sucht", ",", "da\u00df", "ein", "Deutschland", "sich", "be\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-++--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gesetzlich frei, volkskr\u00e4ftig, unzersplittert;", "tokens": ["Ge\u00b7setz\u00b7lich", "frei", ",", "volks\u00b7kr\u00e4f\u00b7tig", ",", "un\u00b7zer\u00b7split\u00b7tert", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch andres wei\u00df ich, und vernehmt ihr's gerne,", "tokens": ["Doch", "and\u00b7res", "wei\u00df", "ich", ",", "und", "ver\u00b7nehmt", "ih\u00b7r's", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So will ich einen m\u00e4cht'gen Bund verraten,", "tokens": ["So", "will", "ich", "ei\u00b7nen", "m\u00e4cht'\u00b7gen", "Bund", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sich in stillen N\u00e4chten angesponnen:", "tokens": ["Der", "sich", "in", "stil\u00b7len", "N\u00e4ch\u00b7ten", "an\u00b7ge\u00b7spon\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist der gro\u00dfe Bund zahlloser Sterne,", "tokens": ["Es", "ist", "der", "gro\u00b7\u00dfe", "Bund", "zahl\u00b7lo\u00b7ser", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie mir Sp\u00e4her j\u00fcngst zu wissen taten,", "tokens": ["Und", "wie", "mir", "Sp\u00e4\u00b7her", "j\u00fcngst", "zu", "wis\u00b7sen", "ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So steckt dahinter selbst das Licht der Sonnen.", "tokens": ["So", "steckt", "da\u00b7hin\u00b7ter", "selbst", "das", "Licht", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}