{"dta.poem.2214": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es giebet von der Wahrheit Wesen mir die\u00df die deut-", "tokens": ["Es", "gie\u00b7bet", "von", "der", "Wahr\u00b7heit", "We\u00b7sen", "mir", "die\u00df", "die", "deut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "PPER", "PDS", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "lichst' Offenbarung,", "tokens": ["lichst'", "Of\u00b7fen\u00b7ba\u00b7rung", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und den verst\u00e4ndlichsten Begriff: die Wahrheit ist nichts,", "tokens": ["Und", "den", "ver\u00b7st\u00e4nd\u00b7lichs\u00b7ten", "Be\u00b7griff", ":", "die", "Wahr\u00b7heit", "ist", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "als Erfahrung.", "tokens": ["als", "Er\u00b7fah\u00b7rung", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}