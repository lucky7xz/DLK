{"textgrid.poem.53570": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Nach f\u00fcnf Jahren", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Vater tot und Bruder tot", "tokens": ["Und", "Va\u00b7ter", "tot", "und", "Bru\u00b7der", "tot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und einer kriegsgefangen;", "tokens": ["und", "ei\u00b7ner", "kriegs\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und Mutter sitzt in Rentennot:", "tokens": ["und", "Mut\u00b7ter", "sitzt", "in", "Ren\u00b7ten\u00b7not", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was essen meine Rangen . . . ?", "tokens": ["Was", "es\u00b7sen", "mei\u00b7ne", "Ran\u00b7gen", ".", ".", ".", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So stehn wir da im sch\u00e4bigen Kleid", "tokens": ["So", "stehn", "wir", "da", "im", "sch\u00e4\u00b7bi\u00b7gen", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "und denken an die alte Zeit.", "tokens": ["und", "den\u00b7ken", "an", "die", "al\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hassen.", "tokens": ["Und", "has\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Und hassen jenen Preu\u00dfengeist,", "tokens": ["Und", "has\u00b7sen", "je\u00b7nen", "Preu\u00b7\u00dfen\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der uns geduckt, betrogen.", "tokens": ["der", "uns", "ge\u00b7duckt", ",", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hassen, was von Orden glei\u00dft.", "tokens": ["Und", "has\u00b7sen", ",", "was", "von", "Or\u00b7den", "glei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Aar ist fortgeflogen.", "tokens": ["Ihr", "Aar", "ist", "fort\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er hinterlie\u00df als armen Rest", "tokens": ["Er", "hin\u00b7ter\u00b7lie\u00df", "als", "ar\u00b7men", "Rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "uns nur ein ganz beschmutztes Nest", "tokens": ["uns", "nur", "ein", "ganz", "be\u00b7schmutz\u00b7tes", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und graue Elendsmassen.", "tokens": ["und", "grau\u00b7e", "E\u00b7lends\u00b7mas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir hassen.", "tokens": ["Wir", "has\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "H\u00f6r, Bruder, standest du nicht stramm", "tokens": ["H\u00f6r", ",", "Bru\u00b7der", ",", "stan\u00b7dest", "du", "nicht", "stramm"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vor Knechten und vor Schiebern?", "tokens": ["vor", "Knech\u00b7ten", "und", "vor", "Schie\u00b7bern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du gingst zur Schlacht als Opferlamm.", "tokens": ["Du", "gingst", "zur", "Schlacht", "als", "Op\u00b7fer\u00b7lamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir fiebern, fiebern, fiebern . . .", "tokens": ["Wir", "fie\u00b7bern", ",", "fie\u00b7bern", ",", "fie\u00b7bern", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wach auf! Du warst so lange krank!", "tokens": ["Wach", "auf", "!", "Du", "warst", "so", "lan\u00b7ge", "krank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es dauert nicht ein Leben lang!", "tokens": ["Es", "dau\u00b7ert", "nicht", "ein", "Le\u00b7ben", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00dfts nur nicht gehen lassen!", "tokens": ["Mu\u00dfts", "nur", "nicht", "ge\u00b7hen", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wir hassen.", "tokens": ["Wir", "has\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Brenn aus! Brenn aus! Mit Stumpf und Stiel!", "tokens": ["Brenn", "aus", "!", "Brenn", "aus", "!", "Mit", "Stumpf", "und", "Stiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NE", "PTKVZ", "$.", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greif mutig in den Himmel!", "tokens": ["Greif", "mu\u00b7tig", "in", "den", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Oberschicht \u2013 sie z\u00e4hlt nicht viel \u2013", "tokens": ["Die", "O\u00b7ber\u00b7schicht", "\u2013", "sie", "z\u00e4hlt", "nicht", "viel", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "versinkt in dem Gewimmel.", "tokens": ["ver\u00b7sinkt", "in", "dem", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Dreck und Blut und Schlamm und Schmerz", "tokens": ["In", "Dreck", "und", "Blut", "und", "Schlamm", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "blieb uns ein warmes Menschenherz.", "tokens": ["blieb", "uns", "ein", "war\u00b7mes", "Men\u00b7schen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schlag zu mit wuchtigen Hieben!", "tokens": ["Schlag", "zu", "mit", "wuch\u00b7ti\u00b7gen", "Hie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Wir lieben!", "tokens": ["Wir", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}