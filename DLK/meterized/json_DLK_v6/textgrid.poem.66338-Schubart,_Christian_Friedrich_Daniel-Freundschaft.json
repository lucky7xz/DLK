{"textgrid.poem.66338": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundschaft, Himmelstochter,", "tokens": ["Freund\u00b7schaft", ",", "Him\u00b7mel\u00b7stoch\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Komm und h\u00f6re mich!", "tokens": ["Komm", "und", "h\u00f6\u00b7re", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Im geweihten Liede", "tokens": ["Im", "ge\u00b7weih\u00b7ten", "Lie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "G\u00f6ttin, sing ich dich.", "tokens": ["G\u00f6t\u00b7tin", ",", "sing", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "La\u00df von Sympathieen", "tokens": ["La\u00df", "von", "Sym\u00b7pa\u00b7thi\u00b7een"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Meine Seele gl\u00fchen,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df von deinem Licht erhellt,", "tokens": ["Da\u00df", "von", "dei\u00b7nem", "Licht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir das Lied gef\u00e4llt.", "tokens": ["Dir", "das", "Lied", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In der W\u00fcste trauernd", "tokens": ["In", "der", "W\u00fcs\u00b7te", "trau\u00b7ernd"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hat ein Menschenfreund", "tokens": ["Hat", "ein", "Men\u00b7schen\u00b7freund"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einstens vor dem Himmel", "tokens": ["Eins\u00b7tens", "vor", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seinen Gram geweint:", "tokens": ["Sei\u00b7nen", "Gram", "ge\u00b7weint", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "\u00bbsch\u00f6pfer meines Herzens,", "tokens": ["\u00bb", "sch\u00f6p\u00b7fer", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kenner meines Schmerzens,", "tokens": ["Ken\u00b7ner", "mei\u00b7nes", "Schmer\u00b7zens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sprich, was soll dies Zittern hier,", "tokens": ["Sprich", ",", "was", "soll", "dies", "Zit\u00b7tern", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VMFIN", "PDS", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser Drang in mir?", "tokens": ["Die\u00b7ser", "Drang", "in", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "L\u00f6w' und Wolf und Tiger,", "tokens": ["L\u00f6\u00b7w'", "und", "Wolf", "und", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wild und zahmes Vieh", "tokens": ["Wild", "und", "zah\u00b7mes", "Vieh"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Haben f\u00fcr mich Armen", "tokens": ["Ha\u00b7ben", "f\u00fcr", "mich", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Keine Sympathie.", "tokens": ["Kei\u00b7ne", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Felsen, Berge, Meere", "tokens": ["Fel\u00b7sen", ",", "Ber\u00b7ge", ",", "Mee\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00fcllen nicht die Leere,", "tokens": ["F\u00fcl\u00b7len", "nicht", "die", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hellen nicht die Dunkelheit,", "tokens": ["Hel\u00b7len", "nicht", "die", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mein Herz entweiht.\u00ab", "tokens": ["Die", "mein", "Herz", "ent\u00b7weiht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gott der Menschenvater", "tokens": ["Gott", "der", "Men\u00b7schen\u00b7va\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00f6rt den Klager an;", "tokens": ["H\u00f6rt", "den", "Kla\u00b7ger", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und, mit Himmelsklarheit", "tokens": ["Und", ",", "mit", "Him\u00b7mels\u00b7klar\u00b7heit"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lieblich angethan,", "tokens": ["Lieb\u00b7lich", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kam zum Menschenfreunde,", "tokens": ["Kam", "zum", "Men\u00b7schen\u00b7freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der in W\u00fcsten weinte,", "tokens": ["Der", "in", "W\u00fcs\u00b7ten", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Freundschaft. \u2013 Gro\u00df und gut und mild", "tokens": ["Freund\u00b7schaft", ".", "\u2013", "Gro\u00df", "und", "gut", "und", "mild"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "NE", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "War der G\u00f6ttin Bild.", "tokens": ["War", "der", "G\u00f6t\u00b7tin", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ihre Lippe hauchte", "tokens": ["Ih\u00b7re", "Lip\u00b7pe", "hauch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sanft ins Menschenherz", "tokens": ["Sanft", "ins", "Men\u00b7schen\u00b7herz"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mitgef\u00fchl f\u00fcr Freude,", "tokens": ["Mit\u00b7ge\u00b7f\u00fchl", "f\u00fcr", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mitgef\u00fchl f\u00fcr Schmerz;", "tokens": ["Mit\u00b7ge\u00b7f\u00fchl", "f\u00fcr", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Seelen wurden Flammen,", "tokens": ["See\u00b7len", "wur\u00b7den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schlangen sich zusammen,", "tokens": ["Schlan\u00b7gen", "sich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und zum Herzenbilder drang", "tokens": ["Und", "zum", "Her\u00b7zen\u00b7bil\u00b7der", "drang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun ein ", "tokens": ["Nun", "ein"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Freundschaft macht die Menschen", "tokens": ["Freund\u00b7schaft", "macht", "die", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gottes Engeln gleich,", "tokens": ["Got\u00b7tes", "En\u00b7geln", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Macht sie froh im Kummer,", "tokens": ["Macht", "sie", "froh", "im", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der Armuth reich;", "tokens": ["In", "der", "Ar\u00b7muth", "reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.5": {"text": "Und an ihrem Stabe", "tokens": ["Und", "an", "ih\u00b7rem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wandeln wir zum Grabe,", "tokens": ["Wan\u00b7deln", "wir", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sprechen zu dem Freunde: dort", "tokens": ["Spre\u00b7chen", "zu", "dem", "Freun\u00b7de", ":", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Daurt die Freundschaft fort.", "tokens": ["Daurt", "die", "Freund\u00b7schaft", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Freunde, stark und dauernd", "tokens": ["Freun\u00b7de", ",", "stark", "und", "dau\u00b7ernd"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie die Ewigkeit", "tokens": ["Wie", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ist die Brudertreue,", "tokens": ["Ist", "die", "Bru\u00b7der\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die ich Euch geweiht.", "tokens": ["Die", "ich", "Euch", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Macht nicht M\u00e4dchenliebe", "tokens": ["Macht", "nicht", "M\u00e4d\u00b7chen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Oft das Leben tr\u00fcbe?", "tokens": ["Oft", "das", "Le\u00b7ben", "tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nur die Freundschaft hat allein", "tokens": ["Nur", "die", "Freund\u00b7schaft", "hat", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig Sonnenschein.", "tokens": ["E\u00b7wig", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}