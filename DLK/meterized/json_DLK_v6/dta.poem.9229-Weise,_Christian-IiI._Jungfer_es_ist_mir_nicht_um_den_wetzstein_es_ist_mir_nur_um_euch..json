{"dta.poem.9229": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Jungfer/ es ist mir nicht um den wetzstein/ es  \n ist mir nur um euch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Um den wetzstein ist mirs nicht/", "tokens": ["Um", "den", "wetz\u00b7stein", "ist", "mirs", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "NE", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es ist mir nur um euch/", "tokens": ["Es", "ist", "mir", "nur", "um", "euch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "G\u00f6nnt mir euer angesicht/", "tokens": ["G\u00f6nnt", "mir", "eu\u00b7er", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst gilt mir alles gleich:", "tokens": ["Sonst", "gilt", "mir", "al\u00b7les", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein creutz/ mein blut/", "tokens": ["Mein", "creutz", "/", "mein", "blut", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Seyd ihr mir gut/", "tokens": ["Seyd", "ihr", "mir", "gut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So hab ich warlich viel/", "tokens": ["So", "hab", "ich", "war\u00b7lich", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der arme stein", "tokens": ["Der", "ar\u00b7me", "stein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mag immer seyn", "tokens": ["Mag", "im\u00b7mer", "seyn"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VAINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und bleiben/ wo er wil.", "tokens": ["Und", "blei\u00b7ben", "/", "wo", "er", "wil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Nun hab ich die h\u00f6fflichkeit", "tokens": ["Nun", "hab", "ich", "die", "h\u00f6ff\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu ehren angebracht/", "tokens": ["Zu", "eh\u00b7ren", "an\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ich habe lange zeit", "tokens": ["Denn", "ich", "ha\u00b7be", "lan\u00b7ge", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den sachen nachgedacht/", "tokens": ["Den", "sa\u00b7chen", "nach\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun schickt sichs so/", "tokens": ["Nun", "schickt", "sichs", "so", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und ich bin froh/", "tokens": ["Und", "ich", "bin", "froh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df itzt der gute stein", "tokens": ["Da\u00df", "itzt", "der", "gu\u00b7te", "stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zum ersten mahl", "tokens": ["Zum", "ers\u00b7ten", "mahl"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In meiner quaal", "tokens": ["In", "mei\u00b7ner", "qua\u00b7al"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+---", "measure": "dactylic.init"}, "line.10": {"text": "Mu\u00df mein vertretter seyn.", "tokens": ["Mu\u00df", "mein", "ver\u00b7tret\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Da\u00df ich euch lieben darff/", "tokens": ["Da\u00df", "ich", "euch", "lie\u00b7ben", "darff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euer alle-tags gebrauch", "tokens": ["Eu\u00b7er", "al\u00b7le\u00b7tags", "ge\u00b7brauch"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ist ein bi\u00dfgen scharff/", "tokens": ["Der", "ist", "ein", "bi\u00df\u00b7gen", "scharff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was heist es denn/", "tokens": ["Was", "heist", "es", "denn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ach sagt mir wenn/", "tokens": ["Ach", "sagt", "mir", "wenn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "KOUS", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ist meine noth geschlicht/", "tokens": ["Ist", "mei\u00b7ne", "noth", "ge\u00b7schlicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ach/ gebt mir platz/", "tokens": ["Ach", "/", "gebt", "mir", "platz", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Jhr seyd mein schatz/", "tokens": ["Ihr", "seyd", "mein", "schatz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Den wetzstein mag ich nicht.", "tokens": ["Den", "wetz\u00b7stein", "mag", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}