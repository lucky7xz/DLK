{"textgrid.poem.27883": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Armer hat es gut; er f\u00fcrchtet selten sehr,", "tokens": ["Ein", "Ar\u00b7mer", "hat", "es", "gut", ";", "er", "f\u00fcrch\u00b7tet", "sel\u00b7ten", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil er mehr nichts hat, da\u00df er verliere mehr.", "tokens": ["Die\u00b7weil", "er", "mehr", "nichts", "hat", ",", "da\u00df", "er", "ver\u00b7lie\u00b7re", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}