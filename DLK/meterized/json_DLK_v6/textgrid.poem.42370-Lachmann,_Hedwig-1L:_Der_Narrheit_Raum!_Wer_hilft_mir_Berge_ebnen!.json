{"textgrid.poem.42370": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Narrheit Raum! Wer hilft mir Berge ebnen!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Narrheit Raum! Wer hilft mir Berge ebnen!", "tokens": ["Der", "Nar\u00b7rheit", "Raum", "!", "Wer", "hilft", "mir", "Ber\u00b7ge", "eb\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PWS", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Wille ist ein Kreislauf, der nie endet.", "tokens": ["Mein", "Wil\u00b7le", "ist", "ein", "Kreis\u00b7lauf", ",", "der", "nie", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem m\u00fchvoll Nutzlosen und dem Vergebnen", "tokens": ["Dem", "m\u00fch\u00b7voll", "Nutz\u00b7lo\u00b7sen", "und", "dem", "Ver\u00b7geb\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "NN", "KON", "ART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hab ich mit Leib und Leben mich verpf\u00e4ndet.", "tokens": ["Hab", "ich", "mit", "Leib", "und", "Le\u00b7ben", "mich", "ver\u00b7pf\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Hirn gab ich dem Wahn zum Herrensitze,", "tokens": ["Mein", "Hirn", "gab", "ich", "dem", "Wahn", "zum", "Her\u00b7ren\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Truggebilden ist mein Blick umflimmert.", "tokens": ["Von", "Trug\u00b7ge\u00b7bil\u00b7den", "ist", "mein", "Blick", "um\u00b7flim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich klettre mittags auf die Kirchturmspitze,", "tokens": ["Ich", "klett\u00b7re", "mit\u00b7tags", "auf", "die", "Kirch\u00b7turm\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Weil sie so golden in der Sonne schimmert.", "tokens": ["Weil", "sie", "so", "gol\u00b7den", "in", "der", "Son\u00b7ne", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gestreckten Laufs und nimmerm\u00fcden Fusses,", "tokens": ["Ge\u00b7streck\u00b7ten", "Laufs", "und", "nim\u00b7mer\u00b7m\u00fc\u00b7den", "Fus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Horizont, der immer wieder schwindet,", "tokens": ["Zum", "Ho\u00b7ri\u00b7zont", ",", "der", "im\u00b7mer", "wie\u00b7der", "schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Folg ich den Kr\u00fcmmungen des raschen Flusses,", "tokens": ["Folg", "ich", "den", "Kr\u00fcm\u00b7mun\u00b7gen", "des", "ra\u00b7schen", "Flus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bis er sein Bett im fernen Meere findet.", "tokens": ["Bis", "er", "sein", "Bett", "im", "fer\u00b7nen", "Mee\u00b7re", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In Hass und Liebe ohne Mass und Grenzen,", "tokens": ["In", "Hass", "und", "Lie\u00b7be", "oh\u00b7ne", "Mass", "und", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In \u00dcberschwang und Frevel ohne Reue,", "tokens": ["In", "\u00dc\u00b7bersc\u00b7hwang", "und", "Fre\u00b7vel", "oh\u00b7ne", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lass ich mein Gold vor bl\u00f6den Augen gl\u00e4nzen", "tokens": ["Lass", "ich", "mein", "Gold", "vor", "bl\u00f6\u00b7den", "Au\u00b7gen", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und werfe meine Perlen vor die S\u00e4ue.", "tokens": ["Und", "wer\u00b7fe", "mei\u00b7ne", "Per\u00b7len", "vor", "die", "S\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Blind h\u00e4ngt mein Torenherz an seinem Bunde.", "tokens": ["Blind", "h\u00e4ngt", "mein", "To\u00b7ren\u00b7herz", "an", "sei\u00b7nem", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tage geb ich hin verlornem Sinnen", "tokens": ["Die", "Ta\u00b7ge", "geb", "ich", "hin", "ver\u00b7lor\u00b7nem", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lass mir nachts aus einer offnen Wunde", "tokens": ["Und", "lass", "mir", "nachts", "aus", "ei\u00b7ner", "off\u00b7nen", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Heimlich, zum Spass, die roten Tropfen rinnen.", "tokens": ["Heim\u00b7lich", ",", "zum", "Spass", ",", "die", "ro\u00b7ten", "Trop\u00b7fen", "rin\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}