{"dta.poem.3032": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist Gottes unausdenklicher Gedanke,", "tokens": ["Die", "Welt", "ist", "Got\u00b7tes", "un\u00b7aus\u00b7denk\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und g\u00f6ttlich der Beruf zu denken ohne Schranke.", "tokens": ["Und", "g\u00f6tt\u00b7lich", "der", "Be\u00b7ruf", "zu", "den\u00b7ken", "oh\u00b7ne", "Schran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nichts in der Welt, das nicht Gedankenstoff enth\u00e4lt,", "tokens": ["Nichts", "in", "der", "Welt", ",", "das", "nicht", "Ge\u00b7dan\u00b7ken\u00b7stoff", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kein Gedanke, der nicht mitbaut an der Welt.", "tokens": ["Und", "kein", "Ge\u00b7dan\u00b7ke", ",", "der", "nicht", "mit\u00b7baut", "an", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum liebt mein Geist die Welt, weil er das Denken liebt,", "tokens": ["Drum", "liebt", "mein", "Geist", "die", "Welt", ",", "weil", "er", "das", "Den\u00b7ken", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sie ihm \u00fcberall soviel zu denken giebt.", "tokens": ["Und", "sie", "ihm", "\u00fc\u00b7be\u00b7rall", "so\u00b7viel", "zu", "den\u00b7ken", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}