{"textgrid.poem.48028": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Weltfl\u00fcchtig", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Mondlicht \u00fcberflo\u00df den Strand", "tokens": ["Das", "Mond\u00b7licht", "\u00fc\u00b7ber\u00b7flo\u00df", "den", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit sanftem, s\u00fc\u00dfem Schein;", "tokens": ["mit", "sanf\u00b7tem", ",", "s\u00fc\u00b7\u00dfem", "Schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wir gingen beid im D\u00fcnensand", "tokens": ["wir", "gin\u00b7gen", "beid", "im", "D\u00fc\u00b7nen\u00b7sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "weltfl\u00fcchtig und allein.", "tokens": ["welt\u00b7fl\u00fcch\u00b7tig", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Menschenauge hat gesehn,", "tokens": ["Kein", "Men\u00b7schen\u00b7au\u00b7ge", "hat", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie du herab dich bogst", "tokens": ["wie", "du", "her\u00b7ab", "dich", "bogst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und liebesicher l\u00e4chelnd mich", "tokens": ["und", "lie\u00b7be\u00b7si\u00b7cher", "l\u00e4\u00b7chelnd", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in deine Arme zogst.", "tokens": ["in", "dei\u00b7ne", "Ar\u00b7me", "zogst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nicht, war's ein Liebeswort,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wa\u00b7r's", "ein", "Lie\u00b7bes\u00b7wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das fl\u00fcsternd zu mir drang,", "tokens": ["das", "fl\u00fcs\u00b7ternd", "zu", "mir", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "war's tr\u00e4umerischer Nixenruf,", "tokens": ["wa\u00b7r's", "tr\u00e4u\u00b7me\u00b7ri\u00b7scher", "Ni\u00b7xen\u00b7ruf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "der aus den Wassern klang?!", "tokens": ["der", "aus", "den", "Was\u00b7sern", "klang", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}