{"textgrid.poem.46021": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[das Elend soll ich einsam bauen]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Elend soll ich einsam bauen,", "tokens": ["Das", "E\u00b7lend", "soll", "ich", "ein\u00b7sam", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O schweige nur, ich kenn' das Leid,", "tokens": ["O", "schwei\u00b7ge", "nur", ",", "ich", "kenn'", "das", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den hei\u00dfen Schmerz des kranken Pfauen", "tokens": ["Den", "hei\u00b7\u00dfen", "Schmerz", "des", "kran\u00b7ken", "Pfau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nach der Sonne klimmend schreit,", "tokens": ["Der", "nach", "der", "Son\u00b7ne", "klim\u00b7mend", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fchle in dem Abendgrauen", "tokens": ["Ich", "f\u00fch\u00b7le", "in", "dem", "A\u00b7bend\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der N\u00e4chte finstre Bitterkeit", "tokens": ["Der", "N\u00e4ch\u00b7te", "finst\u00b7re", "Bit\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich war im seligsten Vertrauen", "tokens": ["Ich", "war", "im", "se\u00b7ligs\u00b7ten", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Von je dem grimmen Schmerz geweiht", "tokens": ["Von", "je", "dem", "grim\u00b7men", "Schmerz", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und soll das Elend einsam bauen.", "tokens": ["Und", "soll", "das", "E\u00b7lend", "ein\u00b7sam", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Elend soll ich einsam bauen,", "tokens": ["Das", "E\u00b7lend", "soll", "ich", "ein\u00b7sam", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Brunnen die ein Zauberschlag", "tokens": ["Die", "Brun\u00b7nen", "die", "ein", "Zau\u00b7ber\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hervorrief auf den d\u00fcrren Auen", "tokens": ["Her\u00b7vor\u00b7rief", "auf", "den", "d\u00fcr\u00b7ren", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wenden sich, der junge Tag", "tokens": ["Sie", "wen\u00b7den", "sich", ",", "der", "jun\u00b7ge", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will nicht mehr auf mich niedertauen", "tokens": ["Will", "nicht", "mehr", "auf", "mich", "nie\u00b7der\u00b7tau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Leben bricht mir den Vertrag", "tokens": ["Das", "Le\u00b7ben", "bricht", "mir", "den", "Ver\u00b7trag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich soll nun in die W\u00fcste schauen,", "tokens": ["Ich", "soll", "nun", "in", "die", "W\u00fcs\u00b7te", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich, der der Einsamkeit erlag", "tokens": ["Ich", ",", "der", "der", "Ein\u00b7sam\u00b7keit", "er\u00b7lag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Soll einsam nun das Elend bauen.", "tokens": ["Soll", "ein\u00b7sam", "nun", "das", "E\u00b7lend", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Elend soll ich einsam bauen", "tokens": ["Das", "E\u00b7lend", "soll", "ich", "ein\u00b7sam", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wie dem ersten Mann geschah", "tokens": ["Mir", "wie", "dem", "ers\u00b7ten", "Mann", "ge\u00b7schah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als in des Paradieses Auen", "tokens": ["Als", "in", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herr ihn einsam trauern sah", "tokens": ["Der", "Herr", "ihn", "ein\u00b7sam", "trau\u00b7ern", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADJD", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schuf er aus seiner Brust die Frauen,", "tokens": ["Schuf", "er", "aus", "sei\u00b7ner", "Brust", "die", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel war der Erde nah", "tokens": ["Der", "Him\u00b7mel", "war", "der", "Er\u00b7de", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch mit dem menschlichen Vertrauen", "tokens": ["Doch", "mit", "dem", "menschli\u00b7chen", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "War Schlange Frucht und Tod auch da.", "tokens": ["War", "Schlan\u00b7ge", "Frucht", "und", "Tod", "auch", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Drum mu\u00df ich einsam Elend bauen.", "tokens": ["Drum", "mu\u00df", "ich", "ein\u00b7sam", "E\u00b7lend", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Elend soll ich einsam bauen", "tokens": ["Das", "E\u00b7lend", "soll", "ich", "ein\u00b7sam", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdorben war ich durch das Weib", "tokens": ["Ver\u00b7dor\u00b7ben", "war", "ich", "durch", "das", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt' in der Jungfrau neu mich schauen,", "tokens": ["Wollt'", "in", "der", "Jung\u00b7frau", "neu", "mich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Gott verhie\u00dfen, da\u00df sie's bleib'.", "tokens": ["Die", "Gott", "ver\u00b7hie\u00b7\u00dfen", ",", "da\u00df", "sie's", "bleib'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Maria, Zuflucht der Jungfrauen,", "tokens": ["Ma\u00b7ria", ",", "Zu\u00b7flucht", "der", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Erhalt' dem Herren ihren Leib,", "tokens": ["Er\u00b7halt'", "dem", "Her\u00b7ren", "ih\u00b7ren", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df sie nicht blinder Not vertrauen", "tokens": ["La\u00df", "sie", "nicht", "blin\u00b7der", "Not", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ob Erde sie vom Himmel treib'.", "tokens": ["Ob", "Er\u00b7de", "sie", "vom", "Him\u00b7mel", "treib'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich mu\u00df mein Elend einsam bauen.", "tokens": ["Ich", "mu\u00df", "mein", "E\u00b7lend", "ein\u00b7sam", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Elend soll ich einsam bauen", "tokens": ["Das", "E\u00b7lend", "soll", "ich", "ein\u00b7sam", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Jesus h\u00f6re mein Geschrei", "tokens": ["O", "Je\u00b7sus", "h\u00f6\u00b7re", "mein", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Brich meiner Seele tiefes Grauen", "tokens": ["Brich", "mei\u00b7ner", "See\u00b7le", "tie\u00b7fes", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Jesus, f\u00fchr' den Kelch vorbei", "tokens": ["O", "Je\u00b7sus", ",", "f\u00fchr'", "den", "Kelch", "vor\u00b7bei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mach von der H\u00f6lle gift'gen Klauen", "tokens": ["Mach", "von", "der", "H\u00f6l\u00b7le", "gift'\u00b7gen", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "O Jesus meine Seele frei", "tokens": ["O", "Je\u00b7sus", "mei\u00b7ne", "See\u00b7le", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein armes kindliches Vertrauen", "tokens": ["Ein", "ar\u00b7mes", "kind\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Jesus meinem Geist verleih", "tokens": ["O", "Je\u00b7sus", "mei\u00b7nem", "Geist", "ver\u00b7leih"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hilf mir mein Elend einsam bauen.", "tokens": ["Hilf", "mir", "mein", "E\u00b7lend", "ein\u00b7sam", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}