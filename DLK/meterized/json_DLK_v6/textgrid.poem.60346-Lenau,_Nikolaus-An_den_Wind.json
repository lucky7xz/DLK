{"textgrid.poem.60346": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An den Wind", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandre fort ins ferne Land;", "tokens": ["Ich", "wand\u00b7re", "fort", "ins", "fer\u00b7ne", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einmal blickt ich um, bewegt,", "tokens": ["Noch", "ein\u00b7mal", "blickt", "ich", "um", ",", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah, wie sie den Mund geregt,", "tokens": ["Und", "sah", ",", "wie", "sie", "den", "Mund", "ge\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie gewinket ihre Hand.", "tokens": ["Und", "wie", "ge\u00b7win\u00b7ket", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl rief sie noch ein freundlich Wort", "tokens": ["Wohl", "rief", "sie", "noch", "ein", "freund\u00b7lich", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir nach auf meinen tr\u00fcben Gang,", "tokens": ["Mir", "nach", "auf", "mei\u00b7nen", "tr\u00fc\u00b7ben", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch h\u00f6rt ich nicht den liebsten Klang,", "tokens": ["Doch", "h\u00f6rt", "ich", "nicht", "den", "liebs\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ihn der Wind getragen fort.", "tokens": ["Weil", "ihn", "der", "Wind", "ge\u00b7tra\u00b7gen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich mein Gl\u00fcck verlassen mu\u00df,", "tokens": ["Da\u00df", "ich", "mein", "Gl\u00fcck", "ver\u00b7las\u00b7sen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du rauher, kalter Windeshauch,", "tokens": ["Du", "rau\u00b7her", ",", "kal\u00b7ter", "Win\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ists nicht genug, da\u00df du mir auch", "tokens": ["Ists", "nicht", "ge\u00b7nug", ",", "da\u00df", "du", "mir", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entrei\u00dfest ihren letzten Gru\u00df?", "tokens": ["Ent\u00b7rei\u00b7\u00dfest", "ih\u00b7ren", "letz\u00b7ten", "Gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}