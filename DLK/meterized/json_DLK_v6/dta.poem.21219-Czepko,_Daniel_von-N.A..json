IN", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Seit da\u00df bey mir das Werck voll Furchte liegt verborgen.", "tokens": ["Seit", "da\u00df", "bey", "mir", "das", "Werck", "voll", "Furch\u00b7te", "liegt", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "PPER", "ART", "NN", "ADJD", "NN", "VVFIN", "VVPP", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Jtzt bricht es durch die Nacht. Der wundersch\u00f6ne Morgen/", "tokens": ["Jtzt", "bricht", "es", "durch", "die", "Nacht", ".", "Der", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Herr seht Jhr es vor gutt/ so seid darumb begr\u00fcst;", "tokens": ["Herr", "seht", "Ihr", "es", "vor", "gutt", "/", "so", "seid", "da\u00b7rumb", "be\u00b7gr\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "ADJD", "$(", "ADV", "VAFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nehmt dieses kurtze Buch/ vnd reicht es ", "tokens": ["Nehmt", "die\u00b7ses", "kurt\u00b7ze", "Buch", "/", "vnd", "reicht", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So alle Welt verehrt/ vnd schretbt im \u00fcbersenden/", "tokens": ["So", "al\u00b7le", "Welt", "ver\u00b7ehrt", "/", "vnd", "schretbt", "im", "\u00fc\u00b7ber\u00b7sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$(", "KON", "VVFIN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ists/ den Deutschland jtzt von seinem Kayser list.", "tokens": ["Der", "ists", "/", "den", "Deutschland", "jtzt", "von", "sei\u00b7nem", "Kay\u00b7ser", "list", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}