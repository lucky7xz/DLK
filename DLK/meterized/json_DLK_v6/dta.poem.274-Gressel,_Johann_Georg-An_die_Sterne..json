{"dta.poem.274": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Sterne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Spielet ihr Sterne mit g\u00fcldenen Strahlen/", "tokens": ["Spie\u00b7let", "ihr", "Ster\u00b7ne", "mit", "g\u00fcl\u00b7de\u00b7nen", "Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schn\u00fcre dich Himmel in Lieblichkeit ein/", "tokens": ["Schn\u00fc\u00b7re", "dich", "Him\u00b7mel", "in", "Lieb\u00b7lich\u00b7keit", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "ART", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Cynthia mehre dein pr\u00e4chtiges Prahlen/", "tokens": ["Cyn\u00b7thia", "meh\u00b7re", "dein", "pr\u00e4ch\u00b7ti\u00b7ges", "Prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lasse dein Antlitz versilbert dir seyn;", "tokens": ["Las\u00b7se", "dein", "Ant\u00b7litz", "ver\u00b7sil\u00b7bert", "dir", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "PPER", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Br\u00e4nnet euch Wolcken verdoppelt das Prangen/", "tokens": ["Br\u00e4n\u00b7net", "euch", "Wol\u00b7cken", "ver\u00b7dop\u00b7pelt", "das", "Pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Lasset die Fahnen der Sch\u00f6nheit aushangen.", "tokens": ["Las\u00b7set", "die", "Fah\u00b7nen", "der", "Sch\u00f6n\u00b7heit", "aus\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Weichet ihr Zierden/ erblasset ihr Strahlen/", "tokens": ["Wei\u00b7chet", "ihr", "Zier\u00b7den", "/", "er\u00b7blas\u00b7set", "ihr", "Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ziehe nur Himmel die Lieblichkeit ein/", "tokens": ["Zie\u00b7he", "nur", "Him\u00b7mel", "die", "Lieb\u00b7lich\u00b7keit", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "ART", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Doris die spielet mit pr\u00e4chtigerm Prahlen/", "tokens": ["Do\u00b7ris", "die", "spie\u00b7let", "mit", "pr\u00e4ch\u00b7ti\u00b7germ", "Prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Alle die Sch\u00f6nheit besch\u00e4met mu\u00df seyn;", "tokens": ["Al\u00b7le", "die", "Sch\u00f6n\u00b7heit", "be\u00b7sch\u00e4\u00b7met", "mu\u00df", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Denn ihr Gesichte verdoppelt das Prangen/", "tokens": ["Denn", "ihr", "Ge\u00b7sich\u00b7te", "ver\u00b7dop\u00b7pelt", "das", "Pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und l\u00e4st die Fahnen der Sch\u00f6nheit aushangen.", "tokens": ["Und", "l\u00e4st", "die", "Fah\u00b7nen", "der", "Sch\u00f6n\u00b7heit", "aus\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}