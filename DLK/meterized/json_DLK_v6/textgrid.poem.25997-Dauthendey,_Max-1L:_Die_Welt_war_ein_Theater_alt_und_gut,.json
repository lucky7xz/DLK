{"textgrid.poem.25997": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt war ein Theater alt und gut,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt war ein Theater alt und gut,", "tokens": ["Die", "Welt", "war", "ein", "The\u00b7a\u00b7ter", "alt", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sonn' und Sterne hingen als Lampen dort;", "tokens": ["Sonn'", "und", "Ster\u00b7ne", "hin\u00b7gen", "als", "Lam\u00b7pen", "dort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "KOUS", "NN", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nun ist mir die Welt ein vergessener Ort,", "tokens": ["Nun", "ist", "mir", "die", "Welt", "ein", "ver\u00b7ges\u00b7se\u00b7ner", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Sonne wurde mein Tropfen Blut, die Sterne meiner Wonne Tr\u00e4nen.", "tokens": ["Die", "Son\u00b7ne", "wur\u00b7de", "mein", "Trop\u00b7fen", "Blut", ",", "die", "Ster\u00b7ne", "mei\u00b7ner", "Won\u00b7ne", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}