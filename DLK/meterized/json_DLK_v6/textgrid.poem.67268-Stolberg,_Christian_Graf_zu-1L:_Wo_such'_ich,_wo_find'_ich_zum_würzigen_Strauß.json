{"textgrid.poem.67268": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo such' ich, wo find' ich zum w\u00fcrzigen Strau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo such' ich, wo find' ich zum w\u00fcrzigen Strau\u00df", "tokens": ["Wo", "such'", "ich", ",", "wo", "find'", "ich", "zum", "w\u00fcr\u00b7zi\u00b7gen", "Strau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Bl\u00fcmchen und eins noch? ich kann nicht hinaus,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "und", "eins", "noch", "?", "ich", "kann", "nicht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und k\u00f6nnt' ich's, erstorben ist Wies' und ist Flur,", "tokens": ["Und", "k\u00f6nnt'", "ich's", ",", "ers\u00b7tor\u00b7ben", "ist", "Wies'", "und", "ist", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "$,", "VVPP", "VAFIN", "NE", "KON", "VAFIN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im Schleier der Flocken verh\u00fcllt ist Natur.", "tokens": ["Im", "Schlei\u00b7er", "der", "Flo\u00b7cken", "ver\u00b7h\u00fcllt", "ist", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "VAFIN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "O, wenn ich sie finde,", "tokens": ["O", ",", "wenn", "ich", "sie", "fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zum Angebinde", "tokens": ["Zum", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem neugebornen Kinde.", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O, wenn ich sie finde so starr und so kalt,", "tokens": ["O", ",", "wenn", "ich", "sie", "fin\u00b7de", "so", "starr", "und", "so", "kalt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Geschirmt und gepfleget, erwarmen sie bald,", "tokens": ["Ge\u00b7schirmt", "und", "ge\u00b7pfle\u00b7get", ",", "er\u00b7war\u00b7men", "sie", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hier lodert der Heerd und in's K\u00e4mmerlein blickt", "tokens": ["Hier", "lo\u00b7dert", "der", "Heerd", "und", "in's", "K\u00e4m\u00b7mer\u00b7lein", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "APPRART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Sonn', o ihr Bl\u00fcmchen, kommt, werdet erquickt!", "tokens": ["Die", "Sonn'", ",", "o", "ihr", "Bl\u00fcm\u00b7chen", ",", "kommt", ",", "wer\u00b7det", "er\u00b7quickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kommt, Bl\u00fcmchen, geschwinde,", "tokens": ["Kommt", ",", "Bl\u00fcm\u00b7chen", ",", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Angebinde", "tokens": ["Zum", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem neugebornen Kinde.", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sind sie! So bunt und so prangend, so sch\u00f6n,", "tokens": ["Da", "sind", "sie", "!", "So", "bunt", "und", "so", "pran\u00b7gend", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$.", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie Iris im schimmernden Bogen zu sehn,", "tokens": ["Wie", "I\u00b7ris", "im", "schim\u00b7mern\u00b7den", "Bo\u00b7gen", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als K\u00f6niginn Aller die Rose \u2013 doch nein,", "tokens": ["Als", "K\u00f6\u00b7ni\u00b7ginn", "Al\u00b7ler", "die", "Ro\u00b7se", "\u2013", "doch", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ART", "NN", "$(", "ADV", "PTKANT", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie hat ja den Stachel! \u2013 Mein Str\u00e4uschen soll seyn", "tokens": ["Sie", "hat", "ja", "den", "Sta\u00b7chel", "!", "\u2013", "Mein", "Str\u00e4u\u00b7schen", "soll", "seyn"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$.", "$(", "PPOSAT", "NN", "VMFIN", "VAINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "So sanft und so linde,", "tokens": ["So", "sanft", "und", "so", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Angebinde", "tokens": ["Zum", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem neugebornen Kinde.", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df wohl ein G\u00e4rtchen, das keiner entdeckt,", "tokens": ["Ich", "wei\u00df", "wohl", "ein", "G\u00e4rt\u00b7chen", ",", "das", "kei\u00b7ner", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im heimlichsten Schoo\u00dfe des Thales versteckt,", "tokens": ["Im", "heim\u00b7lichs\u00b7ten", "Schoo\u00b7\u00dfe", "des", "Tha\u00b7les", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hoch \u00fcber ihm leuchtet der Himmel so blau", "tokens": ["Hoch", "\u00fc\u00b7ber", "ihm", "leuch\u00b7tet", "der", "Him\u00b7mel", "so", "blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und labend erfrischt es der duftende Thau.", "tokens": ["Und", "la\u00b7bend", "er\u00b7frischt", "es", "der", "duf\u00b7ten\u00b7de", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Dort schonen die Winde", "tokens": ["Dort", "scho\u00b7nen", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Angebinde", "tokens": ["Die", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem neugebornen Kinde. \u2013", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Bl\u00fcmchen der Liebe, in Wonne begl\u00fcckt,", "tokens": ["Die", "Bl\u00fcm\u00b7chen", "der", "Lie\u00b7be", ",", "in", "Won\u00b7ne", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf heimischem Beete die G\u00e4rtnerinn pfl\u00fcckt,", "tokens": ["Auf", "hei\u00b7mi\u00b7schem", "Bee\u00b7te", "die", "G\u00e4rt\u00b7ne\u00b7rinn", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein dornenlos R\u00f6schen und, herzend so warm", "tokens": ["Ein", "dor\u00b7nen\u00b7los", "R\u00f6\u00b7schen", "und", ",", "her\u00b7zend", "so", "warm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KON", "$,", "VVPP", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den st\u00fctzenden Stab mit umschlingendem Arm,", "tokens": ["Den", "st\u00fct\u00b7zen\u00b7den", "Stab", "mit", "um\u00b7schlin\u00b7gen\u00b7dem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die rankende Winde,", "tokens": ["Die", "ran\u00b7ken\u00b7de", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Angebinde", "tokens": ["Zum", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem neugebornen Kinde. \u2013", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist's Odem der Liebe, ist's zaubernde Macht?", "tokens": ["Ist's", "O\u00b7dem", "der", "Lie\u00b7be", ",", "ist's", "zau\u00b7bern\u00b7de", "Macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Heutegeborne zum Mann' ist vollbracht,", "tokens": ["Der", "Heu\u00b7te\u00b7ge\u00b7bor\u00b7ne", "zum", "Mann'", "ist", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zum Mann'! und ich Liebende bin's, die so warm", "tokens": ["Zum", "Mann'", "!", "und", "ich", "Lie\u00b7ben\u00b7de", "bin's", ",", "die", "so", "warm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "KON", "PPER", "NN", "VAFIN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Umschlinge den Trauten mit rankendem Arm!", "tokens": ["Um\u00b7schlin\u00b7ge", "den", "Trau\u00b7ten", "mit", "ran\u00b7ken\u00b7dem", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nun Str\u00e4uschen verschwinde", "tokens": ["Nun", "Str\u00e4u\u00b7schen", "ver\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Als Angebinde", "tokens": ["Als", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem neugebornen Kinde!", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zum Kranze nun flecht' ich euch, Bl\u00fcmchen, zum Kranz,", "tokens": ["Zum", "Kran\u00b7ze", "nun", "flecht'", "ich", "euch", ",", "Bl\u00fcm\u00b7chen", ",", "zum", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Seht, zu euch gesellt sich in purpurnem Glanz", "tokens": ["Seht", ",", "zu", "euch", "ge\u00b7sellt", "sich", "in", "pur\u00b7pur\u00b7nem", "Glanz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das Bl\u00fcmchen ", "tokens": ["Das", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich k\u00fc\u00df' euch und winde", "tokens": ["Ich", "k\u00fc\u00df'", "euch", "und", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das Angebinde", "tokens": ["Das", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dem neugebornen Kinde.", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gen\u00fcgt Ihm mein Kr\u00e4nzchen? \u2013 Nur mir nicht! O schon", "tokens": ["Ge\u00b7n\u00fcgt", "Ihm", "mein", "Kr\u00e4nz\u00b7chen", "?", "\u2013", "Nur", "mir", "nicht", "!", "O", "schon"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "ADV", "PPER", "PTKNEG", "$.", "NE", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umflicht Ihn mit Kr\u00e4nzen ein h\u00f6herer Lohn!", "tokens": ["Um\u00b7flicht", "Ihn", "mit", "Kr\u00e4n\u00b7zen", "ein", "h\u00f6\u00b7he\u00b7rer", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dem Herzen entquillet des Heldenmuths Born,", "tokens": ["Dem", "Her\u00b7zen", "ent\u00b7quil\u00b7let", "des", "Hel\u00b7den\u00b7muths", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sein Ruhm oft ein W\u00fcrfel, oft Frevel sein Sporn.", "tokens": ["Sein", "Ruhm", "oft", "ein", "W\u00fcr\u00b7fel", ",", "oft", "Fre\u00b7vel", "sein", "Sporn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich suchte, ich finde!", "to