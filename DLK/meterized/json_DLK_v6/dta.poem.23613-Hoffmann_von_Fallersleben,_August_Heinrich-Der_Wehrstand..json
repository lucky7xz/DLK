{"dta.poem.23613": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wehrstand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott gr\u00fc\u00df euch, lieben Kriegesknechte!", "tokens": ["Gott", "gr\u00fc\u00df", "euch", ",", "lie\u00b7ben", "Krie\u00b7ges\u00b7knech\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr seid die Friedensherren nun:", "tokens": ["Ihr", "seid", "die", "Frie\u00b7dens\u00b7her\u00b7ren", "nun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sind noch Schlachten, wo Gefechte,", "tokens": ["Wo", "sind", "noch", "Schlach\u00b7ten", ",", "wo", "Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit V\u00f6lkerha\u00df und Zwietracht ruhn?", "tokens": ["Seit", "V\u00f6l\u00b7ker\u00b7ha\u00df", "und", "Zwiet\u00b7racht", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was wart ihr einst im deutschen Reiche?", "tokens": ["Was", "wart", "ihr", "einst", "im", "deut\u00b7schen", "Rei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Eichwald schier mit Schwert und Speer;", "tokens": ["Ein", "Eich\u00b7wald", "schier", "mit", "Schwert", "und", "Speer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt seid ihr an der deutschen Eiche", "tokens": ["Jetzt", "seid", "ihr", "an", "der", "deut\u00b7schen", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mistel nur und sonst nichts mehr.", "tokens": ["Die", "Mis\u00b7tel", "nur", "und", "sonst", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}