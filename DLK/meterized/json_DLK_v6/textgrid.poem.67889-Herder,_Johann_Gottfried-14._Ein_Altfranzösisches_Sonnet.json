{"textgrid.poem.67889": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14. Ein Altfranz\u00f6sisches Sonnet", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach k\u00f6nnt' ich, k\u00f6nnte vergessen Sie!", "tokens": ["Ach", "k\u00f6nnt'", "ich", ",", "k\u00f6nn\u00b7te", "ver\u00b7ges\u00b7sen", "Sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VMFIN", "VVPP", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr sch\u00f6nes, liebes liebliches Wesen,", "tokens": ["Ihr", "sch\u00f6\u00b7nes", ",", "lie\u00b7bes", "lieb\u00b7li\u00b7ches", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Blick, die freundliche Lippe, die!", "tokens": ["Den", "Blick", ",", "die", "freund\u00b7li\u00b7che", "Lip\u00b7pe", ",", "die", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vielleicht ich m\u00f6chte genesen!", "tokens": ["Viel\u00b7leicht", "ich", "m\u00f6ch\u00b7te", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Doch ach! mein Herz, mein Herz kann es nie!", "tokens": ["Doch", "ach", "!", "mein", "Herz", ",", "mein", "Herz", "kann", "es", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und doch ists Wahnsinn, zu hoffen Sie!", "tokens": ["Und", "doch", "ists", "Wahn\u00b7sinn", ",", "zu", "hof\u00b7fen", "Sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "PTKZU", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und um Sie schweben", "tokens": ["Und", "um", "Sie", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gibt Muth und Leben,", "tokens": ["Gibt", "Muth", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zu weichen nie! \u2013", "tokens": ["Zu", "wei\u00b7chen", "nie", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und dann, wie kann ich vergessen Sie,", "tokens": ["Und", "dann", ",", "wie", "kann", "ich", "ver\u00b7ges\u00b7sen", "Sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "VMFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr sch\u00f6nes, liebes liebliches Wesen,", "tokens": ["Ihr", "sch\u00f6\u00b7nes", ",", "lie\u00b7bes", "lieb\u00b7li\u00b7ches", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Blick, die freundliche Lippe die!", "tokens": ["Den", "Blick", ",", "die", "freund\u00b7li\u00b7che", "Lip\u00b7pe", "die", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Viel lieber nimmer genesen!", "tokens": ["Viel", "lie\u00b7ber", "nim\u00b7mer", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}