{"textgrid.poem.36123": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wettertr\u00e4chtig Gew\u00f6lk h\u00e4nget vom Scheitelpunkt", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wettertr\u00e4chtig Gew\u00f6lk h\u00e4nget vom Scheitelpunkt", "tokens": ["Wet\u00b7ter\u00b7tr\u00e4ch\u00b7tig", "Ge\u00b7w\u00f6lk", "h\u00e4n\u00b7get", "vom", "Schei\u00b7tel\u00b7punkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tief, und schwillet und sinkt. Berstend ergeu\u00dft sein Bauch", "tokens": ["Tief", ",", "und", "schwil\u00b7let", "und", "sinkt", ".", "Bers\u00b7tend", "er\u00b7geu\u00dft", "sein", "Bauch"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN", "$.", "VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Str\u00f6me stygischen Schwefels", "tokens": ["Str\u00f6\u00b7me", "sty\u00b7gi\u00b7schen", "Schwe\u00b7fels"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf die blumige Fr\u00fchlingsflur.", "tokens": ["Auf", "die", "blu\u00b7mi\u00b7ge", "Fr\u00fch\u00b7lings\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Und die blumige Flur bebet, und, ach! der Halm", "tokens": ["Und", "die", "blu\u00b7mi\u00b7ge", "Flur", "be\u00b7bet", ",", "und", ",", "ach", "!", "der", "Halm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "$,", "ITJ", "$.", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Senkt sein traurendes Haupt. Blumen, erst aufgebl\u00fcht,", "tokens": ["Senkt", "sein", "trau\u00b7ren\u00b7des", "Haupt", ".", "Blu\u00b7men", ",", "erst", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Schl\u00e4gt der wandelnde Orkan", "tokens": ["Schl\u00e4gt", "der", "wan\u00b7deln\u00b7de", "Or\u00b7kan"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tief zur\u00fcck in der Mutter Schoo\u00df.", "tokens": ["Tief", "zu\u00b7r\u00fcck", "in", "der", "Mut\u00b7ter", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "So, ach! bl\u00fchete mir ", "tokens": ["So", ",", "ach", "!", "bl\u00fc\u00b7he\u00b7te", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ITJ", "$.", "VVFIN", "PPER"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Jung und lieblich und sanft. Aber ein Gr\u00e4bersturm", "tokens": ["Jung", "und", "lieb\u00b7lich", "und", "sanft", ".", "A\u00b7ber", "ein", "Gr\u00e4\u00b7ber\u00b7sturm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "$.", "KON", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schlug das bl\u00fchende M\u00e4dchen", "tokens": ["Schlug", "das", "bl\u00fc\u00b7hen\u00b7de", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tief zur\u00fcck in der Mutter Schoo\u00df.", "tokens": ["Tief", "zu\u00b7r\u00fcck", "in", "der", "Mut\u00b7ter", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Selma, Selma ist hin! \u2013 Dunkle Gewitternacht", "tokens": ["Sel\u00b7ma", ",", "Sel\u00b7ma", "ist", "hin", "!", "\u2013", "Dunk\u00b7le", "Ge\u00b7wit\u00b7ter\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "VAFIN", "ADV", "$.", "$(", "ADJA", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Schwille, birste, und triff Fluren, Blum' und Halm,", "tokens": ["Schwil\u00b7le", ",", "birs\u00b7te", ",", "und", "triff", "Flu\u00b7ren", ",", "Blum'", "und", "Halm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Triff auch mich, und ich fliege", "tokens": ["Triff", "auch", "mich", ",", "und", "ich", "flie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Hoch im Wetter hinauf zu ihr! \u2013", "tokens": ["Hoch", "im", "Wet\u00b7ter", "hin\u00b7auf", "zu", "ihr", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}