{"textgrid.poem.34492": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, es gleicht den G\u00f6ttern der Mann, der fl\u00fcsternd", "tokens": ["Ja", ",", "es", "gleicht", "den", "G\u00f6t\u00b7tern", "der", "Mann", ",", "der", "fl\u00fcs\u00b7ternd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "nah dir sitzt in heimlicher Sommerlaube.", "tokens": ["nah", "dir", "sitzt", "in", "heim\u00b7li\u00b7cher", "Som\u00b7mer\u00b7lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "N\u00e4her r\u00fcckt er \u2013 inniger fleht er \u2013 und du \u2013", "tokens": ["N\u00e4\u00b7her", "r\u00fcckt", "er", "\u2013", "in\u00b7ni\u00b7ger", "fleht", "er", "\u2013", "und", "du", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "ADJD", "VVFIN", "PPER", "$(", "KON", "PPER", "$("], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "l\u00e4chelst verlangend!", "tokens": ["l\u00e4\u00b7chelst", "ver\u00b7lan\u00b7gend", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber mir \u2013 zerschlagen in meinen Br\u00fcsten", "tokens": ["A\u00b7ber", "mir", "\u2013", "zer\u00b7schla\u00b7gen", "in", "mei\u00b7nen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "ist das Herz, die Zunge in meinem Munde", "tokens": ["ist", "das", "Herz", ",", "die", "Zun\u00b7ge", "in", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "stockt \u2013 und l\u00e4hmend rieselt ein eisig Feuer", "tokens": ["stockt", "\u2013", "und", "l\u00e4h\u00b7mend", "rie\u00b7selt", "ein", "ei\u00b7sig", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KON", "ADJD", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "unter der Haut hin.", "tokens": ["un\u00b7ter", "der", "Haut", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Blind die Augen \u2013 nur vor den Ohren gellt es!", "tokens": ["Blind", "die", "Au\u00b7gen", "\u2013", "nur", "vor", "den", "Oh\u00b7ren", "gellt", "es", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Schaudernd vor der Wucht des ergrimmten Schicksals", "tokens": ["Schau\u00b7dernd", "vor", "der", "Wucht", "des", "er\u00b7grimm\u00b7ten", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "wank ich, blasser denn Heu, das an dem Feldrain", "tokens": ["wank", "ich", ",", "blas\u00b7ser", "denn", "Heu", ",", "das", "an", "dem", "Feld\u00b7rain"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "KON", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "welkend vergessen.", "tokens": ["wel\u00b7kend", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}