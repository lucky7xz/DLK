{"dta.poem.5378": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Menschliche Schwachheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich j\u00fcngst die Wunder-Pracht", "tokens": ["Nach\u00b7dem", "ich", "j\u00fcngst", "die", "Wun\u00b7der\u00b7Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nen Welt, wie sie so wundersch\u00f6n,", "tokens": ["Der", "sch\u00f6\u00b7nen", "Welt", ",", "wie", "sie", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An einem heitern Tag\u2019, im Sommer, angesehn;", "tokens": ["An", "ei\u00b7nem", "hei\u00b7tern", "Tag'", ",", "im", "Som\u00b7mer", ",", "an\u00b7ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Folgt\u2019 einem sch\u00f6nen Tag\u2019 ein\u2019 angenehme Nacht.", "tokens": ["Folgt'", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Tag'", "ein'", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die stille Luft, die anfangs schw\u00fchl,", "tokens": ["Die", "stil\u00b7le", "Luft", ",", "die", "an\u00b7fangs", "schw\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ward frisch und allgem\u00e4hlich k\u00fchl.", "tokens": ["Ward", "frisch", "und", "all\u00b7ge\u00b7m\u00e4h\u00b7lich", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kaum da\u00df der helle Glantz der Sonne sich verh\u00fcllte;", "tokens": ["Kaum", "da\u00df", "der", "hel\u00b7le", "Glantz", "der", "Son\u00b7ne", "sich", "ver\u00b7h\u00fcll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als der gek\u00fchlten L\u00fcfte Reich,", "tokens": ["Als", "der", "ge\u00b7k\u00fchl\u00b7ten", "L\u00fcf\u00b7te", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Aus Blumen, Laub\u2019 und Kr\u00e4utern, gleich", "tokens": ["Aus", "Blu\u00b7men", ",", "Laub'", "und", "Kr\u00e4u\u00b7tern", ",", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein tausendfach-gemischter Duft erf\u00fcllte,", "tokens": ["Ein", "tau\u00b7send\u00b7fach\u00b7ge\u00b7mischter", "Duft", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Der, da er seine Kraft auf tausend Art vermischte,", "tokens": ["Der", ",", "da", "er", "sei\u00b7ne", "Kraft", "auf", "tau\u00b7send", "Art", "ver\u00b7mischte", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Geist, der es erwegt, auf tausend Art erfrischte.", "tokens": ["Den", "Geist", ",", "der", "es", "er\u00b7wegt", ",", "auf", "tau\u00b7send", "Art", "er\u00b7frischte", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der D\u00e4mmrung sanftes Licht, das allgemein,", "tokens": ["Der", "D\u00e4mm\u00b7rung", "sanf\u00b7tes", "Licht", ",", "das", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und keine Schatten zeugt, nahm Feld und Garten ein;", "tokens": ["Und", "kei\u00b7ne", "Schat\u00b7ten", "zeugt", ",", "nahm", "Feld", "und", "Gar\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bi\u00df bald hernach", "tokens": ["Bi\u00df", "bald", "her\u00b7nach"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ein bl\u00e4uligtes Gew\u00f6lck\u2019, als wie ein Berg gestalt,", "tokens": ["Ein", "bl\u00e4u\u00b7lig\u00b7tes", "Ge\u00b7w\u00f6lck'", ",", "als", "wie", "ein", "Berg", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Am Firmament sich sanft zerst\u00fcckt\u2019 und brach,", "tokens": ["Am", "Fir\u00b7ma\u00b7ment", "sich", "sanft", "zer\u00b7st\u00fcckt'", "und", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Da alsobald", "tokens": ["Da", "al\u00b7so\u00b7bald"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Der volle Mond, mit r\u00f6thlich-gelben Blitzen,", "tokens": ["Der", "vol\u00b7le", "Mond", ",", "mit", "r\u00f6\u00b7th\u00b7lich\u00b7gel\u00b7ben", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Durch die gespaltnen duncklen Ritzen", "tokens": ["Durch", "die", "ge\u00b7spalt\u00b7nen", "dunck\u00b7len", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Mit schnellem Wandern strahlte,", "tokens": ["Mit", "schnel\u00b7lem", "Wan\u00b7dern", "strahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und die bethaute Welt mit Licht und Schatten mahlte.", "tokens": ["Und", "die", "be\u00b7thau\u00b7te", "Welt", "mit", "Licht", "und", "Schat\u00b7ten", "mahl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein GOtt, wie r\u00fchrte mich, zu deinem Preise,", "tokens": ["Mein", "Gott", ",", "wie", "r\u00fchr\u00b7te", "mich", ",", "zu", "dei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der jetzt, auf eine neue Weise,", "tokens": ["Der", "jetzt", ",", "auf", "ei\u00b7ne", "neu\u00b7e", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Geschm\u00fcckte Kreis der sch\u00f6nen Welt!", "tokens": ["Ge\u00b7schm\u00fcck\u00b7te", "Kreis", "der", "sch\u00f6\u00b7nen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Es war des Monden holder Schein", "tokens": ["Es", "war", "des", "Mon\u00b7den", "hol\u00b7der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Recht ausserordentlich entnebelt, hell und rein,", "tokens": ["Recht", "aus\u00b7ser\u00b7or\u00b7dent\u00b7lich", "ent\u00b7ne\u00b7belt", ",", "hell", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Er war nicht bla\u00df; ein r\u00f6thlich-gelber Strahl", "tokens": ["Er", "war", "nicht", "bla\u00df", ";", "ein", "r\u00f6\u00b7th\u00b7lich\u00b7gel\u00b7ber", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Bedeckt\u2019 und schm\u00fcckte Wald und Feld,", "tokens": ["Be\u00b7deckt'", "und", "schm\u00fcck\u00b7te", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Beflo\u00df und zierte Berg und Thal.", "tokens": ["Be\u00b7flo\u00df", "und", "zier\u00b7te", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}