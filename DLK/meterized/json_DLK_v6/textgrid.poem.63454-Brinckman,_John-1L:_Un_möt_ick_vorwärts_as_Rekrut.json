{"textgrid.poem.63454": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Un m\u00f6t ick vorw\u00e4rts as Rekrut", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Un m\u00f6t ick vorw\u00e4rts as Rekrut", "tokens": ["Un", "m\u00f6t", "ick", "vor\u00b7w\u00e4rts", "as", "Re\u00b7krut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "mit Sabel un Musket,", "tokens": ["mit", "Sa\u00b7bel", "un", "Mus\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "FM", "FM", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "denn helpt dat nich! Hul nich so lut", "tokens": ["denn", "helpt", "dat", "nich", "!", "Hul", "nich", "so", "lut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "PTKNEG", "$.", "NE", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un heww di nich so, Gret!", "tokens": ["un", "heww", "di", "nich", "so", ",", "Gret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM", "FM", "FM", "PTKNEG", "ADV", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "De Trummel sleiht nu trumtritrum!", "tokens": ["De", "Trum\u00b7mel", "sleiht", "nu", "trum\u00b7trit\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kamrad, kumm! Kamrad, kumm!", "tokens": ["Kam\u00b7rad", ",", "kumm", "!", "Kam\u00b7rad", ",", "kumm", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rechten, Linken! Speck un Schinken!", "tokens": ["Rech\u00b7ten", ",", "Lin\u00b7ken", "!", "Speck", "un", "Schin\u00b7ken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "NN", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grad de Been un stif de Nack \u2013", "tokens": ["Grad", "de", "Be\u00b7en", "un", "stif", "de", "Nack", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Kamrad, kumm mit Sack un Pack!", "tokens": ["Kam\u00b7rad", ",", "kumm", "mit", "Sack", "un", "Pack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "NN", "FM", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Un wenn een m\u00f6t, denn m\u00f6t he wull,", "tokens": ["Un", "wenn", "e\u00b7en", "m\u00f6t", ",", "denn", "m\u00f6t", "he", "wull", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NE", "$,", "KON", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "un got is, wenn he kann", "tokens": ["un", "got", "is", ",", "wenn", "he", "kann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "FM", "FM", "$,", "KOUS", "NE", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "un nich glik hett de B\u00fccksen vull", "tokens": ["un", "nich", "glik", "hett", "de", "B\u00fcck\u00b7sen", "vull"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "PTKNEG", "NN", "VAFIN", "NE", "NE", "NE"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "un h\u00f6llt sick as'n Mann.", "tokens": ["un", "h\u00f6llt", "sick", "as'n", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Kommi\u00dfbrot hen! Kommi\u00dfbrot her!", "tokens": ["Kom\u00b7mi\u00df\u00b7brot", "hen", "!", "Kom\u00b7mi\u00df\u00b7brot", "her", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Is Helm dat, is dat Hot \u2013", "tokens": ["Is", "Helm", "dat", ",", "is", "dat", "Hot", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "FM", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "heww dor ick, heww ick hier dat swer,", "tokens": ["heww", "dor", "ick", ",", "heww", "ick", "hier", "dat", "swer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "s\u00fch, Dirn, dat is glik got!", "tokens": ["s\u00fch", ",", "Dirn", ",", "dat", "is", "glik", "got", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ick wull, hier st\u00fcnn man sonn Franzos,", "tokens": ["Ick", "wull", ",", "hier", "st\u00fcnn", "man", "sonn", "Fran\u00b7zos", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "VVFIN", "PIS", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sonn Klas vun Hannemann \u2013", "tokens": ["sonn", "Klas", "vun", "Han\u00b7ne\u00b7mann", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "furtst mit de Plemp fohrt'ck up se los", "tokens": ["furtst", "mit", "de", "Plemp", "fohrt'ck", "up", "se", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un haugt se in de Pann.", "tokens": ["un", "haugt", "se", "in", "de", "Pann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ick weet, du b\u00fcst un bliffst mi tru;", "tokens": ["Ick", "weet", ",", "du", "b\u00fcst", "un", "bliffst", "mi", "tru", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "na, lach eens, Dirn! Wat weenst?", "tokens": ["na", ",", "lach", "e\u00b7ens", ",", "Dirn", "!", "Wat", "weenst", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "$,", "NN", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keen anner ward min Fru as du \u2013", "tokens": ["Ke\u00b7en", "an\u00b7ner", "ward", "min", "Fru", "as", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "un nu adsch\u00fcs noch eens!", "tokens": ["un", "nu", "ad\u00b7sch\u00fcs", "noch", "e\u00b7ens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "De Trummel sleiht nu trumtritrum!", "tokens": ["De", "Trum\u00b7mel", "sleiht", "nu", "trum\u00b7trit\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kamrad, kumm! Kamrad, kumm!", "tokens": ["Kam\u00b7rad", ",", "kumm", "!", "Kam\u00b7rad", ",", "kumm", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Rechten, Linken! Speck un Schinken!", "tokens": ["Rech\u00b7ten", ",", "Lin\u00b7ken", "!", "Speck", "un", "Schin\u00b7ken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "NN", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Grad de Been un stif de Nack \u2013", "tokens": ["Grad", "de", "Be\u00b7en", "un", "stif", "de", "Nack", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Kamrad, kumm mit Sack un Pack!", "tokens": ["Kam\u00b7rad", ",", "kumm", "mit", "Sack", "un", "Pack", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct