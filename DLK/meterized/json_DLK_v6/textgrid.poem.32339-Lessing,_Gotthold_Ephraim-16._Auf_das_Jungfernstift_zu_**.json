{"textgrid.poem.32339": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "16. Auf das Jungfernstift zu **", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkt, wie gesund die Luft, wie rein", "tokens": ["Denkt", ",", "wie", "ge\u00b7sund", "die", "Luft", ",", "wie", "rein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie um dies Jungfernstift mu\u00df sein!", "tokens": ["Sie", "um", "dies", "Jung\u00b7fern\u00b7stift", "mu\u00df", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDS", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit Menschen sich besinnen,", "tokens": ["Seit", "Men\u00b7schen", "sich", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Starb keine Jungfer drinnen.", "tokens": ["Starb", "kei\u00b7ne", "Jung\u00b7fer", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}