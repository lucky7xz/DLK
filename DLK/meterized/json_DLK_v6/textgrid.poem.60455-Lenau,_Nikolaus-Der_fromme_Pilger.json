{"textgrid.poem.60455": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der fromme Pilger", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig wandelt dort ein Ritter", "tokens": ["Se\u00b7lig", "wan\u00b7delt", "dort", "ein", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch Jerusalems Gefilde;", "tokens": ["Durch", "Je\u00b7ru\u00b7sa\u00b7lems", "Ge\u00b7fil\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinend trat er auf den Boden,", "tokens": ["Wei\u00b7nend", "trat", "er", "auf", "den", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo einst wallte Jesus Christus,", "tokens": ["Wo", "einst", "wall\u00b7te", "Je\u00b7sus", "Chris\u00b7tus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "NE", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und die Lippen senkt er k\u00fcssend", "tokens": ["Und", "die", "Lip\u00b7pen", "senkt", "er", "k\u00fcs\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Grund, der ", "tokens": ["Auf", "den", "Grund", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Alles Erdenleids genesen", "tokens": ["Al\u00b7les", "Er\u00b7den\u00b7leids", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchlt sich hier der fromme Pilger;", "tokens": ["F\u00fchlt", "sich", "hier", "der", "from\u00b7me", "Pil\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit der B\u00fcrde seiner S\u00fcnden", "tokens": ["Mit", "der", "B\u00fcr\u00b7de", "sei\u00b7ner", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sind die Lasten seiner Sorgen", "tokens": ["Sind", "die", "Las\u00b7ten", "sei\u00b7ner", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hinter ihm ins Meer versunken. \u2013", "tokens": ["Hin\u00b7ter", "ihm", "ins", "Meer", "ver\u00b7sun\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Anders rauschen hier die Wasser,", "tokens": ["An\u00b7ders", "rau\u00b7schen", "hier", "die", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anders wehen ihm die L\u00fcfte,", "tokens": ["An\u00b7ders", "we\u00b7hen", "ihm", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie erquickend und geheiligt", "tokens": ["Wie", "er\u00b7qui\u00b7ckend", "und", "ge\u00b7hei\u00b7ligt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Z\u00fcge seines Odems!", "tokens": ["Sind", "die", "Z\u00fc\u00b7ge", "sei\u00b7nes", "O\u00b7dems", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wunderbar bewegte Hauche", "tokens": ["Wun\u00b7der\u00b7bar", "be\u00b7weg\u00b7te", "Hau\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00e4useln durch das Laub der B\u00e4ume,", "tokens": ["S\u00e4u\u00b7seln", "durch", "das", "Laub", "der", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gleich als h\u00e4tte hier die Erde", "tokens": ["Gleich", "als", "h\u00e4t\u00b7te", "hier", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihn noch immer nicht vergessen,", "tokens": ["Ihn", "noch", "im\u00b7mer", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der hier einst geliebt, geduldet", "tokens": ["Der", "hier", "einst", "ge\u00b7liebt", ",", "ge\u00b7dul\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADV", "VVPP", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und am Kreuz f\u00fcr uns gestorben;", "tokens": ["Und", "am", "Kreuz", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gleich als rauschten holde M\u00e4ren", "tokens": ["Gleich", "als", "rauschten", "hol\u00b7de", "M\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sein Ged\u00e4chtnis durch die Wipfel,", "tokens": ["Sein", "Ge\u00b7d\u00e4cht\u00b7nis", "durch", "die", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Frohe Kunden, Festges\u00e4nge,", "tokens": ["Fro\u00b7he", "Kun\u00b7den", ",", "Fest\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "G\u00f6ttlich leise Traditionen,", "tokens": ["G\u00f6tt\u00b7lich", "lei\u00b7se", "Tra\u00b7di\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Von den Bl\u00e4ttern, welk und sinkend,", "tokens": ["Von", "den", "Bl\u00e4t\u00b7tern", ",", "welk", "und", "sin\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Zugerauscht den frischen, gr\u00fcnen,", "tokens": ["Zu\u00b7ge\u00b7rauscht", "den", "fri\u00b7schen", ",", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und von Bl\u00fcte hin zu Bl\u00fcte", "tokens": ["Und", "von", "Bl\u00fc\u00b7te", "hin", "zu", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Fortgehaucht durch all die Zeiten.", "tokens": ["Fort\u00b7ge\u00b7haucht", "durch", "all", "die", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}