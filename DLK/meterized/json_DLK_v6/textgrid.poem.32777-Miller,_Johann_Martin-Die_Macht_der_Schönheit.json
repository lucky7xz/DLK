{"textgrid.poem.32777": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Macht der Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fabel und die Wahrheit zeugt,", "tokens": ["Die", "Fa\u00b7bel", "und", "die", "Wahr\u00b7heit", "zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df nichts an Macht der Sch\u00f6nheit gleicht.", "tokens": ["Da\u00df", "nichts", "an", "Macht", "der", "Sch\u00f6n\u00b7heit", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herr Adam, holder Reize wegen,", "tokens": ["Herr", "A\u00b7dam", ",", "hol\u00b7der", "Rei\u00b7ze", "we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verscherzte sich des Himmels Segen:", "tokens": ["Ver\u00b7scherz\u00b7te", "sich", "des", "Him\u00b7mels", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Herr Paris reizte Griechenland", "tokens": ["Herr", "Pa\u00b7ris", "reiz\u00b7te", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Zur Wut; sein Troja ward verbrannt.", "tokens": ["Zur", "Wut", ";", "sein", "Tro\u00b7ja", "ward", "ver\u00b7brannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPOSAT", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein ungl\u00fccksel'ger Apfel brachte", "tokens": ["Ein", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "Ap\u00b7fel", "brach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Staupen her, eh' man es dachte.", "tokens": ["Die", "Stau\u00b7pen", "her", ",", "eh'", "man", "es", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O h\u00e4tten beide ", "tokens": ["O", "h\u00e4t\u00b7ten", "bei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Den Apfel h\u00e4tte mit Begier", "tokens": ["Den", "Ap\u00b7fel", "h\u00e4t\u00b7te", "mit", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Genommen Adam gleich von ihr,", "tokens": ["Ge\u00b7nom\u00b7men", "A\u00b7dam", "gleich", "von", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und Paris w\u00fcrd' ihn ohn Bedenken", "tokens": ["Und", "Pa\u00b7ris", "w\u00fcrd'", "ihn", "ohn", "Be\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Dir, liebensw\u00fcrd'ge Chloe, schenken.", "tokens": ["Dir", ",", "lie\u00b7bens\u00b7w\u00fcrd'\u00b7ge", "Chloe", ",", "schen\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}