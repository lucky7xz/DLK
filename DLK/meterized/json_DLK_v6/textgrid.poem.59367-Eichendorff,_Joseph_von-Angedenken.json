{"textgrid.poem.59367": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Angedenken", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berg' und T\u00e4ler wieder fingen", "tokens": ["Ber\u00b7g'", "und", "T\u00e4\u00b7ler", "wie\u00b7der", "fin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ringsumher zu bl\u00fchen an,", "tokens": ["Rings\u00b7um\u00b7her", "zu", "bl\u00fc\u00b7hen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Walde h\u00f6rt ich singen", "tokens": ["Aus", "dem", "Wal\u00b7de", "h\u00f6rt", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen lust'gen J\u00e4gersmann.", "tokens": ["Ei\u00b7nen", "lust'\u00b7gen", "J\u00e4\u00b7gers\u00b7mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Tr\u00e4nen drangen leise:", "tokens": ["Und", "die", "Tr\u00e4\u00b7nen", "dran\u00b7gen", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So einst bl\u00fcht' es weit und breit,", "tokens": ["So", "einst", "bl\u00fcht'", "es", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Als mein Lieb dieselbe Weise", "tokens": ["Als", "mein", "Lieb", "die\u00b7sel\u00b7be", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich gelehrt vor langer Zeit.", "tokens": ["Mich", "ge\u00b7lehrt", "vor", "lan\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, ein solches Angedenken,", "tokens": ["Ach", ",", "ein", "sol\u00b7ches", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "'s ist nur eitel Klang und Luft,", "tokens": ["'s", "ist", "nur", "ei\u00b7tel", "Klang", "und", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann schimmernd doch versenken", "tokens": ["Und", "kann", "schim\u00b7mernd", "doch", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings in Tr\u00e4nen Tal und Kluft!", "tokens": ["Rings", "in", "Tr\u00e4\u00b7nen", "Tal", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}