{"textgrid.poem.47268": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schaut' am Neujahrsabend", "tokens": ["Ich", "schaut'", "am", "Neu\u00b7jahr\u00b7sa\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Himmel aus und sah:", "tokens": ["Zum", "Him\u00b7mel", "aus", "und", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Westen stand so labend", "tokens": ["Im", "Wes\u00b7ten", "stand", "so", "la\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Stern der Liebe da.", "tokens": ["Der", "Stern", "der", "Lie\u00b7be", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich blickt' am Neujahrsmorgen", "tokens": ["Ich", "blickt'", "am", "Neu\u00b7jahrs\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dann wieder auf, und sieh'!", "tokens": ["Dann", "wie\u00b7der", "auf", ",", "und", "sieh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "KON", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am Himmel wohlgeborgen", "tokens": ["Am", "Him\u00b7mel", "wohl\u00b7ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stand er im Osten hie.", "tokens": ["Stand", "er", "im", "Os\u00b7ten", "hie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hast dich wohl betrogen,", "tokens": ["Du", "hast", "dich", "wohl", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spricht ein gelehrter Mann;", "tokens": ["Spricht", "ein", "ge\u00b7lehr\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil nie am Himmelsbogen", "tokens": ["Weil", "nie", "am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschehn dergleichen kann:", "tokens": ["Ge\u00b7schehn", "derg\u00b7lei\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es l\u00e4\u00dft der Stern entweder", "tokens": ["Es", "l\u00e4\u00dft", "der", "Stern", "ent\u00b7we\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort oder hier sich sehn,", "tokens": ["Dort", "o\u00b7der", "hier", "sich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch kann er nicht in jeder", "tokens": ["Doch", "kann", "er", "nicht", "in", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gestalt zugleich bestehn.", "tokens": ["Ge\u00b7stalt", "zu\u00b7gleich", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das wei\u00df ich selbst am besten,", "tokens": ["Das", "wei\u00df", "ich", "selbst", "am", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df nie euch weisen Herrn", "tokens": ["Da\u00df", "nie", "euch", "wei\u00b7sen", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zugleich in Ost und Westen", "tokens": ["Zu\u00b7gleich", "in", "Ost", "und", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erscheint der Liebe Stern.", "tokens": ["Er\u00b7scheint", "der", "Lie\u00b7be", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der aber, den ich meine,", "tokens": ["Der", "a\u00b7ber", ",", "den", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der steht an jedem Ort,", "tokens": ["Der", "steht", "an", "je\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in viel hellerm Scheine", "tokens": ["Und", "in", "viel", "hel\u00b7lerm", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als der am Himmel dort.", "tokens": ["Als", "der", "am", "Him\u00b7mel", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Stern, da\u00df ich es sage,", "tokens": ["Der", "Stern", ",", "da\u00df", "ich", "es", "sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Stern ist solcher Art,", "tokens": ["Ein", "Stern", "ist", "sol\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ich im Busen trage", "tokens": ["Wie", "ich", "im", "Bu\u00b7sen", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebe hoch und zart;", "tokens": ["Die", "Lie\u00b7be", "hoch", "und", "zart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der hat mich angefunkelt", "tokens": ["Der", "hat", "mich", "an\u00b7ge\u00b7fun\u00b7kelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl zu des Jahres Schlu\u00df", "tokens": ["Wohl", "zu", "des", "Jah\u00b7res", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und strahlet unverdunkelt", "tokens": ["Und", "strah\u00b7let", "un\u00b7ver\u00b7dun\u00b7kelt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir auch den Morgengru\u00df.", "tokens": ["Mir", "auch", "den", "Mor\u00b7gen\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Stern hat mir verhei\u00dfen,", "tokens": ["Der", "Stern", "hat", "mir", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df bei des Himmels Dreh'n", "tokens": ["Da\u00df", "bei", "des", "Him\u00b7mels", "Dreh'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bei der Jahre Kreisen", "tokens": ["Und", "bei", "der", "Jah\u00b7re", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er nie will untergehn;", "tokens": ["Er", "nie", "will", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er will, wie eins sich neiget,", "tokens": ["Er", "will", ",", "wie", "eins", "sich", "nei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stets funkeln hell und klar,", "tokens": ["Stets", "fun\u00b7keln", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wie ein andres steiget,", "tokens": ["Und", "wie", "ein", "and\u00b7res", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch immer heller gar.", "tokens": ["Noch", "im\u00b7mer", "hel\u00b7ler", "gar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O sel'ge Doppelhelle", "tokens": ["O", "sel'\u00b7ge", "Dop\u00b7pel\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von wunderbarem Schein,", "tokens": ["Von", "wun\u00b7der\u00b7ba\u00b7rem", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An jedes Jahres Schwelle", "tokens": ["An", "je\u00b7des", "Jah\u00b7res", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir leuchtend aus und ein!", "tokens": ["Mir", "leuch\u00b7tend", "aus", "und", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht auf und nieder gehend,", "tokens": ["Nicht", "auf", "und", "nie\u00b7der", "ge\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "KON", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald nah' und bald auch fern,", "tokens": ["Bald", "nah'", "und", "bald", "auch", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nein, fest im Wechsel stehend,", "tokens": ["Nein", ",", "fest", "im", "Wech\u00b7sel", "ste\u00b7hend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Morgenabendstern.", "tokens": ["Ein", "Mor\u00b7gen\u00b7a\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wenn nun das ganze Leben", "tokens": ["Wenn", "nun", "das", "gan\u00b7ze", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verrollt ist wie ein Jahr,", "tokens": ["Ver\u00b7rollt", "ist", "wie", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollst du im Abend eben", "tokens": ["Sollst", "du", "im", "A\u00b7bend", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch stehn so hell und klar;", "tokens": ["Noch", "stehn", "so", "hell", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn ein neuer Morgen", "tokens": ["Und", "wenn", "ein", "neu\u00b7er", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aufd\u00e4mmert aus der Nacht,", "tokens": ["Auf\u00b7d\u00e4m\u00b7mert", "aus", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So gr\u00fc\u00df' mich wohlgeborgen", "tokens": ["So", "gr\u00fc\u00df'", "mich", "wohl\u00b7ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zuerst in deiner Pracht.", "tokens": ["Zu\u00b7erst", "in", "dei\u00b7ner", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}