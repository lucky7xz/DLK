{"textgrid.poem.53037": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "2. Characteristische Schilderung", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die bildende Natur, die Mutter aller Sch\u00f6nheit,", "tokens": ["Die", "bil\u00b7den\u00b7de", "Na\u00b7tur", ",", "die", "Mut\u00b7ter", "al\u00b7ler", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hatt' in dem K\u00f6rper selbst ihr Wesen abgedruckt.", "tokens": ["Hatt'", "in", "dem", "K\u00f6r\u00b7per", "selbst", "ihr", "We\u00b7sen", "ab\u00b7ge\u00b7druckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die Jugend gr\u00fcnte noch auf allen ihren Gliedern,", "tokens": ["Die", "Ju\u00b7gend", "gr\u00fcn\u00b7te", "noch", "auf", "al\u00b7len", "ih\u00b7ren", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch ihre frische Zier war jeder Theil belebt,", "tokens": ["Durch", "ih\u00b7re", "fri\u00b7sche", "Zier", "war", "je\u00b7der", "Theil", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr rundes Antlitz schlo\u00df in seinem weissen Kreise", "tokens": ["Ihr", "run\u00b7des", "Ant\u00b7litz", "schlo\u00df", "in", "sei\u00b7nem", "weis\u00b7sen", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Anmuth Reichthum ein. Auf ihrer freyen Stirn,", "tokens": ["Der", "An\u00b7muth", "Reicht\u00b7hum", "ein", ".", "Auf", "ih\u00b7rer", "frey\u00b7en", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die immer heiter blieb, stand Redlichkeit und Treue", "tokens": ["Die", "im\u00b7mer", "hei\u00b7ter", "blieb", ",", "stand", "Red\u00b7lich\u00b7keit", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Im reinsten Glanz gemahlt. Der Augen holdes Paar", "tokens": ["Im", "reins\u00b7ten", "Glanz", "ge\u00b7mahlt", ".", "Der", "Au\u00b7gen", "hol\u00b7des", "Paar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Warf seine Strahlen nicht zu stark, auch nicht zu milde,", "tokens": ["Warf", "sei\u00b7ne", "Strah\u00b7len", "nicht", "zu", "stark", ",", "auch", "nicht", "zu", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "PTKA", "ADJD", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und furchtsam abgeschmackt. Ein jeder freyer Blick", "tokens": ["Und", "furcht\u00b7sam", "ab\u00b7ge\u00b7schmackt", ".", "Ein", "je\u00b7der", "frey\u00b7er", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$.", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schien stets von jedem Trieb des Herzens sanft zu reden,", "tokens": ["Schien", "stets", "von", "je\u00b7dem", "Trieb", "des", "Her\u00b7zens", "sanft", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "War gegen die von Lieb' und sonst von G\u00fcte reich.", "tokens": ["War", "ge\u00b7gen", "die", "von", "Lieb'", "und", "sonst", "von", "G\u00fc\u00b7te", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "APPR", "NN", "KON", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Unschuld reines Feld, die Wangen wurden nimmer", "tokens": ["Der", "Un\u00b7schuld", "rei\u00b7nes", "Feld", ",", "die", "Wan\u00b7gen", "wur\u00b7den", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von eigenem Versehn, nein, nur von andern roth.", "tokens": ["Von", "ei\u00b7ge\u00b7nem", "Ver\u00b7sehn", ",", "nein", ",", "nur", "von", "an\u00b7dern", "roth", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKANT", "$,", "ADV", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und eh ihr Mund noch sprach, verrieth sein holdes Regen", "tokens": ["Und", "eh", "ihr", "Mund", "noch", "sprach", ",", "ver\u00b7rieth", "sein", "hol\u00b7des", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was schon ihr Geist bedacht, und jede Red erhielt", "tokens": ["Was", "schon", "ihr", "Geist", "be\u00b7dacht", ",", "und", "je\u00b7de", "Red", "er\u00b7hielt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von der Geberden Reiz noch eine gr\u00f6ssre Anmuth.", "tokens": ["Von", "der", "Ge\u00b7ber\u00b7den", "Reiz", "noch", "ei\u00b7ne", "gr\u00f6ss\u00b7re", "An\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Sie sprach, man ward bewegt; Sie schwieg und sie gefiel,", "tokens": ["Sie", "sprach", ",", "man", "ward", "be\u00b7wegt", ";", "Sie", "schwieg", "und", "sie", "ge\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und niemals durfte sie ein Wort zur\u00fccke w\u00fcnschen.", "tokens": ["Und", "nie\u00b7mals", "durf\u00b7te", "sie", "ein", "Wort", "zu\u00b7r\u00fc\u00b7cke", "w\u00fcn\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ihr Leib war wohlgesezt, und einer Frau gem\u00e4\u00df,", "tokens": ["Ihr", "Leib", "war", "wohl\u00b7ge\u00b7sezt", ",", "und", "ei\u00b7ner", "Frau", "ge\u00b7m\u00e4\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nicht, wider die Natur, zu enge eingepresset,", "tokens": ["Nicht", ",", "wi\u00b7der", "die", "Na\u00b7tur", ",", "zu", "en\u00b7ge", "ein\u00b7ge\u00b7pres\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die angenehme Art die stets ihr Thun beseelt,", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Art", "die", "stets", "ihr", "Thun", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gab jeden Reizungen ein zierliches Geschicke", "tokens": ["Gab", "je\u00b7den", "Rei\u00b7zun\u00b7gen", "ein", "zier\u00b7li\u00b7ches", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Und alles war demnach nat\u00fcrlich ohne Zwang.", "tokens": ["Und", "al\u00b7les", "war", "dem\u00b7nach", "na\u00b7t\u00fcr\u00b7lich", "oh\u00b7ne", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PAV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}