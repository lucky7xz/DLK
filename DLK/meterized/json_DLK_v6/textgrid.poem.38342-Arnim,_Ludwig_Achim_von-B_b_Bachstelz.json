{"textgrid.poem.38342": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "B b Bachstelz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bachstelz thut oft schnappen, und f\u00e4ngt der M\u00fccken viel,", "tokens": ["Die", "Bach\u00b7stelz", "thut", "oft", "schnap\u00b7pen", ",", "und", "f\u00e4ngt", "der", "M\u00fc\u00b7cken", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es h\u00f6rt nicht auf zu knappen ihr langer Pfannenstiel,", "tokens": ["Es", "h\u00f6rt", "nicht", "auf", "zu", "knap\u00b7pen", "ihr", "lan\u00b7ger", "Pfan\u00b7nen\u00b7stiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den Schweif thut sie stets zwingen, sie l\u00e4\u00dft ihm niemals Ruh,", "tokens": ["Den", "Schweif", "thut", "sie", "stets", "zwin\u00b7gen", ",", "sie", "l\u00e4\u00dft", "ihm", "nie\u00b7mals", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn andre V\u00f6gel singen, schl\u00e4gt sie den Tackt dazu.", "tokens": ["Wenn", "and\u00b7re", "V\u00f6\u00b7gel", "sin\u00b7gen", ",", "schl\u00e4gt", "sie", "den", "Tackt", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}