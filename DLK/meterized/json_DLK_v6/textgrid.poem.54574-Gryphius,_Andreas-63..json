{"textgrid.poem.54574": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich werde gantz erschr\u00f6ckt: so trefflich di\u00df Gesichte/", "tokens": ["Ich", "wer\u00b7de", "gantz", "er\u00b7schr\u00f6ckt", ":", "so", "treff\u00b7lich", "di\u00df", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "ADV", "ADJD", "PDS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sittsam jener Zucht/ so herrlich dieser Geist/", "tokens": ["So", "sitt\u00b7sam", "je\u00b7ner", "Zucht", "/", "so", "herr\u00b7lich", "die\u00b7ser", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "$(", "ADV", "ADJD", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und munterer Verstand/ der alles nach sich reist/", "tokens": ["Und", "mun\u00b7te\u00b7rer", "Ver\u00b7stand", "/", "der", "al\u00b7les", "nach", "sich", "reist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "ART", "PIS", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So liebreich jener Mund/ der gleicht dem Morgenlichte;", "tokens": ["So", "lieb\u00b7reich", "je\u00b7ner", "Mund", "/", "der", "gleicht", "dem", "Mor\u00b7gen\u00b7lich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "$(", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So widrig kommt mir vor di\u00df wegernde Gez\u00fcchte/", "tokens": ["So", "wid\u00b7rig", "kommt", "mir", "vor", "di\u00df", "we\u00b7gern\u00b7de", "Ge\u00b7z\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der/ die nur stets versagt. Wo ihr di\u00df Tugend heist/", "tokens": ["Der", "/", "die", "nur", "stets", "ver\u00b7sagt", ".", "Wo", "ihr", "di\u00df", "Tu\u00b7gend", "heist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "ADV", "ADV", "VVPP", "$.", "PWAV", "PPER", "PDS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Versteh ich warlich nicht wo Hochmuth sich erweist/", "tokens": ["Ver\u00b7steh", "ich", "war\u00b7lich", "nicht", "wo", "Hoch\u00b7muth", "sich", "er\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PWAV", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ob ein solcher Wahn trag angenehme Fr\u00fcchte.", "tokens": ["Und", "ob", "ein", "sol\u00b7cher", "Wahn", "trag", "an\u00b7ge\u00b7neh\u00b7me", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vier Sonnen scheinen uns/ was fragen wir nach der", "tokens": ["Vier", "Son\u00b7nen", "schei\u00b7nen", "uns", "/", "was", "fra\u00b7gen", "wir", "nach", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$(", "PWS", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die nur sich selber acht. Kommt Nymfen kommt heran/", "tokens": ["Die", "nur", "sich", "sel\u00b7ber", "acht", ".", "Kommt", "Nym\u00b7fen", "kommt", "he\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ADV", "CARD", "$.", "VVFIN", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kommt ", "tokens": ["Kommt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Jedoch was w\u00fcnschen wir/ was suchen wir vor Spiel/", "tokens": ["Je\u00b7doch", "was", "w\u00fcn\u00b7schen", "wir", "/", "was", "su\u00b7chen", "wir", "vor", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "$(", "PWS", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn Jugend/ Muth/ Verstand und Sch\u00f6nheit spielen wil/", "tokens": ["Wenn", "Ju\u00b7gend", "/", "Muth", "/", "Ver\u00b7stand", "und", "Sch\u00f6n\u00b7heit", "spie\u00b7len", "wil", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kan sie noch etwas mehr das sch\u00f6ner sey uns weisen/", "tokens": ["Kan", "sie", "noch", "et\u00b7was", "mehr", "das", "sch\u00f6\u00b7ner", "sey", "uns", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}