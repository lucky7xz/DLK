{"textgrid.poem.36536": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "8. Von der Eichel und dem K\u00fcrbis", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sohn, mit Weisheit und Verstand", "tokens": ["Sohn", ",", "mit", "Weis\u00b7heit", "und", "Ver\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ordnete des Sch\u00f6pfers Hand", "tokens": ["Ord\u00b7ne\u00b7te", "des", "Sch\u00f6p\u00b7fers", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Dinge. Sieh umher!", "tokens": ["Al\u00b7le", "Din\u00b7ge", ".", "Sieh", "um\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keines steht von ohngef\u00e4hr,", "tokens": ["Kei\u00b7nes", "steht", "von", "ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo es steht! Das Firmament,", "tokens": ["Wo", "es", "steht", "!", "Das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die gro\u00dfe Sonne brennt,", "tokens": ["Wo", "die", "gro\u00b7\u00dfe", "Son\u00b7ne", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der kleinste Sonnenstaub,", "tokens": ["Und", "der", "kleins\u00b7te", "Son\u00b7nen\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Deines Atems leichter Raub,", "tokens": ["Dei\u00b7nes", "A\u00b7tems", "leich\u00b7ter", "Raub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Trat, auf unsers Gottes Wort,", "tokens": ["Trat", ",", "auf", "un\u00b7sers", "Got\u00b7tes", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Jegliches an seinen Ort.", "tokens": ["Jeg\u00b7li\u00b7ches", "an", "sei\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jedes Ding in seiner Welt", "tokens": ["Je\u00b7des", "Ding", "in", "sei\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ist vollkommen; dennoch h\u00e4lt", "tokens": ["Ist", "voll\u00b7kom\u00b7men", ";", "den\u00b7noch", "h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Mancher Thor es nicht daf\u00fcr,", "tokens": ["Man\u00b7cher", "Thor", "es", "nicht", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und kunstrichtet Gott in ihr!", "tokens": ["Und", "kuns\u00b7trich\u00b7tet", "Gott", "in", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "APPR", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So ein Thor war jener Mann,", "tokens": ["So", "ein", "Thor", "war", "je\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich dir nicht nennen kann,", "tokens": ["Den", "ich", "dir", "nicht", "nen\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, als er an schwachen Ranken", "tokens": ["Der", ",", "als", "er", "an", "schwa\u00b7chen", "Ran\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einen K\u00fcrbis hangen sah,", "tokens": ["Ei\u00b7nen", "K\u00fcr\u00b7bis", "han\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gro\u00df und schwer, wie deiner da,", "tokens": ["Gro\u00df", "und", "schwer", ",", "wie", "dei\u00b7ner", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "PPOSAT", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den du selbst gezogen hast,", "tokens": ["Den", "du", "selbst", "ge\u00b7zo\u00b7gen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Den verwegenen Gedanken", "tokens": ["Den", "ver\u00b7we\u00b7ge\u00b7nen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hegte: ", "tokens": ["Heg\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Also denkend geht er fort,", "tokens": ["Al\u00b7so", "den\u00b7kend", "geht", "er", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gelanget an den Ort", "tokens": ["Und", "ge\u00b7lan\u00b7get", "an", "den", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer Eiche; lagert sich", "tokens": ["Ei\u00b7ner", "Ei\u00b7che", ";", "la\u00b7gert", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4ngelang in ihren Schatten,", "tokens": ["L\u00e4n\u00b7ge\u00b7lang", "in", "ih\u00b7ren", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und schl\u00e4ft ein. \u2013", "tokens": ["Und", "schl\u00e4ft", "ein", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Die Winde hatten", "tokens": ["Die", "Win\u00b7de", "hat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Manchen Monat nicht geweht;", "tokens": ["Man\u00b7chen", "Mo\u00b7nat", "nicht", "ge\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, als er schl\u00e4ft, entsteht", "tokens": ["A\u00b7ber", ",", "als", "er", "schl\u00e4ft", ",", "ent\u00b7steht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Eiche hohem Wipfel", "tokens": ["In", "der", "Ei\u00b7che", "ho\u00b7hem", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Gebrause; starke Weste", "tokens": ["Ein", "Ge\u00b7brau\u00b7se", ";", "star\u00b7ke", "Wes\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fctteln ihre vollen \u00c4ste;", "tokens": ["Sch\u00fct\u00b7teln", "ih\u00b7re", "vol\u00b7len", "\u00c4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Pl\u00f6tzlich st\u00fcrzt, von dem Bewegen,", "tokens": ["Pl\u00f6tz\u00b7lich", "st\u00fcrzt", ",", "von", "dem", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Prasselnd ein geschwinder Regen", "tokens": ["Pras\u00b7selnd", "ein", "ge\u00b7schwin\u00b7der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Reifer Eicheln von dem Gipfel.", "tokens": ["Rei\u00b7fer", "Ei\u00b7cheln", "von", "dem", "Gip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Viele liegen auf dem Grase,", "tokens": ["Vie\u00b7le", "lie\u00b7gen", "auf", "dem", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber eine f\u00e4llt gerade", "tokens": ["A\u00b7ber", "ei\u00b7ne", "f\u00e4llt", "ge\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dem Kunstrichter auf die Nase!", "tokens": ["Dem", "Kuns\u00b7trich\u00b7ter", "auf", "die", "Na\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pl\u00f6tzlich springt er auf und sieht,", "tokens": ["Pl\u00f6tz\u00b7lich", "springt", "er", "auf", "und", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie blutet: ", "tokens": ["Da\u00df", "sie", "blu\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und bereuet auf der Flucht", "tokens": ["Und", "be\u00b7re\u00b7u\u00b7et", "auf", "der", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gedanken, welcher wollte,", "tokens": ["Den", "Ge\u00b7dan\u00b7ken", ",", "wel\u00b7cher", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der Eichbaum eine Frucht,", "tokens": ["Da\u00df", "der", "Eich\u00b7baum", "ei\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich dem K\u00fcrbis, tragen sollte.", "tokens": ["Gleich", "dem", "K\u00fcr\u00b7bis", ",", "tra\u00b7gen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}