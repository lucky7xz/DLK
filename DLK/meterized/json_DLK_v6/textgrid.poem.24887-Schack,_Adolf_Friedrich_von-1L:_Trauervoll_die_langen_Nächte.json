{"textgrid.poem.24887": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trauervoll die langen N\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trauervoll die langen N\u00e4chte", "tokens": ["Trau\u00b7er\u00b7voll", "die", "lan\u00b7gen", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehn' ich an dem Dachgel\u00e4nder,", "tokens": ["Lehn'", "ich", "an", "dem", "Dach\u00b7ge\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und an meine Lippen dr\u00fcck' ich", "tokens": ["Und", "an", "mei\u00b7ne", "Lip\u00b7pen", "dr\u00fcck", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Liebe s\u00fc\u00dfe Pf\u00e4nder;", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Pf\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denke jener sel'gen Stunden,", "tokens": ["Den\u00b7ke", "je\u00b7ner", "sel'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da wir beide, K\u00fcsse tauschend,", "tokens": ["Da", "wir", "bei\u00b7de", ",", "K\u00fcs\u00b7se", "tau\u00b7schend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leben uns und Seele schenkten", "tokens": ["Le\u00b7ben", "uns", "und", "See\u00b7le", "schenk\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als zwei gl\u00fcckliche Verschwender.", "tokens": ["Als", "zwei", "gl\u00fcck\u00b7li\u00b7che", "Ver\u00b7schwen\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wo weilt sie nun, die Holde,", "tokens": ["O", "wo", "weilt", "sie", "nun", ",", "die", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df umsonst ich nach ihr sp\u00e4he?", "tokens": ["Da\u00df", "um\u00b7sonst", "ich", "nach", "ihr", "sp\u00e4\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bergt ihr sie in euren Thalen", "tokens": ["Bergt", "ihr", "sie", "in", "eu\u00b7ren", "Tha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer noch, ihr Euphratl\u00e4nder?", "tokens": ["Im\u00b7mer", "noch", ",", "ihr", "Eu\u00b7phrat\u00b7l\u00e4n\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sehnend in die Weite sp\u00e4h' ich,", "tokens": ["Seh\u00b7nend", "in", "die", "Wei\u00b7te", "sp\u00e4h'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob mein Blick die Karawane", "tokens": ["Ob", "mein", "Blick", "die", "Ka\u00b7ra\u00b7wa\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht erschaut und nicht von ferne", "tokens": ["Nicht", "er\u00b7schaut", "und", "nicht", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "KON", "PTKNEG", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dfe wallende Gew\u00e4nder;", "tokens": ["Wei\u00b7\u00dfe", "wal\u00b7len\u00b7de", "Ge\u00b7w\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber statt des Schalls der Gl\u00f6ckchen", "tokens": ["A\u00b7ber", "statt", "des", "Schalls", "der", "Gl\u00f6ck\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6r' ich nur den Schakal heulen,", "tokens": ["H\u00f6r'", "ich", "nur", "den", "Scha\u00b7kal", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der \u00f6de Morgen d\u00e4mmert", "tokens": ["Bis", "der", "\u00f6\u00b7de", "Mor\u00b7gen", "d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um der W\u00fcste blasse R\u00e4nder.", "tokens": ["Um", "der", "W\u00fcs\u00b7te", "blas\u00b7se", "R\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}