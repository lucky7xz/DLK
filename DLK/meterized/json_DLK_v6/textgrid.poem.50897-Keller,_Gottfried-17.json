{"textgrid.poem.50897": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "17", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerbogen und zerkniffen war der vordre Rand an meinem Hut,", "tokens": ["Zer\u00b7bo\u00b7gen", "und", "zer\u00b7knif\u00b7fen", "war", "der", "vor\u00b7dre", "Rand", "an", "mei\u00b7nem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und r\u00f6tlich f\u00e4rbte er sich auch, wie es des S\u00e4ufers Nase tut.", "tokens": ["Und", "r\u00f6t\u00b7lich", "f\u00e4rb\u00b7te", "er", "sich", "auch", ",", "wie", "es", "des", "S\u00e4u\u00b7fers", "Na\u00b7se", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und wenn ich auf der Stra\u00dfe ging, so fiel ich in der Sp\u00f6tter Schlingen;", "tokens": ["Und", "wenn", "ich", "auf", "der", "Stra\u00b7\u00dfe", "ging", ",", "so", "fiel", "ich", "in", "der", "Sp\u00f6t\u00b7ter", "Schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Das f\u00fcllte mich mit \u00c4rger, denn im ganzen war der Chapeau gut.", "tokens": ["Das", "f\u00fcll\u00b7te", "mich", "mit", "\u00c4r\u00b7ger", ",", "denn", "im", "gan\u00b7zen", "war", "der", "Cha\u00b7pe\u00b7au", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "APPRART", "ADJA", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Drum dreht ich ihn, bis hinter mir des W\u00fcrdigen gel\u00e4hmte Schwingen,", "tokens": ["Drum", "dreht", "ich", "ihn", ",", "bis", "hin\u00b7ter", "mir", "des", "W\u00fcr\u00b7di\u00b7gen", "ge\u00b7l\u00e4hm\u00b7te", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Und, vorn den wohlerhaltnen Rand, trat ich einher mit frischem Mut.", "tokens": ["Und", ",", "vorn", "den", "woh\u00b7ler\u00b7halt\u00b7nen", "Rand", ",", "trat", "ich", "ein\u00b7her", "mit", "fri\u00b7schem", "Mut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Doch weh! an meinem R\u00fccken nun die tausend schlimmen Augen hingen,", "tokens": ["Doch", "weh", "!", "an", "mei\u00b7nem", "R\u00fc\u00b7cken", "nun", "die", "tau\u00b7send", "schlim\u00b7men", "Au\u00b7gen", "hin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "ADV", "ART", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Ich h\u00f6rte zischeln ", "tokens": ["Ich", "h\u00f6r\u00b7te", "zi\u00b7scheln"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und zwang mich, den verdammten Filz flugs wieder vorn herum zu bringen:", "tokens": ["Und", "zwang", "mich", ",", "den", "ver\u00b7damm\u00b7ten", "Filz", "flugs", "wie\u00b7der", "vorn", "he\u00b7rum", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "Denn lieber vor als hinter mir mag ich der Tadler stille Wut.", "tokens": ["Denn", "lie\u00b7ber", "vor", "als", "hin\u00b7ter", "mir", "mag", "ich", "der", "Tad\u00b7ler", "stil\u00b7le", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "KOKOM", "APPR", "PPER", "VMFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "In seinen Schatten neige dich, Schlu\u00dfton von allem meinem Singen,", "tokens": ["In", "sei\u00b7nen", "Schat\u00b7ten", "nei\u00b7ge", "dich", ",", "Schlu\u00df\u00b7ton", "von", "al\u00b7lem", "mei\u00b7nem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "NE", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Mein treues Lieb, und tr\u00f6ste mich mit deiner Lippen s\u00fc\u00dfer Glut.", "tokens": ["Mein", "treu\u00b7es", "Lieb", ",", "und", "tr\u00f6s\u00b7te", "mich", "mit", "dei\u00b7ner", "Lip\u00b7pen", "s\u00fc\u00b7\u00dfer", "Glut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}