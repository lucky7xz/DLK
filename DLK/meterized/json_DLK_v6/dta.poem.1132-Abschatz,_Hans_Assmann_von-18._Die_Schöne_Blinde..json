{"dta.poem.1132": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "18. Die Sch\u00f6ne Blinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner Maulwurff blinder Liebe/ mit Erbarmnis nehm ", "tokens": ["Sch\u00f6\u00b7ner", "Maul\u00b7wurff", "blin\u00b7der", "Lie\u00b7be", "/", "mit", "Er\u00b7barm\u00b7nis", "nehm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "$(", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "ich wahr/ ", "tokens": ["ich", "wahr", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wie der Augenbronnen Bogen seiner Pfeile mu\u00df entbehren/", "tokens": ["Wie", "der", "Au\u00b7gen\u00b7bron\u00b7nen", "Bo\u00b7gen", "sei\u00b7ner", "Pfei\u00b7le", "mu\u00df", "ent\u00b7beh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie die roth-beflammten Wangen unbelebte Glutt ern\u00e4h-", "tokens": ["Wie", "die", "ro\u00b7th\u00b7be\u00b7flamm\u00b7ten", "Wan\u00b7gen", "un\u00b7be\u00b7leb\u00b7te", "Glutt", "er\u00b7n\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "-+++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "ren. ", "tokens": ["ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Wie du must den duncklen Schatten d\u00fcstrer N\u00e4chte stellen dar.", "tokens": ["Wie", "du", "must", "den", "dunck\u00b7len", "Schat\u00b7ten", "d\u00fcst\u00b7rer", "N\u00e4ch\u00b7te", "stel\u00b7len", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wer erkennet in dem Finstern deiner Ros\u2019 und Liljen Schaar/", "tokens": ["Wer", "er\u00b7ken\u00b7net", "in", "dem", "Fins\u00b7tern", "dei\u00b7ner", "Ros'", "und", "Lil\u00b7jen", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Wenn sie keiner Sonne Stralen an das helle Licht geweh-", "tokens": ["Wenn", "sie", "kei\u00b7ner", "Son\u00b7ne", "Stra\u00b7len", "an", "das", "hel\u00b7le", "Licht", "ge\u00b7weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "NN", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "ren? ", "tokens": ["ren", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Doch der \u00e4userliche Mangel kan die Seele nicht beschweren:", "tokens": ["Doch", "der", "\u00e4u\u00b7ser\u00b7li\u00b7che", "Man\u00b7gel", "kan", "die", "See\u00b7le", "nicht", "be\u00b7schwe\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wer die Liebe/ wer die Warheit schauet/ sieht genugsam klar.", "tokens": ["Wer", "die", "Lie\u00b7be", "/", "wer", "die", "War\u00b7heit", "schau\u00b7et", "/", "sieht", "ge\u00b7nug\u00b7sam", "klar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$(", "PWS", "ART", "NN", "VVFIN", "$(", "VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Wird nicht durch den s\u00fcssen Schlaff iedes Auge zugemacht?", "tokens": ["Wird", "nicht", "durch", "den", "s\u00fcs\u00b7sen", "Schlaff", "ie\u00b7des", "Au\u00b7ge", "zu\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "War die junge Welt nicht selbst voller Finsternis und Nacht?", "tokens": ["War", "die", "jun\u00b7ge", "Welt", "nicht", "selbst", "vol\u00b7ler", "Fins\u00b7ter\u00b7nis", "und", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Warum solte dich der Abgang deiner Augen schmertzen k\u00fcn-", "tokens": ["Wa\u00b7rum", "sol\u00b7te", "dich", "der", "Ab\u00b7gang", "dei\u00b7ner", "Au\u00b7gen", "schmert\u00b7zen", "k\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "VVFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "nen? ", "tokens": ["nen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Sonne blendet/ Augen tr\u00fcgen/ aber das Gem\u00fctte nicht/", "tokens": ["Son\u00b7ne", "blen\u00b7det", "/", "Au\u00b7gen", "tr\u00fc\u00b7gen", "/", "a\u00b7ber", "das", "Ge\u00b7m\u00fct\u00b7te", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "NN", "VVINF", "$(", "ADV", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Welches in sich selbst gezogen/ durch die \u00e4userliche Sinnen", "tokens": ["Wel\u00b7ches", "in", "sich", "selbst", "ge\u00b7zo\u00b7gen", "/", "durch", "die", "\u00e4u\u00b7ser\u00b7li\u00b7che", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PRF", "ADV", "VVPP", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Unverleitet/ selbst vom Himmel sch\u00f6pffet ein gewisses Licht.", "tokens": ["Un\u00b7ver\u00b7lei\u00b7tet", "/", "selbst", "vom", "Him\u00b7mel", "sch\u00f6pf\u00b7fet", "ein", "ge\u00b7wis\u00b7ses", "Licht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}