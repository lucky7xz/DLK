{"textgrid.poem.56108": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die dritte Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eines ist, die Geliebte zu singen. Ein anderes, wehe,", "tokens": ["Ei\u00b7nes", "ist", ",", "die", "Ge\u00b7lieb\u00b7te", "zu", "sin\u00b7gen", ".", "Ein", "an\u00b7de\u00b7res", ",", "we\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$.", "ART", "PIS", "$,", "ADJD", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "jenen verborgenen schuldigen Flu\u00df-Gott des Bluts.", "tokens": ["je\u00b7nen", "ver\u00b7bor\u00b7ge\u00b7nen", "schul\u00b7di\u00b7gen", "Flu\u00df\u00b7Gott", "des", "Bluts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--++-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Den sie von weitem erkennt, ihren J\u00fcngling, was wei\u00df er", "tokens": ["Den", "sie", "von", "wei\u00b7tem", "er\u00b7kennt", ",", "ih\u00b7ren", "J\u00fcng\u00b7ling", ",", "was", "wei\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIS", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "selbst von dem Herren der Lust, der aus dem Einsamen oft,", "tokens": ["selbst", "von", "dem", "Her\u00b7ren", "der", "Lust", ",", "der", "aus", "dem", "Ein\u00b7sa\u00b7men", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "ehe das M\u00e4dchen noch linderte, oft auch als w\u00e4re sie nicht,", "tokens": ["e\u00b7he", "das", "M\u00e4d\u00b7chen", "noch", "lin\u00b7der\u00b7te", ",", "oft", "auch", "als", "w\u00e4\u00b7re", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,", "ADV", "ADV", "KOKOM", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.6": {"text": "ach, von welchem Unkenntlichen triefend, das Gotthaupt", "tokens": ["ach", ",", "von", "wel\u00b7chem", "Un\u00b7kennt\u00b7li\u00b7chen", "trie\u00b7fend", ",", "das", "Gott\u00b7haupt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["XY", "$,", "APPR", "PWAT", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "aufhob, aufrufend die Nacht zu unendlichem Aufruhr.", "tokens": ["auf\u00b7hob", ",", "auf\u00b7ru\u00b7fend", "die", "Nacht", "zu", "un\u00b7end\u00b7li\u00b7chem", "Auf\u00b7ruhr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.8": {"text": "O des Blutes Neptun, o sein furchtbarer Dreizack.", "tokens": ["O", "des", "Blu\u00b7tes", "Nep\u00b7tun", ",", "o", "sein", "furcht\u00b7ba\u00b7rer", "Drei\u00b7zack", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,", "FM", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-++-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "O der dunkele Wind seiner Brust aus gewundener Muschel.", "tokens": ["O", "der", "dun\u00b7ke\u00b7le", "Wind", "sei\u00b7ner", "Brust", "aus", "ge\u00b7wun\u00b7de\u00b7ner", "Mu\u00b7schel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Horch, wie die Nacht sich muldet und h\u00f6hlt. Ihr Sterne,", "tokens": ["Horch", ",", "wie", "die", "Nacht", "sich", "mul\u00b7det", "und", "h\u00f6hlt", ".", "Ihr", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$.", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "stammt nicht von euch des Liebenden Lust zu dem Antlitz", "tokens": ["stammt", "nicht", "von", "euch", "des", "Lie\u00b7ben\u00b7den", "Lust", "zu", "dem", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "seiner Geliebten? Hat er die innige Einsicht", "tokens": ["sei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "?", "Hat", "er", "die", "in\u00b7ni\u00b7ge", "Ein\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "in ihr reines Gesicht nicht aus dem reinen Gestirn?", "tokens": ["in", "ihr", "rei\u00b7nes", "Ge\u00b7sicht", "nicht", "aus", "dem", "rei\u00b7nen", "Ge\u00b7stirn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du nicht hast ihm, wehe, nicht seine Mutter", "tokens": ["Du", "nicht", "hast", "ihm", ",", "we\u00b7he", ",", "nicht", "sei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "VAFIN", "PPER", "$,", "ADJD", "$,", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "hat ihm die Bogen der Braun so zur Erwartung gespannt.", "tokens": ["hat", "ihm", "die", "Bo\u00b7gen", "der", "Braun", "so", "zur", "Er\u00b7war\u00b7tung", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Nicht an dir, ihn f\u00fchlendes M\u00e4dchen, an dir nicht", "tokens": ["Nicht", "an", "dir", ",", "ihn", "f\u00fch\u00b7len\u00b7des", "M\u00e4d\u00b7chen", ",", "an", "dir", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "APPR", "PPER", "PTKNEG"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "bog seine Lippe sich zum fruchtbarern Ausdruck.", "tokens": ["bog", "sei\u00b7ne", "Lip\u00b7pe", "sich", "zum", "frucht\u00b7ba\u00b7rern", "Aus\u00b7druck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Meinst du wirklich, ihn h\u00e4tte dein leichter Auftritt", "tokens": ["Meinst", "du", "wirk\u00b7lich", ",", "ihn", "h\u00e4t\u00b7te", "dein", "leich\u00b7ter", "Auf\u00b7tritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "also ersch\u00fcttert, du, die wandelt wie Fr\u00fchwind?", "tokens": ["al\u00b7so", "er\u00b7sch\u00fct\u00b7tert", ",", "du", ",", "die", "wan\u00b7delt", "wie", "Fr\u00fch\u00b7wind", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PPER", "$,", "PRELS", "VVFIN", "KOKOM", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Zwar du erschrakst ihm das Herz; doch \u00e4ltere Schrecken", "tokens": ["Zwar", "du", "er\u00b7schrakst", "ihm", "das", "Herz", ";", "doch", "\u00e4l\u00b7te\u00b7re", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "st\u00fcrzten in ihn bei dem ber\u00fchrenden Ansto\u00df.", "tokens": ["st\u00fcrz\u00b7ten", "in", "ihn", "bei", "dem", "be\u00b7r\u00fch\u00b7ren\u00b7den", "An\u00b7sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Ruf ihn... du rufst ihn nicht ganz aus dunkelem Umgang.", "tokens": ["Ruf", "ihn", "...", "du", "rufst", "ihn", "nicht", "ganz", "aus", "dun\u00b7ke\u00b7lem", "Um\u00b7gang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Freilich, er ", "tokens": ["Frei\u00b7lich", ",", "er"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "sich in dein heimliches Herz und nimmt und beginnt sich.", "tokens": ["sich", "in", "dein", "heim\u00b7li\u00b7ches", "Herz", "und", "nimmt", "und", "be\u00b7ginnt", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Aber begann er sich je?", "tokens": ["A\u00b7ber", "be\u00b7gann", "er", "sich", "je", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Mutter, ", "tokens": ["Mut\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "dir war er neu, du beugtest \u00fcber die neuen", "tokens": ["dir", "war", "er", "neu", ",", "du", "beug\u00b7test", "\u00fc\u00b7ber", "die", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Augen die freundliche Welt und wehrtest der fremden.", "tokens": ["Au\u00b7gen", "die", "freund\u00b7li\u00b7che", "Welt", "und", "wehr\u00b7test", "der", "frem\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Wo, ach, hin sind die Jahre, da du ihm einfach", "tokens": ["Wo", ",", "ach", ",", "hin", "sind", "die", "Jah\u00b7re", ",", "da", "du", "ihm", "ein\u00b7fach"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ITJ", "$,", "ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "mit der schlanken Gestalt wallendes Chaos vertratst?", "tokens": ["mit", "der", "schlan\u00b7ken", "Ge\u00b7stalt", "wal\u00b7len\u00b7des", "Chaos", "ver\u00b7tratst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Vieles verbargst du ihm so; das n\u00e4chtlich-verd\u00e4chtige Zimmer", "tokens": ["Vie\u00b7les", "ver\u00b7bargst", "du", "ihm", "so", ";", "das", "n\u00e4cht\u00b7lich\u00b7ver\u00b7d\u00e4ch\u00b7ti\u00b7ge", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.19": {"text": "machtest du harmlos, aus deinem Herzen voll Zuflucht", "tokens": ["mach\u00b7test", "du", "harm\u00b7los", ",", "aus", "dei\u00b7nem", "Her\u00b7zen", "voll", "Zu\u00b7flucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "APPR", "PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "mischtest du menschlichern Raum seinem Nacht-Raum hinzu.", "tokens": ["mischtest", "du", "menschli\u00b7chern", "Raum", "sei\u00b7nem", "Nacht\u00b7Raum", "hin\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Nicht in die Finsternis, nein, in dein n\u00e4heres Dasein", "tokens": ["Nicht", "in", "die", "Fins\u00b7ter\u00b7nis", ",", "nein", ",", "in", "dein", "n\u00e4\u00b7he\u00b7res", "Da\u00b7sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "PTKANT", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "hast du das Nachtlicht gestellt, und es schien wie aus Freundschaft.", "tokens": ["hast", "du", "das", "Nacht\u00b7licht", "ge\u00b7stellt", ",", "und", "es", "schien", "wie", "aus", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Nirgends ein Knistern, das du nicht l\u00e4chelnd erkl\u00e4rtest,", "tokens": ["Nir\u00b7gends", "ein", "Knis\u00b7tern", ",", "das", "du", "nicht", "l\u00e4\u00b7chelnd", "er\u00b7kl\u00e4r\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "so als w\u00fc\u00dftest du l\u00e4ngst, ", "tokens": ["so", "als", "w\u00fc\u00df\u00b7test", "du", "l\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Und er horchte und linderte sich. So vieles vermochte", "tokens": ["Und", "er", "horch\u00b7te", "und", "lin\u00b7der\u00b7te", "sich", ".", "So", "vie\u00b7les", "ver\u00b7moch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "$.", "ADV", "PIS", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "z\u00e4rtlich dein Aufstehn; hinter den Schrank trat", "tokens": ["z\u00e4rt\u00b7lich", "dein", "Auf\u00b7stehn", ";", "hin\u00b7ter", "den", "Schrank", "trat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.27": {"text": "hoch im Mantel sein Schicksal, und in die Falten des Vorhangs", "tokens": ["hoch", "im", "Man\u00b7tel", "sein", "Schick\u00b7sal", ",", "und", "in", "die", "Fal\u00b7ten", "des", "Vor\u00b7hangs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "pa\u00dfte, die leicht sich verschob, seine unruhige Zukunft.", "tokens": ["pa\u00df\u00b7te", ",", "die", "leicht", "sich", "ver\u00b7schob", ",", "sei\u00b7ne", "un\u00b7ru\u00b7hi\u00b7ge", "Zu\u00b7kunft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+++-+-+-+-", "measure": "iambic.octa.plus.invert"