{"textgrid.poem.39381": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich klage nicht vor dir: du kennst die Trauer;", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich klage nicht vor dir: du kennst die Trauer;", "tokens": ["Ich", "kla\u00b7ge", "nicht", "vor", "dir", ":", "du", "kennst", "die", "Trau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wei\u00dft wie an des Scheiterhaufens Flammen", "tokens": ["Du", "wei\u00dft", "wie", "an", "des", "Schei\u00b7ter\u00b7hau\u00b7fens", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe gl\u00fch'nder ihre Fackel z\u00fcndet.", "tokens": ["Die", "Lie\u00b7be", "gl\u00fch'n\u00b7der", "ih\u00b7re", "Fa\u00b7ckel", "z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freuden Tempel st\u00fcrzt' auch dir zusammen,", "tokens": ["Der", "Freu\u00b7den", "Tem\u00b7pel", "st\u00fcrzt'", "auch", "dir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es hauchten kalt herein des Todes Schauer,", "tokens": ["Es", "hauch\u00b7ten", "kalt", "her\u00b7ein", "des", "To\u00b7des", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo Reiz und Huld ein Brautgemach gegr\u00fcndet.", "tokens": ["Wo", "Reiz", "und", "Huld", "ein", "Braut\u00b7ge\u00b7mach", "ge\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Drum sei mit mir verb\u00fcndet,", "tokens": ["Drum", "sei", "mit", "mir", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geliebter Freund, das Himmlische zu suchen,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Freund", ",", "das", "Himm\u00b7li\u00b7sche", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf da\u00df ich lerne, durch Gebet und Glauben", "tokens": ["Auf", "da\u00df", "ich", "ler\u00b7ne", ",", "durch", "Ge\u00b7bet", "und", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dem Tod sein Opfer rauben,", "tokens": ["Dem", "Tod", "sein", "Op\u00b7fer", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und nicht dem tauben Schicksal m\u00f6ge fluchen,", "tokens": ["Und", "nicht", "dem", "tau\u00b7ben", "Schick\u00b7sal", "m\u00f6\u00b7ge", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Des Zorn den Kelch des Lebens mir verbittert,", "tokens": ["Des", "Zorn", "den", "Kelch", "des", "Le\u00b7bens", "mir", "ver\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df mein Gebein vor solchem Tranke zittert.", "tokens": ["Da\u00df", "mein", "Ge\u00b7bein", "vor", "sol\u00b7chem", "Tran\u00b7ke", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du schienest, losgeri\u00dfen von der Erde,", "tokens": ["Du", "schie\u00b7nest", ",", "los\u00b7ge\u00b7ri\u00b7\u00dfen", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit leichten Geistertritten schon zu wandeln,", "tokens": ["Mit", "leich\u00b7ten", "Geis\u00b7ter\u00b7trit\u00b7ten", "schon", "zu", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ohne Tod der Sterblichkeit genesen.", "tokens": ["Und", "oh\u00b7ne", "Tod", "der", "Sterb\u00b7lich\u00b7keit", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du riefst hervor in dir durch geistig Handeln,", "tokens": ["Du", "riefst", "her\u00b7vor", "in", "dir", "durch", "geis\u00b7tig", "Han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPER", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Zauberer durch Zeichen und Geberde,", "tokens": ["Wie", "Zau\u00b7be\u00b7rer", "durch", "Zei\u00b7chen", "und", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Herzvereine das entschwundne Wesen.", "tokens": ["Zum", "Herz\u00b7ver\u00b7ei\u00b7ne", "das", "ent\u00b7schwund\u00b7ne", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df mich denn jetzo lesen", "tokens": ["La\u00df", "mich", "denn", "jet\u00b7zo", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was deiner Brust die Himmel anvertrauen;", "tokens": ["Was", "dei\u00b7ner", "Brust", "die", "Him\u00b7mel", "an\u00b7ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das heil'ge Dr\u00fcben zwar entweihen Worte,", "tokens": ["Das", "heil'\u00b7ge", "Dr\u00fc\u00b7ben", "zwar", "ent\u00b7wei\u00b7hen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Lie\u00df' auch die ew'ge Pforte", "tokens": ["Lie\u00df'", "auch", "die", "ew'\u00b7ge", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Noch wen zur\u00fcck, er schwiege: la\u00df nur schauen", "tokens": ["Noch", "wen", "zu\u00b7r\u00fcck", ",", "er", "schwie\u00b7ge", ":", "la\u00df", "nur", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "$.", "VVIMP", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mein Aug' in deinem, wenn ich bang erbleiche,", "tokens": ["Mein", "Aug'", "in", "dei\u00b7nem", ",", "wenn", "ich", "bang", "er\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "$,", "KOUS", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den Wiederschein der sel'gen Geisterreiche.", "tokens": ["Den", "Wie\u00b7der\u00b7schein", "der", "sel'\u00b7gen", "Geis\u00b7ter\u00b7rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ruft uns mit lebendigem Ger\u00e4usche", "tokens": ["Es", "ruft", "uns", "mit", "le\u00b7ben\u00b7di\u00b7gem", "Ge\u00b7r\u00e4u\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Tages Licht zu irdischen Gesch\u00e4ften,", "tokens": ["Des", "Ta\u00b7ges", "Licht", "zu", "ir\u00b7di\u00b7schen", "Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr leiblich Theil verleihend den Naturen.", "tokens": ["Ihr", "leib\u00b7lich", "Theil", "ver\u00b7lei\u00b7hend", "den", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne will auf sich den Blick nur heften,", "tokens": ["Die", "Son\u00b7ne", "will", "auf", "sich", "den", "Blick", "nur", "hef\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und duldet, da\u00df sie allgebietend t\u00e4usche,", "tokens": ["Und", "dul\u00b7det", ",", "da\u00df", "sie", "all\u00b7ge\u00b7bie\u00b7tend", "t\u00e4u\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein Jenseits an den himmlischen Azuren.", "tokens": ["Kein", "Jen\u00b7seits", "an", "den", "himm\u00b7li\u00b7schen", "A\u00b7zu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch wenn die stillen Fluren", "tokens": ["Doch", "wenn", "die", "stil\u00b7len", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Scheinbar die Nacht mit ihrer H\u00fcll' umdunkelt,", "tokens": ["Schein\u00b7bar", "die", "Nacht", "mit", "ih\u00b7rer", "H\u00fcll'", "um\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Dann \u00f6ffnet sich der R\u00e4um' und Zeiten Ferne;", "tokens": ["Dann", "\u00f6ff\u00b7net", "sich", "der", "R\u00e4um'", "und", "Zei\u00b7ten", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da winken so die Sterne,", "tokens": ["Da", "win\u00b7ken", "so", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df unserm Geist ein innres Licht entfunkelt.", "tokens": ["Da\u00df", "un\u00b7serm", "Geist", "ein", "inn\u00b7res", "Licht", "ent\u00b7fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bei Nacht ward die Unsterblichkeit ersonnen,", "tokens": ["Bei", "Nacht", "ward", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Denn sehend blind sind wir im Licht der Sonnen.", "tokens": ["Denn", "se\u00b7hend", "blind", "sind", "wir", "im", "Licht", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bei Nacht auch \u00fcberschreiten k\u00fchne Tr\u00e4ume", "tokens": ["Bei", "Nacht", "auch", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", "k\u00fch\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kluft, die von den Abgeschiednen trennet,", "tokens": ["Die", "Kluft", ",", "die", "von", "den", "Ab\u00b7ge\u00b7schied\u00b7nen", "tren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fchren sie herbei, mit uns zu kosen:", "tokens": ["Und", "f\u00fch\u00b7ren", "sie", "her\u00b7bei", ",", "mit", "uns", "zu", "ko\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir staunen nicht, wenn ihre Stimm' uns nennet,", "tokens": ["Wir", "stau\u00b7nen", "nicht", ",", "wenn", "ih\u00b7re", "Stimm'", "uns", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie ruhn mit uns im Schatten gr\u00fcner B\u00e4ume,", "tokens": ["Sie", "ruhn", "mit", "uns", "im", "Schat\u00b7ten", "gr\u00fc\u00b7ner", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Derweil sich ihre Gr\u00fcfte schon bemoosen.", "tokens": ["Der\u00b7weil", "sich", "ih\u00b7re", "Gr\u00fcf\u00b7te", "schon", "be\u00b7moo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach die erblichnen Rosen", "tokens": ["Ach", "die", "er\u00b7blich\u00b7nen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf dem jungfr\u00e4ulich zarten Angesichte,", "tokens": ["Auf", "dem", "jung\u00b7fr\u00e4u\u00b7lich", "zar\u00b7ten", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--++-+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Das selbst der Tod, gleich nach der That vers\u00f6hnet,", "tokens": ["Das", "selbst", "der", "Tod", ",", "gleich", "nach", "der", "That", "ver\u00b7s\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Entstellt nicht, nein, versch\u00f6net,", "tokens": ["Ent\u00b7stellt