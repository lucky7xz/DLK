{"textgrid.poem.25841": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[drau\u00dfen \u00fcber dem Wiesengr\u00fcn]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen \u00fcber dem Wiesengr\u00fcn", "tokens": ["Drau\u00b7\u00dfen", "\u00fc\u00b7ber", "dem", "Wie\u00b7sen\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Starrt das Schwarz zackiger W\u00e4lder.", "tokens": ["Starrt", "das", "Schwarz", "za\u00b7cki\u00b7ger", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Winden aufgescharrt,", "tokens": ["Von", "den", "Win\u00b7den", "auf\u00b7ge\u00b7scharrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fchen blaue Wetterwolken,", "tokens": ["Gl\u00fc\u00b7hen", "blau\u00b7e", "Wet\u00b7ter\u00b7wol\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Sonne f\u00e4llt so grinsend", "tokens": ["Und", "die", "Son\u00b7ne", "f\u00e4llt", "so", "grin\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den gelben Ahornbaum,", "tokens": ["Auf", "den", "gel\u00b7ben", "A\u00b7horn\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In die gelben Haferfelder.", "tokens": ["In", "die", "gel\u00b7ben", "Ha\u00b7fer\u00b7fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Sonne f\u00e4llt so grinsend", "tokens": ["Und", "die", "Son\u00b7ne", "f\u00e4llt", "so", "grin\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In mein fahles, wundes Hirn,", "tokens": ["In", "mein", "fah\u00b7les", ",", "wun\u00b7des", "Hirn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Horch, vom Walde rollt der Donner!", "tokens": ["Horch", ",", "vom", "Wal\u00b7de", "rollt", "der", "Don\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Es pocht kalt an meine Stirn.", "tokens": ["Es", "pocht", "kalt", "an", "mei\u00b7ne", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}