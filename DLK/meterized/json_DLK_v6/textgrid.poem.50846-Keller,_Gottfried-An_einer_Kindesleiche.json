{"textgrid.poem.50846": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An einer Kindesleiche", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hat geweht, der Wind, den niemand sieht", "tokens": ["Er", "hat", "ge\u00b7weht", ",", "der", "Wind", ",", "den", "nie\u00b7mand", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und niemand h\u00f6rt; er hat den Baum geschwungen,", "tokens": ["Und", "nie\u00b7mand", "h\u00f6rt", ";", "er", "hat", "den", "Baum", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Wurzelwerk die Erde \u00fcberzieht,", "tokens": ["Des", "Wur\u00b7zel\u00b7werk", "die", "Er\u00b7de", "\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dessen Krone ich dies Lied gesungen.", "tokens": ["In", "des\u00b7sen", "Kro\u00b7ne", "ich", "dies", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das j\u00fcngste Blatt, das gestern dran gebl\u00fcht,", "tokens": ["Das", "j\u00fcngs\u00b7te", "Blatt", ",", "das", "ge\u00b7stern", "dran", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hat \u00fcber Nacht sich leise losgerungen", "tokens": ["Hat", "\u00fc\u00b7ber", "Nacht", "sich", "lei\u00b7se", "los\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und fiel; und niemand gab wohl weiter acht", "tokens": ["Und", "fiel", ";", "und", "nie\u00b7mand", "gab", "wohl", "wei\u00b7ter", "acht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KON", "PIS", "VVFIN", "ADV", "ADV", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als ich, der da zun\u00e4chst dabei gewacht", "tokens": ["Als", "ich", ",", "der", "da", "zu\u00b7n\u00e4chst", "da\u00b7bei", "ge\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So bist erl\u00f6scht du, lieblich junges Licht,", "tokens": ["So", "bist", "er\u00b7l\u00f6scht", "du", ",", "lieb\u00b7lich", "jun\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das mir erquickend in das Herz gez\u00fcndet?", "tokens": ["Das", "mir", "er\u00b7qui\u00b7ckend", "in", "das", "Herz", "ge\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch sprach drei Worte deine Zunge nicht,", "tokens": ["Noch", "sprach", "drei", "Wor\u00b7te", "dei\u00b7ne", "Zun\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch hat dein Lallen mir soviel verk\u00fcndet!", "tokens": ["Doch", "hat", "dein", "Lal\u00b7len", "mir", "so\u00b7viel", "ver\u00b7k\u00fcn\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Sehnen, das die feinsten Bande flicht,", "tokens": ["Das", "Seh\u00b7nen", ",", "das", "die", "feins\u00b7ten", "Ban\u00b7de", "flicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es hat mich innig auch mit dir verb\u00fcndet.", "tokens": ["Es", "hat", "mich", "in\u00b7nig", "auch", "mit", "dir", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ja, vor viel Gro\u00dfem unter dieser Sonnen", "tokens": ["Ja", ",", "vor", "viel", "Gro\u00b7\u00dfem", "un\u00b7ter", "die\u00b7ser", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PIAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hab ich dich, Kleiner, wert und lieb gewonnen!", "tokens": ["Hab", "ich", "dich", ",", "Klei\u00b7ner", ",", "wert", "und", "lieb", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ob ich gen Himmel sah ins blaue Meer,", "tokens": ["Ob", "ich", "gen", "Him\u00b7mel", "sah", "ins", "blau\u00b7e", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob in dein Aug, es war das gleiche Schauen:", "tokens": ["Ob", "in", "dein", "Aug", ",", "es", "war", "das", "glei\u00b7che", "Schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es leuchtete aus diesen Sternen her", "tokens": ["Es", "leuch\u00b7te\u00b7te", "aus", "die\u00b7sen", "Ster\u00b7nen", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Urspr\u00fcnglich reines Licht von sch\u00f6nern Auen.", "tokens": ["Ur\u00b7spr\u00fcng\u00b7lich", "rei\u00b7nes", "Licht", "von", "sch\u00f6\u00b7nern", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie oft senkt ich den Blick, von M\u00fchsal schwer,", "tokens": ["Wie", "oft", "senkt", "ich", "den", "Blick", ",", "von", "M\u00fch\u00b7sal", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Erfrischend tief in dies verkl\u00e4rte Blauen!", "tokens": ["Er\u00b7fri\u00b7schend", "tief", "in", "dies", "ver\u00b7kl\u00e4r\u00b7te", "Blau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "APPR", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie war das Lachen deines Munds so fein!", "tokens": ["Wie", "war", "das", "La\u00b7chen", "dei\u00b7nes", "Munds", "so", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie heimlich unsre Freundschaft, still und rein!", "tokens": ["Wie", "heim\u00b7lich", "uns\u00b7re", "Freund\u00b7schaft", ",", "still", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nie hab an deine Zukunft ich gedacht,", "tokens": ["Nie", "hab", "an", "dei\u00b7ne", "Zu\u00b7kunft", "ich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Gegenwart war ja so sch\u00f6n und heiter!", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "war", "ja", "so", "sch\u00f6n", "und", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast wie eine Blume mir gelacht,", "tokens": ["Du", "hast", "wie", "ei\u00b7ne", "Blu\u00b7me", "mir", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und an die Sommerfrucht dacht ich nicht weiter;", "tokens": ["Und", "an", "die", "Som\u00b7mer\u00b7frucht", "dacht", "ich", "nicht", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob einst vielleicht ein Held in dir erwacht',", "tokens": ["Ob", "einst", "viel\u00b7leicht", "ein", "Held", "in", "dir", "er\u00b7wacht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie hoch du steigest auf der gro\u00dfen Leiter:", "tokens": ["Wie", "hoch", "du", "stei\u00b7gest", "auf", "der", "gro\u00b7\u00dfen", "Lei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du lieblich Kind warst in dir selbst vollkommen \u2013", "tokens": ["Du", "lieb\u00b7lich", "Kind", "warst", "in", "dir", "selbst", "voll\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was sollte dir und mir die Sorge frommen?", "tokens": ["Was", "soll\u00b7te", "dir", "und", "mir", "die", "Sor\u00b7ge", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zu der du wiederkehrst, gr\u00fc\u00df mir die Quelle,", "tokens": ["Zu", "der", "du", "wie\u00b7der\u00b7kehrst", ",", "gr\u00fc\u00df", "mir", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Born, doch besser: gr\u00fc\u00df das Meer,", "tokens": ["Des", "Le\u00b7bens", "Born", ",", "doch", "bes\u00b7ser", ":", "gr\u00fc\u00df", "das", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "ADJD", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Hoch flutet um die dunkle Klippe her,", "tokens": ["Hoch", "flu\u00b7tet", "um", "die", "dunk\u00b7le", "Klip\u00b7pe", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darauf er sitzt, der traurige Geselle,", "tokens": ["Da\u00b7rauf", "er", "sitzt", ",", "der", "trau\u00b7ri\u00b7ge", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Tod \u2013 verlassen, einsam, tr\u00e4nenschwer,", "tokens": ["Der", "Tod", "\u2013", "ver\u00b7las\u00b7sen", ",", "ein\u00b7sam", ",", "tr\u00e4\u00b7nen\u00b7schwer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "VVINF", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn ihm die frohen Seelen, kaum gefangen,", "tokens": ["Wenn", "ihm", "die", "fro\u00b7hen", "See\u00b7len", ",", "kaum", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit lautem Jubel wieder auf die See gegangen!", "tokens": ["Mit", "lau\u00b7tem", "Ju\u00b7bel", "wie\u00b7der", "auf", "die", "See", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}