{"dta.poem.7546": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschenrede werth ist nicht was Menschen thaten;", "tokens": ["Der", "Men\u00b7schen\u00b7re\u00b7de", "werth", "ist", "nicht", "was", "Men\u00b7schen", "tha\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "PTKNEG", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der Natur und Gott soll sich mein Geist berathen.", "tokens": ["Mit", "der", "Na\u00b7tur", "und", "Gott", "soll", "sich", "mein", "Geist", "be\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Weisheit Indiens hat vergessen der Geschichte,", "tokens": ["Die", "Weis\u00b7heit", "In\u00b7di\u00b7ens", "hat", "ver\u00b7ges\u00b7sen", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df sie allein von Gott, Natur und Geist berichte.", "tokens": ["Da\u00df", "sie", "al\u00b7lein", "von", "Gott", ",", "Na\u00b7tur", "und", "Geist", "be\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und so ihr Sch\u00fcler ich hab' auch, was ich besessen,", "tokens": ["Und", "so", "ihr", "Sch\u00fc\u00b7ler", "ich", "hab'", "auch", ",", "was", "ich", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PPER", "VAFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gethan und thun gesehn, mit Gott in Gott vergessen;", "tokens": ["Ge\u00b7than", "und", "thun", "ge\u00b7sehn", ",", "mit", "Gott", "in", "Gott", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVINF", "VVPP", "$,", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wei\u00df nur Eines noch, und wei\u00df dis Eine ganz:", "tokens": ["Und", "wei\u00df", "nur", "Ei\u00b7nes", "noch", ",", "und", "wei\u00df", "dis", "Ei\u00b7ne", "ganz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "ADV", "$,", "KON", "VVFIN", "PDS", "ART", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott ist die Geistersonn' und die Natur sein Glanz.", "tokens": ["Gott", "ist", "die", "Geis\u00b7ter\u00b7sonn'", "und", "die", "Na\u00b7tur", "sein", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}