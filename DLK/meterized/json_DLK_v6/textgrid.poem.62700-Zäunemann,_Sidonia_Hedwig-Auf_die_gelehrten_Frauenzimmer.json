{"textgrid.poem.62700": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die gelehrten Frauenzimmer", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr M\u00e4nner bildet euch nicht ein,", "tokens": ["Ihr", "M\u00e4n\u00b7ner", "bil\u00b7det", "euch", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob Vernunft, Verstand, Gelehrsamkeit und aufgekl\u00e4rter Sinn", "tokens": ["Als", "ob", "Ver\u00b7nunft", ",", "Ver\u00b7stand", ",", "Ge\u00b7lehr\u00b7sam\u00b7keit", "und", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Solt euer Eigenthum und Erbrecht seyn;", "tokens": ["Solt", "eu\u00b7er", "Ei\u00b7gen\u00b7thum", "und", "Er\u00b7brecht", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nein! warlich, der das Firmament gesetzt,", "tokens": ["Nein", "!", "war\u00b7lich", ",", "der", "das", "Fir\u00b7ma\u00b7ment", "ge\u00b7setzt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der hat das Frauen-Volk nichts minder hoch gesch\u00e4tzt:", "tokens": ["Der", "hat", "das", "Frau\u00b7en\u00b7Volk", "nichts", "min\u00b7der", "hoch", "ge\u00b7sch\u00e4tzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihnen auch Verstand und Witz verliehen.", "tokens": ["Und", "ih\u00b7nen", "auch", "Ver\u00b7stand", "und", "Witz", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es soll wie ihr, des hohen Geistes Gaben,", "tokens": ["Es", "soll", "wie", "ihr", ",", "des", "ho\u00b7hen", "Geis\u00b7tes", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "PPER", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auch im Besitze haben.", "tokens": ["Auch", "im", "Be\u00b7sit\u00b7ze", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drum mu\u00df ihr Lorber-Zweig, so wie der eure bl\u00fchen.", "tokens": ["Drum", "mu\u00df", "ihr", "Lor\u00b7ber\u00b7Zweig", ",", "so", "wie", "der", "eu\u00b7re", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "ART", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Z\u00f6rnet, tobet, l\u00e4stert, neidet immerhin,", "tokens": ["Z\u00f6r\u00b7net", ",", "to\u00b7bet", ",", "l\u00e4s\u00b7tert", ",", "nei\u00b7det", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Ihr werdt es doch nicht hindern k\u00f6nnen,", "tokens": ["Ihr", "werdt", "es", "doch", "nicht", "hin\u00b7dern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ihr solt und m\u00fc\u00dft denselben doch die Ehre g\u00f6nnen!", "tokens": ["Ihr", "solt", "und", "m\u00fc\u00dft", "den\u00b7sel\u00b7ben", "doch", "die", "Eh\u00b7re", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PDS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum bildet euch ihr M\u00e4nner ja nichts ein!", "tokens": ["Drum", "bil\u00b7det", "euch", "ihr", "M\u00e4n\u00b7ner", "ja", "nichts", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}