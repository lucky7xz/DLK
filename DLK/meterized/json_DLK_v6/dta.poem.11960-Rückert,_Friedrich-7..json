{"dta.poem.11960": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Sprecher sind sein Beutel und sein Becher;", "tokens": ["Des", "Men\u00b7schen", "Spre\u00b7cher", "sind", "sein", "Beu\u00b7tel", "und", "sein", "Be\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der spricht: mild oder karg, der: n\u00fcchtern oder Zecher.", "tokens": ["Der", "spricht", ":", "mild", "o\u00b7der", "karg", ",", "der", ":", "n\u00fcch\u00b7tern", "o\u00b7der", "Ze\u00b7cher", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "ADJD", "KON", "ADJD", "$,", "PRELS", "$.", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der kleine Becher zeugt von gro\u00dfer M\u00e4\u00dfigkeit,", "tokens": ["Der", "klei\u00b7ne", "Be\u00b7cher", "zeugt", "von", "gro\u00b7\u00dfer", "M\u00e4\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der enge Beutel sagt: das Sparen geht zu weit.", "tokens": ["Der", "en\u00b7ge", "Beu\u00b7tel", "sagt", ":", "das", "Spa\u00b7ren", "geht", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA"