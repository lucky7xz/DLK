{"textgrid.poem.60644": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Weggenossen hatte einst der Fuchs den Bock.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Weggenossen hatte einst der Fuchs den Bock.", "tokens": ["Als", "Weg\u00b7ge\u00b7nos\u00b7sen", "hat\u00b7te", "einst", "der", "Fuchs", "den", "Bock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ADV", "ART", "NE", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der trug das sch\u00f6nste Bartgelock", "tokens": ["Der", "trug", "das", "sch\u00f6ns\u00b7te", "Bart\u00b7ge\u00b7lock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein gewaltiges H\u00f6rnerpaar,", "tokens": ["Und", "ein", "ge\u00b7wal\u00b7ti\u00b7ges", "H\u00f6r\u00b7ner\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenngleich er sonst kein Wunderexemplar", "tokens": ["Wenn\u00b7gleich", "er", "sonst", "kein", "Wun\u00b7der\u00b7ex\u00b7emp\u00b7lar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von einem starken Kopfe war.", "tokens": ["Von", "ei\u00b7nem", "star\u00b7ken", "Kop\u00b7fe", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem andern aber war ein Kopf voll T\u00fccke eigen.", "tokens": ["Dem", "an\u00b7dern", "a\u00b7ber", "war", "ein", "Kopf", "voll", "T\u00fc\u00b7cke", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "ART", "NN", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nach langer Wandrung zwang der Durst die zwei,", "tokens": ["Nach", "lan\u00b7ger", "Wand\u00b7rung", "zwang", "der", "Durst", "die", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "CARD", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "In einen Brunnenschacht hinabzusteigen;", "tokens": ["In", "ei\u00b7nen", "Brun\u00b7nen\u00b7schacht", "hin\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn Wasser war die beste Arzenei.", "tokens": ["Denn", "Was\u00b7ser", "war", "die", "bes\u00b7te", "Ar\u00b7ze\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und als sie reichlich davon eingenommen,", "tokens": ["Und", "als", "sie", "reich\u00b7lich", "da\u00b7von", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sprach Meister Fuchs: \u00bbDer Trunk hat gut gelabt.", "tokens": ["Sprach", "Meis\u00b7ter", "Fuchs", ":", "\u00bb", "Der", "Trunk", "hat", "gut", "ge\u00b7labt", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nun aber, Vetter Bock, nun sagt mir, habt", "tokens": ["Nun", "a\u00b7ber", ",", "Vet\u00b7ter", "Bock", ",", "nun", "sagt", "mir", ",", "habt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "NE", "NE", "$,", "ADV", "VVFIN", "PPER", "$,", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ihr einen Rat, wie wieder hier hinauszukommen?", "tokens": ["Ihr", "ei\u00b7nen", "Rat", ",", "wie", "wie\u00b7der", "hier", "hin\u00b7aus\u00b7zu\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PWAV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch h\u00f6rt, ich wei\u00df: Ihr hebt Euch auf die Hinterf\u00fc\u00dfe", "tokens": ["Doch", "h\u00f6rt", ",", "ich", "wei\u00df", ":", "Ihr", "hebt", "Euch", "auf", "die", "Hin\u00b7ter\u00b7f\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und reckt die H\u00f6rner hoch und stemmt sie an die Wand,", "tokens": ["Und", "reckt", "die", "H\u00f6r\u00b7ner", "hoch", "und", "stemmt", "sie", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich klettre dran hinauf und gr\u00fc\u00dfe", "tokens": ["Ich", "klett\u00b7re", "dran", "hin\u00b7auf", "und", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zuerst die Oberwelt und ziehe auf den Rand", "tokens": ["Zu\u00b7erst", "die", "O\u00b7ber\u00b7welt", "und", "zie\u00b7he", "auf", "den", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Brunnens Euch dann nach.\u00ab", "tokens": ["Des", "Brun\u00b7nens", "Euch", "dann", "nach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Der Bock, beistimmend, sprach:", "tokens": ["Der", "Bock", ",", "bei\u00b7stim\u00b7mend", ",", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "\u00bbbei meinem Bart, das nenne ich Verstand!", "tokens": ["\u00bb", "bei", "mei\u00b7nem", "Bart", ",", "das", "nen\u00b7ne", "ich", "Ver\u00b7stand", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "So kluge Leute sch\u00e4tz ich sehr.", "tokens": ["So", "klu\u00b7ge", "Leu\u00b7te", "sch\u00e4tz", "ich", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich selber h\u00e4tte nimmermehr", "tokens": ["Ich", "sel\u00b7ber", "h\u00e4t\u00b7te", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Den Ausweg hier heraus gefunden.\u00ab", "tokens": ["Den", "Aus\u00b7weg", "hier", "he\u00b7raus", "ge\u00b7fun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "APZR", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nun schafft der Fuchs sich hoch und l\u00e4\u00dft den Bock da unten", "tokens": ["Nun", "schafft", "der", "Fuchs", "sich", "hoch", "und", "l\u00e4\u00dft", "den", "Bock", "da", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "PRF", "ADJD", "KON", "VVFIN", "ART", "NE", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und mahnt ihn tr\u00f6stend zur Geduld.", "tokens": ["Und", "mahnt", "ihn", "tr\u00f6s\u00b7tend", "zur", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "\u00bbh\u00e4tt Euch\u00ab, sagt er, \u00bbdes Himmels Huld", "tokens": ["\u00bb", "h\u00e4tt", "Euch", "\u00ab", ",", "sagt", "er", ",", "\u00bb", "des", "Him\u00b7mels", "Huld"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Den Geist so gro\u00df wie Bart und Horn gemacht,", "tokens": ["Den", "Geist", "so", "gro\u00df", "wie", "Bart", "und", "Horn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "So h\u00e4ttet Ihr Euch wohl bedacht,", "tokens": ["So", "h\u00e4t\u00b7tet", "Ihr", "Euch", "wohl", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Bevor Ihr da hinabgesprungen.", "tokens": ["Be\u00b7vor", "Ihr", "da", "hin\u00b7ab\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Nun sorgt auch Ihr, wie Ihr herauskommt aus dem Schacht.", "tokens": ["Nun", "sorgt", "auch", "Ihr", ",", "wie", "Ihr", "her\u00b7aus\u00b7kommt", "aus", "dem", "Schacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Mir eilt's, ich hab Gesch\u00e4fte, lebet wohl!\u00ab Er ging.", "tokens": ["Mir", "eilt's", ",", "ich", "hab", "Ge\u00b7sch\u00e4f\u00b7te", ",", "le\u00b7bet", "wohl", "!", "\u00ab", "Er", "ging", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,", "VVFIN", "ADV", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bedenket wohl das Ende stets bei jedem Ding!", "tokens": ["Be\u00b7den\u00b7ket", "wohl", "das", "En\u00b7de", "stets", "bei", "je\u00b7dem", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}