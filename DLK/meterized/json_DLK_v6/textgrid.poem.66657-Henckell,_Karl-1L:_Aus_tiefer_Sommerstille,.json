{"textgrid.poem.66657": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus tiefer Sommerstille,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus tiefer Sommerstille,", "tokens": ["Aus", "tie\u00b7fer", "Som\u00b7mer\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Rosenduft umhaucht,", "tokens": ["Von", "Ro\u00b7sen\u00b7duft", "um\u00b7haucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun alles Rohe, Schrille", "tokens": ["Nun", "al\u00b7les", "Ro\u00b7he", ",", "Schril\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In scheuer Ferne untertaucht ...", "tokens": ["In", "scheu\u00b7er", "Fer\u00b7ne", "un\u00b7ter\u00b7taucht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Biet' ich mit frommem Munde", "tokens": ["Biet'", "ich", "mit", "from\u00b7mem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Frieden leisen Gru\u00df,", "tokens": ["Dem", "Frie\u00b7den", "lei\u00b7sen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Streue Bl\u00fcten in die Runde,", "tokens": ["Streu\u00b7e", "Bl\u00fc\u00b7ten", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo immer segnend weilt sein Fu\u00df.", "tokens": ["Wo", "im\u00b7mer", "seg\u00b7nend", "weilt", "sein", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihm zum Willkommen trinke", "tokens": ["Ihm", "zum", "Will\u00b7kom\u00b7men", "trin\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Mildesten ich zu ...", "tokens": ["Vom", "Mil\u00b7des\u00b7ten", "ich", "zu", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich winke ihm, ich winke", "tokens": ["Ich", "win\u00b7ke", "ihm", ",", "ich", "win\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schaffensheiligen Daseinsruh.", "tokens": ["Der", "schaf\u00b7fens\u00b7hei\u00b7li\u00b7gen", "Da\u00b7seins\u00b7ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie aus uralten Tr\u00e4umen", "tokens": ["Wie", "aus", "ur\u00b7al\u00b7ten", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kampfecho klirrt ans Ohr \u2013", "tokens": ["Kam\u00b7pfe\u00b7cho", "klirrt", "ans", "Ohr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kriegesrosse seh' ich sch\u00e4umen", "tokens": ["Krie\u00b7ges\u00b7ros\u00b7se", "seh'", "ich", "sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wild sich b\u00e4umen hoch empor ...", "tokens": ["Und", "wild", "sich", "b\u00e4u\u00b7men", "hoch", "em\u00b7por", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir sind der Wildheit m\u00fcde,", "tokens": ["Wir", "sind", "der", "Wild\u00b7heit", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Hasses weh und wund,", "tokens": ["Des", "Has\u00b7ses", "weh", "und", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein heiliger Menschenfriede", "tokens": ["Ein", "hei\u00b7li\u00b7ger", "Men\u00b7schen\u00b7frie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Quillt uns aus tiefstem Herzensgrund.", "tokens": ["Quillt", "uns", "aus", "tiefs\u00b7tem", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich mu\u00df ihn leise gr\u00fc\u00dfen", "tokens": ["Ich", "mu\u00df", "ihn", "lei\u00b7se", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit frommem Dichtermund,", "tokens": ["Mit", "from\u00b7mem", "Dich\u00b7ter\u00b7mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Streue Bl\u00fcten ihm zu F\u00fc\u00dfen", "tokens": ["Streu\u00b7e", "Bl\u00fc\u00b7ten", "ihm", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings auf dem weiten Erdenrund.", "tokens": ["Rings", "auf", "dem", "wei\u00b7ten", "Er\u00b7den\u00b7rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus reifer Sommerstille,", "tokens": ["Aus", "rei\u00b7fer", "Som\u00b7mer\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Rosenduft umhaucht,", "tokens": ["Von", "Ro\u00b7sen\u00b7duft", "um\u00b7haucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun alles Rohe, Schrille", "tokens": ["Nun", "al\u00b7les", "Ro\u00b7he", ",", "Schril\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In scheuer Ferne untertaucht.", "tokens": ["In", "scheu\u00b7er", "Fer\u00b7ne", "un\u00b7ter\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}