{"textgrid.poem.23976": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Lichtglaube", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern die Welt in Grau,", "tokens": ["Ge\u00b7stern", "die", "Welt", "in", "Grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Rieselnder Regen troff,", "tokens": ["Rie\u00b7seln\u00b7der", "Re\u00b7gen", "troff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Himmel und Erde ersoff;", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "er\u00b7soff", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Heute der Himmel blau.", "tokens": ["Heu\u00b7te", "der", "Him\u00b7mel", "blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sonnenschein goldg\u00fcssig tr\u00e4uft,", "tokens": ["Son\u00b7nen\u00b7schein", "gold\u00b7g\u00fcs\u00b7sig", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber die Halme l\u00e4uft", "tokens": ["Ue\u00b7ber", "die", "Hal\u00b7me", "l\u00e4uft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wogewind lau.", "tokens": ["Wo\u00b7ge\u00b7wind", "lau", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Z\u00fcrnegotts Reich zerf\u00e4llt!", "tokens": ["Z\u00fcr\u00b7ne\u00b7gotts", "Reich", "zer\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Heiteres Heidentum", "tokens": ["Hei\u00b7te\u00b7res", "Hei\u00b7den\u00b7tum"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Leuchtet das Leidentum", "tokens": ["Leuch\u00b7tet", "das", "Lei\u00b7den\u00b7tum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Froh aus der fr\u00f6hlichen Welt.", "tokens": ["Froh", "aus", "der", "fr\u00f6h\u00b7li\u00b7chen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.12": {"text": "Siegendes Licht zerri\u00df", "tokens": ["Sie\u00b7gen\u00b7des", "Licht", "zer\u00b7ri\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Hockende Finsternis.", "tokens": ["Ho\u00b7cken\u00b7de", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "Alles erhellt!", "tokens": ["Al\u00b7les", "er\u00b7hellt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}