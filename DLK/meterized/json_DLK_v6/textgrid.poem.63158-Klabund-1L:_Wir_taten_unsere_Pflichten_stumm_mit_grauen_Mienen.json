{"textgrid.poem.63158": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir taten unsere Pflichten stumm mit grauen Mienen", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir taten unsere Pflichten stumm mit grauen Mienen", "tokens": ["Wir", "ta\u00b7ten", "un\u00b7se\u00b7re", "Pflich\u00b7ten", "stumm", "mit", "grau\u00b7en", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und pfl\u00fcgten schweigend unser Feld.", "tokens": ["Und", "pfl\u00fcg\u00b7ten", "schwei\u00b7gend", "un\u00b7ser", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun schweifen wir wie Beduinen", "tokens": ["Nun", "schwei\u00b7fen", "wir", "wie", "Be\u00b7du\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach durch die W\u00fcste dieser Welt.", "tokens": ["Ach", "durch", "die", "W\u00fcs\u00b7te", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Uns d\u00f6rrte die verdorrte Sonne Flandern,", "tokens": ["Uns", "d\u00f6rr\u00b7te", "die", "ver\u00b7dorr\u00b7te", "Son\u00b7ne", "Flan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Polensumpf war uns nicht fremd.", "tokens": ["Der", "Po\u00b7len\u00b7sumpf", "war", "uns", "nicht", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man hiess uns nach dem Goldnen Horne wandern,", "tokens": ["Man", "hiess", "uns", "nach", "dem", "Gold\u00b7nen", "Hor\u00b7ne", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir wuschen in der Drina unser Hemd.", "tokens": ["Wir", "wu\u00b7schen", "in", "der", "Dri\u00b7na", "un\u00b7ser", "Hemd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wenn des Fr\u00fchlings heilige Mythe", "tokens": ["Doch", "wenn", "des", "Fr\u00fch\u00b7lings", "hei\u00b7li\u00b7ge", "My\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Schnee um unsere Herzen schmilzt,", "tokens": ["Den", "Schnee", "um", "un\u00b7se\u00b7re", "Her\u00b7zen", "schmilzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Steht eine Kiefer aus der Mark in Bl\u00fcte", "tokens": ["Steht", "ei\u00b7ne", "Kie\u00b7fer", "aus", "der", "Mark", "in", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu unsern H\u00e4upten, dunkel und verfilzt.", "tokens": ["Zu", "un\u00b7sern", "H\u00e4up\u00b7ten", ",", "dun\u00b7kel", "und", "ver\u00b7filzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Deutschland unser, das du bist im Himmel!", "tokens": ["O", "Deutschland", "un\u00b7ser", ",", "das", "du", "bist", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "$,", "PRELS", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wir f\u00fchlen tausendfach dein Weh.", "tokens": ["Wir", "f\u00fch\u00b7len", "tau\u00b7send\u00b7fach", "dein", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner S\u00f6hne grauestes Gewimmel", "tokens": ["Und", "dei\u00b7ner", "S\u00f6h\u00b7ne", "grau\u00b7es\u00b7tes", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Stein zu deiner ewigen Statue.", "tokens": ["Ist", "Stein", "zu", "dei\u00b7ner", "e\u00b7wi\u00b7gen", "Sta\u00b7tue", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}