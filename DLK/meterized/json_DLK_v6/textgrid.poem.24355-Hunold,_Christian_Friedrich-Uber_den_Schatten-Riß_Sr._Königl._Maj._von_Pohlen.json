{"textgrid.poem.24355": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber den Schatten-Ri\u00df Sr. K\u00f6nigl. Maj. von Pohlen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur geht \u00fcber Kunst/ wenn sie vollkommen ist.", "tokens": ["Na\u00b7tur", "geht", "\u00fc\u00b7ber", "Kunst", "/", "wenn", "sie", "voll\u00b7kom\u00b7men", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du an Majest\u00e4t nun unvergleichlich bist:", "tokens": ["Weil", "du", "an", "Ma\u00b7jes\u00b7t\u00e4t", "nun", "un\u00b7ver\u00b7gleich\u00b7lich", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So will die Kunst/ o Held/ die sich vor dir mu\u00df sch\u00e4men/", "tokens": ["So", "will", "die", "Kunst", "/", "o", "Held", "/", "die", "sich", "vor", "dir", "mu\u00df", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$(", "FM", "NN", "$(", "PRELS", "PRF", "APPR", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hie blo\u00df ein Schatten-Werck zu deiner Sch\u00f6nheit nehmen/", "tokens": ["Hie", "blo\u00df", "ein", "Schat\u00b7ten\u00b7\u00b7Werck", "zu", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Damit doch die Natur/ wenn sie von Ehr entz\u00fcckt/", "tokens": ["Da\u00b7mit", "doch", "die", "Na\u00b7tur", "/", "wenn", "sie", "von", "Ehr", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf dich ihr Meisterst\u00fcck und gr\u00f6stes Wunder blickt/", "tokens": ["Auf", "dich", "ihr", "Meis\u00b7ter\u00b7st\u00fcck", "und", "gr\u00f6s\u00b7tes", "Wun\u00b7der", "blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und diesen Ri\u00df beschaut/ die Kunst vern\u00fcnfftig schatzet/", "tokens": ["Und", "die\u00b7sen", "Ri\u00df", "be\u00b7schaut", "/", "die", "Kunst", "ver\u00b7n\u00fcnff\u00b7tig", "schat\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVPP", "$(", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie von deinem Glantz den Schatten nur gesetzet.", "tokens": ["Weil", "sie", "von", "dei\u00b7nem", "Glantz", "den", "Schat\u00b7ten", "nur", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}