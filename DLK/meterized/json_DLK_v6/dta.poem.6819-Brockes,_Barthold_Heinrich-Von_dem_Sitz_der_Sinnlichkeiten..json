{"dta.poem.6819": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag des Donners Knall erklingen,", "tokens": ["Es", "mag", "des", "Don\u00b7ners", "Knall", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns mag ein scharffer Stahl durchdringen,", "tokens": ["Uns", "mag", "ein", "scharf\u00b7fer", "Stahl", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es mag uns Marmor, Eis, wie oder Feur ber\u00fchren;", "tokens": ["Es", "mag", "uns", "Mar\u00b7mor", ",", "Eis", ",", "wie", "o\u00b7der", "Feur", "be\u00b7r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "$,", "NN", "$,", "PWAV", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Geist allein kan es bemercken und versp\u00fcren,", "tokens": ["Der", "Geist", "al\u00b7lein", "kan", "es", "be\u00b7mer\u00b7cken", "und", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und nennt es K\u00e4lte, Hitz, W\u00e4rm, H\u00e4rtigkeit und Pein,", "tokens": ["Und", "nennt", "es", "K\u00e4l\u00b7te", ",", "Hitz", ",", "W\u00e4rm", ",", "H\u00e4r\u00b7tig\u00b7keit", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem der Druck in ihn von Werckzeug ist gesencket:", "tokens": ["Nach\u00b7dem", "der", "Druck", "in", "ihn", "von", "Wer\u00b7ck\u00b7zeug", "ist", "ge\u00b7sen\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Der Geist, zu welchem sich ein jeder Vorwurff lencket,", "tokens": ["Der", "Geist", ",", "zu", "wel\u00b7chem", "sich", "ein", "je\u00b7der", "Vor\u00b7wurff", "len\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fcgt Nahmen zum Begriff, er \u00fcberleget, dencket,", "tokens": ["F\u00fcgt", "Nah\u00b7men", "zum", "Be\u00b7griff", ",", "er", "\u00fc\u00b7ber\u00b7le\u00b7get", ",", "den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und durch Bewegungen ger\u00fchrt, macht er den Schlu\u00df,", "tokens": ["Und", "durch", "Be\u00b7we\u00b7gun\u00b7gen", "ge\u00b7r\u00fchrt", ",", "macht", "er", "den", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Wem man zu folgen hat, und was man meiden mu\u00df.", "tokens": ["Wem", "man", "zu", "fol\u00b7gen", "hat", ",", "und", "was", "man", "mei\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKZU", "VVINF", "VAFIN", "$,", "KON", "PWS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}