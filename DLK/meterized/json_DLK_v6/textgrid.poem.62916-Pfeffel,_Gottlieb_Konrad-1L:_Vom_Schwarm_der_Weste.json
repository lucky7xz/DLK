{"textgrid.poem.62916": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Schwarm der Weste", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Schwarm der Weste", "tokens": ["Vom", "Schwarm", "der", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Verbuhlt, umweht,", "tokens": ["Ver\u00b7buhlt", ",", "um\u00b7weht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bego\u00df Alceste", "tokens": ["Be\u00b7go\u00df", "Al\u00b7ces\u00b7te"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihr Blumenbeet.", "tokens": ["Ihr", "Blu\u00b7men\u00b7beet", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie sah schon lange", "tokens": ["Sie", "sah", "schon", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Nelkchen bl\u00fchn,", "tokens": ["Ein", "Nelk\u00b7chen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gleich ihrer Wange,", "tokens": ["Gleich", "ih\u00b7rer", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wei\u00df und karmin.", "tokens": ["Wei\u00df", "und", "kar\u00b7min", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Sie wollt es pfl\u00fccken,", "tokens": ["Sie", "wollt", "es", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Um ihre Brust", "tokens": ["Um", "ih\u00b7re", "Brust"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Damit zu schm\u00fccken,", "tokens": ["Da\u00b7mit", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Thron der Lust.", "tokens": ["Den", "Thron", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "La\u00df, rief es bange,", "tokens": ["La\u00df", ",", "rief", "es", "ban\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mich heut noch stehn,", "tokens": ["Mich", "heut", "noch", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis morgen prange", "tokens": ["Bis", "mor\u00b7gen", "pran\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich noch so sch\u00f6n.", "tokens": ["Ich", "noch", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbgut, ich kann borgen,", "tokens": ["\u00bb", "gut", ",", "ich", "kann", "bor\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du hast noch Frist,", "tokens": ["Du", "hast", "noch", "Frist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis da\u00df den Morgen", "tokens": ["Bis", "da\u00df", "den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bardale gr\u00fc\u00dft.\u00ab", "tokens": ["Bar\u00b7da\u00b7le", "gr\u00fc\u00dft", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Er kam. Es flehet,", "tokens": ["Er", "kam", ".", "Es", "fle\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Es klagt und ruft:", "tokens": ["Es", "klagt", "und", "ruft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Am Abend wehet", "tokens": ["Am", "A\u00b7bend", "we\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mein reinster Duft.", "tokens": ["Mein", "reins\u00b7ter", "Duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Sie gab, voll Milde,", "tokens": ["Sie", "gab", ",", "voll", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Es wieder los,", "tokens": ["Es", "wie\u00b7der", "los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis aufs Gefilde", "tokens": ["Bis", "aufs", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Sp\u00e4tthau flo\u00df.", "tokens": ["Der", "Sp\u00e4t\u00b7thau", "flo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Da fand sie \u2013 G\u00f6tter!", "tokens": ["Da", "fand", "sie", "\u2013", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nichts \u2013 ein Gew\u00fchl", "tokens": ["Nichts", "\u2013", "ein", "Ge\u00b7w\u00fchl"], "token_info": ["word", "punct", "word", "word"], "pos": ["PIS", "$(", "ART", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Verdorrter Bl\u00e4tter", "tokens": ["Ver\u00b7dorr\u00b7ter", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Am lahmen Stiel.", "tokens": ["Am", "lah\u00b7men", "Stiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Sie starrt und dr\u00fccket", "tokens": ["Sie", "starrt", "und", "dr\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Augen zu:", "tokens": ["Die", "Au\u00b7gen", "zu", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbach, ungepfl\u00fccket", "tokens": ["\u00bb", "ach", ",", "un\u00b7ge\u00b7pfl\u00fc\u00b7cket"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "ITJ", "$,", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Verwelkest du!\u00ab", "tokens": ["Ver\u00b7wel\u00b7kest", "du", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ja, seufzt es, gestern", "tokens": ["Ja", ",", "seufzt", "es", ",", "ge\u00b7stern"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "ADJA"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Noch frisch, heut kahl!", "tokens": ["Noch", "frisch", ",", "heut", "kahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Merkt, spr\u00f6de Schwestern,", "tokens": ["Merkt", ",", "spr\u00f6\u00b7de", "Schwes\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Euch die Moral.", "tokens": ["Euch", "die", "Mo\u00b7ral", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}