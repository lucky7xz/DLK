{"dta.poem.1134": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "20. Die Sch\u00f6ne Falschz\u00fcngigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ieder schaut dich an/ ein ieder sch\u00e4zt dich werth/ ", "tokens": ["Ein", "ie\u00b7der", "schaut", "dich", "an", "/", "ein", "ie\u00b7der", "sch\u00e4zt", "dich", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "PIS", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch keiner h\u00f6rt dir zu und keiner gl\u00e4ubet dir.", "tokens": ["Doch", "kei\u00b7ner", "h\u00f6rt", "dir", "zu", "und", "kei\u00b7ner", "gl\u00e4u\u00b7bet", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die s\u00fcsse Zauberey der angebohrnen Zier", "tokens": ["Die", "s\u00fcs\u00b7se", "Zau\u00b7be\u00b7rey", "der", "an\u00b7ge\u00b7bohr\u00b7nen", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00e4lt nur das Auge/ nicht zugleich das Ohr beschwert.", "tokens": ["H\u00e4lt", "nur", "das", "Au\u00b7ge", "/", "nicht", "zu\u00b7gleich", "das", "Ohr", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat dich Betrug und List von Jugend auff gen\u00e4hrt/", "tokens": ["Hat", "dich", "Be\u00b7trug", "und", "List", "von", "Ju\u00b7gend", "auff", "ge\u00b7n\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "APPR", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wei\u00df man nicht/ wenn Ha\u00df/ wenn Liebe bricht herf\u00fcr/", "tokens": ["So", "wei\u00df", "man", "nicht", "/", "wenn", "Ha\u00df", "/", "wenn", "Lie\u00b7be", "bricht", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$(", "KOUS", "NN", "$(", "KOUS", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich suche deine Gunst/ dein Eyfer weist sich mir.", "tokens": ["Ich", "su\u00b7che", "dei\u00b7ne", "Gunst", "/", "dein", "Ey\u00b7fer", "weist", "sich", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "VVFIN", "PRF", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jedoch werd ich dadurch von dir nicht abgekehrt.", "tokens": ["Je\u00b7doch", "werd", "ich", "da\u00b7durch", "von", "dir", "nicht", "ab\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "APPR", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mir ist so angenehm das sonst verha\u00dfte Nein", "tokens": ["Mir", "ist", "so", "an\u00b7ge\u00b7nehm", "das", "sonst", "ver\u00b7ha\u00df\u00b7te", "Nein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Des Mundes/ welcher nie dem Hertzen stimmet ein/", "tokens": ["Des", "Mun\u00b7des", "/", "wel\u00b7cher", "nie", "dem", "Hert\u00b7zen", "stim\u00b7met", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich kein lachend Ja begehre zu erlangen.", "tokens": ["Da\u00df", "ich", "kein", "la\u00b7chend", "Ja", "be\u00b7geh\u00b7re", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich kenne deinen Sinn/ und sch\u00e4tze l\u00e4ngst gewi\u00df/", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7nen", "Sinn", "/", "und", "sch\u00e4t\u00b7ze", "l\u00e4ngst", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df dein Verbot Gebot/ dein Wehrn Gewehren hi\u00df:", "tokens": ["Da\u00df", "dein", "Ver\u00b7bot", "Ge\u00b7bot", "/", "dein", "Wehrn", "Ge\u00b7weh\u00b7ren", "hi\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$(", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sagst du mir aber Ja/ was hab ich zu empfangen?", "tokens": ["Sagst", "du", "mir", "a\u00b7ber", "Ja", "/", "was", "hab", "ich", "zu", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKANT", "$(", "PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}