{"textgrid.poem.24424": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als Er Ader lie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So fliesse denn verderbtes Blut/", "tokens": ["So", "flies\u00b7se", "denn", "ver\u00b7derb\u00b7tes", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mehr was s\u00fcndliches/ als tugendhafftes thut.", "tokens": ["Das", "mehr", "was", "s\u00fcnd\u00b7li\u00b7ches", "/", "als", "tu\u00b7gend\u00b7haff\u00b7tes", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PWS", "ADJA", "$(", "KOUS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach Himmel la\u00df mich doch statt dessen neues sehn/", "tokens": ["Ach", "Him\u00b7mel", "la\u00df", "mich", "doch", "statt", "des\u00b7sen", "neu\u00b7es", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "PPER", "ADV", "APPR", "PRELAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit dem die Tugend kan stets durch mein Hertze gehn.", "tokens": ["Mit", "dem", "die", "Tu\u00b7gend", "kan", "stets", "durch", "mein", "Hert\u00b7ze", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}