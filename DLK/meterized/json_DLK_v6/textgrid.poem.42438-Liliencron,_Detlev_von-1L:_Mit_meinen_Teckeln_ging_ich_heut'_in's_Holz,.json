{"textgrid.poem.42438": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit meinen Teckeln ging ich heut' in's Holz,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit meinen Teckeln ging ich heut' in's Holz,", "tokens": ["Mit", "mei\u00b7nen", "Te\u00b7ckeln", "ging", "ich", "heut'", "in's", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Strick sie f\u00fchrend, da\u00df die hitzigen Kleinen", "tokens": ["Am", "Strick", "sie", "f\u00fch\u00b7rend", ",", "da\u00df", "die", "hit\u00b7zi\u00b7gen", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht kl\u00e4ffend mir vertrautes Wild verscheuchten.", "tokens": ["Nicht", "kl\u00e4f\u00b7fend", "mir", "ver\u00b7trau\u00b7tes", "Wild", "ver\u00b7scheuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Morgen gl\u00e4nzt wie ein Paradefeld.", "tokens": ["Der", "Mor\u00b7gen", "gl\u00e4nzt", "wie", "ein", "Pa\u00b7ra\u00b7de\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Tau perlt Blume noch und Blatt und Gras,", "tokens": ["Im", "Tau", "perlt", "Blu\u00b7me", "noch", "und", "Blatt", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "ADV", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur trocken da, wo sich die Sommersonne", "tokens": ["Nur", "tro\u00b7cken", "da", ",", "wo", "sich", "die", "Som\u00b7mer\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit hei\u00dfen Lippen schon den Trunk geholt.", "tokens": ["Mit", "hei\u00b7\u00dfen", "Lip\u00b7pen", "schon", "den", "Trunk", "ge\u00b7holt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Walde schwieg es heilig \u00fcberall.", "tokens": ["Im", "Wal\u00b7de", "schwieg", "es", "hei\u00b7lig", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als vom gewohnten Weg ich abw\u00e4rts bog,", "tokens": ["Als", "vom", "ge\u00b7wohn\u00b7ten", "Weg", "ich", "ab\u00b7w\u00e4rts", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um eine Wiesenbl\u00f6\u00dfe aufzusuchen,", "tokens": ["Um", "ei\u00b7ne", "Wie\u00b7sen\u00b7bl\u00f6\u00b7\u00dfe", "auf\u00b7zu\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entdeckt' ich dort, von Himmelslicht umleuchtet,", "tokens": ["Ent\u00b7deckt'", "ich", "dort", ",", "von", "Him\u00b7mels\u00b7licht", "um\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist's Gaukelspiel, kann ich den Augen trauen,", "tokens": ["Ist's", "Gau\u00b7kel\u00b7spiel", ",", "kann", "ich", "den", "Au\u00b7gen", "trau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sanft eingeschlafen, mit dem Haupt im Schatten,", "tokens": ["Sanft", "ein\u00b7ge\u00b7schla\u00b7fen", ",", "mit", "dem", "Haupt", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den kleinen Gott, der so viel Unheil stiftet.", "tokens": ["Den", "klei\u00b7nen", "Gott", ",", "der", "so", "viel", "Un\u00b7heil", "stif\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wie das Kind, das seine Weihnachtspuppe", "tokens": ["Und", "wie", "das", "Kind", ",", "das", "sei\u00b7ne", "Weih\u00b7nachts\u00b7pup\u00b7pe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "In's Bettchen nahm, gl\u00fcckselig dann entschlief,", "tokens": ["In's", "Bett\u00b7chen", "nahm", ",", "gl\u00fcck\u00b7se\u00b7lig", "dann", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So hielt er fest mit seinen kecken F\u00e4ustchen", "tokens": ["So", "hielt", "er", "fest", "mit", "sei\u00b7nen", "ke\u00b7cken", "F\u00e4ust\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "An seine Brust geschlossen Pfeil und Bogen.", "tokens": ["An", "sei\u00b7ne", "Brust", "ge\u00b7schlos\u00b7sen", "Pfeil", "und", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er wandte mir den rosigen R\u00fccken zu,", "tokens": ["Er", "wand\u00b7te", "mir", "den", "ro\u00b7si\u00b7gen", "R\u00fc\u00b7cken", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Den K\u00f6cher zwischen seinen Fl\u00fcgeln zeigend.", "tokens": ["Den", "K\u00f6\u00b7cher", "zwi\u00b7schen", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "zei\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und nun die D\u00e4chsel ... Wollt ihr!.. Da\u00df der Kukuk!..", "tokens": ["Und", "nun", "die", "D\u00e4ch\u00b7sel", "...", "Wollt", "ihr", "!", "..", "Da\u00df", "der", "Ku\u00b7kuk", "!", ".."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$(", "VMFIN", "PPER", "$.", "$(", "KOUS", "ART", "NN", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich schn\u00fcre ihnen fast die Kehlen zu,", "tokens": ["Ich", "schn\u00fc\u00b7re", "ih\u00b7nen", "fast", "die", "Keh\u00b7len", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "So emsig, mit gestr\u00e4ubtem Nackenhaar,", "tokens": ["So", "em\u00b7sig", ",", "mit", "ge\u00b7str\u00e4ub\u00b7tem", "Na\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "War ihr Gezerr ... Um Himmelswillen! Wollt ihr!", "tokens": ["War", "ihr", "Ge\u00b7zerr", "...", "Um", "Him\u00b7mels\u00b7wil\u00b7len", "!", "Wollt", "ihr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "KOUI", "NN", "$.", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df ihr mir nicht den s\u00fc\u00dfen Bengel weckt!", "tokens": ["Da\u00df", "ihr", "mir", "nicht", "den", "s\u00fc\u00b7\u00dfen", "Ben\u00b7gel", "weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Weh mir, wenn er erwacht, er schenkt sofort", "tokens": ["Weh", "mir", ",", "wenn", "er", "er\u00b7wacht", ",", "er", "schenkt", "so\u00b7fort"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der Senne seinen Pfeil, den ersten, besten,", "tokens": ["Der", "Sen\u00b7ne", "sei\u00b7nen", "Pfeil", ",", "den", "ers\u00b7ten", ",", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und trifft mein Herz, und trifft es unbarmherzig,", "tokens": ["Und", "trifft", "mein", "Herz", ",", "und", "trifft", "es", "un\u00b7barm\u00b7her\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Da\u00df ich der Liebe Qualen dulden mu\u00df,", "tokens": ["Da\u00df", "ich", "der", "Lie\u00b7be", "Qua\u00b7len", "dul\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Liebe Leiden, die vieltausendmal,", "tokens": ["Der", "Lie\u00b7be", "Lei\u00b7den", ",", "die", "viel\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "H\u00e4lt Venus, wiegend, in der Hand die Wage,", "tokens": ["H\u00e4lt", "Ve\u00b7nus", ",", "wie\u00b7gend", ",", "in", "der", "Hand", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVPP", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Der Liebe Lust schwerlastend niederdr\u00fccken.", "tokens": ["Der", "Lie\u00b7be", "Lust", "schwer\u00b7las\u00b7tend", "nie\u00b7der\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon bin am Holzesrand ich, immer noch", "tokens": ["Schon", "bin", "am", "Hol\u00b7zes\u00b7rand", "ich", ",", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die heftigen Hunde \u00e4ngstlich mit mir ziehend.", "tokens": ["Die", "hef\u00b7ti\u00b7gen", "Hun\u00b7de", "\u00e4ngst\u00b7lich", "mit", "mir", "zie\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und vor mir schimmert weit ein helles Land.", "tokens": ["Und", "vor", "mir", "schim\u00b7mert", "weit", "ein", "hel\u00b7les", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In seine Stille schau' ich lang hinein.", "tokens": ["In", "sei\u00b7ne", "Stil\u00b7le", "schau'", "ich", "lang", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und m\u00e4hlig, w\u00e4hrend ich die Augen tr\u00e4nke", "tokens": ["Und", "m\u00e4h\u00b7lig", ",", "w\u00e4h\u00b7rend", "ich", "die", "Au\u00b7gen", "tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In all' den Morgenfarben, steigt ein Wunsch:", "tokens": ["In", "all'", "den", "Mor\u00b7gen\u00b7far\u00b7ben", ",", "steigt", "ein", "Wunsch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn dort um eine sch\u00f6ne Schulter ich,", "tokens": ["Wenn", "dort", "um", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Schul\u00b7ter", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch Duft und bunte Blumengr\u00fc\u00dfe schreitend,", "tokens": ["Durch", "Duft", "und", "bun\u00b7te", "Blu\u00b7men\u00b7gr\u00fc\u00b7\u00dfe", "schrei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Arm gelegt, inde\u00df die andre Hand", "tokens": ["Den", "Arm", "ge\u00b7legt", ",", "in\u00b7de\u00df", "die", "and\u00b7re", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein liebes H\u00e4ndchen h\u00e4lt; und zu mir auf", "tokens": ["Ein", "lie\u00b7bes", "H\u00e4nd\u00b7chen", "h\u00e4lt", ";", "und", "zu", "mir", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "KON", "APPR", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein Auge blickt, das ich mein Leben nennte ...", "tokens": ["Ein", "Au\u00b7ge", "blickt", ",", "das", "ich", "mein", "Le\u00b7ben", "nenn\u00b7te", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Teckel la\u00df ich fort, da\u00df ihr Gel\u00e4ut", "tokens": ["Die", "Te\u00b7ckel", "la\u00df", "ich", "fort", ",", "da\u00df", "ihr", "Ge\u00b7l\u00e4ut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "PPER", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In fernen Gr\u00fcnden bald erstirbt, verhallt.", "tokens": ["In", "fer\u00b7nen", "Gr\u00fcn\u00b7den", "bald", "er\u00b7stirbt", ",", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich selber dann, nicht hastiger kann ein Mensch", "tokens": ["Ich", "sel\u00b7ber", "dann", ",", "nicht", "has\u00b7ti\u00b7ger", "kann", "ein", "Mensch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "