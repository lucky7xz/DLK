{"textgrid.poem.49196": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich empfinde fast ein Grauen", "tokens": ["Ich", "emp\u00b7fin\u00b7de", "fast", "ein", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich, Plato, f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "ich", ",", "Pla\u00b7to", ",", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin gesessen \u00fcber dir;", "tokens": ["Bin", "ge\u00b7ses\u00b7sen", "\u00fc\u00b7ber", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist Zeit hinau\u00df zu schauen", "tokens": ["Es", "ist", "Zeit", "hin\u00b7au\u00df", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APZR", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sich bey den frischen Quellen", "tokens": ["Und", "sich", "bey", "den", "fri\u00b7schen", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Gr\u00fcnen zu ergehn,", "tokens": ["In", "dem", "Gr\u00fc\u00b7nen", "zu", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo die sch\u00f6nen Blumen stehn", "tokens": ["Wo", "die", "sch\u00f6\u00b7nen", "Blu\u00b7men", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Fischer Netze stellen.", "tokens": ["Und", "die", "Fi\u00b7scher", "Net\u00b7ze", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Worzu dienet das Studieren", "tokens": ["Wor\u00b7zu", "die\u00b7net", "das", "Stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Als zu lauter Ungemach?", "tokens": ["Als", "zu", "lau\u00b7ter", "Un\u00b7ge\u00b7mach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter dessen laufft die Bach", "tokens": ["Un\u00b7ter", "des\u00b7sen", "laufft", "die", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsers Lebens, das wir f\u00fchren,", "tokens": ["Un\u00b7sers", "Le\u00b7bens", ",", "das", "wir", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ehe wir es inne werden,", "tokens": ["E\u00b7he", "wir", "es", "in\u00b7ne", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKVZ", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auff ihr letztes Ende hin,", "tokens": ["Auff", "ihr", "letz\u00b7tes", "En\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann k\u00f6mpt ohne Geist und Sinn", "tokens": ["Dann", "k\u00f6mpt", "oh\u00b7ne", "Geist", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses alles in die Erden.", "tokens": ["Die\u00b7ses", "al\u00b7les", "in", "die", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hola, Junger, geh' und frage", "tokens": ["Ho\u00b7la", ",", "Jun\u00b7ger", ",", "geh'", "und", "fra\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der beste Trunck mag seyn,", "tokens": ["Wo", "der", "bes\u00b7te", "Trunck", "mag", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimb den Krug und f\u00fclle Wein.", "tokens": ["Nimb", "den", "Krug", "und", "f\u00fcl\u00b7le", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Trauren, Leid und Klage", "tokens": ["Al\u00b7les", "Trau\u00b7ren", ",", "Leid", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie wir Menschen t\u00e4glich haben,", "tokens": ["Wie", "wir", "Men\u00b7schen", "t\u00e4g\u00b7lich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' uns Clotho fort gerafft,", "tokens": ["Eh'", "uns", "Clo\u00b7tho", "fort", "ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will ich in den s\u00fcssen Safft,", "tokens": ["Will", "ich", "in", "den", "s\u00fcs\u00b7sen", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den die Traube gibt, vergraben.", "tokens": ["Den", "die", "Trau\u00b7be", "gibt", ",", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kauffe gleichfals auch Melonen.", "tokens": ["Kauf\u00b7fe", "gleich\u00b7fals", "auch", "Me\u00b7lo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vergie\u00df de\u00df Zuckers nicht;", "tokens": ["Und", "ver\u00b7gie\u00df", "de\u00df", "Zu\u00b7ckers", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaue nur, da\u00df nichts gebricht.", "tokens": ["Schau\u00b7e", "nur", ",", "da\u00df", "nichts", "ge\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener mag der Heller schonen,", "tokens": ["Je\u00b7ner", "mag", "der", "Hel\u00b7ler", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der bey seinem Gold' und Sch\u00e4tzen", "tokens": ["Der", "bey", "sei\u00b7nem", "Gold'", "und", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tolle sich zu krencken pflegt", "tokens": ["Tol\u00b7le", "sich", "zu", "kren\u00b7cken", "pflegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nicht satt zu Bette legt;", "tokens": ["Und", "nicht", "satt", "zu", "Bet\u00b7te", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich wil, weil ich kan, mich letzen.", "tokens": ["Ich", "wil", ",", "weil", "ich", "kan", ",", "mich", "let\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Bitte meine gute Br\u00fcder", "tokens": ["Bit\u00b7te", "mei\u00b7ne", "gu\u00b7te", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auff die Music und ein Gla\u00df;", "tokens": ["Auff", "die", "Mu\u00b7sic", "und", "ein", "Gla\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Kein Ding schickt sich, d\u00fcnck mich, ba\u00df,", "tokens": ["Kein", "Ding", "schickt", "sich", ",", "d\u00fcnck", "mich", ",", "ba\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "VVIMP", "PPER", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein Trunck und gute Lieder.", "tokens": ["Als", "ein", "Trunck", "und", "gu\u00b7te", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df' ich schon nicht viel zu erben,", "tokens": ["La\u00df'", "ich", "schon", "nicht", "viel", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ey, so hab ich edlen Wein;", "tokens": ["Ey", ",", "so", "hab", "ich", "ed\u00b7len", "Wein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wil mit Andern lustig seyn,", "tokens": ["Wil", "mit", "An\u00b7dern", "lus\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann ich gleich allein mu\u00df sterben.", "tokens": ["Wann", "ich", "gleich", "al\u00b7lein", "mu\u00df", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}