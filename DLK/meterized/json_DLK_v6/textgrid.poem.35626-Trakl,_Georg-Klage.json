{"textgrid.poem.35626": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Klage", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngling aus kristallnem Munde", "tokens": ["J\u00fcng\u00b7ling", "aus", "kris\u00b7tall\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sank dein goldner Blick ins Tal;", "tokens": ["Sank", "dein", "gold\u00b7ner", "Blick", "ins", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waldes Woge rot und fahl", "tokens": ["Wal\u00b7des", "Wo\u00b7ge", "rot", "und", "fahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der schwarzen Abendstunde.", "tokens": ["In", "der", "schwar\u00b7zen", "A\u00b7bends\u00b7tun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Abend schl\u00e4gt so tiefe Wunde!", "tokens": ["A\u00b7bend", "schl\u00e4gt", "so", "tie\u00b7fe", "Wun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Angst! des Todes Traumbeschwerde,", "tokens": ["Angst", "!", "des", "To\u00b7des", "Traum\u00b7be\u00b7schwer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abgestorben Grab und gar", "tokens": ["Ab\u00b7ge\u00b7stor\u00b7ben", "Grab", "und", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut aus Baum und Wild das Jahr;", "tokens": ["Schaut", "aus", "Baum", "und", "Wild", "das", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kahles Feld und Ackererde.", "tokens": ["Kah\u00b7les", "Feld", "und", "A\u00b7cker\u00b7er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruft der Hirt die bange Herde.", "tokens": ["Ruft", "der", "Hirt", "die", "ban\u00b7ge", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwester, deine blauen Brauen", "tokens": ["Schwes\u00b7ter", ",", "dei\u00b7ne", "blau\u00b7en", "Brau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winken leise in der Nacht.", "tokens": ["Win\u00b7ken", "lei\u00b7se", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Orgel seufzt und H\u00f6lle lacht", "tokens": ["Or\u00b7gel", "seufzt", "und", "H\u00f6l\u00b7le", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es fa\u00dft das Herz ein Grauen;", "tokens": ["Und", "es", "fa\u00dft", "das", "Herz", "ein", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6chte Stern und Engel schauen.", "tokens": ["M\u00f6ch\u00b7te", "Stern", "und", "En\u00b7gel", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mutter mu\u00df ums Kindlein zagen;", "tokens": ["Mut\u00b7ter", "mu\u00df", "ums", "Kin\u00b7dlein", "za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rot ert\u00f6nt im Schacht das Erz,", "tokens": ["Rot", "er\u00b7t\u00f6nt", "im", "Schacht", "das", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollust, Tr\u00e4nen, steinern Schmerz,", "tokens": ["Wol\u00b7lust", ",", "Tr\u00e4\u00b7nen", ",", "stei\u00b7nern", "Schmerz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Titanen dunkle Sagen.", "tokens": ["Der", "Ti\u00b7ta\u00b7nen", "dunk\u00b7le", "Sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwermut! einsam Adler klagen.", "tokens": ["Schwer\u00b7mut", "!", "ein\u00b7sam", "Ad\u00b7ler", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}