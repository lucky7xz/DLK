{"textgrid.poem.55647": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Timur", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was? Ihr mi\u00dfbilliget den kr\u00e4ft'gen Sturm", "tokens": ["Was", "?", "Ihr", "mi\u00df\u00b7bil\u00b7li\u00b7get", "den", "kr\u00e4ft'\u00b7gen", "Sturm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des \u00dcbermuts, verlogne Pfaffen!", "tokens": ["Des", "\u00dc\u00b7ber\u00b7muts", ",", "ver\u00b7log\u00b7ne", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt Allah mich bestimmt zum Wurm,", "tokens": ["H\u00e4tt", "Al\u00b7lah", "mich", "be\u00b7stimmt", "zum", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00e4tt er mich als Wurm geschaffen.", "tokens": ["So", "h\u00e4tt", "er", "mich", "als", "Wurm", "ge\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}