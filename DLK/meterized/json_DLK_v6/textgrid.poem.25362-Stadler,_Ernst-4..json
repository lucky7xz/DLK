{"textgrid.poem.25362": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann brenn' ich n\u00e4chtelang, mich zu kasteien,", "tokens": ["Dann", "brenn'", "ich", "n\u00e4ch\u00b7tel\u00b7ang", ",", "mich", "zu", "kas\u00b7tei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sp\u00fcre Stock und Gei\u00dfel \u00fcber meinen Leib geschwenkt:", "tokens": ["Und", "sp\u00fc\u00b7re", "Stock", "und", "Gei\u00b7\u00dfel", "\u00fc\u00b7ber", "mei\u00b7nen", "Leib", "ge\u00b7schwenkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ich will mich ganz von meinem Selbst befreien,", "tokens": ["Ich", "will", "mich", "ganz", "von", "mei\u00b7nem", "Selbst", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis ich an alle Welt mich ausgeschenkt.", "tokens": ["Bis", "ich", "an", "al\u00b7le", "Welt", "mich", "aus\u00b7ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich will den K\u00f6rper so mit Schmerzen n\u00e4hren,", "tokens": ["Ich", "will", "den", "K\u00f6r\u00b7per", "so", "mit", "Schmer\u00b7zen", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis Weltenleid mich sternengleich umkreist \u2013", "tokens": ["Bis", "Wel\u00b7ten\u00b7leid", "mich", "ster\u00b7nen\u00b7gleich", "um\u00b7kreist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Blut und Marter aufgepeitschter Schw\u00e4ren", "tokens": ["In", "Blut", "und", "Mar\u00b7ter", "auf\u00b7ge\u00b7peitschter", "Schw\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erf\u00fcllt sich Liebe und erl\u00f6st sich Geist.", "tokens": ["Er\u00b7f\u00fcllt", "sich", "Lie\u00b7be", "und", "er\u00b7l\u00f6st", "sich", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}