{"dta.poem.10153": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Mahlerey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich recht die Tieffe, H\u00f6he,", "tokens": ["Wenn", "ich", "recht", "die", "Tief\u00b7fe", ",", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Natur der Mahlerey,", "tokens": ["Die", "Na\u00b7tur", "der", "Mah\u00b7le\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie gro\u00df ihr Umkreis sey,", "tokens": ["Und", "wie", "gro\u00df", "ihr", "Um\u00b7kreis", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Seelen Augen sehe;", "tokens": ["Mit", "der", "See\u00b7len", "Au\u00b7gen", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stutz\u2019 ich: denn ein helles Licht", "tokens": ["Stutz'", "ich", ":", "denn", "ein", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$.", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Strahlet mir in mein Gesicht.", "tokens": ["Strah\u00b7let", "mir", "in", "mein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Selbst von unsrer Seelen-Wesen,", "tokens": ["Selbst", "von", "uns\u00b7rer", "See\u00b7len\u00b7We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deucht mich in ihr was zu lesen,", "tokens": ["Deucht", "mich", "in", "ihr", "was", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welches man", "tokens": ["Wel\u00b7ches", "man"], "token_info": ["word", "word"], "pos": ["PWS", "PIS"], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Sonst nicht leicht erblicken kann.", "tokens": ["Sonst", "nicht", "leicht", "er\u00b7bli\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann, mit etwas schwartzer Kreide,", "tokens": ["Wann", ",", "mit", "et\u00b7was", "schwart\u00b7zer", "Krei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie sch\u00f6nes in sich h\u00e4lt,", "tokens": ["Was", "sie", "sch\u00f6\u00b7nes", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns so klar vor Augen stellt,", "tokens": ["Uns", "so", "klar", "vor", "Au\u00b7gen", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, aus nichts fast, etwas ziehet,", "tokens": ["Und", ",", "aus", "nichts", "fast", ",", "et\u00b7was", "zie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PIS", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So, da\u00df man, nicht sonder Freude,", "tokens": ["So", ",", "da\u00df", "man", ",", "nicht", "son\u00b7der", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "$,", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In der Pracht der Creatur,", "tokens": ["In", "der", "Pracht", "der", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Von dem Sch\u00f6pfer selbst die Spur", "tokens": ["Von", "dem", "Sch\u00f6p\u00b7fer", "selbst", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In den klugen Z\u00fcgen siehet;", "tokens": ["In", "den", "klu\u00b7gen", "Z\u00fc\u00b7gen", "sie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deucht mich, da\u00df die Mahlerey", "tokens": ["Deucht", "mich", ",", "da\u00df", "die", "Mah\u00b7le\u00b7rey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Fast ein Bild der Sch\u00f6pfung sey;", "tokens": ["Fast", "ein", "Bild", "der", "Sch\u00f6p\u00b7fung", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da du aus der schwartzen Erden", "tokens": ["Da", "du", "aus", "der", "schwart\u00b7zen", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Thier\u2019 und Pflantzen l\u00e4ssest werden.", "tokens": ["Thier'", "und", "Pflant\u00b7zen", "l\u00e4s\u00b7sest", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! rieff ich, von Andacht hei\u00df:", "tokens": ["Ach", "!", "rieff", "ich", ",", "von", "An\u00b7dacht", "hei\u00df", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da wir in der Menschen Seelen", "tokens": ["Da", "wir", "in", "der", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Solche Wunder-Kr\u00e4fft\u2019 entdecken,", "tokens": ["Sol\u00b7che", "Wun\u00b7der\u00b7Kr\u00e4fft'", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die man nicht vermag zu zehlen;", "tokens": ["Die", "man", "nicht", "ver\u00b7mag", "zu", "zeh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie so gar unendlich weit", "tokens": ["Wie", "so", "gar", "un\u00b7end\u00b7lich", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcssen sich, voll Herrlichkeit,", "tokens": ["M\u00fcs\u00b7sen", "sich", ",", "voll", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PRF", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsers Sch\u00f6pfers Kr\u00e4fft\u2019 erstrecken!", "tokens": ["Un\u00b7sers", "Sch\u00f6p\u00b7fers", "Kr\u00e4fft'", "er\u00b7stre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}