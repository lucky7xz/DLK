{"textgrid.poem.29095": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein alter Mann wird zwar veracht,", "tokens": ["Ein", "al\u00b7ter", "Mann", "wird", "zwar", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der aber doch der jungen lacht,", "tokens": ["Der", "a\u00b7ber", "doch", "der", "jun\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihnen selbst ein Lied ertichten,", "tokens": ["Die", "ih\u00b7nen", "selbst", "ein", "Lied", "er\u00b7tich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das man dann auch auff sie wird richten.", "tokens": ["Das", "man", "dann", "auch", "auff", "sie", "wird", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "ADV", "APPR", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}