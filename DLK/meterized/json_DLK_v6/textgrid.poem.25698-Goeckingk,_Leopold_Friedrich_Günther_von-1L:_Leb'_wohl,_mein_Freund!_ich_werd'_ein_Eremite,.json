{"textgrid.poem.25698": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leb' wohl, mein Freund! ich werd' ein Eremite,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb' wohl, mein Freund! ich werd' ein Eremite,", "tokens": ["Leb'", "wohl", ",", "mein", "Freund", "!", "ich", "werd'", "ein", "E\u00b7re\u00b7mi\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zieh' in eine Siedelei,", "tokens": ["Und", "zieh'", "in", "ei\u00b7ne", "Sie\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo von des Zwanges Fesseln frei,", "tokens": ["Wo", "von", "des", "Zwan\u00b7ges", "Fes\u00b7seln", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich meinen Stunden selbst gebiete.", "tokens": ["Ich", "mei\u00b7nen", "Stun\u00b7den", "selbst", "ge\u00b7bie\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die gro\u00dfe Welt berauschte nur die Sinnen,", "tokens": ["Die", "gro\u00b7\u00dfe", "Welt", "be\u00b7rauschte", "nur", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und lie\u00df Gehirn und Herz mir leer;", "tokens": ["Und", "lie\u00df", "Ge\u00b7hirn", "und", "Herz", "mir", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Prahlerin! was lehrt sie mehr,", "tokens": ["Die", "Prah\u00b7le\u00b7rin", "!", "was", "lehrt", "sie", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Muth, ihr selber zu entrinnen?", "tokens": ["Als", "Muth", ",", "ihr", "sel\u00b7ber", "zu", "ent\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch den mu\u00df man um hohen Preis erkaufen:", "tokens": ["Auch", "den", "mu\u00df", "man", "um", "ho\u00b7hen", "Preis", "er\u00b7kau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VMFIN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Denn Zwang, den freie Seelen scheun,", "tokens": ["Denn", "Zwang", ",", "den", "frei\u00b7e", "See\u00b7len", "scheun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und Langeweil' und T\u00e4ndelein,", "tokens": ["Und", "Lan\u00b7ge\u00b7weil'", "und", "T\u00e4n\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Stehn unbemerkt in ihrem Haufen.", "tokens": ["Stehn", "un\u00b7be\u00b7merkt", "in", "ih\u00b7rem", "Hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie sehnt' ich mich, aus ihr hinweg zu schleichen,", "tokens": ["Wie", "sehnt'", "ich", "mich", ",", "aus", "ihr", "hin\u00b7weg", "zu", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "APPR", "PPER", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In meine s\u00fc\u00dfe Dunkelheit,", "tokens": ["In", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wo, mit gewohnter Freundlichkeit", "tokens": ["Wo", ",", "mit", "ge\u00b7wohn\u00b7ter", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Musen mir die Laute reichen.", "tokens": ["Die", "Mu\u00b7sen", "mir", "die", "Lau\u00b7te", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Hier darf ich mich vor keinem Laster b\u00fccken,", "tokens": ["Hier", "darf", "ich", "mich", "vor", "kei\u00b7nem", "Las\u00b7ter", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Hier rechnet mir kein stolzer Thor", "tokens": ["Hier", "rech\u00b7net", "mir", "kein", "stol\u00b7zer", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die sechszehn kl\u00fcgern Ahnherrn vor,", "tokens": ["Die", "sechs\u00b7zehn", "kl\u00fc\u00b7gern", "Ahn\u00b7herrn", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Um keck auf mich herab zu blicken.", "tokens": ["Um", "keck", "auf", "mich", "her\u00b7ab", "zu", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Hier schmiedet man mich nicht zu halben Tagen,", "tokens": ["Hier", "schmie\u00b7det", "man", "mich", "nicht", "zu", "hal\u00b7ben", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "PTKNEG", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und ganze N\u00e4cht', am Spieltisch' an,", "tokens": ["Und", "gan\u00b7ze", "N\u00e4cht'", ",", "am", "Spiel\u00b7tisch'", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Um, was doch nur die Muse kann,", "tokens": ["Um", ",", "was", "doch", "nur", "die", "Mu\u00b7se", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mir meine Grillen zu verjagen.", "tokens": ["Mir", "mei\u00b7ne", "Gril\u00b7len", "zu", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Hier faselt mir kein Held, der hinter Schanzen", "tokens": ["Hier", "fa\u00b7selt", "mir", "kein", "Held", ",", "der", "hin\u00b7ter", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Schon zitterte, von Krieges List,", "tokens": ["Schon", "zit\u00b7ter\u00b7te", ",", "von", "Krie\u00b7ges", "List", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kein Mann, der selbst ein Prasser ist,", "tokens": ["Kein", "Mann", ",", "der", "selbst", "ein", "Pras\u00b7ser", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Erbaut Systeme von Finanzen.", "tokens": ["Er\u00b7baut", "Sys\u00b7te\u00b7me", "von", "Fi\u00b7nan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.29": {"text": "Hier gr\u00fcbelt nicht die laurende Chikane", "tokens": ["Hier", "gr\u00fc\u00b7belt", "nicht", "die", "lau\u00b7ren\u00b7de", "Chi\u00b7ka\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Aus einem Scherze Hochverrath;", "tokens": ["Aus", "ei\u00b7nem", "Scher\u00b7ze", "Hoch\u00b7ver\u00b7rath", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Hier nagt am Lorbeer' guter That", "tokens": ["Hier", "nagt", "am", "Lor\u00b7beer'", "gu\u00b7ter", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Kein Neid mit seinem Wolfeszahne.", "tokens": ["Kein", "Neid", "mit", "sei\u00b7nem", "Wol\u00b7fes\u00b7zah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Hier wispert nicht, mit List, von Sitz zu Sitze,", "tokens": ["Hier", "wis\u00b7pert", "nicht", ",", "mit", "List", ",", "von", "Sitz", "zu", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Die Schm\u00e4hsucht, da\u00df sie fressend Gift", "tokens": ["Die", "Schm\u00e4h\u00b7sucht", ",", "da\u00df", "sie", "fres\u00b7send", "Gift"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Auf jeden Namen, den es trifft,", "tokens": ["Auf", "je\u00b7den", "Na\u00b7men", ",", "den", "es", "trifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Aus ihrer Vipernzunge spr\u00fctze.", "tokens": ["Aus", "ih\u00b7rer", "Vi\u00b7pern\u00b7zun\u00b7ge", "spr\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Mein B\u00fccherschrank, mein unbelauschtes Zimmer,", "tokens": ["Mein", "B\u00fc\u00b7cher\u00b7schrank", ",", "mein", "un\u00b7be\u00b7lauschtes", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Ist mein Gespr\u00e4ch und meine Welt:", "tokens": ["Ist", "mein", "Ge\u00b7spr\u00e4ch", "und", "mei\u00b7ne", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Mein Wunsch nach aller Gr\u00f6\u00dfe f\u00e4llt,", "tokens": ["Mein", "Wunsch", "nach", "al\u00b7ler", "Gr\u00f6\u00b7\u00dfe", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und Friede baut auf seine Tr\u00fcmmer.", "tokens": ["Und", "Frie\u00b7de", "baut", "auf", "sei\u00b7ne", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Weg mit den T\u00e4nzen und den Pf\u00e4nderspielen,", "tokens": ["Weg", "mit", "den", "T\u00e4n\u00b7zen", "und", "den", "Pf\u00e4n\u00b7der\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.42": {"text": "Und mit der Zeitverderberin,", "tokens": ["Und", "mit", "der", "Zeit\u00b7ver\u00b7der\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Kart'! Ist Weisheit nur Gewinn:", "tokens": ["Der", "Kart'", "!", "Ist", "Weis\u00b7heit", "nur", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "So la\u00df in ihrem Schatz' mich w\u00fchlen.", "tokens": ["So", "la\u00df", "in", "ih\u00b7rem", "Schatz'", "mich", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und ehe sollst du mich nicht wieder sehen,", "tokens": ["Und", "e\u00b7he", "sollst", "du", "mich", "nicht", "wie\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Als bis mein Geist die gro\u00dfe Welt", "tokens": ["Als", "bis", "mein", "Geist", "die", "gro\u00b7\u00dfe", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "F\u00fcr eine Puppenbude h\u00e4lt,", "tokens": ["F\u00fcr", "ei\u00b7ne", "Pup\u00b7pen\u00b7bu\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und ihre Riesen, f\u00fcr Pigm\u00e4en.", "tokens": ["Und", "ih\u00b7re", "Rie\u00b7sen", ",", "f\u00fcr", "Pig\u00b7m\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}}}}}