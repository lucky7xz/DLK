{"dta.poem.2171": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Feder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geschw\u00e4tzige Zunge, wodurch sich die Seelen,", "tokens": ["Ge\u00b7schw\u00e4t\u00b7zi\u00b7ge", "Zun\u00b7ge", ",", "wo\u00b7durch", "sich", "die", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In stummer Beredsamkeit, nah\u2019 und entfernt,", "tokens": ["In", "stum\u00b7mer", "Be\u00b7red\u00b7sam\u00b7keit", ",", "nah'", "und", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Einander ihr Wirken und Leiden erz\u00e4hlen!", "tokens": ["Ein\u00b7an\u00b7der", "ihr", "Wir\u00b7ken", "und", "Lei\u00b7den", "er\u00b7z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Betr\u00e4chtliches Werkzeug, wodurch wir gelernt,", "tokens": ["Be\u00b7tr\u00e4cht\u00b7li\u00b7ches", "Werk\u00b7zeug", ",", "wo\u00b7durch", "wir", "ge\u00b7lernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+---+-++-+", "measure": "dactylic.init"}, "line.5": {"text": "O Wunder! die Geister mit Geistern verm\u00e4hlen,", "tokens": ["O", "Wun\u00b7der", "!", "die", "Geis\u00b7ter", "mit", "Geis\u00b7tern", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Gedanken verk\u00f6rpern, erzeugen, erhalten,", "tokens": ["Ge\u00b7dan\u00b7ken", "ver\u00b7k\u00f6r\u00b7pern", ",", "er\u00b7zeu\u00b7gen", ",", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und binden, so da\u00df wir, auch wenn wir erkalten,", "tokens": ["Und", "bin\u00b7den", ",", "so", "da\u00df", "wir", ",", "auch", "wenn", "wir", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADV", "KOUS", "PPER", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Noch lange der Lebenden Stellen verwalten.", "tokens": ["Noch", "lan\u00b7ge", "der", "Le\u00b7ben\u00b7den", "Stel\u00b7len", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Du stellest den Augen, durch Schatten und Licht,", "tokens": ["Du", "stel\u00b7lest", "den", "Au\u00b7gen", ",", "durch", "Schat\u00b7ten", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Jm Dunklen der Dinte, bey wei\u00dfem Papier", "tokens": ["Jm", "Dunk\u00b7len", "der", "Din\u00b7te", ",", "bey", "wei\u00b7\u00dfem", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "In leiblich- und sichtbaren Formen uns f\u00fcr,", "tokens": ["In", "leib\u00b7lich", "und", "sicht\u00b7ba\u00b7ren", "For\u00b7men", "uns", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "PPER", "APPR", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Was in dem Verborgnen der Seelen geschicht.", "tokens": ["Was", "in", "dem", "Ver\u00b7borg\u00b7nen", "der", "See\u00b7len", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}