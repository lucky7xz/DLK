{"dta.poem.5547": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist uns denn nun gleich viel zu wissen,", "tokens": ["Ist", "uns", "denn", "nun", "gleich", "viel", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob unser Leben uns auf ewig dort,", "tokens": ["Ob", "un\u00b7ser", "Le\u00b7ben", "uns", "auf", "e\u00b7wig", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch die Ver\u00e4nderung des C\u00f6rpers, werd\u2019 entrissen?", "tokens": ["Durch", "die", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "des", "C\u00f6r\u00b7pers", ",", "werd'", "ent\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Wie, oder ob der Tod, an jenem seel\u2019gen Ort,", "tokens": ["Wie", ",", "o\u00b7der", "ob", "der", "Tod", ",", "an", "je\u00b7nem", "seel'\u00b7gen", "Ort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "ART", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Geistern, die der Sterblichkeit entladen,", "tokens": ["Den", "Geis\u00b7tern", ",", "die", "der", "Sterb\u00b7lich\u00b7keit", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht mehr verm\u00f6gend sey zu schaden?", "tokens": ["Nicht", "mehr", "ver\u00b7m\u00f6\u00b7gend", "sey", "zu", "scha\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}