{"textgrid.poem.61468": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl flog mit roten Wipfeln einst", "tokens": ["Wohl", "flog", "mit", "ro\u00b7ten", "Wip\u00b7feln", "einst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Schiff in junger Zeit;", "tokens": ["Mein", "Schiff", "in", "jun\u00b7ger", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann kamen Sturm und Wetter,", "tokens": ["Dann", "ka\u00b7men", "Sturm", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da trug ich schweres Leid.", "tokens": ["Da", "trug", "ich", "schwe\u00b7res", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wie der fr\u00fche goldne Traum", "tokens": ["Doch", "wie", "der", "fr\u00fc\u00b7he", "gold\u00b7ne", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerging des Kummers Last;", "tokens": ["Zer\u00b7ging", "des", "Kum\u00b7mers", "Last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun schau' ich nach den Sternen", "tokens": ["Nun", "schau'", "ich", "nach", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Steuer, ernst gefa\u00dft.", "tokens": ["Vom", "Steu\u00b7er", ",", "ernst", "ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was immer kam, ich hab's erkannt,", "tokens": ["Was", "im\u00b7mer", "kam", ",", "ich", "hab's", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am letzten war es gut;", "tokens": ["Am", "letz\u00b7ten", "war", "es", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hat mein Herz geg\u00fcrtet", "tokens": ["Das", "hat", "mein", "Herz", "ge\u00b7g\u00fcr\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem festen Mut.", "tokens": ["Mit", "ei\u00b7nem", "fes\u00b7ten", "Mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Fahr zu, mein Schiff, fahr fr\u00f6hlich zu", "tokens": ["Fahr", "zu", ",", "mein", "Schiff", ",", "fahr", "fr\u00f6h\u00b7lich", "zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Glanz und Nebelrauch!", "tokens": ["Durch", "Glanz", "und", "Ne\u00b7bel\u00b7rauch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deinen raschen Segeln", "tokens": ["In", "dei\u00b7nen", "ra\u00b7schen", "Se\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wind ist Gottes Hauch.", "tokens": ["Der", "Wind", "ist", "Got\u00b7tes", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}