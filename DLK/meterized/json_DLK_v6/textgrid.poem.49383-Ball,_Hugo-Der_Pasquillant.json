{"textgrid.poem.49383": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der Pasquillant", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch konnt es unserm Scharfsinn nicht entgehen,", "tokens": ["Auch", "konnt", "es", "un\u00b7serm", "Scharf\u00b7sinn", "nicht", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ein Herr Geist uns zu bem\u00e4keln pflegt,", "tokens": ["Da\u00df", "ein", "Herr", "Geist", "uns", "zu", "be\u00b7m\u00e4\u00b7keln", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indem er ein Pasquill zusammentr\u00e4gt,", "tokens": ["In\u00b7dem", "er", "ein", "Pas\u00b7quill", "zu\u00b7sam\u00b7men\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ihm die Winde um die Ohren s\u00e4en.", "tokens": ["Das", "ihm", "die", "Win\u00b7de", "um", "die", "Oh\u00b7ren", "s\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald kritzelt er, bald h\u00fcpft er aufgeregt", "tokens": ["Bald", "krit\u00b7zelt", "er", ",", "bald", "h\u00fcpft", "er", "auf\u00b7ge\u00b7regt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um uns herum, dann bleibt er zuckend stehen", "tokens": ["Um", "uns", "he\u00b7rum", ",", "dann", "bleibt", "er", "zu\u00b7ckend", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reckt den Schwartenhals, um zu ersp\u00e4hen,", "tokens": ["Und", "reckt", "den", "Schwar\u00b7ten\u00b7hals", ",", "um", "zu", "er\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sich in unserm Kabinett bewegt.", "tokens": ["Was", "sich", "in", "un\u00b7serm", "Ka\u00b7bi\u00b7nett", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Bleistiftstummel hat er ganz zerbissen,", "tokens": ["Den", "Blei\u00b7stift\u00b7stum\u00b7mel", "hat", "er", "ganz", "zer\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Drillichnaht ist hinten aufgeschlissen,", "tokens": ["Die", "Dril\u00b7lich\u00b7naht", "ist", "hin\u00b7ten", "auf\u00b7ge\u00b7schlis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch d\u00fcnkt er sich ein Diplomatenj\u00e4ger.", "tokens": ["Doch", "d\u00fcnkt", "er", "sich", "ein", "Dip\u00b7lo\u00b7ma\u00b7ten\u00b7j\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "De fakto dient bewu\u00dfter Schlingenleger", "tokens": ["De", "fak\u00b7to", "dient", "be\u00b7wu\u00df\u00b7ter", "Schlin\u00b7gen\u00b7le\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Kastellan als Flur- und Stra\u00dfenfeger", "tokens": ["Dem", "Kas\u00b7tel\u00b7lan", "als", "Flur", "und", "Stra\u00b7\u00dfen\u00b7fe\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hat das Recht die K\u00fcbel auszugie\u00dfen.", "tokens": ["Und", "hat", "das", "Recht", "die", "K\u00fc\u00b7bel", "aus\u00b7zu\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}