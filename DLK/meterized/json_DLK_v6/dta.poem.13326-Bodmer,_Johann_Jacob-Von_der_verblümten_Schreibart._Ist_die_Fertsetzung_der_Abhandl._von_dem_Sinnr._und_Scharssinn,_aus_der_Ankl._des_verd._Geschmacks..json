{"dta.poem.13326": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst aber war die Wand so dicke,", "tokens": ["Sonst", "a\u00b7ber", "war", "die", "Wand", "so", "di\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, wann die Augen oftermahl", "tokens": ["Da\u00df", ",", "wann", "die", "Au\u00b7gen", "of\u00b7ter\u00b7mahl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Blat auf Blat in Schatten-reichen ", "tokens": ["Von", "Blat", "auf", "Blat", "in", "Schat\u00b7ten\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwirret hin und wieder liefen,", "tokens": ["Ver\u00b7wir\u00b7ret", "hin", "und", "wie\u00b7der", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie keine Th\u00fcr zu finden wu\u00dften,", "tokens": ["Sie", "kei\u00b7ne", "Th\u00fcr", "zu", "fin\u00b7den", "wu\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und, angenehm besch\u00e4mt, zur\u00fccke kehren mu\u00dften.", "tokens": ["Und", ",", "an\u00b7ge\u00b7nehm", "be\u00b7sch\u00e4mt", ",", "zu\u00b7r\u00fc\u00b7cke", "keh\u00b7ren", "mu\u00df\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}