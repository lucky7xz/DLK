{"textgrid.poem.42545": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Nonnen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhebet euch mit heil'gem Triebe,", "tokens": ["Er\u00b7he\u00b7bet", "euch", "mit", "heil'\u00b7gem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr frommen Schwestern, himmelan", "tokens": ["Ihr", "from\u00b7men", "Schwes\u00b7tern", ",", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schwebt auf bl\u00fchnder Wolkenbahn!", "tokens": ["Und", "schwebt", "auf", "bl\u00fchn\u00b7der", "Wol\u00b7ken\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da leuchtet uns die reinste Sonne,", "tokens": ["Da", "leuch\u00b7tet", "uns", "die", "reins\u00b7te", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da singen wir in Fr\u00fchlingswonne", "tokens": ["Da", "sin\u00b7gen", "wir", "in", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Lied von dir, du ew'ge Liebe!", "tokens": ["Ein", "Lied", "von", "dir", ",", "du", "ew'\u00b7ge", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob welken alle zarten Bl\u00fcten", "tokens": ["Ob", "wel\u00b7ken", "al\u00b7le", "zar\u00b7ten", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem Genu\u00df der ird'schen Glut:", "tokens": ["Von", "dem", "Ge\u00b7nu\u00df", "der", "ird'\u00b7schen", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein ewig Jugendblut", "tokens": ["Du", "bist", "ein", "e\u00b7wig", "Ju\u00b7gend\u00b7blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsrer Busen stete F\u00fclle,", "tokens": ["Und", "uns\u00b7rer", "Bu\u00b7sen", "ste\u00b7te", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ew'ge Flamme, die wir stille", "tokens": ["Die", "ew'\u00b7ge", "Flam\u00b7me", ",", "die", "wir", "stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Altar und im Herzen h\u00fcten.", "tokens": ["Am", "Al\u00b7tar", "und", "im", "Her\u00b7zen", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du stiegest nieder, ew'ge G\u00fcte,", "tokens": ["Du", "stie\u00b7gest", "nie\u00b7der", ",", "ew'\u00b7ge", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du lagst, ein l\u00e4chelnd Himmelskind,", "tokens": ["Du", "lagst", ",", "ein", "l\u00e4\u00b7chelnd", "Him\u00b7mel\u00b7skind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Arm der Jungfrau, s\u00fc\u00df und lind;", "tokens": ["Im", "Arm", "der", "Jung\u00b7frau", ",", "s\u00fc\u00df", "und", "lind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie durft aus deinen hellen Augen", "tokens": ["Sie", "durft", "aus", "dei\u00b7nen", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Glanz der Himmel in sich saugen,", "tokens": ["Den", "Glanz", "der", "Him\u00b7mel", "in", "sich", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis sie die Glorie umgl\u00fchte.", "tokens": ["Bis", "sie", "die", "Glo\u00b7rie", "um\u00b7gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Du hast mit g\u00f6ttlichem Erbarmen", "tokens": ["Du", "hast", "mit", "g\u00f6tt\u00b7li\u00b7chem", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Kreuz die Arme ausgespannt.", "tokens": ["Am", "Kreuz", "die", "Ar\u00b7me", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ruft der Sturm, da dr\u00f6hnt das Land:", "tokens": ["Da", "ruft", "der", "Sturm", ",", "da", "dr\u00f6hnt", "das", "Land", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt her, kommt her von allen Orten!", "tokens": ["Kommt", "her", ",", "kommt", "her", "von", "al\u00b7len", "Or\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Tote, sprengt des Grabes Pforten!", "tokens": ["Ihr", "To\u00b7te", ",", "sprengt", "des", "Gra\u00b7bes", "Pfor\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er nimmt euch auf mit offnen Armen.", "tokens": ["Er", "nimmt", "euch", "auf", "mit", "off\u00b7nen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Wunderlieb, o Liebeswonne!", "tokens": ["O", "Wun\u00b7der\u00b7lieb", ",", "o", "Lie\u00b7bes\u00b7won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist diese Zeit ein Schlummer mir,", "tokens": ["Ist", "die\u00b7se", "Zeit", "ein", "Schlum\u00b7mer", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So tr\u00e4um ich sehnlich nur von dir;", "tokens": ["So", "tr\u00e4um", "ich", "sehn\u00b7lich", "nur", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein Erwachen wird es geben,", "tokens": ["Und", "ein", "Er\u00b7wa\u00b7chen", "wird", "es", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da werd ich ganz in dich verschweben,", "tokens": ["Da", "werd", "ich", "ganz", "in", "dich", "ver\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Glutstrahl in die gro\u00dfe Sonne.", "tokens": ["Ein", "Glut\u00b7strahl", "in", "die", "gro\u00b7\u00dfe", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}