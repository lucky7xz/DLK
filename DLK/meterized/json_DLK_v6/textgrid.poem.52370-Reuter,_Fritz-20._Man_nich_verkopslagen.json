{"textgrid.poem.52370": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "20. Man nich verkopslagen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tau Kopmann Schulten tau Swerin", "tokens": ["Tau", "Kop\u00b7mann", "Schul\u00b7ten", "tau", "Swe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fcmmt mal 'ne olle Fru herin,", "tokens": ["K\u00fcmmt", "mal", "'ne", "ol\u00b7le", "Fru", "he\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Witt bomwull'n Halflinn'n sall dat sin.", "tokens": ["Witt", "bom\u00b7wull'n", "Ha\u00b7lf\u00b7linn'n", "sall", "dat", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbganz woll\u00ab, seggt Schult, \u00bbun denn wo vel?\u00ab", "tokens": ["\u00bb", "ganz", "woll", "\u00ab", ",", "seggt", "Schult", ",", "\u00bb", "un", "denn", "wo", "vel", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$(", "$,", "VVFIN", "NN", "$,", "$(", "FM", "KON", "PWAV", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bboh\u00ab, seggt de Ollsch, \u00bbman blot 'ne Ehl.\u00ab", "tokens": ["\u00bb", "oh", "\u00ab", ",", "seggt", "de", "Ollsch", ",", "\u00bb", "man", "blot", "'ne", "Ehl", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "PIS", "ADV", "ART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "\u00bbna\u00ab, seggt denn Schult, \u00bbwil du dat b\u00fcst;", "tokens": ["\u00bb", "na", "\u00ab", ",", "seggt", "denn", "Schult", ",", "\u00bb", "wil", "du", "dat", "b\u00fcst", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "$,", "VVFIN", "KON", "NN", "$,", "$(", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ick heww tauf\u00e4llig hir noch j\u00fcst", "tokens": ["Ick", "heww", "tau\u00b7f\u00e4l\u00b7lig", "hir", "noch", "j\u00fcst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "En l\u00fctten Rest von annerthalwen,", "tokens": ["En", "l\u00fct\u00b7ten", "Rest", "von", "an\u00b7ner\u00b7thal\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den will'ck di f\u00f6r vir Schilling laten.\u00ab", "tokens": ["Den", "will'ck", "di", "f\u00f6r", "vir", "Schil\u00b7ling", "la\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbne\u00ab, seggt de Ollsch, \u00bbdat is tau vel;", "tokens": ["\u00bb", "ne", "\u00ab", ",", "seggt", "de", "Ollsch", ",", "\u00bb", "dat", "is", "tau", "vel", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ick heww f\u00f6r eine ganze Ehl", "tokens": ["Ick", "heww", "f\u00f6r", "ei\u00b7ne", "gan\u00b7ze", "Ehl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bi unsern nigen Kopmann Platen", "tokens": ["Bi", "un\u00b7sern", "ni\u00b7gen", "Kop\u00b7mann", "Pla\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wat sei denn grot noch anners wull,", "tokens": ["Wat", "sei", "denn", "grot", "noch", "an\u00b7ners", "wull", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "ADV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hei wull de ", "tokens": ["Hei", "wull", "de"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Ehr jo gor f\u00f6r ", "tokens": ["Ehr", "jo", "gor", "f\u00f6r"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "NE"], "meter": "---+", "measure": "unknown.measure.single"}, "line.16": {"text": "\u00bbne\u00ab, seggt de Ollsch, \u00bbdat's \u00e4werdrewen,", "tokens": ["\u00bb", "ne", "\u00ab", ",", "seggt", "de", "Ollsch", ",", "\u00bb", "dat's", "\u00e4\u00b7wer\u00b7dre\u00b7wen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "NE", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Dat's f\u00f6r dat T\u00fcg doch vel tau vel!", "tokens": ["Dat's", "f\u00f6r", "dat", "T\u00fcg", "doch", "vel", "tau", "vel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mihr as ", "tokens": ["Mihr", "as"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+", "measure": "iambic.single"}}}}}