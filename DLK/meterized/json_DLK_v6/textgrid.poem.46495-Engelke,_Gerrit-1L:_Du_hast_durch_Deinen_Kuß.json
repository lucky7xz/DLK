{"textgrid.poem.46495": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast durch Deinen Ku\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1918, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast durch Deinen Ku\u00df", "tokens": ["Du", "hast", "durch", "Dei\u00b7nen", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein stromvoll Blut geweckt", "tokens": ["Mein", "strom\u00b7voll", "Blut", "ge\u00b7weckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mein Gesicht warm aufgehoben aus dem Tag,", "tokens": ["Und", "mein", "Ge\u00b7sicht", "warm", "auf\u00b7ge\u00b7ho\u00b7ben", "aus", "dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mich nun uferlose gro\u00dfe Nacht umsp\u00fclt,", "tokens": ["Da\u00df", "mich", "nun", "u\u00b7fer\u00b7lo\u00b7se", "gro\u00b7\u00dfe", "Nacht", "um\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herwehend Glanz und Taumel.", "tokens": ["Her\u00b7we\u00b7hend", "Glanz", "und", "Tau\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Einwiegend Zittern schwillt in meiner F\u00fc\u00dfe Wurzeln,", "tokens": ["Ein\u00b7wie\u00b7gend", "Zit\u00b7tern", "schwillt", "in", "mei\u00b7ner", "F\u00fc\u00b7\u00dfe", "Wur\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Einstr\u00f6men lassend Erde und Get\u00f6n,", "tokens": ["Eins\u00b7tr\u00f6\u00b7men", "las\u00b7send", "Er\u00b7de", "und", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und springt aus meiner Kniee Schreiten in die Brust", "tokens": ["Und", "springt", "aus", "mei\u00b7ner", "Kni\u00b7ee", "Schrei\u00b7ten", "in", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu meerbewegter Melodie,", "tokens": ["Zu", "meer\u00b7be\u00b7weg\u00b7ter", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Darin mein Herz, die Orgel rauscht.", "tokens": ["Da\u00b7rin", "mein", "Herz", ",", "die", "Or\u00b7gel", "rauscht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nun sich im Takte meine Sohlen heben", "tokens": ["Nun", "sich", "im", "Tak\u00b7te", "mei\u00b7ne", "Soh\u00b7len", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPRART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und grenzenlos beseeltes Schweben", "tokens": ["Und", "gren\u00b7zen\u00b7los", "be\u00b7seel\u00b7tes", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Glieder \u00fcbergie\u00dft:", "tokens": ["Die", "Glie\u00b7der", "\u00fc\u00b7berg\u00b7ie\u00dft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Hab ich die Arme aufgehoben,", "tokens": ["Hab", "ich", "die", "Ar\u00b7me", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und f\u00fchl, wie meiner aufgelockten Haare Schopf", "tokens": ["Und", "f\u00fchl", ",", "wie", "mei\u00b7ner", "auf\u00b7ge\u00b7lock\u00b7ten", "Haa\u00b7re", "Schopf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die nachtbemalten Wolken streift,", "tokens": ["Die", "nacht\u00b7be\u00b7mal\u00b7ten", "Wol\u00b7ken", "streift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sternbl\u00fctenkranz die Stirn umgreift,", "tokens": ["Stern\u00b7bl\u00fc\u00b7ten\u00b7kranz", "die", "Stirn", "um\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Und tanze, tanze zu Dir hin!", "tokens": ["Und", "tan\u00b7ze", ",", "tan\u00b7ze", "zu", "Dir", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Denn meiner segelwilden Sehnsucht Schauer,", "tokens": ["Denn", "mei\u00b7ner", "se\u00b7gel\u00b7wil\u00b7den", "Sehn\u00b7sucht", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "All meiner Einsamkeiten Trauer,", "tokens": ["All", "mei\u00b7ner", "Ein\u00b7sam\u00b7kei\u00b7ten", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Mein hin und her durchflutet Sein,", "tokens": ["Mein", "hin", "und", "her", "durch\u00b7flu\u00b7tet", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKVZ", "KON", "ADV", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und nun des selgen Leibes neue Lust:", "tokens": ["Und", "nun", "des", "sel\u00b7gen", "Lei\u00b7bes", "neu\u00b7e", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "St\u00fcrmt fort und fort an Deine Brust,", "tokens": ["St\u00fcrmt", "fort", "und", "fort", "an", "Dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Will nur in Dir geborgen sein.", "tokens": ["Will", "nur", "in", "Dir", "ge\u00b7bor\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}