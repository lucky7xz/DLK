{"dta.poem.8832": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und Jasmine duften wieder;", "tokens": ["Und", "Jas\u00b7mi\u00b7ne", "duf\u00b7ten", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und der Ost, der kecke Freyer,", "tokens": ["Und", "der", "Ost", ",", "der", "ke\u00b7cke", "Frey\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6s't den Knospen ihre Mieder:", "tokens": ["L\u00f6s't", "den", "Knos\u00b7pen", "ih\u00b7re", "Mie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du allein verh\u00fcllst dich ewig,", "tokens": ["Du", "al\u00b7lein", "ver\u00b7h\u00fcllst", "dich", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4gst vor mir die Augen nieder!", "tokens": ["Schl\u00e4gst", "vor", "mir", "die", "Au\u00b7gen", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bliese doch ein Wind und legte", "tokens": ["Blie\u00b7se", "doch", "ein", "Wind", "und", "leg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Gewand an deine Glieder!", "tokens": ["Das", "Ge\u00b7wand", "an", "dei\u00b7ne", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4hm' er meiner Seufzer einen", "tokens": ["N\u00e4hm'", "er", "mei\u00b7ner", "Seuf\u00b7zer", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf sein rauschendes Gefieder!", "tokens": ["Auf", "sein", "rau\u00b7schen\u00b7des", "Ge\u00b7fie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "O belohne deinen Sclaven,", "tokens": ["O", "be\u00b7loh\u00b7ne", "dei\u00b7nen", "Scla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Der so treu dir ist und bieder!", "tokens": ["Der", "so", "treu", "dir", "ist", "und", "bie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPER", "VAFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Doch du sprichst: Begl\u00fcck' ich jenen,", "tokens": ["Doch", "du", "sprichst", ":", "Be\u00b7gl\u00fcck", "ich", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "VVIMP", "PPER", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "So verstummen seine Lieder.", "tokens": ["So", "ver\u00b7stum\u00b7men", "sei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}