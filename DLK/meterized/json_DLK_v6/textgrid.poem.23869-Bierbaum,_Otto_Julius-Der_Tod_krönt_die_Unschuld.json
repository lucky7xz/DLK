{"textgrid.poem.23869": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod kr\u00f6nt die Unschuld", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind, ich schenke dir den Reif der Reine,", "tokens": ["Kind", ",", "ich", "schen\u00b7ke", "dir", "den", "Reif", "der", "Rei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kind, ich kr\u00f6ne dich mit goldenem Scheine,", "tokens": ["Kind", ",", "ich", "kr\u00f6\u00b7ne", "dich", "mit", "gol\u00b7de\u00b7nem", "Schei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Kind, ich nehme dich in meinen Schoo\u00df.", "tokens": ["Kind", ",", "ich", "neh\u00b7me", "dich", "in", "mei\u00b7nen", "Schoo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Mutter mu\u00df dich mir verlassen,", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "mu\u00df", "dich", "mir", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meine Fittiche wollen dich umfassen,", "tokens": ["Mei\u00b7ne", "Fit\u00b7ti\u00b7che", "wol\u00b7len", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Meine Fittiche sind weich und gro\u00df.", "tokens": ["Mei\u00b7ne", "Fit\u00b7ti\u00b7che", "sind", "weich", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ruhst darin wie unterm Mutterherzen", "tokens": ["Ruhst", "da\u00b7rin", "wie", "un\u00b7term", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlafumfangen, ledig aller Schmerzen;", "tokens": ["Schlaf\u00b7um\u00b7fan\u00b7gen", ",", "le\u00b7dig", "al\u00b7ler", "Schmer\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deine Seele bleibt vom Leben rein.", "tokens": ["Dei\u00b7ne", "See\u00b7le", "bleibt", "vom", "Le\u00b7ben", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Linde bin ich, eine gute Amme,", "tokens": ["Lin\u00b7de", "bin", "ich", ",", "ei\u00b7ne", "gu\u00b7te", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Tr\u00e4nke dich mit Tr\u00e4umen, \u2013 kleine Flamme,", "tokens": ["Tr\u00e4n\u00b7ke", "dich", "mit", "Tr\u00e4u\u00b7men", ",", "\u2013", "klei\u00b7ne", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schlafe, schlaf auf meinem Schoo\u00dfe ein.", "tokens": ["Schla\u00b7fe", ",", "schlaf", "auf", "mei\u00b7nem", "Schoo\u00b7\u00dfe", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}