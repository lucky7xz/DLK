{"textgrid.poem.36220": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Traum", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im dunkelgr\u00fcnen Walde", "tokens": ["Tief", "im", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War ein Leu von hohem Mute;", "tokens": ["War", "ein", "Leu", "von", "ho\u00b7hem", "Mu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser liebte seinen Herren,", "tokens": ["Die\u00b7ser", "lieb\u00b7te", "sei\u00b7nen", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War ihm treu von Herzensgrunde.", "tokens": ["War", "ihm", "treu", "von", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Tiere ritt der J\u00e4ger,", "tokens": ["Auf", "dem", "Tie\u00b7re", "ritt", "der", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprengte durch die gr\u00fcnen Fluren;", "tokens": ["Spreng\u00b7te", "durch", "die", "gr\u00fc\u00b7nen", "Flu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollt' er auf den R\u00fccken springen,", "tokens": ["Wollt'", "er", "auf", "den", "R\u00fc\u00b7cken", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stand der L\u00f6we ganz geduldig.", "tokens": ["Stand", "der", "L\u00f6\u00b7we", "ganz", "ge\u00b7dul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch nach vielen Tagen einmal,", "tokens": ["Doch", "nach", "vie\u00b7len", "Ta\u00b7gen", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Da er wieder es versuchte,", "tokens": ["Da", "er", "wie\u00b7der", "es", "ver\u00b7such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "Ward die alte Wildheit rege,", "tokens": ["Ward", "die", "al\u00b7te", "Wild\u00b7heit", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dreht' er sich in zorn'gem Mute,", "tokens": ["Dreht'", "er", "sich", "in", "zorn'\u00b7gem", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Als ob er ihn t\u00f6ten wollte,", "tokens": ["Als", "ob", "er", "ihn", "t\u00f6\u00b7ten", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Seinen Herrn, den lieben, guten.", "tokens": ["Sei\u00b7nen", "Herrn", ",", "den", "lie\u00b7ben", ",", "gu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch alsbalde ward er's inne,", "tokens": ["Doch", "als\u00b7bal\u00b7de", "ward", "er's", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und da war er still und ruhig.", "tokens": ["Und", "da", "war", "er", "still", "und", "ru\u00b7hig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Traurig senkt' er nun die Blicke,", "tokens": ["Trau\u00b7rig", "senkt'", "er", "nun", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und es nagt' ihn bittrer Kummer,", "tokens": ["Und", "es", "nagt'", "ihn", "bit\u00b7trer", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df er seinem Herren z\u00fcrnte,", "tokens": ["Da\u00df", "er", "sei\u00b7nem", "Her\u00b7ren", "z\u00fcrn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ihn gar balde h\u00e4tt' verwundet;", "tokens": ["Ihn", "gar", "bal\u00b7de", "h\u00e4tt'", "ver\u00b7wun\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Das zernagt sein gro\u00dfes Herz ihm,", "tokens": ["Das", "zer\u00b7nagt", "sein", "gro\u00b7\u00dfes", "Herz", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und es wird ihm immer dunkler.", "tokens": ["Und", "es", "wird", "ihm", "im\u00b7mer", "dunk\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Nieder legt er sich zu Boden,", "tokens": ["Nie\u00b7der", "legt", "er", "sich", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Hingestreckt auf hartem Grunde,", "tokens": ["Hin\u00b7ge\u00b7streckt", "auf", "har\u00b7tem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Liegt er da zehntausend Jahre,", "tokens": ["Liegt", "er", "da", "zehn\u00b7tau\u00b7send", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wie von Schmerz und Reu verwundet,", "tokens": ["Wie", "von", "Schmerz", "und", "Reu", "ver\u00b7wun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Achtet nicht der Freundes Reden,", "tokens": ["Ach\u00b7tet", "nicht", "der", "Freun\u00b7des", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ganz versteint in herbem Kummer.", "tokens": ["Ganz", "ver\u00b7steint", "in", "her\u00b7bem", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}