{"dta.poem.23545": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der dreizehnte Artikel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und seid ihr auch in Jugendfrische,", "tokens": ["Und", "seid", "ihr", "auch", "in", "Ju\u00b7gend\u00b7fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ganz gesund, noch gar nicht alt \u2014", "tokens": ["Noch", "ganz", "ge\u00b7sund", ",", "noch", "gar", "nicht", "alt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo einmal dreizehn sind bei Tische,", "tokens": ["Wo", "ein\u00b7mal", "drei\u00b7zehn", "sind", "bei", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "CARD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stirbt einer von den dreizehn bald.", "tokens": ["Stirbt", "ei\u00b7ner", "von", "den", "drei\u00b7zehn", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "CARD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So ging es, als der Bundesacte", "tokens": ["So", "ging", "es", ",", "als", "der", "Bun\u00b7de\u00b7sac\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dreizehnter mit bei Tische sa\u00df:", "tokens": ["Drei\u00b7zehn\u00b7ter", "mit", "bei", "Ti\u00b7sche", "sa\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df da der Tod den J\u00fcngsten packte!", "tokens": ["Da\u00df", "da", "der", "Tod", "den", "J\u00fcng\u00b7sten", "pack\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O weh! das war ein schlechter Spa\u00df.", "tokens": ["O", "weh", "!", "das", "war", "ein", "schlech\u00b7ter", "Spa\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}