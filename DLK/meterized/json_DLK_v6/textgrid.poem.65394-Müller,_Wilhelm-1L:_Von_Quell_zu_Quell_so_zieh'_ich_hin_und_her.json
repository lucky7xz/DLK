{"textgrid.poem.65394": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Quell zu Quell so zieh' ich hin und her", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Quell zu Quell so zieh' ich hin und her", "tokens": ["Von", "Quell", "zu", "Quell", "so", "zieh'", "ich", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und finde hier und dort mein Heil nicht mehr.", "tokens": ["Und", "fin\u00b7de", "hier", "und", "dort", "mein", "Heil", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist die Nymphe, die in Purpurschalen", "tokens": ["Du", "bist", "die", "Nym\u00b7phe", ",", "die", "in", "Pur\u00b7pur\u00b7scha\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Wundertrank bewahrt f\u00fcr meine Qualen.", "tokens": ["Den", "Wun\u00b7der\u00b7trank", "be\u00b7wahrt", "f\u00fcr", "mei\u00b7ne", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O la\u00df ihn bald aus deinem Herzen springen", "tokens": ["O", "la\u00df", "ihn", "bald", "aus", "dei\u00b7nem", "Her\u00b7zen", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und voll zu seiner s\u00fc\u00dfen M\u00fcndung dringen!", "tokens": ["Und", "voll", "zu", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "M\u00fcn\u00b7dung", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Becher werf' ich weg; mit meinen Lippen", "tokens": ["Den", "Be\u00b7cher", "wer\u00b7f'", "ich", "weg", ";", "mit", "mei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Will ich des Sprudels erste Perle nippen.", "tokens": ["Will", "ich", "des", "Spru\u00b7dels", "ers\u00b7te", "Per\u00b7le", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}