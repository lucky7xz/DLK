{"dta.poem.10134": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich die gr\u00fcne Pracht der B\u00e4ume z\u00e4rtlich liebe,", "tokens": ["Da", "ich", "die", "gr\u00fc\u00b7ne", "Pracht", "der", "B\u00e4u\u00b7me", "z\u00e4rt\u00b7lich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und folglich mich anietzt im Herbst, bey ihrem Fall,", "tokens": ["Und", "folg\u00b7lich", "mich", "an\u00b7ietzt", "im", "Herbst", ",", "bey", "ih\u00b7rem", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey der Entbl\u00e4tterung der Wipfel \u00fcberall,", "tokens": ["Bey", "der", "Ent\u00b7bl\u00e4t\u00b7te\u00b7rung", "der", "Wip\u00b7fel", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und der Vernichtigung des Laubes recht betr\u00fcbe;", "tokens": ["Und", "der", "Ver\u00b7nich\u00b7ti\u00b7gung", "des", "Lau\u00b7bes", "recht", "be\u00b7tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "So deucht mir doch, ob h\u00f6r\u2019 ich sie, im fallen,", "tokens": ["So", "deucht", "mir", "doch", ",", "ob", "h\u00f6r'", "ich", "sie", ",", "im", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "VVFIN", "PPER", "PPER", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu meinem Troste die\u00df, mit sanftem Lispeln lallen:", "tokens": ["Zu", "mei\u00b7nem", "Tros\u00b7te", "die\u00df", ",", "mit", "sanf\u00b7tem", "Lis\u00b7peln", "lal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PDS", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edu siehest uns, von dem geliebten Baum,", "tokens": ["\u201e", "du", "sie\u00b7hest", "uns", ",", "von", "dem", "ge\u00b7lieb\u00b7ten", "Baum", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u201enicht, \u00fcm denselben zu entkleiden,", "tokens": ["\u201e", "nicht", ",", "\u00fcm", "den\u00b7sel\u00b7ben", "zu", "ent\u00b7klei\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "$,", "APPRART", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u201enoch \u00fcm ihn nackt und blo\u00df zu lassen, scheiden;", "tokens": ["\u201e", "noch", "\u00fcm", "ihn", "nackt", "und", "blo\u00df", "zu", "las\u00b7sen", ",", "schei\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "ADJD", "KON", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u201each nein, wir machen frisch- und sch\u00f6nern Bl\u00e4ttern Raum.", "tokens": ["\u201e", "ach", "nein", ",", "wir", "ma\u00b7chen", "frisch", "und", "sch\u00f6\u00b7nern", "Bl\u00e4t\u00b7tern", "Raum", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKANT", "$,", "PPER", "VVINF", "TRUNC", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}