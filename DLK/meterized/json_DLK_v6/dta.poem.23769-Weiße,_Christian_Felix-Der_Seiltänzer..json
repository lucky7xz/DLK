{"dta.poem.23769": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Seilt\u00e4nzer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab ihn gesehen,", "tokens": ["Ich", "hab", "ihn", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den k\u00fcnstlichen Mann,", "tokens": ["Den", "k\u00fcnst\u00b7li\u00b7chen", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf einem Seile gehen,", "tokens": ["Auf", "ei\u00b7nem", "Sei\u00b7le", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gut ichs auf der Ebne kann.", "tokens": ["So", "gut", "ichs", "auf", "der", "Eb\u00b7ne", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich mu\u00df es wohl sagen,", "tokens": ["Ich", "mu\u00df", "es", "wohl", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das fordert viel M\u00fch:", "tokens": ["Das", "for\u00b7dert", "viel", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch m\u00f6cht ich etwas fragen:", "tokens": ["Doch", "m\u00f6cht", "ich", "et\u00b7was", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die seltne Kunst \u2014 was n\u00fctzet sie?", "tokens": ["Die", "selt\u00b7ne", "Kunst", "was", "n\u00fct\u00b7zet", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWS", "VVFIN", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}