{"textgrid.poem.45831": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ist des Lebens Band mit Schmerz gel\u00f6set]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist des Lebens Band mit Schmerz gel\u00f6set,", "tokens": ["Ist", "des", "Le\u00b7bens", "Band", "mit", "Schmerz", "ge\u00b7l\u00f6\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liegt der K\u00f6rper ohne Blick, ohn' Leben,", "tokens": ["Liegt", "der", "K\u00f6r\u00b7per", "oh\u00b7ne", "Blick", ",", "ohn'", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fremde Liebe weint, und er geneset.", "tokens": ["Frem\u00b7de", "Lie\u00b7be", "weint", ",", "und", "er", "ge\u00b7ne\u00b7set", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "KON", "PPER", "VVPP", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Seine Liebe mu\u00df zum Himmel schweben,", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "mu\u00df", "zum", "Him\u00b7mel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Von dem tr\u00e4gen Leibe keusch entbl\u00f6\u00dfet,", "tokens": ["Von", "dem", "tr\u00e4\u00b7gen", "Lei\u00b7be", "keusch", "ent\u00b7bl\u00f6\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kann zu Gott der Engel sie erheben.", "tokens": ["Kann", "zu", "Gott", "der", "En\u00b7gel", "sie", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und er h\u00e4lt sie mit dem Arm umfasset,", "tokens": ["Und", "er", "h\u00e4lt", "sie", "mit", "dem", "Arm", "um\u00b7fas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schwebet h\u00f6her, bis das Grab erblasset.", "tokens": ["Schwe\u00b7bet", "h\u00f6\u00b7her", ",", "bis", "das", "Grab", "er\u00b7blas\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ist er durchs Verg\u00e4ngliche gedrungen,", "tokens": ["Ist", "er", "durchs", "Ver\u00b7g\u00e4ng\u00b7li\u00b7che", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kehrt die Seele in die Ewigkeit,", "tokens": ["Kehrt", "die", "See\u00b7le", "in", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O, so ist dem Tod genug gelungen,", "tokens": ["O", ",", "so", "ist", "dem", "Tod", "ge\u00b7nug", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und er st\u00fcrzet r\u00fcckw\u00e4rts in die Zeit.", "tokens": ["Und", "er", "st\u00fcr\u00b7zet", "r\u00fcck\u00b7w\u00e4rts", "in", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Um die Seele bleibet Wonn' geschlungen,", "tokens": ["Um", "die", "See\u00b7le", "blei\u00b7bet", "Wonn'", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Alles giebt sich ihr, die alles beut,", "tokens": ["Al\u00b7les", "giebt", "sich", "ihr", ",", "die", "al\u00b7les", "beut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PPER", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wird zum ew'gen Geben und Empfangen,", "tokens": ["Wird", "zum", "ew'\u00b7gen", "Ge\u00b7ben", "und", "Emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kann des Wechsels Ende nie erlangen.", "tokens": ["Kann", "des", "Wech\u00b7sels", "En\u00b7de", "nie", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}