{"textgrid.poem.64335": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebliche Bl\u00fcmchen, die am kalten Busen", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebliche Bl\u00fcmchen, die am kalten Busen", "tokens": ["Lieb\u00b7li\u00b7che", "Bl\u00fcm\u00b7chen", ",", "die", "am", "kal\u00b7ten", "Bu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Noch das sterbende Jahr mit Liebe hegte,", "tokens": ["Noch", "das", "ster\u00b7ben\u00b7de", "Jahr", "mit", "Lie\u00b7be", "heg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Novembers rauschender, starrer Fittich", "tokens": ["Die", "No\u00b7vem\u00b7bers", "rau\u00b7schen\u00b7der", ",", "star\u00b7rer", "Fit\u00b7tich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schonend vorbeiflog:", "tokens": ["Scho\u00b7nend", "vor\u00b7bei\u00b7flog", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Seid mir gesegnet, Bl\u00fcmchen! Ich verstehe", "tokens": ["Seid", "mir", "ge\u00b7seg\u00b7net", ",", "Bl\u00fcm\u00b7chen", "!", "Ich", "ver\u00b7ste\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euren winkenden Wunsch: ich will euch pfl\u00fccken;", "tokens": ["Eu\u00b7ren", "win\u00b7ken\u00b7den", "Wunsch", ":", "ich", "will", "euch", "pfl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "An den Herzen klopfender Unschuld, Bl\u00fcmchen,", "tokens": ["An", "den", "Her\u00b7zen", "klop\u00b7fen\u00b7der", "Un\u00b7schuld", ",", "Bl\u00fcm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sollt ihr verbl\u00fchen!", "tokens": ["Sollt", "ihr", "ver\u00b7bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Duftet indessen s\u00fc\u00df, und lispelt freundlich", "tokens": ["Duf\u00b7tet", "in\u00b7des\u00b7sen", "s\u00fc\u00df", ",", "und", "lis\u00b7pelt", "freund\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Diesen klopfenden Herzen sanfter Unschuld:", "tokens": ["Die\u00b7sen", "klop\u00b7fen\u00b7den", "Her\u00b7zen", "sanf\u00b7ter", "Un\u00b7schuld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da\u00df der Freundschaft z\u00e4rtliche Hand euch heute", "tokens": ["Da\u00df", "der", "Freund\u00b7schaft", "z\u00e4rt\u00b7li\u00b7che", "Hand", "euch", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "PPER", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Sorgsam gepfl\u00fcckt hat.", "tokens": ["Sorg\u00b7sam", "ge\u00b7pfl\u00fcckt", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}