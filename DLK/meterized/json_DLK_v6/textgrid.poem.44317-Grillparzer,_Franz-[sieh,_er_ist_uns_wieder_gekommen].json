{"textgrid.poem.44317": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[sieh, er ist uns wieder gekommen]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, er ist uns wieder gekommen,", "tokens": ["Sieh", ",", "er", "ist", "uns", "wie\u00b7der", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "der heitere, festliche Tag.", "tokens": ["der", "hei\u00b7te\u00b7re", ",", "fest\u00b7li\u00b7che", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Vieles hat uns das Schicksal genommen,", "tokens": ["Vie\u00b7les", "hat", "uns", "das", "Schick\u00b7sal", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ach, wir litten so manchen Schlag,", "tokens": ["ach", ",", "wir", "lit\u00b7ten", "so", "man\u00b7chen", "Schlag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "seit ein kurzes Jahr entschwunden,", "tokens": ["seit", "ein", "kur\u00b7zes", "Jahr", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und noch bluten frische Wunden.", "tokens": ["und", "noch", "blu\u00b7ten", "fri\u00b7sche", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einen Bruder hinausgetrieben", "tokens": ["Ei\u00b7nen", "Bru\u00b7der", "hin\u00b7aus\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "in das Get\u00fcmmel der tobenden Welt,", "tokens": ["in", "das", "Ge\u00b7t\u00fcm\u00b7mel", "der", "to\u00b7ben\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "fern von allen, die ihn lieben,", "tokens": ["fern", "von", "al\u00b7len", ",", "die", "ihn", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf des Krieges blutigem Feld,", "tokens": ["auf", "des", "Krie\u00b7ges", "blu\u00b7ti\u00b7gem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "und \u2013 ach, la\u00df mich weinen und schweigen,", "tokens": ["und", "\u2013", "ach", ",", "la\u00df", "mich", "wei\u00b7nen", "und", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "XY", "$,", "VVIMP", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "soll ich auch ", "tokens": ["soll", "ich", "auch"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Doch es konnt uns nicht alles rauben,", "tokens": ["Doch", "es", "konnt", "uns", "nicht", "al\u00b7les", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "das verfolgende, harte Geschick,", "tokens": ["das", "ver\u00b7fol\u00b7gen\u00b7de", ",", "har\u00b7te", "Ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Liebe lie\u00df es uns und den Glauben", "tokens": ["Lie\u00b7be", "lie\u00df", "es", "uns", "und", "den", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "KON", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "an ein k\u00fcnftiges, neues Gl\u00fcck,", "tokens": ["an", "ein", "k\u00fcnf\u00b7ti\u00b7ges", ",", "neu\u00b7es", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Vertraun und Geduld und reine Herzen;", "tokens": ["Ver\u00b7traun", "und", "Ge\u00b7duld", "und", "rei\u00b7ne", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "O! das entwaffnet die herbsten Schmerzen!", "tokens": ["O", "!", "das", "ent\u00b7waff\u00b7net", "die", "herbs\u00b7ten", "Schmer\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}