{"textgrid.poem.60870": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Der deutsche Hexameter", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du Chor\u00e4n einreihst, statt voller Spond\u00e4n, es entsteht dann", "tokens": ["Wenn", "du", "Cho\u00b7r\u00e4n", "ein\u00b7reihst", ",", "statt", "vol\u00b7ler", "Spon\u00b7d\u00e4n", ",", "es", "ent\u00b7steht", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "KOUI", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ein zwar schw\u00e4chlicher stets, aber verzeihlicher Vers:", "tokens": ["Ein", "zwar", "schw\u00e4ch\u00b7li\u00b7cher", "stets", ",", "a\u00b7ber", "ver\u00b7zeih\u00b7li\u00b7cher", "Vers", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn du jedoch bleischwere Spond\u00e4n als Daktylusanfang", "tokens": ["Wenn", "du", "je\u00b7doch", "blei\u00b7schwe\u00b7re", "Spon\u00b7d\u00e4n", "als", "Dak\u00b7ty\u00b7lus\u00b7an\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "KOUS", "NN"], "meter": "--+-++-++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Einreihst, mitleidslos wirst du zerfleischen das Ohr.", "tokens": ["Ein\u00b7reihst", ",", "mit\u00b7leids\u00b7los", "wirst", "du", "zer\u00b7flei\u00b7schen", "das", "Ohr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}