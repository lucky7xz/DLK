{"textgrid.poem.48638": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dieses feurige Beginnen,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieses feurige Beginnen,", "tokens": ["Die\u00b7ses", "feu\u00b7ri\u00b7ge", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieser seufzerhei\u00dfe Dunst", "tokens": ["die\u00b7ser", "seuf\u00b7zer\u00b7hei\u00b7\u00dfe", "Dunst"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ist nicht, Lieb, nach deinen Sinnen", "tokens": ["ist", "nicht", ",", "Lieb", ",", "nach", "dei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ein' Erfrischung meiner Brunst.", "tokens": ["ein'", "Er\u00b7fri\u00b7schung", "mei\u00b7ner", "Brunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich mu\u00df sie, recht zu bekennen,", "tokens": ["Ich", "mu\u00df", "sie", ",", "recht", "zu", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "ungest\u00fcme St\u00fcrme nennen.", "tokens": ["un\u00b7ge\u00b7st\u00fc\u00b7me", "St\u00fcr\u00b7me", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Wenn sie auf die dicken Flammen", "tokens": ["Wenn", "sie", "auf", "die", "di\u00b7cken", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meiner Liebe blasen zu,", "tokens": ["mei\u00b7ner", "Lie\u00b7be", "bla\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "so schl\u00e4gt \u00fcber mich zusammen", "tokens": ["so", "schl\u00e4gt", "\u00fc\u00b7ber", "mich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihrer Wirbel ganze Loh'.", "tokens": ["ih\u00b7rer", "Wir\u00b7bel", "gan\u00b7ze", "Loh'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also duppeln sie die Hitze,", "tokens": ["Al\u00b7so", "dup\u00b7peln", "sie", "die", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df ich warme Tr\u00e4nen schwitze.", "tokens": ["da\u00df", "ich", "war\u00b7me", "Tr\u00e4\u00b7nen", "schwit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn auf di\u00df seh' ich sich schwingen", "tokens": ["Denn", "auf", "di\u00df", "seh'", "ich", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDS", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "dicke schwarze Wolken auf,", "tokens": ["di\u00b7cke", "schwar\u00b7ze", "Wol\u00b7ken", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "so uns armen Bulern bringen", "tokens": ["so", "uns", "ar\u00b7men", "Bu\u00b7lern", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leid und Pein und Schmerz vollauf,", "tokens": ["Leid", "und", "Pein", "und", "Schmerz", "vol\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df wir nichts als Angst begegnen", "tokens": ["da\u00df", "wir", "nichts", "als", "Angst", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und mit steten Tr\u00e4nen regnen.", "tokens": ["und", "mit", "ste\u00b7ten", "Tr\u00e4\u00b7nen", "reg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}