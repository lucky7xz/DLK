{"textgrid.poem.52189": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Konradins Knappe", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbauf diesem kurzen Bergesrasen hier,", "tokens": ["\u00bb", "auf", "die\u00b7sem", "kur\u00b7zen", "Ber\u00b7ges\u00b7ra\u00b7sen", "hier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur wen'ge Monde sind es, zechten wir,", "tokens": ["Nur", "wen'\u00b7ge", "Mon\u00b7de", "sind", "es", ",", "zech\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er und das Edelvolk, in hohem Raum \u2013", "tokens": ["Er", "und", "das", "E\u00b7del\u00b7volk", ",", "in", "ho\u00b7hem", "Raum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcben war Italien wie ein Traum.", "tokens": ["Und", "dr\u00fc\u00b7ben", "war", "I\u00b7ta\u00b7li\u00b7en", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "In diesem Passe lagen wir gestreckt,", "tokens": ["In", "die\u00b7sem", "Pas\u00b7se", "la\u00b7gen", "wir", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Staufe hat mich minniglich geneckt:", "tokens": ["Der", "Stau\u00b7fe", "hat", "mich", "min\u00b7nig\u00b7lich", "ge\u00b7neckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u203anicht bl\u00f6de, Hans! Sprich! Was begehrst du gleich?", "tokens": ["\u203a", "nicht", "bl\u00f6\u00b7de", ",", "Hans", "!", "Sprich", "!", "Was", "be\u00b7gehrst", "du", "gleich", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADJA", "$,", "NE", "$.", "VVIMP", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich geb es dir in meinem K\u00f6nigreich!\u2039", "tokens": ["Ich", "geb", "es", "dir", "in", "mei\u00b7nem", "K\u00f6\u00b7nig\u00b7reich", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann klomm die Fahrt an W\u00e4nden schwarz und kahl!", "tokens": ["Dann", "klomm", "die", "Fahrt", "an", "W\u00e4n\u00b7den", "schwarz", "und", "kahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich der Mutter Gottes mich empfahl.", "tokens": ["Wo", "ich", "der", "Mut\u00b7ter", "Got\u00b7tes", "mich", "emp\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch eh ich Amen sagte, glitt mein Tier \u2013", "tokens": ["Noch", "eh", "ich", "A\u00b7men", "sag\u00b7te", ",", "glitt", "mein", "Tier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Staufen und die Sinne schwanden mir.", "tokens": ["Der", "Stau\u00b7fen", "und", "die", "Sin\u00b7ne", "schwan\u00b7den", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann lag ich im Hospize fieberbang,", "tokens": ["Dann", "lag", "ich", "im", "Hos\u00b7pi\u00b7ze", "fie\u00b7ber\u00b7bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo ich verzweifelnd mit den M\u00f6nchen rang,", "tokens": ["Wo", "ich", "ver\u00b7zwei\u00b7felnd", "mit", "den", "M\u00f6n\u00b7chen", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich focht und schrie: \u203aDem jungen Staufen nach!", "tokens": ["Ich", "focht", "und", "schrie", ":", "\u203a", "Dem", "jun\u00b7gen", "Stau\u00b7fen", "nach", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hie Napoli!\u2039 Bis ich zusammenbrach.", "tokens": ["Hie", "Na\u00b7po\u00b7li", "!", "\u2039", "Bis", "ich", "zu\u00b7sam\u00b7men\u00b7brach", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$.", "$(", "KOUS", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Jetzt schlepp ich jeden Tag mich hier empor,", "tokens": ["Jetzt", "schlepp", "ich", "je\u00b7den", "Tag", "mich", "hier", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich den Staufen aus dem Blick verlor.", "tokens": ["Wo", "ich", "den", "Stau\u00b7fen", "aus", "dem", "Blick", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Genesen ist der Leib, die Seele schmerzt,", "tokens": ["Ge\u00b7ne\u00b7sen", "ist", "der", "Leib", ",", "die", "See\u00b7le", "schmerzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn all mein Erdengl\u00fcck hab ich verscherzt.", "tokens": ["Denn", "all", "mein", "Er\u00b7den\u00b7gl\u00fcck", "hab", "ich", "ver\u00b7scherzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Und z\u00f6g ich heut, ich k\u00e4me doch zu sp\u00e4t,", "tokens": ["Und", "z\u00f6g", "ich", "heut", ",", "ich", "k\u00e4\u00b7me", "doch", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon kr\u00f6nte sich die junge Majest\u00e4t,", "tokens": ["Schon", "kr\u00f6n\u00b7te", "sich", "die", "jun\u00b7ge", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Edelblut empfing den Ritterschlag,", "tokens": ["Das", "E\u00b7del\u00b7blut", "emp\u00b7fing", "den", "Rit\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich aber fluche meinem Ungl\u00fcckstag.\u00ab \u2013", "tokens": ["Ich", "a\u00b7ber", "flu\u00b7che", "mei\u00b7nem", "Un\u00b7gl\u00fccks\u00b7tag", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Knechtlein kommt berg\u00fcber. \u00bbGib Bescheid!", "tokens": ["Ein", "Knecht\u00b7lein", "kommt", "berg\u00b7\u00fc\u00b7ber", ".", "\u00bb", "Gib", "Be\u00b7scheid", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$(", "VVIMP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Staufenknabe thront in Herrlichkeit?\u00ab \u2013", "tokens": ["Der", "Stau\u00b7fen\u00b7kna\u00b7be", "thront", "in", "Herr\u00b7lich\u00b7keit", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbja, Herr. Er litt gemach den Todesstreich", "tokens": ["\u00bb", "ja", ",", "Herr", ".", "Er", "litt", "ge\u00b7mach", "den", "To\u00b7des\u00b7streich"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und thront getr\u00f6stet nun im Himmelreich.\u00ab", "tokens": ["Und", "thront", "ge\u00b7tr\u00f6s\u00b7tet", "nun", "im", "Him\u00b7mel\u00b7reich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "VVPP", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}