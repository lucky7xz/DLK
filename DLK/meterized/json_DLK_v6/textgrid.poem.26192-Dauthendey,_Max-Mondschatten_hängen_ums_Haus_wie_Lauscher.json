{"textgrid.poem.26192": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mondschatten h\u00e4ngen ums Haus wie Lauscher", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mondschatten h\u00e4ngen ums Haus wie Lauscher,", "tokens": ["Mond\u00b7schat\u00b7ten", "h\u00e4n\u00b7gen", "ums", "Haus", "wie", "Lau\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KOKOM", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die ihre Ohren an die L\u00e4den dr\u00e4ngen.", "tokens": ["Die", "ih\u00b7re", "Oh\u00b7ren", "an", "die", "L\u00e4\u00b7den", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Mond begleitete uns in den Baumg\u00e4ngen,", "tokens": ["Der", "Mond", "be\u00b7glei\u00b7te\u00b7te", "uns", "in", "den", "Baum\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+++-++-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Wo die B\u00fcsche hockten wie V\u00f6gel mit Federn und F\u00e4ngen.", "tokens": ["Wo", "die", "B\u00fc\u00b7sche", "hock\u00b7ten", "wie", "V\u00f6\u00b7gel", "mit", "Fe\u00b7dern", "und", "F\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KOKOM", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Am Tag wollen alle Dinge erscheinen und gefallen,", "tokens": ["Am", "Tag", "wol\u00b7len", "al\u00b7le", "Din\u00b7ge", "er\u00b7schei\u00b7nen", "und", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIAT", "NN", "VVINF", "KON", "VVPP", "$,"], "meter": "-++-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Aber nachts nur lauschen und pl\u00f6tzlich erschallen.", "tokens": ["A\u00b7ber", "nachts", "nur", "lau\u00b7schen", "und", "pl\u00f6tz\u00b7lich", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Alle B\u00e4ume beginnen auf einmal zu rauschen,", "tokens": ["Al\u00b7le", "B\u00e4u\u00b7me", "be\u00b7gin\u00b7nen", "auf", "ein\u00b7mal", "zu", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Als greifen H\u00e4nde hinein und sch\u00fctteln drinnen,", "tokens": ["Als", "grei\u00b7fen", "H\u00e4n\u00b7de", "hin\u00b7ein", "und", "sch\u00fct\u00b7teln", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und die Nacht ist ersch\u00fcttert vom Erwachen und Besinnen.", "tokens": ["Und", "die", "Nacht", "ist", "er\u00b7sch\u00fct\u00b7tert", "vom", "Er\u00b7wa\u00b7chen", "und", "Be\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}