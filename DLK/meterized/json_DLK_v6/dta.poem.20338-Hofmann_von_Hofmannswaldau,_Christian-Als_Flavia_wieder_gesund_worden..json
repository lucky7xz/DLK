{"dta.poem.20338": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Als Flavia wieder gesund worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist di\u00df nicht Flavia? Ihr augen irret ihr?", "tokens": ["Ist", "di\u00df", "nicht", "Fla\u00b7via", "?", "Ihr", "au\u00b7gen", "ir\u00b7ret", "ihr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NE", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Schau ich den sch\u00f6nen glantz der sonnen nicht f\u00fcr mir?", "tokens": ["Schau", "ich", "den", "sch\u00f6\u00b7nen", "glantz", "der", "son\u00b7nen", "nicht", "f\u00fcr", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "PTKNEG", "APPR", "PPER", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der sonnen/ die der tod mit ungemeinen flecken", "tokens": ["Der", "son\u00b7nen", "/", "die", "der", "tod", "mit", "un\u00b7ge\u00b7mei\u00b7nen", "fle\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Unl\u00e4ngst bem\u00fchet war mir neidisch zu verdecken?", "tokens": ["Un\u00b7l\u00e4ngst", "be\u00b7m\u00fc\u00b7het", "war", "mir", "nei\u00b7disch", "zu", "ver\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Es ist ja Flavia/ o angenehmer tag!", "tokens": ["Es", "ist", "ja", "Fla\u00b7via", "/", "o", "an\u00b7ge\u00b7neh\u00b7mer", "tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da ich/ o sonne dich/ als adler schauen mag/", "tokens": ["Da", "ich", "/", "o", "son\u00b7ne", "dich", "/", "als", "ad\u00b7ler", "schau\u00b7en", "mag", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "FM", "KON", "PPER", "$(", "KOUS", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und meine liechter kan in deinen strahlen weiden.", "tokens": ["Und", "mei\u00b7ne", "liech\u00b7ter", "kan", "in", "dei\u00b7nen", "strah\u00b7len", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verzeihe Flavia/ red\u2019 ich zu unbescheiden.", "tokens": ["Ver\u00b7zei\u00b7he", "Fla\u00b7via", "/", "red'", "ich", "zu", "un\u00b7be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die freyheit leget mich mit k\u00fcnheits-fl\u00fcgeln an/", "tokens": ["Die", "frey\u00b7heit", "le\u00b7get", "mich", "mit", "k\u00fcn\u00b7heits\u00b7fl\u00fc\u00b7geln", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und f\u00fchret mich zu dem/ was nicht vergehen kan/", "tokens": ["Und", "f\u00fch\u00b7ret", "mich", "zu", "dem", "/", "was", "nicht", "ver\u00b7ge\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "$(", "PWS", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und mich zur asche macht. Ich hoffe bey den s\u00fcnden", "tokens": ["Und", "mich", "zur", "asc\u00b7he", "macht", ".", "Ich", "hof\u00b7fe", "bey", "den", "s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "(so lieben irrthum ist) genade noch zu finden.", "tokens": ["(", "so", "lie\u00b7ben", "irr\u00b7thum", "ist", ")", "ge\u00b7na\u00b7de", "noch", "zu", "fin\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "VAFIN", "$(", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo freundschafft fehler ist/ so heist es sch\u00f6ne schuld/", "tokens": ["Wo", "freund\u00b7schafft", "feh\u00b7ler", "ist", "/", "so", "heist", "es", "sch\u00f6\u00b7ne", "schuld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja selbst der himmel hat mit solcher noth gedult.", "tokens": ["Ja", "selbst", "der", "him\u00b7mel", "hat", "mit", "sol\u00b7cher", "noth", "ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo lauff ich aber hin? O freundin! meine sinnen", "tokens": ["Wo", "lauff", "ich", "a\u00b7ber", "hin", "?", "O", "freun\u00b7din", "!", "mei\u00b7ne", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "NE", "PTKVZ", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die klagen/ da\u00df sie sich nicht gnugsam freuen k\u00f6nnen.", "tokens": ["Die", "kla\u00b7gen", "/", "da\u00df", "sie", "sich", "nicht", "gnug\u00b7sam", "freu\u00b7en", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein auge trauet ihm itzund fast selber nicht/", "tokens": ["Mein", "au\u00b7ge", "trau\u00b7et", "ihm", "it\u00b7zund", "fast", "sel\u00b7ber", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Es zweiffelt/ ob der schein/ es zweiffelt ob das licht", "tokens": ["Es", "zweif\u00b7felt", "/", "ob", "der", "schein", "/", "es", "zweif\u00b7felt", "ob", "das", "licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "$(", "PPER", "VVFIN", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hier recht nat\u00fcrlich sey. Es f\u00fcrchtet/ da\u00df die kertzen", "tokens": ["Hier", "recht", "na\u00b7t\u00fcr\u00b7lich", "sey", ".", "Es", "f\u00fcrch\u00b7tet", "/", "da\u00df", "die", "kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "$.", "PPER", "VVFIN", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der tod hat angesteckt/ zu mehren meine schmertzen:", "tokens": ["Der", "tod", "hat", "an\u00b7ge\u00b7steckt", "/", "zu", "meh\u00b7ren", "mei\u00b7ne", "schmert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "PTKZU", "VVINF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Als rufft er mir zum hohn und mehrung meiner noth/", "tokens": ["Als", "rufft", "er", "mir", "zum", "hohn", "und", "meh\u00b7rung", "mei\u00b7ner", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was vor die liebe that/ das thut itzund der tod.", "tokens": ["Was", "vor", "die", "lie\u00b7be", "that", "/", "das", "thut", "it\u00b7zund", "der", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VVFIN", "$(", "PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nein! Nein! Hier ist kein tod. Der reinen r\u00f6the prangen/", "tokens": ["Nein", "!", "Nein", "!", "Hier", "ist", "kein", "tod", ".", "Der", "rei\u00b7nen", "r\u00f6\u00b7the", "pran\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "ADV", "VAFIN", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das lebet hier so gut/ als auff Aurorens wangen:", "tokens": ["Das", "le\u00b7bet", "hier", "so", "gut", "/", "als", "auff", "Au\u00b7ro\u00b7rens", "wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "$(", "KOKOM", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die rosen/ die itzund auff deinen lippen stehn/", "tokens": ["Die", "ro\u00b7sen", "/", "die", "it\u00b7zund", "auff", "dei\u00b7nen", "lip\u00b7pen", "stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die w\u00e4hren unverdeckt/ und konten nicht vergehn.", "tokens": ["Die", "w\u00e4h\u00b7ren", "un\u00b7ver\u00b7deckt", "/", "und", "kon\u00b7ten", "nicht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$(", "KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der himmel dem du gleichst/ der gibt dich sch\u00f6ner wieder/", "tokens": ["Der", "him\u00b7mel", "dem", "du", "gleichst", "/", "der", "gibt", "dich", "sch\u00f6\u00b7ner", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "$(", "ART", "VVFIN", "PPER", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als er dich von uns ri\u00df; Ich schaue deine glieder", "tokens": ["Als", "er", "dich", "von", "uns", "ri\u00df", ";", "Ich", "schau\u00b7e", "dei\u00b7ne", "glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "(es kan kein irrthum seyn) umgl\u00e4ntzt mit solcher pracht/", "tokens": ["(", "es", "kan", "kein", "irr\u00b7thum", "seyn", ")", "um\u00b7gl\u00e4ntzt", "mit", "sol\u00b7cher", "pracht", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PIAT", "NN", "VAINF", "$(", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die mich heist knechtisch seyn/ und dich zur g\u00f6ttin macht.", "tokens": ["Die", "mich", "heist", "knech\u00b7tisch", "seyn", "/", "und", "dich", "zur", "g\u00f6t\u00b7tin", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ADJD", "VAINF", "$(", "KON", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Geehrte Flavia! Sey lange so zu schauen!", "tokens": ["Ge\u00b7ehr\u00b7te", "Fla\u00b7via", "!", "Sey", "lan\u00b7ge", "so", "zu", "schau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Die freude woll auff dich ihr wohn- und lust-hau\u00df bauen/", "tokens": ["Die", "freu\u00b7de", "woll", "auff", "dich", "ihr", "wohn", "und", "lust\u00b7hau\u00df", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PPOSAT", "TRUNC", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der jugend sch\u00f6ner lentz/ der sch\u00f6nheit sonnenschein/", "tokens": ["Der", "ju\u00b7gend", "sch\u00f6\u00b7ner", "lentz", "/", "der", "sch\u00f6n\u00b7heit", "son\u00b7nen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der m\u00fcsse lange zeit dein treuer nachbar seyn.", "tokens": ["Der", "m\u00fcs\u00b7se", "lan\u00b7ge", "zeit", "dein", "treu\u00b7er", "nach\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "NN", "PPOSAT", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein auge schaue nichts/ als nur gel\u00fccke bl\u00fchen/", "tokens": ["Dein", "au\u00b7ge", "schau\u00b7e", "nichts", "/", "als", "nur", "ge\u00b7l\u00fc\u00b7cke", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "$(", "KOKOM", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der seegen wolle dich/ als wolcken \u00fcberziehen;", "tokens": ["Der", "see\u00b7gen", "wol\u00b7le", "dich", "/", "als", "wol\u00b7cken", "\u00fc\u00b7ber\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "PPER", "$(", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der sch\u00f6nen bergen paar/ die schwanen weise brust", "tokens": ["Der", "sch\u00f6\u00b7nen", "ber\u00b7gen", "paar", "/", "die", "schwa\u00b7nen", "wei\u00b7se", "brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PIAT", "$(", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Sey stets ein libanon von cedern gr\u00fcner lust.", "tokens": ["Sey", "stets", "ein", "li\u00b7ba\u00b7non", "von", "ce\u00b7dern", "gr\u00fc\u00b7ner", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Bleib lange/ wie du bist/ ein zierath dieser erden;", "tokens": ["Bleib", "lan\u00b7ge", "/", "wie", "du", "bist", "/", "ein", "zie\u00b7rath", "die\u00b7ser", "er\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PWAV", "PPER", "VAFIN", "$(", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "La\u00df deiner haare gold gar langsam silber werden.", "tokens": ["La\u00df", "dei\u00b7ner", "haa\u00b7re", "gold", "gar", "lang\u