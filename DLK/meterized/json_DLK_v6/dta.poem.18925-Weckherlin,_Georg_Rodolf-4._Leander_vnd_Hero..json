{"dta.poem.18925": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  Leander vnd Hero.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wje vns die Lieb ein Liecht/ ein Lufft das leben", "tokens": ["Wie", "vns", "die", "Lieb", "ein", "Liecht", "/", "ein", "Lufft", "das", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "PDS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gab/", "tokens": ["gab", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vnd wie ein Fewr (vns beed verliebend) vns er-", "tokens": ["Vnd", "wie", "ein", "Fewr", "(", "vns", "beed", "ver\u00b7lie\u00b7bend", ")", "vns", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$(", "PPER", "ADJD", "VVPP", "$(", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "leuchtet:", "tokens": ["leuch\u00b7tet", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So hat ein Wasser auch erdrenckend vns befeuchtet/", "tokens": ["So", "hat", "ein", "Was\u00b7ser", "auch", "er\u00b7dren\u00b7ckend", "vns", "be\u00b7feuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd nu v