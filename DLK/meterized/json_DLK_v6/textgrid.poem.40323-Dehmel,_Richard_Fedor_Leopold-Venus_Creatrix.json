{"textgrid.poem.40323": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Creatrix", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O meine bleiche Braut! du blasse Wolke", "tokens": ["O", "mei\u00b7ne", "blei\u00b7che", "Braut", "!", "du", "blas\u00b7se", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im Arm des Sturms! du bebend Haupt,", "tokens": ["im", "Arm", "des", "Sturms", "!", "du", "be\u00b7bend", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "an meine Brust geneigt aus deinen Schleiern:", "tokens": ["an", "mei\u00b7ne", "Brust", "ge\u00b7neigt", "aus", "dei\u00b7nen", "Schlei\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erbleichst, erbebst du ", "tokens": ["er\u00b7bleichst", ",", "er\u00b7bebst", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O nun ergl\u00fchst du, heimlich Willige du,", "tokens": ["O", "nun", "er\u00b7gl\u00fchst", "du", ",", "heim\u00b7lich", "Wil\u00b7li\u00b7ge", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "ADJD", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "nun \u00f6ffnest du die herzverkl\u00e4rten Augen,", "tokens": ["nun", "\u00f6ff\u00b7nest", "du", "die", "herz\u00b7ver\u00b7kl\u00e4r\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "nun ringt sich von den Lippen dir mein Name,", "tokens": ["nun", "ringt", "sich", "von", "den", "Lip\u00b7pen", "dir", "mein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.8": {"text": "und inniger k\u00fcss ich dich \u2013 wir sind allein.", "tokens": ["und", "in\u00b7ni\u00b7ger", "k\u00fcss", "ich", "dich", "\u2013", "wir", "sind", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "$(", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein. O komm, das Licht der Ampel", "tokens": ["Al\u00b7lein", ".", "O", "komm", ",", "das", "Licht", "der", "Am\u00b7pel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "NE", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wirft Schatten; komm! heut soll kein Schatten sein,", "tokens": ["wirft", "Schat\u00b7ten", ";", "komm", "!", "heut", "soll", "kein", "Schat\u00b7ten", "sein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "$.", "ADV", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "heut sollen alle, alle Lichter leuchten,", "tokens": ["heut", "sol\u00b7len", "al\u00b7le", ",", "al\u00b7le", "Lich\u00b7ter", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in einer See von Licht sollst du mir schwimmen,", "tokens": ["in", "ei\u00b7ner", "See", "von", "Licht", "sollst", "du", "mir", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "du wei\u00dfe M\u00f6we meine! Fl\u00fcchte nicht:", "tokens": ["du", "wei\u00b7\u00dfe", "M\u00f6\u00b7we", "mei\u00b7ne", "!", "Fl\u00fcch\u00b7te", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "$.", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "sieh, selbst dem keuschen Himmel noch verwehr ich", "tokens": ["sieh", ",", "selbst", "dem", "keu\u00b7schen", "Him\u00b7mel", "noch", "ver\u00b7wehr", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zu lauschen \u2013 horch: der Vorhang rauscht, o komm!", "tokens": ["zu", "lau\u00b7schen", "\u2013", "horch", ":", "der", "Vor\u00b7hang", "rauscht", ",", "o", "komm", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "VVIMP", "$.", "ART", "NN", "VVFIN", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und jeden Spalt verschlie\u00df ich faltenschwer,", "tokens": ["und", "je\u00b7den", "Spalt", "ver\u00b7schlie\u00df", "ich", "fal\u00b7ten\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "da\u00df nicht die Nacht, die silbern blauende,", "tokens": ["da\u00df", "nicht", "die", "Nacht", ",", "die", "sil\u00b7bern", "blau\u00b7en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "err\u00f6te, mu\u00df sie deine Sch\u00f6nheit dulden,", "tokens": ["er\u00b7r\u00f6\u00b7te", ",", "mu\u00df", "sie", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "dul\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "da\u00df nicht der Sterne reine Glut", "tokens": ["da\u00df", "nicht", "der", "Ster\u00b7ne", "rei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "sich neidisch tr\u00fcbe, sehn sie Deine Reinheit.", "tokens": ["sich", "nei\u00b7disch", "tr\u00fc\u00b7be", ",", "sehn", "sie", "Dei\u00b7ne", "Rein\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJA", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tu ab die Myrtenkrone, den G\u00fcrtel, komm,", "tokens": ["Tu", "ab", "die", "Myr\u00b7ten\u00b7kro\u00b7ne", ",", "den", "G\u00fcr\u00b7tel", ",", "komm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "du bist allein! Die jungen Rosen nur,", "tokens": ["du", "bist", "al\u00b7lein", "!", "Die", "jun\u00b7gen", "Ro\u00b7sen", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "schlaftrunken \u00fcber unser Bett gebeugt,", "tokens": ["schlaf\u00b7trun\u00b7ken", "\u00fc\u00b7ber", "un\u00b7ser", "Bett", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "spinnen duftbange Tr\u00e4ume", "tokens": ["spin\u00b7nen", "duft\u00b7ban\u00b7ge", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "von purpurner Entfaltung scheuer Knospen;", "tokens": ["von", "pur\u00b7pur\u00b7ner", "Ent\u00b7fal\u00b7tung", "scheu\u00b7er", "Knos\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "die Rosen nur \u2013 und ich.", "tokens": ["die", "Ro\u00b7sen", "nur", "\u2013", "und", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie in Tr\u00e4umen, wie auf D\u00fcften leicht,", "tokens": ["Und", "wie", "in", "Tr\u00e4u\u00b7men", ",", "wie", "auf", "D\u00fcf\u00b7ten", "leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "$,", "PWAV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von Licht zu Licht mit leuchtenden H\u00e4nden gleit'ich", "tokens": ["von", "Licht", "zu", "Licht", "mit", "leuch\u00b7ten\u00b7den", "H\u00e4n\u00b7den", "gleit'\u00b7ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und winke \u2013 und du kommst.", "tokens": ["und", "win\u00b7ke", "\u2013", "und", "du", "kommst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da sinken und schwinden", "tokens": ["Da", "sin\u00b7ken", "und", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "hell von uns weg die irdischen H\u00fcllen alle:", "tokens": ["hell", "von", "uns", "weg", "die", "ir\u00b7di\u00b7schen", "H\u00fcl\u00b7len", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "PIS", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "aus seidnen Wogen steigst du her zu mir,", "tokens": ["aus", "seid\u00b7nen", "Wo\u00b7gen", "steigst", "du", "her", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und Brust an Brust gedr\u00e4ngt von blendenden Schauern,", "tokens": ["und", "Brust", "an", "Brust", "ge\u00b7dr\u00e4ngt", "von", "blen\u00b7den\u00b7den", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "von goldnen Dunkelheiten weit umw\u00f6lkt,", "tokens": ["von", "gold\u00b7nen", "Dun\u00b7kel\u00b7hei\u00b7ten", "weit", "um\u00b7w\u00f6lkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "wiegen uns fernhintastende Schwingen", "tokens": ["wie\u00b7gen", "uns", "fern\u00b7hin\u00b7tas\u00b7ten\u00b7de", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Schoo\u00df an Schoo\u00df hin\u00fcber", "tokens": ["Schoo\u00df", "an", "Schoo\u00df", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "in die G\u00e4rten der Ewigkeit.", "tokens": ["in", "die", "G\u00e4r\u00b7ten", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Flammen der Sehnsucht wachsen da,", "tokens": ["Flam\u00b7men", "der", "Sehn\u00b7sucht", "wach\u00b7sen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "gl\u00fchende B\u00e4che voller Erf\u00fcllung treiben", "tokens": ["gl\u00fc\u00b7hen\u00b7de", "B\u00e4\u00b7che", "vol\u00b7ler", "Er\u00b7f\u00fcl\u00b7lung", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "da in Eins die einsam pulsenden Seelen,", "tokens": ["da", "in", "Eins", "die", "ein\u00b7sam", "pul\u00b7sen\u00b7den", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Puls in Puls in Glanz ergossen verbluten", "tokens": ["Puls", "in", "Puls", "in", "Glanz", "er\u00b7gos\u00b7sen", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "heimwehwild die zuckenden W\u00fcnsche,", "tokens": ["heim\u00b7weh\u00b7wild", "die", "zu\u00b7cken\u00b7den", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "hoch auf strudelt todesselig der Wille,", "tokens": ["hoch", "auf", "stru\u00b7delt", "to\u00b7des\u00b7se\u00b7lig", "der", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "d\u00fcrstend umsaust ihn der Odem der Allmacht,", "tokens": ["d\u00