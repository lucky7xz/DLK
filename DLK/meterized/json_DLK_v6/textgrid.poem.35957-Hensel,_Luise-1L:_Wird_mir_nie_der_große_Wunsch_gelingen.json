{"textgrid.poem.35957": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wird mir nie der gro\u00dfe Wunsch gelingen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird mir nie der gro\u00dfe Wunsch gelingen", "tokens": ["Wird", "mir", "nie", "der", "gro\u00b7\u00dfe", "Wunsch", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In des Lebens treibendem Gew\u00fchl", "tokens": ["In", "des", "Le\u00b7bens", "trei\u00b7ben\u00b7dem", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einmal noch den Arm um dich zu schlingen,", "tokens": ["Ein\u00b7mal", "noch", "den", "Arm", "um", "dich", "zu", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dir den treuen Schwesterku\u00df zu bringen;", "tokens": ["Dir", "den", "treu\u00b7en", "Schwes\u00b7ter\u00b7ku\u00df", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eint uns nie ein freundliches Asyl? \u2013", "tokens": ["Eint", "uns", "nie", "ein", "freund\u00b7li\u00b7ches", "A\u00b7syl", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ruhe floh, seitdem du mich verlassen,", "tokens": ["Ru\u00b7he", "floh", ",", "seit\u00b7dem", "du", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sehnsucht f\u00fcllt mein Herz mit bangem Weh'n. \u2013", "tokens": ["Sehn\u00b7sucht", "f\u00fcllt", "mein", "Herz", "mit", "ban\u00b7gem", "Weh'", "n.", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Einmal nur, nur einmal dich umfassen,", "tokens": ["Ein\u00b7mal", "nur", ",", "nur", "ein\u00b7mal", "dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Herrlicher! und ruhig dann erblassen;", "tokens": ["Herr\u00b7li\u00b7cher", "!", "und", "ru\u00b7hig", "dann", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ADJD", "ADV", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Neidenswerth ist noch dies Wiedersehn.", "tokens": ["Nei\u00b7dens\u00b7werth", "ist", "noch", "dies", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PDS", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}