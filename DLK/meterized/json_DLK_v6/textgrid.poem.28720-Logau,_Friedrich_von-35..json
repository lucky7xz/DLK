{"textgrid.poem.28720": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ofen w\u00e4rmt die Stube, thut solches unbereut,", "tokens": ["Der", "O\u00b7fen", "w\u00e4rmt", "die", "Stu\u00b7be", ",", "thut", "sol\u00b7ches", "un\u00b7be\u00b7reut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ob gleich ein alte Mutter die Hinter-Stirn ihm beut.", "tokens": ["Ob", "gleich", "ein", "al\u00b7te", "Mut\u00b7ter", "die", "Hin\u00b7ter\u00b7Stirn", "ihm", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer recht geht, gehe weiter und frage nichts darnach,", "tokens": ["Wer", "recht", "geht", ",", "ge\u00b7he", "wei\u00b7ter", "und", "fra\u00b7ge", "nichts", "dar\u00b7nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ob Hasser oder Sp\u00f6tter braucht List, Verleumdung, Schmach.", "tokens": ["Ob", "Has\u00b7ser", "o\u00b7der", "Sp\u00f6t\u00b7ter", "braucht", "List", ",", "Ver\u00b7leum\u00b7dung", ",", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}