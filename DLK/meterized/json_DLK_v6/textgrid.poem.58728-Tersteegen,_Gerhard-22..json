{"textgrid.poem.58728": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hat seinen Bogen gespannt und mich dem Pfeil zum Ziel gesteckt. Klaglied. 3, 12", "tokens": ["Er", "hat", "sei\u00b7nen", "Bo\u00b7gen", "ge\u00b7spannt", "und", "mich", "dem", "Pfeil", "zum", "Ziel", "ge\u00b7steckt", ".", "Klag\u00b7lied", ".", "3", ",", "12"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "KON", "PRF", "ART", "NN", "APPRART", "NN", "VVPP", "$.", "NN", "$.", "CARD", "$,", "CARD"], "meter": "-+--+--+-+-+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist wie ein Ziel, wonach du stets l\u00e4\u00dft fliegen", "tokens": ["Mein", "Herz", "ist", "wie", "ein", "Ziel", ",", "wo\u00b7nach", "du", "stets", "l\u00e4\u00dft", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pfeile, die so gl\u00fchend sein;", "tokens": ["Die", "Pfei\u00b7le", ",", "die", "so", "gl\u00fc\u00b7hend", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werd' zu matt, ich mu\u00df erliegen,", "tokens": ["Ich", "werd'", "zu", "matt", ",", "ich", "mu\u00df", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du mich liebst, so halte ein!", "tokens": ["Wenn", "du", "mich", "liebst", ",", "so", "hal\u00b7te", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nein, es tut mir leid, ich wu\u00dft' nicht, was ich sagte,", "tokens": ["Doch", "nein", ",", "es", "tut", "mir", "leid", ",", "ich", "wu\u00dft'", "nicht", ",", "was", "ich", "sag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schie\u00df zu, du s\u00fc\u00dfe Lieb', ich steh,", "tokens": ["Schie\u00df", "zu", ",", "du", "s\u00fc\u00b7\u00dfe", "Lieb'", ",", "ich", "steh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein eitles F\u00fcrchten gar nicht achte,", "tokens": ["Mein", "eit\u00b7les", "F\u00fcrch\u00b7ten", "gar", "nicht", "ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Br\u00e4ut'gams Pfeile tun nicht weh!", "tokens": ["Des", "Br\u00e4ut'\u00b7gams", "Pfei\u00b7le", "tun", "nicht", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVINF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schie\u00df zu, schie\u00df zu, mein Herr, den ich anbet' alleine,", "tokens": ["Schie\u00df", "zu", ",", "schie\u00df", "zu", ",", "mein", "Herr", ",", "den", "ich", "an\u00b7bet'", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mein Herz hinfort das Ziel von deinen Pfeilen nenn,", "tokens": ["Mein", "Herz", "hin\u00b7fort", "das", "Ziel", "von", "dei\u00b7nen", "Pfei\u00b7len", "nenn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verwund es nur, es sei ganz deine,", "tokens": ["Ver\u00b7wund", "es", "nur", ",", "es", "sei", "ganz", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib, da\u00df ich dich lieb' und erkenn'!", "tokens": ["Gib", ",", "da\u00df", "ich", "dich", "lieb'", "und", "er\u00b7kenn'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Mein Herz durchschneidest du, mein Heil,", "tokens": ["Mein", "Herz", "durch\u00b7schnei\u00b7dest", "du", ",", "mein", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00f6chst durchdringend ist dein Pfeil;", "tokens": ["So", "h\u00f6chst", "durch\u00b7drin\u00b7gend", "ist", "dein", "Pfeil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Behandelst du also, liebwerter \u00dcberwinder,", "tokens": ["Be\u00b7han\u00b7delst", "du", "al\u00b7so", ",", "lieb\u00b7wer\u00b7ter", "\u00dc\u00b7berw\u00b7in\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Seelen, die ganz dein, die treu verliebten Kinder?", "tokens": ["Die", "See\u00b7len", ",", "die", "ganz", "dein", ",", "die", "treu", "ver\u00b7lieb\u00b7ten", "Kin\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "$,", "PRELS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}