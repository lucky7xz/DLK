{"textgrid.poem.26521": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jahr um Jahr hab' ich gerungen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahr um Jahr hab' ich gerungen", "tokens": ["Jahr", "um", "Jahr", "hab'", "ich", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erlitten Schmerz um Schmerz;", "tokens": ["Und", "er\u00b7lit\u00b7ten", "Schmerz", "um", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber stark und unbezwungen", "tokens": ["A\u00b7ber", "stark", "und", "un\u00b7be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt sich mein gequ\u00e4ltes Herz.", "tokens": ["Hielt", "sich", "mein", "ge\u00b7qu\u00e4l\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sich auch die Wolken ballten,", "tokens": ["Wie", "sich", "auch", "die", "Wol\u00b7ken", "ball\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Leben sich verschwor \u2013", "tokens": ["Wie", "das", "Le\u00b7ben", "sich", "ver\u00b7schwor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit stets reinerem Entfalten", "tokens": ["Mit", "stets", "rei\u00b7ne\u00b7rem", "Ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwang sich still mein Geist empor.", "tokens": ["Schwang", "sich", "still", "mein", "Geist", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Treu ergl\u00fchend f\u00fcr das \u00c4chte,", "tokens": ["Treu", "er\u00b7gl\u00fc\u00b7hend", "f\u00fcr", "das", "\u00c4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich fast das Ziel erreicht;", "tokens": ["Hab'", "ich", "fast", "das", "Ziel", "er\u00b7reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt mich an, ihr ew'gen M\u00e4chte:", "tokens": ["Blickt", "mich", "an", ",", "ihr", "ew'\u00b7gen", "M\u00e4ch\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Scheitel ist gebleicht.", "tokens": ["Die\u00b7ser", "Schei\u00b7tel", "ist", "ge\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Flamme meines Lebens", "tokens": ["Und", "die", "Flam\u00b7me", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neigt sich m\u00e4lig zum Vergl\u00fch'n \u2013", "tokens": ["Neigt", "sich", "m\u00e4\u00b7lig", "zum", "Ver\u00b7gl\u00fch'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nnt mir noch den Rest des Strebens,", "tokens": ["G\u00f6nnt", "mir", "noch", "den", "Rest", "des", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6nnt mir noch ein letztes M\u00fch'n.", "tokens": ["G\u00f6nnt", "mir", "noch", "ein", "letz\u00b7tes", "M\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00dft mich noch getrost vollenden,", "tokens": ["La\u00dft", "mich", "noch", "ge\u00b7trost", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich ernst und fest begann,", "tokens": ["Was", "ich", "ernst", "und", "fest", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf sanften G\u00f6tterh\u00e4nden", "tokens": ["Und", "auf", "sanf\u00b7ten", "G\u00f6t\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traget mich von hinnen dann! \u2013", "tokens": ["Tra\u00b7get", "mich", "von", "hin\u00b7nen", "dann", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also fleh' ich, von den Schwingen", "tokens": ["Al\u00b7so", "fleh'", "ich", ",", "von", "den", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Erf\u00fcllung leis' umweht \u2013", "tokens": ["Der", "Er\u00b7f\u00fcl\u00b7lung", "leis'", "um\u00b7weht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch f\u00fcrchtend, da\u00df mein Ringen", "tokens": ["Und", "doch", "f\u00fcrch\u00b7tend", ",", "da\u00df", "mein", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Verh\u00e4ngni\u00df untergeht!", "tokens": ["Im", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "un\u00b7ter\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}