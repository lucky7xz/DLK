{"textgrid.poem.40799": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Widmung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aug' in Auge l\u00e4chelnd schlangen", "tokens": ["Aug'", "in", "Au\u00b7ge", "l\u00e4\u00b7chelnd", "schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arm in Arm einst West und Ost,", "tokens": ["Arm", "in", "Arm", "einst", "West", "und", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwillingspaar, das liebumfangen", "tokens": ["Zwil\u00b7lings\u00b7paar", ",", "das", "lieb\u00b7um\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch in Einer Wiege kos't!", "tokens": ["Noch", "in", "Ei\u00b7ner", "Wie\u00b7ge", "kos't", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ahriman ersah's, der Schlimme,", "tokens": ["Ah\u00b7ri\u00b7man", "er\u00b7sah's", ",", "der", "Schlim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn erbaut der Anblick nicht,", "tokens": ["Ihn", "er\u00b7baut", "der", "An\u00b7blick", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwingt den Zauberstab im Grimme,", "tokens": ["Schwingt", "den", "Zau\u00b7ber\u00b7stab", "im", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Draus manch rother Blitzstrahl bricht.", "tokens": ["Draus", "manch", "ro\u00b7ther", "Blitz\u00b7strahl", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wirft als Riesenschlang' ins Bette,", "tokens": ["Wirft", "als", "Rie\u00b7sen\u00b7schlang'", "ins", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringelnd, b\u00e4umend, zwischen sie", "tokens": ["Rin\u00b7gelnd", ",", "b\u00e4u\u00b7mend", ",", "zwi\u00b7schen", "sie"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "$,", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Berg' urew'ge Kette,", "tokens": ["Je\u00b7ner", "Ber\u00b7g'", "u\u00b7re\u00b7w'\u00b7ge", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die nie bricht und endet nie.", "tokens": ["Die", "nie", "bricht", "und", "en\u00b7det", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "L\u00e4\u00dft der L\u00fcfte Vorhang rollend", "tokens": ["L\u00e4\u00dft", "der", "L\u00fcf\u00b7te", "Vor\u00b7hang", "rol\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Undurchdringlich niederziehn,", "tokens": ["Un\u00b7durch\u00b7dring\u00b7lich", "nie\u00b7der\u00b7ziehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spannt des Meers Sahara grollend", "tokens": ["Spannt", "des", "Meers", "Sa\u00b7ha\u00b7ra", "grol\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlos zwischen Beiden hin.", "tokens": ["End\u00b7los", "zwi\u00b7schen", "Bei\u00b7den", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch Ormusd, der Milde, Gute,", "tokens": ["Doch", "Or\u00b7musd", ",", "der", "Mil\u00b7de", ",", "Gu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "L\u00e4chelnd ob dem schlechten Schwank,", "tokens": ["L\u00e4\u00b7chelnd", "ob", "dem", "schlech\u00b7ten", "Schwank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winkt mit seiner Zauberruthe,", "tokens": ["Winkt", "mit", "sei\u00b7ner", "Zau\u00b7ber\u00b7ru\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sternefunkelnd, goldesblank.", "tokens": ["Ster\u00b7ne\u00b7fun\u00b7kelnd", ",", "gol\u00b7des\u00b7blank", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, auf Taubenfitt'gen f\u00e4chelnd,", "tokens": ["Sieh", ",", "auf", "Tau\u00b7ben\u00b7fitt'\u00b7gen", "f\u00e4\u00b7chelnd", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der fernsten Luft gek\u00fc\u00dft,", "tokens": ["Von", "der", "ferns\u00b7ten", "Luft", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schifft die Liebe, kundig l\u00e4chelnd;", "tokens": ["Schifft", "die", "Lie\u00b7be", ",", "kun\u00b7dig", "l\u00e4\u00b7chelnd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sich Ost und Westen gr\u00fc\u00dft!", "tokens": ["Wie", "sich", "Ost", "und", "Wes\u00b7ten", "gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bl\u00fcthenduft und Thau und Segen", "tokens": ["Bl\u00fc\u00b7then\u00b7duft", "und", "Thau", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Saugt im Osten Menschengeist,", "tokens": ["Saugt", "im", "Os\u00b7ten", "Men\u00b7schen\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigt als Wolke, die als Regen", "tokens": ["Steigt", "als", "Wol\u00b7ke", ",", "die", "als", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "NN", "$,", "PRELS", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mild auf Westens Flur dann fleu\u00dft!", "tokens": ["Mild", "auf", "Wes\u00b7tens", "Flur", "dann", "fleu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und die Br\u00fccke hat gezogen,", "tokens": ["Und", "die", "Br\u00fc\u00b7cke", "hat", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vom Ost zum West sich schwingt,", "tokens": ["Die", "vom", "Ost", "zum", "West", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Phantasie als Regenbogen,", "tokens": ["Phan\u00b7ta\u00b7sie", "als", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die Berge \u00fcberspringt!", "tokens": ["Der", "die", "Ber\u00b7ge", "\u00fc\u00b7bers\u00b7pringt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Durch die weiten Meeresw\u00fcsten,", "tokens": ["Durch", "die", "wei\u00b7ten", "Mee\u00b7res\u00b7w\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steuernd, wie ein Silberschwan,", "tokens": ["Steu\u00b7ernd", ",", "wie", "ein", "Sil\u00b7ber\u00b7schwan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen Osts und Westens K\u00fcsten", "tokens": ["Zwi\u00b7schen", "Osts", "und", "Wes\u00b7tens", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wogt des Lieds melod'scher Kahn.", "tokens": ["Wogt", "des", "Lieds", "me\u00b7lo\u00b7d'\u00b7scher", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}