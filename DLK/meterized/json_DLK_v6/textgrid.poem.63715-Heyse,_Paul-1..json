{"textgrid.poem.63715": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine fand ich, eine fette,", "tokens": ["Ei\u00b7ne", "fand", "ich", ",", "ei\u00b7ne", "fet\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vor ihrem Schlupfloch sa\u00df,", "tokens": ["Die", "vor", "ih\u00b7rem", "Schlupf\u00b7loch", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehrbar, sauber und behaglich", "tokens": ["Ehr\u00b7bar", ",", "sau\u00b7ber", "und", "be\u00b7hag\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Augen hell wie Glas.", "tokens": ["Und", "die", "Au\u00b7gen", "hell", "wie", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KOKOM", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "An dem warmbesonnten Steine", "tokens": ["An", "dem", "warm\u00b7be\u00b7sonn\u00b7ten", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Putzte sie das N\u00e4schen blank,", "tokens": ["Putz\u00b7te", "sie", "das", "N\u00e4\u00b7schen", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fing sich dann und wann ein M\u00fcckchen,", "tokens": ["Fing", "sich", "dann", "und", "wann", "ein", "M\u00fcck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "KON", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sich ihr zu nahe schwang.", "tokens": ["Das", "sich", "ihr", "zu", "na\u00b7he", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rechts und links durch alle Ritzen", "tokens": ["Rechts", "und", "links", "durch", "al\u00b7le", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Raschelte die junge Brut.", "tokens": ["Ra\u00b7schel\u00b7te", "die", "jun\u00b7ge", "Brut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Sie allein blieb stattlich sitzen,", "tokens": ["Sie", "al\u00b7lein", "blieb", "statt\u00b7lich", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie gereifte Weisheit tut.", "tokens": ["Wie", "ge\u00b7reif\u00b7te", "Weis\u00b7heit", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur zuweilen mit dem Schw\u00e4nzchen", "tokens": ["Nur", "zu\u00b7wei\u00b7len", "mit", "dem", "Schw\u00e4nz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zuckte sie bedeutungsvoll,", "tokens": ["Zuck\u00b7te", "sie", "be\u00b7deu\u00b7tungs\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trieben es die jungen Leute", "tokens": ["Trie\u00b7ben", "es", "die", "jun\u00b7gen", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Kammern gar zu toll.", "tokens": ["In", "den", "Kam\u00b7mern", "gar", "zu", "toll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So in innres Schaun versunken", "tokens": ["So", "in", "inn\u00b7res", "Schaun", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Genu\u00df des Sonnenlichts,", "tokens": ["Und", "Ge\u00b7nu\u00df", "des", "Son\u00b7nen\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht erschrak sie, da ich nahte,", "tokens": ["Nicht", "er\u00b7schrak", "sie", ",", "da", "ich", "nah\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Weise f\u00fcrchtet nichts.", "tokens": ["Denn", "der", "Wei\u00b7se", "f\u00fcrch\u00b7tet", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie der Philosoph der Tonne", "tokens": ["Wie", "der", "Phi\u00b7lo\u00b7soph", "der", "Ton\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah sie mich gelassen an:", "tokens": ["Sah", "sie", "mich", "ge\u00b7las\u00b7sen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh mir etwas aus der Sonne,", "tokens": ["Geh", "mir", "et\u00b7was", "aus", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbekannter, junger Mann!", "tokens": ["Un\u00b7be\u00b7kann\u00b7ter", ",", "jun\u00b7ger", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}