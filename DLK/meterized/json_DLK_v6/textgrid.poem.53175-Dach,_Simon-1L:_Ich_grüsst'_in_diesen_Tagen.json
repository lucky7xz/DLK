{"textgrid.poem.53175": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich gr\u00fcsst' in diesen Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fcsst' in diesen Tagen", "tokens": ["Ich", "gr\u00fcsst'", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Friedel\u00e4nder Thor,", "tokens": ["Das", "Frie\u00b7de\u00b7l\u00e4n\u00b7der", "Thor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hatte sechs geschlagen,", "tokens": ["Es", "hat\u00b7te", "sechs", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne stieg empor:", "tokens": ["Die", "Son\u00b7ne", "stieg", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was sah' ich nicht f\u00fcr Frewden?", "tokens": ["Was", "sah'", "ich", "nicht", "f\u00fcr", "Frew\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Reiff lag vmb das Gra\u00df,", "tokens": ["Der", "Reiff", "lag", "vmb", "das", "Gra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Finck sang auff den Weyden,", "tokens": ["Ein", "Finck", "sang", "auff", "den", "Wey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Pregel stund wie Gla\u00df.", "tokens": ["Der", "Pre\u00b7gel", "stund", "wie", "Gla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich war die Br\u00fcck' hin\u00fcber,", "tokens": ["Ich", "war", "die", "Br\u00fcck", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sprang das geile Vieh,", "tokens": ["Wie", "sprang", "das", "gei\u00b7le", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der stoltze Stier, jhr Lieber,", "tokens": ["Der", "stolt\u00b7ze", "Stier", ",", "jhr", "Lie\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trat mitten vnter sie,", "tokens": ["Trat", "mit\u00b7ten", "vn\u00b7ter", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Hirt hub an zu blasen,", "tokens": ["Der", "Hirt", "hub", "an", "zu", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie tantzten sie vmbher", "tokens": ["Wie", "tantz\u00b7ten", "sie", "vm\u00b7bher"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auff den betawten Rasen", "tokens": ["Auff", "den", "be\u00b7taw\u00b7ten", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Als wenn es Hochzeit w\u00e4r!", "tokens": ["Als", "wenn", "es", "Hoch\u00b7zeit", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie wurden ausgetrieben", "tokens": ["Sie", "wur\u00b7den", "aus\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die\u00df Jahr zum erstenmal,", "tokens": ["Die\u00df", "Jahr", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachdem sie lang geblieben", "tokens": ["Nach\u00b7dem", "sie", "lang", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jhrem finstern Stall,", "tokens": ["In", "jhrem", "fins\u00b7tern", "Stall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich sprach: der Freyheit Gaben", "tokens": ["Ich", "sprach", ":", "der", "Frey\u00b7heit", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Thun diesem Vieh auch wol,", "tokens": ["Thun", "die\u00b7sem", "Vieh", "auch", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer dieses Gut kan haben,", "tokens": ["Wer", "die\u00b7ses", "Gut", "kan", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist alles Reichthums vol.", "tokens": ["Ist", "al\u00b7les", "Reicht\u00b7hums", "vol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vnd h\u00e4tt' ich Goldes-Tonnen", "tokens": ["Vnd", "h\u00e4tt'", "ich", "Gol\u00b7des\u00b7Ton\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd was des Pregels Rand", "tokens": ["Vnd", "was", "des", "Pre\u00b7gels", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An Sch\u00e4tzen hegt, gewonnen,", "tokens": ["An", "Sch\u00e4t\u00b7zen", "hegt", ",", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "S\u00e4\u00df' aber eingespannt", "tokens": ["S\u00e4\u00df'", "a\u00b7ber", "ein\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd k\u00f6nte mich nicht retten", "tokens": ["Vnd", "k\u00f6n\u00b7te", "mich", "nicht", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus Sorgen, Furcht vnd Pein,", "tokens": ["Aus", "Sor\u00b7gen", ",", "Furcht", "vnd", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich w\u00fcrd' in g\u00fcldnen Ketten", "tokens": ["Ich", "w\u00fcrd'", "in", "g\u00fcld\u00b7nen", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dennoch ein Sclave seyn.", "tokens": ["Den\u00b7noch", "ein", "Scla\u00b7ve", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wolt ich ein V\u00f6glein schliessen", "tokens": ["Wolt", "ich", "ein", "V\u00f6\u00b7glein", "schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich in ein silbern Hau\u00df,", "tokens": ["Gleich", "in", "ein", "sil\u00b7bern", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Freyheit zu geniessen", "tokens": ["Der", "Frey\u00b7heit", "zu", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sehnt sich es doch hinaus:", "tokens": ["Sehnt", "sich", "es", "doch", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Sch\u00e4tze sampt den W\u00fcrden", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "sampt", "den", "W\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind ein geschminckter Schmertz,", "tokens": ["Sind", "ein", "ge\u00b7schminck\u00b7ter", "Schmertz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sind Dienst vnd schwere B\u00fcrden,", "tokens": ["Sind", "Dienst", "vnd", "schwe\u00b7re", "B\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich lob ein freyes Hertz.", "tokens": ["Ich", "lob", "ein", "frey\u00b7es", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Vnschuldreiches Leben,", "tokens": ["Ein", "Vn\u00b7schul\u00b7drei\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sich des Herren Zucht", "tokens": ["Das", "sich", "des", "Her\u00b7ren", "Zucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gehorsam vntergeben", "tokens": ["Ge\u00b7hor\u00b7sam", "vn\u00b7ter\u00b7ge\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd jhm zu dienen sucht:", "tokens": ["Vnd", "jhm", "zu", "die\u00b7nen", "sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kan ich nur den Schatz werben,", "tokens": ["Kan", "ich", "nur", "den", "Schatz", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Vnd, nimmt der Tod mich hin,", "tokens": ["Vnd", ",", "nimmt", "der", "Tod", "mich", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihn lassen meinen Erben,", "tokens": ["Ihn", "las\u00b7sen", "mei\u00b7nen", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So hab ich gnug Gewinn.", "tokens": ["So", "hab", "ich", "gnug", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir reisen hin vnd wieder", "tokens": ["Wir", "rei\u00b7sen", "hin", "vnd", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit \u00fcber Land vnd See,", "tokens": ["Weit", "\u00fc\u00b7ber", "Land", "vnd", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vern\u00fctzen vnsre Glieder,", "tokens": ["Ver\u00b7n\u00fct\u00b7zen", "vns\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thun vnserm Hertzen weh:", "tokens": ["Thun", "vn\u00b7serm", "Hert\u00b7zen", "weh", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das wahre Gut zu kriegen,", "tokens": ["Das", "wah\u00b7re", "Gut", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das vns in vns nur f\u00fchrt", "tokens": ["Das", "vns", "in", "vns", "nur", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd ewig kan begn\u00fcgen,", "tokens": ["Vnd", "e\u00b7wig", "kan", "be\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird wenig flei\u00df gesp\u00fcrt.", "tokens": ["Wird", "we\u00b7nig", "flei\u00df", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Lasst bleiben, liebe Leute,", "tokens": ["Lasst", "blei\u00b7ben", ",", "lie\u00b7be", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das reiche Morgen-Land,", "tokens": ["Das", "rei\u00b7che", "Mor\u00b7gen\u00b7Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht nicht nach grosser Beute", "tokens": ["Steht", "nicht", "nach", "gros\u00b7ser", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fern vmb Hydaspes Strand,", "tokens": ["Fern", "vmb", "Hy\u00b7das\u00b7pes", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein jeder thu die Reise", "tokens": ["Ein", "je\u00b7der", "thu", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tieff in sein Hertz hinein,", "tokens": ["Tieff", "in", "sein", "Hertz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das la\u00df er aller Weise", "tokens": ["Das", "la\u00df", "er", "al\u00b7ler", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Schuld gesaubert seyn.", "tokens": ["Von", "Schuld", "ge\u00b7sau\u00b7bert", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Vnd dan erst wird er finden", "tokens": ["Vnd", "dan", "erst", "wi