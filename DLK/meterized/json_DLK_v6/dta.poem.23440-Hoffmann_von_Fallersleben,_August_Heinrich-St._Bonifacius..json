{"dta.poem.23440": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "St. Bonifacius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sprach der heilige Bonifacius:", "tokens": ["Sprach", "der", "hei\u00b7li\u00b7ge", "Bo\u00b7ni\u00b7fa\u00b7cius", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eines, Eines erst vor allen!", "tokens": ["Ei\u00b7nes", ",", "Ei\u00b7nes", "erst", "vor", "al\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "ADV", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure G\u00f6tzen m\u00fcssen fallen,", "tokens": ["Eu\u00b7re", "G\u00f6t\u00b7zen", "m\u00fcs\u00b7sen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fallen mu\u00df des Teufels Spott! \u2014", "tokens": ["Fal\u00b7len", "mu\u00df", "des", "Teu\u00b7fels", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter seines Beiles Streiche", "tokens": ["Un\u00b7ter", "sei\u00b7nes", "Bei\u00b7les", "Strei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sank des Volkes heil'ge Eiche,", "tokens": ["Sank", "des", "Vol\u00b7kes", "heil'\u00b7ge", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Stieg empor der Glaub' an Gott.", "tokens": ["Stieg", "em\u00b7por", "der", "Glaub'", "an", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So der heilige Bonifacius:", "tokens": ["So", "der", "hei\u00b7li\u00b7ge", "Bo\u00b7ni\u00b7fa\u00b7cius", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eines, Eins auch uns vor allen!", "tokens": ["Ei\u00b7nes", ",", "Eins", "auch", "uns", "vor", "al\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "ADV", "PPER", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre G\u00f6tzen m\u00fcssen fallen,", "tokens": ["Uns\u00b7re", "G\u00f6t\u00b7zen", "m\u00fcs\u00b7sen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fallen mu\u00df ihr Priesterchor!", "tokens": ["Fal\u00b7len", "mu\u00df", "ihr", "Pries\u00b7ter\u00b7chor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter welches Beiles Streiche", "tokens": ["Un\u00b7ter", "wel\u00b7ches", "Bei\u00b7les", "Strei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00e4llt der Knechtschaft heil'ge Leiche,", "tokens": ["F\u00e4llt", "der", "Knecht\u00b7schaft", "heil'\u00b7ge", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Steigt der Freiheit Geist empor?", "tokens": ["Steigt", "der", "Frei\u00b7heit", "Geist", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}