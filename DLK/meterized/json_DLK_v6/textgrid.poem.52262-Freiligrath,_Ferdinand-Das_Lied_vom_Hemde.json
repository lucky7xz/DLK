{"textgrid.poem.52262": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Hemde", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Fingern mager und m\u00fcd,", "tokens": ["Mit", "Fin\u00b7gern", "ma\u00b7ger", "und", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mit Augen schwer und rot,", "tokens": ["Mit", "Au\u00b7gen", "schwer", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In schlechten Hadern sa\u00df ein Weib", "tokens": ["In", "schlech\u00b7ten", "Ha\u00b7dern", "sa\u00df", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "N\u00e4hend f\u00fcrs liebe Brot.", "tokens": ["N\u00e4\u00b7hend", "f\u00fcrs", "lie\u00b7be", "Brot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Stich! Stich! Stich!", "tokens": ["Stich", "!", "Stich", "!", "Stich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aufsah sie wirr und fremde;", "tokens": ["Auf\u00b7sah", "sie", "wirr", "und", "frem\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In Hunger und Armut flehentlich", "tokens": ["In", "Hun\u00b7ger", "und", "Ar\u00b7mut", "fle\u00b7hent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Sang sie das \u00bbLied vom Hemde\u00ab.", "tokens": ["Sang", "sie", "das", "\u00bb", "Lied", "vom", "Hem\u00b7de", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "$(", "NN", "APPRART", "NN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbschaffen! Schaffen! Schaffen!", "tokens": ["\u00bb", "schaf\u00b7fen", "!", "Schaf\u00b7fen", "!", "Schaf\u00b7fen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVINF", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sobald der Haushahn wach!", "tokens": ["So\u00b7bald", "der", "Haus\u00b7hahn", "wach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Schaffen \u2013 Schaffen \u2013 Schaffen,", "tokens": ["Und", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$(", "NN", "$(", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis die Sterne gl\u00fchn durchs Dach!", "tokens": ["Bis", "die", "Ster\u00b7ne", "gl\u00fchn", "durchs", "Dach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O, lieber Sklavin Sein", "tokens": ["O", ",", "lie\u00b7ber", "Skla\u00b7vin", "Sein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bei T\u00fcrken und bei Heiden,", "tokens": ["Bei", "T\u00fcr\u00b7ken", "und", "bei", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo das Weib keine Seele zu retten hat,", "tokens": ["Wo", "das", "Weib", "kei\u00b7ne", "See\u00b7le", "zu", "ret\u00b7ten", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Als so bei Christen leiden!", "tokens": ["Als", "so", "bei", "Chris\u00b7ten", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schaffen \u2013 Schaffen \u2013 Schaffen,", "tokens": ["Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bis das Hirn beginnt zu rollen!", "tokens": ["Bis", "das", "Hirn", "be\u00b7ginnt", "zu", "rol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffen \u2013 Schaffen \u2013 Schaffen,", "tokens": ["Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bis die Augen springen wollen!", "tokens": ["Bis", "die", "Au\u00b7gen", "sprin\u00b7gen", "wol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Saum und Zwickel und Band,", "tokens": ["Saum", "und", "Zwi\u00b7ckel", "und", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Band und Zwickel und Saum \u2013", "tokens": ["Band", "und", "Zwi\u00b7ckel", "und", "Saum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Dann \u00fcber deb Kn\u00f6pfen schlaf' ich ein,", "tokens": ["Dann", "\u00fc\u00b7ber", "deb", "Kn\u00f6p\u00b7fen", "schlaf'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und n\u00e4he sie fort im Traum.", "tokens": ["Und", "n\u00e4\u00b7he", "sie", "fort", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "O M\u00e4nner, denen Gott", "tokens": ["O", "M\u00e4n\u00b7ner", ",", "de\u00b7nen", "Gott"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weib, Mutter, Schwestern gegeben:", "tokens": ["Weib", ",", "Mut\u00b7ter", ",", "Schwes\u00b7tern", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nicht Linnen ist's, was ihr verschlei\u00dft \u2013", "tokens": ["Nicht", "Lin\u00b7nen", "ist's", ",", "was", "ihr", "ver\u00b7schlei\u00dft", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NE", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, warmes Menschenleben!", "tokens": ["Nein", ",", "war\u00b7mes", "Men\u00b7schen\u00b7le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Stich! Stich! Stich!", "tokens": ["Stich", "!", "Stich", "!", "Stich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Das ist der Armut Fluch:", "tokens": ["Das", "ist", "der", "Ar\u00b7mut", "Fluch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit doppeltem Faden n\u00e4h' ich Hemd,", "tokens": ["Mit", "dop\u00b7pel\u00b7tem", "Fa\u00b7den", "n\u00e4h'", "ich", "Hemd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Ja, Hemd und Leichentuch!", "tokens": ["Ja", ",", "Hemd", "und", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch was red' ich nur vom Tod,", "tokens": ["Doch", "was", "red'", "ich", "nur", "vom", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Knochenmanne! \u2013 Ha!", "tokens": ["Dem", "Kno\u00b7chen\u00b7man\u00b7ne", "!", "\u2013", "Ha", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum f\u00fcrcht' ich seine Schreckgestalt,", "tokens": ["Kaum", "f\u00fcrcht'", "ich", "sei\u00b7ne", "Schreck\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gleicht meiner eignen ja!", "tokens": ["Sie", "gleicht", "mei\u00b7ner", "eig\u00b7nen", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "PTKANT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie gleicht mir, weil ich faste,", "tokens": ["Sie", "gleicht", "mir", ",", "weil", "ich", "fas\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weil ich lange nicht geruht.", "tokens": ["Weil", "ich", "lan\u00b7ge", "nicht", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O Gott, da\u00df Brot so teuer ist,", "tokens": ["O", "Gott", ",", "da\u00df", "Brot", "so", "teu\u00b7er", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und so wohlfeil Fleisch und Blut!", "tokens": ["Und", "so", "wohl\u00b7feil", "Fleisch", "und", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schaffen \u2013 Schaffen \u2013 Schaffen!", "tokens": ["Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und der Lohn? Ein Wasserhumpen,", "tokens": ["Und", "der", "Lohn", "?", "Ein", "Was\u00b7ser\u00b7hum\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Kruste Brot, ein Bett von Stroh,", "tokens": ["Ei\u00b7ne", "Krus\u00b7te", "Brot", ",", "ein", "Bett", "von", "Stroh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dort das morsche Dach \u2013 und Lumpen!", "tokens": ["Dort", "das", "mor\u00b7sche", "Dach", "\u2013", "und", "Lum\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$(", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein alter Tisch, ein zerbrochner Stuhl,", "tokens": ["Ein", "al\u00b7ter", "Tisch", ",", "ein", "zer\u00b7broch\u00b7ner", "Stuhl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sonst nichts auf Gottes Welt!", "tokens": ["Sonst", "nichts", "auf", "Got\u00b7tes", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Eine Wand so bar \u2013 's ist ein Trost sogar,", "tokens": ["Ei\u00b7ne", "Wand", "so", "bar", "\u2013", "'s", "ist", "ein", "Trost", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$(", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Wenn mein Schatten nur drauf f\u00e4lt!", "tokens": ["Wenn", "mein", "Schat\u00b7ten", "nur", "drauf", "f\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schaffen \u2013 Schaffen \u2013 Schaffen \u2013", "tokens": ["Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vom Fr\u00fch- zum Nachtgel\u00e4ut!", "tokens": ["Vom", "Fr\u00fch", "zum", "Nacht\u00b7ge\u00b7l\u00e4ut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaffen \u2013 Schaffen \u2013 Schaffen,", "tokens": ["Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie zur Straf' gefangne Leut'!", "tokens": ["Wie", "zur", "Straf'", "ge\u00b7fang\u00b7ne", "Leut'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Band und Zwickel und Saum,", "tokens": ["Band", "und", "Zwi\u00b7ckel", "und", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Saum und Zwickel und Band,", "tokens": ["Saum", "und", "Zwi\u00b7ckel", "und", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Bis vom ewigen B\u00fccken mir schwindig wird,", "tokens": ["Bis", "vom", "e\u00b7wi\u00b7gen", "B\u00fc\u00b7cken", "mir", "schwin\u00b7dig", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Bis das Hirn mir starrt und die Hand!", "tokens": ["Bis", "das", "Hirn", "mir", "starrt", "und", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "KON", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Schaffen \u2013 Schaffen \u2013 Schaffen,", "tokens": ["Schaf\u00b7fen", "\u2013", "Schaf\u00b7fen", "\u2