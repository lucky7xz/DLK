{"textgrid.poem.27907": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Christus heissen Christen wir;", "tokens": ["Von", "Chris\u00b7tus", "heis\u00b7sen", "Chris\u00b7ten", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die That ist weg, der Nam ist hier.", "tokens": ["Die", "That", "ist", "weg", ",", "der", "Nam", "ist", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Christus heist, was Christus lehrt,", "tokens": ["Was", "Chris\u00b7tus", "heist", ",", "was", "Chris\u00b7tus", "lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird nicht gethan, wird kaum geh\u00f6rt;", "tokens": ["Wird", "nicht", "ge\u00b7than", ",", "wird", "kaum", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$,", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur da sind wir de\u00df Namens werth,", "tokens": ["Nur", "da", "sind", "wir", "de\u00df", "Na\u00b7mens", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wann uns f\u00fcr Friede k\u00fcmt das Schwerdt.", "tokens": ["Wann", "uns", "f\u00fcr", "Frie\u00b7de", "k\u00fcmt", "das", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}