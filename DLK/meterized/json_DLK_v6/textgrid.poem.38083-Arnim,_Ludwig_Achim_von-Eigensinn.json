{"textgrid.poem.38083": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Eigensinn", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du's nicht gefischet,", "tokens": ["Hast", "du's", "nicht", "ge\u00b7fi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So fisch es aber noch,", "tokens": ["So", "fisch", "es", "a\u00b7ber", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat sie der Schimpf gereuet,", "tokens": ["Hat", "sie", "der", "Schimpf", "ge\u00b7reu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So thu' ers aber noch.", "tokens": ["So", "thu'", "ers", "a\u00b7ber", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist es denn Ungl\u00fcck heuer alles mein,", "tokens": ["Ist", "es", "denn", "Un\u00b7gl\u00fcck", "heu\u00b7er", "al\u00b7les", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "ADV", "PIS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ade du sch\u00f6nes Liebelein,", "tokens": ["A\u00b7de", "du", "sch\u00f6\u00b7nes", "Lie\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Du must mein eigen seyn.", "tokens": ["Du", "must", "mein", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wei\u00df ich mir ein M\u00e4delein", "tokens": ["Wei\u00df", "ich", "mir", "ein", "M\u00e4\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dieser Erden,", "tokens": ["Auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist sie mir beschert,", "tokens": ["Ist", "sie", "mir", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "So mu\u00df sie mir auch werden,", "tokens": ["So", "mu\u00df", "sie", "mir", "auch", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohl \u00fcber allen Dank,", "tokens": ["Wohl", "\u00fc\u00b7ber", "al\u00b7len", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geschieht es aber heuer nicht,", "tokens": ["Ge\u00b7schieht", "es", "a\u00b7ber", "heu\u00b7er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So geschieht es \u00fcberlang.", "tokens": ["So", "ge\u00b7schieht", "es", "\u00fc\u00b7berl\u00b7ang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da kauft er ihr ein G\u00fcrtlein schmal,", "tokens": ["Da", "kauft", "er", "ihr", "ein", "G\u00fcrt\u00b7lein", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war gesprenkelt \u00fcberall,", "tokens": ["Das", "war", "ge\u00b7spren\u00b7kelt", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hing gesprenkelt wohl auf den Fu\u00df,", "tokens": ["Es", "hing", "ge\u00b7spren\u00b7kelt", "wohl", "auf", "den", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es reut mich, da\u00df ich sterben mu\u00df.", "tokens": ["Es", "reut", "mich", ",", "da\u00df", "ich", "ster\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sterb ich denn so bin ich todt,", "tokens": ["Sterb", "ich", "denn", "so", "bin", "ich", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So gr\u00e4bt man mich in die R\u00f6slein roth,", "tokens": ["So", "gr\u00e4bt", "man", "mich", "in", "die", "R\u00f6s\u00b7lein", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Inne die Rosen, inne den Klee,", "tokens": ["In\u00b7ne", "die", "Ro\u00b7sen", ",", "in\u00b7ne", "den", "Klee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Kein solch braun M\u00e4dlein bekomm ich nimmermehr.", "tokens": ["Kein", "solch", "braun", "M\u00e4d\u00b7lein", "be\u00b7komm", "ich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Von der Erden wohl in das Haus,", "tokens": ["Von", "der", "Er\u00b7den", "wohl", "in", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schau liebe Frau Mutter wie bin ich so gro\u00df,", "tokens": ["Schau", "lie\u00b7be", "Frau", "Mut\u00b7ter", "wie", "bin", "ich", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "NN", "KOKOM", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Da kauft er ihr ein Ringelein von Gold,", "tokens": ["Da", "kauft", "er", "ihr", "ein", "Rin\u00b7ge\u00b7lein", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach ja du sch\u00f6nes M\u00e4delein, wie bin ich dir so hold.", "tokens": ["Ach", "ja", "du", "sch\u00f6\u00b7nes", "M\u00e4\u00b7de\u00b7lein", ",", "wie", "bin", "ich", "dir", "so", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Da war bedecket ein Bettlein mit Flei\u00df,", "tokens": ["Da", "war", "be\u00b7de\u00b7cket", "ein", "Bet\u00b7tlein", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da begr\u00fc\u00dft er das M\u00e4gdlein mit ganzem Flei\u00df,", "tokens": ["Da", "be\u00b7gr\u00fc\u00dft", "er", "das", "M\u00e4gd\u00b7lein", "mit", "gan\u00b7zem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Er drucket sie mit lieblicher Art,", "tokens": ["Er", "dru\u00b7cket", "sie", "mit", "lieb\u00b7li\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hat mir dasselbe M\u00e4gdelein drey Jahr zu Lieb gewart.", "tokens": ["Hat", "mir", "das\u00b7sel\u00b7be", "M\u00e4g\u00b7del\u00b7ein", "drey", "Jahr", "zu", "Lieb", "ge\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "CARD", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}