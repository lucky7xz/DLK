{"dta.poem.8006": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Fr\u00e4ulein sahn vom Schlosse", "tokens": ["Zwei", "Fr\u00e4u\u00b7lein", "sahn", "vom", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in\u2019s tiefe Thal.", "tokens": ["Hin\u00b7ab", "in's", "tie\u00b7fe", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Vater kam zu Rosse,", "tokens": ["Ihr", "Va\u00b7ter", "kam", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er trug ein Kleid von Stahl.", "tokens": ["Er", "trug", "ein", "Kleid", "von", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ewillkomm, Herr Vater, Gottwillkomm!", "tokens": ["\u201e", "will\u00b7komm", ",", "Herr", "Va\u00b7ter", ",", "Gott\u00b7will\u00b7komm", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was bringst du deinen Kindern?", "tokens": ["Was", "bringst", "du", "dei\u00b7nen", "Kin\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir waren beide fromm.\u201c", "tokens": ["Wir", "wa\u00b7ren", "bei\u00b7de", "fromm", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein Kind im gr\u00fcnen Kleide!", "tokens": ["\u201e", "mein", "Kind", "im", "gr\u00fc\u00b7nen", "Klei\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heut hab\u2019 ich dein gedacht.", "tokens": ["Heut", "hab'", "ich", "dein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jagd ist deine Freude", "tokens": ["Die", "Jagd", "ist", "dei\u00b7ne", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Tag und auch bei Nacht.", "tokens": ["Bei", "Tag", "und", "auch", "bei", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Spie\u00df an goldnem Bande hier", "tokens": ["Den", "Spie\u00df", "an", "gold\u00b7nem", "Ban\u00b7de", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm ich dem wilden J\u00e4ger,", "tokens": ["Nahm", "ich", "dem", "wil\u00b7den", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gab ihm den Tod daf\u00fcr.\u201c", "tokens": ["Gab", "ihm", "den", "Tod", "da\u00b7f\u00fcr", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie nahm den Spie\u00df zu H\u00e4nden,", "tokens": ["Sie", "nahm", "den", "Spie\u00df", "zu", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den ihr der Vater bot,", "tokens": ["Den", "ihr", "der", "Va\u00b7ter", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Th\u00e4t in den Wald sich wenden,", "tokens": ["Th\u00e4t", "in", "den", "Wald", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ihr Jagdruf war der Tod.", "tokens": ["Ihr", "Jag\u00b7druf", "war", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort in der Linde Schatten traf", "tokens": ["Dort", "in", "der", "Lin\u00b7de", "Schat\u00b7ten", "traf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie bei den treuen Bracken", "tokens": ["Sie", "bei", "den", "treu\u00b7en", "Bra\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Lieb im tiefen Schlaf.", "tokens": ["Ihr", "Lieb", "im", "tie\u00b7fen", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eich komme zu der Linde,", "tokens": ["\u201e", "ich", "kom\u00b7me", "zu", "der", "Lin\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ich dem Lieb verhie\u00df.\u201c", "tokens": ["Wie", "ich", "dem", "Lieb", "ver\u00b7hie\u00df", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stie\u00df sie gar geschwinde", "tokens": ["Da", "stie\u00df", "sie", "gar", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihre Brust den Spie\u00df.", "tokens": ["In", "ih\u00b7re", "Brust", "den", "Spie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ruhten bei einander k\u00fchl,", "tokens": ["Sie", "ruh\u00b7ten", "bei", "ein\u00b7an\u00b7der", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Waldv\u00f6glein sangen oben,", "tokens": ["Wald\u00b7v\u00f6\u00b7glein", "san\u00b7gen", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Gr\u00fcn Laub herunter fiel.", "tokens": ["Gr\u00fcn", "Laub", "her\u00b7un\u00b7ter", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}