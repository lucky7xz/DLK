{"textgrid.poem.57654": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rzschnee", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4rzschnee rieselt durch die Zweige", "tokens": ["M\u00e4rz\u00b7schnee", "rie\u00b7selt", "durch", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und umspinnt den weiten Wald,", "tokens": ["Und", "um\u00b7spinnt", "den", "wei\u00b7ten", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Alle Vogellieder schweigen", "tokens": ["Al\u00b7le", "Vo\u00b7gel\u00b7lie\u00b7der", "schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es wird so stumm und kalt.", "tokens": ["Und", "es", "wird", "so", "stumm", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine kleine graue Meise", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "grau\u00b7e", "Mei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trillert einmal noch ihr Lied,", "tokens": ["Tril\u00b7lert", "ein\u00b7mal", "noch", "ihr", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal noch ein Sonnenstreifen", "tokens": ["Ein\u00b7mal", "noch", "ein", "Son\u00b7nen\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcnn den stillen Wald durchzieht.", "tokens": ["D\u00fcnn", "den", "stil\u00b7len", "Wald", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf den kalten, nassen Wegen", "tokens": ["Auf", "den", "kal\u00b7ten", ",", "nas\u00b7sen", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gehe ich mit leichtem Fu\u00df,", "tokens": ["Ge\u00b7he", "ich", "mit", "leich\u00b7tem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Lied war mir dein L\u00e4cheln", "tokens": ["Wie", "ein", "Lied", "war", "mir", "dein", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie Sonnenschein dein Gru\u00df.", "tokens": ["Und", "wie", "Son\u00b7nen\u00b7schein", "dein", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}