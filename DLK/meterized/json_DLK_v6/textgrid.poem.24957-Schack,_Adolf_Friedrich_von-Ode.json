{"textgrid.poem.24957": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ode", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder suche, was ihn gl\u00fccklich macht,", "tokens": ["Ein", "je\u00b7der", "su\u00b7che", ",", "was", "ihn", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Reichtum der und der den edlern Schacht,", "tokens": ["Den", "Reich\u00b7tum", "der", "und", "der", "den", "ed\u00b7lern", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "KON", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem er des Wissens Erz entringe;", "tokens": ["Dem", "er", "des", "Wis\u00b7sens", "Erz", "ent\u00b7rin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begeistrung, da\u00df ich aufw\u00e4rts wie der Aar,", "tokens": ["Be\u00b7geis\u00b7trung", ",", "da\u00df", "ich", "auf\u00b7w\u00e4rts", "wie", "der", "Aar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der sonnentrunkene, mich schwinge,", "tokens": ["Der", "son\u00b7nen\u00b7trun\u00b7ke\u00b7ne", ",", "mich", "schwin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Den Becher ich, der mit dem Rebenblut,", "tokens": ["Den", "Be\u00b7cher", "ich", ",", "der", "mit", "dem", "Re\u00b7ben\u00b7blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem geistverkl\u00e4rten durch die Sommerglut,", "tokens": ["Dem", "geist\u00b7ver\u00b7kl\u00e4r\u00b7ten", "durch", "die", "Som\u00b7mer\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Randvoll an meiner Lippe sch\u00e4ume,", "tokens": ["Rand\u00b7voll", "an", "mei\u00b7ner", "Lip\u00b7pe", "sch\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und ich das Saitenspiel, das, wie der Wald", "tokens": ["Und", "ich", "das", "Sai\u00b7ten\u00b7spiel", ",", "das", ",", "wie", "der", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "$,", "PDS", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem leisen Windeshauche, wiederhallt", "tokens": ["Dem", "lei\u00b7sen", "Win\u00b7des\u00b7hau\u00b7che", ",", "wie\u00b7der\u00b7hallt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Beim Atemzuge meiner Tr\u00e4ume.", "tokens": ["Beim", "A\u00b7tem\u00b7zu\u00b7ge", "mei\u00b7ner", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, G\u00f6ttliche, die schon das Kind begl\u00fcckt", "tokens": ["Komm", ",", "G\u00f6tt\u00b7li\u00b7che", ",", "die", "schon", "das", "Kind", "be\u00b7gl\u00fcckt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auf die Lippen ihm den Ku\u00df gedr\u00fcckt,", "tokens": ["Und", "auf", "die", "Lip\u00b7pen", "ihm", "den", "Ku\u00df", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der es zu hohen Dingen weihte;", "tokens": ["Der", "es", "zu", "ho\u00b7hen", "Din\u00b7gen", "weih\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die meiner Jugend D\u00e4mmrung dann erhellt", "tokens": ["Die", "mei\u00b7ner", "Ju\u00b7gend", "D\u00e4mm\u00b7rung", "dann", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und um den Pfad mir eine Wunderwelt", "tokens": ["Und", "um", "den", "Pfad", "mir", "ei\u00b7ne", "Wun\u00b7der\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Tr\u00e4umen und Gesichten reihte!", "tokens": ["Von", "Tr\u00e4u\u00b7men", "und", "Ge\u00b7sich\u00b7ten", "reih\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Quell bist du, dem alles Sein entflie\u00dft;", "tokens": ["Der", "Quell", "bist", "du", ",", "dem", "al\u00b7les", "Sein", "ent\u00b7flie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "PIS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Tropfen deiner Flut nur, und es sprie\u00dft", "tokens": ["Ein", "Trop\u00b7fen", "dei\u00b7ner", "Flut", "nur", ",", "und", "es", "sprie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Winter auf in Fr\u00fchlingsprangen;", "tokens": ["Der", "Win\u00b7ter", "auf", "in", "Fr\u00fch\u00b7lings\u00b7pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Seelenlose selbst im \u00f6den Raum", "tokens": ["Das", "See\u00b7len\u00b7lo\u00b7se", "selbst", "im", "\u00f6\u00b7den", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erhebt sich atmend aus dem dumpfen Traum", "tokens": ["Er\u00b7hebt", "sich", "at\u00b7mend", "aus", "dem", "dum\u00b7pfen", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der K\u00f6rperwelt, die es befangen.", "tokens": ["Der", "K\u00f6r\u00b7per\u00b7welt", ",", "die", "es", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gl\u00fccklich der Staubgeborne, den du liebst,", "tokens": ["Gl\u00fcck\u00b7lich", "der", "Staub\u00b7ge\u00b7bor\u00b7ne", ",", "den", "du", "liebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du Einz'ge, die du Licht und Leben giebst", "tokens": ["Du", "Einz'\u00b7ge", ",", "die", "du", "Licht", "und", "Le\u00b7ben", "giebst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Kelch, um den die Bienen summen,", "tokens": ["Vom", "Kelch", ",", "um", "den", "die", "Bie\u00b7nen", "sum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu der Mark des letzten Sonnenballs,", "tokens": ["Bis", "zu", "der", "Mark", "des", "letz\u00b7ten", "Son\u00b7nen\u00b7balls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo in der Nacht des abgrundtiefen Alls", "tokens": ["Wo", "in", "der", "Nacht", "des", "ab\u00b7grund\u00b7tie\u00b7fen", "Alls"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die T\u00f6ne, graunerf\u00fcllt, verstummen.", "tokens": ["Die", "T\u00f6\u00b7ne", ",", "grau\u00b7ner\u00b7f\u00fcllt", ",", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer einmal nur geruht in deinem Arm,", "tokens": ["Wer", "ein\u00b7mal", "nur", "ge\u00b7ruht", "in", "dei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stets sehnt er sich vom lauten Menschenschwarm", "tokens": ["Stets", "sehnt", "er", "sich", "vom", "lau\u00b7ten", "Men\u00b7schen\u00b7schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In deine heil'ge Stille wieder,", "tokens": ["In", "dei\u00b7ne", "heil'\u00b7ge", "Stil\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, sanft bewegt von deinem Atemzug,", "tokens": ["Und", ",", "sanft", "be\u00b7wegt", "von", "dei\u00b7nem", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Tr\u00e4gt hoch und h\u00f6her ihn im Himmelsflug", "tokens": ["Tr\u00e4gt", "hoch", "und", "h\u00f6\u00b7her", "ihn", "im", "Him\u00b7mels\u00b7flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Dichtung g\u00f6ttliches Gefieder.", "tokens": ["Der", "Dich\u00b7tung", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Aufs Auge hast du Sehkraft ihm getaut,", "tokens": ["Aufs", "Au\u00b7ge", "hast", "du", "Seh\u00b7kraft", "ihm", "ge\u00b7taut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ungeblendet er zur Sonne schaut", "tokens": ["Da\u00df", "un\u00b7ge\u00b7blen\u00b7det", "er", "zur", "Son\u00b7ne", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sicher in des Abgrunds Tiefen;", "tokens": ["Und", "si\u00b7cher", "in", "des", "Ab\u00b7grunds", "Tie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was k\u00fcnftig ist, erschlie\u00dft dein Zauberstab", "tokens": ["Was", "k\u00fcnf\u00b7tig", "ist", ",", "er\u00b7schlie\u00dft", "dein", "Zau\u00b7ber\u00b7stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vor seinem Blick und weckt ihm aus dem Grab", "tokens": ["Vor", "sei\u00b7nem", "Blick", "und", "weckt", "ihm", "aus", "dem", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Geschlechter, welche lang entschliefen.", "tokens": ["Ge\u00b7schlech\u00b7ter", ",", "wel\u00b7che", "lang", "ent\u00b7schlie\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, alle stehn sie um ihn her im Chor,", "tokens": ["Ja", ",", "al\u00b7le", "stehn", "sie", "um", "ihn", "her", "im", "Chor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "PPER", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie alle m\u00fcssen ihm von dem Zuvor", "tokens": ["Sie", "al\u00b7le", "m\u00fcs\u00b7sen", "ihm", "von", "dem", "Zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und von dem k\u00fcnft'gen Einst erz\u00e4hlen;", "tokens": ["Und", "von", "dem", "k\u00fcnft'\u00b7gen", "Einst", "er\u00b7z\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er pre\u00dft sie an sein Herz in Liebesglut,", "tokens": ["Er", "pre\u00dft", "sie", "an", "sein", "Herz", "in", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie tr\u00e4nken ihn mit ihrer Lebensflut", "tokens": ["Sie", "tr\u00e4n\u00b7ken", "ihn", "mit", "ih\u00b7rer", "Le\u00b7bens\u00b7flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und str\u00f6men in ihn ihre Seelen.", "tokens": ["Und", "str\u00f6\u00b7men", "in", "ihn", "ih\u00b7re", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie junge Sonnen an dem Sch\u00f6pfungsherd,", "tokens": ["Wie", "jun\u00b7ge", "Son\u00b7nen", "an", "dem", "Sch\u00f6p\u00b7fungs\u00b7herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo fort und fort das gro\u00dfe Werden g\u00e4rt,", "tokens": ["Wo", "fort", "und", "fort", "das", "gro\u00b7\u00dfe", "Wer\u00b7den", "g\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKVZ", "KON", "PTKVZ", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich in der Flammenglut entz\u00fcnden", "tokens": ["Sich", "in", "der", "Flam\u00b7men\u00b7glut", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bis zum fernsten Raum mit Sturmesmacht", "tokens": ["Und", "bis", "zum", "ferns\u00b7ten", "Raum", "mit", "Stur\u00b7mes\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Begeistert taumeln, um der alten Nacht", "tokens": ["Be\u00b7geis\u00b7tert", "tau\u00b7meln", ",", "um", "der", "al\u00b7ten", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VVINF", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Lichtes Herrlichkeit zu k\u00fcnden;", "tokens": ["Des", "Lich\u00b7tes", "Herr\u00b7lich\u00b7keit", "zu", "k\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So seine Lieder. Ihre Bahn entlang,", "tokens": ["So", "sei\u00b7ne", "Lie\u00b7der", ".", "Ih\u00b7re", "Bahn", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die gro\u00dfe, rollen sie mit Donnergang", "tokens": ["Die", "gro\u00b7\u00dfe", ",", "rol\u00b7len", "sie", "mit", "Don\u00b7ner\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Schwung der flatternden Kometen,", "tokens": ["Im", "Schwung", "der", "flat\u00b7tern\u00b7den", "Ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch l\u00e4cheln wieder milde dann und hold,", "tokens": ["Doch", "l\u00e4\u00b7cheln", "wie\u00b7der", "mil\u00b7de", "dann", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Sternen gleich, die durch des Abends Gold", "tokens": ["Den", "Ster\u00b7nen", "gleich", ",", "die", "durch", "des", "A\u00b7bends", "Gold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zuerst, als Friedensboten, treten.", "tokens": ["Zu\u00b7erst", ",", "als", "Frie\u00b7dens\u00b7bo\u00b7ten", ",", "tre\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Bei ihrem Schein furchtlos durch Grab und Tod", "tokens": ["Bei", "ih\u00b7rem", "Schein", "furcht\u00b7los", "durch", "Grab", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schreitet er hin zum gro\u00dfen Morgenrot;", "tokens": ["Schrei\u00b7tet", "er", "hin", "zum", "gro\u00b7\u00dfen", "Mor\u00b7gen\u00b7rot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Zeit legt ihre Sichel nieder;", "tokens": ["Die", "Zeit", "legt", "ih\u00b7re", "