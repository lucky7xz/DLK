{"textgrid.poem.51203": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Nikolai", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unabsehbar auf der Steppe lieget nah und lieget ferne", "tokens": ["Un\u00b7ab\u00b7seh\u00b7bar", "auf", "der", "Step\u00b7pe", "lie\u00b7get", "nah", "und", "lie\u00b7get", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "ADJA"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ohne Ton die Himmelsglocke, sonder Farbe, sonder Sterne.", "tokens": ["Oh\u00b7ne", "Ton", "die", "Him\u00b7mels\u00b7glo\u00b7cke", ",", "son\u00b7der", "Far\u00b7be", ",", "son\u00b7der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Unaufh\u00f6rlich Schneegest\u00f6ber niederweht auf Dorn und Steine,", "tokens": ["Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "nie\u00b7der\u00b7weht", "auf", "Dorn", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Deckend in den Wagengleisen bleiche polnische Gebeine.", "tokens": ["De\u00b7ckend", "in", "den", "Wa\u00b7gen\u00b7glei\u00b7sen", "blei\u00b7che", "pol\u00b7ni\u00b7sche", "Ge\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Horch, was sauset im Galoppe wie ein Geisterzug vor\u00fcber?", "tokens": ["Horch", ",", "was", "sau\u00b7set", "im", "Ga\u00b7lop\u00b7pe", "wie", "ein", "Geis\u00b7ter\u00b7zug", "vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Langgestreckt schwirrt an der Erde eine wilde Jagd hin\u00fcber.", "tokens": ["Lang\u00b7ge\u00b7streckt", "schwirrt", "an", "der", "Er\u00b7de", "ei\u00b7ne", "wil\u00b7de", "Jagd", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "M\u00e4ntel flattern, Reiter flogen, b\u00e4rt'ge Reiter windgetragen,", "tokens": ["M\u00e4n\u00b7tel", "flat\u00b7tern", ",", "Rei\u00b7ter", "flo\u00b7gen", ",", "b\u00e4rt'\u00b7ge", "Rei\u00b7ter", "wind\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NE", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Rings umschwebt von ihren Lanzen ohne R\u00e4der glitt ein Wagen.", "tokens": ["Rings", "um\u00b7schwebt", "von", "ih\u00b7ren", "Lan\u00b7zen", "oh\u00b7ne", "R\u00e4\u00b7der", "glitt", "ein", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Leise zittert noch die Heide; doch dann wird es stille wieder,", "tokens": ["Lei\u00b7se", "zit\u00b7tert", "noch", "die", "Hei\u00b7de", ";", "doch", "dann", "wird", "es", "stil\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "ADV", "VAFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nur der Schnee in wei\u00dfen Flocken f\u00e4llt mit stummer Last hernieder.", "tokens": ["Nur", "der", "Schnee", "in", "wei\u00b7\u00dfen", "Flo\u00b7cken", "f\u00e4llt", "mit", "stum\u00b7mer", "Last", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und ein Rabe sitzt im Dorne, rauscht empor und kr\u00e4chzet heiser", "tokens": ["Und", "ein", "Ra\u00b7be", "sitzt", "im", "Dor\u00b7ne", ",", "rauscht", "em\u00b7por", "und", "kr\u00e4ch\u00b7zet", "hei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch die ausgestorbnen L\u00fcfte: \u00bbRussenkaiser! Russenkaiser!\u00ab", "tokens": ["Durch", "die", "aus\u00b7ge\u00b7storb\u00b7nen", "L\u00fcf\u00b7te", ":", "\u00bb", "Rus\u00b7sen\u00b7kai\u00b7ser", "!", "Rus\u00b7sen\u00b7kai\u00b7ser", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "NE", "$.", "NE", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Widerhallt es in den H\u00f6hen, und die grauen L\u00fcfte sprechen,", "tokens": ["Wi\u00b7der\u00b7hallt", "es", "in", "den", "H\u00f6\u00b7hen", ",", "und", "die", "grau\u00b7en", "L\u00fcf\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie mich d\u00fcnkt, mit kaltem Hauche: \u00bbWie ein Rohr wird er zerbrechen!\u00ab", "tokens": ["Wie", "mich", "d\u00fcnkt", ",", "mit", "kal\u00b7tem", "Hau\u00b7che", ":", "\u00bb", "Wie", "ein", "Rohr", "wird", "er", "zer\u00b7bre\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$.", "$(", "PWAV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}