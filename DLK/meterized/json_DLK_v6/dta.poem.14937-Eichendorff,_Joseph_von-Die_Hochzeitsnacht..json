{"dta.poem.14937": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeitsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da trat ihr Br\u00e4ut'gam s\u00fc\u00dfe", "tokens": ["Da", "trat", "ihr", "Br\u00e4ut'\u00b7gam", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu ihr aus stiller Nacht,", "tokens": ["Zu", "ihr", "aus", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So freundlich er sie gr\u00fc\u00dfte,", "tokens": ["So", "freund\u00b7lich", "er", "sie", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihr das Herze lacht.", "tokens": ["Da\u00df", "ihr", "das", "Her\u00b7ze", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}