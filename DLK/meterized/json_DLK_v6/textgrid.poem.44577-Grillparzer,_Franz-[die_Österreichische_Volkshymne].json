{"textgrid.poem.44577": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die \u00d6sterreichische Volkshymne]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott erhalte unsern Kaiser", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in ihm das Vaterland,", "tokens": ["Und", "in", "ihm", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Der du Throne h\u00e4ltst und H\u00e4user,", "tokens": ["Der", "du", "Thro\u00b7ne", "h\u00e4ltst", "und", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schirm ihn, Herr, mit starker Hand!", "tokens": ["Schirm", "ihn", ",", "Herr", ",", "mit", "star\u00b7ker", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ein Guter und ein Weiser,", "tokens": ["Da\u00df", "ein", "Gu\u00b7ter", "und", "ein", "Wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er ein Strahl von deinem Blick.", "tokens": ["Er", "ein", "Strahl", "von", "dei\u00b7nem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott erhalte unsern Kaiser,", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Liebe, unser Gl\u00fcck.", "tokens": ["Uns\u00b7re", "Lie\u00b7be", ",", "un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df in seinem Rate sitzen", "tokens": ["La\u00df", "in", "sei\u00b7nem", "Ra\u00b7te", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weisheit und Gerechtigkeit,", "tokens": ["Weis\u00b7heit", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieg von seinen Fahnen blitzen,", "tokens": ["Sieg", "von", "sei\u00b7nen", "Fah\u00b7nen", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt das Recht ihn in den Streit.", "tokens": ["F\u00fchrt", "das", "Recht", "ihn", "in", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch verschm\u00e4hend Lorbeerreiser,", "tokens": ["Doch", "ver\u00b7schm\u00e4\u00b7hend", "Lor\u00b7beer\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei der Friede sein Geschick,", "tokens": ["Sei", "der", "Frie\u00b7de", "sein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott erhalte unsern Kaiser,", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Liebe, unser Gl\u00fcck.", "tokens": ["Uns\u00b7re", "Lie\u00b7be", ",", "un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mach uns einig, Herr der Welten,", "tokens": ["Mach", "uns", "ei\u00b7nig", ",", "Herr", "der", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tilg der Zwietracht Stachel aus,", "tokens": ["Tilg", "der", "Zwiet\u00b7racht", "Sta\u00b7chel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir nur als S\u00f6hne gelten", "tokens": ["Da\u00df", "wir", "nur", "als", "S\u00f6h\u00b7ne", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In desselben Vaters Haus.", "tokens": ["In", "des\u00b7sel\u00b7ben", "Va\u00b7ters", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Vaterherz beweis er", "tokens": ["Und", "ein", "Va\u00b7ter\u00b7herz", "be\u00b7weis", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Ganzen fernstem St\u00fcck.", "tokens": ["In", "des", "Gan\u00b7zen", "ferns\u00b7tem", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott erhalte unsern Kaiser,", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Liebe, unser Gl\u00fcck.", "tokens": ["Uns\u00b7re", "Lie\u00b7be", ",", "un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag dann eine Welt uns dr\u00e4uen,", "tokens": ["Mag", "dann", "ei\u00b7ne", "Welt", "uns", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er mit uns und wir f\u00fcr ihn!", "tokens": ["Er", "mit", "uns", "und", "wir", "f\u00fcr", "ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neu im alten, alt im neuen", "tokens": ["Neu", "im", "al\u00b7ten", ",", "alt", "im", "neu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "$,", "ADJD", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df uns unsre Bahnen ziehn.", "tokens": ["La\u00df", "uns", "uns\u00b7re", "Bah\u00b7nen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sein letzter Pulsschlag leiser,", "tokens": ["Wenn", "sein", "letz\u00b7ter", "Puls\u00b7schlag", "lei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schau er segnend noch zur\u00fcck.", "tokens": ["Schau", "er", "seg\u00b7nend", "noch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott erhalte unsern Kaiser,", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Liebe, unser Gl\u00fcck.", "tokens": ["Uns\u00b7re", "Lie\u00b7be", ",", "un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}