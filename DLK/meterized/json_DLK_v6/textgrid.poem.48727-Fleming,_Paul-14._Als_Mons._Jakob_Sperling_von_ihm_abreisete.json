{"textgrid.poem.48727": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. Als Mons. Jakob Sperling von ihm abreisete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht, der ich war, sonst wolt ich dir was singen,", "tokens": ["Ich", "bin", "nicht", ",", "der", "ich", "war", ",", "sonst", "wolt", "ich", "dir", "was", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du lieber G\u00f6ttersohn und was der Himmel liebt,", "tokens": ["du", "lie\u00b7ber", "G\u00f6t\u00b7ter\u00b7sohn", "und", "was", "der", "Him\u00b7mel", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was, das mein reger Sinn vor diesem hat ge\u00fcbt,", "tokens": ["was", ",", "das", "mein", "re\u00b7ger", "Sinn", "vor", "die\u00b7sem", "hat", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "das bis an den Saturn und h\u00f6her k\u00f6nt' erklingen.", "tokens": ["das", "bis", "an", "den", "Sa\u00b7turn", "und", "h\u00f6\u00b7her", "k\u00f6nt'", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Itzt bin ich ungeschickt zu solchen sch\u00f6nen Dingen.", "tokens": ["Itzt", "bin", "ich", "un\u00b7ge\u00b7schickt", "zu", "sol\u00b7chen", "sch\u00f6\u00b7nen", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Geist will nicht empor, mein Geist der ist betr\u00fcbt.", "tokens": ["Mein", "Geist", "will", "nicht", "em\u00b7por", ",", "mein", "Geist", "der", "ist", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NN", "ART", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Thalia weint mit mir, die mir sonst Freude gibt,", "tokens": ["Tha\u00b7lia", "weint", "mit", "mir", ",", "die", "mir", "sonst", "Freu\u00b7de", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "so da\u00df ich nichts, was wert und lieb ist, vor kan bringen.", "tokens": ["so", "da\u00df", "ich", "nichts", ",", "was", "wert", "und", "lieb", "ist", ",", "vor", "kan", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zeuch hin an deinen Ort mit g\u00f6ttlichen Gef\u00e4hrten,", "tokens": ["Zeuch", "hin", "an", "dei\u00b7nen", "Ort", "mit", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Luft, Wetter, Erde, See, und wo du wirst durchhin,", "tokens": ["Luft", ",", "Wet\u00b7ter", ",", "Er\u00b7de", ",", "See", ",", "und", "wo", "du", "wirst", "durch\u00b7hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "PWAV", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das lasse dich mit Gunst und Willen \u00fcberziehn.", "tokens": ["das", "las\u00b7se", "dich", "mit", "Gunst", "und", "Wil\u00b7len", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Mutter sehe dich, dich Lieben und Begehrten,", "tokens": ["Die", "Mut\u00b7ter", "se\u00b7he", "dich", ",", "dich", "Lie\u00b7ben", "und", "Be\u00b7gehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und die dich sonst gehn an. Und bleib des Freundes Freund", "tokens": ["und", "die", "dich", "sonst", "gehn", "an", ".", "Und", "bleib", "des", "Freun\u00b7des", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "VVFIN", "PTKVZ", "$.", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und mein' es so mit ihm, als wie ers mit dir meint.", "tokens": ["und", "mein'", "es", "so", "mit", "ihm", ",", "als", "wie", "ers", "mit", "dir", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KOUS", "KOKOM", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}}}}