{"textgrid.poem.42244": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Junges Blut", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tanz, mein Liebchen, so wild du", "tokens": ["Tanz", ",", "mein", "Lieb\u00b7chen", ",", "so", "wild", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Tanzen kannst, tanzen kannst!", "tokens": ["Tan\u00b7zen", "kannst", ",", "tan\u00b7zen", "kannst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hurtig tummle dich, wie kein", "tokens": ["Hur\u00b7tig", "tumm\u00b7le", "dich", ",", "wie", "kein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Satan tanzt, Satan tanzt!", "tokens": ["Sa\u00b7tan", "tanzt", ",", "Sa\u00b7tan", "tanzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wirf dir \u00fcbern Kopf die Schuh,", "tokens": ["Wirf", "dir", "\u00fc\u00b7bern", "Kopf", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirf dein R\u00f6ckchen auch dazu!", "tokens": ["Wirf", "dein", "R\u00f6ck\u00b7chen", "auch", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlenkre Fu\u00df und", "tokens": ["Schlen\u00b7kre", "Fu\u00df", "und"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "KON"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Waden ohne Ruh!", "tokens": ["Wa\u00b7den", "oh\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bis es knackt, schwing exakt", "tokens": ["Bis", "es", "knackt", ",", "schwing", "ex\u00b7akt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auch im tollsten Takt", "tokens": ["Auch", "im", "tolls\u00b7ten", "Takt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hurtig, wie vorher nie,", "tokens": ["Hur\u00b7tig", ",", "wie", "vor\u00b7her", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADV", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Deine wei\u00dfen Knie!", "tokens": ["Dei\u00b7ne", "wei\u00b7\u00dfen", "Knie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Lustbefl\u00fcgelt derweil", "tokens": ["Lust\u00b7be\u00b7fl\u00fc\u00b7gelt", "der\u00b7weil"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Zucht dein Hinterteil.", "tokens": ["Zucht", "dein", "Hin\u00b7ter\u00b7teil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Frisch fang an, hei\u00dfer dann,", "tokens": ["Frisch", "fang", "an", ",", "hei\u00b7\u00dfer", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "ADJA", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als dein erster Tanz begann!", "tokens": ["Als", "dein", "ers\u00b7ter", "Tanz", "be\u00b7gann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}