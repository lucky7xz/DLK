{"dta.poem.14856": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Hauptmann aber lehnt' am Baum,", "tokens": ["Ihr", "Haupt\u00b7mann", "a\u00b7ber", "lehnt'", "am", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Todtm\u00fcde von schweren Wunden,", "tokens": ["Todt\u00b7m\u00fc\u00b7de", "von", "schwe\u00b7ren", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schaute wie im Fiebertraum", "tokens": ["Und", "schau\u00b7te", "wie", "im", "Fie\u00b7ber\u00b7traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dem tiefschw\u00fclen Grunde.", "tokens": ["Nach", "dem", "tief\u00b7schw\u00fc\u00b7len", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}