{"textgrid.poem.52581": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lieb um Liebe tauscht ich gern,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb um Liebe tauscht ich gern,", "tokens": ["Lieb", "um", "Lie\u00b7be", "tauscht", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4re Lieb entglommen;", "tokens": ["W\u00e4\u00b7re", "Lieb", "ent\u00b7glom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber Liebe steht so fern,", "tokens": ["A\u00b7ber", "Lie\u00b7be", "steht", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe will nicht kommen.", "tokens": ["Lie\u00b7be", "will", "nicht", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Liebe, Liebe nur von dir,", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", "nur", "von", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebste, w\u00e4re Liebe mir!", "tokens": ["Liebs\u00b7te", ",", "w\u00e4\u00b7re", "Lie\u00b7be", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst du so zufrieden sein,", "tokens": ["Kannst", "du", "so", "zu\u00b7frie\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer ohne Sorgen,", "tokens": ["Im\u00b7mer", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie der Himmel, still und rein,", "tokens": ["Wie", "der", "Him\u00b7mel", ",", "still", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fch am Fr\u00fchlingsmorgen,", "tokens": ["Fr\u00fch", "am", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kennst du nicht des Mittags M\u00fchn,", "tokens": ["Kennst", "du", "nicht", "des", "Mit\u00b7tags", "M\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht das s\u00fc\u00dfe Abendgl\u00fchn?", "tokens": ["Nicht", "das", "s\u00fc\u00b7\u00dfe", "A\u00b7bend\u00b7gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lerne Liebe, Lust und Leid", "tokens": ["Ler\u00b7ne", "Lie\u00b7be", ",", "Lust", "und", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfer Liebe lerne!", "tokens": ["Hei\u00b7\u00dfer", "Lie\u00b7be", "ler\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Endlich auch im Feierkleid", "tokens": ["End\u00b7lich", "auch", "im", "Fei\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeige dich der Sterne!", "tokens": ["Zei\u00b7ge", "dich", "der", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Tr\u00e4ume selig, schm\u00fccke dich,", "tokens": ["Tr\u00e4u\u00b7me", "se\u00b7lig", ",", "schm\u00fc\u00b7cke", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber liebst du, liebe ", "tokens": ["A\u00b7ber", "liebst", "du", ",", "lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}