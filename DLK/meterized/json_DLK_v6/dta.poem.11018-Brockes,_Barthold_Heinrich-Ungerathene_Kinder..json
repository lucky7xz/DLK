{"dta.poem.11018": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungerathene Kinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind unsere Gedanken Kinder der Seelen, wie sie wirklich\nsind:", "tokens": ["Sind", "un\u00b7se\u00b7re", "Ge\u00b7dan\u00b7ken", "Kin\u00b7der", "der", "See\u00b7len", ",", "wie", "sie", "wirk\u00b7lich", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So scheinet (da ja der Gedanke die Seel oft selbst verdammt)", "tokens": ["So", "schei\u00b7net", "(", "da", "ja", "der", "Ge\u00b7dan\u00b7ke", "die", "Seel", "oft", "selbst", "ver\u00b7dammt", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "ADV", "ART", "NN", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Da\u00df man, so oft, als die\u00df geschicht, dasselbige mit Recht wird", "tokens": ["Da\u00df", "man", ",", "so", "oft", ",", "als", "die\u00df", "ge\u00b7schicht", ",", "das\u00b7sel\u00b7bi\u00b7ge", "mit", "Recht", "wird"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "ADV", "ADV", "$,", "KOUS", "PDS", "VVPP", "$,", "PDS", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ja da\u00df man m\u00fc\u00df, es einen M\u00f6rder, von seiner eignen Mut-", "tokens": ["Ja", "da\u00df", "man", "m\u00fc\u00df", ",", "es", "ei\u00b7nen", "M\u00f6r\u00b7der", ",", "von", "sei\u00b7ner", "eig\u00b7nen", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PIS", "VMFIN", "$,", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}