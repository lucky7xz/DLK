{"dta.poem.8239": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Junge Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Aus eigenem Herzen geboren,", "tokens": ["Aus", "ei\u00b7ge\u00b7nem", "Her\u00b7zen", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nie besessen, dennoch verloren.", "tokens": ["Nie", "be\u00b7ses\u00b7sen", ",", "den\u00b7noch", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Schelmenmund, wie jemals einer war,", "tokens": ["Ein", "Schel\u00b7men\u00b7mund", ",", "wie", "je\u00b7mals", "ei\u00b7ner", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein launisch Kind; doch all' ihr Widerstreben", "tokens": ["Ein", "lau\u00b7nisch", "Kind", ";", "doch", "all'", "ihr", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "ADV", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bezwingt ihr Herz, das mir so ganz ergeben.", "tokens": ["Be\u00b7zwingt", "ihr", "Herz", ",", "das", "mir", "so", "ganz", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon lange sitzt sie vor mir, tr\u00e4umerisch", "tokens": ["Schon", "lan\u00b7ge", "sitzt", "sie", "vor", "mir", ",", "tr\u00e4u\u00b7me\u00b7risch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihren Beinchen baumelnd, auf dem Tisch;", "tokens": ["Mit", "ih\u00b7ren", "Be\u00b7in\u00b7chen", "bau\u00b7melnd", ",", "auf", "dem", "Tisch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun springt sie auf; an meines Stuhles Lehne", "tokens": ["Nun", "springt", "sie", "auf", ";", "an", "mei\u00b7nes", "Stuh\u00b7les", "Leh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4ngt sie sich, schmollend ob der stummen Scene.", "tokens": ["H\u00e4ngt", "sie", "sich", ",", "schmol\u00b7lend", "ob", "der", "stum\u00b7men", "Sce\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJD", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "\u201eich liebe dich!\u201c \u2014 \u201eDu bist sehr interessant.\u201c", "tokens": ["\u201e", "ich", "lie\u00b7be", "dich", "!", "\u201c", "\u201e", "Du", "bist", "sehr", "in\u00b7ter\u00b7es\u00b7sant", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "$(", "$(", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eich liebe dich!\u201c \u2014 \u201eAch das ist l\u00e4ngst bekannt!", "tokens": ["\u201e", "ich", "lie\u00b7be", "dich", "!", "\u201c", "\u201e", "Ach", "das", "ist", "l\u00e4ngst", "be\u00b7kannt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "$(", "$(", "$(", "ITJ", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich lieb' Geschichten, neu und nicht erfunden \u2014", "tokens": ["Ich", "lieb'", "Ge\u00b7schich\u00b7ten", ",", "neu", "und", "nicht", "er\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJD", "KON", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erz\u00e4hlst du nicht, ich bin im Nu verschwunden.\u201c \u2014", "tokens": ["Er\u00b7z\u00e4hlst", "du", "nicht", ",", "ich", "bin", "im", "Nu", "ver\u00b7schwun\u00b7den", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "APPRART", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u201eso h\u00f6r'! J\u00fcngst tr\u00e4umte mir\u201c \u2014 \u2014 \u201eDas ist", "tokens": ["\u201e", "so", "h\u00f6r'", "!", "J\u00fcngst", "tr\u00e4um\u00b7te", "mir", "\u201c", "\u201e", "Das", "ist"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$.", "NN", "VVFIN", "PPER", "$(", "$(", "$(", "$(", "PDS", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "nicht wahr!\u201c \u2014", "tokens": ["nicht", "wahr", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "$.", "$(", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u201ewahr ist's! Mir tr\u00e4umt', ich s\u00e4he auf ein Haar", "tokens": ["\u201e", "wahr", "ist's", "!", "Mir", "tr\u00e4umt'", ",", "ich", "s\u00e4\u00b7he", "auf", "ein", "Haar"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dich selbst Stra\u00df auf und ab in Prachtgew\u00e4ndern", "tokens": ["Dich", "selbst", "Stra\u00df", "auf", "und", "ab", "in", "Pracht\u00b7ge\u00b7w\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "PTKVZ", "KON", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "An eines Mannes Arm gem\u00e4chlich schlendern;", "tokens": ["An", "ei\u00b7nes", "Man\u00b7nes", "Arm", "ge\u00b7m\u00e4ch\u00b7lich", "schlen\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und dieser Mann\u201c \u2014 \u2014 \u201eder war?\u201c \u2014 \u201eder war", "tokens": ["Und", "die\u00b7ser", "Mann", "\u201c", "\u201e", "der", "war", "?", "\u201c", "\u201e", "der", "war"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word"], "pos": ["KON", "PDAT", "NN", "$(", "$(", "$(", "$(", "PDS", "VAFIN", "$.", "$(", "$(", "$(", "PDS", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "nicht ich!\u201c \u2014", "tokens": ["nicht", "ich", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["PTKNEG", "PPER", "$.", "$(", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u201edu l\u00fcgst!\u201c \u2014 \u201eMein Herz, ich sah dich sicherlich \u2014", "tokens": ["\u201e", "du", "l\u00fcgst", "!", "\u201c", "\u201e", "Mein", "Herz", ",", "ich", "sah", "dich", "si\u00b7cher\u00b7lich"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "$(", "$(", "$(", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihr senktet Aug' in Auge voll Entz\u00fccken,", "tokens": ["Ihr", "senk\u00b7tet", "Aug'", "in", "Au\u00b7ge", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich stand seitab, gleichg\u00fcltig deinen Blicken.\u201c", "tokens": ["Ich", "stand", "seit\u00b7ab", ",", "gleich\u00b7g\u00fcl\u00b7tig", "dei\u00b7nen", "Bli\u00b7cken", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u201eder Mutter sag ich's!\u201c ruft das tolle Kind,", "tokens": ["\u201e", "der", "Mut\u00b7ter", "sag", "ich's", "!", "\u201c", "ruft", "das", "tol\u00b7le", "Kind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PIS", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201ewas f\u00fcr ein Traum!\u201c Da hasch' ich sie geschwind,", "tokens": ["\u201e", "was", "f\u00fcr", "ein", "Traum", "!", "\u201c", "Da", "hasch'", "ich", "sie", "ge\u00b7schwind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und diese frevelhaften Lippen m\u00fcssen,", "tokens": ["Und", "die\u00b7se", "fre\u00b7vel\u00b7haf\u00b7ten", "Lip\u00b7pen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sie verbrochen, ohne Gnade b\u00fc\u00dfen.", "tokens": ["Was", "sie", "ver\u00b7bro\u00b7chen", ",", "oh\u00b7ne", "Gna\u00b7de", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "KOUI", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}