{"dta.poem.8093": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00f6nigssohn, mir aber ist", "tokens": ["Ein", "K\u00f6\u00b7nigs\u00b7sohn", ",", "mir", "a\u00b7ber", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heimath l\u00e4ngst verloreu.", "tokens": ["Die", "Hei\u00b7math", "l\u00e4ngst", "ver\u00b7lo\u00b7reu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erst hat die schwache Mutter mich,", "tokens": ["Erst", "hat", "die", "schwa\u00b7che", "Mut\u00b7ter", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die irdische, geboren.", "tokens": ["Die", "ir\u00b7di\u00b7sche", ",", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch nun gebar die zweite Mutter,", "tokens": ["Doch", "nun", "ge\u00b7bar", "die", "zwei\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das starke Meer, mich wieder.", "tokens": ["Das", "star\u00b7ke", "Meer", ",", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Riesenarmen wiegte sie", "tokens": ["In", "Rie\u00b7se\u00b7nar\u00b7men", "wieg\u00b7te", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich selbst und meine Br\u00fcder.", "tokens": ["Mich", "selbst", "und", "mei\u00b7ne", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Andern all ertrugen\u2019s nicht,", "tokens": ["Die", "An\u00b7dern", "all", "er\u00b7tru\u00b7gen's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich brachte sie hier zum Strande.", "tokens": ["Mich", "brach\u00b7te", "sie", "hier", "zum", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zum Reiche wohl erkor sie mir", "tokens": ["Zum", "Rei\u00b7che", "wohl", "er\u00b7kor", "sie", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All diese weiten Lande.", "tokens": ["All", "die\u00b7se", "wei\u00b7ten", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}}}}