{"dta.poem.18690": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Geheimes .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber meines Liebchens Aeugeln", "tokens": ["Ue\u00b7ber", "mei\u00b7nes", "Lieb\u00b7chens", "A\u00b7e\u00b7u\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stehn verwundert alle Leute,", "tokens": ["Stehn", "ver\u00b7wun\u00b7dert", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich, der Wissende, dagegen", "tokens": ["Ich", ",", "der", "Wis\u00b7sen\u00b7de", ",", "da\u00b7ge\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PAV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Weiss recht gut was das bedeute.", "tokens": ["Weiss", "recht", "gut", "was", "das", "be\u00b7deu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PWS", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn es heisst; Ich liebe diesen,", "tokens": ["Denn", "es", "heisst", ";", "Ich", "lie\u00b7be", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PDAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nicht etwa den und jenen,", "tokens": ["Und", "nicht", "et\u00b7wa", "den", "und", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "KON", "PDS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lasset nur ihr guten Leute", "tokens": ["Las\u00b7set", "nur", "ihr", "gu\u00b7ten", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Euer Wundern, euer Sehnen.", "tokens": ["Eu\u00b7er", "Wun\u00b7dern", ",", "eu\u00b7er", "Seh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, mit ungeheuren M\u00e4chten", "tokens": ["Ja", ",", "mit", "un\u00b7ge\u00b7heu\u00b7ren", "M\u00e4ch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicket sie wohl in die Runde;", "tokens": ["Bli\u00b7cket", "sie", "wohl", "in", "die", "Run\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Doch sie sucht nur zu verk\u00fcnden", "tokens": ["Doch", "sie", "sucht", "nur", "zu", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm die n\u00e4chste s\u00fcsse Stunde.", "tokens": ["Ihm", "die", "n\u00e4chs\u00b7te", "s\u00fcs\u00b7se", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}