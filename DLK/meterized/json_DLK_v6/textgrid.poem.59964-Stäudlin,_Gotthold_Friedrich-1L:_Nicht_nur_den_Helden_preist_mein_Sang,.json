{"textgrid.poem.59964": {"metadata": {"author": {"name": "St\u00e4udlin, Gotthold Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht nur den Helden preist mein Sang,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht nur den Helden preist mein Sang,", "tokens": ["Nicht", "nur", "den", "Hel\u00b7den", "preist", "mein", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der blut'ge Lorbeern sich errang;", "tokens": ["Der", "blut'\u00b7ge", "Lor\u00b7beern", "sich", "er\u00b7rang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dich, das sonder Grau'n mit ihm", "tokens": ["Auch", "dich", ",", "das", "son\u00b7der", "Grau'n", "mit", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich st\u00fcrzt ins Schlachtenungest\u00fcm;", "tokens": ["Sich", "st\u00fcrzt", "ins", "Schlach\u00b7ten\u00b7un\u00b7ge\u00b7st\u00fcm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir, wert vor andern Tieren mir,", "tokens": ["Dir", ",", "wert", "vor", "an\u00b7dern", "Tie\u00b7ren", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du mutiges und edles Tier!", "tokens": ["Du", "mu\u00b7ti\u00b7ges", "und", "ed\u00b7les", "Tier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir \u2013 denn begeistern kannst auch du \u2013", "tokens": ["Dir", "\u2013", "denn", "be\u00b7geis\u00b7tern", "kannst", "auch", "du", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KON", "VVINF", "VMFIN", "ADV", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jauchzt mein Gesang Begeistrung zu.", "tokens": ["Jauchzt", "mein", "Ge\u00b7sang", "Be\u00b7geis\u00b7trung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was hoch der Sch\u00f6pfung Herrn, den Mann,", "tokens": ["Was", "hoch", "der", "Sch\u00f6p\u00b7fung", "Herrn", ",", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor seinen Br\u00fcdern heben kann,", "tokens": ["Vor", "sei\u00b7nen", "Br\u00fc\u00b7dern", "he\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das gab auch dir, der alles schafft,", "tokens": ["Das", "gab", "auch", "dir", ",", "der", "al\u00b7les", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gab Sch\u00f6nheit dir und Stolz und Kraft!", "tokens": ["Gab", "Sch\u00f6n\u00b7heit", "dir", "und", "Stolz", "und", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hoch hebest du und frei dein Haupt;", "tokens": ["Hoch", "he\u00b7best", "du", "und", "frei", "dein", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es beben, wenn die Nase schnaubt", "tokens": ["Es", "be\u00b7ben", ",", "wenn", "die", "Na\u00b7se", "schnaubt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Blitze wirft dein Flammenblick,", "tokens": ["Und", "Blit\u00b7ze", "wirft", "dein", "Flam\u00b7men\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die kleinern Tiere scheu zur\u00fcck.", "tokens": ["Die", "klei\u00b7nern", "Tie\u00b7re", "scheu", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n w\u00f6lbte Gottes Bildnerhand", "tokens": ["Sch\u00f6n", "w\u00f6lb\u00b7te", "Got\u00b7tes", "Bild\u00b7ner\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Felsennacken und umwand", "tokens": ["Den", "Fel\u00b7sen\u00b7na\u00b7cken", "und", "um\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn mit der goldnen Majest\u00e4t", "tokens": ["Ihn", "mit", "der", "gold\u00b7nen", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der M\u00e4hne, die im Sturme weht!", "tokens": ["Der", "M\u00e4h\u00b7ne", ",", "die", "im", "Stur\u00b7me", "weht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie schmiedet' hart und fein wie Stahl", "tokens": ["Sie", "schmie\u00b7det'", "hart", "und", "fein", "wie", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir deine Knochen allzumal,", "tokens": ["Dir", "dei\u00b7ne", "Kno\u00b7chen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gab Eisendauer deiner Kraft,", "tokens": ["Gab", "Ei\u00b7sen\u00b7dau\u00b7er", "dei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die selbst dem Alter nicht erschlafft.", "tokens": ["Die", "selbst", "dem", "Al\u00b7ter", "nicht", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie gab noch, als so sch\u00f6n gebaut", "tokens": ["Sie", "gab", "noch", ",", "als", "so", "sch\u00f6n", "ge\u00b7baut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du standest mit der Spiegelhaut,", "tokens": ["Du", "stan\u00b7dest", "mit", "der", "Spie\u00b7gel\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, da\u00df vollkommen sei und reif", "tokens": ["Dir", ",", "da\u00df", "voll\u00b7kom\u00b7men", "sei", "und", "reif"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ADJD", "VAFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Meisterwerk, den stolzen Schweif!", "tokens": ["Ihr", "Meis\u00b7ter\u00b7werk", ",", "den", "stol\u00b7zen", "Schweif", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du eilest, wie ein deutscher Mann,", "tokens": ["Du", "ei\u00b7lest", ",", "wie", "ein", "deut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine gro\u00dfe That begann,", "tokens": ["Der", "ei\u00b7ne", "gro\u00b7\u00dfe", "That", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll hei\u00dfen Eifers, sonder Ruh'", "tokens": ["Voll", "hei\u00b7\u00dfen", "Ei\u00b7fers", ",", "son\u00b7der", "Ruh'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem fern gesteckten Ziele zu;", "tokens": ["Dem", "fern", "ge\u00b7steck\u00b7ten", "Zie\u00b7le", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dich h\u00e4lt nicht, wie den kleinen Mann,", "tokens": ["Dich", "h\u00e4lt", "nicht", ",", "wie", "den", "klei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine gro\u00dfe That begann,", "tokens": ["Der", "ei\u00b7ne", "gro\u00b7\u00dfe", "That", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinem windeschnellen Lauf", "tokens": ["In", "dei\u00b7nem", "win\u00b7de\u00b7schnel\u00b7len", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hindernisse Dr\u00e4uen auf!", "tokens": ["Der", "Hin\u00b7der\u00b7nis\u00b7se", "Dr\u00e4u\u00b7en", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du fliegst in dunkler Mitternacht,", "tokens": ["Du", "fliegst", "in", "dunk\u00b7ler", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eh' dein Lenker es gedacht,", "tokens": ["Noch", "eh'", "dein", "Len\u00b7ker", "es", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fels\u00fcber hier, strom\u00fcber dort", "tokens": ["Fel\u00b7s\u00fc\u00b7ber", "hier", ",", "strom\u00b7\u00fc\u00b7ber", "dort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringst ihn jach an seinen Ort;", "tokens": ["Und", "bringst", "ihn", "jach", "an", "sei\u00b7nen", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Bist willig \u2013 aber stolz und frei,", "tokens": ["Bist", "wil\u00b7lig", "\u2013", "a\u00b7ber", "stolz", "und", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tyrannisch gegen Tyrannei,", "tokens": ["Ty\u00b7ran\u00b7nisch", "ge\u00b7gen", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirfst deinen b\u00fcb'schen Qu\u00e4ler ab", "tokens": ["Wirfst", "dei\u00b7nen", "b\u00fcb'\u00b7schen", "Qu\u00e4\u00b7ler", "ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00f6ffnest ihm ein blutig Grab!", "tokens": ["Und", "\u00f6ff\u00b7nest", "ihm", "ein", "blu\u00b7tig", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du wei\u00dft's: dir gab, der dich erschuf,", "tokens": ["Du", "wei\u00dft's", ":", "dir", "gab", ",", "der", "dich", "er\u00b7schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wetterschlag in deinen Huf;", "tokens": ["Den", "Wet\u00b7ter\u00b7schlag", "in", "dei\u00b7nen", "Huf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hast du nie die Macht entweiht,", "tokens": ["Doch", "hast", "du", "nie", "die", "Macht", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie K\u00f6nige, durch Grausamkeit!", "tokens": ["Wie", "K\u00f6\u00b7ni\u00b7ge", ",", "durch", "Grau\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Den Feuersinn verk\u00fcndet schon", "tokens": ["Den", "Feu\u00b7er\u00b7sinn", "ver\u00b7k\u00fcn\u00b7det", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wieherns freudigwilder Ton;", "tokens": ["Des", "Wie\u00b7herns", "freu\u00b7dig\u00b7wil\u00b7der", "Ton", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du stampfest, da\u00df die R\u00fcstung t\u00f6nt", "tokens": ["Du", "stamp\u00b7fest", ",", "da\u00df", "die", "R\u00fcs\u00b7tung", "t\u00f6nt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unter dir der Boden dr\u00f6hnt;", "tokens": ["Und", "un\u00b7ter", "dir", "der", "Bo\u00b7den", "dr\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und bei\u00dfest knirschend deinen Zaum,", "tokens": ["Und", "bei\u00b7\u00dfest", "knir\u00b7schend", "dei\u00b7nen", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df auf die Schenkel spritzt der Schaum,", "tokens": ["Da\u00df", "auf", "die", "Schen\u00b7kel", "spritzt", "der", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dampfst Wolken aus von hei\u00dfem Duft,", "tokens": ["Dampfst", "Wol\u00b7ken", "aus", "von", "hei\u00b7\u00dfem", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos":