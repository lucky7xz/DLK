{"dta.poem.2847": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir ein Schritt entschl\u00fcpft ist ein unebener,", "tokens": ["Wenn", "dir", "ein", "Schritt", "ent\u00b7schl\u00fcpft", "ist", "ein", "un\u00b7e\u00b7be\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "So sorge da\u00df auch der sei kein vergebener.", "tokens": ["So", "sor\u00b7ge", "da\u00df", "auch", "der", "sei", "kein", "ver\u00b7ge\u00b7be\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "ADV", "ART", "VAFIN", "PIAT", "ADJA", "$."], "meter": "-+--+--+----", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Nachsichtiger mach' er dich f\u00fcr Unebenheiten", "tokens": ["Nach\u00b7sich\u00b7ti\u00b7ger", "mach'", "er", "dich", "f\u00fcr", "Un\u00b7e\u00b7ben\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In fremden Haus- und Stadt- und Weltbegebenheiten.", "tokens": ["In", "frem\u00b7den", "Haus", "und", "Stadt", "und", "Welt\u00b7be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "TRUNC", "KON", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn lerne, weil die Welt ist so uneben nun,", "tokens": ["Denn", "ler\u00b7ne", ",", "weil", "die", "Welt", "ist", "so", "un\u00b7e\u00b7ben", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VAFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Vorsichtiger den Schritt ein andermal zu thun.", "tokens": ["Vor\u00b7sich\u00b7ti\u00b7ger", "den", "Schritt", "ein", "an\u00b7der\u00b7mal", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So bleibt der Fehltritt dir in jeder Hinsicht werth,", "tokens": ["So", "bleibt", "der", "Fehl\u00b7tritt", "dir", "in", "je\u00b7der", "Hin\u00b7sicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er so Vorsicht hat als Nachsicht dich gelehrt.", "tokens": ["Weil", "er", "so", "Vor\u00b7sicht", "hat", "als", "Nach\u00b7sicht", "dich", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "KOKOM", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}