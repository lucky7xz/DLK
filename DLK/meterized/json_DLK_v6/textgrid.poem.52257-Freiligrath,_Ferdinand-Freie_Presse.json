{"textgrid.poem.52257": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Freie Presse", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Festen Tons zu seinen Leuten spricht der Herr der Druckerei:", "tokens": ["Fes\u00b7ten", "Tons", "zu", "sei\u00b7nen", "Leu\u00b7ten", "spricht", "der", "Herr", "der", "Dru\u00b7cke\u00b7rei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbmorgen, wi\u00dft ihr, soll es losgehn, und zum Schie\u00dfen braucht man Blei!", "tokens": ["\u00bb", "mor\u00b7gen", ",", "wi\u00dft", "ihr", ",", "soll", "es", "los\u00b7gehn", ",", "und", "zum", "Schie\u00b7\u00dfen", "braucht", "man", "Blei", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,", "KON", "APPRART", "NN", "VVFIN", "PIS", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wohl, wir haben unsre Schriften: \u2013 morgen in die Reihn getreten!", "tokens": ["Wohl", ",", "wir", "ha\u00b7ben", "uns\u00b7re", "Schrif\u00b7ten", ":", "\u2013", "mor\u00b7gen", "in", "die", "Reihn", "ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "APPR", "ART", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Heute Munition gegossen aus metallnen Alphabeten!", "tokens": ["Heu\u00b7te", "Mu\u00b7ni\u00b7tion", "ge\u00b7gos\u00b7sen", "aus", "me\u00b7tall\u00b7nen", "Al\u00b7pha\u00b7be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Hier die Formen, hier die Tiegel! auch die Kohlen fach' ich an!", "tokens": ["Hier", "die", "For\u00b7men", ",", "hier", "die", "Tie\u00b7gel", "!", "auch", "die", "Koh\u00b7len", "fach'", "ich", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$.", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und die Pforten sind verrammelt, da\u00df uns niemand st\u00f6ren kann!", "tokens": ["Und", "die", "Pfor\u00b7ten", "sind", "ver\u00b7ram\u00b7melt", ",", "da\u00df", "uns", "nie\u00b7mand", "st\u00f6\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An die Arbeit denn, ihr Herren! Alle, die ihr setzt und pre\u00dft!", "tokens": ["An", "die", "Ar\u00b7beit", "denn", ",", "ihr", "Her\u00b7ren", "!", "Al\u00b7le", ",", "die", "ihr", "setzt", "und", "pre\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "PPOSAT", "NN", "$.", "PIS", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Helft mir auf die Beine bringen dieses Freiheitsmanifest!\u00ab", "tokens": ["Helft", "mir", "auf", "die", "Bei\u00b7ne", "brin\u00b7gen", "die\u00b7ses", "Frei\u00b7heits\u00b7ma\u00b7ni\u00b7fest", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Spricht's, und wirft die ersten Lettern in den Tiegel frischer Hand.", "tokens": ["Spricht's", ",", "und", "wirft", "die", "ers\u00b7ten", "Let\u00b7tern", "in", "den", "Tie\u00b7gel", "fri\u00b7scher", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Von der Hitze bald geschmolzen brodeln Perl' und Diamant;", "tokens": ["Von", "der", "Hit\u00b7ze", "bald", "ge\u00b7schmol\u00b7zen", "bro\u00b7deln", "Perl'", "und", "Di\u00b7a\u00b7mant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Brodeln Kolonel und Korpus; hier die Antiqua, dort Fraktur", "tokens": ["Bro\u00b7deln", "Ko\u00b7lo\u00b7nel", "und", "Kor\u00b7pus", ";", "hier", "die", "An\u00b7ti\u00b7qua", ",", "dort", "Frak\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "KON", "NE", "$.", "ADV", "ART", "NE", "$,", "ADV", "NN"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.4": {"text": "Werfen radikale Blasen, dreist umgehend die Zensur.", "tokens": ["Wer\u00b7fen", "ra\u00b7di\u00b7ka\u00b7le", "Bla\u00b7sen", ",", "dreist", "um\u00b7ge\u00b7hend", "die", "Zen\u00b7sur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "VVPP", "ART", "NN", "$."], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Dampfend in die Kugelformen zischt die gl\u00fchnde Masse dann: \u2013", "tokens": ["Damp\u00b7fend", "in", "die", "Ku\u00b7gel\u00b7for\u00b7men", "zischt", "die", "gl\u00fchn\u00b7de", "Mas\u00b7se", "dann", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So die ganze lange Herbstnacht schaffen diese zwanzig Mann;", "tokens": ["So", "die", "gan\u00b7ze", "lan\u00b7ge", "Herbst\u00b7nacht", "schaf\u00b7fen", "die\u00b7se", "zwan\u00b7zig", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "PDAT", "CARD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Atmen r\u00fcstig in die Kohlen; sch\u00fcren, schmelzen unverdrossen,", "tokens": ["At\u00b7men", "r\u00fcs\u00b7tig", "in", "die", "Koh\u00b7len", ";", "sch\u00fc\u00b7ren", ",", "schmel\u00b7zen", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$.", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis in runde, blanke Kugeln Schrift und Zeug sie ungegossen!", "tokens": ["Bis", "in", "run\u00b7de", ",", "blan\u00b7ke", "Ku\u00b7geln", "Schrift", "und", "Zeug", "sie", "un\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "$,", "ADJA", "NN", "NN", "KON", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wohlverpackt in grauen Beuteln liegt der Vorrat an der Erde,", "tokens": ["Wohl\u00b7ver\u00b7packt", "in", "grau\u00b7en", "Beu\u00b7teln", "liegt", "der", "Vor\u00b7rat", "an", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Fertig, da\u00df er mit der Fr\u00fche br\u00fchwarm ausgegeben werde!", "tokens": ["Fer\u00b7tig", ",", "da\u00df", "er", "mit", "der", "Fr\u00fc\u00b7he", "br\u00fch\u00b7warm", "aus\u00b7ge\u00b7ge\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eine dreiste Morgensitzung! Wahrlich, gleich beherzt und k\u00fchn", "tokens": ["Ei\u00b7ne", "dreis\u00b7te", "Mor\u00b7gen\u00b7sit\u00b7zung", "!", "Wahr\u00b7lich", ",", "gleich", "be\u00b7herzt", "und", "k\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sah man keine noch entschwirren dieser alren Offizin!", "tokens": ["Sah", "man", "kei\u00b7ne", "noch", "ent\u00b7schwir\u00b7ren", "die\u00b7ser", "al\u00b7ren", "Of\u00b7fi\u00b7zin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "ADV", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und der Meister sieht es d\u00fcster, legt die Rechte auf sein Herz:", "tokens": ["Und", "der", "Meis\u00b7ter", "sieht", "es", "d\u00fcs\u00b7ter", ",", "legt", "die", "Rech\u00b7te", "auf", "sein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbda\u00df es also mu\u00dfte kommen, mir und vielen macht es Schmerz!", "tokens": ["\u00bb", "da\u00df", "es", "al\u00b7so", "mu\u00df\u00b7te", "kom\u00b7men", ",", "mir", "und", "vie\u00b7len", "macht", "es", "Schmerz", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,", "PPER", "KON", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch \u2013 welch Mittel noch ist \u00fcbrig, und wie ", "tokens": ["Doch", "\u2013", "welch", "Mit\u00b7tel", "noch", "ist", "\u00fcb\u00b7rig", ",", "und", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$(", "PWAT", "NN", "ADV", "VAFIN", "ADJD", "$,", "KON", "PWAV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur als Kugel mag die Type dieser Tage sich befrein!", "tokens": ["Nur", "als", "Ku\u00b7gel", "mag", "die", "Ty\u00b7pe", "die\u00b7ser", "Ta\u00b7ge", "sich", "be\u00b7fr\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VMFIN", "ART", "NN", "PDAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Wohl soll der Gedanke siegen \u2013 nicht des Stoffes rohe Kraft!", "tokens": ["Wohl", "soll", "der", "Ge\u00b7dan\u00b7ke", "sie\u00b7gen", "\u2013", "nicht", "des", "Stof\u00b7fes", "ro\u00b7he", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$(", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Doch man band ihn, man zertrat ihn, doch man warf ihn schn\u00f6d in Haft!", "tokens": ["Doch", "man", "band", "ihn", ",", "man", "zer\u00b7trat", "ihn", ",", "doch", "man", "warf", "ihn", "schn\u00f6d", "in", "Haft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "PPER", "$,", "KON", "PIS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sei es denn! In die Muskete mit dem Ladstock la\u00dft euch rammen!", "tokens": ["Sei", "es", "denn", "!", "In", "die", "Mus\u00b7ke\u00b7te", "mit", "dem", "Lad\u00b7stock", "la\u00dft", "euch", "ram\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$.", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Auch in solchem Winkelhaken steht als K\u00e4mpfer treu beisammen!", "tokens": ["Auch", "in", "sol\u00b7chem", "Win\u00b7kel\u00b7ha\u00b7ken", "steht", "als", "K\u00e4mp\u00b7fer", "treu", "bei\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "KOUS", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Jauchzt ein rauhes Lied der Freiheit, jauchzt und pfeift es hoch in L\u00fcften!", "tokens": ["Jauchzt", "ein", "rau\u00b7hes", "Lied", "der", "Frei\u00b7heit", ",", "jauchzt", "und", "pfeift", "es", "hoch", "in", "L\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schlagt die Knechte, schlagt die S\u00f6ldner, schlagt den allerh\u00f6chsten Toren,", "tokens": ["Schlagt", "die", "Knech\u00b7te", ",", "schlagt", "die", "S\u00f6ld\u00b7ner", ",", "schlagt", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der sich ", "tokens": ["Der", "sich"], "token_info": ["word", "word"], "pos": ["ART", "PRF"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "F\u00fcr die ", "tokens": ["F\u00fcr", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Bald aus Leichen und aus Tr\u00fcmmern graben wir euch wieder aus!", "tokens": ["Bald", "aus", "Lei\u00b7chen", "und", "aus", "Tr\u00fcm\u00b7mern", "gra\u00b7ben", "wir", "euch", "wie\u00b7der", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gie\u00dfen euch aus stumpfen Kugeln wieder um in scharfe Lettern \u2013", "tokens": ["Gie\u00b7\u00dfen", "euch", "aus", "stump\u00b7fen", "Ku\u00b7geln", "wie\u00b7der", "um", "in", "schar\u00b7fe", "Let\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "+-+++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Horch! ein Pochen an der Haust\u00fcr! und Trompeten h\u00f6r' ich schmettern!", "tokens": ["Horch", "!", "ein", "Po\u00b7chen", "an", "der", "Haus\u00b7t\u00fcr", "!", "und", "Trom\u00b7pe\u00b7ten", "h\u00f6r'", "ich", "schmet\u00b7tern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPR", "ART", "NN", "$.", "KON", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+--++-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Jetzt ein Schu\u00df! \u2013 Und wieder einer! \u2013 Die Signale sind's Gesellen!", "tokens": ["Jetzt", "ein", "Schu\u00df", "!", "\u2013", "Und", "wie\u00b7der", "ei\u00b7ner", "!", "\u2013", "Die", "Sig\u00b7na\u00b7le", "sin\u00b7d's", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "wo