{"dta.poem.12113": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil du irrgiengest, weil du dich irrf\u00fchren lie\u00dfest,", "tokens": ["Weil", "du", "irr\u00b7gien\u00b7gest", ",", "weil", "du", "dich", "irr\u00b7f\u00fch\u00b7ren", "lie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kamst du an be\u00dfres Ziel als du dir selbst verhie\u00dfest.", "tokens": ["Kamst", "du", "an", "be\u00df\u00b7res", "Ziel", "als", "du", "dir", "selbst", "ver\u00b7hie\u00b7\u00dfest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist recht sch\u00f6n vom Gl\u00fcck, das ist von Gott recht gut,", "tokens": ["Das", "ist", "recht", "sch\u00f6n", "vom", "Gl\u00fcck", ",", "das", "ist", "von", "Gott", "recht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "APPRART", "NN", "$,", "PDS", "VAFIN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Herrn, auf des Gehei\u00df die Magd solch Wunder thut.", "tokens": ["Dem", "Herrn", ",", "auf", "des", "Ge\u00b7hei\u00df", "die", "Magd", "solch", "Wun\u00b7der", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei daf\u00fcr dankbar nur! doch w\u00e4r' es hinterr\u00fccks,", "tokens": ["Sei", "da\u00b7f\u00fcr", "dank\u00b7bar", "nur", "!", "doch", "w\u00e4r'", "es", "hin\u00b7ter\u00b7r\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADJD", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du mit Flei\u00df irrgiengst in Hoffnung gleichen Gl\u00fccks.", "tokens": ["Wenn", "du", "mit", "Flei\u00df", "irr\u00b7giengst", "in", "Hoff\u00b7nung", "glei\u00b7chen", "Gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}