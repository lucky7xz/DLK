{"textgrid.poem.35538": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Erste Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gerne lass' ich von der ersten Sonne", "tokens": ["Wie", "ger\u00b7ne", "lass'", "ich", "von", "der", "ers\u00b7ten", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich bescheinen! \u2013 Wenn der Januar", "tokens": ["Mich", "be\u00b7schei\u00b7nen", "!", "\u2013", "Wenn", "der", "Ja\u00b7nu\u00b7ar"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "$(", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit seiner Atemz\u00fcge Eishauch wich \u2013", "tokens": ["Mit", "sei\u00b7ner", "A\u00b7tem\u00b7z\u00fc\u00b7ge", "Eis\u00b7hauch", "wich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn in der Monde Schnur die zweite Perle", "tokens": ["Wenn", "in", "der", "Mon\u00b7de", "Schnur", "die", "zwei\u00b7te", "Per\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NE", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich \u00fcbertropfen l\u00e4\u00dft von Goldreflexen \u2013", "tokens": ["Sich", "\u00fc\u00b7bert\u00b7rop\u00b7fen", "l\u00e4\u00dft", "von", "Gold\u00b7re\u00b7fle\u00b7xen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Winternebel Vorhang in zwei St\u00fccke", "tokens": ["Der", "Win\u00b7ter\u00b7ne\u00b7bel", "Vor\u00b7hang", "in", "zwei", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Geborsten ist ... und ihrer Gnaden Truhe", "tokens": ["Ge\u00b7bors\u00b7ten", "ist", "...", "und", "ih\u00b7rer", "Gna\u00b7den", "Tru\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$(", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach tr\u00e4umerischer Rast die Sonne leert \u2013", "tokens": ["Nach", "tr\u00e4u\u00b7me\u00b7ri\u00b7scher", "Rast", "die", "Son\u00b7ne", "leert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den ganzen K\u00f6cher ihrer funkelnden Pfeile:", "tokens": ["Den", "gan\u00b7zen", "K\u00f6\u00b7cher", "ih\u00b7rer", "fun\u00b7keln\u00b7den", "Pfei\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie gerne lass' ich mich von dieser Sonne,", "tokens": ["Wie", "ger\u00b7ne", "lass'", "ich", "mich", "von", "die\u00b7ser", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von dieser Sonne sanft verk\u00fchltem Licht", "tokens": ["Von", "die\u00b7ser", "Son\u00b7ne", "sanft", "ver\u00b7k\u00fchl\u00b7tem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bescheinen! Leise kommt auf leichten Sohlen", "tokens": ["Be\u00b7schei\u00b7nen", "!", "Lei\u00b7se", "kommt", "auf", "leich\u00b7ten", "Soh\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein Sinnen \u00fcber mich ... ein dunkles Suchen \u2013", "tokens": ["Ein", "Sin\u00b7nen", "\u00fc\u00b7ber", "mich", "...", "ein", "dunk\u00b7les", "Su\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und doch, wie so klar und wunschlos still ...", "tokens": ["Und", "doch", ",", "wie", "so", "klar", "und", "wun\u00b7schlos", "still", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "All' Winterunrast hab' ich abgetan \u2013", "tokens": ["All'", "Win\u00b7ter\u00b7un\u00b7rast", "hab'", "ich", "ab\u00b7ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Als schritte ich auf Wolken, treib' ich hin ...", "tokens": ["Als", "schrit\u00b7te", "ich", "auf", "Wol\u00b7ken", ",", "treib'", "ich", "hin", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Augen halb geschlossen ... seltsam m\u00fcde \u2013", "tokens": ["Die", "Au\u00b7gen", "halb", "ge\u00b7schlos\u00b7sen", "...", "selt\u00b7sam", "m\u00fc\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und an den Sonnenstrahl, der mich ber\u00fchrt ...", "tokens": ["Und", "an", "den", "Son\u00b7nen\u00b7strahl", ",", "der", "mich", "be\u00b7r\u00fchrt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Leise, ganz leise meine Wange streift,", "tokens": ["Lei\u00b7se", ",", "ganz", "lei\u00b7se", "mei\u00b7ne", "Wan\u00b7ge", "streift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "M\u00f6cht' ich mich lehnen ... und in seiner Goldspur", "tokens": ["M\u00f6cht'", "ich", "mich", "leh\u00b7nen", "...", "und", "in", "sei\u00b7ner", "Gold\u00b7spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$(", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Verd\u00e4mmern lassen meiner Seele Leben ...", "tokens": ["Ver\u00b7d\u00e4m\u00b7mern", "las\u00b7sen", "mei\u00b7ner", "See\u00b7le", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}