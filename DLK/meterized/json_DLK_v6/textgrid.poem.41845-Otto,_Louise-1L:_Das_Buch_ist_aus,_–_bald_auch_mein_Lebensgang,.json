{"textgrid.poem.41845": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Buch ist aus, \u2013 bald auch mein Lebensgang,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Buch ist aus, \u2013 bald auch mein Lebensgang,", "tokens": ["Das", "Buch", "ist", "aus", ",", "\u2013", "bald", "auch", "mein", "Le\u00b7bens\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "$(", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von dem es k\u00fcndet als ein Echot\u00f6nen,", "tokens": ["Von", "dem", "es", "k\u00fcn\u00b7det", "als", "ein", "E\u00b7cho\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil alles zu begleiten mit Gesang", "tokens": ["Weil", "al\u00b7les", "zu", "be\u00b7glei\u00b7ten", "mit", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKZU", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von je mir war erhebendes Gew\u00f6hnen.", "tokens": ["Von", "je", "mir", "war", "er\u00b7he\u00b7ben\u00b7des", "Ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und so hab ich von meinem ganzen Leben", "tokens": ["Und", "so", "hab", "ich", "von", "mei\u00b7nem", "gan\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein treu bewahrtes Abbild hier gegeben.", "tokens": ["Ein", "treu", "be\u00b7wahr\u00b7tes", "Ab\u00b7bild", "hier", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und allen geb' ich's, allen sei's geweiht,", "tokens": ["Und", "al\u00b7len", "geb'", "ich's", ",", "al\u00b7len", "sei's", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIS", "$,", "PIAT", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die gern empor sich aus dem Staube ringen,", "tokens": ["Die", "gern", "em\u00b7por", "sich", "aus", "dem", "Stau\u00b7be", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKVZ", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und allen gilt's \u2013 aus alt' und neuer Zeit \u2013", "tokens": ["Und", "al\u00b7len", "gilt's", "\u2013", "aus", "alt'", "und", "neu\u00b7er", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$(", "APPR", "ADJA", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Gru\u00df von f\u00fcnf Jahrzehnten darzubringen,", "tokens": ["Den", "Gru\u00df", "von", "f\u00fcnf", "Jahr\u00b7zehn\u00b7ten", "dar\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In denen ich gestrebt, gek\u00e4mpft, gesungen", "tokens": ["In", "de\u00b7nen", "ich", "ge\u00b7strebt", ",", "ge\u00b7k\u00e4mpft", ",", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und unentwegt nach hohem Ziel gerungen.", "tokens": ["Und", "un\u00b7ent\u00b7wegt", "nach", "ho\u00b7hem", "Ziel", "ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sei zuerst Erinnerungsgru\u00df gebracht", "tokens": ["So", "sei", "zu\u00b7erst", "E\u00b7rin\u00b7ne\u00b7rungs\u00b7gru\u00df", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Euch, die mit mir verbunden als Genossen,", "tokens": ["Euch", ",", "die", "mit", "mir", "ver\u00b7bun\u00b7den", "als", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPER", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da allgemach das deutsche Volk erwacht,", "tokens": ["Da", "all\u00b7ge\u00b7mach", "das", "deut\u00b7sche", "Volk", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich seinem Dienst zu weihen froh entschlossen!", "tokens": ["Sich", "sei\u00b7nem", "Dienst", "zu", "wei\u00b7hen", "froh", "ent\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Freiheitsaltar in Begeist'rungsflammen", "tokens": ["Am", "Frei\u00b7heits\u00b7al\u00b7tar", "in", "Be\u00b7geist'\u00b7rungs\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Klang Schwur und Lied in stolzem Klang zusammen.", "tokens": ["Klang", "Schwur", "und", "Lied", "in", "stol\u00b7zem", "Klang", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So gr\u00fc\u00df' ich alle, die \u2013 ob fern, ob nah \u2013", "tokens": ["So", "gr\u00fc\u00df'", "ich", "al\u00b7le", ",", "die", "\u2013", "ob", "fern", ",", "ob", "nah", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PRELS", "$(", "KOUS", "ADJD", "$,", "KOUS", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den h\u00f6chsten Idealen treu geblieben", "tokens": ["Den", "h\u00f6chs\u00b7ten", "I\u00b7dea\u00b7len", "treu", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wandellos, was Schlimmes auch geschah,", "tokens": ["Und", "wan\u00b7del\u00b7los", ",", "was", "Schlim\u00b7mes", "auch", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Volk, das Vaterland, die Menschheit lieben;", "tokens": ["Das", "Volk", ",", "das", "Va\u00b7ter\u00b7land", ",", "die", "Menschheit", "lie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "So gr\u00fc\u00df' ich alle M\u00e4nner, alle Frauen,", "tokens": ["So", "gr\u00fc\u00df'", "ich", "al\u00b7le", "M\u00e4n\u00b7ner", ",", "al\u00b7le", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die an der Zukunft Friedenstempel bauen.", "tokens": ["Die", "an", "der", "Zu\u00b7kunft", "Frie\u00b7dens\u00b7tem\u00b7pel", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So gr\u00fc\u00df' ich alle Frauen nah und fern,", "tokens": ["So", "gr\u00fc\u00df'", "ich", "al\u00b7le", "Frau\u00b7en", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die mutig wandeln auf den neuen Bahnen,", "tokens": ["Die", "mu\u00b7tig", "wan\u00b7deln", "auf", "den", "neu\u00b7en", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die uns gezeigt der neuen Hoffnung Stern,", "tokens": ["Die", "uns", "ge\u00b7zeigt", "der", "neu\u00b7en", "Hoff\u00b7nung", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den wir erschauen mit prophet'schem Ahnen!", "tokens": ["Den", "wir", "er\u00b7schau\u00b7en", "mit", "pro\u00b7phet'\u00b7schem", "Ah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wollt so f\u00fcr mich ein freundliches Gedenken", "tokens": ["Wollt", "so", "f\u00fcr", "mich", "ein", "freund\u00b7li\u00b7ches", "Ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nun diesem Buch und meinem Leben schenken.", "tokens": ["Nun", "die\u00b7sem", "Buch", "und", "mei\u00b7nem", "Le\u00b7ben", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}