{"textgrid.poem.43140": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zuckt durch meine Seel' ein Blitz", "tokens": ["Es", "zuckt", "durch", "mei\u00b7ne", "Seel'", "ein", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gelben unheimlichen Flammen,", "tokens": ["Mit", "gel\u00b7ben", "un\u00b7heim\u00b7li\u00b7chen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er leuchtet wie der Verzweiflung Witz,", "tokens": ["Er", "leuch\u00b7tet", "wie", "der", "Ver\u00b7zwei\u00b7flung", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er zischet wie kaltes Verdammen,", "tokens": ["Er", "zi\u00b7schet", "wie", "kal\u00b7tes", "Ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er zeigt mir in seinem fahlen Licht", "tokens": ["Er", "zeigt", "mir", "in", "sei\u00b7nem", "fah\u00b7len", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nur einen einz'gen Gedanken:", "tokens": ["Nur", "ei\u00b7nen", "einz'\u00b7gen", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich seh' ein wei\u00dfes Todtengesicht", "tokens": ["Ich", "seh'", "ein", "wei\u00b7\u00dfes", "Tod\u00b7ten\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Auf dem Wasser im Sturme schwanken!", "tokens": ["Auf", "dem", "Was\u00b7ser", "im", "Stur\u00b7me", "schwan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und immer taucht es wieder empor,", "tokens": ["Und", "im\u00b7mer", "taucht", "es", "wie\u00b7der", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "So wei\u00df \u2013 so sch\u00f6n \u2013 so erhaben! ...", "tokens": ["So", "wei\u00df", "\u2013", "so", "sch\u00f6n", "\u2013", "so", "er\u00b7ha\u00b7ben", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "O, da\u00df es \u00f6de w\u00e4r', wie zuvor,", "tokens": ["O", ",", "da\u00df", "es", "\u00f6\u00b7de", "w\u00e4r'", ",", "wie", "zu\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PWAV", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "In der Tiefe Alles begraben! \u2013", "tokens": ["In", "der", "Tie\u00b7fe", "Al\u00b7les", "be\u00b7gra\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVPP", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}