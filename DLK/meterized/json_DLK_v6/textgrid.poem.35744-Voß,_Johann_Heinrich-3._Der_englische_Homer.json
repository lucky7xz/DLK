{"textgrid.poem.35744": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Der englische Homer", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit sicherm Z\u00fcgel, still und hehr,", "tokens": ["Mit", "si\u00b7cherm", "Z\u00fc\u00b7gel", ",", "still", "und", "hehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lenkt auf der Himmelsbahn der G\u00f6ttersohn Homer", "tokens": ["Lenkt", "auf", "der", "Him\u00b7mels\u00b7bahn", "der", "G\u00f6t\u00b7ter\u00b7sohn", "Ho\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "NE"], "meter": "++-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die Sonnenross' Apolls, und \u00fcberstrahlet milde", "tokens": ["Die", "Son\u00b7nen\u00b7ross'", "A\u00b7polls", ",", "und", "\u00fc\u00b7bers\u00b7trah\u00b7let", "mil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Lebenskraft und Reiz elysische Gefilde.", "tokens": ["Mit", "Le\u00b7bens\u00b7kraft", "und", "Reiz", "e\u00b7ly\u00b7si\u00b7sche", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da h\u00fcpft, neumodisch angethan,", "tokens": ["Da", "h\u00fcpft", ",", "neu\u00b7mo\u00b7disch", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Pope leicht daher, ersucht den Wundermann,", "tokens": ["Herr", "Po\u00b7pe", "leicht", "da\u00b7her", ",", "er\u00b7sucht", "den", "Wun\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "PAV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihm seine Staatskaro\u00df ein wenig abzutreten;", "tokens": ["Ihm", "sei\u00b7ne", "Staats\u00b7ka\u00b7ro\u00df", "ein", "we\u00b7nig", "ab\u00b7zu\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ART", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und l\u00e4chelnd weicht Homer dem schm\u00e4chtigen Poeten", "tokens": ["Und", "l\u00e4\u00b7chelnd", "weicht", "Ho\u00b7mer", "dem", "schm\u00e4ch\u00b7ti\u00b7gen", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "NE", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Er h\u00e4ngt den Rossen Schellen an,", "tokens": ["Er", "h\u00e4ngt", "den", "Ros\u00b7sen", "Schel\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Setzt breit sich auf den Sonnenwagen,", "tokens": ["Setzt", "breit", "sich", "auf", "den", "Son\u00b7nen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dem reichen Brittenvolk eins vorzujagen,", "tokens": ["Dem", "rei\u00b7chen", "Brit\u00b7ten\u00b7volk", "eins", "vor\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und knallt galant: Mit Ungest\u00fcm", "tokens": ["Und", "knallt", "ga\u00b7lant", ":", "Mit", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Entkollern dem schm\u00e4chtigen Manne die stolzen unsterblichen Rappen,", "tokens": ["Ent\u00b7kol\u00b7lern", "dem", "schm\u00e4ch\u00b7ti\u00b7gen", "Man\u00b7ne", "die", "stol\u00b7zen", "uns\u00b7terb\u00b7li\u00b7chen", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.14": {"text": "Hoch \u00fcber den Sirius hin; und tief, tief unter ihm", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "Si\u00b7rius", "hin", ";", "und", "tief", ",", "tief", "un\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$.", "KON", "ADJD", "$,", "ADJD", "APPR", "PPER"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Herrscht Gr\u00f6nlands Winternacht, Geheul und Z\u00e4hneklappen.", "tokens": ["Herrscht", "Gr\u00f6n\u00b7lands", "Win\u00b7ter\u00b7nacht", ",", "Ge\u00b7heul", "und", "Z\u00e4h\u00b7ne\u00b7klap\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}