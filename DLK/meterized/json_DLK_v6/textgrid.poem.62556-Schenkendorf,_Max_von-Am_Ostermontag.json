{"textgrid.poem.62556": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Ostermontag", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich der Sp\u00f6tter mir will rauben,", "tokens": ["Wenn", "dich", "der", "Sp\u00f6t\u00b7ter", "mir", "will", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn mich der Zweifel Heer best\u00fcrmt,", "tokens": ["Wenn", "mich", "der", "Zwei\u00b7fel", "Heer", "be\u00b7st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann, Heiland, st\u00e4rke mir den Glauben,", "tokens": ["Dann", ",", "Hei\u00b7land", ",", "st\u00e4r\u00b7ke", "mir", "den", "Glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' Angst sich um die Seele th\u00fcrmt.", "tokens": ["Eh'", "Angst", "sich", "um", "die", "See\u00b7le", "th\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprich dann zu mir, wie zu dem J\u00fcnger,", "tokens": ["Sprich", "dann", "zu", "mir", ",", "wie", "zu", "dem", "J\u00fcn\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der auch erlag der Zweifelsqual \u2013", "tokens": ["Der", "auch", "er\u00b7lag", "der", "Zwei\u00b7fels\u00b7qual", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbhier bin ich! lege deine Finger", "tokens": ["\u00bb", "hier", "bin", "ich", "!", "le\u00b7ge", "dei\u00b7ne", "Fin\u00b7ger"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner H\u00e4nde N\u00e4gelmal.\u00ab", "tokens": ["In", "mei\u00b7ner", "H\u00e4n\u00b7de", "N\u00e4\u00b7gel\u00b7mal", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, la\u00df mich dich zu allen Stunden", "tokens": ["Ach", ",", "la\u00df", "mich", "dich", "zu", "al\u00b7len", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor meines Geistes Augen sehn,", "tokens": ["Vor", "mei\u00b7nes", "Geis\u00b7tes", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fl\u00fcchte mich in deine Wunden,", "tokens": ["Und", "fl\u00fcch\u00b7te", "mich", "in", "dei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dieses Lebens St\u00fcrme wehn.", "tokens": ["Wenn", "die\u00b7ses", "Le\u00b7bens", "St\u00fcr\u00b7me", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}