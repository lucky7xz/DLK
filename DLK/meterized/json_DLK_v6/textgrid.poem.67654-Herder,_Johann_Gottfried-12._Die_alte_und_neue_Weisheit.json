{"textgrid.poem.67654": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12. Die alte und neue Weisheit", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleiner Bart,", "tokens": ["Ein", "klei\u00b7ner", "Bart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und kann so fragen!", "tokens": ["Und", "kann", "so", "fra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und wenn ich Dir nun Alles wollte sagen,", "tokens": ["Und", "wenn", "ich", "Dir", "nun", "Al\u00b7les", "woll\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du B\u00e4rtlein zart,", "tokens": ["Du", "B\u00e4rt\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wo Du's denn tragen?", "tokens": ["Wo", "Du's", "denn", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}