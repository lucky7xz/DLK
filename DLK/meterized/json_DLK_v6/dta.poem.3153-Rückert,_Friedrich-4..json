{"dta.poem.3153": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Millionen, die nun auf der Erde wohnen,", "tokens": ["Der", "Mil\u00b7lion\u00b7en", ",", "die", "nun", "auf", "der", "Er\u00b7de", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und aller schon zuvor gegangnen Millionen,", "tokens": ["Und", "al\u00b7ler", "schon", "zu\u00b7vor", "ge\u00b7gang\u00b7nen", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie viele sind es, die nachlie\u00dfen eine Spur?", "tokens": ["Wie", "vie\u00b7le", "sind", "es", ",", "die", "nach\u00b7lie\u00b7\u00dfen", "ei\u00b7ne", "Spur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Tausende sind im Verh\u00e4ltnis wenig nur.", "tokens": ["Viel", "Tau\u00b7sen\u00b7de", "sind", "im", "Ver\u00b7h\u00e4lt\u00b7nis", "we\u00b7nig", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die allermeisten sind verschollen ohne Namen,", "tokens": ["Die", "al\u00b7ler\u00b7meis\u00b7ten", "sind", "ver\u00b7schol\u00b7len", "oh\u00b7ne", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch denen ungekannt, die hart nach ihnen kamen.", "tokens": ["Auch", "de\u00b7nen", "un\u00b7ge\u00b7kannt", ",", "die", "hart", "nach", "ih\u00b7nen", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und jene Wenigen, die nicht sind namenlos,", "tokens": ["Und", "je\u00b7ne", "We\u00b7ni\u00b7gen", ",", "die", "nicht", "sind", "na\u00b7men\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie leben auch im Buch und dem Gelehrten blos.", "tokens": ["Sie", "le\u00b7ben", "auch", "im", "Buch", "und", "dem", "Ge\u00b7lehr\u00b7ten", "blos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Von tausend einer nur lebt in des Volkes Munde;", "tokens": ["Von", "tau\u00b7send", "ei\u00b7ner", "nur", "lebt", "in", "des", "Vol\u00b7kes", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ART", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und diesem auch was hilft die undankbare Kunde?", "tokens": ["Und", "die\u00b7sem", "auch", "was", "hilft", "die", "un\u00b7dank\u00b7ba\u00b7re", "Kun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gewohnheit nennet ihn, kein Wunsch doch kennet ihn;", "tokens": ["Ge\u00b7wohn\u00b7heit", "nen\u00b7net", "ihn", ",", "kein", "Wunsch", "doch", "ken\u00b7net", "ihn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PIAT", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schad' um die Todtenruh, von der man trennet ihn.", "tokens": ["Schad'", "um", "die", "Tod\u00b7ten\u00b7ruh", ",", "von", "der", "man", "tren\u00b7net", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}