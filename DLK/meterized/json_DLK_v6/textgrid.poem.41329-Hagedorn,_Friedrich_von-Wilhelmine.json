{"textgrid.poem.41329": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wilhelmine", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie lebt' und liebt', und nun ist sie dahin,", "tokens": ["Sie", "lebt'", "und", "liebt'", ",", "und", "nun", "ist", "sie", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "ADV", "VAFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Fl\u00fcchtigste der Wilhelminen.", "tokens": ["Die", "Fl\u00fcch\u00b7tigs\u00b7te", "der", "Wil\u00b7hel\u00b7mi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An Witz, an Lust, an freiem Sinn", "tokens": ["An", "Witz", ",", "an", "Lust", ",", "an", "frei\u00b7em", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Glich sie den Ninons, wie den Phrynen:", "tokens": ["Glich", "sie", "den", "Ni\u00b7nons", ",", "wie", "den", "Phry\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NE", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr war genug, als Sch\u00e4ferin,", "tokens": ["Ihr", "war", "ge\u00b7nug", ",", "als", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kenner Neigung zu verdienen,", "tokens": ["Der", "Ken\u00b7ner", "Nei\u00b7gung", "zu", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sie beneidete sonst keine K\u00f6nigin,", "tokens": ["Und", "sie", "be\u00b7nei\u00b7de\u00b7te", "sonst", "kei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als dich, du K\u00f6nigin der Bienen.", "tokens": ["Als", "dich", ",", "du", "K\u00f6\u00b7ni\u00b7gin", "der", "Bie\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}