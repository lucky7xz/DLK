{"textgrid.poem.47884": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mariamne war ein Zweig aus der Makkabeer Stammen/", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mariamne war ein Zweig aus der Makkabeer Stammen/", "tokens": ["Ma\u00b7ri\u00b7am\u00b7ne", "war", "ein", "Zweig", "aus", "der", "Mak\u00b7ka\u00b7beer", "Stam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wie Herodes ihr Gemahl hegte List-Lust-Lasterflammen;", "tokens": ["Wie", "He\u00b7ro\u00b7des", "ihr", "Ge\u00b7mahl", "heg\u00b7te", "List\u00b7Lust\u00b7Las\u00b7ter\u00b7flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So war ihrer Sch\u00f6nheit Gabe mit der Tugend ausgeschm\u00fckkt/", "tokens": ["So", "war", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Ga\u00b7be", "mit", "der", "Tu\u00b7gend", "aus\u00b7ge\u00b7schm\u00fckkt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Die auch in den Todesn\u00f6hten mit Gedult hervorgeblikkt.", "tokens": ["Die", "auch", "in", "den", "To\u00b7des\u00b7n\u00f6h\u00b7ten", "mit", "Ge\u00b7dult", "her\u00b7vor\u00b7geb\u00b7likkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "An der frommen K\u00f6nigin hat die Bo\u00dfheit sich gerochen/", "tokens": ["An", "der", "from\u00b7men", "K\u00f6\u00b7ni\u00b7gin", "hat", "die", "Bo\u00df\u00b7heit", "sich", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "PRF", "VVPP", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "So gar/ da\u00df der L\u00fcgenneid \u00fcber sie den Stab gebrochen.", "tokens": ["So", "gar", "/", "da\u00df", "der", "L\u00fc\u00b7gen\u00b7neid", "\u00fc\u00b7ber", "sie", "den", "Stab", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Sauff du Greuel-meuchelm\u00f6rder/ sauff der Heldin edles Blut/", "tokens": ["Sauff", "du", "Greu\u00b7el\u00b7meu\u00b7chel\u00b7m\u00f6r\u00b7der", "/", "sauff", "der", "Hel\u00b7din", "ed\u00b7les", "Blut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$(", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Die dir/ sonder Weiberthrenen/ weiset mehr als M\u00e4nnermut/", "tokens": ["Die", "dir", "/", "son\u00b7der", "Wei\u00b7berth\u00b7re\u00b7nen", "/", "wei\u00b7set", "mehr", "als", "M\u00e4n\u00b7ner\u00b7mut", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KON", "NN", "$(", "VVFIN", "ADV", "KOUS", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Herodes der Tyrann/ von d\u00fcstrem Angesicht/", "tokens": ["He\u00b7ro\u00b7des", "der", "Ty\u00b7rann", "/", "von", "d\u00fcst\u00b7rem", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Sitten w\u00fctig wild/ Barbarisch in Geberden/", "tokens": ["In", "Sit\u00b7ten", "w\u00fc\u00b7tig", "wild", "/", "Bar\u00b7ba\u00b7risch", "in", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$(", "NN", "APPR", "NN", "$("], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hat seine Grausamkeit gar gegen Gott gericht/", "tokens": ["Hat", "sei\u00b7ne", "Grau\u00b7sam\u00b7keit", "gar", "ge\u00b7gen", "Gott", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihn der Menschen Mord nicht s\u00e4ttigt' auf der Erd\u1ebd,", "tokens": ["Weil", "ihn", "der", "Men\u00b7schen", "Mord", "nicht", "s\u00e4t\u00b7tigt'", "auf", "der", "Erd\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er trug mit Trug die Kron/ der blutgemengte Koht/", "tokens": ["Er", "trug", "mit", "Trug", "die", "Kron", "/", "der", "blut\u00b7ge\u00b7meng\u00b7te", "Koht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der B\u00f6sen bester Freund/ der Frommen Tod und Noht:", "tokens": ["Der", "B\u00f6\u00b7sen", "bes\u00b7ter", "Freund", "/", "der", "From\u00b7men", "Tod", "und", "Noht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bis ihn der Frevelstoltz mit Jammer hat gest\u00fcrtzet/", "tokens": ["Bis", "ihn", "der", "Fre\u00b7vel\u00b7stoltz", "mit", "Jam\u00b7mer", "hat", "ge\u00b7st\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd letzt die Madensucht das Leben abgek\u00fcrtzet.", "tokens": ["Vnd", "letzt", "die", "Ma\u00b7den\u00b7sucht", "das", "Le\u00b7ben", "ab\u00b7ge\u00b7k\u00fcrt\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}