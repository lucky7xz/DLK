{"textgrid.poem.35282": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich gr\u00fc\u00dfe dich, verlass'nes Fischerhaus!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich, verlass'nes Fischerhaus!", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "ver\u00b7lass'\u00b7nes", "Fi\u00b7scher\u00b7haus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft von deiner meerbesp\u00fclten Schwelle", "tokens": ["Wie", "oft", "von", "dei\u00b7ner", "meer\u00b7be\u00b7sp\u00fcl\u00b7ten", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blickt ich verlangend in die Nacht hinaus,", "tokens": ["Blickt", "ich", "ver\u00b7lan\u00b7gend", "in", "die", "Nacht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die tropenwarme, sternenhelle!", "tokens": ["Die", "tro\u00b7pen\u00b7war\u00b7me", ",", "ster\u00b7nen\u00b7hel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun ist es wieder, wie es damals war,", "tokens": ["Nun", "ist", "es", "wie\u00b7der", ",", "wie", "es", "da\u00b7mals", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch funkeln goldne Thr\u00e4nen dort im Sande,", "tokens": ["Noch", "fun\u00b7keln", "gold\u00b7ne", "Thr\u00e4\u00b7nen", "dort", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein ew'ger Sommer waltet noch im Lande,", "tokens": ["Ein", "ew'\u00b7ger", "Som\u00b7mer", "wal\u00b7tet", "noch", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und nur mein Herz ist aller Freude bar.", "tokens": ["Und", "nur", "mein", "Herz", "ist", "al\u00b7ler", "Freu\u00b7de", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch damals \u2013 ob ich wachte oder schlief,", "tokens": ["Doch", "da\u00b7mals", "\u2013", "ob", "ich", "wach\u00b7te", "o\u00b7der", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nie war ich so verwaist, so ganz allein,", "tokens": ["Nie", "war", "ich", "so", "ver\u00b7waist", ",", "so", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Denn ferne Liebe stillte meine Pein,", "tokens": ["Denn", "fer\u00b7ne", "Lie\u00b7be", "still\u00b7te", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und jeden Monat kam ein Brief \u2013 ein Brief.", "tokens": ["Und", "je\u00b7den", "Mo\u00b7nat", "kam", "ein", "Brief", "\u2013", "ein", "Brief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl hundertmal, beim Rauschen der See,", "tokens": ["Wohl", "hun\u00b7dert\u00b7mal", ",", "beim", "Rau\u00b7schen", "der", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Las ich und las, um jedes Wort zu deuten;", "tokens": ["Las", "ich", "und", "las", ",", "um", "je\u00b7des", "Wort", "zu", "deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "VVFIN", "$,", "KOUI", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich teilte Freude und Weh", "tokens": ["Ich", "teil\u00b7te", "Freu\u00b7de", "und", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mit den armen Fischersleuten;", "tokens": ["Mit", "den", "ar\u00b7men", "Fi\u00b7schers\u00b7leu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich ging umher auf dem sandigen Plan,", "tokens": ["Ich", "ging", "um\u00b7her", "auf", "dem", "san\u00b7di\u00b7gen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bis der Gestirne Glanz erblich,", "tokens": ["Bis", "der", "Ge\u00b7stir\u00b7ne", "Glanz", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und der gewaltige Ocean", "tokens": ["Und", "der", "ge\u00b7wal\u00b7ti\u00b7ge", "O\u00b7cean"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weinte um sie und um mich.", "tokens": ["Wein\u00b7te", "um", "sie", "und", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nun, ihr leuchtenden D\u00fcnen,", "tokens": ["Doch", "nun", ",", "ihr", "leuch\u00b7ten\u00b7den", "D\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was soll der Wellen Gesang?", "tokens": ["Was", "soll", "der", "Wel\u00b7len", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nordische Brandung verschlang", "tokens": ["Nor\u00b7di\u00b7sche", "Bran\u00b7dung", "ver\u00b7schlang"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Den Myrtenzweig mir, den gr\u00fcnen.", "tokens": ["Den", "Myr\u00b7ten\u00b7zweig", "mir", ",", "den", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie lebt \u2013 nur ihr Herz ist umnachtet,", "tokens": ["Sie", "lebt", "\u2013", "nur", "ihr", "Herz", "ist", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ich lebe \u2013 arm und verachtet;", "tokens": ["Ich", "le\u00b7be", "\u2013", "arm", "und", "ver\u00b7ach\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ewig dahin ist die Jugendlust.", "tokens": ["E\u00b7wig", "da\u00b7hin", "ist", "die", "Ju\u00b7gend\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wo find' ich Trost? Ich kenne keinen, keinen", "tokens": ["Wo", "find'", "ich", "Trost", "?", "Ich", "ken\u00b7ne", "kei\u00b7nen", ",", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "PIAT", "$,", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Hier oder dort \u2013 auch nicht an Freundesbrust", "tokens": ["Hier", "o\u00b7der", "dort", "\u2013", "auch", "nicht", "an", "Freun\u00b7des\u00b7brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "$(", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "M\u00f6cht' ich \u00fcber mein Elend weinen;", "tokens": ["M\u00f6cht'", "ich", "\u00fc\u00b7ber", "mein", "E\u00b7lend", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Kenne mich selbst nicht mehr,", "tokens": ["Ken\u00b7ne", "mich", "selbst", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Vergessen bin ich, veraltet,", "tokens": ["Ver\u00b7ges\u00b7sen", "bin", "ich", ",", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und Lava, halb erkaltet,", "tokens": ["Und", "La\u00b7va", ",", "halb", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Roll' ich im Busen hin und her,", "tokens": ["Roll'", "ich", "im", "Bu\u00b7sen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nach einsam vertr\u00e4umter Jugendzeit,", "tokens": ["Nach", "ein\u00b7sam", "ver\u00b7tr\u00e4um\u00b7ter", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wie ein Vulkan, der nicht mehr Feuer speit.", "tokens": ["Wie", "ein", "Vul\u00b7kan", ",", "der", "nicht", "mehr", "Feu\u00b7er", "speit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "$,", "PRELS", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O meine s\u00fc\u00dfe Dame!", "tokens": ["O", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Da\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was bist du mir? Ein stets geliebter Name.", "tokens": ["Was", "bist", "du", "mir", "?", "Ein", "stets", "ge\u00b7lieb\u00b7ter", "Na\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "$.", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-++--+---", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was bin ich dir? Ein Vorwurf. Doch gesetzt,", "tokens": ["Was", "bin", "ich", "dir", "?", "Ein", "Vor\u00b7wurf", ".", "Doch", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "$.", "ART", "NN", "$.", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir w\u00fcrden noch Papier und Feder brauchen,", "tokens": ["Wir", "w\u00fcr\u00b7den", "noch", "Pa\u00b7pier", "und", "Fe\u00b7der", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um schmerzliche Gef\u00fchle auszuhauchen,", "tokens": ["Um", "schmerz\u00b7li\u00b7che", "Ge\u00b7f\u00fch\u00b7le", "aus\u00b7zu\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie anders \u2013 anders schrieben wir uns jetzt!", "tokens": ["Wie", "an\u00b7ders", "\u2013", "an\u00b7ders", "schrie\u00b7ben", "wir", "uns", "jetzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$(", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}