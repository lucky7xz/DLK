{"textgrid.poem.65707": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So hast Du auch den Lauff vollbracht,", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hast Du auch den Lauff vollbracht,", "tokens": ["So", "hast", "Du", "auch", "den", "Lauff", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ber\u00fchmtes Haubt! so sinckst Du nider.", "tokens": ["Be\u00b7r\u00fchm\u00b7tes", "Haubt", "!", "so", "sinckst", "Du", "ni\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schlie\u00dfst die m\u00fcden Augenlider,", "tokens": ["Und", "schlie\u00df\u00b7st", "die", "m\u00fc\u00b7den", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die lang f\u00fcr deine Stadt gewacht!", "tokens": ["Die", "lang", "f\u00fcr", "dei\u00b7ne", "Stadt", "ge\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich hat ein sp\u00e4tes End entrissen:", "tokens": ["Dich", "hat", "ein", "sp\u00e4\u00b7tes", "End", "ent\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wer Dir an Verdiensten gleicht,", "tokens": ["Doch", "wer", "Dir", "an", "Ver\u00b7diens\u00b7ten", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00e4tt er Tausend Jahr erreicht,", "tokens": ["Und", "h\u00e4tt", "er", "Tau\u00b7send", "Jahr", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den mu\u00df man stets zu zeitlich missen.", "tokens": ["Den", "mu\u00df", "man", "stets", "zu", "zeit\u00b7lich", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie, da\u00df ihr jede Last der Welt,", "tokens": ["Wie", ",", "da\u00df", "ihr", "je\u00b7de", "Last", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Dichter, zu verg\u00f6ttern suchet,", "tokens": ["Ihr", "Dich\u00b7ter", ",", "zu", "ver\u00b7g\u00f6t\u00b7tern", "su\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Dohrheitsvoll dem Tode fluchet,", "tokens": ["Und", "Dohr\u00b7heits\u00b7voll", "dem", "To\u00b7de", "flu\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So oft er einen Menschen f\u00e4llt!", "tokens": ["So", "oft", "er", "ei\u00b7nen", "Men\u00b7schen", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun, nun ermuntert Kunst und Dichten!", "tokens": ["Nun", ",", "nun", "er\u00b7mun\u00b7tert", "Kunst", "und", "Dich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun heischts ein w\u00fcrdig Trauerfest;", "tokens": ["Nun", "heischts", "ein", "w\u00fcr\u00b7dig", "Trau\u00b7er\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was schn\u00f6des He\u00fccheln oft erpre\u00dft,", "tokens": ["Was", "schn\u00f6\u00b7des", "He\u00fc\u00b7cheln", "oft", "er\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das fordern jetzund Tre\u00fc und Pflichten.", "tokens": ["Das", "for\u00b7dern", "je\u00b7tzund", "Tre\u00fc", "und", "Pflich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch was soll e\u00fcer schwacher Tohn", "tokens": ["Doch", "was", "soll", "e\u00b7\u00fc\u00b7er", "schwa\u00b7cher", "Tohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Um unsers Haubtes Gruft erschallen?", "tokens": ["Um", "un\u00b7sers", "Haub\u00b7tes", "Gruft", "er\u00b7schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer weinet nicht, da\u00df er gefallen?", "tokens": ["Wer", "wei\u00b7net", "nicht", ",", "da\u00df", "er", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein grosses Volk betr\u00e4hnt Ihn schon.", "tokens": ["Ein", "gros\u00b7ses", "Volk", "be\u00b7tr\u00e4hnt", "Ihn", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Helvetiens vereintes Klagen", "tokens": ["Hel\u00b7ve\u00b7ti\u00b7ens", "ver\u00b7ein\u00b7tes", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird seinen wolverdienten Preis", "tokens": ["Wird", "sei\u00b7nen", "wol\u00b7ver\u00b7dien\u00b7ten", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mehr, als der gr\u00f6sten Dichter Flei\u00df,", "tokens": ["Mehr", ",", "als", "der", "gr\u00f6s\u00b7ten", "Dich\u00b7ter", "Flei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis auf die sp\u00e4te Nachwelt tragen.", "tokens": ["Bis", "auf", "die", "sp\u00e4\u00b7te", "Nach\u00b7welt", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Umsonst! da\u00df man in Schrift und Buch", "tokens": ["Um\u00b7sonst", "!", "da\u00df", "man", "in", "Schrift", "und", "Buch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nach der Ewigkeit bestrebet.", "tokens": ["Sich", "nach", "der", "E\u00b7wig\u00b7keit", "be\u00b7stre\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ein verha\u00dfter Name lebet,", "tokens": ["Wenn", "ein", "ver\u00b7ha\u00df\u00b7ter", "Na\u00b7me", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lebt er doch in stetem Fluch.", "tokens": ["So", "lebt", "er", "doch", "in", "ste\u00b7tem", "Fluch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergebens! da\u00df in manchem Lande", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "da\u00df", "in", "man\u00b7chem", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man dem Tyrannen Tempel setzt.", "tokens": ["Man", "dem", "Ty\u00b7ran\u00b7nen", "Tem\u00b7pel", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was werden solche denn zuletzt?", "tokens": ["Was", "wer\u00b7den", "sol\u00b7che", "denn", "zu\u00b7letzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein dauernd Merkmahl seiner Schande.", "tokens": ["Ein", "dau\u00b7ernd", "Merk\u00b7mahl", "sei\u00b7ner", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr, denen wahrer Ruhm gef\u00e4llt,", "tokens": ["Ihr", ",", "de\u00b7nen", "wah\u00b7rer", "Ruhm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O folget unsers Haubtes Tugend,", "tokens": ["O", "fol\u00b7get", "un\u00b7sers", "Haub\u00b7tes", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und widmet e\u00fcch von erster Jugend", "tokens": ["Und", "wid\u00b7met", "e\u00b7\u00fcch", "von", "ers\u00b7ter", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Wol des Standes und der Welt!", "tokens": ["Dem", "Wol", "des", "Stan\u00b7des", "und", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da m\u00fc\u00dft ihr seyn, was Er gewesen;", "tokens": ["Da", "m\u00fc\u00dft", "ihr", "seyn", ",", "was", "Er", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "$,", "PRELS", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wittwen Arm, der Waisen Schutz!", "tokens": ["Der", "Witt\u00b7wen", "Arm", ",", "der", "Wai\u00b7sen", "Schutz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sey kein Stolz, noch Eigennutz", "tokens": ["Da", "sey", "kein", "Stolz", ",", "noch", "Ei\u00b7gen\u00b7nutz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus e\u00fcrer Tahten Zweck zu lesen!", "tokens": ["Aus", "e\u00b7\u00fc\u00b7rer", "Tah\u00b7ten", "Zweck", "zu", "le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Dann wird aus ganzer V\u00f6lker Mund", "tokens": ["Dann", "wird", "aus", "gan\u00b7zer", "V\u00f6l\u00b7ker", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "E\u00fcch ein gerechtes Lob ert\u00f6hnen.", "tokens": ["E\u00b7\u00fcch", "ein", "ge\u00b7rech\u00b7tes", "Lob", "er\u00b7t\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Muter macht es ihren S\u00f6hnen,", "tokens": ["Die", "Mu\u00b7ter", "macht", "es", "ih\u00b7ren", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein Geschlecht dem andern kund.", "tokens": ["Und", "ein", "Ge\u00b7schlecht", "dem", "an\u00b7dern", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Tausend Schriften nicht gew\u00e4hren,", "tokens": ["Was", "Tau\u00b7send", "Schrif\u00b7ten", "nicht", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist der Tugend Eigentum.", "tokens": ["Das", "ist", "der", "Tu\u00b7gend", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie baut sich selbsten ihren Ruhm,", "tokens": ["Sie", "baut", "sich", "selbs\u00b7ten", "ih\u00b7ren", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und zwingt die Welt, sie zu verehren.", "tokens": ["Und", "zwingt", "die", "Welt", ",", "sie", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So wurd auch jener Wettstein gro\u00df,", "tokens": ["So", "wurd", "auch", "je\u00b7ner", "Wett\u00b7stein", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schutz der Freyheit vieler Staten:", "tokens": ["Der", "Schutz", "der", "Frey\u00b7heit", "vie\u00b7ler", "Sta\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem ein Trieb nach gleichen Tahten", "tokens": ["Von", "dem", "ein", "Trieb", "nach", "glei\u00b7chen", "Tah\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seines Enckels Adern flo\u00df.", "tokens": ["In", "sei\u00b7nes", "En\u00b7ckels", "A\u00b7dern", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist zuletzt nach Sturm und Kriegen", "tokens": ["So", "ist", "zu\u00b7letzt", "nach", "Sturm", "und", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die weltbelobte Heldenschaar,", "tokens": ["Die", "welt\u00b7be\u00b7lob\u00b7te", "Hel\u00b7den\u00b7schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die einst Helvetien gebahr,", "tokens": ["Die", "einst", "Hel\u00b7ve\u00b7ti\u00b7en", "ge\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Ehrentempel aufgestiegen.", "tokens": ["Zum", "Eh\u00b7ren\u00b7tem\u00b7pel", "auf\u00b7ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wolan! Du hast das sch\u00f6ne Ziel,", "tokens": ["Wo\u00b7lan", "!", "Du", "hast", "das", "sch\u00f6\u00b7ne", "Ziel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hochseliger! nun auch erstrebet.", "tokens": ["Hoch\u00b7se\u00b7li\u00b7ger", "!", "nun", "auch", "er\u00b7stre\u00b7bet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "O da\u00df der Fall, der Dich erhebet,", "tokens": ["O", "da\u00df", "der", "Fall", ",", "der", "Dich", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns nur nicht unertr\u00e4glich fiel!", "tokens": ["Uns", "nur", "nicht", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-