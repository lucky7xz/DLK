{"textgrid.poem.57265": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Anblick der Glanznacht, Sternheere,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Anblick der Glanznacht, Sternheere,", "tokens": ["O", "An\u00b7blick", "der", "Glanz\u00b7nacht", ",", "Stern\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie erhebt ihr! Wie entz\u00fcckst du, Anschauung", "tokens": ["Wie", "er\u00b7hebt", "ihr", "!", "Wie", "ent\u00b7z\u00fcckst", "du", ",", "An\u00b7schau\u00b7ung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "$,", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der herlichen Welt! Gott Sch\u00f6pfer!", "tokens": ["Der", "her\u00b7li\u00b7chen", "Welt", "!", "Gott", "Sch\u00f6p\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie erhaben bist du, Gott Sch\u00f6pfer!", "tokens": ["Wie", "er\u00b7ha\u00b7ben", "bist", "du", ",", "Gott", "Sch\u00f6p\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie freut sich des Emporschauns zum Sternheer wer empfindet", "tokens": ["Wie", "freut", "sich", "des", "Em\u00b7por\u00b7schauns", "zum", "Stern\u00b7heer", "wer", "emp\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "PWS", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie gering er, und wer Gott, welch ein Staub er, und wer Gott", "tokens": ["Wie", "ge\u00b7ring", "er", ",", "und", "wer", "Gott", ",", "welch", "ein", "Staub", "er", ",", "und", "wer", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "$,", "KON", "PWS", "NN", "$,", "PWAT", "ART", "NN", "PPER", "$,", "KON", "PWS", "NN"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sein Gott ist! O sey dann, Gef\u00fchl", "tokens": ["Sein", "Gott", "ist", "!", "O", "sey", "dann", ",", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "NE", "VAFIN", "ADV", "$,", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der Entz\u00fcckung, wenn auch ich sterbe, mit mir!", "tokens": ["Der", "Ent\u00b7z\u00fc\u00b7ckung", ",", "wenn", "auch", "ich", "ster\u00b7be", ",", "mit", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,", "APPR", "PPER", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Was erschreckst du denn so, Tod, des Beladnen Schlaf?", "tokens": ["Was", "er\u00b7schreckst", "du", "denn", "so", ",", "Tod", ",", "des", "Be\u00b7lad\u00b7nen", "Schlaf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O bew\u00f6lke den Genuss himlischer Freude nicht mehr!", "tokens": ["O", "be\u00b7w\u00f6l\u00b7ke", "den", "Ge\u00b7nuss", "him\u00b7li\u00b7scher", "Freu\u00b7de", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-++--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ich sink' in den Staub, Gottes Saat! was schreckst", "tokens": ["Ich", "sink'", "in", "den", "Staub", ",", "Got\u00b7tes", "Saat", "!", "was", "schreckst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "NN", "$.", "PWS", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Unsterblichen du, t\u00e4uschender Tod?", "tokens": ["Den", "U\u00b7nsterb\u00b7li\u00b7chen", "du", ",", "t\u00e4u\u00b7schen\u00b7der", "Tod", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+----+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mit hinab, o mein Leib, denn zur Verwesung!", "tokens": ["Mit", "hin\u00b7ab", ",", "o", "mein", "Leib", ",", "denn", "zur", "Ver\u00b7we\u00b7sung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "$,", "FM", "PPOSAT", "NN", "$,", "KON", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In ihr Thal sanken hinab die Gefallnen", "tokens": ["In", "ihr", "Thal", "san\u00b7ken", "hin\u00b7ab", "die", "Ge\u00b7fall\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vom Beginn her! mit hinab, o mein Staub,", "tokens": ["Vom", "Be\u00b7ginn", "her", "!", "mit", "hin\u00b7ab", ",", "o", "mein", "Staub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "APPR", "PTKVZ", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zur Heerschaar, die entschlief!", "tokens": ["Zur", "Heer\u00b7schaar", ",", "die", "ent\u00b7schlief", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}