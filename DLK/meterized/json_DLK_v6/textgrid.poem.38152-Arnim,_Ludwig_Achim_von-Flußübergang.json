{"textgrid.poem.38152": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Flu\u00df\u00fcbergang", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatten sich siebenzig Schneider verschworen,", "tokens": ["Es", "hat\u00b7ten", "sich", "sie\u00b7ben\u00b7zig", "Schnei\u00b7der", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "NE", "NE", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie wollten zusammen ins Niederland fahren,", "tokens": ["Sie", "woll\u00b7ten", "zu\u00b7sam\u00b7men", "ins", "Nie\u00b7der\u00b7land", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "PIS", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da n\u00e4hten sie einen papierenen Wagen,", "tokens": ["Da", "n\u00e4h\u00b7ten", "sie", "ei\u00b7nen", "pa\u00b7pie\u00b7re\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der siebenzig tapfere Schneider konnt tragen,", "tokens": ["Der", "sie\u00b7ben\u00b7zig", "tap\u00b7fe\u00b7re", "Schnei\u00b7der", "konnt", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Zottelgei\u00df spannten sie dran,", "tokens": ["Die", "Zot\u00b7tel\u00b7gei\u00df", "spann\u00b7ten", "sie", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Hott Hott, Meck Meck, ihr lustigen Br\u00fcder,", "tokens": ["Hott", "Hott", ",", "Meck", "Meck", ",", "ihr", "lus\u00b7ti\u00b7gen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nun setzt euer Leben daran.", "tokens": ["Nun", "setzt", "eu\u00b7er", "Le\u00b7ben", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie fuhren, da trat wohl an einem Stege", "tokens": ["Sie", "fuh\u00b7ren", ",", "da", "trat", "wohl", "an", "ei\u00b7nem", "Ste\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schneidern der Gei\u00df ihr B\u00f6cklein entgegen,", "tokens": ["Den", "Schnei\u00b7dern", "der", "Gei\u00df", "ihr", "B\u00f6c\u00b7klein", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schaute die Meister gar trotziglich an,", "tokens": ["Und", "schau\u00b7te", "die", "Meis\u00b7ter", "gar", "trot\u00b7zig\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Darunter war aber ein herzhafter Mann,", "tokens": ["Da\u00b7run\u00b7ter", "war", "a\u00b7ber", "ein", "herz\u00b7haf\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der zog wohl den kupfernen Fingerhut an,", "tokens": ["Der", "zog", "wohl", "den", "kup\u00b7fer\u00b7nen", "Fin\u00b7ger\u00b7hut", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und zog eine rostige Nadel heraus,", "tokens": ["Und", "zog", "ei\u00b7ne", "ros\u00b7ti\u00b7ge", "Na\u00b7del", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und stach das Gei\u00dfb\u00f6cklein da\u00df es sprang.", "tokens": ["Und", "stach", "das", "Gei\u00df\u00b7b\u00f6c\u00b7klein", "da\u00df", "es", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Da sch\u00fcttelt das B\u00f6cklein gewaltig die H\u00f6rner,", "tokens": ["Da", "sch\u00fct\u00b7telt", "das", "B\u00f6c\u00b7klein", "ge\u00b7wal\u00b7tig", "die", "H\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und jagte die Meister durch Distel und D\u00f6rner.", "tokens": ["Und", "jag\u00b7te", "die", "Meis\u00b7ter", "durch", "Dis\u00b7tel", "und", "D\u00f6r\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zerri\u00df auch dem Held den Manchesternen Kragen,", "tokens": ["Zer\u00b7ri\u00df", "auch", "dem", "Held", "den", "Man\u00b7ches\u00b7ter\u00b7nen", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erbeutet viel Ellen und Scheren im Wagen,", "tokens": ["Er\u00b7beu\u00b7tet", "viel", "El\u00b7len", "und", "Sche\u00b7ren", "im", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und weil acht und sechzig gesprungen in Bach,", "tokens": ["Und", "weil", "acht", "und", "sech\u00b7zig", "ge\u00b7sprun\u00b7gen", "in", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "CARD", "KON", "CARD", "VVPP", "APPR", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So hat nur ein einz'ger sein Leben verloren,", "tokens": ["So", "hat", "nur", "ein", "einz'\u00b7ger", "sein", "Le\u00b7ben", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Weil er nicht konnt springen, er war zu schwach.", "tokens": ["Weil", "er", "nicht", "konnt", "sprin\u00b7gen", ",", "er", "war", "zu", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}}}}