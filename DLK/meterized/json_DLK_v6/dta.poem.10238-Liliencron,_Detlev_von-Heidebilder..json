{"dta.poem.10238": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Heidebilder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mittagsonne br\u00fctet auf der Heide,", "tokens": ["Die", "Mit\u00b7ta\u00b7gson\u00b7ne", "br\u00fc\u00b7tet", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im S\u00fcden droht ein schwarzer Ring.", "tokens": ["Im", "S\u00fc\u00b7den", "droht", "ein", "schwar\u00b7zer", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdurstet h\u00e4ngt das magere Getreide,", "tokens": ["Ver\u00b7durs\u00b7tet", "h\u00e4ngt", "das", "ma\u00b7ge\u00b7re", "Ge\u00b7trei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Behaglich treibt der Schmetterling.", "tokens": ["Be\u00b7hag\u00b7lich", "treibt", "der", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ermattet ruhn der Hirt und seine Schafe,", "tokens": ["Er\u00b7mat\u00b7tet", "ruhn", "der", "Hirt", "und", "sei\u00b7ne", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ente tr\u00e4umt im Binsenkraut,", "tokens": ["Die", "En\u00b7te", "tr\u00e4umt", "im", "Bin\u00b7sen\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ringelnatter sonnt in tr\u00e4gem Schlafe", "tokens": ["Die", "Rin\u00b7gel\u00b7nat\u00b7ter", "sonnt", "in", "tr\u00e4\u00b7gem", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unregbar ihre Tigerhaut.", "tokens": ["Un\u00b7reg\u00b7bar", "ih\u00b7re", "Ti\u00b7ger\u00b7haut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Zickzack zuckt ein Blitz, und Wasserfluten", "tokens": ["Im", "Zick\u00b7zack", "zuckt", "ein", "Blitz", ",", "und", "Was\u00b7ser\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entst\u00fcrzen gierig feuchtem Zelt.", "tokens": ["Ent\u00b7st\u00fcr\u00b7zen", "gie\u00b7rig", "feuch\u00b7tem", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es jauchzt der Sturm und peitscht mit seinen Ruten", "tokens": ["Es", "jauchzt", "der", "Sturm", "und", "peitscht", "mit", "sei\u00b7nen", "Ru\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erl\u00f6send meine Heidewelt.", "tokens": ["Er\u00b7l\u00f6\u00b7send", "mei\u00b7ne", "Hei\u00b7de\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}