{"textgrid.poem.62960": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Sultan gab mit flammendem Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sultan gab mit flammendem Gesicht", "tokens": ["Ein", "Sul\u00b7tan", "gab", "mit", "flam\u00b7men\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einst seinem Gro\u00dfvezier die schmeichelhafte Lehre,", "tokens": ["Einst", "sei\u00b7nem", "Gro\u00df\u00b7ve\u00b7zier", "die", "schmei\u00b7chel\u00b7haf\u00b7te", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er sey ein Dummkopf. Herr, das bin ich nicht,", "tokens": ["Er", "sey", "ein", "Dumm\u00b7kopf", ".", "Herr", ",", "das", "bin", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "NN", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rief dieser aufgebracht, ich bin ein Mann von Ehre,", "tokens": ["Rief", "die\u00b7ser", "auf\u00b7ge\u00b7bracht", ",", "ich", "bin", "ein", "Mann", "von", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und h\u00f6chstens nur ein B\u00f6sewicht.", "tokens": ["Und", "h\u00f6chs\u00b7tens", "nur", "ein", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}