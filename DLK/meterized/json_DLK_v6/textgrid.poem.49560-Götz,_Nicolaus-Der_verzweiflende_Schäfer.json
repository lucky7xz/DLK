{"textgrid.poem.49560": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der verzweiflende Sch\u00e4fer", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du reiner Bach, der seine Quelle flieht", "tokens": ["Du", "rei\u00b7ner", "Bach", ",", "der", "sei\u00b7ne", "Quel\u00b7le", "flieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcber schattenreiche Gr\u00fcnde", "tokens": ["Und", "\u00fc\u00b7ber", "schat\u00b7ten\u00b7rei\u00b7che", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den krummen Lauf durch Klee und Blumen zieht;", "tokens": ["Den", "krum\u00b7men", "Lauf", "durch", "Klee", "und", "Blu\u00b7men", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Ruhe such' ich hier, die ich nicht bey dir finde.", "tokens": ["Die", "Ru\u00b7he", "such'", "ich", "hier", ",", "die", "ich", "nicht", "bey", "dir", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein sanft Gemurmel n\u00e4hrt mein Leid;", "tokens": ["Dein", "sanft", "Ge\u00b7mur\u00b7mel", "n\u00e4hrt", "mein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein dunkler Rand ist mir ein Bild der Traurigkeit,", "tokens": ["Dein", "dunk\u00b7ler", "Rand", "ist", "mir", "ein", "Bild", "der", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo ich mich ungest\u00f6rt betr\u00fcbe.", "tokens": ["Wo", "ich", "mich", "un\u00b7ge\u00b7st\u00f6rt", "be\u00b7tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da flieh' ich vor der Welt, vor mir flieht was ich liebe,", "tokens": ["Da", "flieh'", "ich", "vor", "der", "Welt", ",", "vor", "mir", "flieht", "was", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und ich, ich trage noch des Lebens schwere Last?", "tokens": ["Und", "ich", ",", "ich", "tra\u00b7ge", "noch", "des", "Le\u00b7bens", "schwe\u00b7re", "Last", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O reiner Bach, umzirkt mit Rosenb\u00fcschen,", "tokens": ["O", "rei\u00b7ner", "Bach", ",", "um\u00b7zirkt", "mit", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der meine Thr\u00e4nen aufgefast,", "tokens": ["Der", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "auf\u00b7ge\u00b7fast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich will, o Bach, mein Blut zu deinen Wellen mischen;", "tokens": ["Ich", "will", ",", "o", "Bach", ",", "mein", "Blut", "zu", "dei\u00b7nen", "Wel\u00b7len", "mi\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du solst mich sterben sehn, der du mich leben sahst.", "tokens": ["Du", "solst", "mich", "ster\u00b7ben", "sehn", ",", "der", "du", "mich", "le\u00b7ben", "sahst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$,", "PRELS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}