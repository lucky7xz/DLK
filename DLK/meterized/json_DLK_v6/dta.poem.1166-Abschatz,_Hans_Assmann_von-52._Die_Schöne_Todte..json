{"dta.poem.1166": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "52. Die Sch\u00f6ne Todte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast numehr/ mein Kind/ den lezten Hafen funden/ ", "tokens": ["Du", "hast", "nu\u00b7mehr", "/", "mein", "Kind", "/", "den", "lez\u00b7ten", "Ha\u00b7fen", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Lieb und Hoffen dich nicht ferner h\u00e4lt gebunden:", "tokens": ["Wo", "Lieb", "und", "Hof\u00b7fen", "dich", "nicht", "fer\u00b7ner", "h\u00e4lt", "ge\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bist mir doch so sch\u00f6n in deinem Trauer-Kleide/", "tokens": ["Und", "bist", "mir", "doch", "so", "sch\u00f6n", "in", "dei\u00b7nem", "Trau\u00b7e\u00b7rKlei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich den Schatten noch des Todes selbst beneide.", "tokens": ["Da\u00df", "ich", "den", "Schat\u00b7ten", "noch", "des", "To\u00b7des", "selbst", "be\u00b7nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich werde nun gewahr/ indem ich von dir scheide/", "tokens": ["Ich", "wer\u00b7de", "nun", "ge\u00b7wahr", "/", "in\u00b7dem", "ich", "von", "dir", "schei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man sich auch mit Lust an d\u00fcrren Blumen weyde/", "tokens": ["Da\u00df", "man", "sich", "auch", "mit", "Lust", "an", "d\u00fcr\u00b7ren", "Blu\u00b7men", "wey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df Fr\u00fcchte besser seyn gepfl\u00fcckt/ als da sie stunden/", "tokens": ["Da\u00df", "Fr\u00fcch\u00b7te", "bes\u00b7ser", "seyn", "ge\u00b7pfl\u00fcckt", "/", "als", "da", "sie", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAINF", "VVPP", "$(", "KOKOM", "KOUS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df s\u00fcsser sey die Lust/ ie eher sie verschwunden.", "tokens": ["Da\u00df", "s\u00fcs\u00b7ser", "sey", "die", "Lust", "/", "ie", "e\u00b7her", "sie", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$(", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor blizt\u2019 ein jeder Blick/ es brennt\u2019 ein iedes Haar/", "tokens": ["Vor", "blizt'", "ein", "je\u00b7der", "Blick", "/", "es", "brennt'", "ein", "ie\u00b7des", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "PIAT", "NN", "$(", "PPER", "VVFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Wort bezauberte/ dein Weigern brach die Sinnen/", "tokens": ["Dein", "Wort", "be\u00b7zau\u00b7ber\u00b7te", "/", "dein", "Wei\u00b7gern", "brach", "die", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein gantzes Leben war uns Unruh und Gefahr:", "tokens": ["Dein", "gant\u00b7zes", "Le\u00b7ben", "war", "uns", "Un\u00b7ruh", "und", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Izt wird man keines Streits und keiner Furcht mehr innen.", "tokens": ["Izt", "wird", "man", "kei\u00b7nes", "Streits", "und", "kei\u00b7ner", "Furcht", "mehr", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIAT", "NN", "KON", "PIAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Sch\u00f6nheit/ die du hast im Tode/ zu erwerben/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "die", "du", "hast", "im", "To\u00b7de", "/", "zu", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VAFIN", "APPRART", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "W\u00fcrd\u2019 auch der selbste Tod/ im Fall er k\u00f6nte/ sterben.", "tokens": ["W\u00fcrd'", "auch", "der", "selbs\u00b7te", "Tod", "/", "im", "Fall", "er", "k\u00f6n\u00b7te", "/", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$(", "APPRART", "NN", "PPER", "VMFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}