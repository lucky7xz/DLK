{"textgrid.poem.59509": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sprach zum Morgenrot: Was gl\u00e4nzest du", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sprach zum Morgenrot: Was gl\u00e4nzest du", "tokens": ["Ich", "sprach", "zum", "Mor\u00b7gen\u00b7rot", ":", "Was", "gl\u00e4n\u00b7zest", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit deinem Rosenlicht?", "tokens": ["Mit", "dei\u00b7nem", "Ro\u00b7sen\u00b7licht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sprach zur Jungfrau sch\u00f6n: Was kr\u00e4nzest du", "tokens": ["Ich", "sprach", "zur", "Jung\u00b7frau", "sch\u00f6n", ":", "Was", "kr\u00e4n\u00b7zest", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein junges Angesicht?", "tokens": ["Dein", "jun\u00b7ges", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Morgenrot, du einst erbleichen mu\u00dft,", "tokens": ["Mor\u00b7gen\u00b7rot", ",", "du", "einst", "er\u00b7blei\u00b7chen", "mu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Jungfrau sch\u00f6n, du einst auch sterben mu\u00dft;", "tokens": ["Jung\u00b7frau", "sch\u00f6n", ",", "du", "einst", "auch", "ster\u00b7ben", "mu\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Drum schm\u00fccket euch nicht.", "tokens": ["Drum", "schm\u00fc\u00b7cket", "euch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich schm\u00fccke mich, so sprach das Morgenrot,", "tokens": ["Ich", "schm\u00fc\u00b7cke", "mich", ",", "so", "sprach", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit hellem Rosenlicht;", "tokens": ["Mit", "hel\u00b7lem", "Ro\u00b7sen\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob mir dereinst ein andres Schicksal droht,", "tokens": ["Ob", "mir", "de\u00b7reinst", "ein", "and\u00b7res", "Schick\u00b7sal", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das wei\u00df und frag' ich nicht.", "tokens": ["Das", "wei\u00df", "und", "frag'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der dem Mond, den Sternen gab den Schein,", "tokens": ["Der", "dem", "Mond", ",", "den", "Ster\u00b7nen", "gab", "den", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Auch gef\u00e4rbt hat rot die Wangen mein,", "tokens": ["Auch", "ge\u00b7f\u00e4rbt", "hat", "rot", "die", "Wan\u00b7gen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ADJD", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Drum traure ich nicht.", "tokens": ["Drum", "trau\u00b7re", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ich kr\u00e4nze mich, so sprach die Jungfrau sch\u00f6n,", "tokens": ["Ich", "kr\u00e4n\u00b7ze", "mich", ",", "so", "sprach", "die", "Jung\u00b7frau", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil noch mein Fr\u00fchling bl\u00fcht.", "tokens": ["Weil", "noch", "mein", "Fr\u00fch\u00b7ling", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollt' ich darum in stetem Trauern gehn,", "tokens": ["Sollt'", "ich", "da\u00b7rum", "in", "ste\u00b7tem", "Trau\u00b7ern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df einst die Jugend flieht?", "tokens": ["Da\u00df", "einst", "die", "Ju\u00b7gend", "flieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der beschirmt und h\u00e4lt der V\u00f6glein Nest,", "tokens": ["Der", "be\u00b7schirmt", "und", "h\u00e4lt", "der", "V\u00f6\u00b7glein", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "KON", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der die Blumen bl\u00fchn und welken l\u00e4\u00dft,", "tokens": ["Der", "die", "Blu\u00b7men", "bl\u00fchn", "und", "wel\u00b7ken", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "KON", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dem traut mein Gem\u00fct.", "tokens": ["Dem", "traut", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}