{"textgrid.poem.40443": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Jubil\u00e4um der Universit\u00e4t Heidelberg", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun gr\u00fc\u00df' dich Gott, Altheidelberg!", "tokens": ["Nun", "gr\u00fc\u00df'", "dich", "Gott", ",", "Al\u00b7thei\u00b7del\u00b7berg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut rufen alle Glocken", "tokens": ["Laut", "ru\u00b7fen", "al\u00b7le", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Heil'gen Geist durch Tal und Berg", "tokens": ["Vom", "Heil'\u00b7gen", "Geist", "durch", "Tal", "und", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu jubelndem Frohlocken:", "tokens": ["Zu", "ju\u00b7beln\u00b7dem", "Froh\u00b7lo\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "F\u00fcnfhundert Jahr' \u2013 ein hohes Wort,", "tokens": ["F\u00fcnf\u00b7hun\u00b7dert", "Jahr'", "\u2013", "ein", "ho\u00b7hes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch lang' noch nicht das hehrste;", "tokens": ["Doch", "lang'", "noch", "nicht", "das", "hehrs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bl\u00fch' du nur gl\u00fcckhaft tausend fort,", "tokens": ["Bl\u00fch'", "du", "nur", "gl\u00fcck\u00b7haft", "tau\u00b7send", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann kommt das tausend erste.", "tokens": ["Dann", "kommt", "das", "tau\u00b7send", "ers\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Segen ist's der Wissenschaft,", "tokens": ["Ein", "Se\u00b7gen", "ist's", "der", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets Neues zu gestalten", "tokens": ["Stets", "Neu\u00b7es", "zu", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gleich des Fr\u00fchlings Zauberkraft", "tokens": ["Und", "gleich", "des", "Fr\u00fch\u00b7lings", "Zau\u00b7ber\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lichtspendend nie zu alten.", "tokens": ["Lichts\u00b7pen\u00b7dend", "nie", "zu", "al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wer sich in solchem Jungbrunn feit,", "tokens": ["Wer", "sich", "in", "sol\u00b7chem", "Jung\u00b7brunn", "feit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchlt jung sein Herzblut kreisen,", "tokens": ["F\u00fchlt", "jung", "sein", "Herz\u00b7blut", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Solang' ums Schlo\u00df im Maienkleid", "tokens": ["So\u00b7lang'", "ums", "Schlo\u00df", "im", "Mai\u00b7en\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die W\u00e4lder Knospen weisen.", "tokens": ["Die", "W\u00e4l\u00b7der", "Knos\u00b7pen", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Kurf\u00fcrst Ruprecht ward der Ruhm,", "tokens": ["Dem", "Kur\u00b7f\u00fcrst", "Ru\u00b7precht", "ward", "der", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In rauher Zeit der Waffen", "tokens": ["In", "rau\u00b7her", "Zeit", "der", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier ein geweihtes Heiligtum", "tokens": ["Hier", "ein", "ge\u00b7weih\u00b7tes", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Studiums zu schaffen.", "tokens": ["Des", "Stu\u00b7di\u00b7ums", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und treulich half ein frommer Mann", "tokens": ["Und", "treu\u00b7lich", "half", "ein", "from\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Bildungswerk vollbringen,", "tokens": ["Das", "Bil\u00b7dungs\u00b7werk", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den er sich in Paris gewann:", "tokens": ["Den", "er", "sich", "in", "Pa\u00b7ris", "ge\u00b7wann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Marsilius ab Inghen.", "tokens": ["Mar\u00b7si\u00b7lius", "ab", "In\u00b7ghen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Wer nennt sie all' von nah und fern", "tokens": ["Wer", "nennt", "sie", "all'", "von", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "APPR", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kutten und Talaren", "tokens": ["In", "Kut\u00b7ten", "und", "Ta\u00b7la\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hier intitulierten Herrn", "tokens": ["Die", "hier", "in\u00b7ti\u00b7tu\u00b7lier\u00b7ten", "Herrn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Magister und Scholaren?", "tokens": ["Ma\u00b7gis\u00b7ter", "und", "Scho\u00b7la\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wetteifernd wie um Pfingstenzeit", "tokens": ["Wett\u00b7ei\u00b7fernd", "wie", "um", "Pfings\u00b7ten\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich Bl\u00fcte dr\u00e4ngt an Bl\u00fcte,", "tokens": ["Sich", "Bl\u00fc\u00b7te", "dr\u00e4ngt", "an", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Tat jeder seine Schuldigkeit,", "tokens": ["Tat", "je\u00b7der", "sei\u00b7ne", "Schul\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch in verschiedner G\u00fcte.", "tokens": ["Doch", "in", "ver\u00b7schied\u00b7ner", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Scholastiker und Humanist,", "tokens": ["Scho\u00b7las\u00b7ti\u00b7ker", "und", "Hu\u00b7ma\u00b7nist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schiffhut, Barett, Per\u00fccken", "tokens": ["Schiff\u00b7hut", ",", "Ba\u00b7rett", ",", "Pe\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Erprobten Kunst, Talent und List", "tokens": ["Er\u00b7prob\u00b7ten", "Kunst", ",", "Ta\u00b7lent", "und", "List"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In wechselnden Geschicken:", "tokens": ["In", "wech\u00b7seln\u00b7den", "Ge\u00b7schi\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gottlob, da\u00df starrer Zunftverstand", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "star\u00b7rer", "Zunft\u00b7ver\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nie Zeit fand, lang' zu schaden,", "tokens": ["Nie", "Zeit", "fand", ",", "lang'", "zu", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und neu als Stifter auferstand", "tokens": ["Und", "neu", "als", "Stif\u00b7ter", "auf\u00b7er\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Karl Friederich von Baden.", "tokens": ["Karl", "Frie\u00b7de\u00b7rich", "von", "Ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Des Ahnherrn weise Milde hat", "tokens": ["Des", "Ahn\u00b7herrn", "wei\u00b7se", "Mil\u00b7de", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Enkel treu bew\u00e4hret,", "tokens": ["Sein", "En\u00b7kel", "treu", "be\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den froh als h\u00f6chsten Rektor Stadt", "tokens": ["Den", "froh", "als", "h\u00f6chs\u00b7ten", "Rek\u00b7tor", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hohe Schule ehret.", "tokens": ["Wie", "ho\u00b7he", "Schu\u00b7le", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ruperto-Karolina will", "tokens": ["Ru\u00b7per\u00b7to\u00b7Ka\u00b7ro\u00b7li\u00b7na", "will"], "token_info": ["word", "word"], "pos": ["NE", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Beschirmt durch ", "tokens": ["Be\u00b7schirmt", "durch"], "token_info": ["word", "word"], "pos": ["VVPP", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ob ihr und ihm schwebt hoch und still", "tokens": ["Ob", "ihr", "und", "ihm", "schwebt", "hoch", "und", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Stern des Idealen.", "tokens": ["Der", "Stern", "des", "I\u00b7dea\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und jubelnd d\u00fcrfen alt wie jung", "tokens": ["Und", "ju\u00b7belnd", "d\u00fcr\u00b7fen", "alt", "wie", "jung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "ADJD", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sp\u00e4t nach vielhundert Jahren", "tokens": ["Sp\u00e4t", "nach", "viel\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "CARD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "In heiliger Begeisterung", "tokens": ["In", "hei\u00b7li\u00b7ger", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wied'rum des Weges fahren:", "tokens": ["Wie\u00b7d'\u00b7rum", "des", "We\u00b7ges", "fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Geist ist's, der das Rechte weist,", "tokens": ["Der", "Geist", "ist's", ",", "der", "das", "Rech\u00b7te", "weist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wahrheit schafft und Leben,", "tokens": ["Der", "Wahr\u00b7heit", "schafft", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der starke, freie, deutsche Geist,", "tokens": ["Der", "star\u00b7ke", ",", "frei\u00b7e", ",", "deut\u00b7sche", "Geist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der uns das Reich gegeben!", "tokens": ["Der", "uns", "das", "Reich", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Heil allen, die im Wissensschacht", "tokens": ["Heil", "al\u00b7len", ",", "die", "im", "Wis\u00b7sens\u00b7schacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIAT", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht M\u00fch' noch Arbeit scheuten,", "tokens": ["Nicht", "M\u00fch'", "noch", "Ar\u00b7beit", "scheu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die manche Nacht durchdacht, durchwacht", "tokens": ["Die", "man\u00b7che", "Nacht", "durch\u00b7dacht", ",", "durch\u00b7wacht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich der Jugend freuten.", "tokens": ["Und", "sich", "der", "Ju\u00b7gend", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Heil der Stadt, wo Sch\u00f6pfungspracht", "tokens": ["Und", "Heil", "der", "Stadt", ",", "wo", "Sch\u00f6p\u00b7fungs\u00b7pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Weisheit im Vereine:", "tokens": ["Mit", "Weis\u00b7heit", "im", "Ver\u00b7ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein brausend Hoch sei dir gebracht,", "tokens": ["Ein", "brau\u00b7send", "Hoch", "sei", "dir", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Altheidelberg, du Feine!", "tokens": ["Al\u00b7thei\u00b7del\u00b7berg", ",", "du", "Fei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}