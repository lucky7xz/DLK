{"textgrid.poem.57765": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Scheidewind weht auf der Heide,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scheidewind weht auf der Heide,", "tokens": ["Schei\u00b7de\u00b7wind", "weht", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meidewind weht in dem Moor;", "tokens": ["Mei\u00b7de\u00b7wind", "weht", "in", "dem", "Moor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich suche und suche die Stelle,", "tokens": ["Ich", "su\u00b7che", "und", "su\u00b7che", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wo ich mein Herz verlor.", "tokens": ["Wo", "ich", "mein", "Herz", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier war es, wo ich es verloren,", "tokens": ["Hier", "war", "es", ",", "wo", "ich", "es", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es mu\u00df doch hier irgendwo sein;", "tokens": ["Es", "mu\u00df", "doch", "hier", "ir\u00b7gend\u00b7wo", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es liegt hier im Laube und Moose", "tokens": ["Es", "liegt", "hier", "im", "Lau\u00b7be", "und", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So mutterseelenallein.", "tokens": ["So", "mut\u00b7ter\u00b7see\u00b7len\u00b7al\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich suche und suche und suche", "tokens": ["Ich", "su\u00b7che", "und", "su\u00b7che", "und", "su\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und suche wohl hin und wohl her;", "tokens": ["Und", "su\u00b7che", "wohl", "hin", "und", "wohl", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6re und h\u00f6re es klopfen,", "tokens": ["Ich", "h\u00f6\u00b7re", "und", "h\u00f6\u00b7re", "es", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und finde es nimmermehr.", "tokens": ["Und", "fin\u00b7de", "es", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Scheidewind fl\u00fcstert im Laube,", "tokens": ["Schei\u00b7de\u00b7wind", "fl\u00fcs\u00b7tert", "im", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Meidewind fl\u00fcstert im Gras;", "tokens": ["Mei\u00b7de\u00b7wind", "fl\u00fcs\u00b7tert", "im", "Gras", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Irrkraut w\u00e4chst auf der Stelle,", "tokens": ["Irr\u00b7kraut", "w\u00e4chst", "auf", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wo ich mein Herz verga\u00df.", "tokens": ["Wo", "ich", "mein", "Herz", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}