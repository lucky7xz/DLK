{"textgrid.poem.28046": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Venus weiland sa\u00df und den Adonis k\u00fcste,", "tokens": ["Wo", "Ve\u00b7nus", "wei\u00b7land", "sa\u00df", "und", "den", "A\u00b7do\u00b7nis", "k\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wuchs Gras und Blumen auff, ob gleich der Ort war w\u00fcste.", "tokens": ["Wuchs", "Gras", "und", "Blu\u00b7men", "auff", ",", "ob", "gleich", "der", "Ort", "war", "w\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Bacchus weiland zoh, da wuchsen lauter Reben,", "tokens": ["Wo", "Bac\u00b7chus", "wei\u00b7land", "zoh", ",", "da", "wuch\u00b7sen", "lau\u00b7ter", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aller d\u00fcrrer Strauch m\u00fcst eitel Trauben geben.", "tokens": ["Und", "al\u00b7ler", "d\u00fcr\u00b7rer", "Strauch", "m\u00fcst", "ei\u00b7tel", "Trau\u00b7ben", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kans nicht die Steuer auch? Ein wolversteuret Grund", "tokens": ["Kans", "nicht", "die", "Steu\u00b7er", "auch", "?", "Ein", "wol\u00b7ver\u00b7steu\u00b7ret", "Grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Soll geben her iemehr, iemeh er w\u00fcste stund.", "tokens": ["Soll", "ge\u00b7ben", "her", "ie\u00b7mehr", ",", "ie\u00b7meh", "er", "w\u00fcs\u00b7te", "stund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer wei\u00df, ob jenes wahr? Wer wei\u00df, ob di\u00df kan seyn?", "tokens": ["Wer", "wei\u00df", ",", "ob", "je\u00b7nes", "wahr", "?", "Wer", "wei\u00df", ",", "ob", "di\u00df", "kan", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PDS", "PTKVZ", "$.", "PWS", "VVFIN", "$,", "KOUS", "PDS", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dort glaube, wer da wil! hier gibts der Augenschein.", "tokens": ["Dort", "glau\u00b7be", ",", "wer", "da", "wil", "!", "hier", "gibts", "der", "Au\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "ADV", "VMFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}