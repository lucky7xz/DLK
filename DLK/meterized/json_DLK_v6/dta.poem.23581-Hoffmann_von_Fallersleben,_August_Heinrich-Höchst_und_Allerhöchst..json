{"dta.poem.23581": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6chst und Allerh\u00f6chst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist nur der H\u00f6chst' auf Erden,", "tokens": ["Gott", "ist", "nur", "der", "H\u00f6chst'", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch der Allerh\u00f6chste nicht.", "tokens": ["Doch", "der", "Al\u00b7ler\u00b7h\u00f6chs\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du dessen inne werden,", "tokens": ["Willst", "du", "des\u00b7sen", "in\u00b7ne", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "PTKVZ", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun, so hast du hier Bericht:", "tokens": ["Nun", ",", "so", "hast", "du", "hier", "Be\u00b7richt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles Allerh\u00f6chst' auf Erden", "tokens": ["Al\u00b7les", "Al\u00b7ler\u00b7h\u00f6chst'", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist von K\u00f6nigesgeschlecht,", "tokens": ["Ist", "von", "K\u00f6\u00b7ni\u00b7ges\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das kann doch Gott nicht werden,", "tokens": ["Und", "das", "kann", "doch", "Gott", "nicht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "NN", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn das ist f\u00fcr ihn zu schlecht.", "tokens": ["Denn", "das", "ist", "f\u00fcr", "ihn", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}