{"textgrid.poem.43156": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fremder Menschen bunte Massen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fremder Menschen bunte Massen,", "tokens": ["Frem\u00b7der", "Men\u00b7schen", "bun\u00b7te", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremder Sprache milder Laut,", "tokens": ["Frem\u00b7der", "Spra\u00b7che", "mil\u00b7der", "Laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00dfe H\u00e4user, helle Stra\u00dfen,", "tokens": ["Gro\u00b7\u00dfe", "H\u00e4u\u00b7ser", ",", "hel\u00b7le", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst der Himmel heller schaut.", "tokens": ["Selbst", "der", "Him\u00b7mel", "hel\u00b7ler", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seltsam fremd, wie nie besessen,", "tokens": ["Selt\u00b7sam", "fremd", ",", "wie", "nie", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWAV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klingt mir hier der Name mein,", "tokens": ["Klingt", "mir", "hier", "der", "Na\u00b7me", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch mein Herz lernt hier vergessen,", "tokens": ["Auch", "mein", "Herz", "lernt", "hier", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lernt vielleicht hier gl\u00fccklich sein.", "tokens": ["Lernt", "viel\u00b7leicht", "hier", "gl\u00fcck\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}