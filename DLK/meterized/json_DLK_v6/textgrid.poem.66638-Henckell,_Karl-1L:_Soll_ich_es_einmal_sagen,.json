los\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sinkst mir so schl\u00e4frig hin.", "tokens": ["Sinkst", "mir", "so", "schl\u00e4f\u00b7rig", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun treiben alle Tage", "tokens": ["Nun", "trei\u00b7ben", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleichg\u00fcltig ab und zu;", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "ab", "und", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie trinkt mein Aug' noch Leben?", "tokens": ["Wie", "trinkt", "mein", "Aug'", "noch", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo f\u00fchl' ich's brausend beben?", "tokens": ["Wo", "f\u00fchl'", "ich's", "brau\u00b7send", "be\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kaum d\u00e4mmert dumpfe Sage \u2013", "tokens": ["Kaum", "d\u00e4m\u00b7mert", "dump\u00b7fe", "Sa\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun treiben alle Tage", "tokens": ["Nun", "trei\u00b7be