{"textgrid.poem.24827": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schmerz, der keinen Namen kennt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schmerz, der keinen Namen kennt,", "tokens": ["Schmerz", ",", "der", "kei\u00b7nen", "Na\u00b7men", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber allempfunden", "tokens": ["A\u00b7ber", "al\u00b7lemp\u00b7fun\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch das Herz der Wesen brennt", "tokens": ["Durch", "das", "Herz", "der", "We\u00b7sen", "brennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Myriaden Wunden;", "tokens": ["In", "My\u00b7ria\u00b7den", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "M\u00e4cht'ger, welchem unbewu\u00dft", "tokens": ["M\u00e4cht'\u00b7ger", ",", "wel\u00b7chem", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon die zarten Kleinen,", "tokens": ["Schon", "die", "zar\u00b7ten", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Saugend an der Mutter Brust,", "tokens": ["Sau\u00b7gend", "an", "der", "Mut\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Thr\u00e4nen weinen;", "tokens": ["Ih\u00b7re", "Thr\u00e4\u00b7nen", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Den der Tag, der junge, haucht", "tokens": ["Den", "der", "Tag", ",", "der", "jun\u00b7ge", ",", "haucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Morgenwinden,", "tokens": ["In", "den", "Mor\u00b7gen\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und in den zur\u00fcckgetaucht", "tokens": ["Und", "in", "den", "zu\u00b7r\u00fcck\u00b7ge\u00b7taucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seine Strahlen schwinden;", "tokens": ["Sei\u00b7ne", "Strah\u00b7len", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Der in jedem Glockenschlag,", "tokens": ["Der", "in", "je\u00b7dem", "Glo\u00b7cken\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit eh'rnem Hammer,", "tokens": ["Wie", "mit", "eh'r\u00b7nem", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du das Herz, das schon zerbrach,", "tokens": ["Du", "das", "Herz", ",", "das", "schon", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brichst durch neuen Jammer:", "tokens": ["Brichst", "durch", "neu\u00b7en", "Jam\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O, wird ewig deine Macht,", "tokens": ["O", ",", "wird", "e\u00b7wig", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird sie ewig w\u00e4hren", "tokens": ["Wird", "sie", "e\u00b7wig", "w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und noch in der Grabesnacht", "tokens": ["Und", "noch", "in", "der", "Gra\u00b7bes\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unsern Schlummer st\u00f6ren?", "tokens": ["Un\u00b7sern", "Schlum\u00b7mer", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}