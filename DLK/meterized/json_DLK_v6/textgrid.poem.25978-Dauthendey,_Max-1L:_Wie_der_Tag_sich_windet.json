{"textgrid.poem.25978": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie der Tag sich windet", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Tag sich windet", "tokens": ["Wie", "der", "Tag", "sich", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und kein Ende findet!", "tokens": ["Und", "kein", "En\u00b7de", "fin\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die Minuten stehen,", "tokens": ["Die", "Mi\u00b7nu\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00fcssen r\u00fcckw\u00e4rts sehen.", "tokens": ["M\u00fcs\u00b7sen", "r\u00fcck\u00b7w\u00e4rts", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Seit der Morgenstunde,", "tokens": ["Seit", "der", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die mit starrem Munde", "tokens": ["Die", "mit", "star\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dich zum Abschied weckte,", "tokens": ["Dich", "zum", "Ab\u00b7schied", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich nur \u00d6de streckte.", "tokens": ["Sich", "nur", "\u00d6\u00b7de", "streck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "F\u00fchl' die Haut erkalten", "tokens": ["F\u00fchl'", "die", "Haut", "er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die Stirn sich falten,", "tokens": ["Und", "die", "Stirn", "sich", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Mu\u00df ins Leere schauen", "tokens": ["Mu\u00df", "ins", "Lee\u00b7re", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und dem Tag mi\u00dftrauen.", "tokens": ["Und", "dem", "Tag", "mi\u00df\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Trennen ist ein Sterben,", "tokens": ["Tren\u00b7nen", "ist", "ein", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schl\u00e4gt die Welt in Scherben.", "tokens": ["Schl\u00e4gt", "die", "Welt", "in", "Scher\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}