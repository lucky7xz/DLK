{"textgrid.poem.56506": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sturm heult immer laut in den Kaminen", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sturm heult immer laut in den Kaminen", "tokens": ["Der", "Sturm", "heult", "im\u00b7mer", "laut", "in", "den", "Ka\u00b7mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jede Nacht ist blutig-rot und dunkel.", "tokens": ["Und", "je\u00b7de", "Nacht", "ist", "blu\u00b7tig\u00b7rot", "und", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die H\u00e4user recken sich mit leeren Mienen.", "tokens": ["Die", "H\u00e4u\u00b7ser", "re\u00b7cken", "sich", "mit", "lee\u00b7ren", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun wohnen wir in rings umbauter Enge,", "tokens": ["Nun", "woh\u00b7nen", "wir", "in", "rings", "um\u00b7bau\u00b7ter", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im kargen Licht und Dunkel unserer Gruben,", "tokens": ["Im", "kar\u00b7gen", "Licht", "und", "Dun\u00b7kel", "un\u00b7se\u00b7rer", "Gru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Seiler zerrend grauer Stunden L\u00e4nge.", "tokens": ["Wie", "Sei\u00b7ler", "zer\u00b7rend", "grau\u00b7er", "Stun\u00b7den", "L\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Tage zw\u00e4ngen sich in niedre Stuben,", "tokens": ["Die", "Ta\u00b7ge", "zw\u00e4n\u00b7gen", "sich", "in", "nied\u00b7re", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo heisres Feuer kr\u00e4chzt in gro\u00dfen \u00d6fen.", "tokens": ["Wo", "heis\u00b7res", "Feu\u00b7er", "kr\u00e4chzt", "in", "gro\u00b7\u00dfen", "\u00d6\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir stehen an den ausgefrornen Scheiben", "tokens": ["Wir", "ste\u00b7hen", "an", "den", "aus\u00b7ge\u00b7fror\u00b7nen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und starren schr\u00e4ge nach den leeren H\u00f6fen.", "tokens": ["Und", "star\u00b7ren", "schr\u00e4\u00b7ge", "nach", "den", "lee\u00b7ren", "H\u00f6\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}