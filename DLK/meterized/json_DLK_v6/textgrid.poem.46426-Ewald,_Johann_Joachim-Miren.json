{"textgrid.poem.46426": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Miren", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Miren wohnt sch\u00f6n, doch alles Hausger\u00e4the", "tokens": ["Mi\u00b7ren", "wohnt", "sch\u00f6n", ",", "doch", "al\u00b7les", "Haus\u00b7ge\u00b7r\u00e4\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das um ihn gl\u00e4nzt, Tisch, Spiegel, St\u00fchl und Bette,", "tokens": ["Das", "um", "ihn", "gl\u00e4nzt", ",", "Tisch", ",", "Spie\u00b7gel", ",", "St\u00fchl", "und", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tapet, Gem\u00e4hld und Schrank und alles ist nicht sein ...", "tokens": ["Ta\u00b7pet", ",", "Ge\u00b7m\u00e4hld", "und", "Schrank", "und", "al\u00b7les", "ist", "nicht", "sein", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "KON", "PIS", "VAFIN", "PTKNEG", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ich mag bey mir nicht fremde seyn!", "tokens": ["Ich", "mag", "bey", "mir", "nicht", "frem\u00b7de", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}