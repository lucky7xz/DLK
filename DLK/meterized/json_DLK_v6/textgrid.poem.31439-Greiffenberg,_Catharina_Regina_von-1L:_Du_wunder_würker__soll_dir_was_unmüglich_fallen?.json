{"textgrid.poem.31439": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wunder w\u00fcrker / soll dir was unm\u00fcglich fallen?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wunder w\u00fcrker / soll dir was unm\u00fcglich fallen?", "tokens": ["Du", "wun\u00b7der", "w\u00fcr\u00b7ker", "/", "soll", "dir", "was", "un\u00b7m\u00fcg\u00b7lich", "fal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VMFIN", "PPER", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bey dir auch keines wegs die wunder wunder seyn!", "tokens": ["bey", "dir", "auch", "kei\u00b7nes", "wegs", "die", "wun\u00b7der", "wun\u00b7der", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "voll unerh\u00f6rter ding' / ist deiner Allmacht schrein /", "tokens": ["voll", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "ding'", "/", "ist", "dei\u00b7ner", "All\u00b7macht", "schrein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$(", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die sich erweist und preist unendlich hoch in Allen.", "tokens": ["die", "sich", "er\u00b7weist", "und", "preist", "un\u00b7end\u00b7lich", "hoch", "in", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVPP", "KON", "VVFIN", "ADJD", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da / wo die Sonne sitzt / entdecken sich die strahlen:", "tokens": ["Da", "/", "wo", "die", "Son\u00b7ne", "sitzt", "/", "ent\u00b7de\u00b7cken", "sich", "die", "strah\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "ART", "NN", "VVFIN", "$(", "VVFIN", "PRF", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wo Gott ist / siehet man der wunder reinen schein /", "tokens": ["wo", "Gott", "ist", "/", "sie\u00b7het", "man", "der", "wun\u00b7der", "rei\u00b7nen", "schein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$(", "VVFIN", "PIS", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "die ihm / wie uns das gehn / sind eigen und gemein.", "tokens": ["die", "ihm", "/", "wie", "uns", "das", "gehn", "/", "sind", "ei\u00b7gen", "und", "ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "PWAV", "PPER", "PDS", "VVINF", "$(", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "in ihm sie / voll begierd uns zugefallen / wallen.", "tokens": ["in", "ihm", "sie", "/", "voll", "be\u00b7gierd", "uns", "zu\u00b7ge\u00b7fal\u00b7len", "/", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "$(", "ADJD", "VVFIN", "PPER", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Glaubens-Donner bricht die Wolken / da\u00df der blitz /", "tokens": ["Der", "Glau\u00b7bens\u00b7Don\u00b7ner", "bricht", "die", "Wol\u00b7ken", "/", "da\u00df", "der", "blitz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "die G\u00f6ttlich Herrlichkeit / in werken sich entdecket /", "tokens": ["die", "G\u00f6tt\u00b7lich", "Herr\u00b7lich\u00b7keit", "/", "in", "wer\u00b7ken", "sich", "ent\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "APPR", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "gezeugt aus tr\u00fcbsals k\u00e4lt' und Menschen-Liebehitz.", "tokens": ["ge\u00b7zeugt", "aus", "tr\u00fcb\u00b7sals", "k\u00e4lt'", "und", "Men\u00b7schen\u00b7Lie\u00b7be\u00b7hitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gott / zu erquicken / offt uns eine Angst erwecket.", "tokens": ["Gott", "/", "zu", "er\u00b7qui\u00b7cken", "/", "offt", "uns", "ei\u00b7ne", "Angst", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PTKZU", "VVINF", "$(", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "In Vngl\u00fccks Abgrund hat sein H\u00f6h-Art ihren sitz:", "tokens": ["In", "Vn\u00b7gl\u00fccks", "Ab\u00b7grund", "hat", "sein", "H\u00f6h\u00b7Art", "ih\u00b7ren", "sitz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das s\u00fc\u00df auf bitterkeit / und Freud' auf Leid / wol schmecket.", "tokens": ["das", "s\u00fc\u00df", "auf", "bit\u00b7ter\u00b7keit", "/", "und", "Freud'", "auf", "Leid", "/", "wol", "schme\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$(", "KON", "NN", "APPR", "NN", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}