{"dta.poem.80": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er giebt ihr seine Liebe zu verstehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin verliebt/ mein Hertze ist entz\u00fcndet/", "tokens": ["Ich", "bin", "ver\u00b7liebt", "/", "mein", "Hert\u00b7ze", "ist", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPOSAT", "VVFIN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch deine Pracht/ die mehr als menschlich ist:", "tokens": ["Durch", "dei\u00b7ne", "Pracht", "/", "die", "mehr", "als", "menschlich", "ist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "PIAT", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil nun bey dir sich auch Erbarmni\u00df findet/", "tokens": ["Weil", "nun", "bey", "dir", "sich", "auch", "Er\u00b7barm\u00b7ni\u00df", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PRF", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So gieb/ da\u00df mich gew\u00fcnschte H\u00fclffe k\u00fcst/", "tokens": ["So", "gieb", "/", "da\u00df", "mich", "ge\u00b7w\u00fcnschte", "H\u00fclf\u00b7fe", "k\u00fcst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$(", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Eh ", "tokens": ["Eh"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ich bin verliebt.", "tokens": ["Ich", "bin", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Augen Pracht/ dein angenehmes Wesen/", "tokens": ["Der", "Au\u00b7gen", "Pracht", "/", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und was noch sonst dir ", "tokens": ["Und", "was", "noch", "sonst", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADV", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Macht/ wenn ich todt/ mich wiederum genesen:", "tokens": ["Macht", "/", "wenn", "ich", "todt", "/", "mich", "wie\u00b7de\u00b7rum", "ge\u00b7ne\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADJD", "$(", "PPER", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Ein s\u00fcsser Ku\u00df der Seelen-Wunde heilt/", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Ku\u00df", "der", "See\u00b7len\u00b7Wun\u00b7de", "heilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Krancke jauchzt/ wenn tr\u00f6stlich ihn anlacht/", "tokens": ["Der", "Kran\u00b7cke", "jauchzt", "/", "wenn", "tr\u00f6st\u00b7lich", "ihn", "an\u00b7lacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KOUS", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Augen Pracht.", "tokens": ["Der", "Au\u00b7gen", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}