{"dta.poem.9790": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem lob-gedichte  \n Friedrich Wilhelms des Gro\u00dfen/  \n Chur-F\u00fcrstens zu Brandenburg.  \n Der  \n seegen und abschied  \n Seiner Churf\u00fcrstlichen Durchl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du aber/ Friederich/ mein \u00e4ltst- und theurster sohn/", "tokens": ["Du", "a\u00b7ber", "/", "Frie\u00b7de\u00b7rich", "/", "mein", "\u00e4lt\u00b7st", "und", "theurs\u00b7ter", "sohn", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "NE", "$(", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du erbe meines reichs/ nim ein den vater-thron/", "tokens": ["Du", "er\u00b7be", "mei\u00b7nes", "reichs", "/", "nim", "ein", "den", "va\u00b7ter\u00b7thron", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "VVIMP", "ART", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey freudig und getrost/ und la\u00df vor nichts dir grauen:", "tokens": ["Sey", "freu\u00b7dig", "und", "ge\u00b7trost", "/", "und", "la\u00df", "vor", "nichts", "dir", "grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVPP", "$(", "KON", "VVIMP", "APPR", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So schwer des zepters hefft auch immer anzuschauen.", "tokens": ["So", "schwer", "des", "zep\u00b7ters", "hefft", "auch", "im\u00b7mer", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich lasse dir viel land; doch auch viel f\u00e4higkeit/", "tokens": ["Ich", "las\u00b7se", "dir", "viel", "land", ";", "doch", "auch", "viel", "f\u00e4\u00b7hig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$.", "ADV", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich kenne deinen muth zum frieden und zum streit.", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7nen", "muth", "zum", "frie\u00b7den", "und", "zum", "streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die thaten meines lauffs kan zwar kein jahrbuch fa\u00dfen;", "tokens": ["Die", "tha\u00b7ten", "mei\u00b7nes", "lauffs", "kan", "zwar", "kein", "jahr\u00b7buch", "fa\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch hab ich viel/ noch viel zu thun dir hinterlassen.", "tokens": ["Doch", "hab", "ich", "viel", "/", "noch", "viel", "zu", "thun", "dir", "hin\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$(", "ADV", "PIS", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die gr\u00f6sse hat/ nechst GOtt/ dein Brandenburg von mir;", "tokens": ["Die", "gr\u00f6s\u00b7se", "hat", "/", "nechst", "Gott", "/", "dein", "Bran\u00b7den\u00b7burg", "von", "mir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "VVFIN", "NN", "$(", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch die gl\u00fcckseeligkeit erwartet es von dir.", "tokens": ["Doch", "die", "gl\u00fcck\u00b7see\u00b7lig\u00b7keit", "er\u00b7war\u00b7tet", "es", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du bist der eintzige/ den ich mir kan erkiesen/", "tokens": ["Du", "bist", "der", "eint\u00b7zi\u00b7ge", "/", "den", "ich", "mir", "kan", "er\u00b7kie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$(", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von der verstorbenen Oranischen Luysen.", "tokens": ["Von", "der", "ver\u00b7stor\u00b7be\u00b7nen", "O\u00b7ra\u00b7ni\u00b7schen", "Luy\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Doch lebt von dem gebl\u00fct ein Wilhelm noch der welt/", "tokens": ["Doch", "lebt", "von", "dem", "ge\u00b7bl\u00fct", "ein", "Wil\u00b7helm", "noch", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NE", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der mit dir sein geschlecht und meinen ruhm erh\u00e4lt.", "tokens": ["Der", "mit", "dir", "sein", "ge\u00b7schlecht", "und", "mei\u00b7nen", "ruhm", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPOSAT", "VVPP", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es werden (wo mein tod vermag zu prophezeyen)", "tokens": ["Es", "wer\u00b7den", "(", "wo", "mein", "tod", "ver\u00b7mag", "zu", "pro\u00b7phe\u00b7ze\u00b7yen", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Vor euch sich k\u00f6nige der gr\u00f6sten reiche scheuen.", "tokens": ["Vor", "euch", "sich", "k\u00f6\u00b7ni\u00b7ge", "der", "gr\u00f6s\u00b7ten", "rei\u00b7che", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VMFIN", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er nimt/ (ich seh\u2019 es schon) sein k\u00f6nigs-erbtheil ein/", "tokens": ["Er", "nimt", "/", "(", "ich", "seh'", "es", "schon", ")", "sein", "k\u00f6\u00b7nigs\u00b7erbt\u00b7heil", "ein", "/"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "VAINF", "VMFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und du wirst ihm/ als freund/ hierinn beh\u00fclflich seyn.", "tokens": ["Und", "du", "wirst", "ihm", "/", "als", "freund", "/", "hie\u00b7rinn", "be\u00b7h\u00fcl\u00b7flich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$(", "KOUS", "NN", "$(", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er wird/ nach dem gesetz/ ein frevel-kind verjagen/", "tokens": ["Er", "wird", "/", "nach", "dem", "ge\u00b7setz", "/", "ein", "fre\u00b7vel\u00b7kind", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "APPR", "ART", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und du wirst unterde\u00df den bunds-genossen schlagen.", "tokens": ["Und", "du", "wirst", "un\u00b7ter\u00b7de\u00df", "den", "bunds\u00b7ge\u00b7nos\u00b7sen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der neid hat euch bi\u00dfher ver\u00e4chtlich angesehn/", "tokens": ["Der", "neid", "hat", "euch", "bi\u00df\u00b7her", "ver\u00b7\u00e4cht\u00b7lich", "an\u00b7ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nun aber wird euch GOtt selbst \u00fcber ihn erh\u00f6hn.", "tokens": ["Nun", "a\u00b7ber", "wird", "euch", "Gott", "selbst", "\u00fc\u00b7ber", "ihn", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jhr werdet beyde seyn/ die Streitbaren und Dritten;", "tokens": ["Ihr", "wer\u00b7det", "bey\u00b7de", "seyn", "/", "die", "Streit\u00b7ba\u00b7ren", "und", "Drit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VAINF", "$(", "ART", "NN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du Friederich bey uns/ er Wilhelm bey den Britten;", "tokens": ["Du", "Frie\u00b7de\u00b7rich", "bey", "uns", "/", "er", "Wil\u00b7helm", "bey", "den", "Brit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "PPER", "$(", "PPER", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du in dem R\u00f6mschen reich/ er aber \u00fcber meer/", "tokens": ["Du", "in", "dem", "R\u00f6m\u00b7schen", "reich", "/", "er", "a\u00b7ber", "\u00fc\u00b7ber", "meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$(", "PPER", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und beydes wird bestehn durch euer krieges-heer.", "tokens": ["Und", "bey\u00b7des", "wird", "be\u00b7stehn", "durch", "eu\u00b7er", "krie\u00b7ge\u00b7sheer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jhr habt euch beyde zwar getheilt in meinen nahmen;", "tokens": ["Ihr", "habt", "euch", "bey\u00b7de", "zwar", "ge\u00b7theilt", "in", "mei\u00b7nen", "nah\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch werdt ihr/ ungetheilt/ besch\u00fctzen euren saamen.", "tokens": ["Doch", "werdt", "ihr", "/", "un\u00b7ge\u00b7theilt", "/", "be\u00b7sch\u00fct\u00b7zen", "eu\u00b7ren", "saa\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$(", "ADJD", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein blut/ ein muth/ ein hertz wird schrecken euren seind/", "tokens": ["Ein", "blut", "/", "ein", "muth", "/", "ein", "hertz", "wird", "schre\u00b7cken", "eu\u00b7ren", "seind", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "VAFIN", "VVINF", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wenn ihr euch und mich also in euch vereint/", "tokens": ["Und", "wenn", "ihr", "euch", "und", "mich", "al\u00b7so", "in", "euch", "ver\u00b7eint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "KON", "PPER", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wird Fridrich Wilhelms geist (der himmel woll\u2019 es geben!)", "tokens": ["Wird", "Frid\u00b7rich", "Wil\u00b7helms", "geist", "(", "der", "him\u00b7mel", "woll'", "es", "ge\u00b7ben", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NE", "NE", "VVPP", "$(", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In euren tugenden und nahmen ewig leben.", "tokens": ["In", "eu\u00b7ren", "tu\u00b7gen\u00b7den", "und", "nah\u00b7men", "e\u00b7wig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}