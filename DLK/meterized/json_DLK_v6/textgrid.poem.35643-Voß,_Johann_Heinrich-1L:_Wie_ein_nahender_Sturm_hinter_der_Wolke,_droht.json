{"textgrid.poem.35643": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ein nahender Sturm hinter der Wolke, droht", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein nahender Sturm hinter der Wolke, droht", "tokens": ["Wie", "ein", "na\u00b7hen\u00b7der", "Sturm", "hin\u00b7ter", "der", "Wol\u00b7ke", ",", "droht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schon von ferne der Tag, welcher mit eh'rner Faust", "tokens": ["Schon", "von", "fer\u00b7ne", "der", "Tag", ",", "wel\u00b7cher", "mit", "eh'r\u00b7ner", "Faust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dich, mein redlicher E[smarch],", "tokens": ["Dich", ",", "mein", "red\u00b7li\u00b7cher", "E", "smarch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$(", "VVIMP", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Armen entrei\u00dfen wird!", "tokens": ["Mei\u00b7nen", "Ar\u00b7men", "ent\u00b7rei\u00b7\u00dfen", "wird", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ach! dann sch\u00fctzt uns die Glut unserer Liebe nicht,", "tokens": ["Ach", "!", "dann", "sch\u00fctzt", "uns", "die", "Glut", "un\u00b7se\u00b7rer", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Nicht die selige Lust, welche wir kosteten,", "tokens": ["Nicht", "die", "se\u00b7li\u00b7ge", "Lust", ",", "wel\u00b7che", "wir", "kos\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wenn vertraulich die Lenzflur", "tokens": ["Wenn", "ver\u00b7trau\u00b7lich", "die", "Lenz\u00b7flur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aus dem Wirbel der Stadt uns rief;", "tokens": ["Aus", "dem", "Wir\u00b7bel", "der", "Stadt", "uns", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder, wenn uns die Nacht bei dem dirc\u00ebischen", "tokens": ["O\u00b7der", ",", "wenn", "uns", "die", "Nacht", "bei", "dem", "dirc\u00ebi\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "P\u00e4an, oder beim Sang brittischer Barden fand,", "tokens": ["P\u00e4\u00b7an", ",", "o\u00b7der", "beim", "Sang", "brit\u00b7ti\u00b7scher", "Bar\u00b7den", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und beim holden Gefl\u00fcster,", "tokens": ["Und", "beim", "hol\u00b7den", "Ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo sich Herzen begegneten!", "tokens": ["Wo", "sich", "Her\u00b7zen", "be\u00b7ge\u00b7gne\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gestern sah ich ein Paar Tauben in jenem Ulm;", "tokens": ["Ge\u00b7stern", "sah", "ich", "ein", "Paar", "Tau\u00b7ben", "in", "je\u00b7nem", "Ulm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie umfl\u00fcgelten sich, kos'ten und schn\u00e4belten:", "tokens": ["Sie", "um\u00b7fl\u00fc\u00b7gel\u00b7ten", "sich", ",", "kos'\u00b7ten", "und", "schn\u00e4\u00b7bel\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "--++--+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dennoch st\u00fcrzte der Habicht", "tokens": ["Den\u00b7noch", "st\u00fcrz\u00b7te", "der", "Ha\u00b7bicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf das Weibchen \u2013 der Witwer girrt!", "tokens": ["Auf", "das", "Weib\u00b7chen", "\u2013", "der", "Wit\u00b7wer", "girrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Zu der K\u00fcste zur\u00fcck tapferer Angeln rei\u00dft", "tokens": ["Zu", "der", "K\u00fcs\u00b7te", "zu\u00b7r\u00fcck", "tap\u00b7fe\u00b7rer", "An\u00b7geln", "rei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dich das st\u00e4ubende Rad: \u00fcber ein kleines trau'rt", "tokens": ["Dich", "das", "st\u00e4u\u00b7ben\u00b7de", "Rad", ":", "\u00fc\u00b7ber", "ein", "klei\u00b7nes", "trau'rt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "An der Warne dein Bester,", "tokens": ["An", "der", "War\u00b7ne", "dein", "Bes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und wohin ihn das Gl\u00fcck verweht!", "tokens": ["Und", "wo\u00b7hin", "ihn", "das", "Gl\u00fcck", "ver\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn du dort, an der Hand deiner Emilia,", "tokens": ["Wenn", "du", "dort", ",", "an", "der", "Hand", "dei\u00b7ner", "E\u00b7mi\u00b7lia", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "NN", "PPOSAT", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Einst beim Schimmer des Monds Nachtigallstimmen horchst;", "tokens": ["Einst", "beim", "Schim\u00b7mer", "des", "Monds", "Nach\u00b7ti\u00b7gall\u00b7stim\u00b7men", "horchst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dann sprich seufzend: Geliebte,", "tokens": ["Dann", "sprich", "seuf\u00b7zend", ":", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So geliebet, wie jetzt von dir,", "tokens": ["So", "ge\u00b7lie\u00b7bet", ",", "wie", "jetzt", "von", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWAV", "ADV", "APPR", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Horcht' ich vormals und Vo\u00df' Nachtigallstimmen oft!", "tokens": ["Horcht'", "ich", "vor\u00b7mals", "und", "Vo\u00df'", "Nach\u00b7ti\u00b7gall\u00b7stim\u00b7men", "oft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "NE", "NN", "ADV", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ach! jetzt h\u00f6ret er dich, Nachtigall, nicht! Jetzt trau'rt", "tokens": ["Ach", "!", "jetzt", "h\u00f6\u00b7ret", "er", "dich", ",", "Nach\u00b7ti\u00b7gall", ",", "nicht", "!", "Jetzt", "trau'rt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "PTKNEG", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "An der Warne mein Bester,", "tokens": ["An", "der", "War\u00b7ne", "mein", "Bes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und wohin ihn das Gl\u00fcck verweht!", "tokens": ["Und", "wo\u00b7hin", "ihn", "das", "Gl\u00fcck", "ver\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}