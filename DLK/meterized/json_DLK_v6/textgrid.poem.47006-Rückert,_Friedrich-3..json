{"textgrid.poem.47006": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt', da\u00df Berge, starr von W\u00e4ldern grausend,", "tokens": ["Ich", "wollt'", ",", "da\u00df", "Ber\u00b7ge", ",", "starr", "von", "W\u00e4l\u00b7dern", "grau\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "NN", "$,", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Felsenh\u00f6hn von nie gesprengten H\u00e4rten", "tokens": ["Und", "Fel\u00b7sen\u00b7h\u00f6hn", "von", "nie", "ge\u00b7spreng\u00b7ten", "H\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Sandesw\u00fcsten mir den Zugang sperrten,", "tokens": ["Und", "San\u00b7des\u00b7w\u00fcs\u00b7ten", "mir", "den", "Zu\u00b7gang", "sperr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Meeresfluten wild im Sturme brausend;", "tokens": ["Und", "Mee\u00b7res\u00b7flu\u00b7ten", "wild", "im", "Stur\u00b7me", "brau\u00b7send", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Riesen wie vor Zauberschl\u00f6ssern hausend,", "tokens": ["Und", "Rie\u00b7sen", "wie", "vor", "Zau\u00b7ber\u00b7schl\u00f6s\u00b7sern", "hau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Drachen wie vor Hesperideng\u00e4rten,", "tokens": ["Und", "Dra\u00b7chen", "wie", "vor", "Hes\u00b7pe\u00b7ri\u00b7den\u00b7g\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scharw\u00e4chter mit entbl\u00f6\u00dften tausend Schwerten,", "tokens": ["Schar\u00b7w\u00e4ch\u00b7ter", "mit", "ent\u00b7bl\u00f6\u00df\u00b7ten", "tau\u00b7send", "Schwer\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zuchth\u00fcter auch mit offnen Augen tausend:", "tokens": ["Zucht\u00b7h\u00fc\u00b7ter", "auch", "mit", "off\u00b7nen", "Au\u00b7gen", "tau\u00b7send", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So k\u00f6nnt' ich doch bei aller Not noch hoffen,", "tokens": ["So", "k\u00f6nnt'", "ich", "doch", "bei", "al\u00b7ler", "Not", "noch", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Mut, durch List, am Ende zu bemeistern", "tokens": ["Durch", "Mut", ",", "durch", "List", ",", "am", "En\u00b7de", "zu", "be\u00b7meis\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Trotz der einen und der andern Schlauheit;", "tokens": ["Den", "Trotz", "der", "ei\u00b7nen", "und", "der", "an\u00b7dern", "Schlau\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Statt da\u00df mir jetzo Th\u00fcr und Thor ist offen,", "tokens": ["Statt", "da\u00df", "mir", "jet\u00b7zo", "Th\u00fcr", "und", "Thor", "ist", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sie sitzt da, mehr als von tausend Geistern", "tokens": ["Und", "sie", "sitzt", "da", ",", "mehr", "als", "von", "tau\u00b7send", "Geis\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PIAT", "KOKOM", "APPR", "CARD", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bewacht von nichts als ihrer eignen Rauheit.", "tokens": ["Be\u00b7wacht", "von", "nichts", "als", "ih\u00b7rer", "eig\u00b7nen", "Rau\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}