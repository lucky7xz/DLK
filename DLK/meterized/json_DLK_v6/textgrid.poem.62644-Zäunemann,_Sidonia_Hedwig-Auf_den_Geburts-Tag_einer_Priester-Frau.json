{"textgrid.poem.62644": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Geburts-Tag einer Priester-Frau", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Mensch in dieser Welt, wer er nur immer sey,", "tokens": ["Kein", "Mensch", "in", "die\u00b7ser", "Welt", ",", "wer", "er", "nur", "im\u00b7mer", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "$,", "PWS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist von den Regungen und von Affecten frey.", "tokens": ["Ist", "von", "den", "Re\u00b7gun\u00b7gen", "und", "von", "Af\u00b7fec\u00b7ten", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bald \u00fcbereilet ihn Furcht, Zorn, Neid, Hoffarth, Liebe,", "tokens": ["Bald", "\u00fc\u00b7be\u00b7rei\u00b7let", "ihn", "Furcht", ",", "Zorn", ",", "Neid", ",", "Hof\u00b7farth", ",", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald Rachgier, Eifersucht, und auch des Geitzes Triebe.", "tokens": ["Bald", "Rach\u00b7gier", ",", "Ei\u00b7fer\u00b7sucht", ",", "und", "auch", "des", "Geit\u00b7zes", "Trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die streiten denn in ihn: da mu\u00df er munter seyn,", "tokens": ["Die", "strei\u00b7ten", "denn", "in", "ihn", ":", "da", "mu\u00df", "er", "mun\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPER", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn sonsten b\u00fcsset er gewi\u00df im Kampfe ein:", "tokens": ["Denn", "sons\u00b7ten", "b\u00fcs\u00b7set", "er", "ge\u00b7wi\u00df", "im", "Kamp\u00b7fe", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und tr\u00e4gt den Ruhm nicht weg, den solche Streiter kriegen,", "tokens": ["Und", "tr\u00e4gt", "den", "Ruhm", "nicht", "weg", ",", "den", "sol\u00b7che", "Strei\u00b7ter", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ihre Regungen, und ihren Muth besiegen.", "tokens": ["Die", "ih\u00b7re", "Re\u00b7gun\u00b7gen", ",", "und", "ih\u00b7ren", "Muth", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Der weise K\u00f6nig bringt ja selbst das Zeugni\u00df bey:", "tokens": ["Der", "wei\u00b7se", "K\u00f6\u00b7nig", "bringt", "ja", "selbst", "das", "Zeug\u00b7ni\u00df", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df der, der seinen Muth bezwinget, gr\u00f6sser sey,", "tokens": ["Da\u00df", "der", ",", "der", "sei\u00b7nen", "Muth", "be\u00b7zwin\u00b7get", ",", "gr\u00f6s\u00b7ser", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als der, so St\u00e4dte zwingt. Bald will der Zorn erwachen,", "tokens": ["Als", "der", ",", "so", "St\u00e4d\u00b7te", "zwingt", ".", "Bald", "will", "der", "Zorn", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "NN", "VVFIN", "$.", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und den Beleidiger sogleich zu Schanden machen.", "tokens": ["Und", "den", "Be\u00b7lei\u00b7di\u00b7ger", "sog\u00b7leich", "zu", "Schan\u00b7den", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald regt sich wiederum im Herzen Lust und Freud;", "tokens": ["Bald", "regt", "sich", "wie\u00b7de\u00b7rum", "im", "Her\u00b7zen", "Lust", "und", "Freud", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein andermahl empfindt die Seele Traurigkeit.", "tokens": ["Ein", "an\u00b7der\u00b7mahl", "emp\u00b7findt", "die", "See\u00b7le", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Drum merket, da\u00df der Mensch sich wohl zu h\u00fcten habe,", "tokens": ["Drum", "mer\u00b7ket", ",", "da\u00df", "der", "Mensch", "sich", "wohl", "zu", "h\u00fc\u00b7ten", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df er bey dem Verlust der und auch jener Gabe,", "tokens": ["Da\u00df", "er", "bey", "dem", "Ver\u00b7lust", "der", "und", "auch", "je\u00b7ner", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "KON", "ADV", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Nicht allzu traurig wird, und aus den Schranken geht,", "tokens": ["Nicht", "all\u00b7zu", "trau\u00b7rig", "wird", ",", "und", "aus", "den", "Schran\u00b7ken", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "VAFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Worin ein guter Christ und tapfrer Streitet steht.", "tokens": ["Wo\u00b7rin", "ein", "gu\u00b7ter", "Christ", "und", "tapf\u00b7rer", "Strei\u00b7tet", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Leid hat seine Zeit: wenn Gott uns Plagen schicket;", "tokens": ["Das", "Leid", "hat", "sei\u00b7ne", "Zeit", ":", "wenn", "Gott", "uns", "Pla\u00b7gen", "schi\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "KOUS", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er den Hagel ruft; wenn er das Rachschwerdt z\u00fccket;", "tokens": ["Wenn", "er", "den", "Ha\u00b7gel", "ruft", ";", "wenn", "er", "das", "Rach\u00b7schwerdt", "z\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wofern wir wieder Gott und N\u00e4chsten mi\u00dfgethan,", "tokens": ["Wo\u00b7fern", "wir", "wie\u00b7der", "Gott", "und", "N\u00e4chs\u00b7ten", "mi\u00df\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ist es Zeit und Stund darinn man weinen kan.", "tokens": ["Da", "ist", "es", "Zeit", "und", "Stund", "da\u00b7rinn", "man", "wei\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "PAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Lachen hat zugleich auch seine Zeit und Stunde;", "tokens": ["Das", "La\u00b7chen", "hat", "zu\u00b7gleich", "auch", "sei\u00b7ne", "Zeit", "und", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht etwan, da\u00df man sich mit Geist, mit Seel und Munde", "tokens": ["Nicht", "et\u00b7wan", ",", "da\u00df", "man", "sich", "mit", "Geist", ",", "mit", "Seel", "und", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PIS", "PRF", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erfreuet/ wenn man soll bey Tanzen, Spiel und Wein,", "tokens": ["Er\u00b7freu\u00b7et", "/", "wenn", "man", "soll", "bey", "Tan\u00b7zen", ",", "Spiel", "und", "Wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PIS", "VMFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey eitelem Geschw\u00e4tz und Lust zugegen seyn;", "tokens": ["Bey", "ei\u00b7te\u00b7lem", "Ge\u00b7schw\u00e4tz", "und", "Lust", "zu\u00b7ge\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht, wo man Narrethey auf denen Lippen heget;", "tokens": ["Nicht", ",", "wo", "man", "Nar\u00b7ret\u00b7hey", "auf", "de\u00b7nen", "Lip\u00b7pen", "he\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PIS", "NN", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Nicht, wenn die Hand des Herrn den N\u00e4chsten dr\u00fcckt und schl\u00e4get,", "tokens": ["Nicht", ",", "wenn", "die", "Hand", "des", "Herrn", "den", "N\u00e4chs\u00b7ten", "dr\u00fcckt", "und", "schl\u00e4\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der uns zuwider ist; nicht, wenn wir unserm Feind,", "tokens": ["Der", "uns", "zu\u00b7wi\u00b7der", "ist", ";", "nicht", ",", "wenn", "wir", "un\u00b7serm", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "$.", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Freund, Nachbar, Fremdling, Gast und wen man sonst noch meint,", "tokens": ["Freund", ",", "Nach\u00b7bar", ",", "Fremd\u00b7ling", ",", "Gast", "und", "wen", "man", "sonst", "noch", "meint", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "PWS", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An seinem Amt und Gut und Ehren schaden k\u00f6nnen;", "tokens": ["An", "sei\u00b7nem", "Amt", "und", "Gut", "und", "Eh\u00b7ren", "scha\u00b7den", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJD", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht wenn ihn andere sein Leid und Ungl\u00fcck g\u00f6nnen;", "tokens": ["Nicht", "wenn", "ihn", "an\u00b7de\u00b7re", "sein", "Leid", "und", "Un\u00b7gl\u00fcck", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PIS", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nicht wenn ein b\u00f6ser Rath und Anschlag uns gelingt;", "tokens": ["Nicht", "wenn", "ein", "b\u00f6\u00b7ser", "Rath", "und", "An\u00b7schlag", "uns", "ge\u00b7lingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ART", "ADJA", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht, wenn Gott unsre Feind in unsre H\u00e4nde bringt.", "tokens": ["Nicht", ",", "wenn", "Gott", "uns\u00b7re", "Feind", "in", "uns\u00b7re", "H\u00e4n\u00b7de", "bringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nein! diese Freud ist toll, die\u00df Lachen heiset S\u00fcnde,", "tokens": ["Nein", "!", "die\u00b7se", "Freud", "ist", "toll", ",", "die\u00df", "La\u00b7chen", "hei\u00b7set", "S\u00fcn\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDAT", "NN", "VAFIN", "ADJD", "$,", "PDS", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und hat nie seine Zeit: Denn einen Gottes Kinde", "tokens": ["Und", "hat", "nie", "sei\u00b7ne", "Zeit", ":", "Denn", "ei\u00b7nen", "Got\u00b7tes", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Geziehmt sich dieses nicht. Ein andres Lachen hat", "tokens": ["Ge\u00b7ziehmt", "sich", "die\u00b7ses", "nicht", ".", "Ein", "and\u00b7res", "La\u00b7chen", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PDS", "PTKNEG", "$.", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wohl seine Zeit und Stund: Wenn Gott uns fr\u00fch und spat", "tokens": ["Wohl", "sei\u00b7ne", "Zeit", "und", "Stund", ":", "Wenn", "Gott", "uns", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "$.", "KOUS", "NN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit Str\u00f6men seiner G\u00fct und Seegen \u00fcbersch\u00fcttet,", "tokens": ["Mit", "Str\u00f6\u00b7men", "sei\u00b7ner", "G\u00fct", "und", "See\u00b7gen", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und uns vor mancherley Gefahr und Noth beh\u00fctet.", "tokens": ["Und", "uns", "vor", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr", "und", "Noth", "be\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn Gott sein Wort uns schenkt; wenn er die S\u00fcnd vergiebt,", "tokens": ["Wenn", "Gott", "sein", "Wort", "uns", "schenkt", ";", "wenn", "er", "die", "S\u00fcnd", "ver\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und uns in seinem Sohn treu, z\u00e4rtlich, ewig liebt.", "tokens": ["Und", "uns", "in", "sei\u00b7nem", "Sohn", "treu", ",", "z\u00e4rt\u00b7lich", ",", "e\u00b7wig", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bey solchen Zeiten soll sich Geist und Mund erheben,", "tokens": ["Bey", "sol\u00b7chen", "Zei\u00b7ten", "soll", "sich", "Geist", "und", "Mund", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da soll es Fr\u00f6lichkeit und Jauchzen von sich geben.", "tokens": ["Da", "soll", "es", "Fr\u00f6\u00b7lich\u00b7keit", "und", "Jauch\u00b7zen", "von", "sich", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nebst diesen giebt Gott auch zuweilen einen Tag", "tokens": ["Nebst", "die\u00b7sen", "giebt", "Gott", "auch", "zu\u00b7wei\u00b7len", "ei\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An welchem man sich wohl besonders freuen mag.", "tokens": ["An", "wel\u00b7chem", "man", "sich", "wohl", "be\u00b7son\u00b7ders", "freu\u00b7en", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PRELS", "PIS", "PRF", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald l\u00e4sset uns der Herr ein Freuden-Fest begehen,", "tokens": ["Bald", "l\u00e4s\u00b7set", "uns", "der", "Herr", "ein", "Freu\u00b7den\u00b7Fest", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald soll uns eine Zeit zur Ehren-Bahn erh\u00f6hen,", "tokens": ["Bald", "soll", "uns", "ei\u00b7ne", "Zeit", "zur", "Eh\u00b7ren\u00b7Bahn", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald l\u00e4\u00dft uns seine G\u00fct dieselbe Stunde sehn,", "tokens": ["Bald", "l\u00e4\u00dft", "uns", "sei\u00b7ne", "G\u00fct", "die\u00b7sel\u00b7be", "Stun\u00b7de", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In der wir aus dem Leib der Mutter konten gehn.", "tokens": ["In", "der", "wir", "aus", "dem", "Leib", "der", "Mut\u00b7ter", "kon\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In der er uns das Licht der Erden schauen liesse,", "tokens": ["In", "der", "er", "uns", "das", "Licht", "der", "Er\u00b7den", "schau\u00b7en", "lies\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und uns durchs Wasserbad zu seiner Kindschaft wiese.", "tokens": ["Und", "uns", "durchs", "Was\u00b7ser\u00b7bad", "zu", "sei\u00b7ner", "Kind\u00b7schaft", "wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu einer solchen Zeit und Stund geziemt es sich,", "tokens": ["Zu", "ei\u00b7ner", "sol\u00b7chen", "Zeit", "und", "Stund", "ge\u00b7ziemt", "es", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "KON", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ist Gott angenehm, da\u00df man sich sonderlich,", "tokens": ["Und", "ist", "Gott", "an\u00b7ge\u00b7nehm", ",", "da\u00df", "man", "sich", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADJD", "$,", "KOUS", "PIS", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit Herz und Mund erfreut: Da mag des Mundes Lachen", "tokens": ["Mit", "Herz", "und", "Mund", "er\u00b7freut", ":", "Da", "mag", "des", "Mun\u00b7des", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Freudigkeit der Brust klar, laut und ruchbar machen.", "tokens": ["Die", "Freu\u00b7dig\u00b7keit", "der", "Brust", "klar", ",", "laut", "und", "ruch\u00b7bar", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nun einen solchen Tag, ", "tokens": ["Nun", "ei\u00b7nen", "sol\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft dir derselbige, der unser aller Sinn", "tokens": ["L\u00e4\u00dft", "dir", "der\u00b7sel\u00b7bi\u00b7ge", ",", "der", "un\u00b7ser", "al\u00b7ler", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erforschet und ergr\u00fcndt, bey allen Wohl erblicken.", "tokens": ["Er\u00b7for\u00b7schet", "und", "er\u00b7gr\u00fcndt", ",", "bey", "al\u00b7len", "Wohl", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heut ist es dir erlaubt die Freude auszudr\u00fccken", "tokens": ["Heut", "ist", "es", "dir", "er\u00b7laubt", "die", "Freu\u00b7de", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die deine Seel empfindt. Ich seh bereits im Geist,", "tokens": ["Die", "dei\u00b7ne", "Seel", "emp\u00b7findt", ".", "Ich", "seh", "be\u00b7reits", "im", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie du die Vaters-Hand vor solche Wohlthat prei\u00dft.", "tokens": ["Wie", "du", "die", "Va\u00b7ter\u00b7sHand", "vor", "sol\u00b7che", "Wohlt\u00b7hat", "prei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ich kan ebenfals die Freude nicht verhehlen,", "tokens": ["Und", "ich", "kan", "e\u00b7ben\u00b7fals", "die", "Freu\u00b7de", "nicht", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Worein mich, ", "tokens": ["Wo\u00b7rein", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$,"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Und darum hab ich auch an meine Pflicht gedacht,", "tokens": ["Und", "da\u00b7rum", "hab", "ich", "auch", "an", "mei\u00b7ne", "Pflicht", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und such dieselbige jetzunder abzutragen.", "tokens": ["Und", "such", "die\u00b7sel\u00b7bi\u00b7ge", "je\u00b7tzun\u00b7der", "ab\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will zwar kurz und schlecht, jedoch von Herzen sagen:", "tokens": ["Ich", "will", "zwar", "kurz", "und", "schlecht", ",", "je\u00b7doch", "von", "Her\u00b7zen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott sende dir bey Gl\u00fcck, bey Seegen, Fried und Ruh,", "tokens": ["Gott", "sen\u00b7de", "dir", "bey", "Gl\u00fcck", ",", "bey", "See\u00b7gen", ",", "Fried", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Den heutgen Freuden-Tag noch oftermahlen zu:", "tokens": ["Den", "heut\u00b7gen", "Freu\u00b7den\u00b7Tag", "noch", "of\u00b7ter\u00b7mah\u00b7len", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es m\u00fcsse seine Hand dich wieder alles schrecken,", "tokens": ["Es", "m\u00fcs\u00b7se", "sei\u00b7ne", "Hand", "dich", "wie\u00b7der", "al\u00b7les", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was dir schaden kan, besch\u00fctzen und bedecken.", "tokens": ["Und", "was", "dir", "scha\u00b7den", "kan", ",", "be\u00b7sch\u00fct\u00b7zen", "und", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}