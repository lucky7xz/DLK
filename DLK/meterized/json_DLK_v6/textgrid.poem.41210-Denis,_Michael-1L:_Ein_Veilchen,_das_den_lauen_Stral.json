{"textgrid.poem.41210": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Veilchen, das den lauen Stral", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Veilchen, das den lauen Stral", "tokens": ["Ein", "Veil\u00b7chen", ",", "das", "den", "lau\u00b7en", "Stral"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lenzes in der Erde f\u00fchlt,", "tokens": ["Des", "Len\u00b7zes", "in", "der", "Er\u00b7de", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00e4ngt auf, und Mutter Erde wehrt", "tokens": ["Dr\u00e4ngt", "auf", ",", "und", "Mut\u00b7ter", "Er\u00b7de", "wehrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ihrem Kinde nicht.", "tokens": ["Es", "ih\u00b7rem", "Kin\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Lied, das in der Bardenbrust", "tokens": ["Ein", "Lied", ",", "das", "in", "der", "Bar\u00b7den\u00b7brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freundschaft holde W\u00e4rme f\u00fchlt,", "tokens": ["Der", "Freund\u00b7schaft", "hol\u00b7de", "W\u00e4r\u00b7me", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00e4ngt auf, und welche Bardenbrust", "tokens": ["Dr\u00e4ngt", "auf", ",", "und", "wel\u00b7che", "Bar\u00b7den\u00b7brust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstickt ein solches Lied?", "tokens": ["Er\u00b7stickt", "ein", "sol\u00b7ches", "Lied", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht meine, Freund! denn Freundschaft ist", "tokens": ["Nicht", "mei\u00b7ne", ",", "Freund", "!", "denn", "Freund\u00b7schaft", "ist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "$,", "NN", "$.", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Auge bunte Wiesenpracht,", "tokens": ["Im", "Au\u00b7ge", "bun\u00b7te", "Wie\u00b7sen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Ohre Nachtigallenschall,", "tokens": ["Im", "Oh\u00b7re", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gaumen Honig mir.", "tokens": ["Im", "Gau\u00b7men", "Ho\u00b7nig", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und sang ich Den, der oben herrscht,", "tokens": ["Und", "sang", "ich", "Den", ",", "der", "o\u00b7ben", "herrscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Joseph und Theresien,", "tokens": ["Und", "Jo\u00b7se\u00b7ph", "und", "The\u00b7re\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich, mein deutsches Vaterland!", "tokens": ["Und", "dich", ",", "mein", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wen s\u00e4ng' ich Barde nun?", "tokens": ["Wen", "s\u00e4ng'", "ich", "Bar\u00b7de", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als euch, Bewohner meiner Brust!", "tokens": ["Als", "euch", ",", "Be\u00b7woh\u00b7ner", "mei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unter ihnen, Wei\u00dfe! dich,", "tokens": ["Und", "un\u00b7ter", "ih\u00b7nen", ",", "Wei\u00b7\u00dfe", "!", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "NN", "$.", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dem der edlen Pleisse Trost", "tokens": ["In", "dem", "der", "ed\u00b7len", "Pleis\u00b7se", "Trost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit Gellerts Falle gr\u00fcnt.", "tokens": ["Seit", "Gel\u00b7lerts", "Fal\u00b7le", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du gl\u00e4nzest in den Hunderten", "tokens": ["Du", "gl\u00e4n\u00b7zest", "in", "den", "Hun\u00b7der\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Barden Manas", "tokens": ["Der", "Bar\u00b7den", "Ma\u00b7nas"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Abendf\u00fchrer, der die Glut", "tokens": ["Der", "A\u00b7bend\u00b7f\u00fch\u00b7rer", ",", "der", "die", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sommermonden d\u00e4mpft.", "tokens": ["Der", "Som\u00b7mer\u00b7mon\u00b7den", "d\u00e4mpft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Druiden locket er hervor", "tokens": ["Dru\u00b7i\u00b7den", "lo\u00b7cket", "er", "her\u00b7vor"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihrer H\u00f6hle, freudig sieht", "tokens": ["Aus", "ih\u00b7rer", "H\u00f6h\u00b7le", ",", "freu\u00b7dig", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der braune Sohn der Arbeit ihn,", "tokens": ["Der", "brau\u00b7ne", "Sohn", "der", "Ar\u00b7beit", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trocknet seinen Schwei\u00df.", "tokens": ["Und", "trock\u00b7net", "sei\u00b7nen", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auf Knabenspiele blinket er", "tokens": ["Auf", "Kna\u00b7ben\u00b7spie\u00b7le", "blin\u00b7ket", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von seiner Bahn ins Gr\u00fcn' herab,", "tokens": ["Von", "sei\u00b7ner", "Bahn", "ins", "Gr\u00fcn'", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und M\u00e4dchen ruft in's Thaugefild'", "tokens": ["Und", "M\u00e4d\u00b7chen", "ruft", "in's", "Thau\u00b7ge\u00b7fild'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Mitgef\u00e4hrt' der West.", "tokens": ["Sein", "Mit\u00b7ge\u00b7f\u00e4hrt'", "der", "West", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So ward die sanfte Liederkraft", "tokens": ["So", "ward", "die", "sanf\u00b7te", "Lie\u00b7der\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr jedes Alter und Geschlecht;", "tokens": ["F\u00fcr", "je\u00b7des", "Al\u00b7ter", "und", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wohnet Lust f\u00fcr jedes Ohr", "tokens": ["So", "woh\u00b7net", "Lust", "f\u00fcr", "je\u00b7des", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner Harfe, Freund", "tokens": ["In", "dei\u00b7ner", "Har\u00b7fe", ",", "Freund"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auch Lust der Thr\u00e4nen gibst uns du,", "tokens": ["Auch", "Lust", "der", "Thr\u00e4\u00b7nen", "gibst", "uns", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich zur Feier deines Volks", "tokens": ["Wenn", "sich", "zur", "Fei\u00b7er", "dei\u00b7nes", "Volks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herrscher Fall, der Helden Weh", "tokens": ["Der", "Herr\u00b7scher", "Fall", ",", "der", "Hel\u00b7den", "Weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ernster B\u00fchne zeigt.", "tokens": ["Von", "erns\u00b7ter", "B\u00fch\u00b7ne", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auch heitre Scherze giebst uns du,", "tokens": ["Auch", "heit\u00b7re", "Scher\u00b7ze", "giebst", "uns", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich zur Feier deines Volks", "tokens": ["Wenn", "sich", "zur", "Fei\u00b7er", "dei\u00b7nes", "Volks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Thoren l\u00e4cherliches Bild", "tokens": ["Der", "Tho\u00b7ren", "l\u00e4\u00b7cher\u00b7li\u00b7ches", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von munt'rer B\u00fchne zeigt.", "tokens": ["Von", "munt'\u00b7rer", "B\u00fch\u00b7ne", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die tief umstand'ne B\u00fchne schallt", "tokens": ["Die", "tief", "um\u00b7stan\u00b7d'\u00b7ne", "B\u00fch\u00b7ne", "schallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Lob, und mancher Feirer ruft:", "tokens": ["Dein", "Lob", ",", "und", "man\u00b7cher", "Fei\u00b7rer", "ruft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbhab't ihr denn B\u00fchnen nur allein?", "tokens": ["\u00bb", "hab't", "ihr", "denn", "B\u00fch\u00b7nen", "nur", "al\u00b7lein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Fremden! ihr allein?\u00ab", "tokens": ["Ihr", "Frem\u00b7den", "!", "ihr", "al\u00b7lein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er ruft es, und du wendest dich,", "tokens": ["Er", "ruft", "es", ",", "und", "du", "wen\u00b7dest", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und horchest deinem Lobe nicht,", "tokens": ["Und", "hor\u00b7chest", "dei\u00b7nem", "Lo\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deinen Werth, den alle seh'n,", "tokens": ["Und", "dei\u00b7nen", "Werth", ",", "den", "al\u00b7le", "seh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den siehst du, Barde! nicht.", "tokens": ["Den", "siehst", "du", ",", "Bar\u00b7de", "!", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$.", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Du siehst nur deiner Br\u00fcder Werth,", "tokens": ["Du", "siehst", "nur", "dei\u00b7ner", "Br\u00fc\u00b7der", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00f6nnest jedem seinen Kranz,", "tokens": ["Und", "g\u00f6n\u00b7nest", "je\u00b7dem", "sei\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und preisest sie den Kindern Teut's", "tokens": ["Und", "prei\u00b7sest", "sie", "den", "Kin\u00b7dern", "Teut's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus vollem Herzen an", "tokens": ["Aus", "vol\u00b7lem", "Her\u00b7zen", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und glimmet unter Br\u00fcdern Zwist,", "tokens": ["Und", "glim\u00b7met", "un\u00b7ter", "Br\u00fc\u00b7dern", "Zwist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann stehst du mitten. Niemal haucht", "tokens": ["Dann", "stehst", "du", "mit\u00b7ten", ".", "Nie\u00b7mal", "haucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Odem 