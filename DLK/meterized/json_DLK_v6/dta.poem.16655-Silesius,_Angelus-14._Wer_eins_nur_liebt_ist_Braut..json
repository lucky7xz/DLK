{"dta.poem.16655": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "14.  Wer eins nur liebt ist Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele/ die nichts wei\u00df/ nichts wil/ nichts liebt/ danns", "tokens": ["Die", "See\u00b7le", "/", "die", "nichts", "wei\u00df", "/", "nichts", "wil", "/", "nichts", "liebt", "/", "danns"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$(", "ART", "PIS", "VVFIN", "$(", "PIS", "VMFIN", "$(", "PIS", "VVFIN", "$(", "NE"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein/", "tokens": ["Ein", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mu\u00df heute noch die Braut de\u00df Ewgen Br\u00e4utgams seyn", "tokens": ["Mu\u00df", "heu\u00b7te", "noch", "die", "Braut", "de\u00df", "Ew\u00b7gen", "Br\u00e4ut\u00b7gams", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}