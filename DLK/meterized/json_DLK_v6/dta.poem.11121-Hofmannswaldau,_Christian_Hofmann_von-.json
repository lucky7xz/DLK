{"dta.poem.11121": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagest immerfort, da\u00df dich die sterne qu\u00e4len,", "tokens": ["Du", "kla\u00b7gest", "im\u00b7mer\u00b7fort", ",", "da\u00df", "dich", "die", "ster\u00b7ne", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df die sonne dir nur ein comete sey.", "tokens": ["Und", "da\u00df", "die", "son\u00b7ne", "dir", "nur", "ein", "co\u00b7me\u00b7te", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, mein freund! mich deucht, du giebst dich gar zu frey:", "tokens": ["Al\u00b7lein", ",", "mein", "freund", "!", "mich", "deucht", ",", "du", "giebst", "dich", "gar", "zu", "frey", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der rechte stern wird dir in deiner stirne fehlen.", "tokens": ["Der", "rech\u00b7te", "stern", "wird", "dir", "in", "dei\u00b7ner", "stir\u00b7ne", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word