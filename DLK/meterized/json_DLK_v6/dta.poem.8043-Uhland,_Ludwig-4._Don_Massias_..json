{"dta.poem.8043": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4.  Don Massias .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.85", "it:0.14"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Don Massias aus Gallizien", "tokens": ["Don", "Mas\u00b7si\u00b7as", "aus", "Gal\u00b7li\u00b7zi\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem Namen: ", "tokens": ["Mit", "dem", "Na\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sa\u00df im Thurm zu Arjonilla,", "tokens": ["Sa\u00df", "im", "Thurm", "zu", "Ar\u00b7jo\u00b7nil\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Klagend um die Treugeliebte.", "tokens": ["Kla\u00b7gend", "um", "die", "Treu\u00b7ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Grafen, reich und m\u00e4chtig,", "tokens": ["Ei\u00b7nen", "Gra\u00b7fen", ",", "reich", "und", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gab man j\u00fcngst ihr zum Genossen,", "tokens": ["Gab", "man", "j\u00fcngst", "ihr", "zum", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und den vielgetreuen S\u00e4nger", "tokens": ["Und", "den", "viel\u00b7ge\u00b7treu\u00b7en", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4lt man ferngebannt, verschlossen.", "tokens": ["H\u00e4lt", "man", "fern\u00b7ge\u00b7bannt", ",", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Traurig sang er oft am Gitter,", "tokens": ["Trau\u00b7rig", "sang", "er", "oft", "am", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Machte jeden Wandrer lauschen,", "tokens": ["Mach\u00b7te", "je\u00b7den", "Wand\u00b7rer", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Theure Bl\u00e4tter, liederreiche,", "tokens": ["Theu\u00b7re", "Bl\u00e4t\u00b7ter", ",", "lie\u00b7der\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Lie\u00df er oft vom Fenster rauschen.", "tokens": ["Lie\u00df", "er", "oft", "vom", "Fens\u00b7ter", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ob es Wandrer fortgesungen,", "tokens": ["Ob", "es", "Wand\u00b7rer", "fort\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ob es Winde hingetragen:", "tokens": ["Ob", "es", "Win\u00b7de", "hin\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wohl vernahm die Hei\u00dfgeliebte", "tokens": ["Wohl", "ver\u00b7nahm", "die", "Hei\u00df\u00b7ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ihres treuen S\u00e4ngers Klagen.", "tokens": ["Ih\u00b7res", "treu\u00b7en", "S\u00e4n\u00b7gers", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ihr Gemahl, argw\u00f6hnisch sp\u00e4hend,", "tokens": ["Ihr", "Ge\u00b7mahl", ",", "arg\u00b7w\u00f6h\u00b7nisch", "sp\u00e4\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hatt\u2019 es alles gut beachtet:", "tokens": ["Hatt'", "es", "al\u00b7les", "gut", "be\u00b7ach\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "\u201emu\u00df ich vor dem S\u00e4nger beben", "tokens": ["\u201e", "mu\u00df", "ich", "vor", "dem", "S\u00e4n\u00b7ger", "be\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Selbst wann er im Kerker schmachtet?\u201c", "tokens": ["Selbst", "wann", "er", "im", "Ker\u00b7ker", "schmach\u00b7tet", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Einsmals schwang er sich zu Pferde,", "tokens": ["Eins\u00b7mals", "schwang", "er", "sich", "zu", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wohlgewaffnet, wie zum Sturme,", "tokens": ["Wohl\u00b7ge\u00b7waff\u00b7net", ",", "wie", "zum", "Stur\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Sprengte nach Granada\u2019s Grenze", "tokens": ["Spreng\u00b7te", "nach", "Gra\u00b7na\u00b7da's", "Gren\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.24": {"text": "Und zu Arjonilla\u2019s Thurme.", "tokens": ["Und", "zu", "Ar\u00b7jo\u00b7nil\u00b7la's", "Thur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Don Massias, der Verliebte,", "tokens": ["Don", "Mas\u00b7si\u00b7as", ",", "der", "Ver\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Stand gerade dort am Gitter,", "tokens": ["Stand", "ge\u00b7ra\u00b7de", "dort", "am", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sang so gl\u00fchend seine Liebe,", "tokens": ["Sang", "so", "gl\u00fc\u00b7hend", "sei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Schlug so zierlich seine Zither.", "tokens": ["Schlug", "so", "zier\u00b7lich", "sei\u00b7ne", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Jener hub sich in den B\u00fcgeln,", "tokens": ["Je\u00b7ner", "hub", "sich", "in", "den", "B\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Wuthvoll seine Lanze schwingend;", "tokens": ["Wuth\u00b7voll", "sei\u00b7ne", "Lan\u00b7ze", "schwin\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Don Massias ist durchbohret,", "tokens": ["Don", "Mas\u00b7si\u00b7as", "ist", "durch\u00b7boh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Wie ein Schwan verschied er singend.", "tokens": ["Wie", "ein", "Schwan", "ver\u00b7schied", "er", "sin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und der Graf, des Siegs versichert,", "tokens": ["Und", "der", "Graf", ",", "des", "Siegs", "ver\u00b7si\u00b7chert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.34": {"text": "Kehret nach Gallizien wieder.", "tokens": ["Keh\u00b7ret", "nach", "Gal\u00b7li\u00b7zi\u00b7en", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADV", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Eitler Wahn! es starb der S\u00e4nger,", "tokens": ["Eit\u00b7ler", "Wahn", "!", "es", "starb", "der", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Doch es leben seine Lieder;", "tokens": ["Doch", "es", "le\u00b7ben", "sei\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Die durch alle span\u2019schen Reiche", "tokens": ["Die", "durch", "al\u00b7le", "span'\u00b7schen", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "T\u00f6nevoll, gefl\u00fcgelt, ziehen,", "tokens": ["T\u00f6\u00b7ne\u00b7voll", ",", "ge\u00b7fl\u00fc\u00b7gelt", ",", "zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Andern sind sie Philomelen,", "tokens": ["An\u00b7dern", "sind", "sie", "Phi\u00b7lo\u00b7me\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Jenem nur sind sie Harpyjen.", "tokens": ["Je\u00b7nem", "nur", "sind", "sie", "Har\u00b7py\u00b7jen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "VAFIN", "PPER", "NE", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.41": {"text": "Pl\u00f6tzlich oft vom Freudenmahle", "tokens": ["Pl\u00f6tz\u00b7lich", "oft", "vom", "Freu\u00b7den\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Haben sie ihn aufgeschrecket,", "tokens": ["Ha\u00b7ben", "sie", "ihn", "auf\u00b7ge\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Aus dem mittern\u00e4cht\u2019gen Schlummer", "tokens": ["Aus", "dem", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7gen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Wird er peinlich oft erwecket:", "tokens": ["Wird", "er", "pein\u00b7lich", "oft", "er\u00b7we\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "In den G\u00e4rten, in den Stra\u00dfen", "tokens": ["In", "den", "G\u00e4r\u00b7ten", ",", "in", "den", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "H\u00f6rt er Zithern hin und wieder,", "tokens": ["H\u00f6rt", "er", "Zi\u00b7thern", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Und wie Geisterstimmen t\u00f6nen", "tokens": ["Und", "wie", "Geis\u00b7ters\u00b7tim\u00b7men", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Des Massias Liebeslieder.", "tokens": ["Des", "Mas\u00b7si\u00b7as", "Lie\u00b7bes\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}