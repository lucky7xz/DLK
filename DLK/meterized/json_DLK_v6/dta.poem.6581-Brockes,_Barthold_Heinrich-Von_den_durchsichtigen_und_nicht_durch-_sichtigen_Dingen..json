{"dta.poem.6581": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wird demnach das Licht, das \u00fcber C\u00f6rper f\u00e4llt,", "tokens": ["So", "wird", "dem\u00b7nach", "das", "Licht", ",", "das", "\u00fc\u00b7ber", "C\u00f6r\u00b7per", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch solche C\u00f6rper selbst in Farben ausgebreitet.", "tokens": ["Durch", "sol\u00b7che", "C\u00f6r\u00b7per", "selbst", "in", "Far\u00b7ben", "aus\u00b7ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch sieht man C\u00f6rper auch, die dergestalt bereitet,", "tokens": ["Doch", "sieht", "man", "C\u00f6r\u00b7per", "auch", ",", "die", "der\u00b7ge\u00b7stalt", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NE", "ADV", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wir in ihrer Form und Zubereitung finden,", "tokens": ["Da\u00df", "wir", "in", "ih\u00b7rer", "Form", "und", "Zu\u00b7be\u00b7rei\u00b7tung", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sie sich mit dem Glantz des Lichts weit mehr verbinden.", "tokens": ["Wie", "sie", "sich", "mit", "dem", "Glantz", "des", "Lichts", "weit", "mehr", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}