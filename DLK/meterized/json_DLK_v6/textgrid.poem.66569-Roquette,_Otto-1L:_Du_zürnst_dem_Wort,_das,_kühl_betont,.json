{"textgrid.poem.66569": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du z\u00fcrnst dem Wort, das, k\u00fchl betont,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du z\u00fcrnst dem Wort, das, k\u00fchl betont,", "tokens": ["Du", "z\u00fcrnst", "dem", "Wort", ",", "das", ",", "k\u00fchl", "be\u00b7tont", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PDS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Undank dich getroffen,", "tokens": ["Wie", "Un\u00b7dank", "dich", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fchlst mit Bitterkeit belohnt", "tokens": ["Und", "f\u00fchlst", "mit", "Bit\u00b7ter\u00b7keit", "be\u00b7lohnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Geben und dein Hoffen.", "tokens": ["Dein", "Ge\u00b7ben", "und", "dein", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Befrag' dich selbst, und halt' in Ruh", "tokens": ["Be\u00b7frag'", "dich", "selbst", ",", "und", "halt'", "in", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Vorwurfs Pfeil im K\u00f6cher,", "tokens": ["Des", "Vor\u00b7wurfs", "Pfeil", "im", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob bittre Tropfen nicht auch du", "tokens": ["Ob", "bitt\u00b7re", "Trop\u00b7fen", "nicht", "auch", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gemischt in fremden Becher!", "tokens": ["Ge\u00b7mischt", "in", "frem\u00b7den", "Be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}