{"textgrid.poem.66507": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie im kreise wir uns finden,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie im kreise wir uns finden,", "tokens": ["Wie", "im", "krei\u00b7se", "wir", "uns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freunde, hier zu heitrer Stund',", "tokens": ["Freun\u00b7de", ",", "hier", "zu", "hei\u00b7trer", "Stund'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll die Muse uns verbinden,", "tokens": ["Soll", "die", "Mu\u00b7se", "uns", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht aus tiefstem Herzensgrund!", "tokens": ["Recht", "aus", "tiefs\u00b7tem", "Her\u00b7zens\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was vereint aus allen Weiten", "tokens": ["Was", "ver\u00b7eint", "aus", "al\u00b7len", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat ein g\u00fcnstiges Geschick,", "tokens": ["Hat", "ein", "g\u00fcns\u00b7ti\u00b7ges", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Halte fest f\u00fcr alle Zeiten", "tokens": ["Hal\u00b7te", "fest", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Froh bewegt den Augenblick!", "tokens": ["Froh", "be\u00b7wegt", "den", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat am Tageswerk gebauet", "tokens": ["Hat", "am", "Ta\u00b7ges\u00b7werk", "ge\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder in des Schaffens Drang,", "tokens": ["Je\u00b7der", "in", "des", "Schaf\u00b7fens", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6ne, wenn die Nacht uns schauet,", "tokens": ["T\u00f6\u00b7ne", ",", "wenn", "die", "Nacht", "uns", "schau\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell vereinter Freudensang!", "tokens": ["Hell", "ver\u00b7ein\u00b7ter", "Freu\u00b7den\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was als strebende Genossen", "tokens": ["Was", "als", "stre\u00b7ben\u00b7de", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "KOUS", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Uns vereint in guter Stund,", "tokens": ["Uns", "ver\u00b7eint", "in", "gu\u00b7ter", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die Stunde l\u00e4ngst verflossen", "tokens": ["Wenn", "die", "Stun\u00b7de", "l\u00e4ngst", "ver\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mach' es uns Erinnrung kund!", "tokens": ["Mach'", "es", "uns", "E\u00b7rinn\u00b7rung", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieder sind die goldne Kette,", "tokens": ["Lie\u00b7der", "sind", "die", "gold\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die unl\u00f6sbar uns umschlingt,", "tokens": ["Die", "un\u00b7l\u00f6s\u00b7bar", "uns", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und auch zu der fernsten St\u00e4tte", "tokens": ["Und", "auch", "zu", "der", "ferns\u00b7ten", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Freundesgru\u00df zum Freunde bringt.", "tokens": ["Freun\u00b7des\u00b7gru\u00df", "zum", "Freun\u00b7de", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob des Lebens bunt Getriebe", "tokens": ["Ob", "des", "Le\u00b7bens", "bunt", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleudert uns nach S\u00fcd und Nord,", "tokens": ["Schleu\u00b7dert", "uns", "nach", "S\u00fcd", "und", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Findet doch die alte Liebe", "tokens": ["Fin\u00b7det", "doch", "die", "al\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Immer auch den alten Ort.", "tokens": ["Im\u00b7mer", "auch", "den", "al\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}