{"textgrid.poem.25384": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: War man gl\u00fccklich eingestaubten B\u00e4nken,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War man gl\u00fccklich eingestaubten B\u00e4nken,", "tokens": ["War", "man", "gl\u00fcck\u00b7lich", "ein\u00b7ge\u00b7staub\u00b7ten", "B\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lehrerquengeln und den Zeichen an der Tafel,", "tokens": ["Leh\u00b7rer\u00b7quen\u00b7geln", "und", "den", "Zei\u00b7chen", "an", "der", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "die man nicht verstand, entzogen,", "tokens": ["die", "man", "nicht", "ver\u00b7stand", ",", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abends im Theater, auf die Br\u00fcstung hingebogen,", "tokens": ["A\u00b7bends", "im", "The\u00b7a\u00b7ter", ",", "auf", "die", "Br\u00fcs\u00b7tung", "hin\u00b7ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "F\u00fchlte man sich Himmel k\u00f6stlich niedersenken.", "tokens": ["F\u00fchl\u00b7te", "man", "sich", "Him\u00b7mel", "k\u00f6st\u00b7lich", "nie\u00b7der\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur im Spiele wollte Gl\u00fcck sich geben,", "tokens": ["Nur", "im", "Spie\u00b7le", "woll\u00b7te", "Gl\u00fcck", "sich", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo sich Traum ein ungeheures Sein erfand,", "tokens": ["Wo", "sich", "Traum", "ein", "un\u00b7ge\u00b7heu\u00b7res", "Sein", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und den H\u00e4nden, die zum ersten Mal nach Leben", "tokens": ["Und", "den", "H\u00e4n\u00b7den", ",", "die", "zum", "ers\u00b7ten", "Mal", "nach", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Griffen, rollte Wirklichkeit dahin wie loser Sand.", "tokens": ["Grif\u00b7fen", ",", "roll\u00b7te", "Wirk\u00b7lich\u00b7keit", "da\u00b7hin", "wie", "lo\u00b7ser", "Sand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PAV", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Aber wenn du vor den B\u00fchnenlichtern schrittest,", "tokens": ["A\u00b7ber", "wenn", "du", "vor", "den", "B\u00fch\u00b7nen\u00b7lich\u00b7tern", "schrit\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "L\u00e4cheltest und eingelernte Worte sprachst,", "tokens": ["L\u00e4\u00b7chel\u00b7test", "und", "ein\u00b7ge\u00b7lern\u00b7te", "Wor\u00b7te", "sprachst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "war Wunder aufgehellt,", "tokens": ["war", "Wun\u00b7der", "auf\u00b7ge\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Musik und Beifall und geputzter Menge glittest", "tokens": ["Mit", "Mu\u00b7sik", "und", "Bei\u00b7fall", "und", "ge\u00b7putz\u00b7ter", "Men\u00b7ge", "glit\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du ins Herz, warst Weib und Ruhm und Welt.", "tokens": ["Du", "ins", "Herz", ",", "warst", "Weib", "und", "Ruhm", "und", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Herrlich lag beisammen,", "tokens": ["Herr\u00b7lich", "lag", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "was sich dann zerst\u00fcckte,", "tokens": ["was", "sich", "dann", "zer\u00b7st\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In beseelte Stummheit", "tokens": ["In", "be\u00b7seel\u00b7te", "Stumm\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "waren tausend Liebesworte eingedr\u00e4ngt,", "tokens": ["wa\u00b7ren", "tau\u00b7send", "Lie\u00b7bes\u00b7wor\u00b7te", "ein\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wenn man Abends scheu und hei\u00df", "tokens": ["Wenn", "man", "A\u00b7bends", "scheu", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "an deinen Fenstern sich vor\u00fcberdr\u00fcckte,", "tokens": ["an", "dei\u00b7nen", "Fens\u00b7tern", "sich", "vor\u00b7\u00fc\u00b7berd\u00b7r\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "War Erf\u00fcllung schimmernd", "tokens": ["War", "Er\u00b7f\u00fcl\u00b7lung", "schim\u00b7mernd"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "wie ein Rosenregen ausgesprengt.", "tokens": ["wie", "ein", "Ro\u00b7sen\u00b7re\u00b7gen", "aus\u00b7ge\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}