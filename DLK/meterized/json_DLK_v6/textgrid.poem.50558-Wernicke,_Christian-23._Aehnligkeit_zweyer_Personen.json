{"textgrid.poem.50558": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "23. Aehnligkeit zweyer Personen", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und ihm gesaget ward, es finde sich alldar", "tokens": ["Und", "ihm", "ge\u00b7sa\u00b7get", "ward", ",", "es", "fin\u00b7de", "sich", "all\u00b7dar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$,", "PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Mann, den seine Freund' offt ", "tokens": ["Ein", "Mann", ",", "den", "sei\u00b7ne", "Freund'", "offt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So liess' er ihn so gleich nach seinem Pallast kommen.", "tokens": ["So", "liess'", "er", "ihn", "so", "gleich", "nach", "sei\u00b7nem", "Pal\u00b7last", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er sah' ihn, und befand wahrhafftig den Bericht:", "tokens": ["Er", "sah'", "ihn", ",", "und", "be\u00b7fand", "wahr\u00b7haff\u00b7tig", "den", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dass an ", "tokens": ["Und", "dass", "an"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Mein Herr, antwortete der Tropf einf\u00e4ltig, Nein;", "tokens": ["Mein", "Herr", ",", "ant\u00b7wor\u00b7te\u00b7te", "der", "Tropf", "ein\u00b7f\u00e4l\u00b7tig", ",", "Nein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}