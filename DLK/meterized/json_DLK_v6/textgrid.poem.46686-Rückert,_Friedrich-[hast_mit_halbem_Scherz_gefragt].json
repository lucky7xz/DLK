{"textgrid.poem.46686": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hast mit halbem Scherz gefragt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast mit halbem Scherz gefragt", "tokens": ["Hast", "mit", "hal\u00b7bem", "Scherz", "ge\u00b7fragt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit halbem Stolz: Gebar", "tokens": ["Und", "mit", "hal\u00b7bem", "Stolz", ":", "Ge\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich dir nicht ein sch\u00f6nes Paar?", "tokens": ["Ich", "dir", "nicht", "ein", "sch\u00f6\u00b7nes", "Paar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemals hab' ich Ja gesagt,", "tokens": ["Nie\u00b7mals", "hab'", "ich", "Ja", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dacht' ein Ungl\u00fcck k\u00f6nnt' es geben,", "tokens": ["Dacht'", "ein", "Un\u00b7gl\u00fcck", "k\u00f6nnt'", "es", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn nichts Sch\u00f6nes bleibt am Leben.", "tokens": ["Denn", "nichts", "Sch\u00f6\u00b7nes", "bleibt", "am", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun hat's weiter nicht Gefahr,", "tokens": ["Nun", "hat's", "wei\u00b7ter", "nicht", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich will dir Antwort geben:", "tokens": ["Und", "ich", "will", "dir", "Ant\u00b7wort", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja f\u00fcrwahr,", "tokens": ["Ja", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Du gebarst ein sch\u00f6nes Paar,", "tokens": ["Du", "ge\u00b7barst", "ein", "sch\u00f6\u00b7nes", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das zu sch\u00f6n der Erde war.", "tokens": ["Das", "zu", "sch\u00f6n", "der", "Er\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKA", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}