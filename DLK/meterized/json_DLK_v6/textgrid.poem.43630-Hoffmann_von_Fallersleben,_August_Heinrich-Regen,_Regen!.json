{"textgrid.poem.43630": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Regen, Regen!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regen, Regen,", "tokens": ["Re\u00b7gen", ",", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Himmelssegen!", "tokens": ["Him\u00b7mels\u00b7se\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bring' uns K\u00fchle, l\u00f6sch' den Staub", "tokens": ["Bring'", "uns", "K\u00fch\u00b7le", ",", "l\u00f6sch'", "den", "Staub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erquicke Halm und Laub!", "tokens": ["Und", "er\u00b7qui\u00b7cke", "Halm", "und", "Laub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Regen, Regen,", "tokens": ["Re\u00b7gen", ",", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Himmelssegen!", "tokens": ["Him\u00b7mels\u00b7se\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Labe meine Bl\u00fcmelein,", "tokens": ["La\u00b7be", "mei\u00b7ne", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df sie bl\u00fch'n im Sonnenschein!", "tokens": ["La\u00df", "sie", "bl\u00fch'n", "im", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Regen, Regen,", "tokens": ["Re\u00b7gen", ",", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Himmelssegen!", "tokens": ["Him\u00b7mels\u00b7se\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nimm dich auch des B\u00e4chleins an,", "tokens": ["Nimm", "dich", "auch", "des", "B\u00e4ch\u00b7leins", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es wieder rauschen kann!", "tokens": ["Da\u00df", "es", "wie\u00b7der", "rau\u00b7schen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}