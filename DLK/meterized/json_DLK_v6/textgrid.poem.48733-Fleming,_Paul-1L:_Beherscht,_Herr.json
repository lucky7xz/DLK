{"textgrid.poem.48733": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beherscht, Herr ", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beherscht, Herr ", "tokens": ["Be\u00b7herscht", ",", "Herr"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "und doch Behersch'rin auch, f\u00fcgt sich in euren Sinn,", "tokens": ["und", "doch", "Be\u00b7her\u00b7sch'\u00b7rin", "auch", ",", "f\u00fcgt", "sich", "in", "eu\u00b7ren", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ADV", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "wie in das Bett' und Arm, und l\u00e4\u00dft sich in euch h\u00fcllen.", "tokens": ["wie", "in", "das", "Bett'", "und", "Arm", ",", "und", "l\u00e4\u00dft", "sich", "in", "euch", "h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "KON", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So k\u00f6nnt ihr Will' und Mut, so Mut und Willen stillen,", "tokens": ["So", "k\u00f6nnt", "ihr", "Will'", "und", "Mut", ",", "so", "Mut", "und", "Wil\u00b7len", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn eure ", "tokens": ["wenn", "eu\u00b7re"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wi\u00dft, spricht sie, da\u00df auch ich, was ihr wollt, Willens bin,", "tokens": ["Wi\u00dft", ",", "spricht", "sie", ",", "da\u00df", "auch", "ich", ",", "was", "ihr", "wollt", ",", "Wil\u00b7lens", "bin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mein Will' ist euer Mut, den will ich stets erf\u00fcllen.", "tokens": ["mein", "Will'", "ist", "eu\u00b7er", "Mut", ",", "den", "will", "ich", "stets", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "nie keinen Unmut an. Und macht er sich was k\u00fchner,", "tokens": ["nie", "kei\u00b7nen", "Un\u00b7mut", "an", ".", "Und", "macht", "er", "sich", "was", "k\u00fch\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "PRF", "PWS", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als wol ein Diener soll, es sei Tag oder Nacht,", "tokens": ["als", "wol", "ein", "Die\u00b7ner", "soll", ",", "es", "sei", "Tag", "o\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "$,", "PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "so brechet euren Mut und folget seinem Wollen.", "tokens": ["so", "bre\u00b7chet", "eu\u00b7ren", "Mut", "und", "fol\u00b7get", "sei\u00b7nem", "Wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr wi\u00dft, bediente Frau, da\u00df M\u00e4nner herschen sollen,", "tokens": ["Ihr", "wi\u00dft", ",", "be\u00b7dien\u00b7te", "Frau", ",", "da\u00df", "M\u00e4n\u00b7ner", "her\u00b7schen", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und h\u00e4tt' ihr Name sie zu Knechten gleich gemacht.", "tokens": ["und", "h\u00e4tt'", "ihr", "Na\u00b7me", "sie", "zu", "Knech\u00b7ten", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}