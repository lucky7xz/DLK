{"dta.poem.6742": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Unrecht wil man uns die Bilder, die wir sp\u00fcren,", "tokens": ["Mit", "Un\u00b7recht", "wil", "man", "uns", "die", "Bil\u00b7der", ",", "die", "wir", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "PRF", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus eitlem Larven-Werck, ans Gattungen formiren,", "tokens": ["Aus", "eit\u00b7lem", "La\u00b7rven\u00b7\u00b7Werck", ",", "ans", "Gat\u00b7tun\u00b7gen", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die gantz unm\u00f6glich sind. Genung ist, da\u00df das Licht", "tokens": ["Die", "gantz", "un\u00b7m\u00f6g\u00b7lich", "sind", ".", "Ge\u00b7nung", "ist", ",", "da\u00df", "das", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$.", "NN", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit unterschiedenen Beweglichkeiten,", "tokens": ["Mit", "un\u00b7ter\u00b7schie\u00b7de\u00b7nen", "Be\u00b7weg\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die uns verschiedenes Empfinden zubereiten,", "tokens": ["Die", "uns", "ver\u00b7schie\u00b7de\u00b7nes", "Emp\u00b7fin\u00b7den", "zu\u00b7be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In unsers Auges Grund mit stetem Wechsel bricht.", "tokens": ["In", "un\u00b7sers", "Au\u00b7ges", "Grund", "mit", "ste\u00b7tem", "Wech\u00b7sel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es drucken sich mit diesem Schein,", "tokens": ["Es", "dru\u00b7cken", "sich", "mit", "die\u00b7sem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So manche C\u00f6rper, die an allen Orten seyn,", "tokens": ["So", "man\u00b7che", "C\u00f6r\u00b7per", ",", "die", "an", "al\u00b7len", "Or\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In solchen kleinen Raum uns ihre Bilder ein.", "tokens": ["In", "sol\u00b7chen", "klei\u00b7nen", "Raum", "uns", "ih\u00b7re", "Bil\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Je n\u00e4her nun solch Bildni\u00df uns ber\u00fchret;", "tokens": ["Je", "n\u00e4\u00b7her", "nun", "solch", "Bild\u00b7ni\u00df", "uns", "be\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Je gr\u00f6sser wird es auch von uns versp\u00fcret:", "tokens": ["Je", "gr\u00f6s\u00b7ser", "wird", "es", "auch", "von", "uns", "ver\u00b7sp\u00fc\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Je mehr wir ihre Z\u00fcg entfernet von uns sehn;", "tokens": ["Je", "mehr", "wir", "ih\u00b7re", "Z\u00fcg", "ent\u00b7fer\u00b7net", "von", "uns", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Je mehr verkleinert sich dies Mahlwerck unsern Augen:", "tokens": ["Je", "mehr", "ver\u00b7klei\u00b7nert", "sich", "dies", "Mahl\u00b7werck", "un\u00b7sern", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "PDS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Je weiter da\u00df die C\u00f6rper von uns gehn,", "tokens": ["Je", "wei\u00b7ter", "da\u00df", "die", "C\u00f6r\u00b7per", "von", "uns", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Je minder k\u00f6nnen auch die \u00e4usern Fl\u00e4chen taugen,", "tokens": ["Je", "min\u00b7der", "k\u00f6n\u00b7nen", "auch", "die", "\u00e4u\u00b7sern", "Fl\u00e4\u00b7chen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ins Auge sonst, als blo\u00df mit spitzen Ecken,", "tokens": ["Ins", "Au\u00b7ge", "sonst", ",", "als", "blo\u00df", "mit", "spit\u00b7zen", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sich zu erstrecken,", "tokens": ["Sich", "zu", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und lassen folglich allgemach", "tokens": ["Und", "las\u00b7sen", "folg\u00b7lich", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Stets mehr verwirrt und schwach.", "tokens": ["Stets", "mehr", "ver\u00b7wirrt", "und", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Durch gar zu grosse Weit\u2019 entfernet sich das Licht,", "tokens": ["Durch", "gar", "zu", "gros\u00b7se", "Weit'", "ent\u00b7fer\u00b7net", "sich", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So, da\u00df die Kleinheit dann wird als ein Punct gesp\u00fcret,", "tokens": ["So", ",", "da\u00df", "die", "Klein\u00b7heit", "dann", "wird", "als", "ein", "Punct", "ge\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADV", "VAFIN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Die sich sodann verlieret,", "tokens": ["Die", "sich", "so\u00b7dann", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und unser Auge sieht sie nicht.", "tokens": ["Und", "un\u00b7ser", "Au\u00b7ge", "sieht", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}