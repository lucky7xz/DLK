{"textgrid.poem.58646": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00df dich nicht auf \u00e4u\u00dfre Ding', auf eignen Witz und Werke,", "tokens": ["Ver\u00b7la\u00df", "dich", "nicht", "auf", "\u00e4u\u00df\u00b7re", "Ding'", ",", "auf", "eig\u00b7nen", "Witz", "und", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Auf guten Vorsatz, Licht und Trost, nur Gott sei deine St\u00e4rke;", "tokens": ["Auf", "gu\u00b7ten", "Vor\u00b7satz", ",", "Licht", "und", "Trost", ",", "nur", "Gott", "sei", "dei\u00b7ne", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Auf Gott vertrau mit ganzer Macht im Glauben blo\u00df, allein,", "tokens": ["Auf", "Gott", "ver\u00b7trau", "mit", "gan\u00b7zer", "Macht", "im", "Glau\u00b7ben", "blo\u00df", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Er bleibet stehn, wenn alles f\u00e4llt, kehr nur in ihn dich ein!", "tokens": ["Er", "blei\u00b7bet", "stehn", ",", "wenn", "al\u00b7les", "f\u00e4llt", ",", "kehr", "nur", "in", "ihn", "dich", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ach, wie gesegnet ist der Mensch, der so aus allem gehet", "tokens": ["Ach", ",", "wie", "ge\u00b7seg\u00b7net", "ist", "der", "Mensch", ",", "der", "so", "aus", "al\u00b7lem", "ge\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVPP", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und wie ein Baum am Bach gepflanzt in Gott gewurzelt stehet!", "tokens": ["Und", "wie", "ein", "Baum", "am", "Bach", "ge\u00b7pflanzt", "in", "Gott", "ge\u00b7wur\u00b7zelt", "ste\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "APPR", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "In d\u00fcrrer Zeit und Leidenshitz er bleibet, wie er war,", "tokens": ["In", "d\u00fcr\u00b7rer", "Zeit", "und", "Lei\u00b7dens\u00b7hitz", "er", "blei\u00b7bet", ",", "wie", "er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Er saugt aus Gott den Lebenssaft, drum gr\u00fcnt er immerdar.", "tokens": ["Er", "saugt", "aus", "Gott", "den", "Le\u00b7bens\u00b7saft", ",", "drum", "gr\u00fcnt", "er", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}