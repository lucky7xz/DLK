{"textgrid.poem.25746": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diese Gegend sich verirren,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diese Gegend sich verirren,", "tokens": ["In", "die\u00b7se", "Ge\u00b7gend", "sich", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In welcher kaum noch Tauben girren,", "tokens": ["In", "wel\u00b7cher", "kaum", "noch", "Tau\u00b7ben", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Wollust! Ich gefalle mir,", "tokens": ["Ist", "Wol\u00b7lust", "!", "Ich", "ge\u00b7fal\u00b7le", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O du, mein lieber Freund, in ihr", "tokens": ["O", "du", ",", "mein", "lie\u00b7ber", "Freund", ",", "in", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am besten fast, vielleicht auch dir!", "tokens": ["Am", "bes\u00b7ten", "fast", ",", "viel\u00b7leicht", "auch", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "$,", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du h\u00e4ttest mich nicht finden sollen!", "tokens": ["Du", "h\u00e4t\u00b7test", "mich", "nicht", "fin\u00b7den", "sol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich h\u00e4tte tiefer mich verirrt;", "tokens": ["Ich", "h\u00e4t\u00b7te", "tie\u00b7fer", "mich", "ver\u00b7irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich h\u00e4tte, was denn? werden wollen,", "tokens": ["Ich", "h\u00e4t\u00b7te", ",", "was", "denn", "?", "wer\u00b7den", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADV", "$.", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was man so leicht im Alter wird:", "tokens": ["Was", "man", "so", "leicht", "im", "Al\u00b7ter", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADJD", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Menschenfeind! Und du, mein Lieber,", "tokens": ["Ein", "Men\u00b7schen\u00b7feind", "!", "Und", "du", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Du h\u00e4ttest oft Besuch gewagt", "tokens": ["Du", "h\u00e4t\u00b7test", "oft", "Be\u00b7such", "ge\u00b7wagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Beim neuen Timon, und dich \u00fcber", "tokens": ["Beim", "neu\u00b7en", "Ti\u00b7mon", ",", "und", "dich", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "PRF", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die b\u00f6sen Menschen auch beklagt.", "tokens": ["Die", "b\u00f6\u00b7sen", "Men\u00b7schen", "auch", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ei! h\u00f6re doch die Tauben girren,", "tokens": ["Ei", "!", "h\u00f6\u00b7re", "doch", "die", "Tau\u00b7ben", "gir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die deine liebsten Tauben sind!", "tokens": ["Die", "dei\u00b7ne", "liebs\u00b7ten", "Tau\u00b7ben", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ei! h\u00f6re doch den Silberbach,", "tokens": ["Ei", "!", "h\u00f6\u00b7re", "doch", "den", "Sil\u00b7ber\u00b7bach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der \u00fcber harte Kiesel rinnt;", "tokens": ["Der", "\u00fc\u00b7ber", "har\u00b7te", "Kie\u00b7sel", "rinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Man h\u00f6rt ihn doch!", "tokens": ["Man", "h\u00f6rt", "ihn", "doch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Ihr G\u00f6tter! ach!", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "XY", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Fern von den Menschen hier zu leben,", "tokens": ["Fern", "von", "den", "Men\u00b7schen", "hier", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "K\u00f6nnt ihr zu meiner Lebenszeit", "tokens": ["K\u00f6nnt", "ihr", "zu", "mei\u00b7ner", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Zehn tausend Tage mir noch geben!", "tokens": ["Zehn", "tau\u00b7send", "Ta\u00b7ge", "mir", "noch", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "In dieser stillen Einsamkeit", "tokens": ["In", "die\u00b7ser", "stil\u00b7len", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "D\u00fcrft' ich in keine Tiefe mich", "tokens": ["D\u00fcrft'", "ich", "in", "kei\u00b7ne", "Tie\u00b7fe", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Erniedrigen, mich nicht erheben", "tokens": ["Er\u00b7nied\u00b7ri\u00b7gen", ",", "mich", "nicht", "er\u00b7he\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Auf eine H\u00f6he, welche sich", "tokens": ["Auf", "ei\u00b7ne", "H\u00f6\u00b7he", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Feindselig k\u00f6nnte mir beweisen!", "tokens": ["Feind\u00b7se\u00b7lig", "k\u00f6nn\u00b7te", "mir", "be\u00b7wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Ach! o du Zeit! du Zeit von Eisen!", "tokens": ["Ach", "!", "o", "du", "Zeit", "!", "du", "Zeit", "von", "Ei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "FM", "PPER", "NN", "$.", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Am gl\u00fccklichsten ist der zu preisen,", "tokens": ["Am", "gl\u00fcck\u00b7lichs\u00b7ten", "ist", "der", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Der, weit von K\u00f6nigen und Weisen,", "tokens": ["Der", ",", "weit", "von", "K\u00f6\u00b7ni\u00b7gen", "und", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Sein K\u00f6nig und sein Weiser ist,", "tokens": ["Sein", "K\u00f6\u00b7nig", "und", "sein", "Wei\u00b7ser", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wie du's erst itzt, nach deinen Reisen,", "tokens": ["Wie", "du's", "erst", "itzt", ",", "nach", "dei\u00b7nen", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "In deinem Ohnesorge bist.", "tokens": ["In", "dei\u00b7nem", "Oh\u00b7nes\u00b7or\u00b7ge", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mit K\u00f6nigen ist nur zu streiten", "tokens": ["Mit", "K\u00f6\u00b7ni\u00b7gen", "ist", "nur", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.35": {"text": "Um Menschen, und um Menschenblut!", "tokens": ["Um", "Men\u00b7schen", ",", "und", "um", "Men\u00b7schen\u00b7blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und mit den Weisen unsrer Zeiten,", "tokens": ["Und", "mit", "den", "Wei\u00b7sen", "uns\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Vertr\u00e4gt man sich nicht eben gut;", "tokens": ["Ver\u00b7tr\u00e4gt", "man", "sich", "nicht", "e\u00b7ben", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Sie schelten gleich.", "tokens": ["Sie", "schel\u00b7ten", "gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Darum entfliehe,", "tokens": ["Da\u00b7rum", "ent\u00b7flie\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Du Durst nach Ehre, W\u00fcrd' und Stand!", "tokens": ["Du", "Durst", "nach", "Eh\u00b7re", ",", "W\u00fcrd'", "und", "Stand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Um euch geb' ich mir keine M\u00fche,", "tokens": ["Um", "euch", "geb'", "ich", "mir", "kei\u00b7ne", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PPER", "PPER", "PIAT", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Was meine Ruhe st\u00f6rt, ist Tand,", "tokens": ["Was", "mei\u00b7ne", "Ru\u00b7he", "st\u00f6rt", ",", "ist", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Und wenn's der Schatz des Mogols w\u00e4re!", "tokens": ["Und", "wenn's", "der", "Schatz", "des", "Mo\u00b7gols", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Was h\u00e4tt' ich von der gro\u00dfen Ehre,", "tokens": ["Was", "h\u00e4tt'", "ich", "von", "der", "gro\u00b7\u00dfen", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Der erste zu ", "tokens": ["Der", "ers\u00b7te", "zu"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "PTKZU"], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Und meine Seele w\u00e4re klein?", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "w\u00e4\u00b7re", "klein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Was h\u00e4tt' ich von der h\u00f6chsten W\u00fcrde", "tokens": ["Was", "h\u00e4tt'", "ich", "von", "der", "h\u00f6chs\u00b7ten", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Der ganzen werthen Christenheit?", "tokens": ["Der", "gan\u00b7zen", "wert\u00b7hen", "Chris\u00b7ten\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Bei der mir aufgelegten B\u00fcrde,", "tokens": ["Bei", "der", "mir", "auf\u00b7ge\u00b7leg\u00b7ten", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Zu sorgen f\u00fcr die Seligkeit", "tokens": ["Zu", "sor\u00b7gen", "f\u00fcr", "die", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Der Menschen, welche Ha\u00df und Neid", "tokens": ["Der", "Men\u00b7schen", ",", "wel\u00b7che", "Ha\u00df", "und", "Neid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Entbr\u00fcdert, h\u00e4tt' ich keine Zeit,", "tokens": ["Ent\u00b7br\u00fc\u00b7dert", ",", "h\u00e4tt'", "ich", "kei\u00b7ne", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "An mich zu denken.", "tokens": ["An", "mich", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "Welch ein Stand", "tokens": ["Welch", "ein", "Stand"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.55": {"text": "Nimmt nicht dem Stillen seine Freuden?", "tokens": ["Nimmt", "nicht", "dem", "Stil\u00b7len", "sei\u00b7ne", "Freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Wer's haben kann, der geh' aufs Land", "tokens": ["Wer's", "ha\u00b7ben", "kann", ",", "der", "geh'", "aufs", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VAINF", "VMFIN", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Und lasse da sich nicht beneiden!", "tokens": ["Und", "las\u00b7se", "da", "sich", "nicht", "be\u00b7nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Neid macht nur Feinde.", "tokens": ["Neid", "macht", "nur", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.59": {"text": "Ruhe, du", "tokens": ["Ru\u00b7he", ",", "du"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.60": {"text": "Bist meine G\u00f6ttin! Lebensm\u00fcde", "tokens": ["Bist", "mei\u00b7ne", "G\u00f6t\u00b7tin", "!", "Le\u00b7bens\u00b7m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Geh' ich auf deinen Tempel zu,", "tokens": ["Geh'", "ich", "auf", "dei\u00b7nen", "Tem\u00b7pel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Denn unter Menschen ist kein Friede!", "tokens": ["Denn", "un\u00b7ter", "Men\u00b7schen", "ist", "kein", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.63": {"text": "Nun erst? Warum nicht eher, Freund?", "tokens": ["Nun", "erst", "?", "Wa\u00b7rum", "nicht", "e\u00b7her", ",", "Freund", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PWAV", "PTKNEG", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Ach! nimm ein kl\u00e4gliches Exempel", "tokens": ["Ach", "!", "nimm", "ein", "kl\u00e4g\u00b7li\u00b7ches", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.65": {"text": "An mir, und geh in ihren Tempel", "tokens": ["An", "mir", ",", "und", "geh", "in", "ih\u00b7ren", "Tem\u00b7pel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Bei Zeiten, eh ein b\u00f6ser Feind", "tokens": ["Bei", "Zei\u00b7ten", ",", "eh", "ein", "b\u00f6\u00b7ser", "Feind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Hinein dich treibt.", "tokens": ["Hin\u00b7ein", "dich", "treibt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.68": {"text": "Die Menschen alle", "tokens": ["Die", "Men\u00b7schen", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.69": {"text": "Sind b\u00f6se Feinde, die zu Halle,", "tokens": ["Sind", "b\u00f6\u00b7se", "Fein\u00b7de", ",", "die", "zu", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PRELS", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.70": {"text": "Zu Bonn, zu Mannheim, zu Berlin,", "tokens": ["Zu", "Bonn", ",", "zu", "Mann\u00b7heim", ",", "zu", "Ber\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.71": {"text": "Zu D\u00fcsseldorf, zu Z\u00fcrch, zu Wien,", "tokens": ["Zu", "D\u00fcs\u00b7sel\u00b7dorf", ",", "zu", "Z\u00fcrch", ",", "zu", "Wi\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.72": {"text": "Und die zu Mainz in der Carthaus;", "tokens": ["Und", "die", "zu", "Mainz", "in", "der", "Car\u00b7thaus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Zwei, oder dreie nehm' ich aus.", "tokens": ["Zwei", ",", "o\u00b7der", "drei\u00b7e", "nehm'", "ich", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "KON", "CARD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}