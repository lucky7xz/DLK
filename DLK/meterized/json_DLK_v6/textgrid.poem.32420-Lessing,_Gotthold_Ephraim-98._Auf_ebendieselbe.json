{"textgrid.poem.32420": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "98. Auf ebendieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein braunes M\u00e4dchen, Freund, ist sch\u00f6n:", "tokens": ["Dein", "brau\u00b7nes", "M\u00e4d\u00b7chen", ",", "Freund", ",", "ist", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mu\u00df ihr auch der Neid gestehn.", "tokens": ["Das", "mu\u00df", "ihr", "auch", "der", "Neid", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6n, da\u00df man es ganz vergi\u00dft,", "tokens": ["So", "sch\u00f6n", ",", "da\u00df", "man", "es", "ganz", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie ein wenig buhlrisch ist;", "tokens": ["Da\u00df", "sie", "ein", "we\u00b7nig", "buhl\u00b7risch", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sch\u00f6n, da\u00df man es gar vergi\u00dft,", "tokens": ["So", "sch\u00f6n", ",", "da\u00df", "man", "es", "gar", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihr Papa ein Reimschmied ist.", "tokens": ["Da\u00df", "ihr", "Pa\u00b7pa", "ein", "Reimsc\u00b7hmied", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}