{"dta.poem.16297": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verrede nicht, zu thun, was du dir vorgenommen", "tokens": ["Ver\u00b7re\u00b7de", "nicht", ",", "zu", "thun", ",", "was", "du", "dir", "vor\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PTKZU", "VVINF", "$,", "PWS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu lassen! Uebernacht kann es dir anders kommen.", "tokens": ["Zu", "las\u00b7sen", "!", "Ue\u00b7ber\u00b7nacht", "kann", "es", "dir", "an\u00b7ders", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und auch zu lassen das verrede nicht, was du", "tokens": ["Und", "auch", "zu", "las\u00b7sen", "das", "ver\u00b7re\u00b7de", "nicht", ",", "was", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "PDS", "VVFIN", "PTKNEG", "$,", "PWS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu thun dir vorgesetzt; viel \u00e4ndert oft ein Nu.", "tokens": ["Zu", "thun", "dir", "vor\u00b7ge\u00b7setzt", ";", "viel", "\u00e4n\u00b7dert", "oft", "ein", "Nu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVPP", "$.", "ADV", "VVFIN", "ADV", "ART", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schwach ist das Menschenkind, ein Rohr bewegt vom Wind;", "tokens": ["Schwach", "ist", "das", "Men\u00b7schen\u00b7kind", ",", "ein", "Rohr", "be\u00b7wegt", "vom", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O tadle nicht, da\u00df du bist wie die andern sind.", "tokens": ["O", "tad\u00b7le", "nicht", ",", "da\u00df", "du", "bist", "wie", "die", "an\u00b7dern", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VAFIN", "KOKOM", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur wo gebeut die Pflicht, und wo sie widerspricht,", "tokens": ["Nur", "wo", "ge\u00b7beut", "die", "Pflicht", ",", "und", "wo", "sie", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VVFIN", "ART", "NN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da thut und unterl\u00e4\u00dft ein Mann, und \u00e4ndert nicht.", "tokens": ["Da", "thut", "und", "un\u00b7ter\u00b7l\u00e4\u00dft", "ein", "Mann", ",", "und", "\u00e4n\u00b7dert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch vieles kann geschehn und kann auch unterbleiben,", "tokens": ["Doch", "vie\u00b7les", "kann", "ge\u00b7schehn", "und", "kann", "auch", "un\u00b7ter\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "VVINF", "KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In solchem darfst du dich von au\u00dfen lassen treiben.", "tokens": ["In", "sol\u00b7chem", "darfst", "du", "dich", "von", "au\u00b7\u00dfen", "las\u00b7sen", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "PPER", "PRF", "APPR", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}