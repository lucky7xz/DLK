{"textgrid.poem.35331": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht lag auf den Alpen schwer,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht lag auf den Alpen schwer,", "tokens": ["Die", "Nacht", "lag", "auf", "den", "Al\u00b7pen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da zog ein J\u00fcngling noch umher,", "tokens": ["Da", "zog", "ein", "J\u00fcng\u00b7ling", "noch", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Banner tragend weit durchs Land,", "tokens": ["Ein", "Ban\u00b7ner", "tra\u00b7gend", "weit", "durchs", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dem der fremde Wahlspruch stand:", "tokens": ["Auf", "dem", "der", "frem\u00b7de", "Wahl\u00b7spruch", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das Antlitz bleich, das Auge klar,", "tokens": ["Das", "Ant\u00b7litz", "bleich", ",", "das", "Au\u00b7ge", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Blick ein Strahl und wunderbar", "tokens": ["Der", "Blick", "ein", "Strahl", "und", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stimme, hell wie Schwerterklang", "tokens": ["Die", "Stim\u00b7me", ",", "hell", "wie", "Schwer\u00b7ter\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00fc\u00df melodisch, wenn er sang:", "tokens": ["Und", "s\u00fc\u00df", "me\u00b7lo\u00b7disch", ",", "wenn", "er", "sang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Rings aus den stillen H\u00fctten bricht", "tokens": ["Rings", "aus", "den", "stil\u00b7len", "H\u00fct\u00b7ten", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie trauter Gru\u00df des Herdes Licht;", "tokens": ["Wie", "trau\u00b7ter", "Gru\u00df", "des", "Her\u00b7des", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gletscher drohn, Gespenstern gleich,", "tokens": ["Die", "Glet\u00b7scher", "drohn", ",", "Ge\u00b7spens\u00b7tern", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er aber lispelt warm und weich:", "tokens": ["Er", "a\u00b7ber", "lis\u00b7pelt", "warm", "und", "weich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein alter D\u00f6rfner warnt: \u00bbO la\u00df", "tokens": ["Ein", "al\u00b7ter", "D\u00f6rf\u00b7ner", "warnt", ":", "\u00bb", "O", "la\u00df"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$(", "NE", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein nutzlos M\u00fch'n, geh nicht f\u00fcrba\u00df;", "tokens": ["Dein", "nutz\u00b7los", "M\u00fch'n", ",", "geh", "nicht", "f\u00fcr\u00b7ba\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein grauser Schneesturm fliegt herbei.\u00ab", "tokens": ["Ein", "grau\u00b7ser", "Schnee\u00b7sturm", "fliegt", "her\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der J\u00fcngling ruft: Die Bahn ist frei;", "tokens": ["Der", "J\u00fcng\u00b7ling", "ruft", ":", "Die", "Bahn", "ist", "frei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ein M\u00e4dchen fleht: \u00bbO halte Rast;", "tokens": ["Ein", "M\u00e4d\u00b7chen", "fleht", ":", "\u00bb", "O", "hal\u00b7te", "Rast", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei meiner Heimat lieber Gast\u00ab;", "tokens": ["Sei", "mei\u00b7ner", "Hei\u00b7mat", "lie\u00b7ber", "Gast", "\u00ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des J\u00fcnglings Wimpern sind betaut,", "tokens": ["Des", "J\u00fcng\u00b7lings", "Wim\u00b7pern", "sind", "be\u00b7taut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch unbezwungen singt er laut:", "tokens": ["Doch", "un\u00b7be\u00b7zwun\u00b7gen", "singt", "er", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "\u00bbentfleuche dem Lawinenball,", "tokens": ["\u00bb", "ent\u00b7fleu\u00b7che", "dem", "La\u00b7wi\u00b7nen\u00b7ball", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der F\u00f6hren Dr\u00f6h'n, der Wasser Schwall!\u00ab", "tokens": ["Der", "F\u00f6h\u00b7ren", "Dr\u00f6h'n", ",", "der", "Was\u00b7ser", "Schwall", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist des Alten letztes Wort.", "tokens": ["Das", "ist", "des", "Al\u00b7ten", "letz\u00b7tes", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch in den Bergen t\u00f6nt es fort:", "tokens": ["Hoch", "in", "den", "Ber\u00b7gen", "t\u00f6nt", "es", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und als es wieder Morgen war,", "tokens": ["Und", "als", "es", "wie\u00b7der", "Mor\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drang zu der frommen Br\u00fcderschar", "tokens": ["Drang", "zu", "der", "from\u00b7men", "Br\u00fc\u00b7der\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sankt Bernhards, wie aus tiefer Gruft,", "tokens": ["Sankt", "Bern\u00b7hards", ",", "wie", "aus", "tie\u00b7fer", "Gruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Seufzer durch die Winterluft:", "tokens": ["Der", "Seuf\u00b7zer", "durch", "die", "Win\u00b7ter\u00b7luft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Den Wandersmann \u2013 ach, welcher Fund! \u2013", "tokens": ["Den", "Wan\u00b7ders\u00b7mann", "\u2013", "ach", ",", "wel\u00b7cher", "Fund", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "XY", "$,", "PWAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Grub aus dem Schnee der Klosterhund;", "tokens": ["Grub", "aus", "dem", "Schnee", "der", "Klos\u00b7ter\u00b7hund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Noch fest umklammert hielt die Hand", "tokens": ["Noch", "fest", "um\u00b7klam\u00b7mert", "hielt", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Banner, drauf der Wahlspruch stand:", "tokens": ["Das", "Ban\u00b7ner", ",", "drauf", "der", "Wahl\u00b7spruch", "stand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Da lag die herrliche Gestalt,", "tokens": ["Da", "lag", "die", "herr\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erstarrten Herzens, todeskalt;", "tokens": ["Er\u00b7starr\u00b7ten", "Her\u00b7zens", ",", "to\u00b7des\u00b7kalt", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Himmel fiel ein Meteor,", "tokens": ["Vom", "Him\u00b7mel", "fiel", "ein", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und es erklang wie Engelchor:", "tokens": ["Und", "es", "er\u00b7klang", "wie", "En\u00b7gel\u00b7chor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Excelsior!", "tokens": ["Ex\u00b7cel\u00b7si\u00b7or", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}