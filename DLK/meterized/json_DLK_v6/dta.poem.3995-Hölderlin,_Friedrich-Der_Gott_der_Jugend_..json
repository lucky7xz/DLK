{"dta.poem.3995": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Gott der Jugend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Gehn Dir im D\u00e4mmerlichte,", "tokens": ["Gehn", "Dir", "im", "D\u00e4m\u00b7mer\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn in der Sommernacht", "tokens": ["Wenn", "in", "der", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr selige Gesichte", "tokens": ["F\u00fcr", "se\u00b7li\u00b7ge", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dein liebend Auge wacht,", "tokens": ["Dein", "lie\u00b7bend", "Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch oft der Freunde Manen", "tokens": ["Noch", "oft", "der", "Freun\u00b7de", "Ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und, wie der Sterne Chor,", "tokens": ["Und", ",", "wie", "der", "Ster\u00b7ne", "Chor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Geister der Titanen", "tokens": ["Die", "Geis\u00b7ter", "der", "Ti\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Alterthums empor:", "tokens": ["Des", "Al\u00b7ter\u00b7thums", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wird da, wo sich im Sch\u00f6nen,", "tokens": ["Wird", "da", ",", "wo", "sich", "im", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das G\u00f6ttliche verh\u00fcllt,", "tokens": ["Das", "G\u00f6tt\u00b7li\u00b7che", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch oft das tiefe Sehnen", "tokens": ["Noch", "oft", "das", "tie\u00b7fe", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Dir gestillt;", "tokens": ["Der", "Lie\u00b7be", "Dir", "ge\u00b7stillt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Belohnt des Herzens M\u00fchen", "tokens": ["Be\u00b7lohnt", "des", "Her\u00b7zens", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Ruhe Vorgef\u00fchl,", "tokens": ["Der", "Ru\u00b7he", "Vor\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und t\u00f6nt von Melodieen", "tokens": ["Und", "t\u00f6nt", "von", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der Seele Saitenspiel:", "tokens": ["Der", "See\u00b7le", "Sai\u00b7ten\u00b7spiel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So such' im stillsten Thale", "tokens": ["So", "such'", "im", "stills\u00b7ten", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den bl\u00fcthenreichsten Hain", "tokens": ["Den", "bl\u00fc\u00b7then\u00b7reichs\u00b7ten", "Hain"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gie\u00df' aus goldner Schale", "tokens": ["Und", "gie\u00df'", "aus", "gold\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den frohen Opferwein!", "tokens": ["Den", "fro\u00b7hen", "Op\u00b7fer\u00b7wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch l\u00e4chelt unveraltet", "tokens": ["Noch", "l\u00e4\u00b7chelt", "un\u00b7ver\u00b7al\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Herzens Fr\u00fchling Dir,", "tokens": ["Des", "Her\u00b7zens", "Fr\u00fch\u00b7ling", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Gott der Jugend waltet", "tokens": ["Der", "Gott", "der", "Ju\u00b7gend", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch \u00fcber Dir und mir.", "tokens": ["Noch", "\u00fc\u00b7ber", "Dir", "und", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie unter Tiburs B\u00e4umen,", "tokens": ["Wie", "un\u00b7ter", "Ti\u00b7burs", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn da der Dichter sa\u00df,", "tokens": ["Wenn", "da", "der", "Dich\u00b7ter", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unter G\u00f6ttertr\u00e4umen", "tokens": ["Und", "un\u00b7ter", "G\u00f6t\u00b7ter\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Jahre Flucht verga\u00df,", "tokens": ["Der", "Jah\u00b7re", "Flucht", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ihn die Ulme k\u00fchlte,", "tokens": ["Wenn", "ihn", "die", "Ul\u00b7me", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wenn sie stolz und froh", "tokens": ["Und", "wenn", "sie", "stolz", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Um Silberbl\u00fcthen spielte,", "tokens": ["Um", "Sil\u00b7ber\u00b7bl\u00fc\u00b7then", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Flut des Anio;", "tokens": ["Die", "Flut", "des", "A\u00b7nio", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und wie um Platons Hallen,", "tokens": ["Und", "wie", "um", "Pla\u00b7tons", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn durch der Haine Gr\u00fcn,", "tokens": ["Wenn", "durch", "der", "Hai\u00b7ne", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begr\u00fc\u00dft von Nachtigallen,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "von", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Stern der Liebe schien,", "tokens": ["Der", "Stern", "der", "Lie\u00b7be", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn alle L\u00fcfte schliefen,", "tokens": ["Wenn", "al\u00b7le", "L\u00fcf\u00b7te", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und, sanft bewegt vom Schwan,", "tokens": ["Und", ",", "sanft", "be\u00b7wegt", "vom", "Schwan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Cephisus durch Oliven", "tokens": ["Ce\u00b7phi\u00b7sus", "durch", "O\u00b7li\u00b7ven"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Myrthenstr\u00e4uche rann:", "tokens": ["Und", "Myr\u00b7then\u00b7str\u00e4u\u00b7che", "rann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So sch\u00f6n ist's noch hienieden!", "tokens": ["So", "sch\u00f6n", "ist's", "noch", "hien\u00b7ie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch unser Herz erfuhr", "tokens": ["Auch", "un\u00b7ser", "Herz", "er\u00b7fuhr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben und den Frieden", "tokens": ["Das", "Le\u00b7ben", "und", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der freundlichen Natur;", "tokens": ["Der", "freund\u00b7li\u00b7chen", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch bl\u00fcht des Himmels Sch\u00f6ne,", "tokens": ["Noch", "bl\u00fcht", "des", "Him\u00b7mels", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch mischen br\u00fcderlich", "tokens": ["Noch", "mi\u00b7schen", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In unsers Herzens T\u00f6ne", "tokens": ["In", "un\u00b7sers", "Her\u00b7zens", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Fr\u00fchlings Laute sich.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Lau\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum such' im stillsten Thale", "tokens": ["Drum", "such'", "im", "stills\u00b7ten", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den d\u00fcftereichsten Hain,", "tokens": ["Den", "d\u00fcf\u00b7te\u00b7reichs\u00b7ten", "Hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gie\u00df' aus goldner Schale", "tokens": ["Und", "gie\u00df'", "aus", "gold\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den frohen Opferwein!", "tokens": ["Den", "fro\u00b7hen", "Op\u00b7fer\u00b7wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch l\u00e4chelt unveraltet", "tokens": ["Noch", "l\u00e4\u00b7chelt", "un\u00b7ver\u00b7al\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Bild der Erde dir,", "tokens": ["Das", "Bild", "der", "Er\u00b7de", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Gott der Jugend waltet", "tokens": ["Der", "Gott", "der", "Ju\u00b7gend", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch \u00fcber dir und mir.", "tokens": ["Noch", "\u00fc\u00b7ber", "dir", "und", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}