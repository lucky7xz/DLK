{"textgrid.poem.33197": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2163.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis zum grossen hochzeit-fest, unsre treue Liebe! sichtbarlich einholen l\u00e4\u00dft die erkauffte riebe;", "tokens": ["Bis", "zum", "gros\u00b7sen", "hoch\u00b7zeit\u00b7fest", ",", "uns\u00b7re", "treu\u00b7e", "Lie\u00b7be", "!", "sicht\u00b7bar\u00b7lich", "ein\u00b7ho\u00b7len", "l\u00e4\u00dft", "die", "er\u00b7kauff\u00b7te", "rie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "PRF", "VVINF", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "--+-+--+-+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Hat er seinem lieben volk so ein pfand geschenket, dabey seine zeugen-wolk jener Eh gedenket.", "tokens": ["Hat", "er", "sei\u00b7nem", "lie\u00b7ben", "volk", "so", "ein", "pfand", "ge\u00b7schen\u00b7ket", ",", "da\u00b7bey", "sei\u00b7ne", "zeu\u00b7gen\u00b7wolk", "je\u00b7ner", "Eh", "ge\u00b7den\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$,", "PAV", "PPOSAT", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-++--+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Erstlich hat sein S\u00fcnder-weib in dem abendmahle mehr vom heil'gen Marter-leib als im glorie-saale.", "tokens": ["Erst\u00b7lich", "hat", "sein", "S\u00fcn\u00b7der\u00b7weib", "in", "dem", "a\u00b7bend\u00b7mah\u00b7le", "mehr", "vom", "heil'\u00b7gen", "Mar\u00b7ter\u00b7leib", "als", "im", "glo\u00b7rie\u00b7saa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "KOKOM", "APPRART", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Z\u00e4rtliche ersch\u00fctterung, die des leibes glieder zu des Lamms vereinigung pr\u00e4pariret wieder.", "tokens": ["Z\u00e4rt\u00b7li\u00b7che", "er\u00b7sch\u00fct\u00b7te\u00b7rung", ",", "die", "des", "lei\u00b7bes", "glie\u00b7der", "zu", "des", "Lamms", "ver\u00b7ei\u00b7ni\u00b7gung", "pr\u00e4\u00b7pa\u00b7ri\u00b7ret", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "ADV", "$."], "meter": "+---+-++-+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Hat er ein vicariat dadurch eingef\u00fchret, da der mann nach seinem grad Ihn repr\u00e4sentiret.", "tokens": ["Hat", "er", "ein", "vi\u00b7ca\u00b7ri\u00b7at", "da\u00b7durch", "ein\u00b7ge\u00b7f\u00fch\u00b7ret", ",", "da", "der", "mann", "nach", "sei\u00b7nem", "grad", "Ihn", "re\u00b7pr\u00e4\u00b7sen\u00b7ti\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NE", "PAV", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "ADV", "PPER", "VVFIN", "$."], "meter": "---+--+--+-+-+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Dieser venerable bund, der die ehe-sachen uns soll mit dem Lamme kund und ehrw\u00fcrdig machen,", "tokens": ["Die\u00b7ser", "ve\u00b7ne\u00b7rab\u00b7le", "bund", ",", "der", "die", "e\u00b7he\u00b7sa\u00b7chen", "uns", "soll", "mit", "dem", "Lam\u00b7me", "kund", "und", "ehr\u00b7w\u00fcr\u00b7dig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "PPER", "VMFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Wird so gar nicht recht erkennt, und so ignoriret, da\u00df man Gottes sacrament in das fleisch gef\u00fchret.", "tokens": ["Wird", "so", "gar", "nicht", "recht", "er\u00b7kennt", ",", "und", "so", "ig\u00b7no\u00b7ri\u00b7ret", ",", "da\u00df", "man", "Got\u00b7tes", "sa\u00b7cra\u00b7ment", "in", "das", "fleisch", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$,", "KOUS", "PIS", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+--+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Was geht unser eins das an? Lob sey unserm Manne, der den ehbruch abgethan, mit samt seinem banne.", "tokens": ["Was", "geht", "un\u00b7ser", "eins", "das", "an", "?", "Lob", "sey", "un\u00b7serm", "Man\u00b7ne", ",", "der", "den", "eh\u00b7bruch", "ab\u00b7ge\u00b7than", ",", "mit", "samt", "sei\u00b7nem", "ban\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "CARD", "PDS", "PTKVZ", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-+-+-+-+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Der auf die Gemeinen-zeit uns hat aufgehoben, und uns unsre ehe-freud bis dahin verschoben.", "tokens": ["Der", "auf", "die", "Ge\u00b7mei\u00b7nen\u00b7zeit", "uns", "hat", "auf\u00b7ge\u00b7ho\u00b7ben", ",", "und", "uns", "uns\u00b7re", "e\u00b7he\u00b7freud", "bis", "da\u00b7hin", "ver\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VAFIN", "VVPP", "$,", "KON", "PPER", "PPOSAT", "NE", "KON", "PAV", "VVINF", "$."], "meter": "-+--+-+--+-+---+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Herz-geschwister! betet an mit kindlichem beben, da\u00df das Lamm euch euren plan in die eh gegeben.", "tokens": ["Her\u00b7zge\u00b7schwis\u00b7ter", "!", "be\u00b7tet", "an", "mit", "kind\u00b7li\u00b7chem", "be\u00b7ben", ",", "da\u00df", "das", "Lamm", "euch", "eu\u00b7ren", "plan", "in", "die", "eh", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "APPR", "ADJA", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "PPOSAT", "ADV", "APPR", "ART", "KOUS", "VVPP", "$."], "meter": "+-+-+-+-+---+--+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Werdet von der Eh-Mama g\u00f6ttlich sigilliret, bis den Bore", "tokens": ["Wer\u00b7det", "von", "der", "Eh\u00b7Ma\u00b7ma", "g\u00f6tt\u00b7lich", "si\u00b7gil\u00b7li\u00b7ret", ",", "bis", "den", "Bo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+---+--+--+-+--+-", "measure": "trochaic.hexa.relaxed"}}}}}