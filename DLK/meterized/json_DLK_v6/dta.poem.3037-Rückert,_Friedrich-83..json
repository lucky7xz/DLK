{"dta.poem.3037": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Allgemeine schwebt dem Geist best\u00e4ndig vor,", "tokens": ["Das", "All\u00b7ge\u00b7mei\u00b7ne", "schwebt", "dem", "Geist", "be\u00b7st\u00e4n\u00b7dig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur wie ein Bild verh\u00fcllt von des Besondern Flor.", "tokens": ["Nur", "wie", "ein", "Bild", "ver\u00b7h\u00fcllt", "von", "des", "Be\u00b7son\u00b7dern", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn der Geist einmal sich, durch den Flor zu dringen,", "tokens": ["Doch", "wenn", "der", "Geist", "ein\u00b7mal", "sich", ",", "durch", "den", "Flor", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PRF", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gew\u00f6hnt hat, sieht er klar das All in allen Dingen.", "tokens": ["Ge\u00b7w\u00f6hnt", "hat", ",", "sieht", "er", "klar", "das", "All", "in", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist die Aehnlichkeit, die Bild mit Bild verkn\u00fcpft;", "tokens": ["Das", "ist", "die", "A\u00b7ehn\u00b7lich\u00b7keit", ",", "die", "Bild", "mit", "Bild", "ver\u00b7kn\u00fcpft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Fest h\u00e4lt die Dinge, wem der Faden nie entschl\u00fcpft.", "tokens": ["Fest", "h\u00e4lt", "die", "Din\u00b7ge", ",", "wem", "der", "Fa\u00b7den", "nie", "ent\u00b7schl\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PWS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das was sie \u00e4hnlich macht, das macht sie auch verschieden;", "tokens": ["Das", "was", "sie", "\u00e4hn\u00b7lich", "macht", ",", "das", "macht", "sie", "auch", "ver\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer dis Geheimni\u00df kennt, ist selig und zufrieden.", "tokens": ["Wer", "dis", "Ge\u00b7heim\u00b7ni\u00df", "kennt", ",", "ist", "se\u00b7lig", "und", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}