{"textgrid.poem.41769": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte keine Thaten, nur Gebete,", "tokens": ["Ich", "hat\u00b7te", "kei\u00b7ne", "Tha\u00b7ten", ",", "nur", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich war nur gro\u00df im Dulden und Ertragen,", "tokens": ["Ich", "war", "nur", "gro\u00df", "im", "Dul\u00b7den", "und", "Er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wu\u00dft' es nur: ich durfte nicht verzagen,", "tokens": ["Ich", "wu\u00dft'", "es", "nur", ":", "ich", "durf\u00b7te", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott war mit uns, zu dem ich br\u00fcnstig flehte.", "tokens": ["Gott", "war", "mit", "uns", ",", "zu", "dem", "ich", "br\u00fcns\u00b7tig", "fleh\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da kam ein Tag, an dem sein Odem wehte,", "tokens": ["Da", "kam", "ein", "Tag", ",", "an", "dem", "sein", "O\u00b7dem", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freiheit Himmelsstunde lie\u00df er schlagen,", "tokens": ["Der", "Frei\u00b7heit", "Him\u00b7mels\u00b7stun\u00b7de", "lie\u00df", "er", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wir einander Herz am Herzen lagen", "tokens": ["Da\u00df", "wir", "ein\u00b7an\u00b7der", "Herz", "am", "Her\u00b7zen", "la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Jubelseufzer waren unsre Rede.", "tokens": ["Und", "Ju\u00b7bel\u00b7seuf\u00b7zer", "wa\u00b7ren", "uns\u00b7re", "Re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O s\u00fc\u00dfe Wonne! seliges Genie\u00dfen", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Won\u00b7ne", "!", "se\u00b7li\u00b7ges", "Ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach treuem Harren, Dulden und Entbehren \u2013", "tokens": ["Nach", "treu\u00b7em", "Har\u00b7ren", ",", "Dul\u00b7den", "und", "Ent\u00b7beh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welch Triumphieren, da\u00df wir nie uns lie\u00dfen!", "tokens": ["Welch", "Tri\u00b7um\u00b7phie\u00b7ren", ",", "da\u00df", "wir", "nie", "uns", "lie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie k\u00f6nnten wir den Freudenthr\u00e4nen wehren,", "tokens": ["Wie", "k\u00f6nn\u00b7ten", "wir", "den", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Aug' in Aug' beseligt niederflie\u00dfen", "tokens": ["Die", "Aug'", "in", "Aug'", "be\u00b7se\u00b7ligt", "nie\u00b7der\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und so die Macht, die uns besch\u00fctzt, verehren?", "tokens": ["Und", "so", "die", "Macht", ",", "die", "uns", "be\u00b7sch\u00fctzt", ",", "ver\u00b7eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}