{"textgrid.poem.54955": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief unten, wo die Zwerge", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief unten, wo die Zwerge", "tokens": ["Tief", "un\u00b7ten", ",", "wo", "die", "Zwer\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4mmern und feilen,", "tokens": ["H\u00e4m\u00b7mern", "und", "fei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mu\u00df man eilen.", "tokens": ["Mu\u00df", "man", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Hoch oben, wo die Adler", "tokens": ["Hoch", "o\u00b7ben", ",", "wo", "die", "Ad\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jagen und morden,", "tokens": ["Ja\u00b7gen", "und", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mu\u00df man auch eilen \u2013", "tokens": ["Mu\u00df", "man", "auch", "ei\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Nur auf dem Festlande", "tokens": ["Nur", "auf", "dem", "Fest\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--++-", "measure": "dactylic.init"}, "line.2": {"text": "Kann man ruhig sitzen,", "tokens": ["Kann", "man", "ru\u00b7hig", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ohne zu schwitzen \u2013", "tokens": ["Oh\u00b7ne", "zu", "schwit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Man kann da auch liegen.", "tokens": ["Man", "kann", "da", "auch", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ja, ein Festland ist das feste Land!", "tokens": ["Ja", ",", "ein", "Fest\u00b7land", "ist", "das", "fes\u00b7te", "Land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00fc\u00dft ich nur, wo das Festland liegt!", "tokens": ["W\u00fc\u00dft", "ich", "nur", ",", "wo", "das", "Fest\u00b7land", "liegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}