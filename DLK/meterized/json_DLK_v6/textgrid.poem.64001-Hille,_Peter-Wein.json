{"textgrid.poem.64001": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Wein", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mein Wein, Adelsblut der Natur,", "tokens": ["Du", "mein", "Wein", ",", "A\u00b7dels\u00b7blut", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Nicht wahr, du lebst,", "tokens": ["Nicht", "wahr", ",", "du", "lebst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du flie\u00dfendes Juwel?", "tokens": ["Du", "flie\u00b7\u00dfen\u00b7des", "Ju\u00b7wel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenn du dich im Lenz erhebst", "tokens": ["Wenn", "du", "dich", "im", "Lenz", "er\u00b7hebst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und an die F\u00e4sser pochst,", "tokens": ["Und", "an", "die", "F\u00e4s\u00b7ser", "pochst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Willst du hinaus,", "tokens": ["Willst", "du", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Unband du,", "tokens": ["Un\u00b7band", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Hinaus zu den Deinen,", "tokens": ["Hin\u00b7aus", "zu", "den", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Die da bl\u00fchen und innig duften", "tokens": ["Die", "da", "bl\u00fc\u00b7hen", "und", "in\u00b7nig", "duf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADJD", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Auf sanfterlesenen H\u00e4ngen um braunes Gem\u00e4uer.", "tokens": ["Auf", "sanf\u00b7ter\u00b7le\u00b7se\u00b7nen", "H\u00e4n\u00b7gen", "um", "brau\u00b7nes", "Ge\u00b7m\u00e4u\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wie's da r\u00fcttelt dein Feuer,", "tokens": ["Wie's", "da", "r\u00fct\u00b7telt", "dein", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Dein Leben!", "tokens": ["Dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Wie viel Geschlechter hast du schon selig gemacht:", "tokens": ["Wie", "viel", "Ge\u00b7schlech\u00b7ter", "hast", "du", "schon", "se\u00b7lig", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "M\u00e4nner mit reinheitstarrenden Ehrenkrausen", "tokens": ["M\u00e4n\u00b7ner", "mit", "rein\u00b7heits\u00b7tar\u00b7ren\u00b7den", "Eh\u00b7ren\u00b7krau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Auf rankendem, schwarzdamastenem Taft,", "tokens": ["Auf", "ran\u00b7ken\u00b7dem", ",", "schwarz\u00b7da\u00b7mas\u00b7te\u00b7nem", "Taft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Du glutetest ihnen die k\u00fchnen, hellen Augen,", "tokens": ["Du", "glu\u00b7te\u00b7test", "ih\u00b7nen", "die", "k\u00fch\u00b7nen", ",", "hel\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Die weit die Lande umfassen", "tokens": ["Die", "weit", "die", "Lan\u00b7de", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Und folgen den palmenzuwinkenden Schiffen,", "tokens": ["Und", "fol\u00b7gen", "den", "pal\u00b7men\u00b7zu\u00b7win\u00b7ken\u00b7den", "Schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.19": {"text": "Wagemutigen Meeresboten,", "tokens": ["Wa\u00b7ge\u00b7mu\u00b7ti\u00b7gen", "Mee\u00b7res\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Die den gedankenglutenden Westen,", "tokens": ["Die", "den", "ge\u00b7dan\u00b7ken\u00b7glu\u00b7ten\u00b7den", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Den s\u00fc\u00dfentz\u00fcndeten S\u00fcden", "tokens": ["Den", "s\u00fc\u00b7\u00df\u00b7ent\u00b7z\u00fcn\u00b7de\u00b7ten", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Mit st\u00e4hlernem Norden", "tokens": ["Mit", "st\u00e4h\u00b7ler\u00b7nem", "Nor\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Tauschen wollen.", "tokens": ["Tau\u00b7schen", "wol\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Du n\u00e4hrst die schwimmende Tr\u00e4ne des Mannes,", "tokens": ["Du", "n\u00e4hrst", "die", "schwim\u00b7men\u00b7de", "Tr\u00e4\u00b7ne", "des", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Der all\u00fcberwindenden St\u00e4rke,", "tokens": ["Der", "al\u00b7l\u00fc\u00b7berw\u00b7in\u00b7den\u00b7den", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die Tr\u00e4ne, die nur Sieger f\u00fchlen ...", "tokens": ["Die", "Tr\u00e4\u00b7ne", ",", "die", "nur", "Sie\u00b7ger", "f\u00fch\u00b7len", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und an die klar gestaltete Glut", "tokens": ["Und", "an", "die", "klar", "ge\u00b7stal\u00b7te\u00b7te", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Deiner rebkrausen Ratskellerfenster,", "tokens": ["Dei\u00b7ner", "reb\u00b7krau\u00b7sen", "Rats\u00b7kel\u00b7ler\u00b7fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Die tief in die Seele", "tokens": ["Die", "tief", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Scheinen festliche Andacht,", "tokens": ["Schei\u00b7nen", "fest\u00b7li\u00b7che", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Schlug das welterobernde Lachen", "tokens": ["Schlug", "das", "wel\u00b7te\u00b7ro\u00b7bern\u00b7de", "La\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "All dieser sieghaft heitern Geschlechter.", "tokens": ["All", "die\u00b7ser", "sieg\u00b7haft", "hei\u00b7tern", "Ge\u00b7schlech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Du aber throntest", "tokens": ["Du", "a\u00b7ber", "thron\u00b7test"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Hoch auf m\u00e4chtigem Rund", "tokens": ["Hoch", "auf", "m\u00e4ch\u00b7ti\u00b7gem", "Rund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Deines fl\u00fcssigen Reiches:", "tokens": ["Dei\u00b7nes", "fl\u00fcs\u00b7si\u00b7gen", "Rei\u00b7ches", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.36": {"text": "Eine b\u00fcbisch l\u00e4chelnde, schelmische Sonne.", "tokens": ["Ei\u00b7ne", "b\u00fc\u00b7bisch", "l\u00e4\u00b7cheln\u00b7de", ",", "schel\u00b7mi\u00b7sche", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}