{"textgrid.poem.50625": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wird die ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird die ", "tokens": ["Es", "wird", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In das man unsers Leibs entkernte Schalen legt;", "tokens": ["In", "das", "man", "un\u00b7sers", "Leibs", "ent\u00b7kern\u00b7te", "Scha\u00b7len", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wann der Lebensf\u00fcrst erscheinen wird und wann", "tokens": ["Doch", "wann", "der", "Le\u00b7bens\u00b7f\u00fcrst", "er\u00b7schei\u00b7nen", "wird", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "VAFIN", "KON", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die donnernde Posaun' erschallen wird auf Erden,", "tokens": ["Die", "don\u00b7nern\u00b7de", "Po\u00b7saun'", "er\u00b7schal\u00b7len", "wird", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann wird das Grab bewegt, und unser Grab wird dann", "tokens": ["Dann", "wird", "das", "Grab", "be\u00b7wegt", ",", "und", "un\u00b7ser", "Grab", "wird", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}