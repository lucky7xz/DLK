{"dta.poem.3374": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Bl\u00e4ttchen.  \n  An Emma .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Horch, wie sauset der Wind in deinem vertrau-", "tokens": ["Horch", ",", "wie", "sau\u00b7set", "der", "Wind", "in", "dei\u00b7nem", "ver\u00b7trau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "lichen Garten!", "tokens": ["li\u00b7chen", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Schau, wie sch\u00fcttelt sein Hauch", "tokens": ["Schau", ",", "wie", "sch\u00fct\u00b7telt", "sein", "Hauch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Von den B\u00e4umen die letzten, die gelblichen Bl\u00e4tter", "tokens": ["Von", "den", "B\u00e4u\u00b7men", "die", "letz\u00b7ten", ",", "die", "gelb\u00b7li\u00b7chen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "herunter.", "tokens": ["her\u00b7un\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Siehe, wie treiben sie stumm", "tokens": ["Sie\u00b7he", ",", "wie", "trei\u00b7ben", "sie", "stumm"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Auf dem Boden umher, die bebenden sch\u00fcchternen", "tokens": ["Auf", "dem", "Bo\u00b7den", "um\u00b7her", ",", "die", "be\u00b7ben\u00b7den", "sch\u00fcch\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVINF"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Bl\u00e4ttchen,", "tokens": ["Bl\u00e4tt\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Weiland die Krone des Baums. \u2014", "tokens": ["Wei\u00b7land", "die", "Kro\u00b7ne", "des", "Baums", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Haben dich oft so sanft in luftigen Schlummer ge-", "tokens": ["Ha\u00b7ben", "dich", "oft", "so", "sanft", "in", "luf\u00b7ti\u00b7gen", "Schlum\u00b7mer", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "s\u00e4uselt,", "tokens": ["s\u00e4u\u00b7selt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Haben dich oft aus der Ruh", "tokens": ["Ha\u00b7ben", "dich", "oft", "aus", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Aufgerauscht, und wieder in melancholische Stille", "tokens": ["Auf\u00b7ge\u00b7rauscht", ",", "und", "wie\u00b7der", "in", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Stil\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Freundlich niedergewiegt.", "tokens": ["Freund\u00b7lich", "nie\u00b7der\u00b7ge\u00b7wiegt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Arme Bl\u00e4ttchen, ihr werdet nun ferner in Schlum-", "tokens": ["Ar\u00b7me", "Bl\u00e4tt\u00b7chen", ",", "ihr", "wer\u00b7det", "nun", "fer\u00b7ner", "in", "Schlum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPR", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "mer und Wehmuth", "tokens": ["mer", "und", "Weh\u00b7muth"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Emma nicht s\u00e4useln. Ihr rollt", "tokens": ["Em\u00b7ma", "nicht", "s\u00e4u\u00b7seln", ".", "Ihr", "rollt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKNEG", "VVINF", "$.", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.18": {"text": "Hoch in der brausenden Luft, und bald verwes't ihr,", "tokens": ["Hoch", "in", "der", "brau\u00b7sen\u00b7den", "Luft", ",", "und", "bald", "ver\u00b7wes't", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "und stiebet", "tokens": ["und", "stie\u00b7bet"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Staub im Aether umher.", "tokens": ["Staub", "im", "A\u00b7e\u00b7ther", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Emma, ich irr' hinauf und hinab im schaurigen", "tokens": ["Em\u00b7ma", ",", "ich", "irr'", "hin\u00b7auf", "und", "hin\u00b7ab", "im", "schau\u00b7ri\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Garten,", "tokens": ["Gar\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "W\u00fchl' im raschelnden Laub,", "tokens": ["W\u00fchl'", "im", "ra\u00b7scheln\u00b7den", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und es h\u00fcllet mir D\u00e4mmrung die Seele, D\u00e4mmrung", "tokens": ["Und", "es", "h\u00fcl\u00b7let", "mir", "D\u00e4mm\u00b7rung", "die", "See\u00b7le", ",", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "ART", "NN", "$,", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "das Auge.", "tokens": ["das", "Au\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Denn es prediget mir", "tokens": ["Denn", "es", "pre\u00b7di\u00b7get", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Jedes welkende Blatt und jedes sterbende Gr\u00e4schen:", "tokens": ["Je\u00b7des", "wel\u00b7ken\u00b7de", "Blatt", "und", "je\u00b7des", "ster\u00b7ben\u00b7de", "Gr\u00e4sc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "\u201eeinstens gr\u00fcnt' ich, wie du!", "tokens": ["\u201e", "eins\u00b7tens", "gr\u00fcnt'", "ich", ",", "wie", "du", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "\u201eeinstens welkst du, wie ich. Wie Gras auf dem", "tokens": ["\u201e", "eins\u00b7tens", "welkst", "du", ",", "wie", "ich", ".", "Wie", "Gras", "auf", "dem"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "$.", "PWAV", "NN", "APPR", "ART"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Felde sind Menschen,", "tokens": ["Fel\u00b7de", "sind", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "\u201egr\u00fcnen und welken, wie wir.\u201c", "tokens": ["\u201e", "gr\u00fc\u00b7nen", "und", "wel\u00b7ken", ",", "wie", "wir", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "KON", "VVINF", "$,", "PWAV", "PPER", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Rauschendes Bl\u00e4ttchen, du irrst. Du t\u00e4uschest dich,", "tokens": ["Rau\u00b7schen\u00b7des", "Bl\u00e4tt\u00b7chen", ",", "du", "irrst", ".", "Du", "t\u00e4u\u00b7schest", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "welkendes Bl\u00e4ttchen;", "tokens": ["wel\u00b7ken\u00b7des", "Bl\u00e4tt\u00b7chen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn ich bin nicht, wie du.", "tokens": ["Denn", "ich", "bin", "nicht", ",", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zwar ich werde verwelken in meiner gr\u00fcnenden", "tokens": ["Zwar", "ich", "wer\u00b7de", "ver\u00b7wel\u00b7ken", "in", "mei\u00b7ner", "gr\u00fc\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "VVINF", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Jugend.", "tokens": ["Ju\u00b7gend", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "J\u00fcnglingsst\u00e4rke zerschillt;", "tokens": ["J\u00fcng\u00b7lings\u00b7st\u00e4r\u00b7ke", "zer\u00b7schillt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "M\u00e4dchensch\u00f6ne verbl\u00fcht. Wir welken, wie Gras", "tokens": ["M\u00e4d\u00b7chen\u00b7sch\u00f6\u00b7ne", "ver\u00b7bl\u00fcht", ".", "Wir", "wel\u00b7ken", ",", "wie", "Gras"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$.", "PPER", "VVFIN", "$,", "PWAV", "NN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "auf dem Felde;", "tokens": ["auf", "dem", "Fel\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Aber wir welken nicht ganz.", "tokens": ["A\u00b7ber", "wir", "wel\u00b7ken", "nicht", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Freudig entschwingt sich dem Graus der Verwesung", "tokens": ["Freu\u00b7dig", "ent\u00b7schwingt", "sich", "dem", "Graus", "der", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "die ewige Seele,", "tokens": ["die", "e\u00b7wi\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Schwebet jubelnd empor,", "tokens": ["Schwe\u00b7bet", "ju\u00b7belnd", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Lebet von \u00c4on zu \u00c4on. Die morsche gebrechliche", "tokens": ["Le\u00b7bet", "von", "\u00c4\u00b7on", "zu", "\u00c4\u00b7on", ".", "Die", "mor\u00b7sche", "ge\u00b7brech\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.14": {"text": "H\u00fclle", "tokens": ["H\u00fcl\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Welkt und verwes't, wie du!", "tokens": ["Welkt", "und", "ver\u00b7wes't", ",", "wie", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Seufzest du, sterbendes Bl\u00e4ttchen? Mich d\u00fcnkt,", "tokens": ["Seuf\u00b7zest", "du", ",", "ster\u00b7ben\u00b7des", "Bl\u00e4tt\u00b7chen", "?", "Mich", "d\u00fcnkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "du seufzest im Winde", "tokens": ["du", "seuf\u00b7zest", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00dcber dein nichtiges Loos.", "tokens": ["\u00dc\u00b7ber", "dein", "nich\u00b7ti\u00b7ges", "Loos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Bl\u00e4ttchen, du seufzest mit Recht. Geh, klag' es", "tokens": ["Bl\u00e4tt\u00b7chen", ",", "du", "seuf\u00b7zest", "mit", "Recht", ".", "Geh", ",", "klag'", "es"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$.", "NE", "$,", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "dem ewigen Vater!", "tokens": ["dem", "e\u00b7wi\u00b7gen", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Er tr\u00e4gt Stern' und Staub,", "tokens": ["Er", "tr\u00e4gt", "Stern'", "und", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Seinen herrlichen Cherub und seine verduftende Blume,", "tokens": ["Sei\u00b7nen", "herr\u00b7li\u00b7chen", "Che\u00b7rub", "und", "sei\u00b7ne", "ver\u00b7duf\u00b7ten\u00b7de", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und den verschmachtenden Wurm,", "tokens": ["Und", "den", "ver\u00b7schmach\u00b7ten\u00b7den", "Wurm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Und dich, seufzendes Bl\u00e4ttchen, er kennt und", "tokens": ["Und", "dich", ",", "seuf\u00b7zen\u00b7des", "Bl\u00e4tt\u00b7chen", ",", "er", "kennt", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "KON"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "w\u00e4rmet euch alle", "tokens": ["w\u00e4r\u00b7met", "euch", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "PIS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "In dem seligen Schooss,", "tokens": ["In", "dem", "se\u00b7li\u00b7gen", "Scho\u00b7oss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Liebt euch und labt euch, und wird sich deines", "tokens": ["Liebt", "euch", "und", "labt", "euch", ",", "und", "wird", "sich", "dei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,", "KON", "VAFIN", "PRF", "PPOSAT"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Seufzens erbarmen,", "tokens": ["Seuf\u00b7zens", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Wie er sich aller erbarmt.", "tokens": ["Wie", "er", "sich", "al\u00b7ler", "er\u00b7barmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PIAT", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Rauschendes Bl\u00e4ttchen, wo schwebest du hin? Der", "tokens": ["Rau\u00b7schen\u00b7des", "Bl\u00e4tt\u00b7chen", ",", "wo", "schwe\u00b7best", "du", "hin", "?", "Der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "ART"], "meter": "+--+--+--++", "measure": "dactylic.tri.plus"}, "line.2": {"text": "reissende Ostwind", "tokens": ["reis\u00b7sen\u00b7de", "Ost\u00b7wind"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wirbelt dich hoch in die Luft,", "tokens": ["Wir\u00b7belt", "dich", "hoch", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "H\u00f6her und immer h\u00f6her. Du schwindest dem Blicke;", "tokens": ["H\u00f6\u00b7her", "und", "im\u00b7mer", "h\u00f6\u00b7her", ".", "Du", "schwin\u00b7dest", "dem", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "das Auge", "tokens": ["das", "Au\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Sieht dich nicht ferner. \u2014 Doch zuckt", "tokens": ["Sieht", "dich", "nicht", "fer\u00b7ner", ".", "Doch", "zuckt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "$(", "KON", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mir durch die Seele, wie Blitz, ein lichtes, ein", "tokens": ["Mir", "durch", "die", "See\u00b7le", ",", "wie", "Blitz", ",", "ein", "lich\u00b7tes", ",", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PWAV", "NN", "$,", "ART", "ADJA", "$,", "ART"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "tr\u00f6stendes Ahnen:", "tokens": ["tr\u00f6s\u00b7ten\u00b7des", "Ah\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Gott tr\u00e4gt Stern' und Staub,", "tokens": ["Gott", "tr\u00e4gt", "Stern'", "und", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Sonnen und Monden und W\u00fcrmchen an seinem er-", "tokens": ["Son\u00b7nen", "und", "Mon\u00b7den", "und", "W\u00fcrm\u00b7chen", "an", "sei\u00b7nem", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.11": {"text": "quickenden Busen,", "tokens": ["qui\u00b7cken\u00b7den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Wahrt und w\u00e4rmet sie treu,", "tokens": ["Wahrt", "und", "w\u00e4r\u00b7met", "sie", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Und erbarmt sich ihrer Seufzer. Sie seufzen um", "tokens": ["Und", "er\u00b7barmt", "sich", "ih\u00b7rer", "Seuf\u00b7zer", ".", "Sie", "seuf\u00b7zen", "um"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Leben,", "tokens": ["Le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Dass der vernichtende Arm", "tokens": ["Dass", "der", "ver\u00b7nich\u00b7ten\u00b7de", "Arm"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.16": {"text": "Sie nicht ergreife, dass sie der tausend tausendmal", "tokens": ["Sie", "nicht", "er\u00b7grei\u00b7fe", ",", "dass", "sie", "der", "tau\u00b7send", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "ART", "CARD", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "tausend", "tokens": ["tau\u00b7send"], "token_info": ["word"], "pos": ["CARD"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Rollender Jahre Reih'n", "tokens": ["Rol\u00b7len\u00b7der", "Jah\u00b7re", "Reih'n"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Leben m\u00f6gen, und klimmen von Sprosse zu Sprosse", "tokens": ["Le\u00b7ben", "m\u00f6\u00b7gen", ",", "und", "klim\u00b7men", "von", "Spros\u00b7se", "zu", "Spros\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$,", "KON", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "der Leiter,", "tokens": ["der", "Lei\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Die die Geschaffenen tr\u00e4gt,", "tokens": ["Die", "die", "Ge\u00b7schaf\u00b7fe\u00b7nen", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Bis sie die oberste Stuf' erklimmen, des Endlichen", "tokens": ["Bis", "sie", "die", "o\u00b7bers\u00b7te", "Stuf'", "er\u00b7klim\u00b7men", ",", "des", "End\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$,", "ART", "NN"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Gr\u00e4nze.", "tokens": ["Gr\u00e4n\u00b7ze", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Freue dich, welkendes Blatt!", "tokens": ["Freu\u00b7e", "dich", ",", "wel\u00b7ken\u00b7des", "Blatt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Kehrest wieder im Lenz als sch\u00f6nste Rose des", "tokens": ["Keh\u00b7rest", "wie\u00b7der", "im", "Lenz", "als", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPRART", "NN", "KOUS", "ADJA", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Gartens,", "tokens": ["Gar\u00b7tens", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Duftest den Sommer hindurch,", "tokens": ["Duf\u00b7test", "den", "Som\u00b7mer", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Bl\u00fchst und erblassest, und welkst, und um dich", "tokens": ["Bl\u00fchst", "und", "er\u00b7blas\u00b7sest", ",", "und", "welkst", ",", "und", "um", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "APPR", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "trauert das M\u00e4dchen,", "tokens": ["trau\u00b7ert", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Der du am Busen verwelkst.", "tokens": ["Der", "du", "am", "Bu\u00b7sen", "ver\u00b7welkst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Lass sie trauern, und traure nicht mit. Im kehren-", "tokens": ["Lass", "sie", "trau\u00b7ern", ",", "und", "trau\u00b7re", "nicht", "mit", ".", "Im", "keh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$.", "APPRART", "TRUNC"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "den Fr\u00fchling", "tokens": ["den", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "H\u00f6rt sie dich einsam im Busch", "tokens": ["H\u00f6rt", "sie", "dich", "ein\u00b7sam", "im", "Busch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "APPRART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Eine Nachtigall klagen, und weinet dir Thr\u00e4nen", "tokens": ["Ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "kla\u00b7gen", ",", "und", "wei\u00b7net", "dir", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "VVFIN", "PPER", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "der R\u00fchrung. \u2014", "tokens": ["der", "R\u00fch\u00b7rung", "."], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Zwar auch die Nachtigall stirbt;", "tokens": ["Zwar", "auch", "die", "Nach\u00b7ti\u00b7gall", "stirbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Aber es keimt aus der Nachtigall Asch' ein bl\u00fchen-", "tokens": ["A\u00b7ber", "es", "keimt", "aus", "der", "Nach\u00b7ti\u00b7gall", "Asch'", "ein", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "wo