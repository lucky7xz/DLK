{"textgrid.poem.42526": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend sinkt", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehne mich, am Schlu\u00df der Dissonanzen,", "tokens": ["Ich", "seh\u00b7ne", "mich", ",", "am", "Schlu\u00df", "der", "Dis\u00b7so\u00b7nan\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die auch den sommerhellsten Tag verschneien,", "tokens": ["Die", "auch", "den", "som\u00b7mer\u00b7hells\u00b7ten", "Tag", "ver\u00b7schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach frohen Stunden endlich, b\u00fcrdefreien,", "tokens": ["Nach", "fro\u00b7hen", "Stun\u00b7den", "end\u00b7lich", ",", "b\u00fcr\u00b7de\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um hinter guten Wein mich zu verschanzen.", "tokens": ["Um", "hin\u00b7ter", "gu\u00b7ten", "Wein", "mich", "zu", "ver\u00b7schan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nach Witz und freiem Wort, statt Schild und Lanzen,", "tokens": ["Nach", "Witz", "und", "frei\u00b7em", "Wort", ",", "statt", "Schild", "und", "Lan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach warmen Sch\u00fcsseln, Firlefanzereien,", "tokens": ["Nach", "war\u00b7men", "Sch\u00fcs\u00b7seln", ",", "Fir\u00b7le\u00b7fan\u00b7ze\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach sch\u00f6nen Frauen, Liedern und Schalmeien,", "tokens": ["Nach", "sch\u00f6\u00b7nen", "Frau\u00b7en", ",", "Lie\u00b7dern", "und", "Schal\u00b7mei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nach T\u00e4nzerinnen, die Fandango tanzen.", "tokens": ["Nach", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", ",", "die", "Fan\u00b7dan\u00b7go", "tan\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf Polstern liegend mit dem Nargileh,", "tokens": ["Auf", "Pols\u00b7tern", "lie\u00b7gend", "mit", "dem", "Nar\u00b7gi\u00b7leh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vertreib ich, wie die Hummeln aus dem Klee,", "tokens": ["Ver\u00b7treib", "ich", ",", "wie", "die", "Hum\u00b7meln", "aus", "dem", "Klee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit blauem Rauch die letzten Sorgensummer.", "tokens": ["Mit", "blau\u00b7em", "Rauch", "die", "letz\u00b7ten", "Sor\u00b7gen\u00b7sum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Garten drau\u00dfen heult, ganz ohne Kummer,", "tokens": ["Im", "Gar\u00b7ten", "drau\u00b7\u00dfen", "heult", ",", "ganz", "oh\u00b7ne", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sturm und stemmt den ungeschlachten Nacken", "tokens": ["Der", "Sturm", "und", "stemmt", "den", "un\u00b7ge\u00b7schlach\u00b7ten", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An meine Klause, da\u00df die Pfosten knacken.", "tokens": ["An", "mei\u00b7ne", "Klau\u00b7se", ",", "da\u00df", "die", "Pfos\u00b7ten", "kna\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}