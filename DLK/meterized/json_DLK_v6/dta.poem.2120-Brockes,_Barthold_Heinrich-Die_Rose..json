{"dta.poem.2120": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem zu Gut, o Mensch, als dir? Sollt aus solcher Krea-", "tokens": ["Wem", "zu", "Gut", ",", "o", "Mensch", ",", "als", "dir", "?", "Sollt", "aus", "sol\u00b7cher", "Krea"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKA", "ADJD", "$,", "FM", "NN", "$,", "KOUS", "PPER", "$.", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "tur,", "tokens": ["tur", ","], "token_info": ["word", "punct"], "pos": ["FM.la", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die f\u00fcr dich nur lieblich riecht, welche blo\u00df f\u00fcr dich so", "tokens": ["Die", "f\u00fcr", "dich", "nur", "lieb\u00b7lich", "riecht", ",", "wel\u00b7che", "blo\u00df", "f\u00fcr", "dich", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPER", "ADV"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "sch\u00f6n,", "tokens": ["sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Gegen den, der sie erschuf und sie dir geschenkt, nicht nur", "tokens": ["Ge\u00b7gen", "den", ",", "der", "sie", "er\u00b7schuf", "und", "sie", "dir", "ge\u00b7schenkt", ",", "nicht", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "KON", "PPER", "PPER", "VVPP", "$,", "PTKNEG", "ADV"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Ein bemerkendes Empfinden, ein\u2019 Erkenntlichkeit, ent-", "tokens": ["Ein", "be\u00b7mer\u00b7ken\u00b7des", "Emp\u00b7fin\u00b7den", ",", "ein'", "Er\u00b7kennt\u00b7lich\u00b7keit", ",", "ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "stehn?", "tokens": ["stehn", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ich bewundere, Gott Lob! dieser Blume Wunderpracht,", "tokens": ["Ich", "be\u00b7wun\u00b7de\u00b7re", ",", "Gott", "Lob", "!", "die\u00b7ser", "Blu\u00b7me", "Wun\u00b7der\u00b7pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "NN", "$.", "PDAT", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Und in der empfundnen Wollust preis\u2019 ich den, der sie", "tokens": ["Und", "in", "der", "emp\u00b7fund\u00b7nen", "Wol\u00b7lust", "preis'", "ich", "den", ",", "der", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "$,", "PRELS", "PPER"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "gemacht.", "tokens": ["ge\u00b7macht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Herr! wenn Rosen mich vergn\u00fcgen, seufzet die ger\u00fchrte", "tokens": ["Herr", "!", "wenn", "Ro\u00b7sen", "mich", "ver\u00b7gn\u00fc\u00b7gen", ",", "seuf\u00b7zet", "die", "ge\u00b7r\u00fchr\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "NN", "PPER", "VVPP", "$,", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Brust:", "tokens": ["Brust", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Nimm, zum R\u00e4uchwerk, mein Bewundern, und, zum Opfer,", "tokens": ["Nimm", ",", "zum", "R\u00e4uch\u00b7werk", ",", "mein", "Be\u00b7wun\u00b7dern", ",", "und", ",", "zum", "Op\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "meine Lust!", "tokens": ["mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}