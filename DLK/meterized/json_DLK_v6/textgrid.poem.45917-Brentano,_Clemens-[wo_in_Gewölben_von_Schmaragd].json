{"textgrid.poem.45917": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wo in Gew\u00f6lben von Schmaragd]", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo in Gew\u00f6lben von Schmaragd", "tokens": ["Wo", "in", "Ge\u00b7w\u00f6l\u00b7ben", "von", "Schma\u00b7ragd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die frischen B\u00e4chlein sp\u00fclen", "tokens": ["Die", "fri\u00b7schen", "B\u00e4ch\u00b7lein", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will sich bei H\u00f6rnerklang die Jagd", "tokens": ["Will", "sich", "bei", "H\u00f6r\u00b7ner\u00b7klang", "die", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ku\u00df und Wein erk\u00fchlen.", "tokens": ["Mit", "Ku\u00df", "und", "Wein", "er\u00b7k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie schallet und hallet der H\u00f6rnerklang", "tokens": ["Wie", "schal\u00b7let", "und", "hal\u00b7let", "der", "H\u00f6r\u00b7ner\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie rauscht der wilde Bronnen", "tokens": ["Wie", "rauscht", "der", "wil\u00b7de", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es widerklinget der Felsenhang", "tokens": ["Es", "wi\u00b7der\u00b7klin\u00b7get", "der", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Fliege tanzt in der Sonnen", "tokens": ["Die", "Flie\u00b7ge", "tanzt", "in", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber Frau Echo, Frau Echo, Frau Echo,", "tokens": ["A\u00b7ber", "Frau", "E\u00b7cho", ",", "Frau", "E\u00b7cho", ",", "Frau", "E\u00b7cho", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,", "NN", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Du widerspiegelst die Wonnen.", "tokens": ["Du", "wi\u00b7der\u00b7spie\u00b7gelst", "die", "Won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Gegr\u00fc\u00dfet sei du Waldgeb\u00e4u,", "tokens": ["Ge\u00b7gr\u00fc\u00b7\u00dfet", "sei", "du", "Wald\u00b7ge\u00b7b\u00e4u", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr hochbelaubten Eichen,", "tokens": ["Ihr", "hoch\u00b7be\u00b7laub\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm M\u00e4gdlein, setz dich neben bei,", "tokens": ["Komm", "M\u00e4gd\u00b7lein", ",", "setz", "dich", "ne\u00b7ben", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "PRF", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tu mir den Becher reichen.", "tokens": ["Tu", "mir", "den", "Be\u00b7cher", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie webet und schwebet das gr\u00fcne Dach", "tokens": ["Wie", "we\u00b7bet", "und", "schwe\u00b7bet", "das", "gr\u00fc\u00b7ne", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie stehn die ew'gen Eichen,", "tokens": ["Wie", "stehn", "die", "ew'\u00b7gen", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schau wie die Bl\u00fcmlein zu dem Bach", "tokens": ["Und", "schau", "wie", "die", "Bl\u00fcm\u00b7lein", "zu", "dem", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Kelche durstig neigen.", "tokens": ["Die", "Kel\u00b7che", "durs\u00b7tig", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aber dir Bachus, dir Bachus, dir Bachus", "tokens": ["A\u00b7ber", "dir", "Ba\u00b7chus", ",", "dir", "Ba\u00b7chus", ",", "dir", "Ba\u00b7chus"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "NE", "$,", "PPER", "NE", "$,", "PPER", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Mu\u00df alle Seligkeit weichen.", "tokens": ["Mu\u00df", "al\u00b7le", "Se\u00b7lig\u00b7keit", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und den vielgoldnen Sonnenglanz", "tokens": ["Und", "den", "viel\u00b7gold\u00b7nen", "Son\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df in den Becher schauen,", "tokens": ["La\u00df", "in", "den", "Be\u00b7cher", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und flicht mir einen Blumenkranz", "tokens": ["Und", "flicht", "mir", "ei\u00b7nen", "Blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wolle mir vertrauen.", "tokens": ["Und", "wol\u00b7le", "mir", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es blinket und winket der goldne Wein", "tokens": ["Es", "blin\u00b7ket", "und", "win\u00b7ket", "der", "gold\u00b7ne", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es lassen die Blumen sich pfl\u00fccken", "tokens": ["Es", "las\u00b7sen", "die", "Blu\u00b7men", "sich", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie m\u00f6chten gern all gebrochen sein,", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "gern", "all", "ge\u00b7bro\u00b7chen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So sch\u00f6n wei\u00df sie sich zu b\u00fccken.", "tokens": ["So", "sch\u00f6n", "wei\u00df", "sie", "sich", "zu", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber Frau Venus, Frau Venus, Frau Venus", "tokens": ["A\u00b7ber", "Frau", "Ve\u00b7nus", ",", "Frau", "Ve\u00b7nus", ",", "Frau", "Ve\u00b7nus"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "NE", "$,", "NN", "NE", "$,", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Kredenzet das ird'sche Entz\u00fccken.", "tokens": ["Kre\u00b7den\u00b7zet", "das", "ird'\u00b7sche", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und weil die Sonne hei\u00dfer scheint", "tokens": ["Und", "weil", "die", "Son\u00b7ne", "hei\u00b7\u00dfer", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm in die dunkle Laube,", "tokens": ["Komm", "in", "die", "dunk\u00b7le", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn gleich die wilde Rebe weint", "tokens": ["Wenn", "gleich", "die", "wil\u00b7de", "Re\u00b7be", "weint"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lacht doch die Turteltaube.", "tokens": ["Lacht", "doch", "die", "Tur\u00b7tel\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mag weinen die Rebe, die Taube lacht", "tokens": ["Mag", "wei\u00b7nen", "die", "Re\u00b7be", ",", "die", "Tau\u00b7be", "lacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Lerche jubelt in L\u00fcften", "tokens": ["Die", "Ler\u00b7che", "ju\u00b7belt", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Birkhuhn falzt in Waldesnacht,", "tokens": ["Das", "Birk\u00b7huhn", "falzt", "in", "Wal\u00b7des\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hirschkuh lockt in den Kl\u00fcften", "tokens": ["Die", "Hirschkuh", "lockt", "in", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Keusche Diana, Diana, Diana", "tokens": ["Keu\u00b7sche", "Di\u00b7a\u00b7na", ",", "Di\u00b7a\u00b7na", ",", "Di\u00b7a\u00b7na"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "NE", "$,", "NE"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Endimion naht in den Triften.", "tokens": ["En\u00b7di\u00b7mi\u00b7on", "naht", "in", "den", "Trif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie bringt den Wein in Bechersglanz,", "tokens": ["Sie", "bringt", "den", "Wein", "in", "Be\u00b7chers\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Veilchen und Narzissen", "tokens": ["Aus", "Veil\u00b7chen", "und", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Reicht sie ihm einen s\u00fc\u00dfen Kranz", "tokens": ["Reicht", "sie", "ihm", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Waldes Finsternissen.", "tokens": ["In", "Wal\u00b7des", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da lispelt und wispelt die Nachtigall,", "tokens": ["Da", "lis\u00b7pelt", "und", "wis\u00b7pelt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr Stimmlein wollt' \u00fcbersteigen,", "tokens": ["Ihr", "Stimm\u00b7lein", "wollt'", "\u00fc\u00b7bers\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es lacht und klagt der s\u00fc\u00dfe Schall", "tokens": ["Es", "lacht", "und", "klagt", "der", "s\u00fc\u00b7\u00dfe", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Orgel, Laute und Geigen,", "tokens": ["Wie", "Or\u00b7gel", ",", "Lau\u00b7te", "und", "Gei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber du Amor, du Amor, du Amor", "tokens": ["A\u00b7ber", "du", "A\u00b7mor", ",", "du", "A\u00b7mor", ",", "du", "A\u00b7mor"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "NE", "$,", "PPER", "NE", "$,", "PPER", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Vor dir mu\u00df alles ja schweigen.", "tokens": ["Vor", "dir", "mu\u00df", "al\u00b7les", "ja", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}