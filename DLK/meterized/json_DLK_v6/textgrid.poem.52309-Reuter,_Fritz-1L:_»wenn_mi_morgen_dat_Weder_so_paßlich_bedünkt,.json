{"textgrid.poem.52309": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwenn mi morgen dat Weder so pa\u00dflich bed\u00fcnkt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwenn mi morgen dat Weder so pa\u00dflich bed\u00fcnkt,", "tokens": ["\u00bb", "wenn", "mi", "mor\u00b7gen", "dat", "We\u00b7der", "so", "pa\u00df\u00b7lich", "be\u00b7d\u00fcnkt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NE", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "will ick ledden de Ossen tau Stadt;", "tokens": ["will", "ick", "led\u00b7den", "de", "Os\u00b7sen", "tau", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "NE", "NE", "NE", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sei s\u00fcnd grot und s\u00fcnd stark, denn ick heww sei fiw Johr", "tokens": ["Sei", "s\u00fcnd", "grot", "und", "s\u00fcnd", "stark", ",", "denn", "ick", "heww", "sei", "fiw", "Johr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,", "KON", "PPER", "NE", "VAFIN", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "up de Weid und in Fauder all hatt.", "tokens": ["up", "de", "Weid", "und", "in", "Fau\u00b7der", "all", "hatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "KON", "APPR", "NN", "PIAT", "VAFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Doch ick krigs' nich verhandelt, ji s\u00e4lt dat man seihn\u00ab,", "tokens": ["Doch", "ick", "krigs'", "nich", "ver\u00b7han\u00b7delt", ",", "ji", "s\u00e4lt", "dat", "man", "seihn", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "VVPP", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "seggt de Bur, \u00bbgewt blot man mal acht!", "tokens": ["seggt", "de", "Bur", ",", "\u00bb", "gewt", "blot", "man", "mal", "acht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$,", "$(", "VVPP", "VVFIN", "PIS", "ADV", "CARD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Denn meindag' nich is't kamen, as ick mi't heww dr\u00f6mt,", "tokens": ["Denn", "mein\u00b7dag'", "nich", "is't", "ka\u00b7men", ",", "as", "ick", "mi't", "heww", "dr\u00f6mt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "un meindag' nich, as ick mi't heww dacht.\u00ab", "tokens": ["un", "mein\u00b7dag'", "nich", ",", "as", "ick", "mi't", "heww", "dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "PTKNEG", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Un den annern Morgen tau richtige Tid,", "tokens": ["Un", "den", "an\u00b7nern", "Mor\u00b7gen", "tau", "rich\u00b7ti\u00b7ge", "Tid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "dor hett hei de Ossen in't Strick.", "tokens": ["dor", "hett", "hei", "de", "Os\u00b7sen", "in't", "Strick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbna, en Dalerne f\u00f6ftig\u00ab, so denkt hei, \u00bbde krig'ck,", "tokens": ["\u00bb", "na", ",", "en", "Da\u00b7ler\u00b7ne", "f\u00f6f\u00b7tig", "\u00ab", ",", "so", "denkt", "hei", ",", "\u00bb", "de", "krig'ck", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "FM", "FM", "FM", "$(", "$,", "ADV", "VVFIN", "PTKVZ", "$,", "$(", "NE", "NE", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "wenn ick jichtens up Morgen heww Gl\u00fcck.", "tokens": ["wenn", "ick", "jich\u00b7tens", "up", "Mor\u00b7gen", "heww", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "NE", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wat ick f\u00f6dder, s\u00fcnd s' wirt, denn sei beid s\u00fcnd gesund,", "tokens": ["Wat", "ick", "f\u00f6d\u00b7der", ",", "s\u00fcnd", "s'", "wirt", ",", "denn", "sei", "beid", "s\u00fcnd", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$,", "KON", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "un sei beid s\u00fcnd so glatt und so wacht;", "tokens": ["un", "sei", "beid", "s\u00fcnd", "so", "glatt", "und", "so", "wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Doch meindag' nich is't kamen, as ick mi't heww dr\u00f6mt,", "tokens": ["Doch", "mein\u00b7dag'", "nich", "is't", "ka\u00b7men", ",", "as", "ick", "mi't", "heww", "dr\u00f6mt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und meindag nich, as ick mi't heww dacht.\u00ab", "tokens": ["und", "mein\u00b7dag", "nich", ",", "as", "ick", "mi't", "heww", "dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Un de K\u00f6pers, de kamen und sch\u00fcddeln den Kopp:", "tokens": ["Un", "de", "K\u00f6\u00b7pers", ",", "de", "ka\u00b7men", "und", "sch\u00fcd\u00b7deln", "den", "Kopp", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "NE", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "\u00bbne! De Pris, min oll Fr\u00fcnd, is tau stark.\u00ab", "tokens": ["\u00bb", "ne", "!", "De", "Pris", ",", "min", "oll", "Fr\u00fcnd", ",", "is", "tau", "stark", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "NE", "NE", "$,", "PPOSAT", "ADJD", "NN", "$,", "FM", "FM", "FM", "$.", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Un de Bur, de t\u00e4uwt, un de Bur, de lurt,", "tokens": ["Un", "de", "Bur", ",", "de", "t\u00e4uwt", ",", "un", "de", "Bur", ",", "de", "lurt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "NE", "NE", "$,", "FM", "FM", "FM", "$,", "NE", "NE", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "un hei steiht, bet v\u00f6rbi is dat Mark.", "tokens": ["un", "hei", "steiht", ",", "bet", "v\u00f6r\u00b7bi", "is", "dat", "Mark", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$,", "VVFIN", "FM", "FM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "As dat Mark nu v\u00f6rbi is, dunn ledd't hei dat Veih", "tokens": ["As", "dat", "Mark", "nu", "v\u00f6r\u00b7bi", "is", ",", "dunn", "ledd't", "hei", "dat", "Veih"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADV", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "in de Straten hendalen ganz sacht,", "tokens": ["in", "de", "Stra\u00b7ten", "hen\u00b7da\u00b7len", "ganz", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "\u00bb't is ok ditmal nich kamen, as ick mi't heww dr\u00f6mt,", "tokens": ["\u00bb", "'t", "is", "ok", "dit\u00b7mal", "nich", "ka\u00b7men", ",", "as", "ick", "mi't", "heww", "dr\u00f6mt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "ADV", "PTKNEG", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.8": {"text": "un ok ditmal nich, as ick mi't dacht.\u00ab", "tokens": ["un", "ok", "dit\u00b7mal", "nich", ",", "as", "ick", "mi't", "dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "ADV", "PTKNEG", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "As hei ledd't nu nah Hus', dor dr\u00f6ppt hei 'ne B\u00e4k,", "tokens": ["As", "hei", "ledd't", "nu", "nah", "Hus'", ",", "dor", "dr\u00f6ppt", "hei", "'ne", "B\u00e4k", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADJD", "NE", "$,", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "un \u00e4wer de B\u00e4k liggt en Steg,", "tokens": ["un", "\u00e4\u00b7wer", "de", "B\u00e4k", "liggt", "en", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Un nich rechtsch und nich linksch kann hei kamen v\u00f6rbi,", "tokens": ["Un", "nich", "rechtsch", "und", "nich", "link\u00b7sch", "kann", "hei", "ka\u00b7men", "v\u00f6r\u00b7bi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "VMFIN", "NE", "VVFIN", "NE", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "hei m\u00f6t \u00e4wer dat Steg \u00e4werweg.", "tokens": ["hei", "m\u00f6t", "\u00e4\u00b7wer", "dat", "Steg", "\u00e4\u00b7wer\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Un so steiht hei bedenklich un kratzt sick den Kopp,", "tokens": ["Un", "so", "steiht", "hei", "be\u00b7denk\u00b7lich", "un", "kratzt", "sick", "den", "Kopp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "ADJD", "FM", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "un hei seggt tau sick s\u00fclwst mit Bedacht:", "tokens": ["un", "hei", "seggt", "tau", "sick", "s\u00fclwst", "mit", "Be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "APPR", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "\u00bbs\u00fcll dat ditmal woll kamen, so as mi dat dr\u00f6mt,", "tokens": ["\u00bb", "s\u00fcll", "dat", "dit\u00b7mal", "woll", "ka\u00b7men", ",", "so", "as", "mi", "dat", "dr\u00f6mt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "ADV", "ADV", "VVFIN", "$,", "ADV", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-----+--+---", "measure": "iambic.di.relaxed"}, "line.8": {"text": "un woll ditmal, so as ick mi't dacht?\u00ab", "tokens": ["un", "woll", "dit\u00b7mal", ",", "so", "as", "ick", "mi't", "dacht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "ADV", "ADV", "$,", "ADV", "FM", "FM", "FM", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Un de Ossen, de gahn nu woll \u00e4wer de B\u00e4k,", "tokens": ["Un", "de", "Os\u00b7sen", ",", "de", "gahn", "nu", "woll", "\u00e4\u00b7wer", "de", "B\u00e4k", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "un de Bur, de geiht in de Midd,", "tokens": ["un", "de", "Bur", ",", "de", "geiht", "in", "de", "Midd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$,", "NE", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Un de Ossen, de st\u00f6ten un riten an't Strick,", "tokens": ["Un", "de", "Os\u00b7sen", ",", "de", "st\u00f6\u00b7ten", "un", "ri\u00b7ten", "an't", "Strick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "FM", "FM", "FM", "VVFIN", "VVFIN", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "un sei riten em rin in de P\u00fctt;", "tokens": ["un", "sei", "ri\u00b7ten", "em", "rin", "in", "de", "P\u00fctt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Un de Bur, de krawwelt sick rut ut den Paul,", "tokens": ["Un", "de", "Bur", ",", "de", "kraw\u00b7welt", "sick", "rut", "ut", "den", "Paul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "ART", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "un hei stellt sick hen, freut sick un lacht:", "tokens": ["un", "hei", "stellt", "sick", "hen", ",", "freut", "sick", "un", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "\u00bbja! Un ditmal is't kamen, as ick mi't heww dr\u00f6mt,", "tokens": ["\u00bb", "ja", "!", "Un", "dit\u00b7mal", "is't", "ka\u00b7men", ",", "as", "ick", "mi't", "heww", "dr\u00f6mt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "NE", "ADV", "ADV", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja, un ditmal, as ick mi't heww dacht.\u00ab", "tokens": ["Ja", ",", "un", "dit\u00b7mal", ",", "as", "ick", "mi't", "heww", "dacht", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "FM", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}