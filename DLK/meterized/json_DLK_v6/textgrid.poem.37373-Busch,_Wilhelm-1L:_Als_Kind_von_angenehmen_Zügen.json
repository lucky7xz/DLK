{"textgrid.poem.37373": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Kind von angenehmen Z\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Kind von angenehmen Z\u00fcgen", "tokens": ["Als", "Kind", "von", "an\u00b7ge\u00b7neh\u00b7men", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War R\u00f6schen ein gar lustig Ding.", "tokens": ["War", "R\u00f6\u00b7schen", "ein", "gar", "lus\u00b7tig", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gern zupfte sie das Bein der Fliegen,", "tokens": ["Gern", "zupf\u00b7te", "sie", "das", "Bein", "der", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sie geschickt mit Spucke fing.", "tokens": ["Die", "sie", "ge\u00b7schickt", "mit", "Spu\u00b7cke", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie wuchs, und gr\u00f6\u00dfere Objekte", "tokens": ["Sie", "wuchs", ",", "und", "gr\u00f6\u00b7\u00dfe\u00b7re", "Ob\u00b7jek\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lockt sie von nun an in ihr Garn,", "tokens": ["Lockt", "sie", "von", "nun", "an", "in", "ihr", "Garn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nicht nur die jungen, nein, sie neckte", "tokens": ["Nicht", "nur", "die", "jun\u00b7gen", ",", "nein", ",", "sie", "neck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "$,", "PTKANT", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rupft auch manchen alten Narrn.", "tokens": ["Und", "rupft", "auch", "man\u00b7chen", "al\u00b7ten", "Narrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Inzwischen tat in stillem Walten", "tokens": ["I\u00b7nzwi\u00b7schen", "tat", "in", "stil\u00b7lem", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Zeit getreulich ihre Pflicht.", "tokens": ["Die", "Zeit", "ge\u00b7treu\u00b7lich", "ih\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch wundersame B\u00fcgelfalten", "tokens": ["Durch", "wun\u00b7der\u00b7sa\u00b7me", "B\u00fc\u00b7gel\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verziert sie R\u00f6schens Angesicht.", "tokens": ["Ver\u00b7ziert", "sie", "R\u00f6\u00b7schens", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und locker wurden R\u00f6schens Z\u00e4hne.", "tokens": ["Und", "lo\u00b7cker", "wur\u00b7den", "R\u00f6\u00b7schens", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Freier stellte sich mehr ein.", "tokens": ["Kein", "Frei\u00b7er", "stell\u00b7te", "sich", "mehr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schlie\u00dflich kriegt sie gar Migr\u00e4ne,", "tokens": ["Und", "schlie\u00df\u00b7lich", "kriegt", "sie", "gar", "Mig\u00b7r\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die pflegt dauerhaft zu sein.", "tokens": ["Und", "die", "pflegt", "dau\u00b7er\u00b7haft", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dies f\u00fchrte sie zum Aberglauben,", "tokens": ["Dies", "f\u00fchr\u00b7te", "sie", "zum", "A\u00b7berg\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obwohl sie sonst nicht gl\u00e4ubig schien.", "tokens": ["Ob\u00b7wohl", "sie", "sonst", "nicht", "gl\u00e4u\u00b7big", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie meinte fest, da\u00df Turteltauben", "tokens": ["Sie", "mein\u00b7te", "fest", ",", "da\u00df", "Tur\u00b7tel\u00b7tau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schmerz der Menschen an sich ziehn.", "tokens": ["Den", "Schmerz", "der", "Men\u00b7schen", "an", "sich", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zwei St\u00fcck davon hat sie im Bauer,", "tokens": ["Zwei", "St\u00fcck", "da\u00b7von", "hat", "sie", "im", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PAV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein P\u00e4rchen, welches z\u00e4rtlich girrt;", "tokens": ["Ein", "P\u00e4r\u00b7chen", ",", "wel\u00b7ches", "z\u00e4rt\u00b7lich", "girrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt liegt sie t\u00e4glich auf der Lauer,", "tokens": ["Jetzt", "liegt", "sie", "t\u00e4g\u00b7lich", "auf", "der", "Lau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ihnen noch nicht \u00fcbel wird.", "tokens": ["Ob", "ih\u00b7nen", "noch", "nicht", "\u00fc\u00b7bel", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}