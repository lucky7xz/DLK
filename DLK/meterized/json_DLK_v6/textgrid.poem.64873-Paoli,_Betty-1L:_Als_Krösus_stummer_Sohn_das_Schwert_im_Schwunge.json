{"textgrid.poem.64873": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Kr\u00f6sus stummer Sohn das Schwert im Schwunge", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Kr\u00f6sus stummer Sohn das Schwert im Schwunge", "tokens": ["Als", "Kr\u00f6\u00b7sus", "stum\u00b7mer", "Sohn", "das", "Schwert", "im", "Schwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob seines Vaters theuerm Haupt sah schweben,", "tokens": ["Ob", "sei\u00b7nes", "Va\u00b7ters", "theu\u00b7erm", "Haupt", "sah", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerri\u00df das Weh die Fessel seiner Zunge,", "tokens": ["Zer\u00b7ri\u00df", "das", "Weh", "die", "Fes\u00b7sel", "sei\u00b7ner", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und flehend rief er: \u00bbSchont des K\u00f6nigs Leben!\u00ab", "tokens": ["Und", "fle\u00b7hend", "rief", "er", ":", "\u00bb", "Schont", "des", "K\u00f6\u00b7nigs", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "$(", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ist in meiner Brust auch lange, lange", "tokens": ["So", "ist", "in", "mei\u00b7ner", "Brust", "auch", "lan\u00b7ge", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Leidenschaft verh\u00fcllt und stumm geblieben,", "tokens": ["Die", "Lei\u00b7den\u00b7schaft", "ver\u00b7h\u00fcllt", "und", "stumm", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht folgte ich der Seele m\u00e4cht'gem Drange,", "tokens": ["Nicht", "folg\u00b7te", "ich", "der", "See\u00b7le", "m\u00e4cht'\u00b7gem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu sprechen dir von meinem tiefen Lieben.", "tokens": ["Zu", "spre\u00b7chen", "dir", "von", "mei\u00b7nem", "tie\u00b7fen", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch jetzo, da der Augenblick gekommen,", "tokens": ["Doch", "jet\u00b7zo", ",", "da", "der", "Au\u00b7gen\u00b7blick", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der irdisch ew'ge Trennung uns soll bringen,", "tokens": ["Der", "ir\u00b7disch", "ew'\u00b7ge", "Tren\u00b7nung", "uns", "soll", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will mir mein stolzes Schweigen nicht mehr frommen,", "tokens": ["Will", "mir", "mein", "stol\u00b7zes", "Schwei\u00b7gen", "nicht", "mehr", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Angst der Seele l\u00e4\u00dft sich nicht bezwingen.", "tokens": ["Die", "Angst", "der", "See\u00b7le", "l\u00e4\u00dft", "sich", "nicht", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt, da erschienen ist die n\u00e4cht'ge Stunde", "tokens": ["Jetzt", ",", "da", "er\u00b7schie\u00b7nen", "ist", "die", "n\u00e4cht'\u00b7ge", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo du f\u00fcr stets mir sollst entrissen werden,", "tokens": ["Wo", "du", "f\u00fcr", "stets", "mir", "sollst", "ent\u00b7ris\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADV", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ringt sich der Schrei vom langverstummten Munde:", "tokens": ["Ringt", "sich", "der", "Schrei", "vom", "lang\u00b7ver\u00b7stumm\u00b7ten", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbich liebe dich wie weiter nichts auf Erden.\u00ab", "tokens": ["\u00bb", "ich", "lie\u00b7be", "dich", "wie", "wei\u00b7ter", "nichts", "auf", "Er\u00b7den", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "KOKOM", "ADV", "PIS", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbich liebe dich am unruhvollen Tage", "tokens": ["\u00bb", "ich", "lie\u00b7be", "dich", "am", "un\u00b7ruh\u00b7vol\u00b7len", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in der N\u00e4chte stillen Einsamkeiten,", "tokens": ["Und", "in", "der", "N\u00e4ch\u00b7te", "stil\u00b7len", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit jedem Blick, mit jedem Herzensschlage,", "tokens": ["Mit", "je\u00b7dem", "Blick", ",", "mit", "je\u00b7dem", "Her\u00b7zens\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich liebe dich f\u00fcr alle Ewigkeiten.\u00ab", "tokens": ["Ich", "lie\u00b7be", "dich", "f\u00fcr", "al\u00b7le", "E\u00b7wig\u00b7kei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch weh! nicht wie der lyd'sche F\u00fcrst beschirmen", "tokens": ["Doch", "weh", "!", "nicht", "wie", "der", "ly\u00b7d'\u00b7sche", "F\u00fcrst", "be\u00b7schir\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kann ich mein Gl\u00fcck vom grausen Todesstreiche;", "tokens": ["Kann", "ich", "mein", "Gl\u00fcck", "vom", "grau\u00b7sen", "To\u00b7des\u00b7strei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Ruf drang siegreich durch des Schicksals St\u00fcrmen: \u2013", "tokens": ["Sein", "Ruf", "drang", "sieg\u00b7reich", "durch", "des", "Schick\u00b7sals", "St\u00fcr\u00b7men", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der meine klagt nur mehr an einer Leiche.", "tokens": ["Der", "mei\u00b7ne", "klagt", "nur", "mehr", "an", "ei\u00b7ner", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}