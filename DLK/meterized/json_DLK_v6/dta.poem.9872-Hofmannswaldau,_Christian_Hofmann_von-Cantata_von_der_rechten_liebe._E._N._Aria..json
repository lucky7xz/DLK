{"dta.poem.9872": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Cantata von der rechten liebe.  \n E. N.  \n  Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist s\u00fcsser als das lieben/", "tokens": ["Nichts", "ist", "s\u00fcs\u00b7ser", "als", "das", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "KOKOM", "PDS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieben ist ein himmelreich;", "tokens": ["Lie\u00b7ben", "ist", "ein", "him\u00b7mel\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Menschen/ die das wesen \u00fcben/", "tokens": ["Men\u00b7schen", "/", "die", "das", "we\u00b7sen", "\u00fc\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind dadurch den g\u00f6ttern gleich.", "tokens": ["Sind", "da\u00b7durch", "den", "g\u00f6t\u00b7tern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja zwey recht vertraute hertzen", "tokens": ["Ja", "zwey", "recht", "ver\u00b7trau\u00b7te", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "CARD", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sind zwey engel auff der welt/", "tokens": ["Sind", "zwey", "en\u00b7gel", "auff", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Weil ihr angenehmes schertzen", "tokens": ["Weil", "ihr", "an\u00b7ge\u00b7neh\u00b7mes", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott und menschen wohlgef\u00e4llt.", "tokens": ["Gott", "und", "men\u00b7schen", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wiewohl die liebe mu\u00df auf rechtem fusse stehn.", "tokens": ["Wie\u00b7wohl", "die", "lie\u00b7be", "mu\u00df", "auf", "rech\u00b7tem", "fus\u00b7se", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo keine treu/ wo keine keuschheit ist/", "tokens": ["Wo", "kei\u00b7ne", "treu", "/", "wo", "kei\u00b7ne", "keuschheit", "ist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJD", "$(", "PWAV", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.11": {"text": "Wo man das tugend-ziel vergist/", "tokens": ["Wo", "man", "das", "tu\u00b7gen\u00b7dziel", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da mu\u00df die sch\u00f6ne lust zergehn.", "tokens": ["Da", "mu\u00df", "die", "sch\u00f6\u00b7ne", "lust", "zer\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und was ein himmel heist", "tokens": ["Und", "was", "ein", "him\u00b7mel", "heist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Mu\u00df eine h\u00f6lle werden.", "tokens": ["Mu\u00df", "ei\u00b7ne", "h\u00f6l\u00b7le", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Jedoch ein reiner geist", "tokens": ["Je\u00b7doch", "ein", "rei\u00b7ner", "geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Befleckt sich nicht.", "tokens": ["Be\u00b7fleckt", "sich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Gedancken und geberden", "tokens": ["Ge\u00b7dan\u00b7cken", "und", "ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.18": {"text": "Sind tugendhafft und edel eingericht.", "tokens": ["Sind", "tu\u00b7gend\u00b7hafft", "und", "e\u00b7del", "ein\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die k\u00fcsse sind die seele bey dem lieben/", "tokens": ["Die", "k\u00fcs\u00b7se", "sind", "die", "see\u00b7le", "bey", "dem", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wann diese rein geblieben/", "tokens": ["Wann", "die\u00b7se", "rein", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "So mu\u00df die seele leben/", "tokens": ["So", "mu\u00df", "die", "see\u00b7le", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und tausendfache lust verliebten c\u00f6rpern geben.", "tokens": ["Und", "tau\u00b7send\u00b7fa\u00b7che", "lust", "ver\u00b7lieb\u00b7ten", "c\u00f6r\u00b7pern", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}