{"textgrid.poem.37545": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nur leise", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr h\u00e4ufig traf Studiosus D\u00f6ppe", "tokens": ["Sehr", "h\u00e4u\u00b7fig", "traf", "Stu\u00b7di\u00b7o\u00b7sus", "D\u00f6p\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Paulinen auf des Hauses Treppe,", "tokens": ["Pau\u00b7li\u00b7nen", "auf", "des", "Hau\u00b7ses", "Trep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie als Witwe tugendsam", "tokens": ["Wenn", "sie", "als", "Wit\u00b7we", "tu\u00b7gend\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Morgens aus der Stube kam.", "tokens": ["Des", "Mor\u00b7gens", "aus", "der", "Stu\u00b7be", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sie Besitzerin vom Haus,", "tokens": ["Da", "sie", "Be\u00b7sit\u00b7ze\u00b7rin", "vom", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sprach sich D\u00f6ppe schlie\u00dflich aus", "tokens": ["So", "sprach", "sich", "D\u00f6p\u00b7pe", "schlie\u00df\u00b7lich", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NE", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bat mit Liebe und Empfindung", "tokens": ["Und", "bat", "mit", "Lie\u00b7be", "und", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um eine dauernde Verbindung.", "tokens": ["Um", "ei\u00b7ne", "dau\u00b7ern\u00b7de", "Ver\u00b7bin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbherr D\u00f6ppe\u00ab, sprach Pauline k\u00fchl,", "tokens": ["\u00bb", "herr", "D\u00f6p\u00b7pe", "\u00ab", ",", "sprach", "Pau\u00b7li\u00b7ne", "k\u00fchl", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$(", "$,", "VVFIN", "NE", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbich ehr und achte Ihr Gef\u00fchl,", "tokens": ["\u00bb", "ich", "ehr", "und", "ach\u00b7te", "Ihr", "Ge\u00b7f\u00fchl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "KON", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dies Gepolter auf der Treppe,", "tokens": ["Doch", "dies", "Ge\u00b7pol\u00b7ter", "auf", "der", "Trep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fast jede Nacht, ist b\u00f6s, Herr D\u00f6ppe!\u00ab", "tokens": ["Fast", "je\u00b7de", "Nacht", ",", "ist", "b\u00f6s", ",", "Herr", "D\u00f6p\u00b7pe", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "VAFIN", "ADJD", "$,", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Worauf denn D\u00f6ppe fest beschwor,", "tokens": ["Wo\u00b7rauf", "denn", "D\u00f6p\u00b7pe", "fest", "be\u00b7schwor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sache k\u00e4me nicht mehr vor.", "tokens": ["Die", "Sa\u00b7che", "k\u00e4\u00b7me", "nicht", "mehr", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dies Schw\u00f6ren sollte wenig n\u00fctzen.", "tokens": ["Dies", "Schw\u00f6\u00b7ren", "soll\u00b7te", "we\u00b7nig", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachts hat er wieder einen sitzen.", "tokens": ["Nachts", "hat", "er", "wie\u00b7der", "ei\u00b7nen", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er kommt nach Haus in sp\u00e4ter Stund", "tokens": ["Er", "kommt", "nach", "Haus", "in", "sp\u00e4\u00b7ter", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Pfeife, Rausch und Pudelhund.", "tokens": ["Mit", "Pfei\u00b7fe", ",", "Rausch", "und", "Pu\u00b7del\u00b7hund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Behutsam zieht er auf dem Gang", "tokens": ["Be\u00b7hut\u00b7sam", "zieht", "er", "auf", "dem", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stiefel aus, die schwer und lang,", "tokens": ["Die", "Stie\u00b7fel", "aus", ",", "die", "schwer", "und", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Um auf den Socken, auf den weichen,", "tokens": ["Um", "auf", "den", "So\u00b7cken", ",", "auf", "den", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ger\u00e4uschlos sich emporzuschleichen.", "tokens": ["Ge\u00b7r\u00e4uschlos", "sich", "em\u00b7por\u00b7zu\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fast ist er schon dem Gipfel nah", "tokens": ["Fast", "ist", "er", "schon", "dem", "Gip\u00b7fel", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denkt, der letzte Tritt ist da.", "tokens": ["Und", "denkt", ",", "der", "letz\u00b7te", "Tritt", "ist", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dies denkt er aber ohne Grund.", "tokens": ["Dies", "denkt", "er", "a\u00b7ber", "oh\u00b7ne", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pfeife bohrt sich in den Schlund.", "tokens": ["Die", "Pfei\u00b7fe", "bohrt", "sich", "in", "den", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die alte Treppe knackt und knirrt,", "tokens": ["Die", "al\u00b7te", "Trep\u00b7pe", "knackt", "und", "knirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pfeife l\u00f6st sich auf und klirrt;", "tokens": ["Die", "Pfei\u00b7fe", "l\u00f6st", "sich", "auf", "und", "klirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Erschrecklich t\u00f6nt der Stiefel Krach,", "tokens": ["Er\u00b7schreck\u00b7lich", "t\u00f6nt", "der", "Stie\u00b7fel", "Krach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dumpf rumpelt D\u00f6ppe hinten nach.", "tokens": ["Dumpf", "rum\u00b7pelt", "D\u00f6p\u00b7pe", "hin\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Pudel heult und ist verletzt,", "tokens": ["Der", "Pu\u00b7del", "heult", "und", "ist", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil D\u00f6ppe seinen Schwanz besetzt.", "tokens": ["Weil", "D\u00f6p\u00b7pe", "sei\u00b7nen", "Schwanz", "be\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pauline kommt mit Kerzenlicht;", "tokens": ["Pau\u00b7li\u00b7ne", "kommt", "mit", "Ker\u00b7zen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Besch\u00e4mt verbirgt er sein Gesicht.", "tokens": ["Be\u00b7sch\u00e4mt", "ver\u00b7birgt", "er", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man h\u00f6rt nichts weiter von Paulinen", "tokens": ["Man", "h\u00f6rt", "nichts", "wei\u00b7ter", "von", "Pau\u00b7li\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als: \u00bbD\u00f6ppe, ich verachte Ihnen!\u00ab", "tokens": ["Als", ":", "\u00bb", "D\u00f6p\u00b7pe", ",", "ich", "ver\u00b7ach\u00b7te", "Ih\u00b7nen", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$.", "$(", "NE", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}