{"dta.poem.21043": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Odalia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unser Leben verwallt, meine Odalia!               ", "tokens": ["Un\u00b7ser", "Le\u00b7ben", "ver\u00b7wallt", ",", "mei\u00b7ne", "O\u00b7da\u00b7lia", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NE", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Unser Jubel erstummt, unser Gejammer schweigt,", "tokens": ["Un\u00b7ser", "Ju\u00b7bel", "er\u00b7stummt", ",", "un\u00b7ser", "Ge\u00b7jam\u00b7mer", "schweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie ein L\u00e4cheln im Antlitz,", "tokens": ["Wie", "ein", "L\u00e4\u00b7cheln", "im", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie ein \u00c4chzen in weiter Luft.", "tokens": ["Wie", "ein", "\u00c4ch\u00b7zen", "in", "wei\u00b7ter", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Tage schmelzen wie Schnee, Monden wie Schlossen", "tokens": ["Ta\u00b7ge", "schmel\u00b7zen", "wie", "Schnee", ",", "Mon\u00b7den", "wie", "Schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "KOKOM", "NN", "$,", "NN", "KOKOM", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "hin;", "tokens": ["hin", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Jahre schwinden wie Hauch. Wieder verrollt ist eins,", "tokens": ["Jah\u00b7re", "schwin\u00b7den", "wie", "Hauch", ".", "Wie\u00b7der", "ver\u00b7rollt", "ist", "eins", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "$.", "ADV", "VVPP", "VAFIN", "PIS", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Eins der sch\u00f6nsten, Geliebte,", "tokens": ["Eins", "der", "sch\u00f6ns\u00b7ten", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die mir schwanden im Schwung der Zeit.", "tokens": ["Die", "mir", "schwan\u00b7den", "im", "Schwung", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dich, Odalia, dich f\u00fchrte das freundliche", "tokens": ["Dich", ",", "O\u00b7da\u00b7lia", ",", "dich", "f\u00fchr\u00b7te", "das", "freund\u00b7li\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Schon verscheidende Jahr mir in den heissen Arm,", "tokens": ["Schon", "ver\u00b7schei\u00b7den\u00b7de", "Jahr", "mir", "in", "den", "heis\u00b7sen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Dich, du Reine und Milde,", "tokens": ["Dich", ",", "du", "Rei\u00b7ne", "und", "Mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Dich, du holde Vertrauliche!", "tokens": ["Dich", ",", "du", "hol\u00b7de", "Ver\u00b7trau\u00b7li\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Manches selige mal sah es mich frey und froh,", "tokens": ["Man\u00b7ches", "se\u00b7li\u00b7ge", "mal", "sah", "es", "mich", "frey", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dir und Rosen am Arm, wandeln auf stiller Flur,", "tokens": ["Dir", "und", "Ro\u00b7sen", "am", "Arm", ",", "wan\u00b7deln", "auf", "stil\u00b7ler", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Zwischen Blumen des Fr\u00fchlings,", "tokens": ["Zwi\u00b7schen", "Blu\u00b7men", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zwischen Herbstesverwelkungen.", "tokens": ["Zwi\u00b7schen", "Herbs\u00b7tes\u00b7ver\u00b7wel\u00b7kun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manches leise Gef\u00fchl f\u00e4rbte die Wange dir;", "tokens": ["Man\u00b7ches", "lei\u00b7se", "Ge\u00b7f\u00fchl", "f\u00e4rb\u00b7te", "die", "Wan\u00b7ge", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Manches d\u00e4mmernde Weh tr\u00fcbte dein blaues Aug'.", "tokens": ["Man\u00b7ches", "d\u00e4m\u00b7mern\u00b7de", "Weh", "tr\u00fcb\u00b7te", "dein", "blau\u00b7es", "Aug'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "S\u00e4usel fassten dich, Milde!", "tokens": ["S\u00e4u\u00b7sel", "fass\u00b7ten", "dich", ",", "Mil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "St\u00fcrme Gottes mich Wilderen!", "tokens": ["St\u00fcr\u00b7me", "Got\u00b7tes", "mich", "Wil\u00b7de\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Selten haucht' ich es aus, was mir den Busen hob;", "tokens": ["Sel\u00b7ten", "haucht'", "ich", "es", "aus", ",", "was", "mir", "den", "Bu\u00b7sen", "hob", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Selten riss es sich los, was mir im Herzen rang.", "tokens": ["Sel\u00b7ten", "riss", "es", "sich", "los", ",", "was", "mir", "im", "Her\u00b7zen", "rang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn ich hass', es zu sagen,", "tokens": ["Denn", "ich", "hass'", ",", "es", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Was der Rede zu m\u00e4chtig ist.", "tokens": ["Was", "der", "Re\u00b7de", "zu", "m\u00e4ch\u00b7tig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKA", "ADJD", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Ohne Red' und Gesang fasst uns der Edlere.", "tokens": ["Oh\u00b7ne", "Red'", "und", "Ge\u00b7sang", "fasst", "uns", "der", "Ed\u00b7le\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auch mit Red' und Gesang fasst uns der Rohe nicht.", "tokens": ["Auch", "mit", "Red'", "und", "Ge\u00b7sang", "fasst", "uns", "der", "Ro\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gleichbesaitete Herzen", "tokens": ["Gleich\u00b7be\u00b7sai\u00b7te\u00b7te", "Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ahnden, suchen, erkennen sich.", "tokens": ["Ahn\u00b7den", ",", "su\u00b7chen", ",", "er\u00b7ken\u00b7nen", "sich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Schweigend sass ich bey dir, meine Odalia;", "tokens": ["Schwei\u00b7gend", "sass", "ich", "bey", "dir", ",", "mei\u00b7ne", "O\u00b7da\u00b7lia", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Stumm und schweigend bey dir unter dem Bogengang,", "tokens": ["Stumm", "und", "schwei\u00b7gend", "bey", "dir", "un\u00b7ter", "dem", "Bo\u00b7gen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Im Ged\u00e4mmer des Abends", "tokens": ["Im", "Ge\u00b7d\u00e4m\u00b7mer", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Im wehm\u00fcthigen Mondenschein.", "tokens": ["Im", "weh\u00b7m\u00fct\u00b7hi\u00b7gen", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Schweigend stand ich bey dir unter den grausenden", "tokens": ["Schwei\u00b7gend", "stand", "ich", "bey", "dir", "un\u00b7ter", "den", "grau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Burggew\u00f6lben. Ihr wart, grausende Tr\u00fcmmer, mir", "tokens": ["Burg\u00b7ge\u00b7w\u00f6l\u00b7ben", ".", "Ihr", "wart", ",", "grau\u00b7sen\u00b7de", "Tr\u00fcm\u00b7mer", ",", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "An Odaliens Busen", "tokens": ["An", "O\u00b7da\u00b7li\u00b7ens", "Bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ein krystallenes Feyenschloss.", "tokens": ["Ein", "krys\u00b7tal\u00b7le\u00b7nes", "Fe\u00b7yensc\u00b7hloss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Schweigend sahest du mich, moosiger Golchaberg,", "tokens": ["Schwei\u00b7gend", "sa\u00b7hest", "du", "mich", ",", "moo\u00b7si\u00b7ger", "Golc\u00b7ha\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "(durch den herbstlichen Flor weinte die bleiche Sonn')", "tokens": ["(", "durch", "den", "herbst\u00b7li\u00b7chen", "Flor", "wein\u00b7te", "die", "blei\u00b7che", "Sonn'", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sahst mich gl\u00fchend und schweigend", "tokens": ["Sahst", "mich", "gl\u00fc\u00b7hend", "und", "schwei\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In Odaliens Armen ruhn. \u2014", "tokens": ["In", "O\u00b7da\u00b7li\u00b7ens", "Ar\u00b7men", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Denk, Odalia, mein, wenn du auf Fluren wallst,", "tokens": ["Denk", ",", "O\u00b7da\u00b7lia", ",", "mein", ",", "wenn", "du", "auf", "Flu\u00b7ren", "wallst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PPOSAT", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo ich wallte mit dir, unter dem Bogengang,", "tokens": ["Wo", "ich", "wall\u00b7te", "mit", "dir", ",", "un\u00b7ter", "dem", "Bo\u00b7gen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch die Flieder des Schlosses,", "tokens": ["Durch", "die", "Flie\u00b7der", "des", "Schlos\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf dem heiligen Golchaberg.", "tokens": ["Auf", "dem", "hei\u00b7li\u00b7gen", "Golc\u00b7ha\u00b7berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Denk, Odalia, mein, bis du dich selbst verlierst,", "tokens": ["Denk", ",", "O\u00b7da\u00b7lia", ",", "mein", ",", "bis", "du", "dich", "selbst", "ver\u00b7lierst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PPOSAT", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Bis der R\u00e4uber dein Herz raubet, dem keins entrann:", "tokens": ["Bis", "der", "R\u00e4u\u00b7ber", "dein", "Herz", "rau\u00b7bet", ",", "dem", "keins", "ent\u00b7rann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dann vergiss mich, Geliebte;", "tokens": ["Dann", "ver\u00b7giss", "mich", ",", "Ge\u00b7lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Denn ich hass' es, der Zweyte n.", "tokens": ["Denn", "ich", "hass'", "es", ",", "der", "Zwey\u00b7te", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Mehr, denn starrenden Frost, hass' ich den lauen Sinn.", "tokens": ["Mehr", ",", "denn", "star\u00b7ren\u00b7den", "Frost", ",", "hass'", "ich", "den", "lau\u00b7en", "Sinn", "."], "token_info": ["word", "punct", "word", "word