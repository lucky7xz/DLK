{"textgrid.poem.48016": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4trot", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldene Sonne, kaum gesunken,", "tokens": ["Gol\u00b7de\u00b7ne", "Son\u00b7ne", ",", "kaum", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "wolken\u00fcberschattet Gl\u00fcck,", "tokens": ["wol\u00b7ken\u00b7\u00fc\u00b7bersc\u00b7hat\u00b7tet", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zauberst du in Sp\u00e4trotfunken", "tokens": ["zau\u00b7berst", "du", "in", "Sp\u00e4t\u00b7rot\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen sch\u00f6neren Tag zur\u00fcck?", "tokens": ["ei\u00b7nen", "sch\u00f6\u00b7ne\u00b7ren", "Tag", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Holde Jugend, kehrst du wieder? \u2013", "tokens": ["Hol\u00b7de", "Ju\u00b7gend", ",", "kehrst", "du", "wie\u00b7der", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Abends Rosenschein", "tokens": ["In", "des", "A\u00b7bends", "Ro\u00b7sen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "fallen mir verklungne Lieder,", "tokens": ["fal\u00b7len", "mir", "ver\u00b7klung\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "halb vergessne Weisen ein:", "tokens": ["halb", "ver\u00b7gess\u00b7ne", "Wei\u00b7sen", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seufzer, K\u00fcsse, leise Klagen,", "tokens": ["Seuf\u00b7zer", ",", "K\u00fcs\u00b7se", ",", "lei\u00b7se", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebeslust und Liebesnot \u2013", "tokens": ["Lie\u00b7bes\u00b7lust", "und", "Lie\u00b7bes\u00b7not", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df des Herzens hei\u00dfes Schlagen", "tokens": ["da\u00df", "des", "Her\u00b7zens", "hei\u00b7\u00dfes", "Schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mir die Brust zu sprengen droht!", "tokens": ["mir", "die", "Brust", "zu", "spren\u00b7gen", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sp\u00e4trotschein und solche Lieder . . .!", "tokens": ["Sp\u00e4t\u00b7rot\u00b7schein", "und", "sol\u00b7che", "Lie\u00b7der", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "KON", "PIAT", "NN", "$.", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es geht mir durch den Sinn", "tokens": ["Und", "es", "geht", "mir", "durch", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "leise Mahnung, da\u00df ich wieder", "tokens": ["lei\u00b7se", "Mah\u00b7nung", ",", "da\u00df", "ich", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "unverzeihlich t\u00f6richt bin.", "tokens": ["un\u00b7ver\u00b7zeih\u00b7lich", "t\u00f6\u00b7richt", "bin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}