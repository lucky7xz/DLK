{"textgrid.poem.36592": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Sch\u00f6nen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einer Rose schlief", "tokens": ["Auf", "ei\u00b7ner", "Ro\u00b7se", "schlief"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "J\u00fcngst eine Biene tief,", "tokens": ["J\u00fcngst", "ei\u00b7ne", "Bie\u00b7ne", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Amor sah sie nicht!", "tokens": ["Und", "A\u00b7mor", "sah", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er r\u00fchrt sie an, sie sticht!", "tokens": ["Er", "r\u00fchrt", "sie", "an", ",", "sie", "sticht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er schreit: o weh! o weh!", "tokens": ["Er", "schreit", ":", "o", "weh", "!", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "FM", "PTKVZ", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sieht meine Lalage,", "tokens": ["Sieht", "mei\u00b7ne", "La\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schreit! ach Mama, Mama!", "tokens": ["Schreit", "!", "ach", "Ma\u00b7ma", ",", "Ma\u00b7ma", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "XY", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die kleine Schlange da", "tokens": ["Die", "klei\u00b7ne", "Schlan\u00b7ge", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mit Fittichen, die sie", "tokens": ["Mit", "Fit\u00b7ti\u00b7chen", ",", "die", "sie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Hinfliegen sehen, die", "tokens": ["Hin\u00b7flie\u00b7gen", "se\u00b7hen", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$,", "PRELS"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Der Landmann Biene nennt,", "tokens": ["Der", "Land\u00b7mann", "Bie\u00b7ne", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Stach meine Hand! Es brennt", "tokens": ["Stach", "mei\u00b7ne", "Hand", "!", "Es", "brennt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Entsetzlich! Armes Kind,", "tokens": ["Ent\u00b7setz\u00b7lich", "!", "Ar\u00b7mes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Spricht Lalage, \u2013 Geschwind", "tokens": ["Spricht", "La\u00b7la\u00b7ge", ",", "\u2013", "Ge\u00b7schwind"], "token_info": ["word", "word", "punct", "punct", "word"], "pos": ["VVFIN", "NN", "$,", "$(", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Dein H\u00e4ndchen her, und k\u00fc\u00dft,", "tokens": ["Dein", "H\u00e4nd\u00b7chen", "her", ",", "und", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Bis es geheilet ist.", "tokens": ["Bis", "es", "ge\u00b7hei\u00b7let", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ihr Sch\u00f6nen, welchen Lohn", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", ",", "wel\u00b7chen", "Lohn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Gab ihr Cytherens Sohn,", "tokens": ["Gab", "ihr", "Cy\u00b7the\u00b7rens", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "F\u00fcr den geheilten Stich?", "tokens": ["F\u00fcr", "den", "ge\u00b7heil\u00b7ten", "Stich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Er gab, Er gab ihr mich!", "tokens": ["Er", "gab", ",", "Er", "gab", "ihr", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}