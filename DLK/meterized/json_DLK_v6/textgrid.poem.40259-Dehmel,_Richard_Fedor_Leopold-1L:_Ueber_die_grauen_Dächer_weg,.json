{"textgrid.poem.40259": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber die grauen D\u00e4cher weg,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber die grauen D\u00e4cher weg,", "tokens": ["Ue\u00b7ber", "die", "grau\u00b7en", "D\u00e4\u00b7cher", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "hoch hier oben,", "tokens": ["hoch", "hier", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "durch die langen roten Nelken,", "tokens": ["durch", "die", "lan\u00b7gen", "ro\u00b7ten", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die vor meinem offnen Fenster", "tokens": ["die", "vor", "mei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "leise zwischen mir", "tokens": ["lei\u00b7se", "zwi\u00b7schen", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "und dem blauen Abendhimmel schwanken,", "tokens": ["und", "dem", "blau\u00b7en", "A\u00b7bend\u00b7him\u00b7mel", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "will mein Auge,", "tokens": ["will", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "will meine Seele", "tokens": ["will", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "hinaus, hinauf.", "tokens": ["hin\u00b7aus", ",", "hin\u00b7auf", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Um die h\u00f6chste goldene Kirchturmkugel,", "tokens": ["Um", "die", "h\u00f6chs\u00b7te", "gol\u00b7de\u00b7ne", "Kirch\u00b7turm\u00b7ku\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "im letzten fernen Lichte,", "tokens": ["im", "letz\u00b7ten", "fer\u00b7nen", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mit hellen Fl\u00fcgeln,", "tokens": ["mit", "hel\u00b7len", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "zieht ein Taubenschwarm", "tokens": ["zieht", "ein", "Tau\u00b7ben\u00b7schwarm"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "zitternde Kreise", "tokens": ["zit\u00b7tern\u00b7de", "Krei\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00fcber dem Hause", "tokens": ["\u00fc\u00b7ber", "dem", "Hau\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "meiner Geliebten.", "tokens": ["mei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus dem blassen Westen", "tokens": ["Aus", "dem", "blas\u00b7sen", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "will der erste Stern und \u00fcberflimmert", "tokens": ["will", "der", "ers\u00b7te", "Stern", "und", "\u00fc\u00b7berf\u00b7lim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "KON", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "scheu den lauten Dunst und tr\u00fcben L\u00e4rm", "tokens": ["scheu", "den", "lau\u00b7ten", "Dunst", "und", "tr\u00fc\u00b7ben", "L\u00e4rm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "der gro\u00dfen Stadt hier unten,", "tokens": ["der", "gro\u00b7\u00dfen", "Stadt", "hier", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wie der erste, winkende Traumgedanke", "tokens": ["wie", "der", "ers\u00b7te", ",", "win\u00b7ken\u00b7de", "Traum\u00b7ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "aus dem wirren Schwarm der Lebensfragen", "tokens": ["aus", "dem", "wir\u00b7ren", "Schwarm", "der", "Le\u00b7bens\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "in der Seele des M\u00fcden taucht \u2013", "tokens": ["in", "der", "See\u00b7le", "des", "M\u00fc\u00b7den", "taucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "da klopft es.", "tokens": ["da", "klopft", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Klopft und ist auch schon im St\u00fcbchen,", "tokens": ["Klopft", "und", "ist", "auch", "schon", "im", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sitzt mir auf dem Stuhle gegen\u00fcber,", "tokens": ["sitzt", "mir", "auf", "dem", "Stuh\u00b7le", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "sagt kein Wort, und nur die roten Lippen", "tokens": ["sagt", "kein", "Wort", ",", "und", "nur", "die", "ro\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "unterm schwarzen Ringelhaar", "tokens": ["un\u00b7term", "schwar\u00b7zen", "Rin\u00b7gel\u00b7haar"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "winken roter als die rote Bluse", "tokens": ["win\u00b7ken", "ro\u00b7ter", "als", "die", "ro\u00b7te", "Blu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "auf den scheuen Knospen ihres Busens;", "tokens": ["auf", "den", "scheu\u00b7en", "Knos\u00b7pen", "ih\u00b7res", "Bu\u00b7sens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und ich sage auch nichts.", "tokens": ["und", "ich", "sa\u00b7ge", "auch", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihre schwarzen Augensterne zittern", "tokens": ["Ih\u00b7re", "schwar\u00b7zen", "Au\u00b7gens\u00b7ter\u00b7ne", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "durch die stumme D\u00e4mmerung des St\u00fcbchens", "tokens": ["durch", "die", "stum\u00b7me", "D\u00e4m\u00b7me\u00b7rung", "des", "St\u00fcb\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "hoch hier oben", "tokens": ["hoch", "hier", "o\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "einen s\u00fc\u00dfen jungen Evablick", "tokens": ["ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "jun\u00b7gen", "E\u00b7vab\u00b7lick"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "nach den langen roten Nelken hin;", "tokens": ["nach", "den", "lan\u00b7gen", "ro\u00b7ten", "Nel\u00b7ken", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "ihre Augen!", "tokens": ["ih\u00b7re", "Au\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Und ich angle nach ihr mit den Beinen,", "tokens": ["Und", "ich", "ang\u00b7le", "nach", "ihr", "mit", "den", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "diesen Perpendikeln meines Herzens:", "tokens": ["die\u00b7sen", "Per\u00b7pen\u00b7di\u00b7keln", "mei\u00b7nes", "Her\u00b7zens", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kleine, merkst du,", "tokens": ["Klei\u00b7ne", ",", "merkst", "du", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PPER", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.4": {"text": "was die Uhr geschlagen hat? \u2013", "tokens": ["was", "die", "Uhr", "ge\u00b7schla\u00b7gen", "hat", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}