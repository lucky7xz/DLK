{"dta.poem.8436": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Laura .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Der Seele Dunkel! Nimmer, ach nimmer wird", "tokens": ["Der", "See\u00b7le", "Dun\u00b7kel", "!", "Nim\u00b7mer", ",", "ach", "nim\u00b7mer", "wird"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "NE", "$,", "ITJ", "ADV", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Auge, Laura, meinem Auge", "tokens": ["Dein", "Au\u00b7ge", ",", "Lau\u00b7ra", ",", "mei\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wieder begegnen, und Liebe sprechen!", "tokens": ["Wie\u00b7der", "be\u00b7geg\u00b7nen", ",", "und", "Lie\u00b7be", "spre\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dein ehrner Fusstritt hallte mir oft, o Tod!", "tokens": ["Dein", "ehr\u00b7ner", "Fus\u00b7stritt", "hall\u00b7te", "mir", "oft", ",", "o", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In meiner Kindheit tagender D\u00e4mmerung,", "tokens": ["In", "mei\u00b7ner", "Kind\u00b7heit", "ta\u00b7gen\u00b7der", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und manche Mutterthr\u00e4ne rann mir", "tokens": ["Und", "man\u00b7che", "Mut\u00b7ter\u00b7thr\u00e4\u00b7ne", "rann", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf die verbl\u00fchende Knabenwange.", "tokens": ["Auf", "die", "ver\u00b7bl\u00fc\u00b7hen\u00b7de", "Kna\u00b7ben\u00b7wan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Komm endlich, Tr\u00f6ster, welcher den Sterblichen", "tokens": ["Komm", "end\u00b7lich", ",", "Tr\u00f6s\u00b7ter", ",", "wel\u00b7cher", "den", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Ketten abl\u00f6st, komm und entfessle mich,", "tokens": ["Die", "Ket\u00b7ten", "ab\u00b7l\u00f6st", ",", "komm", "und", "ent\u00b7fess\u00b7le", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Wonnetod! Dann schweb' ich Lauren,", "tokens": ["O", "Won\u00b7ne\u00b7tod", "!", "Dann", "schweb'", "ich", "Lau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lauren entgegen, und bin ihr Engel!", "tokens": ["Lau\u00b7ren", "ent\u00b7ge\u00b7gen", ",", "und", "bin", "ihr", "En\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Du sollst getr\u00f6stet werden, du Weinender!", "tokens": ["Du", "sollst", "ge\u00b7tr\u00f6s\u00b7tet", "wer\u00b7den", ",", "du", "Wei\u00b7nen\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ruft, Palmen tragend, freundlich um Mitternacht", "tokens": ["Ruft", ",", "Pal\u00b7men", "tra\u00b7gend", ",", "freund\u00b7lich", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "VVPP", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Tod; mir schallt der Sterbeklocke", "tokens": ["Der", "Tod", ";", "mir", "schallt", "der", "Ster\u00b7be\u00b7klo\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dumpfes Gel\u00e4ut, und des Grabes Schaufel.", "tokens": ["Dum\u00b7pfes", "Ge\u00b7l\u00e4ut", ",", "und", "des", "Gra\u00b7bes", "Schau\u00b7fel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Bald schweb' ich sch\u00fczend, Wonne mir! Wonne mir!", "tokens": ["Bald", "schweb'", "ich", "sch\u00fc\u00b7zend", ",", "Won\u00b7ne", "mir", "!", "Won\u00b7ne", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "$.", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um meine Laura; str\u00f6me, wo Laura kniet,", "tokens": ["Um", "mei\u00b7ne", "Lau\u00b7ra", ";", "str\u00f6\u00b7me", ",", "wo", "Lau\u00b7ra", "kniet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Anbetung \u00fcber sie und Andacht,", "tokens": ["An\u00b7be\u00b7tung", "\u00fc\u00b7ber", "sie", "und", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wann sie vom Kelche des Bundes trinket;", "tokens": ["Wann", "sie", "vom", "Kel\u00b7che", "des", "Bun\u00b7des", "trin\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und s\u00fcssre Schauer, Schauer der Serafim", "tokens": ["Und", "s\u00fcss\u00b7re", "Schau\u00b7er", ",", "Schau\u00b7er", "der", "Se\u00b7ra\u00b7fim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "NE", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Throne Gottes, wann sie den Preisgesang,", "tokens": ["Am", "Thro\u00b7ne", "Got\u00b7tes", ",", "wann", "sie", "den", "Preis\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "PWAV", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Maienfr\u00fchroth angel\u00e4chelt,", "tokens": ["Vom", "Mai\u00b7en\u00b7fr\u00fch\u00b7roth", "an\u00b7ge\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dem begeisterten Herzen t\u00f6net!", "tokens": ["Aus", "dem", "be\u00b7geis\u00b7ter\u00b7ten", "Her\u00b7zen", "t\u00f6\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Mondenschimmer folg' ich der Denkerin", "tokens": ["Im", "Mon\u00b7den\u00b7schim\u00b7mer", "fol\u00b7g'", "ich", "der", "Den\u00b7ke\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deine K\u00fchlung, duftende Fr\u00fchlingsnacht;", "tokens": ["Durch", "dei\u00b7ne", "K\u00fch\u00b7lung", ",", "duf\u00b7ten\u00b7de", "Fr\u00fch\u00b7lings\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und decke, sinkt ihr Aug' in Schlummer,", "tokens": ["Und", "de\u00b7cke", ",", "sinkt", "ihr", "Aug'", "in", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mit verbreitetem sanftem Fl\u00fcgel.", "tokens": ["Sie", "mit", "ver\u00b7brei\u00b7te\u00b7tem", "sanf\u00b7tem", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Im Morgenschimmer weh' ich den frommen Traum", "tokens": ["Im", "Mor\u00b7gen\u00b7schim\u00b7mer", "weh'", "ich", "den", "from\u00b7men", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ihrer Stirn', und f\u00fchre zum Garten sie,", "tokens": ["Von", "ih\u00b7rer", "Stirn'", ",", "und", "f\u00fch\u00b7re", "zum", "Gar\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Thau durch Blumenbeet' und Bl\u00fcten,", "tokens": ["Im", "Thau", "durch", "Blu\u00b7men\u00b7beet'", "und", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Froh des Gesanges umher, zu wandeln!", "tokens": ["Froh", "des", "Ge\u00b7san\u00b7ges", "um\u00b7her", ",", "zu", "wan\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Des sch\u00f6nen Busens Wallung, des blauen Augs", "tokens": ["Des", "sch\u00f6\u00b7nen", "Bu\u00b7sens", "Wal\u00b7lung", ",", "des", "blau\u00b7en", "Augs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bethr\u00e4ntes Wonnel\u00e4cheln bei edler That,", "tokens": ["Be\u00b7thr\u00e4n\u00b7tes", "Won\u00b7ne\u00b7l\u00e4\u00b7cheln", "bei", "ed\u00b7ler", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dankt mir, und unter Himmelspalmen", "tokens": ["Dankt", "mir", ",", "und", "un\u00b7ter", "Him\u00b7mel\u00b7spal\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fcnftig ein Kuss von dem Rosenmunde!", "tokens": ["K\u00fcnf\u00b7tig", "ein", "Kuss", "von", "dem", "Ro\u00b7sen\u00b7mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}