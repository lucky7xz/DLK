{"textgrid.poem.42527": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ricordo", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Tannenwald verl\u00f6scht die Nebelwand,", "tokens": ["Den", "Tan\u00b7nen\u00b7wald", "ver\u00b7l\u00f6scht", "die", "Ne\u00b7bel\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die wei\u00dfe Birke schl\u00e4ft im Haidekraute;", "tokens": ["Die", "wei\u00b7\u00dfe", "Bir\u00b7ke", "schl\u00e4ft", "im", "Hai\u00b7de\u00b7krau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Zimbelklang erklingt und keine Laute,", "tokens": ["Kein", "Zim\u00b7bel\u00b7klang", "er\u00b7klingt", "und", "kei\u00b7ne", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es schreit die M\u00f6we nur an Odins Strand.", "tokens": ["Es", "schreit", "die", "M\u00f6\u00b7we", "nur", "an", "O\u00b7dins", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6rst du es singen doch? siehst du das Land,", "tokens": ["H\u00f6rst", "du", "es", "sin\u00b7gen", "doch", "?", "siehst", "du", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "ADV", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo klar in goldne Himmel Tizian schaute,", "tokens": ["Wo", "klar", "in", "gold\u00b7ne", "Him\u00b7mel", "Ti\u00b7zi\u00b7an", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo Michelangelo Sanct Peter baute", "tokens": ["Wo", "Mi\u00b7che\u00b7lan\u00b7ge\u00b7lo", "Sanct", "Pe\u00b7ter", "bau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und C\u00e4sar einst die Welt zum Kranze band!", "tokens": ["Und", "C\u00e4\u00b7sar", "einst", "die", "Welt", "zum", "Kran\u00b7ze", "band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir landen, von Orangen \u00fcberdacht;", "tokens": ["Wir", "lan\u00b7den", ",", "von", "O\u00b7ran\u00b7gen", "\u00fc\u00b7berd\u00b7acht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was bleibst du kalt und ohne Interesse,", "tokens": ["Was", "bleibst", "du", "kalt", "und", "oh\u00b7ne", "In\u00b7ter\u00b7es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sehnst du zur\u00fcck die kimmerische Nacht?", "tokens": ["Sehnst", "du", "zu\u00b7r\u00fcck", "die", "kim\u00b7me\u00b7ri\u00b7sche", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O w\u00fc\u00dftest du, wie gestern in der Messe,", "tokens": ["O", "w\u00fc\u00df\u00b7test", "du", ",", "wie", "ge\u00b7stern", "in", "der", "Mes\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als du erschienst in venezianischer Pracht,", "tokens": ["Als", "du", "er\u00b7schienst", "in", "ve\u00b7ne\u00b7zi\u00b7a\u00b7ni\u00b7scher", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Murmelsturm anschwoll: Die Dogaresse!", "tokens": ["Ein", "Mur\u00b7mel\u00b7sturm", "an\u00b7schwoll", ":", "Die", "Do\u00b7ga\u00b7res\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}