{"dta.poem.12676": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Cratinen.  \n J. G. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cratine! deiner sch\u00f6nheit macht", "tokens": ["Cra\u00b7ti\u00b7ne", "!", "dei\u00b7ner", "sch\u00f6n\u00b7heit", "macht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat mich zu deiner liebe bracht,", "tokens": ["Hat", "mich", "zu", "dei\u00b7ner", "lie\u00b7be", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich mich nur darein mu\u00df geben,", "tokens": ["Da\u00df", "ich", "mich", "nur", "da\u00b7rein", "mu\u00df", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PAV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fcnfftig hin als dein gefangner leben:", "tokens": ["Und", "k\u00fcnff\u00b7tig", "hin", "als", "dein", "ge\u00b7fang\u00b7ner", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "KOKOM", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich will auch gerne deine seyn,", "tokens": ["Ich", "will", "auch", "ger\u00b7ne", "dei\u00b7ne", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur setze keinen weiter ein.", "tokens": ["Nur", "set\u00b7ze", "kei\u00b7nen", "wei\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}