{"dta.poem.10206": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Little remembrance. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Schneegest\u00f6ber mag die Stadt ertrinken,", "tokens": ["Im", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "mag", "die", "Stadt", "er\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was k\u00fcmmert\u2019s mich, ich sitze warm und trocken.", "tokens": ["Was", "k\u00fcm\u00b7mert's", "mich", ",", "ich", "sit\u00b7ze", "warm", "und", "tro\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bemerklich kaum h\u00f6r\u2019 ich die Th\u00fcre klinken,", "tokens": ["Be\u00b7mer\u00b7klich", "kaum", "h\u00f6r'", "ich", "die", "Th\u00fc\u00b7re", "klin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und hinter mir schleicht irgendwer auf Socken,", "tokens": ["Und", "hin\u00b7ter", "mir", "schleicht", "ir\u00b7gend\u00b7wer", "auf", "So\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um raschen Sprungs an meine Brust zu sinken!", "tokens": ["Um", "ra\u00b7schen", "Sprungs", "an", "mei\u00b7ne", "Brust", "zu", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich thue wild und grenzenlos erschrocken.", "tokens": ["Ich", "thue", "wild", "und", "gren\u00b7zen\u00b7los", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Sie lacht wie toll, die wei\u00dfen Z\u00e4hne blinken,", "tokens": ["Sie", "lacht", "wie", "toll", ",", "die", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf ihren Backen schmelzen noch die Flocken.", "tokens": ["Auf", "ih\u00b7ren", "Ba\u00b7cken", "schmel\u00b7zen", "noch", "die", "Flo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}