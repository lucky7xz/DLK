{"textgrid.poem.46648": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[geht, Kinder, nicht ans Wasser 'nan!]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgeht, Kinder, nicht ans Wasser 'nan!", "tokens": ["\u00bb", "geht", ",", "Kin\u00b7der", ",", "nicht", "ans", "Was\u00b7ser", "'nan", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PTKNEG", "APPRART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wasser wohnt der Wassermann,", "tokens": ["Im", "Was\u00b7ser", "wohnt", "der", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wird hinein euch ziehen,", "tokens": ["Der", "wird", "hin\u00b7ein", "euch", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ihr nicht werdet fliehen.\u00ab", "tokens": ["Wenn", "ihr", "nicht", "wer\u00b7det", "flie\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da flohn sie, wo ein Wasser rann,", "tokens": ["Da", "flohn", "sie", ",", "wo", "ein", "Was\u00b7ser", "rann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcrchteten den Wassermann,", "tokens": ["Und", "f\u00fcrch\u00b7te\u00b7ten", "den", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da glaubt' ich sie geborgen", "tokens": ["Da", "glaubt'", "ich", "sie", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Trocknen ohne Sorgen.", "tokens": ["Im", "Trock\u00b7nen", "oh\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun wohl! euch that der Wassermann,", "tokens": ["Nun", "wohl", "!", "euch", "that", "der", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch that kein Leid das Wasser an,", "tokens": ["Euch", "that", "kein", "Leid", "das", "Was\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein ihr mu\u00dftet werden", "tokens": ["Al\u00b7lein", "ihr", "mu\u00df\u00b7tet", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begraben in der Erden.", "tokens": ["Be\u00b7gra\u00b7ben", "in", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und tret' ich nun ans Wasser 'ran,", "tokens": ["Und", "tret'", "ich", "nun", "ans", "Was\u00b7ser", "'ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ruft mir zu der Wassermann:", "tokens": ["So", "ruft", "mir", "zu", "der", "Was\u00b7ser\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hast du dich betrogen,", "tokens": ["Wie", "hast", "du", "dich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mir die Lust entzogen!", "tokens": ["Und", "mir", "die", "Lust", "ent\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich h\u00e4tte, spricht der Wassermann,", "tokens": ["Ich", "h\u00e4t\u00b7te", ",", "spricht", "der", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem sch\u00f6nen Paar nie was gethan;", "tokens": ["Dem", "sch\u00f6\u00b7nen", "Paar", "nie", "was", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Bild wollt' ich mit Schweigen", "tokens": ["Ihr", "Bild", "wollt'", "ich", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Spiegel ihnen zeigen.", "tokens": ["Im", "Spie\u00b7gel", "ih\u00b7nen", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}