{"textgrid.poem.42180": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Unterm Apfelbaum", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieschen kletterte flink hinauf", "tokens": ["Lie\u00b7schen", "klet\u00b7ter\u00b7te", "flink", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Bis in die h\u00f6chsten \u00c4ste,", "tokens": ["Bis", "in", "die", "h\u00f6chs\u00b7ten", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fing in der Sch\u00fcrze die \u00c4pfel auf", "tokens": ["Fing", "in", "der", "Sch\u00fcr\u00b7ze", "die", "\u00c4p\u00b7fel", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ihrer Mutter zum Feste.", "tokens": ["Ih\u00b7rer", "Mut\u00b7ter", "zum", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Ich lag unten, verliebt und faul,", "tokens": ["Ich", "lag", "un\u00b7ten", ",", "ver\u00b7liebt", "und", "faul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf dem R\u00fccken im Grase;", "tokens": ["Auf", "dem", "R\u00fc\u00b7cken", "im", "Gra\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mancher Apfel fiel mir ins Maul,", "tokens": ["Man\u00b7cher", "Ap\u00b7fel", "fiel", "mir", "ins", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mancher mir auf die Nase.", "tokens": ["Man\u00b7cher", "mir", "auf", "die", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Jetzt stand Lieschen auf starkem Ast,", "tokens": ["Jetzt", "stand", "Lie\u00b7schen", "auf", "star\u00b7kem", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schelmisch sah sie hernieder;", "tokens": ["Schel\u00b7misch", "sah", "sie", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ihres Leibes liebliche Last", "tokens": ["Ih\u00b7res", "Lei\u00b7bes", "lieb\u00b7li\u00b7che", "Last"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wiegte sich hin und wieder.", "tokens": ["Wieg\u00b7te", "sich", "hin", "und", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "KON", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Innig umschlungen hielten sich", "tokens": ["In\u00b7nig", "um\u00b7schlun\u00b7gen", "hiel\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Splitternackt ihre F\u00fc\u00dfe,", "tokens": ["Split\u00b7ter\u00b7nackt", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Taten sich auf und bef\u00fchlten sich \u2013", "tokens": ["Ta\u00b7ten", "sich", "auf", "und", "be\u00b7f\u00fchl\u00b7ten", "sich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Winkten mir tausend Gr\u00fc\u00dfe.", "tokens": ["Wink\u00b7ten", "mir", "tau\u00b7send", "Gr\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "CARD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Durch das R\u00f6ckchen sandte der Tag", "tokens": ["Durch", "das", "R\u00f6ck\u00b7chen", "sand\u00b7te", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seine goldenen Strahlen,", "tokens": ["Sei\u00b7ne", "gol\u00b7de\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Was darunter geborgen lag,", "tokens": ["Was", "da\u00b7run\u00b7ter", "ge\u00b7bor\u00b7gen", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVPP", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Farbenpr\u00e4chtig zu malen.", "tokens": ["Far\u00b7ben\u00b7pr\u00e4ch\u00b7tig", "zu", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Schimmernd rings um die wei\u00dfe Haut", "tokens": ["Schim\u00b7mernd", "rings", "um", "die", "wei\u00b7\u00dfe", "Haut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wob sich die ged\u00e4mpfte Helle;", "tokens": ["Wob", "sich", "die", "ge\u00b7d\u00e4mpf\u00b7te", "Hel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher Meister hat je gebaut", "tokens": ["Wel\u00b7cher", "Meis\u00b7ter", "hat", "je", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Pr\u00e4chtiger eine Kapelle.", "tokens": ["Pr\u00e4ch\u00b7ti\u00b7ger", "ei\u00b7ne", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Kindlich faltet ich da die H\u00e4nd',", "tokens": ["Kind\u00b7lich", "fal\u00b7tet", "ich", "da", "die", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Forderte hei\u00df und br\u00fcnstig:", "tokens": ["For\u00b7der\u00b7te", "hei\u00df", "und", "br\u00fcns\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was kein irdischer Name nennt,", "tokens": ["Was", "kein", "ir\u00b7di\u00b7scher", "Na\u00b7me", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Werde dem S\u00fcnder g\u00fcnstig.", "tokens": ["Wer\u00b7de", "dem", "S\u00fcn\u00b7der", "g\u00fcns\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Sieh, und am n\u00e4mlichen Abend schon,", "tokens": ["Sieh", ",", "und", "am", "n\u00e4m\u00b7li\u00b7chen", "A\u00b7bend", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Tief in die Kissen gebettet,", "tokens": ["Tief", "in", "die", "Kis\u00b7sen", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wurden der kindlichen Bitte zum Lohn", "tokens": ["Wur\u00b7den", "der", "kind\u00b7li\u00b7chen", "Bit\u00b7te", "zum", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Leib und Seele gerettet.", "tokens": ["Leib", "und", "See\u00b7le", "ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}