{"textgrid.poem.48578": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. Hortulan und Lilie", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich, sprach ", "tokens": ["Seh'", "ich", ",", "sprach"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "zu seiner ", "tokens": ["zu", "sei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "die bunten Blumen an", "tokens": ["die", "bun\u00b7ten", "Blu\u00b7men", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in ihren Zieraten,", "tokens": ["in", "ih\u00b7ren", "Zie\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "so will mich doch f\u00fcr allen", "tokens": ["so", "will", "mich", "doch", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die Lilge nur gefallen:", "tokens": ["die", "Lil\u00b7ge", "nur", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "so, Liebst', du, meine Zier,", "tokens": ["so", ",", "Liebst'", ",", "du", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "f\u00fcr andern Jungfern mir.", "tokens": ["f\u00fcr", "an\u00b7dern", "Jung\u00b7fern", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die keusche ", "tokens": ["Die", "keu\u00b7sche"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "hub so hingegen an;", "tokens": ["hub", "so", "hin\u00b7ge\u00b7gen", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So wahr ich vor dir steh',", "tokens": ["So", "wahr", "ich", "vor", "dir", "steh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "herzliebster ", "tokens": ["herz\u00b7liebs\u00b7ter"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "wie Mancher mich beweget,", "tokens": ["wie", "Man\u00b7cher", "mich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der so der G\u00e4rten pfleget,", "tokens": ["der", "so", "der", "G\u00e4r\u00b7ten", "pfle\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "doch kanst f\u00fcr Allen du", "tokens": ["doch", "kanst", "f\u00fcr", "Al\u00b7len", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "mich ziehen zu dir zu.", "tokens": ["mich", "zie\u00b7hen", "zu", "dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er bot ihr einen Ku\u00df;", "tokens": ["Er", "bot", "ihr", "ei\u00b7nen", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "sie bot ihm einen drauf.", "tokens": ["sie", "bot", "ihm", "ei\u00b7nen", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So, so, sprach ", "tokens": ["So", ",", "so", ",", "sprach"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "ADV", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "das ist der rechte Kauf!", "tokens": ["das", "ist", "der", "rech\u00b7te", "Kauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wird, wie vor den Garten,", "tokens": ["Er", "wird", ",", "wie", "vor", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "so seine ", "tokens": ["so", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sie wurden beide rot.", "tokens": ["Sie", "wur\u00b7den", "bei\u00b7de", "rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Er sprach: So paar' euch Gott!", "tokens": ["Er", "sprach", ":", "So", "paa\u00b7r'", "euch", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}