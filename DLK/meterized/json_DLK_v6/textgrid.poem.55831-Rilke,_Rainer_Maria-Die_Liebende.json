{"textgrid.poem.55831": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebende", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja ich sehne mich nach dir. Ich gleite", "tokens": ["Ja", "ich", "seh\u00b7ne", "mich", "nach", "dir", ".", "Ich", "glei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "mich verlierend selbst mir aus der Hand,", "tokens": ["mich", "ver\u00b7lie\u00b7rend", "selbst", "mir", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ohne Hoffnung, da\u00df ich Das bestreite,", "tokens": ["oh\u00b7ne", "Hoff\u00b7nung", ",", "da\u00df", "ich", "Das", "be\u00b7strei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "was zu mir kommt wie aus deiner Seite", "tokens": ["was", "zu", "mir", "kommt", "wie", "aus", "dei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "KOKOM", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "ernst und unbeirrt und unverwandt.", "tokens": ["ernst", "und", "un\u00b7be\u00b7irrt", "und", "un\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "... jene Zeiten: O wie war ich Eines,", "tokens": ["...", "je\u00b7ne", "Zei\u00b7ten", ":", "O", "wie", "war", "ich", "Ei\u00b7nes", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$.", "NE", "KOKOM", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "nichts was rief und nichts was mich verriet;", "tokens": ["nichts", "was", "rief", "und", "nichts", "was", "mich", "ver\u00b7riet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "VVFIN", "KON", "PIS", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "meine Stille war wie eines Steines,", "tokens": ["mei\u00b7ne", "Stil\u00b7le", "war", "wie", "ei\u00b7nes", "Stei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00fcber den der Bach sein Murmeln zieht.", "tokens": ["\u00fc\u00b7ber", "den", "der", "Bach", "sein", "Mur\u00b7meln", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber jetzt in diesen Fr\u00fchlingswochen", "tokens": ["A\u00b7ber", "jetzt", "in", "die\u00b7sen", "Fr\u00fch\u00b7lings\u00b7wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hat mich etwas langsam abgebrochen", "tokens": ["hat", "mich", "et\u00b7was", "lang\u00b7sam", "ab\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "von dem unbewu\u00dften dunkeln Jahr.", "tokens": ["von", "dem", "un\u00b7be\u00b7wu\u00df\u00b7ten", "dun\u00b7keln", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Etwas hat mein armes warmes Leben", "tokens": ["Et\u00b7was", "hat", "mein", "ar\u00b7mes", "war\u00b7mes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "irgendeinem in die Hand gegeben,", "tokens": ["ir\u00b7gend\u00b7ei\u00b7nem", "in", "die", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "der nicht wei\u00df was ich noch gestern war.", "tokens": ["der", "nicht", "wei\u00df", "was", "ich", "noch", "ge\u00b7stern", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "PWS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}