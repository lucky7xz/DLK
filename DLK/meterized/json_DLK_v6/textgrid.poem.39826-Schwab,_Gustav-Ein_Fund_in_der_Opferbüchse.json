{"textgrid.poem.39826": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Fund in der Opferb\u00fcchse", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Silbern seh' ich's heute glasten", "tokens": ["Sil\u00b7bern", "seh'", "ich's", "heu\u00b7te", "glas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem braunen Kupfermeer.", "tokens": ["In", "dem", "brau\u00b7nen", "Kup\u00b7fer\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seltner Schatz im Opferkasten,", "tokens": ["Selt\u00b7ner", "Schatz", "im", "Op\u00b7fer\u00b7kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00f6schlein, ei, wo stammst du her?", "tokens": ["Gr\u00f6schlein", ",", "ei", ",", "wo", "stammst", "du", "her", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Welch ein ungewohnt Gepr\u00e4ge,", "tokens": ["Welch", "ein", "un\u00b7ge\u00b7wohnt", "Ge\u00b7pr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie man's nicht in Rollen trifft!", "tokens": ["Wie", "man's", "nicht", "in", "Rol\u00b7len", "trifft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh ich dich zum andern lege,", "tokens": ["Eh", "ich", "dich", "zum", "an\u00b7dern", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "PIS", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sprich, we\u00df Bild und Ueberschrift?", "tokens": ["Sprich", ",", "we\u00df", "Bild", "und", "Ue\u00b7ber\u00b7schrift", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was? ein Lorbeer statt der Krone", "tokens": ["Was", "?", "ein", "Lor\u00b7beer", "statt", "der", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem hochgetragnen Haupt?", "tokens": ["Auf", "dem", "hoch\u00b7ge\u00b7trag\u00b7nen", "Haupt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du geh\u00f6rest einem Sohne", "tokens": ["Du", "ge\u00b7h\u00f6\u00b7rest", "ei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Roms, vom Siegerkranz umlaubt.", "tokens": ["Roms", ",", "vom", "Sie\u00b7ger\u00b7kranz", "um\u00b7laubt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie gebietrisch, wie allm\u00e4chtig", "tokens": ["Wie", "ge\u00b7bie\u00b7trisch", ",", "wie", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehn mich Stirn und Augen an!", "tokens": ["Sehn", "mich", "Stirn", "und", "Au\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Umschrift wie so pr\u00e4chtig", "tokens": ["Und", "die", "Um\u00b7schrift", "wie", "so", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KOKOM", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Imperator, \u2013 und ", "tokens": ["Im\u00b7pe\u00b7ra\u00b7tor", ",", "\u2013", "und"], "token_info": ["word", "punct", "punct", "word"], "pos": ["NN", "$,", "$(", "KON"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Du, des gr\u00f6\u00dften Reichs von allen", "tokens": ["Du", ",", "des", "gr\u00f6\u00df\u00b7ten", "Reichs", "von", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverwischter, gro\u00dfer Held,", "tokens": ["Un\u00b7ver\u00b7wischter", ",", "gro\u00b7\u00dfer", "Held", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mu\u00dft als Opferpfennig fallen", "tokens": ["Mu\u00dft", "als", "Op\u00b7fer\u00b7pfen\u00b7nig", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem andern Herrn der Welt!", "tokens": ["Ei\u00b7nem", "an\u00b7dern", "Herrn", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du, der vor des Unthiers Z\u00e4hne", "tokens": ["Du", ",", "der", "vor", "des", "Un\u00b7thiers", "Z\u00e4h\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Bekenner werfen hie\u00df,", "tokens": ["Den", "Be\u00b7ken\u00b7ner", "wer\u00b7fen", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, beim G\u00e4hnen der Hy\u00e4ne,", "tokens": ["Und", ",", "beim", "G\u00e4h\u00b7nen", "der", "Hy\u00b7\u00e4\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Jahrhunderts Milde pries:", "tokens": ["Des", "Jahr\u00b7hun\u00b7derts", "Mil\u00b7de", "pries", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Liegst du, liegst du, stolzer Kaiser,", "tokens": ["Liegst", "du", ",", "liegst", "du", ",", "stol\u00b7zer", "Kai\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Gekreuzigten zu Fu\u00df?", "tokens": ["Dem", "Ge\u00b7kreu\u00b7zig\u00b7ten", "zu", "Fu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fccken deines Lorbeers Reiser", "tokens": ["Pfl\u00fc\u00b7cken", "dei\u00b7nes", "Lor\u00b7beers", "Rei\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsche Bauern ", "tokens": ["Deut\u00b7sche", "Bau\u00b7ern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Ja, in dunkler Zeit erloschen,", "tokens": ["Ja", ",", "in", "dunk\u00b7ler", "Zeit", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00e4rft sich wieder mein Gesicht;", "tokens": ["Sch\u00e4rft", "sich", "wie\u00b7der", "mein", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor mir in diesem Groschen", "tokens": ["Und", "vor", "mir", "in", "die\u00b7sem", "Gro\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt des Menschen Sohn Gericht!", "tokens": ["H\u00e4lt", "des", "Men\u00b7schen", "Sohn", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}