{"textgrid.poem.51025": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon war die letzte Schwalbe fort", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon war die letzte Schwalbe fort", "tokens": ["Schon", "war", "die", "letz\u00b7te", "Schwal\u00b7be", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wohl seit manchen Tagen auch", "tokens": ["Und", "wohl", "seit", "man\u00b7chen", "Ta\u00b7gen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die letzte Rose abgedorrt,", "tokens": ["Die", "letz\u00b7te", "Ro\u00b7se", "ab\u00b7ge\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach altem Erdenbrauch.", "tokens": ["Nach", "al\u00b7tem", "Er\u00b7den\u00b7brauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es flimmerte der Buchenhain", "tokens": ["Es", "flim\u00b7mer\u00b7te", "der", "Bu\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Rauschgold rot im Abendlicht;", "tokens": ["Wie", "Rauschgold", "rot", "im", "A\u00b7bend\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Herbstsonne gibt gar sondern Schein,", "tokens": ["Herbst\u00b7son\u00b7ne", "gibt", "gar", "son\u00b7dern", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der in die Herzen sticht.", "tokens": ["Der", "in", "die", "Her\u00b7zen", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich traf sie da im Walde an,", "tokens": ["Ich", "traf", "sie", "da", "im", "Wal\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach der allein mein Herz begehrt,", "tokens": ["Nach", "der", "al\u00b7lein", "mein", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Tuch und Hut wei\u00df umgetan,", "tokens": ["Mit", "Tuch", "und", "Hut", "wei\u00df", "um\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom g\u00fcldnen Schein verkl\u00e4rt.", "tokens": ["Vom", "g\u00fcld\u00b7nen", "Schein", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie war allein; doch gr\u00fc\u00dft ich sie", "tokens": ["Sie", "war", "al\u00b7lein", ";", "doch", "gr\u00fc\u00dft", "ich", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versch\u00fcchtert kaum im Weitergehn,", "tokens": ["Ver\u00b7sch\u00fcch\u00b7tert", "kaum", "im", "Wei\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich so feierlich sie nie,", "tokens": ["Weil", "ich", "so", "fei\u00b7er\u00b7lich", "sie", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So still und sch\u00f6n, gesehn.", "tokens": ["So", "still", "und", "sch\u00f6n", ",", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es blickt' aus ihrem Angesicht", "tokens": ["Es", "blickt'", "aus", "ih\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein vornehm Etwas neu hervor,", "tokens": ["Ein", "vor\u00b7nehm", "Et\u00b7was", "neu", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihrer Augen Veilchenlicht", "tokens": ["Und", "ih\u00b7rer", "Au\u00b7gen", "Veil\u00b7chen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Glomm hinter einem Flor.", "tokens": ["Glomm", "hin\u00b7ter", "ei\u00b7nem", "Flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein fremder Hirt, ein blasser, ging", "tokens": ["Ein", "frem\u00b7der", "Hirt", ",", "ein", "blas\u00b7ser", ",", "ging"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten dieser Huldgestalt;", "tokens": ["Im", "Schat\u00b7ten", "die\u00b7ser", "Huld\u00b7ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Gurt ein silbern Sichlein hing,", "tokens": ["Im", "Gurt", "ein", "sil\u00b7bern", "Sic\u00b7hlein", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das klang: Ich schneide bald!", "tokens": ["Das", "klang", ":", "Ich", "schnei\u00b7de", "bald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es scheint mir ein Rival erwacht!", "tokens": ["Es", "scheint", "mir", "ein", "Ri\u00b7val", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sprach ich und schaut ins Abendrot,", "tokens": ["Sprach", "ich", "und", "schaut", "ins", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis es erlosch und bis die Nacht", "tokens": ["Bis", "es", "er\u00b7losch", "und", "bis", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dunkle Hand mir bot.", "tokens": ["Die", "dunk\u00b7le", "Hand", "mir", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}