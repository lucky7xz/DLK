{"textgrid.poem.67366": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Magellan", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es strandeten zwei Schiffe Magellan's;", "tokens": ["Es", "stran\u00b7de\u00b7ten", "zwei", "Schif\u00b7fe", "Ma\u00b7gel\u00b7lan's", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Boot kann wenig fassen. \u00bbSo bleib ich,\u00ab", "tokens": ["Das", "Boot", "kann", "we\u00b7nig", "fas\u00b7sen", ".", "\u00bb", "So", "bleib", "ich", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Spricht Magellan, \u00bballein am Strande, bis", "tokens": ["Spricht", "Ma\u00b7gel\u00b7lan", ",", "\u00bb", "al\u00b7lein", "am", "Stran\u00b7de", ",", "bis"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NE", "$,", "$(", "ADV", "APPRART", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Volk gerettet ist.\u00ab Er that's und blieb.", "tokens": ["Mein", "Volk", "ge\u00b7ret\u00b7tet", "ist", ".", "\u00ab", "Er", "that's", "und", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$.", "$(", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gerettet holte man den Admiral", "tokens": ["Ge\u00b7ret\u00b7tet", "hol\u00b7te", "man", "den", "Ad\u00b7mi\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zuletzt hin\u00fcber. Gro\u00df war Wort und That.", "tokens": ["Zu\u00b7letzt", "hin\u00b7\u00fc\u00b7ber", ".", "Gro\u00df", "war", "Wort", "und", "That", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NE", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}