{"textgrid.poem.66098": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der schlafende Wind", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die verh\u00fcllten Abendh\u00fcgel", "tokens": ["\u00dc\u00b7ber", "die", "ver\u00b7h\u00fcll\u00b7ten", "A\u00b7bend\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Steigen schon die ersten Sterne her,", "tokens": ["Stei\u00b7gen", "schon", "die", "ers\u00b7ten", "Ster\u00b7ne", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einmal r\u00fchrt der Wind noch seine Fl\u00fcgel,", "tokens": ["Ein\u00b7mal", "r\u00fchrt", "der", "Wind", "noch", "sei\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles schweigt und tr\u00e4umt, nun tr\u00e4umt auch er.", "tokens": ["Al\u00b7les", "schweigt", "und", "tr\u00e4umt", ",", "nun", "tr\u00e4umt", "auch", "er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Auf den Rosen ist er eingeschlafen,", "tokens": ["Auf", "den", "Ro\u00b7sen", "ist", "er", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00e4umt von einem sch\u00f6nen Wandertag.", "tokens": ["Tr\u00e4umt", "von", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Wan\u00b7der\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, wie lieblich sichs in solchem Hafen", "tokens": ["Ach", ",", "wie", "lieb\u00b7lich", "sichs", "in", "sol\u00b7chem", "Ha\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "PIS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nach der langen Reise schlafen mag.", "tokens": ["Nach", "der", "lan\u00b7gen", "Rei\u00b7se", "schla\u00b7fen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Fr\u00fche, welche s\u00fc\u00dfen D\u00fcfte", "tokens": ["In", "der", "Fr\u00fc\u00b7he", ",", "wel\u00b7che", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Haften noch an seinem Schwingenpaar.", "tokens": ["Haf\u00b7ten", "noch", "an", "sei\u00b7nem", "Schwin\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Neiderf\u00fcllt erzittern alle L\u00fcfte,", "tokens": ["Nei\u00b7der\u00b7f\u00fcllt", "er\u00b7zit\u00b7tern", "al\u00b7le", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6ren sie, wo er zur Nacht heut war.", "tokens": ["H\u00f6\u00b7ren", "sie", ",", "wo", "er", "zur", "Nacht", "heut", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "APPRART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und die M\u00e4dchen, die vor T\u00fcr und Toren", "tokens": ["Und", "die", "M\u00e4d\u00b7chen", ",", "die", "vor", "T\u00fcr", "und", "To\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Halbverschlafen in die Sonne sehn,", "tokens": ["Halb\u00b7ver\u00b7schla\u00b7fen", "in", "die", "Son\u00b7ne", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Strecken sich und fragen traumverloren:", "tokens": ["Stre\u00b7cken", "sich", "und", "fra\u00b7gen", "traum\u00b7ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo doch nur die vielen Rosen stehn?", "tokens": ["Wo", "doch", "nur", "die", "vie\u00b7len", "Ro\u00b7sen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}