{"textgrid.poem.61437": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr wohl, fahr wohl! Du ziehst von hinnen,", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "!", "Du", "ziehst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und all mein Gl\u00fcck zieht mit dir fort;", "tokens": ["Und", "all", "mein", "Gl\u00fcck", "zieht", "mit", "dir", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sahst du keine Tr\u00e4ne rinnen,", "tokens": ["Doch", "sahst", "du", "kei\u00b7ne", "Tr\u00e4\u00b7ne", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und diese Lippe sprach kein Wort;", "tokens": ["Und", "die\u00b7se", "Lip\u00b7pe", "sprach", "kein", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr wohl, fahr wohl! Du ahnest nicht", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "!", "Du", "ah\u00b7nest", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Dorn, der mir ins Leben sticht.", "tokens": ["Den", "Dorn", ",", "der", "mir", "ins", "Le\u00b7ben", "sticht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, als in meines Herbstes Trauer", "tokens": ["Ach", ",", "als", "in", "mei\u00b7nes", "Herbs\u00b7tes", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du tratest, Fr\u00fchlingslicht ums Haupt,", "tokens": ["Du", "tra\u00b7test", ",", "Fr\u00fch\u00b7lings\u00b7licht", "ums", "Haupt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ging durch diese Brust ein Schauer,", "tokens": ["Da", "ging", "durch", "die\u00b7se", "Brust", "ein", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die nie zu lieben mehr geglaubt;", "tokens": ["Die", "nie", "zu", "lie\u00b7ben", "mehr", "ge\u00b7glaubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Wunder, das an mir geschah,", "tokens": ["Am", "Wun\u00b7der", ",", "das", "an", "mir", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "F\u00fchlt' ich, ein Engel war mir nah.", "tokens": ["F\u00fchlt'", "ich", ",", "ein", "En\u00b7gel", "war", "mir", "nah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und da du meinem Spiel dich neigtest,", "tokens": ["Und", "da", "du", "mei\u00b7nem", "Spiel", "dich", "neig\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und forschend nach der Lieder Sinn", "tokens": ["Und", "for\u00b7schend", "nach", "der", "Lie\u00b7der", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die junge Seele ganz mir zeigtest", "tokens": ["Die", "jun\u00b7ge", "See\u00b7le", "ganz", "mir", "zeig\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und aller Himmel Tiefen drin:", "tokens": ["Und", "al\u00b7ler", "Him\u00b7mel", "Tie\u00b7fen", "drin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O wie mir da die Tr\u00e4ne quoll,", "tokens": ["O", "wie", "mir", "da", "die", "Tr\u00e4\u00b7ne", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und war doch h\u00f6chster Freuden voll!", "tokens": ["Und", "war", "doch", "h\u00f6chs\u00b7ter", "Freu\u00b7den", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir war's, der Mond sei aufgegangen,", "tokens": ["Mir", "wa\u00b7r's", ",", "der", "Mond", "sei", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mein dunkler Wandel ward voll Licht;", "tokens": ["Mein", "dunk\u00b7ler", "Wan\u00b7del", "ward", "voll", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich tr\u00e4umte hin im sch\u00f6nen Prangen", "tokens": ["Ich", "tr\u00e4um\u00b7te", "hin", "im", "sch\u00f6\u00b7nen", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dacht', ein Kind, der Zukunft nicht;", "tokens": ["Und", "dacht'", ",", "ein", "Kind", ",", "der", "Zu\u00b7kunft", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr wohl! \u2013 In Wolken sinkt der Mond,", "tokens": ["Fahr", "wohl", "!", "\u2013", "In", "Wol\u00b7ken", "sinkt", "der", "Mond", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "$(", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Nacht wird's. Doch ich bin's gewohnt.", "tokens": ["Und", "Nacht", "wird'", "s.", "Doch", "ich", "bin's", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NE", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Fahr wohl, Holdsel'ge, sei gesegnet,", "tokens": ["Fahr", "wohl", ",", "Hold\u00b7sel'\u00b7ge", ",", "sei", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sei gesegnet, wem du nahst;", "tokens": ["Und", "sei", "ge\u00b7seg\u00b7net", ",", "wem", "du", "nahst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch er, dem einst dein Herz begegnet,", "tokens": ["Auch", "er", ",", "dem", "einst", "dein", "Herz", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann du mich l\u00e4ngst vergessen hast \u2013", "tokens": ["Wann", "du", "mich", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", "hast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr wohl, fahr wohl! Was geht's dich an,", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "!", "Was", "geht's", "dich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich dich nie vergessen kann?", "tokens": ["Da\u00df", "ich", "dich", "nie", "ver\u00b7ges\u00b7sen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}