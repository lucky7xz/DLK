{"dta.poem.20292": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Er liebt vergebens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich finde keinen rath/ die liebe w\u00e4chst alleine/", "tokens": ["Ich", "fin\u00b7de", "kei\u00b7nen", "rath", "/", "die", "lie\u00b7be", "w\u00e4chst", "al\u00b7lei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "ART", "ADJA", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenig neben mir/ es sey denn meine noth/", "tokens": ["Und", "we\u00b7nig", "ne\u00b7ben", "mir", "/", "es", "sey", "denn", "mei\u00b7ne", "noth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "$(", "PPER", "VAFIN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die brunst bestricket mich/ warum nicht auch der tod?", "tokens": ["Die", "brunst", "be\u00b7stri\u00b7cket", "mich", "/", "wa\u00b7rum", "nicht", "auch", "der", "tod", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PWAV", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Frist jene marck und fleisch/ so fresse der die beine.", "tokens": ["Frist", "je\u00b7ne", "marck", "und", "fleisch", "/", "so", "fres\u00b7se", "der", "die", "bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "KON", "ADJD", "$(", "ADV", "VVFIN", "ART", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber hilfft mein wunsch/ was hilffts mich/ da\u00df ich weine?", "tokens": ["Was", "a\u00b7ber", "hilfft", "mein", "wunsch", "/", "was", "hilffts", "mich", "/", "da\u00df", "ich", "wei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPOSAT", "ADJD", "$(", "PWS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der tod h\u00f6rt nicht vielmehr/ als sonst der liebes-gott/", "tokens": ["Der", "tod", "h\u00f6rt", "nicht", "viel\u00b7mehr", "/", "als", "sonst", "der", "lie\u00b7bes\u00b7gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$(", "KOKOM", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo solte meine qvaal und meines lebens-spott", "tokens": ["Wo", "sol\u00b7te", "mei\u00b7ne", "qvaal", "und", "mei\u00b7nes", "le\u00b7bens\u00b7spott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun besser seyn bedeckt/ als unter einem steine?", "tokens": ["Nun", "bes\u00b7ser", "seyn", "be\u00b7deckt", "/", "als", "un\u00b7ter", "ei\u00b7nem", "stei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VVPP", "$(", "KOKOM", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und bin ich endlich todt/ vergraben und verscharrt/", "tokens": ["Und", "bin", "ich", "end\u00b7lich", "todt", "/", "ver\u00b7gra\u00b7ben", "und", "ver\u00b7scharrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$(", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So schwatzt die grab-schrifft noch/ da\u00df dieser mensch genarrt/", "tokens": ["So", "schwatzt", "die", "grab\u00b7schrifft", "noch", "/", "da\u00df", "die\u00b7ser", "mensch", "ge\u00b7narrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$(", "KOUS", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und sagt: Hier liegt ein narr/ und l\u00e4st nicht wenig erben.", "tokens": ["Und", "sagt", ":", "Hier", "liegt", "ein", "narr", "/", "und", "l\u00e4st", "nicht", "we\u00b7nig", "er\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach! da\u00df den schwartzen leib das erste wasser-bad/", "tokens": ["Ach", "!", "da\u00df", "den", "schwart\u00b7zen", "leib", "das", "ers\u00b7te", "was\u00b7ser\u00b7bad", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So mir die mutter gab/ nicht bald ers\u00e4uffet hat/", "tokens": ["So", "mir", "die", "mut\u00b7ter", "gab", "/", "nicht", "bald", "er\u00b7s\u00e4uf\u00b7fet", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$(", "PTKNEG", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So d\u00f6rfft ich itzt allhier nicht wie ein narr verderben.", "tokens": ["So", "d\u00f6rfft", "ich", "itzt", "all\u00b7hier", "nicht", "wie", "ein", "narr", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "KOKOM", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}