{"textgrid.poem.53107": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Vor-Jahrs-Lied. \u2013 Veris tempore regnat Amor", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sehn setzt sich erfrewen", "tokens": ["Wir", "sehn", "setzt", "sich", "er\u00b7fre\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VVFIN", "PRF", "VVINF"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Erden gantzes Hau\u00df,", "tokens": ["Der", "Er\u00b7den", "gant\u00b7zes", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6ne Lust des Meyen", "tokens": ["Die", "sch\u00f6\u00b7ne", "Lust", "des", "Me\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lockt Dorff vnd Stadt hinau\u00df.", "tokens": ["Lockt", "Dorff", "vnd", "Stadt", "hin\u00b7au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Hertz beginnt zu wallen,", "tokens": ["Mein", "Hertz", "be\u00b7ginnt", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn sich das Lufft-Volck schwingt,", "tokens": ["Wenn", "sich", "das", "Lufft\u00b7Volck", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd lesst ein Lied erschallen,", "tokens": ["Vnd", "lesst", "ein", "Lied", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Berg vnd Thal erklingt.", "tokens": ["Da\u00df", "Berg", "vnd", "Thal", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Heerden gehn sich weiden,", "tokens": ["Die", "Heer\u00b7den", "gehn", "sich", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr tr\u00e4ger Hirten-Mann", "tokens": ["Ihr", "tr\u00e4\u00b7ger", "Hir\u00b7ten\u00b7Mann"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hebt hoch auff gr\u00fcner Heyden", "tokens": ["Hebt", "hoch", "auff", "gr\u00fc\u00b7ner", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein freyes Wald-Lied an,", "tokens": ["Ein", "frey\u00b7es", "Wald\u00b7Lied", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sieht wie in grossem Hauffen", "tokens": ["Sieht", "wie", "in", "gros\u00b7sem", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort vmb der Fl\u00fcsse Rand", "tokens": ["Dort", "vmb", "der", "Fl\u00fcs\u00b7se", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Heerden sich belauffen,", "tokens": ["Die", "Heer\u00b7den", "sich", "be\u00b7lauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd w\u00fcnscht jhm gleichen Standt,", "tokens": ["Vnd", "w\u00fcnscht", "jhm", "glei\u00b7chen", "Standt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In dem daselbst von weiten", "tokens": ["In", "dem", "da\u00b7selbst", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PAV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein klares B\u00e4chlein quillt,", "tokens": ["Ein", "kla\u00b7res", "B\u00e4ch\u00b7lein", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sich von beyden Seiten", "tokens": ["Das", "sich", "von", "bey\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Gra\u00df vnd Laub geh\u00fcllt.", "tokens": ["In", "Gra\u00df", "vnd", "Laub", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Schertz herrscht aller massen,", "tokens": ["Der", "Schertz", "herrscht", "al\u00b7ler", "mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lust bezwingt das Leid,", "tokens": ["Die", "Lust", "be\u00b7zwingt", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt ist au\u00dfgelassen", "tokens": ["Die", "Welt", "ist", "au\u00df\u00b7ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Lieb vnd Freundlicheit.", "tokens": ["Mit", "Lieb", "vnd", "Freund\u00b7li\u00b7cheit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auff! Venus, die ich singe,", "tokens": ["Auff", "!", "Ve\u00b7nus", ",", "die", "ich", "sin\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcg mir auch jetzund bey.", "tokens": ["F\u00fcg", "mir", "auch", "je\u00b7tzund", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die willig in mich dringe", "tokens": ["Die", "wil\u00b7lig", "in", "mich", "drin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd meine Liebste sey!", "tokens": ["Vnd", "mei\u00b7ne", "Liebs\u00b7te", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich habe gnug gepriesen", "tokens": ["Ich", "ha\u00b7be", "gnug", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwar dich vnd deinen Sohn,", "tokens": ["Zwar", "dich", "vnd", "dei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich dienstlich gnug erwiesen,", "tokens": ["Mich", "dienst\u00b7lich", "gnug", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df aber ist mein Lohn:", "tokens": ["Die\u00df", "a\u00b7ber", "ist", "mein", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da\u00df ich ohn Maa\u00df vnd Ende", "tokens": ["Da\u00df", "ich", "ohn", "Maa\u00df", "vnd", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00df derer m\u00fcssig gehn,", "tokens": ["Mu\u00df", "de\u00b7rer", "m\u00fcs\u00b7sig", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die mir das Hertz verpf\u00e4nde,", "tokens": ["Die", "mir", "das", "Hertz", "ver\u00b7pf\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir trewlich beyzustehn.", "tokens": ["Mir", "trew\u00b7lich", "bey\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Was fleugt, was kreucht, was schwimmet,", "tokens": ["Was", "fleugt", ",", "was", "kreucht", ",", "was", "schwim\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schmeckt jetzt der Vorjahrs-Kost,", "tokens": ["Schmeckt", "jetzt", "der", "Vor\u00b7jahr\u00b7sKost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Ist Liebe-voll vnd glimmet;", "tokens": ["Ist", "Lie\u00b7be\u00b7voll", "vnd", "glim\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur ich klag' \u00fcber Frost.", "tokens": ["Nur", "ich", "klag'", "\u00fc\u00b7ber", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ist denn in mir kein Leben", "tokens": ["Ist", "denn", "in", "mir", "kein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu deiner Frewden Schein,", "tokens": ["Zu", "dei\u00b7ner", "Frew\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich so gut nicht eben", "tokens": ["Da\u00df", "ich", "so", "gut", "nicht", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Heerd vnd Laub kan seyn?", "tokens": ["Als", "Heerd", "vnd", "Laub", "kan", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}