{"dta.poem.9279": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Komm/ kom\u0303 du junges volck/ willstu dich ferner scheuen/", "tokens": ["Komm", "/", "kom\u0303", "du", "jun\u00b7ges", "volck", "/", "will\u00b7stu", "dich", "fer\u00b7ner", "scheu\u00b7en", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVIMP", "PPER", "ADJA", "NN", "$(", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein leben von der last der sorgen zubefreyen/", "tokens": ["Dein", "le\u00b7ben", "von", "der", "last", "der", "sor\u00b7gen", "zu\u00b7be\u00b7fre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "APPR", "ART", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und deinen zarten leib geb\u00fchrlich einzuweihen:", "tokens": ["Und", "dei\u00b7nen", "zar\u00b7ten", "leib", "ge\u00b7b\u00fchr\u00b7lich", "ein\u00b7zu\u00b7wei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm weil das alter noch die kr\u00e4ffte wil verleihen.", "tokens": ["Komm", "weil", "das", "al\u00b7ter", "noch", "die", "kr\u00e4ff\u00b7te", "wil", "ver\u00b7lei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "ADJA", "ADV", "ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der himmel l\u00e4st ja offt den kalten winter schneyen/", "tokens": ["Der", "him\u00b7mel", "l\u00e4st", "ja", "offt", "den", "kal\u00b7ten", "win\u00b7ter", "schne\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und l\u00e4st den nordenwind mit scharffen wetter dreuen:", "tokens": ["Und", "l\u00e4st", "den", "nor\u00b7den\u00b7wind", "mit", "scharf\u00b7fen", "wet\u00b7ter", "dreu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du aber darffst dich nicht der freunde gantz verzeihen/", "tokens": ["Du", "a\u00b7ber", "darffst", "dich", "nicht", "der", "freun\u00b7de", "gantz", "ver\u00b7zei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dein erhitztes blut durch solchen zwang kasteyen.", "tokens": ["Und", "dein", "er\u00b7hitz\u00b7tes", "blut", "durch", "sol\u00b7chen", "zwang", "kas\u00b7te\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Entbrich dich immer noch von diesen fantaseyen/", "tokens": ["Ent\u00b7brich", "dich", "im\u00b7mer", "noch", "von", "die\u00b7sen", "fan\u00b7ta\u00b7se\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dein hertz ist noch geschickt die rosen auszustreuen/", "tokens": ["Dein", "hertz", "ist", "noch", "ge\u00b7schickt", "die", "ro\u00b7sen", "aus\u00b7zu\u00b7streu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nimm deine seiten hin und stimme die schalmeyen/", "tokens": ["Nimm", "dei\u00b7ne", "sei\u00b7ten", "hin", "und", "stim\u00b7me", "die", "schal\u00b7me\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "ART", "VVINF", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "H\u00f6r an den s\u00fcssen thon mit seinen schmeicheleyen/", "tokens": ["H\u00f6r", "an", "den", "s\u00fcs\u00b7sen", "thon", "mit", "sei\u00b7nen", "schmei\u00b7che\u00b7le\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Und la\u00df di\u00df sch\u00f6ne lied mit vollem halse schreyen;", "tokens": ["Und", "la\u00df", "di\u00df", "sch\u00f6\u00b7ne", "lied", "mit", "vol\u00b7lem", "hal\u00b7se", "schre\u00b7yen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PDS", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das alter ist betr\u00fcbt/ die jugend giebt gedeyen.", "tokens": ["Das", "al\u00b7ter", "ist", "be\u00b7tr\u00fcbt", "/", "die", "ju\u00b7gend", "giebt", "ge\u00b7de\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}