{"textgrid.poem.56418": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ernster Strenge angeborner Zucht", "tokens": ["In", "erns\u00b7ter", "Stren\u00b7ge", "an\u00b7ge\u00b7bor\u00b7ner", "Zucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die M\u00e4nner von Athen zur Walstatt steigen.", "tokens": ["Die", "M\u00e4n\u00b7ner", "von", "A\u00b7then", "zur", "Wals\u00b7tatt", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie auf dem Ringplatz stumm zum Todesreigen,", "tokens": ["Wie", "auf", "dem", "Ring\u00b7platz", "stumm", "zum", "To\u00b7des\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch hallt der Grund von der Sandalen Wucht.", "tokens": ["Doch", "hallt", "der", "Grund", "von", "der", "San\u00b7da\u00b7len", "Wucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erhabne Gr\u00f6\u00dfe der Demokratien,", "tokens": ["Er\u00b7hab\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "der", "De\u00b7mo\u00b7kra\u00b7ti\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Recht Europas zieht mit euch zu Meere.", "tokens": ["Das", "Recht", "Eu\u00b7ro\u00b7pas", "zieht", "mit", "euch", "zu", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Heil der Nachwelt tragt ihr auf dem Speere:", "tokens": ["Das", "Heil", "der", "Nach\u00b7welt", "tragt", "ihr", "auf", "dem", "Spee\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der freien V\u00f6lker gro\u00dfe Harmonien.", "tokens": ["Der", "frei\u00b7en", "V\u00f6l\u00b7ker", "gro\u00b7\u00dfe", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Republiken Los in den Phalangen,", "tokens": ["Der", "Re\u00b7pub\u00b7li\u00b7ken", "Los", "in", "den", "Pha\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Ha\u00df der Freien gegen die Despoten.", "tokens": ["Der", "Ha\u00df", "der", "Frei\u00b7en", "ge\u00b7gen", "die", "Des\u00b7po\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr k\u00e4mpft f\u00fcr Recht, das macht euch frei von Bangen.", "tokens": ["Ihr", "k\u00e4mpft", "f\u00fcr", "Recht", ",", "das", "macht", "euch", "frei", "von", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dem Morgen zu! Der V\u00f6lkerfreiheit Boten,", "tokens": ["Dem", "Mor\u00b7gen", "zu", "!", "Der", "V\u00f6l\u00b7ker\u00b7frei\u00b7heit", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unsterblichkeit auf ewig zu erlangen,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "auf", "e\u00b7wig", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wenn Abend ruht auf eurer Schlachtreihn Toten.", "tokens": ["Wenn", "A\u00b7bend", "ruht", "auf", "eu\u00b7rer", "Schlach\u00b7treihn", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}