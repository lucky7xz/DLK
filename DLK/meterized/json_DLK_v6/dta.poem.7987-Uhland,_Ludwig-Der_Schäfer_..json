{"dta.poem.7987": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00e4fer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ne Sch\u00e4fer zog so nah", "tokens": ["Der", "sch\u00f6\u00b7ne", "Sch\u00e4\u00b7fer", "zog", "so", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor\u00fcber an dem K\u00f6nigsschlo\u00df;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "dem", "K\u00f6\u00b7nigs\u00b7schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jungfrau von der Zinne sah,", "tokens": ["Die", "Jung\u00b7frau", "von", "der", "Zin\u00b7ne", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da war ihr Sehnen gro\u00df.", "tokens": ["Da", "war", "ihr", "Seh\u00b7nen", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie rief ihm zu ein s\u00fc\u00dfes Wort:", "tokens": ["Sie", "rief", "ihm", "zu", "ein", "s\u00fc\u00b7\u00dfes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eo d\u00fcrft\u2019 ich gehn hinab zu dir!", "tokens": ["\u201e", "o", "d\u00fcrft'", "ich", "gehn", "hin\u00b7ab", "zu", "dir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gl\u00e4nzen wei\u00df die L\u00e4mmer dort,", "tokens": ["Wie", "gl\u00e4n\u00b7zen", "wei\u00df", "die", "L\u00e4m\u00b7mer", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie roth die Bl\u00fcmlein hier!\u201c", "tokens": ["Wie", "roth", "die", "Bl\u00fcm\u00b7lein", "hier", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der J\u00fcngling ihr entgegenbot:", "tokens": ["Der", "J\u00fcng\u00b7ling", "ihr", "ent\u00b7ge\u00b7gen\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eo k\u00e4mest du herab zu mir!", "tokens": ["\u201e", "o", "k\u00e4\u00b7mest", "du", "her\u00b7ab", "zu", "mir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gl\u00e4nzen so die W\u00e4nglein roth,", "tokens": ["Wie", "gl\u00e4n\u00b7zen", "so", "die", "W\u00e4ng\u00b7lein", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wei\u00df die Arme dir!\u201c", "tokens": ["Wie", "wei\u00df", "die", "Ar\u00b7me", "dir", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als er nun mit stillem Weh", "tokens": ["Und", "als", "er", "nun", "mit", "stil\u00b7lem", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jeder Fr\u00fch\u2019 vor\u00fcbertrieb:", "tokens": ["In", "je\u00b7der", "Fr\u00fch'", "vor\u00b7\u00fc\u00b7bert\u00b7rieb", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah er hin, bis in der H\u00f6h\u2019", "tokens": ["Da", "sah", "er", "hin", ",", "bis", "in", "der", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Erschien sein holdes Lieb.", "tokens": ["Er\u00b7schien", "sein", "hol\u00b7des", "Lieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann rief er freundlich ihr hinauf:", "tokens": ["Dann", "rief", "er", "freund\u00b7lich", "ihr", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewillkommen, K\u00f6nigst\u00f6chterlein!\u201c", "tokens": ["\u201e", "will\u00b7kom\u00b7men", ",", "K\u00f6\u00b7nig\u00b7st\u00f6ch\u00b7ter\u00b7lein", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr s\u00fc\u00dfes Wort ert\u00f6nte drauf:", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfes", "Wort", "er\u00b7t\u00f6n\u00b7te", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eviel Dank, du Sch\u00e4fer mein!\u201c", "tokens": ["\u201e", "viel", "Dank", ",", "du", "Sch\u00e4\u00b7fer", "mein", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PPER", "NE", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Winter floh, der Lenz erschien,", "tokens": ["Der", "Win\u00b7ter", "floh", ",", "der", "Lenz", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bl\u00fcmlein bl\u00fchten reich umher,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "bl\u00fch\u00b7ten", "reich", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00e4fer th\u00e4t zum Schlosse ziehn,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "th\u00e4t", "zum", "Schlos\u00b7se", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Sie erschien nicht mehr.", "tokens": ["Doch", "Sie", "er\u00b7schien", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er rief hinauf so klagevoll:", "tokens": ["Er", "rief", "hin\u00b7auf", "so", "kla\u00b7ge\u00b7voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewillkommen, K\u00f6nigst\u00f6chterlein!\u201c", "tokens": ["\u201e", "will\u00b7kom\u00b7men", ",", "K\u00f6\u00b7nig\u00b7st\u00f6ch\u00b7ter\u00b7lein", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Geisterlaut herunter scholl:", "tokens": ["Ein", "Geis\u00b7ter\u00b7laut", "her\u00b7un\u00b7ter", "scholl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eade, du Sch\u00e4fer mein!\u201c", "tokens": ["\u201e", "a\u00b7de", ",", "du", "Sch\u00e4\u00b7fer", "mein", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "NE", "PPOSAT", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}