{"textgrid.poem.45963": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[m\u00e4gdlein, schlag die Augen nieder]", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4gdlein, schlag die Augen nieder,", "tokens": ["M\u00e4gd\u00b7lein", ",", "schlag", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicke, die zu heftig steigen,", "tokens": ["Bli\u00b7cke", ",", "die", "zu", "hef\u00b7tig", "stei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Plaudern alles f\u00e4lschlich wieder,", "tokens": ["Plau\u00b7dern", "al\u00b7les", "f\u00e4lschlich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was die Lippen zart verschweigen.", "tokens": ["Was", "die", "Lip\u00b7pen", "zart", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00e4gdlein, woll' die Augen senken", "tokens": ["M\u00e4gd\u00b7lein", ",", "woll'", "die", "Au\u00b7gen", "sen\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Such' den Schl\u00fcssel an der Erde,", "tokens": ["Such'", "den", "Schl\u00fcs\u00b7sel", "an", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie wird ihn der Demut schenken,", "tokens": ["Sie", "wird", "ihn", "der", "De\u00b7mut", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df der Himmel offen werde.", "tokens": ["Da\u00df", "der", "Him\u00b7mel", "of\u00b7fen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4gdlein, la\u00df die Wimper sinken;", "tokens": ["M\u00e4gd\u00b7lein", ",", "la\u00df", "die", "Wim\u00b7per", "sin\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Blumen aufw\u00e4rts sehen,", "tokens": ["Wenn", "die", "Blu\u00b7men", "auf\u00b7w\u00e4rts", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Blick herabzuwinken,", "tokens": ["Dei\u00b7nem", "Blick", "her\u00b7ab\u00b7zu\u00b7win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolle nicht vor\u00fcbergehen.", "tokens": ["Wol\u00b7le", "nicht", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00e4gdlein, nicht die Augen hebe,", "tokens": ["M\u00e4gd\u00b7lein", ",", "nicht", "die", "Au\u00b7gen", "he\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allzuoft und kalt und schnelle,", "tokens": ["All\u00b7zu\u00b7oft", "und", "kalt", "und", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein Blick den Himmel gebe", "tokens": ["Da\u00df", "dein", "Blick", "den", "Him\u00b7mel", "ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem nur an rechter Stelle.", "tokens": ["Ei\u00b7nem", "nur", "an", "rech\u00b7ter", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4gdlein, wer herniederblicket,", "tokens": ["M\u00e4gd\u00b7lein", ",", "wer", "her\u00b7nie\u00b7der\u00b7bli\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der hat wohl sein Herz erbauet,", "tokens": ["Der", "hat", "wohl", "sein", "Herz", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der hat schon sein Haus beschicket,", "tokens": ["Der", "hat", "schon", "sein", "Haus", "be\u00b7schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eh' er sich der Welt vertrauet.", "tokens": ["Eh'", "er", "sich", "der", "Welt", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "M\u00e4gdlein, hast du keinen Spiegel,", "tokens": ["M\u00e4gd\u00b7lein", ",", "hast", "du", "kei\u00b7nen", "Spie\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dich in dich selber scheinet,", "tokens": ["Der", "dich", "in", "dich", "sel\u00b7ber", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Augen sind zwei Siegel,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "sind", "zwei", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Denen ganz dein Heil versteinet.", "tokens": ["De\u00b7nen", "ganz", "dein", "Heil", "ver\u00b7stei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00e4gdlein, senktest du die Augen,", "tokens": ["M\u00e4gd\u00b7lein", ",", "senk\u00b7test", "du", "die", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Endymion zu wecken,", "tokens": ["Den", "En\u00b7dy\u00b7mi\u00b7on", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "W\u00fcrdest du zu lieben taugen,", "tokens": ["W\u00fcr\u00b7dest", "du", "zu", "lie\u00b7ben", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nun taugst du nur zum Necken.", "tokens": ["Und", "nun", "taugst", "du", "nur", "zum", "Ne\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "M\u00e4gdlein, woll' zur Erde sehen,", "tokens": ["M\u00e4gd\u00b7lein", ",", "woll'", "zur", "Er\u00b7de", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasse deine Augen weiden,", "tokens": ["Las\u00b7se", "dei\u00b7ne", "Au\u00b7gen", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie werden auferstehen", "tokens": ["Und", "sie", "wer\u00b7den", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dich wie zwei Sterne kleiden.", "tokens": ["Und", "dich", "wie", "zwei", "Ster\u00b7ne", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOKOM", "CARD", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "M\u00e4gdlein, diese Augensterne", "tokens": ["M\u00e4gd\u00b7lein", ",", "die\u00b7se", "Au\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Magst du dann dem Himmel weihen;", "tokens": ["Magst", "du", "dann", "dem", "Him\u00b7mel", "wei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Erde lieben lerne,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "lie\u00b7ben", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dft du ihr die Augen leihen!", "tokens": ["Mu\u00dft", "du", "ihr", "die", "Au\u00b7gen", "lei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}