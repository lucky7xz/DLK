{"dta.poem.2898": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gute That befreit, die b\u00f6se That bestrickt;", "tokens": ["Die", "gu\u00b7te", "That", "be\u00b7freit", ",", "die", "b\u00f6\u00b7se", "That", "be\u00b7strickt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weit f\u00fchlt sein Herz, wer die, und eng, wer die beschickt.", "tokens": ["Weit", "f\u00fchlt", "sein", "Herz", ",", "wer", "die", ",", "und", "eng", ",", "wer", "die", "be\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "PWS", "ART", "$,", "KON", "ADJD", "$,", "PWS", "ART", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein jedes Band, in das du noch dich f\u00fchlst geschlagen,", "tokens": ["Ein", "je\u00b7des", "Band", ",", "in", "das", "du", "noch", "dich", "f\u00fchlst", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du gewirkt, und mu\u00dft es zu entwirken wagen.", "tokens": ["Hast", "du", "ge\u00b7wirkt", ",", "und", "mu\u00dft", "es", "zu", "ent\u00b7wir\u00b7ken", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KON", "VMFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier ist des Wirkens Zeit, drum wirk' und sei befreit!", "tokens": ["Hier", "ist", "des", "Wir\u00b7kens", "Zeit", ",", "drum", "wirk'", "und", "sei", "be\u00b7freit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "PAV", "VVFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer frei von hinnen geht, der ists in Ewigkeit.", "tokens": ["Wer", "frei", "von", "hin\u00b7nen", "geht", ",", "der", "ists", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ADV", "VVFIN", "$,", "PRELS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}