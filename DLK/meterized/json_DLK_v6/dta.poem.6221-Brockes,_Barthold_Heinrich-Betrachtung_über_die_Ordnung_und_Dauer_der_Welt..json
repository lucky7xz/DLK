{"dta.poem.6221": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die Ordnung  \n und Dauer der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oreine Lust erhabener Gem\u00fchter!", "tokens": ["Or\u00b7ei\u00b7ne", "Lust", "er\u00b7ha\u00b7be\u00b7ner", "Ge\u00b7m\u00fch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die so unendlich sch\u00f6ne G\u00fcter,", "tokens": ["Die", "so", "un\u00b7end\u00b7lich", "sch\u00f6\u00b7ne", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst in den Born des Lichtes finden,", "tokens": ["Selbst", "in", "den", "Born", "des", "Lich\u00b7tes", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gar ergr\u00fcnden,", "tokens": ["Und", "gar", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df unsre gantze Erde", "tokens": ["Da\u00df", "uns\u00b7re", "gant\u00b7ze", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu einem Staub-und Sand-Korn werde.", "tokens": ["Zu", "ei\u00b7nem", "Staub\u00b7\u00b7und", "San\u00b7dKorn", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zum P\u00fcnctgen, das die Augen gar nicht seh\u2019n;", "tokens": ["Zum", "P\u00fcnct\u00b7gen", ",", "das", "die", "Au\u00b7gen", "gar", "nicht", "seh'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erwegt man gegen ihm die weiten Himmels H\u00f6h\u2019n.", "tokens": ["Er\u00b7wegt", "man", "ge\u00b7gen", "ihm", "die", "wei\u00b7ten", "Him\u00b7mels", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}