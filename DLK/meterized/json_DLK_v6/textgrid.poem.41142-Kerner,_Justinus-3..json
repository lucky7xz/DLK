{"textgrid.poem.41142": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier das Tintenfa\u00df mit stummer Feder,", "tokens": ["Hier", "das", "Tin\u00b7ten\u00b7fa\u00df", "mit", "stum\u00b7mer", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn man's umdreht, sieht mit Staunen jeder:", "tokens": ["Wenn", "man's", "um\u00b7dreht", ",", "sieht", "mit", "Stau\u00b7nen", "je\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "APPR", "NN", "PIS", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie in einen D\u00e4mon tierisch kra\u00df", "tokens": ["Wie", "in", "ei\u00b7nen", "D\u00e4\u00b7mon", "tie\u00b7risch", "kra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sich umwandelt oft das Tintenfa\u00df.", "tokens": ["Sich", "um\u00b7wan\u00b7delt", "oft", "das", "Tin\u00b7ten\u00b7fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}