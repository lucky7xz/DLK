{"textgrid.poem.23989": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die junge Fr\u00fchlingssonne", "tokens": ["Die", "jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit zarten Strahlenf\u00e4dchen", "tokens": ["Mit", "zar\u00b7ten", "Strah\u00b7len\u00b7f\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Flirrt um die Jungfer Birke", "tokens": ["Flirrt", "um", "die", "Jung\u00b7fer", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mattgoldenes Filigran.", "tokens": ["Matt\u00b7gol\u00b7de\u00b7nes", "Fi\u00b7li\u00b7gran", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie eine Braut im Schmucke,", "tokens": ["Wie", "ei\u00b7ne", "Braut", "im", "Schmu\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sch\u00e4mig sch\u00f6n, jungfr\u00e4ulich,", "tokens": ["So", "sch\u00e4\u00b7mig", "sch\u00f6n", ",", "jung\u00b7fr\u00e4u\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Steht zwischen schwarzen Tannen", "tokens": ["Steht", "zwi\u00b7schen", "schwar\u00b7zen", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die schlanke junge Birke.", "tokens": ["Die", "schlan\u00b7ke", "jun\u00b7ge", "Bir\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt ich ein Bildchen malen", "tokens": ["K\u00f6nnt", "ich", "ein", "Bild\u00b7chen", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit zartgehauchten Farben,", "tokens": ["Mit", "zart\u00b7ge\u00b7hauch\u00b7ten", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich malte meine Birke", "tokens": ["Ich", "mal\u00b7te", "mei\u00b7ne", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In junger Fr\u00fchlingssonne.", "tokens": ["In", "jun\u00b7ger", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Himmel sollte sie k\u00fcssen,", "tokens": ["Der", "Him\u00b7mel", "soll\u00b7te", "sie", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der heiter helle Himmel,", "tokens": ["Der", "hei\u00b7ter", "hel\u00b7le", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eine wei\u00dfe Wolke", "tokens": ["Und", "ei\u00b7ne", "wei\u00b7\u00dfe", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schw\u00f6mme \u00fcber sie hin.", "tokens": ["Schw\u00f6m\u00b7me", "\u00fc\u00b7ber", "sie", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Das Gras zu ihren F\u00fc\u00dfen,", "tokens": ["Das", "Gras", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Halb hoch im Halm, durchflockt ich", "tokens": ["Halb", "hoch", "im", "Halm", ",", "durch\u00b7flockt", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit zarten Rosakelchen", "tokens": ["Mit", "zar\u00b7ten", "Ro\u00b7sa\u00b7kel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und blassen Margeriten.", "tokens": ["Und", "blas\u00b7sen", "Mar\u00b7ge\u00b7ri\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die sollten still wie Kinder", "tokens": ["Die", "soll\u00b7ten", "still", "wie", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aufblicken mit hellen Augen", "tokens": ["Auf\u00b7bli\u00b7cken", "mit", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur holden Jungfer Birke", "tokens": ["Zur", "hol\u00b7den", "Jung\u00b7fer", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In junger Fr\u00fchlingssonne.", "tokens": ["In", "jun\u00b7ger", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}