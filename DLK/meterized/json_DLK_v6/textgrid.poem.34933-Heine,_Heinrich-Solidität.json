{"textgrid.poem.34933": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Solidit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe sprach zum Gott der Lieder,", "tokens": ["Lie\u00b7be", "sprach", "zum", "Gott", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie verlange Sicherheiten,", "tokens": ["Sie", "ver\u00b7lan\u00b7ge", "Si\u00b7cher\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehe sie sich ganz ergebe,", "tokens": ["E\u00b7he", "sie", "sich", "ganz", "er\u00b7ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es w\u00e4ren schlechte Zeiten.", "tokens": ["Denn", "es", "w\u00e4\u00b7ren", "schlech\u00b7te", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lachend gab der Gott zur Antwort:", "tokens": ["La\u00b7chend", "gab", "der", "Gott", "zur", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbja, die Zeiten sich ver\u00e4ndern,", "tokens": ["\u00bb", "ja", ",", "die", "Zei\u00b7ten", "sich", "ver\u00b7\u00e4n\u00b7dern", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du sprichst jetzt wie ein alter", "tokens": ["Und", "du", "sprichst", "jetzt", "wie", "ein", "al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wuchrer, welcher leiht auf Pf\u00e4ndern.", "tokens": ["Wuch\u00b7rer", ",", "wel\u00b7cher", "leiht", "auf", "Pf\u00e4n\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, ich hab nur eine Leier,", "tokens": ["Ach", ",", "ich", "hab", "nur", "ei\u00b7ne", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sie ist von gutem Golde.", "tokens": ["Doch", "sie", "ist", "von", "gu\u00b7tem", "Gol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieviel K\u00fcsse willst du borgen", "tokens": ["Wie\u00b7viel", "K\u00fcs\u00b7se", "willst", "du", "bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir darauf, o meine Holde?\u00ab", "tokens": ["Mir", "da\u00b7rauf", ",", "o", "mei\u00b7ne", "Hol\u00b7de", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PAV", "$,", "FM", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}