{"dta.poem.10816": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Lehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Menschen kommt und fraget euch", "tokens": ["Ihr", "Men\u00b7schen", "kommt", "und", "fra\u00b7get", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem seid ihr von dem beiden gleich:", "tokens": ["Wem", "seid", "ihr", "von", "dem", "bei\u00b7den", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "PIAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lernt da\u00df vor ", "tokens": ["Lernt", "da\u00df", "vor"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KOUS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Noch ein Gebet von vielen Wizze.", "tokens": ["Noch", "ein", "Ge\u00b7bet", "von", "vie\u00b7len", "Wiz\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Opfer das der Glaube bringt,", "tokens": ["Das", "Op\u00b7fer", "das", "der", "Glau\u00b7be", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Seufzen da das Herze ringt,", "tokens": ["Ein", "Seuf\u00b7zen", "da", "das", "Her\u00b7ze", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist besser, als mit Heuchelwesen", "tokens": ["Ist", "bes\u00b7ser", ",", "als", "mit", "Heu\u00b7chel\u00b7we\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein langes Lobgebet herlesen.", "tokens": ["Ein", "lan\u00b7ges", "Lob\u00b7ge\u00b7bet", "her\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}