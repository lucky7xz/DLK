{"textgrid.poem.60674": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Esel, der Reliquien trug,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Esel, der Reliquien trug,", "tokens": ["Ein", "E\u00b7sel", ",", "der", "Re\u00b7li\u00b7qui\u00b7en", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "War dumm genug,", "tokens": ["War", "dumm", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu glauben, da\u00df man ", "tokens": ["Zu", "glau\u00b7ben", ",", "da\u00df", "man"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Weihrauch und Lobges\u00e4nge nahm", "tokens": ["Weih\u00b7rauch", "und", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", "nahm"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hin, als ob man's ihm beschere.", "tokens": ["Er", "hin", ",", "als", "ob", "man's", "ihm", "be\u00b7sche\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOKOM", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sprach einer, der dahinterkam:", "tokens": ["Sprach", "ei\u00b7ner", ",", "der", "da\u00b7hin\u00b7ter\u00b7kam", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbherr Langohr, la\u00dft die Eitelkeit,", "tokens": ["\u00bb", "herr", "Lan\u00b7gohr", ",", "la\u00dft", "die", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie zeigt uns nur, wie dumm Ihr seid.", "tokens": ["Sie", "zeigt", "uns", "nur", ",", "wie", "dumm", "Ihr", "seid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wir singen nicht zu Eurem Ruhm,", "tokens": ["Wir", "sin\u00b7gen", "nicht", "zu", "Eu\u00b7rem", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir ehren hier das Heiligtum.\u00ab", "tokens": ["Wir", "eh\u00b7ren", "hier", "das", "Hei\u00b7lig\u00b7tum", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Eitle Beamte tun mir leid:", "tokens": ["Eit\u00b7le", "Be\u00b7am\u00b7te", "tun", "mir", "leid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Man gr\u00fc\u00dft nicht sie, man gr\u00fc\u00dft das Kleid.", "tokens": ["Man", "gr\u00fc\u00dft", "nicht", "sie", ",", "man", "gr\u00fc\u00dft", "das", "Kleid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "PPER", "$,", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}