{"dta.poem.8885": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  \n  An Schelling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und lauschen Jeglichen auf seinem Sitze,", "tokens": ["Und", "lau\u00b7schen", "Jeg\u00b7li\u00b7chen", "auf", "sei\u00b7nem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da deines Geistes ungeheure Blitze", "tokens": ["Da", "dei\u00b7nes", "Geis\u00b7tes", "un\u00b7ge\u00b7heu\u00b7re", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schlag auf Schlag in unsre Seele drangen!", "tokens": ["Wie", "Schlag", "auf", "Schlag", "in", "uns\u00b7re", "See\u00b7le", "dran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn wir zerst\u00fcckelt nur die Welt empfangen,", "tokens": ["Wenn", "wir", "zer\u00b7st\u00fc\u00b7ckelt", "nur", "die", "Welt", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Siehst du sie ganz, wie von der Berge Spitze;", "tokens": ["Siehst", "du", "sie", "ganz", ",", "wie", "von", "der", "Ber\u00b7ge", "Spit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was wir zerpfl\u00fcckt mit unserm armen Witze,", "tokens": ["Was", "wir", "zer\u00b7pfl\u00fcckt", "mit", "un\u00b7serm", "ar\u00b7men", "Wit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist als Blume vor dir aufgegangen.", "tokens": ["Das", "ist", "als", "Blu\u00b7me", "vor", "dir", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch sieht man Thoren zwar, erbost dagegen,", "tokens": ["Noch", "sieht", "man", "Tho\u00b7ren", "zwar", ",", "er\u00b7bost", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "ADV", "$,", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit logischen Tiraden \u00fcberkleistern", "tokens": ["Mit", "lo\u00b7gi\u00b7schen", "Ti\u00b7ra\u00b7den", "\u00fc\u00b7ber\u00b7kleis\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Geistesarmuth Eyer, die sie legen;", "tokens": ["Der", "Geis\u00b7te\u00b7sar\u00b7muth", "Ey\u00b7er", ",", "die", "sie", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch dieses V\u00f6lkchen, das dich w\u00e4hnt zu meistern,", "tokens": ["Doch", "die\u00b7ses", "V\u00f6lk\u00b7chen", ",", "das", "dich", "w\u00e4hnt", "zu", "meis\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie wird's die Welt der Wissenschaft bewegen,", "tokens": ["Nie", "wird's", "die", "Welt", "der", "Wis\u00b7sen\u00b7schaft", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und einen Dichter wird es nie begeistern.", "tokens": ["Und", "ei\u00b7nen", "Dich\u00b7ter", "wird", "es", "nie", "be\u00b7geis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}