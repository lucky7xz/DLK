{"textgrid.poem.57981": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "19. Die beste Gesellschaft", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht, Kreaturen, geht, ich kann euch alle missen!", "tokens": ["Geht", ",", "Kre\u00b7a\u00b7tu\u00b7ren", ",", "geht", ",", "ich", "kann", "euch", "al\u00b7le", "mis\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein bester Freund, der bleibt, der mir so innig nah;", "tokens": ["Mein", "bes\u00b7ter", "Freund", ",", "der", "bleibt", ",", "der", "mir", "so", "in\u00b7nig", "nah", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kehr' ins K\u00e4mmerlein, ich will die T\u00fcren schlie\u00dfen", "tokens": ["Ich", "kehr'", "ins", "K\u00e4m\u00b7mer\u00b7lein", ",", "ich", "will", "die", "T\u00fc\u00b7ren", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meinem Seelenfreund Gesellschaft leisten da.", "tokens": ["Und", "mei\u00b7nem", "See\u00b7len\u00b7freund", "Ge\u00b7sell\u00b7schaft", "leis\u00b7ten", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn ich so bin von den Gesch\u00f6pfen einsam,", "tokens": ["Wenn", "ich", "so", "bin", "von", "den", "Ge\u00b7sch\u00f6p\u00b7fen", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So werd' ich mehr dem Sch\u00f6pfer selbst gemeinsam.", "tokens": ["So", "werd'", "ich", "mehr", "dem", "Sch\u00f6p\u00b7fer", "selbst", "ge\u00b7mein\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}