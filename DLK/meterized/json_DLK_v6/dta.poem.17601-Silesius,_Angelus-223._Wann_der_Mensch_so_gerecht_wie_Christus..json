{"dta.poem.17601": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "223.  Wann der Mensch so gerecht wie  \n Christus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wann du vollkommen Eins mit GOTT dem HErren", "tokens": ["Wann", "du", "voll\u00b7kom\u00b7men", "Eins", "mit", "GoTT", "dem", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "NN", "APPR", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bist/", "tokens": ["bist", "/"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So bistu so gerecht als unser Jesus Christ.", "tokens": ["So", "bis\u00b7tu", "so", "ge\u00b7recht", "als", "un\u00b7ser", "Je\u00b7sus", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "KOKOM", "PPOSAT", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}