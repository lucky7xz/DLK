{"dta.poem.11357": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als der Herr  Profess. J. F. Ludovici  in  \n Halle sein eintziges t\u00f6chterlein zur erden  \n bestattete. Jm nahmen seiner  \n tisch-compagnie.  \n G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So must du, zartes kind! schon auf die rauhe bahn,", "tokens": ["So", "must", "du", ",", "zar\u00b7tes", "kind", "!", "schon", "auf", "die", "rau\u00b7he", "bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die auch das alter kaum ohn zittern schauen kan!", "tokens": ["Die", "auch", "das", "al\u00b7ter", "kaum", "ohn", "zit\u00b7tern", "schau\u00b7en", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "ADV", "APPR", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O allzuherber tritt vor noch so weiche f\u00fcsse!", "tokens": ["O", "all\u00b7zu\u00b7her\u00b7ber", "tritt", "vor", "noch", "so", "wei\u00b7che", "f\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hat dein linder mund der eltern treue k\u00fcsse", "tokens": ["Es", "hat", "dein", "lin\u00b7der", "mund", "der", "el\u00b7tern", "treu\u00b7e", "k\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur kurtze zeit geschmeckt, so schenckt des todes hand", "tokens": ["Nur", "kurt\u00b7ze", "zeit", "ge\u00b7schmeckt", ",", "so", "schenckt", "des", "to\u00b7des", "hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir bittre myrrhen ein. Zu schneller unbestand!", "tokens": ["Dir", "bitt\u00b7re", "myr\u00b7rhen", "ein", ".", "Zu", "schnel\u00b7ler", "un\u00b7be\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach allzuschwerer schlu\u00df! Rinnt, rinnt betr\u00fcbte thr\u00e4nen!", "tokens": ["Ach", "all\u00b7zu\u00b7schwe\u00b7rer", "schlu\u00df", "!", "Rinnt", ",", "rinnt", "be\u00b7tr\u00fcb\u00b7te", "thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "$.", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Welch vater soll sich nicht nach einem kinde sehnen,", "tokens": ["Welch", "va\u00b7ter", "soll", "sich", "nicht", "nach", "ei\u00b7nem", "kin\u00b7de", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das er so fr\u00fch vermi\u00dft? Ja welche mutter sinckt,", "tokens": ["Das", "er", "so", "fr\u00fch", "ver\u00b7mi\u00dft", "?", "Ja", "wel\u00b7che", "mut\u00b7ter", "sinckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVPP", "$.", "PTKANT", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn man ihr ebenbild ihr aus den armen ringt,", "tokens": ["Wenn", "man", "ihr", "e\u00b7ben\u00b7bild", "ihr", "aus", "den", "ar\u00b7men", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn es der tod umfast, nicht in ein meer der z\u00e4hren?", "tokens": ["Wenn", "es", "der", "tod", "um\u00b7fast", ",", "nicht", "in", "ein", "meer", "der", "z\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PTKNEG", "APPR", "ART", "NN", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die knospe, die noch erst die blume will gew\u00e4hren,", "tokens": ["Die", "knos\u00b7pe", ",", "die", "noch", "erst", "die", "blu\u00b7me", "will", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist der bedaurung werth, wenn sie vom stocke bricht.", "tokens": ["Ist", "der", "be\u00b7dau\u00b7rung", "werth", ",", "wenn", "sie", "vom", "sto\u00b7cke", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wiewohl ein rosen-knopff gleicht Leonoren nicht,", "tokens": ["Wie\u00b7wohl", "ein", "ro\u00b7sen\u00b7knopff", "gleicht", "Le\u00b7o\u00b7no\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ob sie der unfall schon allhier zusammen paaret.", "tokens": ["Ob", "sie", "der", "un\u00b7fall", "schon", "all\u00b7hier", "zu\u00b7sam\u00b7men", "paa\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein kind ist doch weit mehr, als was ein krug verwahret,", "tokens": ["Ein", "kind", "ist", "doch", "weit", "mehr", ",", "als", "was", "ein", "krug", "ver\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ob es schen Indien uns theuer zugeschickt,", "tokens": ["Ob", "es", "schen", "In\u00b7di\u00b7en", "uns", "theu\u00b7er", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NE", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.18": {"text": "Weil GOtt sein ebenbild in seine seele dr\u00fcckt.", "tokens": ["Weil", "Gott", "sein", "e\u00b7ben\u00b7bild", "in", "sei\u00b7ne", "see\u00b7le", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drum, hoch-betr\u00fcbte! la\u00dft, la\u00dft eure thr\u00e4nen rollen,", "tokens": ["Drum", ",", "hoch\u00b7be\u00b7tr\u00fcb\u00b7te", "!", "la\u00dft", ",", "la\u00dft", "eu\u00b7re", "thr\u00e4\u00b7nen", "rol\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "$.", "VVFIN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die hier den perlen-schmuck zum krantze geben sollen,", "tokens": ["Die", "hier", "den", "per\u00b7len\u00b7schmuck", "zum", "krant\u00b7ze", "ge\u00b7ben", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der eure todte cr\u00f6nt! Ein kind von guter art,", "tokens": ["Der", "eu\u00b7re", "tod\u00b7te", "cr\u00f6nt", "!", "Ein", "kind", "von", "gu\u00b7ter", "art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "VVFIN", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "An dem der himmel nichts von seiner gunst gespart,", "tokens": ["An", "dem", "der", "him\u00b7mel", "nichts", "von", "sei\u00b7ner", "gunst", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist w\u00fcrdig, solchen schatz mit sich ins grab zu schliessen.", "tokens": ["Ist", "w\u00fcr\u00b7dig", ",", "sol\u00b7chen", "schatz", "mit", "sich", "ins", "grab", "zu", "schlies\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PIAT", "NN", "APPR", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie sollt auch nicht der brunn der augen sich ergiessen,", "tokens": ["Wie", "sollt", "auch", "nicht", "der", "brunn", "der", "au\u00b7gen", "sich", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn uns der tod ein st\u00fcck aus unsern hertzen rei\u00dft?", "tokens": ["Wenn", "uns", "der", "tod", "ein", "st\u00fcck", "aus", "un\u00b7sern", "hert\u00b7zen", "rei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das chor der Gratien, so seinen holden geist", "tokens": ["Das", "chor", "der", "Gra\u00b7ti\u00b7en", ",", "so", "sei\u00b7nen", "hol\u00b7den", "geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bald mit der ersten milch Lenoren eingefl\u00f6sset,", "tokens": ["Bald", "mit", "der", "ers\u00b7ten", "milch", "Le\u00b7no\u00b7ren", "ein\u00b7ge\u00b7fl\u00f6s\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Macht selbst die wangen na\u00df. Die eltern-liebe st\u00f6ffet", "tokens": ["Macht", "selbst", "die", "wan\u00b7gen", "na\u00df", ".", "Die", "el\u00b7tern\u00b7lie\u00b7be", "st\u00f6f\u00b7fet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dann billich seufftzer aus. Der anverwandten mund", "tokens": ["Dann", "bil\u00b7lich", "seufft\u00b7zer", "aus", ".", "Der", "an\u00b7ver\u00b7wand\u00b7ten", "mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Giebt bey der kleinen leich ihr grosses beyleyd kund.", "tokens": ["Giebt", "bey", "der", "klei\u00b7nen", "leich", "ihr", "gros\u00b7ses", "bey\u00b7leyd", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJD", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie pflantzen um den sarg die traurigen cypressen.", "tokens": ["Sie", "pflant\u00b7zen", "um", "den", "sarg", "die", "trau\u00b7ri\u00b7gen", "cyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wir k\u00f6nnen selbst hierbey die blumen nicht vergessen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "selbst", "hier\u00b7bey", "die", "blu\u00b7men", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die unsre tieffe pflicht um diese bahre streut.", "tokens": ["Die", "uns\u00b7re", "tief\u00b7fe", "pflicht", "um", "die\u00b7se", "bah\u00b7re", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Doch ist kein balsam da, der die durch traurigkeit", "tokens": ["Doch", "ist", "kein", "bal\u00b7sam", "da", ",", "der", "die", "durch", "trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "ADJD", "PTKVZ", "$,", "PRELS", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Geschlagne wunden heilt? Wir k\u00f6nnen nicht verneinen,", "tokens": ["Ge\u00b7schlag\u00b7ne", "wun\u00b7den", "heilt", "?", "Wir", "k\u00f6n\u00b7nen", "nicht", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df statt der sternen hier nur finstre wolcken scheinen,", "tokens": ["Da\u00df", "statt", "der", "ster\u00b7nen", "hier", "nur", "finst\u00b7re", "wol\u00b7cken", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und da\u00df gar wenig trost in unser hertze steigt,", "tokens": ["Und", "da\u00df", "gar", "we\u00b7nig", "trost", "in", "un\u00b7ser", "hert\u00b7ze", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn man der augen licht hin zu dem grabe neigt.", "tokens": ["Wenn", "man", "der", "au\u00b7gen", "licht", "hin", "zu", "dem", "gra\u00b7be", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Allein, wie offtermahls aus aufgeritzten b\u00e4umen", "tokens": ["Al\u00b7lein", ",", "wie", "off\u00b7ter\u00b7mahls", "aus", "auf\u00b7ge\u00b7ritz\u00b7ten", "b\u00e4u\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein heilsam wasser quillt, und meere perlen sch\u00e4umen,", "tokens": ["Ein", "heil\u00b7sam", "was\u00b7ser", "quillt", ",", "und", "mee\u00b7re", "per\u00b7len", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "$,", "KON", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wenn der erhitzte sturm die h\u00f6chsten wellen schmeist;", "tokens": ["Wenn", "der", "er\u00b7hitz\u00b7te", "sturm", "die", "h\u00f6chs\u00b7ten", "wel\u00b7len", "schmeist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So wird, wenn der von harm zu tieff gebogne geist", "tokens": ["So", "wird", ",", "wenn", "der", "von", "harm", "zu", "tieff", "ge\u00b7bog\u00b7ne", "geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ART", "APPR", "ADJD", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sich von dem schwartzen sarg auf jenes Eden lencket,", "tokens": ["Sich", "von", "dem", "schwart\u00b7zen", "sarg", "auf", "je\u00b7nes", "E\u00b7den", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "VVFIN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wo tausend sonnen sind, das, was es vor gekr\u00e4ncket,", "tokens": ["Wo", "tau\u00b7send", "son\u00b7nen", "sind", ",", "das", ",", "was", "es", "vor", "ge\u00b7kr\u00e4n\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VAFIN", "$,", "PDS", "$,", "PWS", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ein schatten, der so schnell, als wie ein blitz, vergeht.", "tokens": ["Ein", "schat\u00b7ten", ",", "der", "so", "schnell", ",", "als", "wie", "ein", "blitz", ",", "ver\u00b7geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Hier ist es, wo der brunn des wahren trostes steht,", "tokens": ["Hier", "ist", "es", ",", "wo", "der", "brunn", "des", "wah\u00b7ren", "tros\u00b7tes", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dahin kein Seneca betr\u00fcbte seelen f\u00fchret,", "tokens": ["Da\u00b7hin", "kein", "Se\u00b7ne\u00b7ca", "be\u00b7tr\u00fcb\u00b7te", "see\u00b7len", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NE", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So hoch er immer steigt. Ein Christ, der viel verliehret,", "tokens": ["So", "hoch", "er", "im\u00b7mer", "steigt", ".", "Ein", "Christ", ",", "der", "viel", "ver\u00b7lieh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Verliehret dennoch nichts, wenn nur sein geist erwacht,", "tokens": ["Ver\u00b7lieh\u00b7ret", "den\u00b7noch", "nichts", ",", "wenn", "nur", "sein", "geist", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und durch des hertzens krafft dem hertzen fl\u00fcgel macht,", "tokens": ["Und", "durch", "des", "hert\u00b7zens", "krafft", "dem", "hert\u00b7zen", "fl\u00fc\u00b7gel", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Um aus der finsterni\u00df sich dahin aufzuschwingen,", "tokens": ["Um", "aus", "der", "fins\u00b7ter\u00b7ni\u00df", "sich", "da\u00b7hin", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "VVFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Allwo sein alles wohnt, das alles wiederbringen,", "tokens": ["All\u00b7wo", "sein", "al\u00b7les", "wohnt", ",", "das", "al\u00b7les", "wie\u00b7der\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAINF", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und nichts verwerffen kan, was seinen sohn umfast.", "tokens": ["Und", "nichts", "ver\u00b7werf\u00b7fen", "kan", ",", "was", "sei\u00b7nen", "sohn", "um\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Di\u00df, hoch-betr\u00fcbtes haus! w\u00e4ltzt hoffentlich die last", "tokens": ["Di\u00df", ",", "hoch\u00b7be\u00b7tr\u00fcb\u00b7tes", "haus", "!", "w\u00e4ltzt", "hof\u00b7fent\u00b7lich", "die", "last"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ADJA", "NN", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.55": {"text": "Von der beklemmten brust. Denn der die kinder liebet,", "tokens": ["Von", "der", "be\u00b7klemm\u00b7ten", "brust", ".", "Denn", "der", "die", "kin\u00b7der", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Hat das beliebte kind, das ferner nichts betr\u00fcbet,", "tokens": ["Hat", "das", "be\u00b7lieb\u00b7te", "kind", ",", "das", "fer\u00b7ner", "nichts", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Als eine theure blum, ins paradie\u00df versetzt,", "tokens": ["Als", "ei\u00b7ne", "theu\u00b7re", "blum", ",", "ins", "pa\u00b7ra\u00b7die\u00df", "ver\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NE", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wo ihm kein winter dr\u00e4ut. Darum, hoch-edle! netzt", "tokens": ["Wo", "ihm", "kein", "win\u00b7ter", "dr\u00e4ut", ".", "Da\u00b7rum", ",", "hoch\u00b7ed\u00b7le", "!", "netzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$.", "PAV", "$,", "ADJA", "$.", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die wangen nur nicht mehr. Das, was der tod begr\u00e4bet,", "tokens": ["Die", "wan\u00b7gen", "nur", "nicht", "mehr", ".", "Das", ",", "was", "der", "tod", "be\u00b7gr\u00e4\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADV", "$.", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Ist nicht, was ihr beweint; denn Leonora lebet.", "tokens": ["Ist", "nicht", ",", "was", "ihr", "be\u00b7weint", ";", "denn", "Le\u00b7o\u00b7no\u00b7ra", "le\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$.", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}