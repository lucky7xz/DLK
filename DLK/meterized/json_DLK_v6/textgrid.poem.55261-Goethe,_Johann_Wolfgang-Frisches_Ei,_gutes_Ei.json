{"textgrid.poem.55261": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Frisches Ei, gutes Ei", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Enthusiasmus vergleich ich gern", "tokens": ["En\u00b7thu\u00b7si\u00b7as\u00b7mus", "ver\u00b7gleich", "ich", "gern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Auster, meine lieben Herrn,", "tokens": ["Der", "Aus\u00b7ter", ",", "mei\u00b7ne", "lie\u00b7ben", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, wenn ihr sie nicht frisch geno\u00dft,", "tokens": ["Die", ",", "wenn", "ihr", "sie", "nicht", "frisch", "ge\u00b7no\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wahrhaftig ist eine schlechte Kost.", "tokens": ["Wahr\u00b7haf\u00b7tig", "ist", "ei\u00b7ne", "schlech\u00b7te", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Begeistrung ist keine Heringsware,", "tokens": ["Be\u00b7geis\u00b7trung", "ist", "kei\u00b7ne", "He\u00b7rings\u00b7wa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die man einp\u00f6kelt auf einige Jahre.", "tokens": ["Die", "man", "ein\u00b7p\u00f6\u00b7kelt", "auf", "ei\u00b7ni\u00b7ge", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}}}}