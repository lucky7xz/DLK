{"textgrid.poem.64843": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als heute ich ein schlichtes Lied gelesen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als heute ich ein schlichtes Lied gelesen,", "tokens": ["Als", "heu\u00b7te", "ich", "ein", "schlich\u00b7tes", "Lied", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das du einst schriebst, an Lieb' und Jugend reich,", "tokens": ["Das", "du", "einst", "schriebst", ",", "an", "Lieb'", "und", "Ju\u00b7gend", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da zuckt' es schmerzlich durch mein tiefstes Wesen", "tokens": ["Da", "zuckt'", "es", "schmerz\u00b7lich", "durch", "mein", "tiefs\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und regungslos verharrt' ich stumm und bleich.", "tokens": ["Und", "re\u00b7gungs\u00b7los", "ver\u00b7harrt'", "ich", "stumm", "und", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Hand gepre\u00dft in meine dunkeln Haare", "tokens": ["Die", "Hand", "ge\u00b7pre\u00dft", "in", "mei\u00b7ne", "dun\u00b7keln", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Todtenklage im gebrochnen Blick,", "tokens": ["Und", "Tod\u00b7ten\u00b7kla\u00b7ge", "im", "ge\u00b7broch\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief ich mir jene l\u00e4ngstversunknen Jahre,", "tokens": ["Rief", "ich", "mir", "je\u00b7ne", "l\u00e4ngst\u00b7ver\u00b7sun\u00b7knen", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo jugendlich dein Herz gegl\u00fcht, zur\u00fcck.", "tokens": ["Wo", "ju\u00b7gend\u00b7lich", "dein", "Herz", "ge\u00b7gl\u00fcht", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Gott! so tief hast damals du empfunden!", "tokens": ["O", "Gott", "!", "so", "tief", "hast", "da\u00b7mals", "du", "emp\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "ADJD", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hei\u00df geliebt, so k\u00fchn und wild geha\u00dft!", "tokens": ["So", "hei\u00df", "ge\u00b7liebt", ",", "so", "k\u00fchn", "und", "wild", "ge\u00b7ha\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So leicht der Wahrheit Sonnenweg gefunden,", "tokens": ["So", "leicht", "der", "Wahr\u00b7heit", "Son\u00b7nen\u00b7weg", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein stolzer K\u00f6nigsadler ohne Rast!", "tokens": ["Ein", "stol\u00b7zer", "K\u00f6\u00b7ni\u00b7gsad\u00b7ler", "oh\u00b7ne", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jetzt! und jetzt! o sprich, wie soll ich's fassen? \u2013", "tokens": ["Und", "jetzt", "!", "und", "jetzt", "!", "o", "sprich", ",", "wie", "soll", "ich's", "fas\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$.", "KON", "ADV", "$.", "FM", "ADJD", "$,", "PWAV", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was in dein innerst Leben schien getaucht,", "tokens": ["Was", "in", "dein", "in\u00b7nerst", "Le\u00b7ben", "schien", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein str\u00f6mend Lieben, dein allm\u00e4chtig Hassen", "tokens": ["Dein", "str\u00f6\u00b7mend", "Lie\u00b7ben", ",", "dein", "all\u00b7m\u00e4ch\u00b7tig", "Has\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "$,", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versiegt ist's und entschlummert und verhaucht!", "tokens": ["Ver\u00b7siegt", "ist's", "und", "ent\u00b7schlum\u00b7mert", "und", "ver\u00b7haucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der schwerste Fluch hat furchtbar dich getroffen:", "tokens": ["Der", "schwers\u00b7te", "Fluch", "hat", "furcht\u00b7bar", "dich", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein greises Herz in einer jungen Brust,", "tokens": ["Ein", "grei\u00b7ses", "Herz", "in", "ei\u00b7ner", "jun\u00b7gen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das, schon zu alt zum Glauben und zum Hoffen,", "tokens": ["Das", ",", "schon", "zu", "alt", "zum", "Glau\u00b7ben", "und", "zum", "Hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "PTKA", "ADJD", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch Hohn spricht der Erinn'rung heil'ger Lust.", "tokens": ["Noch", "Hohn", "spricht", "der", "Er\u00b7inn'\u00b7rung", "heil'\u00b7ger", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Entthronter F\u00fcrst, dem t\u00fcckisch schlau das Leben", "tokens": ["Ent\u00b7thron\u00b7ter", "F\u00fcrst", ",", "dem", "t\u00fc\u00b7ckisch", "schlau", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Krone Zier, der Sch\u00e4tze Glanz entwand,", "tokens": ["Der", "Kro\u00b7ne", "Zier", ",", "der", "Sch\u00e4t\u00b7ze", "Glanz", "ent\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und zur Entsch\u00e4digung ihm nichts gegeben,", "tokens": ["Und", "zur", "Ent\u00b7sch\u00e4\u00b7di\u00b7gung", "ihm", "nichts", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als nur den kargen Jahrsgehalt: ", "tokens": ["Als", "nur", "den", "kar\u00b7gen", "Jahrs\u00b7ge\u00b7halt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, du bist klug! Allein soll ich es loben,", "tokens": ["Ja", ",", "du", "bist", "klug", "!", "Al\u00b7lein", "soll", "ich", "es", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$.", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, wo ein tr\u00e4um'risch Paradies einst stand,", "tokens": ["Da\u00df", ",", "wo", "ein", "tr\u00e4um'\u00b7risch", "Pa\u00b7ra\u00b7dies", "einst", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo sich ein Athos k\u00f6niglich erhoben,", "tokens": ["Wo", "sich", "ein", "A\u00b7thos", "k\u00f6\u00b7nig\u00b7lich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt nur ein wohlbestelltes Ackerland?", "tokens": ["Jetzt", "nur", "ein", "wohl\u00b7be\u00b7stell\u00b7tes", "A\u00b7cker\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Soll \u2013 doch genug der Worte nun, der herben!", "tokens": ["Soll", "\u2013", "doch", "ge\u00b7nug", "der", "Wor\u00b7te", "nun", ",", "der", "her\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$(", "ADV", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gel\u00f6s't ist ja auf ewig unser Bund!", "tokens": ["Ge\u00b7l\u00f6s't", "ist", "ja", "auf", "e\u00b7wig", "un\u00b7ser", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "APPR", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O k\u00f6nnt ich unvermi\u00dft und einsam sterben,", "tokens": ["O", "k\u00f6nnt", "ich", "un\u00b7ver\u00b7mi\u00dft", "und", "ein\u00b7sam", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein wundes Reh im tiefen Waldesgrund!", "tokens": ["Ein", "wun\u00b7des", "Reh", "im", "tie\u00b7fen", "Wal\u00b7des\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}