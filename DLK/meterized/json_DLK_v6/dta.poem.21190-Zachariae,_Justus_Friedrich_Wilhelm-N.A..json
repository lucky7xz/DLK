{"dta.poem.21190": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch, wie ists m\u00f6glich, da\u00df in steten Freuden", "tokens": ["Doch", ",", "wie", "ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "in", "ste\u00b7ten", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "VAFIN", "ADJD", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Weltmensch, o Messias, deine Leiden", "tokens": ["Der", "Welt\u00b7mensch", ",", "o", "Mes\u00b7si\u00b7as", ",", "dei\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "FM", "FM", "$,", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mit Dankbarkeit ermi\u00dft,", "tokens": ["Mit", "Dank\u00b7bar\u00b7keit", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und nicht vergi\u00dft?", "tokens": ["Und", "nicht", "ver\u00b7gi\u00dft", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie kan er beym Ger\u00e4usch der Saiten,", "tokens": ["Wie", "kan", "er", "beym", "Ge\u00b7r\u00e4usch", "der", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey Liedern der Sirenen;", "tokens": ["Bey", "Lie\u00b7dern", "der", "Si\u00b7re\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jm Strudel m\u00e4chtger Eitelkeiten,", "tokens": ["Jm", "Stru\u00b7del", "m\u00e4cht\u00b7ger", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu innrer Harmonie gestimmten T\u00f6nen,", "tokens": ["Zu", "inn\u00b7rer", "Har\u00b7mo\u00b7nie", "ge\u00b7stimm\u00b7ten", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und zu Empfindungen der Seraphim,", "tokens": ["Und", "zu", "Emp\u00b7fin\u00b7dun\u00b7gen", "der", "Se\u00b7ra\u00b7phim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Sein Herz gew\u00f6hnen:", "tokens": ["Sein", "Herz", "ge\u00b7w\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Da alle wilden Leidenschaften,", "tokens": ["Da", "al\u00b7le", "wil\u00b7den", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Emp\u00f6rt, und voller Ungest\u00fcm,", "tokens": ["Em\u00b7p\u00f6rt", ",", "und", "vol\u00b7ler", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dies Herz bestreiten.", "tokens": ["Dies", "Herz", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}