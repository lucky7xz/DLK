{"dta.poem.7543": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Thoren ists umsonst von einem Schaden heilen,", "tokens": ["Den", "Tho\u00b7ren", "ists", "um\u00b7sonst", "von", "ei\u00b7nem", "Scha\u00b7den", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn seine Thorheit wird sogleich zum andern eilen.", "tokens": ["Denn", "sei\u00b7ne", "Thor\u00b7heit", "wird", "sog\u00b7leich", "zum", "an\u00b7dern", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von einem Aeu\u00dfersten zum andern springt ein Thor;", "tokens": ["Von", "ei\u00b7nem", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7ten", "zum", "an\u00b7dern", "springt", "ein", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom rechten schiebt der Aff' die M\u00fctz' aufs linke Ohr.", "tokens": ["Vom", "rech\u00b7ten", "schiebt", "der", "Aff'", "die", "M\u00fctz'", "aufs", "lin\u00b7ke", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "mete