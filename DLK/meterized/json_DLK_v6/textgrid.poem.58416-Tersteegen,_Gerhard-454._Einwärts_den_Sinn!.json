{"textgrid.poem.58416": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "454. Einw\u00e4rts den Sinn!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00df zu die Fenster deiner Sinnen,", "tokens": ["Schlie\u00df", "zu", "die", "Fens\u00b7ter", "dei\u00b7ner", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst fasset dich der Geist der Welt,", "tokens": ["Sonst", "fas\u00b7set", "dich", "der", "Geist", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und merk auf Gottes Zug von innen,", "tokens": ["Und", "merk", "auf", "Got\u00b7tes", "Zug", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich so gern gesammelt h\u00e4lt!", "tokens": ["Der", "dich", "so", "gern", "ge\u00b7sam\u00b7melt", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er f\u00fchrt uns selbst in seinen Wegen,", "tokens": ["Er", "f\u00fchrt", "uns", "selbst", "in", "sei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch h\u00f6re, wie du wandeln mu\u00dft:", "tokens": ["Doch", "h\u00f6\u00b7re", ",", "wie", "du", "wan\u00b7deln", "mu\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Eigenheit gerad entgegen,", "tokens": ["Der", "Ei\u00b7gen\u00b7heit", "ge\u00b7rad", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So stirbet alle Sinnenlust!", "tokens": ["So", "stir\u00b7bet", "al\u00b7le", "Sin\u00b7nen\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}