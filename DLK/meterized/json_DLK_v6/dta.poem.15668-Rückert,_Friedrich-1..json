{"dta.poem.15668": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An einem Bache steht ein junger Rosenstrauch,", "tokens": ["An", "ei\u00b7nem", "Ba\u00b7che", "steht", "ein", "jun\u00b7ger", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wiegt sein bl\u00fchendes Gezweig im Fr\u00fchlingshauch.", "tokens": ["Und", "wiegt", "sein", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7zweig", "im", "Fr\u00fch\u00b7lings\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wurzel streckt er tief, k\u00fchl in die Flut hinein,", "tokens": ["Die", "Wur\u00b7zel", "streckt", "er", "tief", ",", "k\u00fchl", "in", "die", "Flut", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wandelt, was er saugt, in rothen Wangenschein.", "tokens": ["Und", "wan\u00b7delt", ",", "was", "er", "saugt", ",", "in", "ro\u00b7then", "Wan\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn den Purpurglanz abbleichte Sonnenglut,", "tokens": ["Und", "wenn", "den", "Pur\u00b7pur\u00b7glanz", "ab\u00b7bleich\u00b7te", "Son\u00b7nen\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die welken Bl\u00e4tter streut er wieder auf die Flut.", "tokens": ["Die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "streut", "er", "wie\u00b7der", "auf", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Froh sieht er auf der Flut die welken schwimmen nieder,", "tokens": ["Froh", "sieht", "er", "auf", "der", "Flut", "die", "wel\u00b7ken", "schwim\u00b7men", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sauget wohlgemut f\u00fcr frische Rosen wieder.", "tokens": ["Und", "sau\u00b7get", "wohl\u00b7ge\u00b7mut", "f\u00fcr", "fri\u00b7sche", "Ro\u00b7sen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Am Abend fl\u00fcstern ihm Betr\u00fcbtes L\u00fcfte vor;", "tokens": ["Am", "A\u00b7bend", "fl\u00fcs\u00b7tern", "ihm", "Be\u00b7tr\u00fcb\u00b7tes", "L\u00fcf\u00b7te", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch er, in Duft geh\u00fcllt, leiht ihnen kaum ein Ohr.", "tokens": ["Doch", "er", ",", "in", "Duft", "ge\u00b7h\u00fcllt", ",", "leiht", "ih\u00b7nen", "kaum", "ein", "Ohr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie fl\u00fcstern: Ach, der Bach, der so dich scheint zu laben,", "tokens": ["Sie", "fl\u00fcs\u00b7tern", ":", "Ach", ",", "der", "Bach", ",", "der", "so", "dich", "scheint", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ITJ", "$,", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird w\u00fchlend nach und nach den Grund dir untergraben.", "tokens": ["Wird", "w\u00fch\u00b7lend", "nach", "und", "nach", "den", "Grund", "dir", "un\u00b7ter\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "KON", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wohin du froherg\u00f6tzt wirfst deine Bl\u00fcten jetzt,", "tokens": ["Wo\u00b7hin", "du", "fro\u00b7her\u00b7g\u00f6tzt", "wirfst", "dei\u00b7ne", "Bl\u00fc\u00b7ten", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahin entsinkest du mit deinem Stamm zuletzt.", "tokens": ["Da\u00b7hin", "ent\u00b7sin\u00b7kest", "du", "mit", "dei\u00b7nem", "Stamm", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Darauf der Strauch im Traum mit s\u00fc\u00dfem L\u00e4chelduft:", "tokens": ["Da\u00b7rauf", "der", "Strauch", "im", "Traum", "mit", "s\u00fc\u00b7\u00dfem", "L\u00e4\u00b7chel\u00b7duft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wol bl\u00fcht des Lebens Baum nur auf des Todes Gruft.", "tokens": ["Wol", "bl\u00fcht", "des", "Le\u00b7bens", "Baum", "nur", "auf", "des", "To\u00b7des", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Drum lasset wohlgemut der k\u00fchlen Flut mich trinken,", "tokens": ["Drum", "las\u00b7set", "wohl\u00b7ge\u00b7mut", "der", "k\u00fch\u00b7len", "Flut", "mich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis ich werd' in der Flut ertrinken und versinken.", "tokens": ["Bis", "ich", "werd'", "in", "der", "Flut", "er\u00b7trin\u00b7ken", "und", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "La\u00dft mich nur bl\u00fchn, damit, wenn ich hinunter soll,", "tokens": ["La\u00dft", "mich", "nur", "bl\u00fchn", ",", "da\u00b7mit", ",", "wenn", "ich", "hin\u00b7un\u00b7ter", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$,", "PAV", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinunter ich im Strom noch schwimme rosenvoll.", "tokens": ["Hin\u00b7un\u00b7ter", "ich", "im", "Strom", "noch", "schwim\u00b7me", "ro\u00b7sen\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}