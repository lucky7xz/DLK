{"textgrid.poem.37795": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Schwartenhals", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam vor einer Frau Wirthin Haus,", "tokens": ["Ich", "kam", "vor", "ei\u00b7ner", "Frau", "Wirt\u00b7hin", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Man fragt mich, wer ich w\u00e4re,", "tokens": ["Man", "fragt", "mich", ",", "wer", "ich", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ein armer Schwartenhals,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "Schwar\u00b7ten\u00b7hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich e\u00df und trink so gerne.", "tokens": ["Ich", "e\u00df", "und", "trink", "so", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man f\u00fchrt mich in die Stuben ein,", "tokens": ["Man", "f\u00fchrt", "mich", "in", "die", "Stu\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da bot man mir zu trinken,", "tokens": ["Da", "bot", "man", "mir", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen lie\u00df ich umher gehn,", "tokens": ["Die", "Au\u00b7gen", "lie\u00df", "ich", "um\u00b7her", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Becher lie\u00df ich sinken.", "tokens": ["Den", "Be\u00b7cher", "lie\u00df", "ich", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man setzt mich oben an den Tisch,", "tokens": ["Man", "setzt", "mich", "o\u00b7ben", "an", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich ein Kaufherr w\u00e4re,", "tokens": ["Als", "ich", "ein", "Kauf\u00b7herr", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und da es an ein Zahlen ging,", "tokens": ["Und", "da", "es", "an", "ein", "Zah\u00b7len", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein S\u00e4ckel stand mir leere.", "tokens": ["Mein", "S\u00e4\u00b7ckel", "stand", "mir", "lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da ich des Nachts wollt schlafen gahn,", "tokens": ["Da", "ich", "des", "Nachts", "wollt", "schla\u00b7fen", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "VMFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man wie\u00df mich in die Scheuer,", "tokens": ["Man", "wie\u00df", "mich", "in", "die", "Scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ward mir armen Schwartenhals,", "tokens": ["Da", "ward", "mir", "ar\u00b7men", "Schwar\u00b7ten\u00b7hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Lachen viel zu theuer.", "tokens": ["Mein", "La\u00b7chen", "viel", "zu", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und da ich in die Scheuer kam,", "tokens": ["Und", "da", "ich", "in", "die", "Scheu\u00b7er", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hub ich an zu nisteln,", "tokens": ["Da", "hub", "ich", "an", "zu", "nis\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da stachen mich die Hagendorn,", "tokens": ["Da", "sta\u00b7chen", "mich", "die", "Ha\u00b7gen\u00b7dorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dazu die rauhen Disteln.", "tokens": ["Da\u00b7zu", "die", "rau\u00b7hen", "Dis\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da ich zu Morgens fr\u00fch aufstand,", "tokens": ["Da", "ich", "zu", "Mor\u00b7gens", "fr\u00fch", "auf\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Reif lag auf dem Dache,", "tokens": ["Der", "Reif", "lag", "auf", "dem", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00dft ich armer Schwartenhals", "tokens": ["Da", "mu\u00dft", "ich", "ar\u00b7mer", "Schwar\u00b7ten\u00b7hals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meins Ungl\u00fccks selber lachen.", "tokens": ["Meins", "Un\u00b7gl\u00fccks", "sel\u00b7ber", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich nahm mein Schwerd wohl in die Hand,", "tokens": ["Ich", "nahm", "mein", "Schwerd", "wohl", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00fcrt es an die Seiten,", "tokens": ["Und", "g\u00fcrt", "es", "an", "die", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich armer mu\u00dft zu Fu\u00dfe gehn,", "tokens": ["Ich", "ar\u00b7mer", "mu\u00dft", "zu", "Fu\u00b7\u00dfe", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich nicht hatt' zu reiten.", "tokens": ["Weil", "ich", "nicht", "hatt'", "zu", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich hob mich auf und ging davon", "tokens": ["Ich", "hob", "mich", "auf", "und", "ging", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht mich auf die Stra\u00dfen,", "tokens": ["Und", "macht", "mich", "auf", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir kam ein reicher Kaufmanns-Sohn,", "tokens": ["Mir", "kam", "ein", "rei\u00b7cher", "Kauf\u00b7manns\u00b7Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Tasch mu\u00dft er mir lassen.", "tokens": ["Sein", "Tasch", "mu\u00dft", "er", "mir", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}