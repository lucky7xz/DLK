{"textgrid.poem.59738": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Trost in Gott", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und willst du gar verzagen,", "tokens": ["Und", "willst", "du", "gar", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du armes Menschenherz,", "tokens": ["Du", "ar\u00b7mes", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Sorgen, \u00c4ngsten, Klagen,", "tokens": ["In", "Sor\u00b7gen", ",", "\u00c4ngs\u00b7ten", ",", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im feigen Erdenschmerz?", "tokens": ["Im", "fei\u00b7gen", "Er\u00b7den\u00b7schmerz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und missest du nach Spannen", "tokens": ["Und", "mis\u00b7sest", "du", "nach", "Span\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein kurzes Gl\u00fcck und Leid,", "tokens": ["Dein", "kur\u00b7zes", "Gl\u00fcck", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das rinnt geschwinde dannen", "tokens": ["Das", "rinnt", "ge\u00b7schwin\u00b7de", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ins Meer der Ewigkeit.", "tokens": ["Ins", "Meer", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nach oben mu\u00dft du sehen,", "tokens": ["Nach", "o\u00b7ben", "mu\u00dft", "du", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier unten findst du's nicht,", "tokens": ["Hier", "un\u00b7ten", "findst", "du's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur in den Himmelsh\u00f6hen,", "tokens": ["Nur", "in", "den", "Him\u00b7mels\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur da ist Trost und Licht;", "tokens": ["Nur", "da", "ist", "Trost", "und", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was hier die Stunden bringen,", "tokens": ["Was", "hier", "die", "Stun\u00b7den", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Macht Mut der St\u00e4rksten scheu,", "tokens": ["Macht", "Mut", "der", "St\u00e4rks\u00b7ten", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von oben mu\u00df dir klingen", "tokens": ["Von", "o\u00b7ben", "mu\u00df", "dir", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Klang von Gottes Treu'.", "tokens": ["Der", "Klang", "von", "Got\u00b7tes", "Treu'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom hohen Sterngew\u00f6lbe", "tokens": ["Vom", "ho\u00b7hen", "Stern\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herab erklingt der Klang:", "tokens": ["Her\u00b7ab", "er\u00b7klingt", "der", "Klang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Am hohen Sterngew\u00f6lbe", "tokens": ["Am", "ho\u00b7hen", "Stern\u00b7ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da strahlt in Sternenschrift", "tokens": ["Da", "strahlt", "in", "Ster\u00b7nen\u00b7schrift"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich der Glaube freue,", "tokens": ["Da\u00df", "sich", "der", "Glau\u00b7be", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df zittre Lug und Spott,", "tokens": ["Da\u00df", "zitt\u00b7re", "Lug", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Strahlt dort ", "tokens": ["Strahlt", "dort"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Dahin! Da ist dein Himmel,", "tokens": ["Da\u00b7hin", "!", "Da", "ist", "dein", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist dein Heimatland,", "tokens": ["Da", "ist", "dein", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das dir im Erdgewimmel", "tokens": ["Das", "dir", "im", "Erd\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verdunkeln Leid und Tand,", "tokens": ["Ver\u00b7dun\u00b7keln", "Leid", "und", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da klingen Wunderkl\u00e4nge,", "tokens": ["Da", "klin\u00b7gen", "Wun\u00b7der\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die machen frisch und neu,", "tokens": ["Die", "ma\u00b7chen", "frisch", "und", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da klingen die Ges\u00e4nge", "tokens": ["Da", "klin\u00b7gen", "die", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dahin! Dahin! Und lerne,", "tokens": ["Da\u00b7hin", "!", "Da\u00b7hin", "!", "Und", "ler\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$.", "PAV", "$.", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was so herniederklingt", "tokens": ["Was", "so", "her\u00b7nie\u00b7der\u00b7klingt"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf dem h\u00f6chsten Sterne", "tokens": ["Und", "auf", "dem", "h\u00f6chs\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Dann wird dir stets aufs neue", "tokens": ["Dann", "wird", "dir", "stets", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aufgehn sein Gnadenschein,", "tokens": ["Auf\u00b7gehn", "sein", "Gna\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er selbst, der Ewigtreue,", "tokens": ["Er", "selbst", ",", "der", "E\u00b7wig\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit, in und bei dir sein.", "tokens": ["Mit", ",", "in", "und", "bei", "dir", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "KON", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}