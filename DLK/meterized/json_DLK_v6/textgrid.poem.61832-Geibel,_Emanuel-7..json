{"textgrid.poem.61832": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verruchtes Dilettantenwesen!", "tokens": ["Ver\u00b7ruch\u00b7tes", "Di\u00b7let\u00b7tan\u00b7ten\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat einer wo ein gut Gedicht gelesen,", "tokens": ["Hat", "ei\u00b7ner", "wo", "ein", "gut", "Ge\u00b7dicht", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PWAV", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerpfl\u00fcckt er flugs den sch\u00f6nen Strau\u00df,", "tokens": ["Zer\u00b7pfl\u00fcckt", "er", "flugs", "den", "sch\u00f6\u00b7nen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tut Unkraut, Stroh und Disteln drunter", "tokens": ["Tut", "Un\u00b7kraut", ",", "Stroh", "und", "Dis\u00b7teln", "drun\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und bindet sich vergn\u00fcgt und munter", "tokens": ["Und", "bin\u00b7det", "sich", "ver\u00b7gn\u00fcgt", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Umsehn einen neuen draus.", "tokens": ["Im", "Um\u00b7sehn", "ei\u00b7nen", "neu\u00b7en", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}