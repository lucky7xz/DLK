{"textgrid.poem.63759": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie F\u00fcrsten dieser Welt habt ihr gewohnt", "tokens": ["Wie", "F\u00fcrs\u00b7ten", "die\u00b7ser", "Welt", "habt", "ihr", "ge\u00b7wohnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PDAT", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hoch \u00fcber Stadt und Land und Flutgebrause,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Stadt", "und", "Land", "und", "Flut\u00b7ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr schweigsam stolzen B\u00fc\u00dfer der Kartause,", "tokens": ["Ihr", "schweig\u00b7sam", "stol\u00b7zen", "B\u00fc\u00b7\u00dfer", "der", "Kar\u00b7tau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stumm, weil nur Gottes Wort der M\u00fche lohnt.", "tokens": ["Stumm", ",", "weil", "nur", "Got\u00b7tes", "Wort", "der", "M\u00fc\u00b7he", "lohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ADV", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein Papst noch Kaiser, der so schimmernd thront,", "tokens": ["Kein", "Papst", "noch", "Kai\u00b7ser", ",", "der", "so", "schim\u00b7mernd", "thront", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kunst und Natur umbl\u00fchten eure Klause;", "tokens": ["Kunst", "und", "Na\u00b7tur", "um\u00b7bl\u00fch\u00b7ten", "eu\u00b7re", "Klau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sant' Elmos Fort war Schirmvogt eurem Hause,", "tokens": ["Sant'", "El\u00b7mos", "Fort", "war", "Schirm\u00b7vogt", "eu\u00b7rem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Sch\u00e4tze h\u00e4ufte, die der Rost verschont.", "tokens": ["Das", "Sch\u00e4t\u00b7ze", "h\u00e4uf\u00b7te", ",", "die", "der", "Rost", "ver\u00b7schont", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun hat man euch zur Welt zur\u00fcckgetrieben.", "tokens": ["Nun", "hat", "man", "euch", "zur", "Welt", "zu\u00b7r\u00fcck\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nichts mehr von all dem Glanze blieb euch eigen,", "tokens": ["Nichts", "mehr", "von", "all", "dem", "Glan\u00b7ze", "blieb", "euch", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PIAT", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht eures Kreuzgangs k\u00fchler Marmorfrieden.", "tokens": ["Nicht", "eu\u00b7res", "Kreuz\u00b7gangs", "k\u00fch\u00b7ler", "Mar\u00b7mor\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wenn ihr ", "tokens": ["Doch", "wenn", "ihr"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Denn wer da wei\u00df zu schauen und zu schweigen,", "tokens": ["Denn", "wer", "da", "wei\u00df", "zu", "schau\u00b7en", "und", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleibt, auch entthront, ein F\u00fcrst der Welt hinieden.", "tokens": ["Bleibt", ",", "auch", "ent\u00b7thront", ",", "ein", "F\u00fcrst", "der", "Welt", "hi\u00b7nie\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVPP", "$,", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}