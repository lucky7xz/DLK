{"textgrid.poem.51261": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Narr des Grafen von Zimmern", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was rollt so zierlich, klingt so lieb", "tokens": ["Was", "rollt", "so", "zier\u00b7lich", ",", "klingt", "so", "lieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$,", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treppauf und -ab im Schlo\u00df?", "tokens": ["Trep\u00b7pauf", "und", "im", "Schlo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das ist des Grafen Zeitvertrieb", "tokens": ["Das", "ist", "des", "Gra\u00b7fen", "Zeit\u00b7ver\u00b7trieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00fcndlicher Geno\u00df:", "tokens": ["Und", "st\u00fcnd\u00b7li\u00b7cher", "Ge\u00b7no\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Narr, annoch ein halbes Kind", "tokens": ["Sein", "Narr", ",", "an\u00b7noch", "ein", "hal\u00b7bes", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rosiges Gesellchen,", "tokens": ["Und", "ro\u00b7si\u00b7ges", "Ge\u00b7sell\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So leicht und luftig wie der Wind,", "tokens": ["So", "leicht", "und", "luf\u00b7tig", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und tr\u00e4gt den Kopf voll Schellchen.", "tokens": ["Und", "tr\u00e4gt", "den", "Kopf", "voll", "Schell\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch ohne Arg, wie ohne Bart,", "tokens": ["Noch", "oh\u00b7ne", "Arg", ",", "wie", "oh\u00b7ne", "Bart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Possen reich genug,", "tokens": ["An", "Pos\u00b7sen", "reich", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist doch der Fant von guter Art", "tokens": ["Ist", "doch", "der", "Fant", "von", "gu\u00b7ter", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Torheit klug;", "tokens": ["Und", "in", "der", "Tor\u00b7heit", "klug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und was vergecken und verdrehn", "tokens": ["Und", "was", "ver\u00b7ge\u00b7cken", "und", "ver\u00b7drehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die zappeligen H\u00e4nde,", "tokens": ["Die", "zap\u00b7pe\u00b7li\u00b7gen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ger\u00e4t ihm oft wie aus Versehn", "tokens": ["Ge\u00b7r\u00e4t", "ihm", "oft", "wie", "aus", "Ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zuletzt zum guten Ende.", "tokens": ["Zu\u00b7letzt", "zum", "gu\u00b7ten", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Graf mit seinem Hofgesind", "tokens": ["Der", "Graf", "mit", "sei\u00b7nem", "Hof\u00b7ge\u00b7sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weilt in der Burgkapell,", "tokens": ["Weilt", "in", "der", "Burg\u00b7ka\u00b7pell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da ist, wie schon das Amt beginnt,", "tokens": ["Da", "ist", ",", "wie", "schon", "das", "Amt", "be\u00b7ginnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Ministrant zur Stell;", "tokens": ["Kein", "Mi\u00b7nis\u00b7trant", "zur", "Stell", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Rasch nimmt der Pfaff den Narrn beim Ohr", "tokens": ["Rasch", "nimmt", "der", "Pfaff", "den", "Narrn", "beim", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NE", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht ihn zum Altare;", "tokens": ["Und", "zieht", "ihn", "zum", "Al\u00b7ta\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Knabe sieht sich flei\u00dfig vor,", "tokens": ["Der", "Kna\u00b7be", "sieht", "sich", "flei\u00b7\u00dfig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df er nach Br\u00e4uchen fahre.", "tokens": ["Da\u00df", "er", "nach", "Br\u00e4u\u00b7chen", "fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und gut, als w\u00e4r er's l\u00e4ngst gewohnt,", "tokens": ["Und", "gut", ",", "als", "w\u00e4r", "er's", "l\u00e4ngst", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOKOM", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedient er den Kaplan;", "tokens": ["Be\u00b7dient", "er", "den", "Ka\u00b7plan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn's die M\u00fch am besten lohnt,", "tokens": ["Doch", "wenn's", "die", "M\u00fch", "am", "bes\u00b7ten", "lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bricht oft der Unstern an:", "tokens": ["Bricht", "oft", "der", "Uns\u00b7tern", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn als die heil'ge Hostia", "tokens": ["Denn", "als", "die", "heil'\u00b7ge", "Hos\u00b7tia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Priester wird erhoben,", "tokens": ["Vom", "Pries\u00b7ter", "wird", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Schreck! so ist kein Gl\u00f6cklein da,", "tokens": ["O", "Schreck", "!", "so", "ist", "kein", "Gl\u00f6c\u00b7klein", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den s\u00fc\u00dfen Gott zu loben!", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Gott", "zu", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Weilchen bleibt es totenstill;", "tokens": ["Ein", "Weil\u00b7chen", "bleibt", "es", "to\u00b7tens\u00b7till", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbleichend lauscht der Graf,", "tokens": ["Er\u00b7blei\u00b7chend", "lauscht", "der", "Graf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der gleich ein Unheil ahnen will,", "tokens": ["Der", "gleich", "ein", "Un\u00b7heil", "ah\u00b7nen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ihn vom Himmel traf.", "tokens": ["Das", "ihn", "vom", "Him\u00b7mel", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch schon hat sich der Narr bedacht,", "tokens": ["Doch", "schon", "hat", "sich", "der", "Narr", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Handel zu vers\u00f6hnen:", "tokens": ["Den", "Han\u00b7del", "zu", "ver\u00b7s\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Kappe sch\u00fcttelt er mit Macht,", "tokens": ["Die", "Kap\u00b7pe", "sch\u00fct\u00b7telt", "er", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df alle Gl\u00f6cklein t\u00f6nen!", "tokens": ["Da\u00df", "al\u00b7le", "Gl\u00f6c\u00b7klein", "t\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da strahlt von dem Ciborium", "tokens": ["Da", "strahlt", "von", "dem", "Ci\u00b7bo\u00b7ri\u00b7um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ein goldnes Leuchten aus;", "tokens": ["Ein", "gold\u00b7nes", "Leuch\u00b7ten", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gl\u00e4nzt und duftet um und um", "tokens": ["Es", "gl\u00e4nzt", "und", "duf\u00b7tet", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "KON", "KOUI"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im kleinen Gotteshaus,", "tokens": ["Im", "klei\u00b7nen", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie wenn des Himmels Majest\u00e4t", "tokens": ["Wie", "wenn", "des", "Him\u00b7mels", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In frischen Veilchen l\u00e4ge:", "tokens": ["In", "fri\u00b7schen", "Veil\u00b7chen", "l\u00e4\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Herr, der durch die Wandlung geht \u2013", "tokens": ["Der", "Herr", ",", "der", "durch", "die", "Wand\u00b7lung", "geht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er l\u00e4chelt auf dem Wege!\u00ab", "tokens": ["Er", "l\u00e4\u00b7chelt", "auf", "dem", "We\u00b7ge", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}