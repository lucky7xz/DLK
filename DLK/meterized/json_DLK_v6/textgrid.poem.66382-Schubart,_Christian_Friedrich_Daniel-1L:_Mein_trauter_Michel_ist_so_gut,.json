{"textgrid.poem.66382": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein trauter Michel ist so gut,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein trauter Michel ist so gut,", "tokens": ["Mein", "trau\u00b7ter", "Mi\u00b7chel", "ist", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gut wie er gibt's keinen;", "tokens": ["So", "gut", "wie", "er", "gibt's", "kei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VVFIN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ihn mein Auge sehen thut,", "tokens": ["Wenn", "ihn", "mein", "Au\u00b7ge", "se\u00b7hen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So m\u00f6cht's vor Freuden weinen.", "tokens": ["So", "m\u00f6cht's", "vor", "Freu\u00b7den", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Apfel ist so roth und rund", "tokens": ["Kein", "Ap\u00b7fel", "ist", "so", "roth", "und", "rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sein Gesicht und Wangen;", "tokens": ["Wie", "sein", "Ge\u00b7sicht", "und", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Rosenbl\u00e4tter ist sein Mund,", "tokens": ["Wie", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "ist", "sein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dran Honigtropfen hangen.", "tokens": ["Dran", "Ho\u00b7nig\u00b7trop\u00b7fen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Aeugelein sind rund und scharf", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "sind", "rund", "und", "scharf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als wie Rebh\u00fchneraugen;", "tokens": ["Als", "wie", "Reb\u00b7h\u00fch\u00b7ner\u00b7au\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnten, wenn man's sagen darf,", "tokens": ["Sie", "k\u00f6nn\u00b7ten", ",", "wenn", "man's", "sa\u00b7gen", "darf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nachts f\u00fcr Sternlein taugen.", "tokens": ["Des", "Nachts", "f\u00fcr", "Stern\u00b7lein", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer ist so flink, und rasch wie er,", "tokens": ["Wer", "ist", "so", "flink", ",", "und", "rasch", "wie", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "KON", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Tanzen, Werfen, Springen;", "tokens": ["Im", "Tan\u00b7zen", ",", "Wer\u00b7fen", ",", "Sprin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer kann im Dorfe trefflicher", "tokens": ["Wer", "kann", "im", "Dor\u00b7fe", "treff\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPRART", "NN", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zum Dudelsacke singen?", "tokens": ["Zum", "Du\u00b7del\u00b7sa\u00b7cke", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer ist so launig, so voll Scherz", "tokens": ["Wer", "ist", "so", "lau\u00b7nig", ",", "so", "voll", "Scherz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Flegel und der Sichel;", "tokens": ["Beim", "Fle\u00b7gel", "und", "der", "Si\u00b7chel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer hat ein so gutes Herz,", "tokens": ["Und", "wer", "hat", "ein", "so", "gu\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie mein lieber Michel?", "tokens": ["Als", "wie", "mein", "lie\u00b7ber", "Mi\u00b7chel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "ADV", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denkt nur, er ist erst achtzehn Jahr;", "tokens": ["Denkt", "nur", ",", "er", "ist", "erst", "acht\u00b7zehn", "Jahr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sieht's an seinem Kinne,", "tokens": ["Man", "sieht's", "an", "sei\u00b7nem", "Kin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am schlanken Wuchs, am weichen Haar,", "tokens": ["Am", "schlan\u00b7ken", "Wuchs", ",", "am", "wei\u00b7chen", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und an der hellen Miene.", "tokens": ["Und", "an", "der", "hel\u00b7len", "Mie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wei\u00df wohl, es gibt der M\u00e4dels mehr,", "tokens": ["Wei\u00df", "wohl", ",", "es", "gibt", "der", "M\u00e4\u00b7dels", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen Michel lieben;", "tokens": ["Die", "mei\u00b7nen", "Mi\u00b7chel", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum f\u00e4llt's mir armen M\u00e4del schwer,", "tokens": ["Drum", "f\u00e4llt's", "mir", "ar\u00b7men", "M\u00e4\u00b7del", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hochzeit zu verschieben.", "tokens": ["Die", "Hoch\u00b7zeit", "zu", "ver\u00b7schie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Noch heute werd' ich seine Frau,", "tokens": ["Noch", "heu\u00b7te", "werd'", "ich", "sei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wahr ich Lisel hei\u00dfe!", "tokens": ["So", "wahr", "ich", "Li\u00b7sel", "hei\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nicht ein andres M\u00e4dchen schlau", "tokens": ["Da\u00df", "nicht", "ein", "and\u00b7res", "M\u00e4d\u00b7chen", "schlau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Michel mir entrei\u00dfe.", "tokens": ["Den", "Mi\u00b7chel", "mir", "ent\u00b7rei\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}