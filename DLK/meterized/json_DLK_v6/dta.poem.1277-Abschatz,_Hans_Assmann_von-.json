{"dta.poem.1277": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Schlaf/ angenehmes Kind der stillen Nacht/", "tokens": ["Schlaf", "/", "an\u00b7ge\u00b7neh\u00b7mes", "Kind", "der", "stil\u00b7len", "Nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Arzt der von M\u00fcdigkeit erstorbnen Geister/", "tokens": ["Arzt", "der", "von", "M\u00fc\u00b7dig\u00b7keit", "ers\u00b7torb\u00b7nen", "Geis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Kummers Feind und Tod/ der Sorgen Meister/", "tokens": ["Des", "Kum\u00b7mers", "Feind", "und", "Tod", "/", "der", "Sor\u00b7gen", "Meis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Warum halt ich allein in meinem Bette/", "tokens": ["Wa\u00b7rum", "halt", "ich", "al\u00b7lein", "in", "mei\u00b7nem", "Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(das mir der Unruh Grab soll seyn) die Wette/", "tokens": ["(", "das", "mir", "der", "Un\u00b7ruh", "Grab", "soll", "seyn", ")", "die", "Wet\u00b7te", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "NN", "NN", "VMFIN", "VAINF", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit tausend Sternen an dem Himmel Wacht?", "tokens": ["Mit", "tau\u00b7send", "Ster\u00b7nen", "an", "dem", "Him\u00b7mel", "Wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ist dein s\u00fcsses Thun/ die stille Ruh?", "tokens": ["Wo", "ist", "dein", "s\u00fcs\u00b7ses", "Thun", "/", "die", "stil\u00b7le", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo sind die Tr\u00e4ume/ die uns zu vergn\u00fcgen", "tokens": ["Wo", "sind", "die", "Tr\u00e4u\u00b7me", "/", "die", "uns", "zu", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$(", "PRELS", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bey brauner Nacht um unser Lager fliegen?", "tokens": ["Bey", "brau\u00b7ner", "Nacht", "um", "un\u00b7ser", "La\u00b7ger", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du selber schl\u00e4ffst/ o Schlaf/ wie ich vermeyne:", "tokens": ["Du", "sel\u00b7ber", "schl\u00e4ffst", "/", "o", "Schlaf", "/", "wie", "ich", "ver\u00b7mey\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "FM", "NN", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Komm/ du des Todes Bild/ komm und erscheine/", "tokens": ["Komm", "/", "du", "des", "To\u00b7des", "Bild", "/", "komm", "und", "er\u00b7schei\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "ART", "NN", "NN", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schleu\u00df dieses m\u00fcde Paar der Augen zu.", "tokens": ["Schleu\u00df", "die\u00b7ses", "m\u00fc\u00b7de", "Paar", "der", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}