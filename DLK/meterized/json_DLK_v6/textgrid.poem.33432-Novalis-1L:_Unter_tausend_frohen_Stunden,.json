{"textgrid.poem.33432": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter tausend frohen Stunden,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter tausend frohen Stunden,", "tokens": ["Un\u00b7ter", "tau\u00b7send", "fro\u00b7hen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So im Leben ich gefunden,", "tokens": ["So", "im", "Le\u00b7ben", "ich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blieb nur eine mir getreu;", "tokens": ["Blieb", "nur", "ei\u00b7ne", "mir", "ge\u00b7treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine, wo in tausend Schmerzen", "tokens": ["Ei\u00b7ne", ",", "wo", "in", "tau\u00b7send", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich erfuhr in meinem Herzen,", "tokens": ["Ich", "er\u00b7fuhr", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer f\u00fcr uns gestorben sey.", "tokens": ["Wer", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Welt war mir zerbrochen,", "tokens": ["Mei\u00b7ne", "Welt", "war", "mir", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von einem Wurm gestochen", "tokens": ["Wie", "von", "ei\u00b7nem", "Wurm", "ge\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welkte Herz und Bl\u00fcthe mir;", "tokens": ["Welk\u00b7te", "Herz", "und", "Bl\u00fc\u00b7the", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Lebens ganze Habe,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "gan\u00b7ze", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Wunsch lag mir im Grabe,", "tokens": ["Je\u00b7der", "Wunsch", "lag", "mir", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zur Qual war ich noch hier.", "tokens": ["Und", "zur", "Qual", "war", "ich", "noch", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Da ich so im stillen krankte,", "tokens": ["Da", "ich", "so", "im", "stil\u00b7len", "krank\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ewig weint' und wegverlangte,", "tokens": ["E\u00b7wig", "weint'", "und", "weg\u00b7ver\u00b7lang\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur blieb vor Angst und Wahn:", "tokens": ["Und", "nur", "blieb", "vor", "Angst", "und", "Wahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward mir pl\u00f6tzlich, wie von oben", "tokens": ["Ward", "mir", "pl\u00f6tz\u00b7lich", ",", "wie", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PWAV", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg des Grabes Stein gehoben,", "tokens": ["Weg", "des", "Gra\u00b7bes", "Stein", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Innres aufgetan.", "tokens": ["Und", "mein", "Inn\u00b7res", "auf\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wen ich sah, und wen an seiner", "tokens": ["Wen", "ich", "sah", ",", "und", "wen", "an", "sei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "PWS", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hand erblickte, frage Keiner,", "tokens": ["Hand", "er\u00b7blick\u00b7te", ",", "fra\u00b7ge", "Kei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig werd' ich die\u00df nur sehn;", "tokens": ["E\u00b7wig", "werd'", "ich", "die\u00df", "nur", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PDS", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von allen Lebensstunden", "tokens": ["Und", "von", "al\u00b7len", "Le\u00b7bens\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird nur die, wie meine Wunden", "tokens": ["Wird", "nur", "die", ",", "wie", "mei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig heiter, offen stehn.", "tokens": ["E\u00b7wig", "hei\u00b7ter", ",", "of\u00b7fen", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}