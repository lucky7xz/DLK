{"textgrid.poem.50122": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Fand aber keine Freud' im Ehstand, biss zu letzt", "tokens": ["Fand", "a\u00b7ber", "kei\u00b7ne", "Freud'", "im", "Eh\u00b7stand", ",", "biss", "zu", "letzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "$,", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Unlust seine Frau in eine Schwindsucht setzt',", "tokens": ["Die", "Un\u00b7lust", "sei\u00b7ne", "Frau", "in", "ei\u00b7ne", "Schwind\u00b7sucht", "setzt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ihm erlaubet war auf ihren Tod zu hoffen:", "tokens": ["Und", "ihm", "er\u00b7lau\u00b7bet", "war", "auf", "ih\u00b7ren", "Tod", "zu", "hof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So falsch und wunderlich sind der Begierde Trieb'!", "tokens": ["So", "falsch", "und", "wun\u00b7der\u00b7lich", "sind", "der", "Be\u00b7gier\u00b7de", "Trieb'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was sie leidlich macht, ist ", "tokens": ["Und", "was", "sie", "leid\u00b7lich", "macht", ",", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}