{"textgrid.poem.43425": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was k\u00fcmmert Euch die Wirklichkeit?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00fcmmert Euch die Wirklichkeit?", "tokens": ["Was", "k\u00fcm\u00b7mert", "Euch", "die", "Wirk\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ist euch nicht genehm.", "tokens": ["Die", "ist", "euch", "nicht", "ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr stellt euch au\u00dfer aller Zeit,", "tokens": ["Ihr", "stellt", "euch", "au\u00b7\u00dfer", "al\u00b7ler", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stellung ist bequem.", "tokens": ["Die", "Stel\u00b7lung", "ist", "be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch liegt so au\u00dfer unsrer Zeit", "tokens": ["Doch", "liegt", "so", "au\u00b7\u00dfer", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch eure ganze Wirksamkeit.", "tokens": ["Auch", "eu\u00b7re", "gan\u00b7ze", "Wirk\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr habet Kunst und Wissenschaft,", "tokens": ["Ihr", "ha\u00b7bet", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Habt herrliche Ideen,", "tokens": ["Habt", "herr\u00b7li\u00b7che", "I\u00b7deen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und habt doch weder Muth noch Kraft", "tokens": ["Und", "habt", "doch", "we\u00b7der", "Muth", "noch", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus euch herauszugehn.", "tokens": ["Aus", "euch", "her\u00b7aus\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer auf die Zeit nicht gehet ein,", "tokens": ["Wer", "auf", "die", "Zeit", "nicht", "ge\u00b7het", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann in der Zeit nicht wirksam sein.", "tokens": ["Kann", "in", "der", "Zeit", "nicht", "wirk\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir stehn auf ", "tokens": ["Wir", "stehn", "auf"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wir stehn in ", "tokens": ["Wir", "stehn", "in"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mit Liebe, Freud' und Leid;", "tokens": ["Mit", "Lie\u00b7be", ",", "Freud'", "und", "Leid", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und jeder Odem, jedes Wort,", "tokens": ["Und", "je\u00b7der", "O\u00b7dem", ",", "je\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es findet seinen Fleck und Ort.", "tokens": ["Es", "fin\u00b7det", "sei\u00b7nen", "Fleck", "und", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}