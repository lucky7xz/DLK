{"dta.poem.11202": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie eine landschafft mit der nadel  \n st\u00fcckte.  \n R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oorpheus! lege nur die wunder-harffe hin;", "tokens": ["O\u00b7or\u00b7pheus", "!", "le\u00b7ge", "nur", "die", "wun\u00b7der\u00b7ha\u00b7rf\u00b7fe", "hin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der nadel suchen b\u00e4um\u2019, und thiere nachzuziehn.", "tokens": ["Der", "na\u00b7del", "su\u00b7chen", "b\u00e4um'", ",", "und", "thie\u00b7re", "nach\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}