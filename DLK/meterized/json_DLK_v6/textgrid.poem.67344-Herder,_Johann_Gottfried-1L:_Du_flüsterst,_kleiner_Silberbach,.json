{"textgrid.poem.67344": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fl\u00fcsterst, kleiner Silberbach,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fl\u00fcsterst, kleiner Silberbach,", "tokens": ["Du", "fl\u00fcs\u00b7terst", ",", "klei\u00b7ner", "Sil\u00b7ber\u00b7bach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kosen sanfter Wellen", "tokens": ["Im", "Ko\u00b7sen", "sanf\u00b7ter", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebe s\u00fc\u00dfe W\u00fcnsche nach,", "tokens": ["Der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "W\u00fcn\u00b7sche", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die meinen Busen schwellen.", "tokens": ["Die", "mei\u00b7nen", "Bu\u00b7sen", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voll Ruh, wie Du,", "tokens": ["Voll", "Ruh", ",", "wie", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ist meine Vielgeliebte;", "tokens": ["Ist", "mei\u00b7ne", "Viel\u00b7ge\u00b7lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O, da\u00df nie Sturm und Ungemach", "tokens": ["O", ",", "da\u00df", "nie", "Sturm", "und", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ihr sch\u00f6nes Leben tr\u00fcbte!", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Le\u00b7ben", "tr\u00fcb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, dieses Eichthals Widerhall,", "tokens": ["Du", ",", "die\u00b7ses", "Eicht\u00b7hals", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernimm der Treue Lieder!", "tokens": ["Ver\u00b7nimm", "der", "Treu\u00b7e", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und t\u00f6n' in zwiefach starkem Schall", "tokens": ["Und", "t\u00f6n'", "in", "zwie\u00b7fach", "star\u00b7kem", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Namen ", "tokens": ["Den", "Na\u00b7men"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Vielleicht erreicht", "tokens": ["Viel\u00b7leicht", "er\u00b7reicht"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Ton des Liebchens Wohnung;", "tokens": ["Der", "Ton", "des", "Lieb\u00b7chens", "Woh\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann harrt sie mein am Wasserfall,", "tokens": ["Dann", "harrt", "sie", "mein", "am", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Giebt k\u00fcssend mir Belohnung.", "tokens": ["Giebt", "k\u00fcs\u00b7send", "mir", "Be\u00b7loh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}