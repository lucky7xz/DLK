{"textgrid.poem.41975": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schw\u00e4rmen will ich und t\u00e4ndeln mit dir! O kr\u00e4nze mir, Liebchen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00e4rmen will ich und t\u00e4ndeln mit dir! O kr\u00e4nze mir, Liebchen,", "tokens": ["Schw\u00e4r\u00b7men", "will", "ich", "und", "t\u00e4n\u00b7deln", "mit", "dir", "!", "O", "kr\u00e4n\u00b7ze", "mir", ",", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VVFIN", "APPR", "PPER", "$.", "NE", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Kr\u00e4nze den goldenen Kelch hurtig mit sprudelndem Wein!", "tokens": ["Kr\u00e4n\u00b7ze", "den", "gol\u00b7de\u00b7nen", "Kelch", "hur\u00b7tig", "mit", "spru\u00b7deln\u00b7dem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Koste mit l\u00fcsterner Lippe zuvor, und, wenn der Ber\u00fchrung", "tokens": ["Kos\u00b7te", "mit", "l\u00fcs\u00b7ter\u00b7ner", "Lip\u00b7pe", "zu\u00b7vor", ",", "und", ",", "wenn", "der", "Be\u00b7r\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "$,", "KON", "$,", "KOUS", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Geist am Rande noch schwebt, reiche den Becher mir dar,", "tokens": ["Geist", "am", "Ran\u00b7de", "noch", "schwebt", ",", "rei\u00b7che", "den", "Be\u00b7cher", "mir", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da\u00df ich zugleich mit dem Trank ausschl\u00fcrfe des rosigen Mundes", "tokens": ["Da\u00df", "ich", "zu\u00b7gleich", "mit", "dem", "Trank", "aus\u00b7schl\u00fcr\u00b7fe", "des", "ro\u00b7si\u00b7gen", "Mun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wallenden Hauch, und Wein wandle zum zartesten Ku\u00df.", "tokens": ["Wal\u00b7len\u00b7den", "Hauch", ",", "und", "Wein", "wand\u00b7le", "zum", "zar\u00b7tes\u00b7ten", "Ku\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Z\u00fcchtige, nippe nicht so! Still lauscht in der Tiefe des Bechers", "tokens": ["Z\u00fcch\u00b7ti\u00b7ge", ",", "nip\u00b7pe", "nicht", "so", "!", "Still", "lauscht", "in", "der", "Tie\u00b7fe", "des", "Be\u00b7chers"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ADV", "$.", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.8": {"text": "Amor; weise doch nicht hart den Gewognen Zur\u00fcck.", "tokens": ["A\u00b7mor", ";", "wei\u00b7se", "doch", "nicht", "hart", "den", "Ge\u00b7wog\u00b7nen", "Zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "PTKNEG", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Scherz und schmeichelnde Worte verleiht und leises Verlangen,", "tokens": ["Scherz", "und", "schmei\u00b7cheln\u00b7de", "Wor\u00b7te", "ver\u00b7leiht", "und", "lei\u00b7ses", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Schwingen dem z\u00f6gernden Geist, Rosen den Wangen der Gott;", "tokens": ["Schwin\u00b7gen", "dem", "z\u00f6\u00b7gern\u00b7den", "Geist", ",", "Ro\u00b7sen", "den", "Wan\u00b7gen", "der", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Was dir noch Reizendes schlief in der Brust, das weckt er, und m\u00e4chtig", "tokens": ["Was", "dir", "noch", "Rei\u00b7zen\u00b7des", "schlief", "in", "der", "Brust", ",", "das", "weckt", "er", ",", "und", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Impft er dem duftenden Strauch sch\u00f6nere Bl\u00fcthen noch ein.", "tokens": ["Impft", "er", "dem", "duf\u00b7ten\u00b7den", "Strauch", "sch\u00f6\u00b7ne\u00b7re", "Bl\u00fc\u00b7then", "noch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Holderes L\u00e4cheln umgaukelt den Mund, in dem leuchtenden Blick strahlt", "tokens": ["Hol\u00b7de\u00b7res", "L\u00e4\u00b7cheln", "um\u00b7gau\u00b7kelt", "den", "Mund", ",", "in", "dem", "leuch\u00b7ten\u00b7den", "Blick", "strahlt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+--+--+--++", "measure": "dactylic.tetra.plus"}, "line.14": {"text": "Hellerer Geist, und es t\u00f6nt s\u00fc\u00dfer das kosende Wort;", "tokens": ["Hel\u00b7le\u00b7rer", "Geist", ",", "und", "es", "t\u00f6nt", "s\u00fc\u00b7\u00dfer", "das", "ko\u00b7sen\u00b7de", "Wort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Zartere Scham wohnt still auf den seidenen Wimpern, und Sehnsucht", "tokens": ["Zar\u00b7te\u00b7re", "Scham", "wohnt", "still", "auf", "den", "sei\u00b7de\u00b7nen", "Wim\u00b7pern", ",", "und", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,", "KON", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Lauscht dir im Aug', und es bebt h\u00fcpfend im Busen die Lust.", "tokens": ["Lauscht", "dir", "im", "Aug'", ",", "und", "es", "bebt", "h\u00fcp\u00b7fend", "im", "Bu\u00b7sen", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Schl\u00fcrf' ihn hinab in die Brust: doch ach, nicht lange verweil' er,", "tokens": ["Schl\u00fcr\u00b7f'", "ihn", "hin\u00b7ab", "in", "die", "Brust", ":", "doch", "ach", ",", "nicht", "lan\u00b7ge", "ver\u00b7weil'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$.", "ADV", "ADV", "$,", "PTKNEG", "ADV", "VVFIN", "PPER", "$,"], "meter": "+---+--+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Und im gl\u00fchenden Ku\u00df gieb mir den Holden zur\u00fcck.", "tokens": ["Und", "im", "gl\u00fc\u00b7hen\u00b7den", "Ku\u00df", "gieb", "mir", "den", "Hol\u00b7den", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}