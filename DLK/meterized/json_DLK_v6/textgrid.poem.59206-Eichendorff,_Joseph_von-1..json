{"textgrid.poem.59206": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann wohl manchmal singen,", "tokens": ["Ich", "kann", "wohl", "manch\u00b7mal", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ob ich fr\u00f6hlich sei,", "tokens": ["Als", "ob", "ich", "fr\u00f6h\u00b7lich", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch heimlich Tr\u00e4nen dringen,", "tokens": ["Doch", "heim\u00b7lich", "Tr\u00e4\u00b7nen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wird das Herz mir frei.", "tokens": ["Da", "wird", "das", "Herz", "mir", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So lassen Nachtigallen,", "tokens": ["So", "las\u00b7sen", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spielt drau\u00dfen Fr\u00fchlingsluft,", "tokens": ["Spielt", "drau\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sehnsucht Lied erschallen", "tokens": ["Der", "Sehn\u00b7sucht", "Lied", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus ihres K\u00e4figs Gruft.", "tokens": ["Aus", "ih\u00b7res", "K\u00e4\u00b7figs", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da lauschen alle Herzen,", "tokens": ["Da", "lau\u00b7schen", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und alles ist erfreut,", "tokens": ["Und", "al\u00b7les", "ist", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch keiner f\u00fchlt die Schmerzen,", "tokens": ["Doch", "kei\u00b7ner", "f\u00fchlt", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Lied das tiefe Leid.", "tokens": ["Im", "Lied", "das", "tie\u00b7fe", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}