{"textgrid.poem.67227": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir mich weihen? ich dir? stygische Furie,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir mich weihen? ich dir? stygische Furie,", "tokens": ["Dir", "mich", "wei\u00b7hen", "?", "ich", "dir", "?", "sty\u00b7gi\u00b7sche", "Fu\u00b7rie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$.", "PPER", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Afterthemis, ich dir? die du mit Schlangenlist", "tokens": ["Af\u00b7ter\u00b7the\u00b7mis", ",", "ich", "dir", "?", "die", "du", "mit", "Schlan\u00b7gen\u00b7list"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "PPER", "$.", "PRELS", "PPER", "APPR", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Unser g\u00f6ttliches Recht, welches Natur uns gab,", "tokens": ["Un\u00b7ser", "g\u00f6tt\u00b7li\u00b7ches", "Recht", ",", "wel\u00b7ches", "Na\u00b7tur", "uns", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Raubtest, und mit des Tigers Klau?", "tokens": ["Raub\u00b7test", ",", "und", "mit", "des", "Ti\u00b7gers", "Klau", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "ART", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ha! wie schallt's am Altar! Bosheit und Hadersucht,", "tokens": ["Ha", "!", "wie", "schallt's", "am", "Al\u00b7tar", "!", "Bos\u00b7heit", "und", "Ha\u00b7der\u00b7sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "APPRART", "NN", "$.", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aemsig sp\u00e4hend den Zwist, h\u00e4mische Rachbegier,", "tokens": ["A\u00b7em\u00b7sig", "sp\u00e4\u00b7hend", "den", "Zwist", ",", "h\u00e4\u00b7mi\u00b7sche", "Rach\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Groll und gieriger Geiz, Vater des feilen Spruchs;", "tokens": ["Groll", "und", "gie\u00b7ri\u00b7ger", "Geiz", ",", "Va\u00b7ter", "des", "fei\u00b7len", "Spruchs", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ha, wie tobet die H\u00f6llenbrut!", "tokens": ["Ha", ",", "wie", "to\u00b7bet", "die", "H\u00f6l\u00b7len\u00b7brut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und dein Nattergezisch, schlaue Chikane, du", "tokens": ["Und", "dein", "Nat\u00b7ter\u00b7ge\u00b7zisch", ",", "schlau\u00b7e", "Chi\u00b7ka\u00b7ne", ",", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Misgesch\u00f6pfe des arglistigen Fremdlinges,", "tokens": ["Mis\u00b7ge\u00b7sch\u00f6p\u00b7fe", "des", "arg\u00b7lis\u00b7ti\u00b7gen", "Fremd\u00b7lin\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ungenannt von dem Volk, welches die Zunge spricht,", "tokens": ["Un\u00b7ge\u00b7nannt", "von", "dem", "Volk", ",", "wel\u00b7ches", "die", "Zun\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Die Thuiskon und Mana sprach!", "tokens": ["Die", "Thu\u00b7is\u00b7kon", "und", "Ma\u00b7na", "sprach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "We\u00df der \u00e4chzende Laut? \u2013 \u2013 Ach der bek\u00fcmmerten", "tokens": ["We\u00df", "der", "\u00e4ch\u00b7zen\u00b7de", "Laut", "?", "\u2013", "\u2013", "Ach", "der", "be\u00b7k\u00fcm\u00b7mer\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$.", "$(", "$(", "NN", "ART", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Unschuld Seufzer! Sie naht weinend der G\u00f6ttin sich,", "tokens": ["Un\u00b7schuld", "Seuf\u00b7zer", "!", "Sie", "naht", "wei\u00b7nend", "der", "G\u00f6t\u00b7tin", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN", "PRF", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Fleht Erbarmen. Umsonst! Ihre verruchte Schaar", "tokens": ["Fleht", "Er\u00b7bar\u00b7men", ".", "Um\u00b7sonst", "!", "Ih\u00b7re", "ver\u00b7ruch\u00b7te", "Schaar"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Schreckt mit grimmigem Hohn sie weg!", "tokens": ["Schreckt", "mit", "grim\u00b7mi\u00b7gem", "Hohn", "sie", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "O des goldenen Tag's, da bei dem Volke Teuts", "tokens": ["O", "des", "gol\u00b7de\u00b7nen", "Tag's", ",", "da", "bei", "dem", "Vol\u00b7ke", "Teuts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Noch Gerechtigkeit galt, noch, von der heiligen", "tokens": ["Noch", "Ge\u00b7rech\u00b7tig\u00b7keit", "galt", ",", "noch", ",", "von", "der", "hei\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "$,", "APPR", "ART", "ADJA"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Eiche Schauer umrauscht, sie in dem richtenden", "tokens": ["Ei\u00b7che", "Schau\u00b7er", "um\u00b7rauscht", ",", "sie", "in", "dem", "rich\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Kreis' ehrw\u00fcrdiger V\u00e4ter sa\u00df!", "tokens": ["Kreis'", "ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "V\u00e4\u00b7ter", "sa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Da vom albernen Wahn lauter der hellere", "tokens": ["Da", "vom", "al\u00b7ber\u00b7nen", "Wahn", "lau\u00b7ter", "der", "hel\u00b7le\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Geist, und lauter vom Schwall wirrender Satzungen;", "tokens": ["Geist", ",", "und", "lau\u00b7ter", "vom", "Schwall", "wir\u00b7ren\u00b7der", "Sat\u00b7zun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da noch Tugend, und du, Erbe Germaniens,", "tokens": ["Da", "noch", "Tu\u00b7gend", ",", "und", "du", ",", "Er\u00b7be", "Ger\u00b7ma\u00b7ni\u00b7ens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "Treue, lehrtet den Biederspruch!", "tokens": ["Treu\u00b7e", ",", "lehr\u00b7tet", "den", "Bie\u00b7der\u00b7spruch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Ach, entflohn ist, entflohn l\u00e4ngst die Gerechtigkeit", "tokens": ["Ach", ",", "ent\u00b7flohn", "ist", ",", "ent\u00b7flohn", "l\u00e4ngst", "die", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vom entarteten Stamm! Wenigen Lieblingen", "tokens": ["Vom", "ent\u00b7ar\u00b7te\u00b7ten", "Stamm", "!", "We\u00b7ni\u00b7gen", "Lieb\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "L\u00e4chelt Weihe nur noch, segnend, vom n\u00e4chtlichen", "tokens": ["L\u00e4\u00b7chelt", "Wei\u00b7he", "nur", "noch", ",", "seg\u00b7nend", ",", "vom", "n\u00e4cht\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "ADV", "ADV", "$,", "VVPP", "$,", "APPRART", "ADJA"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Pol herab, die Geflohene.", "tokens": ["Pol", "her\u00b7ab", ",", "die", "Ge\u00b7flo\u00b7he\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Weihe l\u00e4chelte sie, edler Cheruskasohn,", "tokens": ["Wei\u00b7he", "l\u00e4\u00b7chel\u00b7te", "sie", ",", "ed\u00b7ler", "Che\u00b7rus\u00b7ka\u00b7sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dir, o B\u00fcrger, der du heiligen Druden gleich,", "tokens": ["Dir", ",", "o", "B\u00fcr\u00b7ger", ",", "der", "du", "hei\u00b7li\u00b7gen", "Dru\u00b7den", "gleich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "PRELS", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Richter-Tugenden \u00fcbst, heiligen Barden gleich,", "tokens": ["Rich\u00b7ter\u00b7Tu\u00b7gen\u00b7den", "\u00fcbst", ",", "hei\u00b7li\u00b7gen", "Bar\u00b7den", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Braga's Kranz um die Locke schlingst.", "tokens": ["Bra\u00b7ga's", "Kranz", "um", "die", "Lo\u00b7cke", "schlingst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}