{"textgrid.poem.47004": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, o du mein Liebling, dich betrachte,", "tokens": ["Wenn", "ich", ",", "o", "du", "mein", "Lieb\u00b7ling", ",", "dich", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "PPER", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Amaryllis, meiner Kunst Gebilde,", "tokens": ["O", "A\u00b7ma\u00b7ryl\u00b7lis", ",", "mei\u00b7ner", "Kunst", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist's oft, als ob ich fast der Dichtergilde", "tokens": ["Ist's", "oft", ",", "als", "ob", "ich", "fast", "der", "Dich\u00b7ter\u00b7gil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anzugeh\u00f6ren f\u00fcr was rechtes achte.", "tokens": ["An\u00b7zu\u00b7ge\u00b7h\u00f6\u00b7ren", "f\u00fcr", "was", "rech\u00b7tes", "ach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRELS", "ADJA", "ADJA", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Denn, wenn ich dich mit in Gesellschaft brachte,", "tokens": ["Denn", ",", "wenn", "ich", "dich", "mit", "in", "Ge\u00b7sell\u00b7schaft", "brach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo seinen Rang sonst jeder f\u00fchrt im Schilde,", "tokens": ["Wo", "sei\u00b7nen", "Rang", "sonst", "je\u00b7der", "f\u00fchrt", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich, die Erzeugte l\u00e4ndlicher Gefilde;", "tokens": ["Dich", ",", "die", "Er\u00b7zeug\u00b7te", "l\u00e4nd\u00b7li\u00b7cher", "Ge\u00b7fil\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wer war's, der da dich zu verachten dachte?", "tokens": ["Wer", "wa\u00b7r's", ",", "der", "da", "dich", "zu", "ver\u00b7ach\u00b7ten", "dach\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "KOUS", "PPER", "PTKZU", "VVFIN", "VVFIN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Zu zweifeln schien man nicht an deinem Adel,", "tokens": ["Zu", "zwei\u00b7feln", "schien", "man", "nicht", "an", "dei\u00b7nem", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PIS", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schien nicht zu ahnden oder nicht zu ahnen,", "tokens": ["Schien", "nicht", "zu", "ahn\u00b7den", "o\u00b7der", "nicht", "zu", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVINF", "KON", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du gekommen seist von H\u00fcrd' und Stadel.", "tokens": ["Da\u00df", "du", "ge\u00b7kom\u00b7men", "seist", "von", "H\u00fcrd'", "und", "Sta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer ist's nun, der dir so ersetzt die Ahnen?", "tokens": ["Wer", "ist's", "nun", ",", "der", "dir", "so", "er\u00b7setzt", "die", "Ah\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der Dichter, der drum ohne Tadel", "tokens": ["Das", "ist", "der", "Dich\u00b7ter", ",", "der", "drum", "oh\u00b7ne", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich selbst wohl als ein Pfalzgraf mag gemahnen.", "tokens": ["Sich", "selbst", "wohl", "als", "ein", "Pfalz\u00b7graf", "mag", "ge\u00b7mah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}