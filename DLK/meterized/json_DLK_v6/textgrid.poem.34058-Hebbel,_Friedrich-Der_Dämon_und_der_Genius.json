{"textgrid.poem.34058": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der D\u00e4mon und der Genius", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich willst du nicht hei\u00dfen, noch weniger jubeln und jauchzen,", "tokens": ["Gl\u00fcck\u00b7lich", "willst", "du", "nicht", "hei\u00b7\u00dfen", ",", "noch", "we\u00b7ni\u00b7ger", "ju\u00b7beln", "und", "jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df du den D\u00e4mon nicht weckst, der nur die Stillen verschont;", "tokens": ["Da\u00df", "du", "den", "D\u00e4\u00b7mon", "nicht", "weckst", ",", "der", "nur", "die", "Stil\u00b7len", "ver\u00b7schont", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber zitterst du nicht, den Genius selbst zu verletzen,", "tokens": ["A\u00b7ber", "zit\u00b7terst", "du", "nicht", ",", "den", "Ge\u00b7nius", "selbst", "zu", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Welcher dich segnet und schirmt, wenn du den Dank ihm entziehst?", "tokens": ["Wel\u00b7cher", "dich", "seg\u00b7net", "und", "schirmt", ",", "wenn", "du", "den", "Dank", "ihm", "ent\u00b7ziehst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}