{"textgrid.poem.64111": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Tacitus", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Jungfrau \u00e4hnlich, die in Trojas Jubel", "tokens": ["Der", "Jung\u00b7frau", "\u00e4hn\u00b7lich", ",", "die", "in", "Tro\u00b7jas", "Ju\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Weheruf geahnten Unheils warf,", "tokens": ["Den", "We\u00b7he\u00b7ruf", "ge\u00b7ahn\u00b7ten", "Un\u00b7heils", "warf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ungl\u00e4ub'gen Spott allein als Antwort findend,", "tokens": ["Un\u00b7gl\u00e4ub'\u00b7gen", "Spott", "al\u00b7lein", "als", "Ant\u00b7wort", "fin\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "KOUS", "NN", "VVPP", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Kassandra gleich steh' ich in dieser Zeit!", "tokens": ["Kas\u00b7sand\u00b7ra", "gleich", "steh'", "ich", "in", "die\u00b7ser", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verderben seh' ich rings, wohin ich schaue,", "tokens": ["Ver\u00b7der\u00b7ben", "seh'", "ich", "rings", ",", "wo\u00b7hin", "ich", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit leisen Geistertritten eilend nahn,", "tokens": ["Mit", "lei\u00b7sen", "Geis\u00b7ter\u00b7trit\u00b7ten", "ei\u00b7lend", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Indes das Volk im Zirkus brausend l\u00e4rmt", "tokens": ["In\u00b7des", "das", "Volk", "im", "Zir\u00b7kus", "brau\u00b7send", "l\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und seine wilden Bacchanale h\u00e4lt.", "tokens": ["Und", "sei\u00b7ne", "wil\u00b7den", "Bac\u00b7cha\u00b7na\u00b7le", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Tempel darbt des Opfers und das Herz", "tokens": ["Der", "Tem\u00b7pel", "darbt", "des", "Op\u00b7fers", "und", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Andacht; ungeglaubte G\u00f6tter lehrt", "tokens": ["Der", "An\u00b7dacht", ";", "un\u00b7ge\u00b7glaub\u00b7te", "G\u00f6t\u00b7ter", "lehrt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Priester: fremden Sagen lauscht das Volk,", "tokens": ["Der", "Pries\u00b7ter", ":", "frem\u00b7den", "Sa\u00b7gen", "lauscht", "das", "Volk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die nicht verkn\u00fcpft sind mit der V\u00e4ter Taten.", "tokens": ["Die", "nicht", "ver\u00b7kn\u00fcpft", "sind", "mit", "der", "V\u00e4\u00b7ter", "Ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Weisen spotten \u00fcber Jupiter", "tokens": ["Die", "Wei\u00b7sen", "spot\u00b7ten", "\u00fc\u00b7ber", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und finden keinen andern Gott statt seiner.", "tokens": ["Und", "fin\u00b7den", "kei\u00b7nen", "an\u00b7dern", "Gott", "statt", "sei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Kaiser aber kr\u00e4nzen sich mit Rosen,", "tokens": ["Die", "Kai\u00b7ser", "a\u00b7ber", "kr\u00e4n\u00b7zen", "sich", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Denn selten ward der Lorbeer in dem Land;", "tokens": ["Denn", "sel\u00b7ten", "ward", "der", "Lor\u00b7beer", "in", "dem", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und will ein F\u00fcrst, der noch ein R\u00f6mer ist,", "tokens": ["Und", "will", "ein", "F\u00fcrst", ",", "der", "noch", "ein", "R\u00f6\u00b7mer", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Dem Unheil steuern, ist's, wie wenn ein Mann", "tokens": ["Dem", "Un\u00b7heil", "steu\u00b7ern", ",", "ist's", ",", "wie", "wenn", "ein", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mit Schwert und Schild den Strom des Weltmeers hemmt.", "tokens": ["Mit", "Schwert", "und", "Schild", "den", "Strom", "des", "Welt\u00b7meers", "hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die Jugend schwelgt mit griechischen Het\u00e4ren,", "tokens": ["Die", "Ju\u00b7gend", "schwelgt", "mit", "grie\u00b7chi\u00b7schen", "He\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Indessen Sklaven die Legionen f\u00fcllen,", "tokens": ["In\u00b7des\u00b7sen", "Skla\u00b7ven", "die", "Le\u00b7gi\u00b7o\u00b7nen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Die nur mit Scham zur Schlacht der Adler f\u00fchrt,", "tokens": ["Die", "nur", "mit", "Scham", "zur", "Schlacht", "der", "Ad\u00b7ler", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und Laster, ungeheure Laster thronen", "tokens": ["Und", "Las\u00b7ter", ",", "un\u00b7ge\u00b7heu\u00b7re", "Las\u00b7ter", "thro\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Auf allen sieben H\u00fcgeln dieser Stadt.", "tokens": ["Auf", "al\u00b7len", "sie\u00b7ben", "H\u00fc\u00b7geln", "die\u00b7ser", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Auf steilem Fels steht dieser Riesenbau:", "tokens": ["Auf", "stei\u00b7lem", "Fels", "steht", "die\u00b7ser", "Rie\u00b7sen\u00b7bau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Er wankt und t\u00e4glich mehr neigt er zu Fall.", "tokens": ["Er", "wankt", "und", "t\u00e4g\u00b7lich", "mehr", "neigt", "er", "zu", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Sie k\u00f6mmt nicht mehr, die Zeit der Scipionen!", "tokens": ["Sie", "k\u00f6mmt", "nicht", "mehr", ",", "die", "Zeit", "der", "Sci\u00b7pi\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Umsonst singt von Triumph der Dichter Mund:", "tokens": ["Um\u00b7sonst", "singt", "von", "Tri\u00b7umph", "der", "Dich\u00b7ter", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Es sind die letzten Fl\u00fcgelschl\u00e4ge nur", "tokens": ["Es", "sind", "die", "letz\u00b7ten", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7ge", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Des Adlers, dem der Pfeil im Herzen steckt.", "tokens": ["Des", "Ad\u00b7lers", ",", "dem", "der", "Pfeil", "im", "Her\u00b7zen", "steckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Im Osten fliegt des Parthers leicht Gescho\u00df", "tokens": ["Im", "Os\u00b7ten", "fliegt", "des", "Par\u00b7thers", "leicht", "Ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Schon ungestraft in r\u00f6mische Provinzen,", "tokens": ["Schon", "un\u00b7ge\u00b7straft", "in", "r\u00f6\u00b7mi\u00b7sche", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und furchtbar pocht die Streitaxt des Germanen", "tokens": ["Und", "furcht\u00b7bar", "pocht", "die", "Strei\u00b7taxt", "des", "Ger\u00b7ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "An dieses Reiches morschgewordne T\u00fcr.", "tokens": ["An", "die\u00b7ses", "Rei\u00b7ches", "morschge\u00b7word\u00b7ne", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.35": {"text": "Uns h\u00e4lt der Feinde Zwist, nicht eigne Macht;", "tokens": ["Uns", "h\u00e4lt", "der", "Fein\u00b7de", "Zwist", ",", "nicht", "eig\u00b7ne", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Weh uns, wenn diese waldgeborne Kraft,", "tokens": ["Weh", "uns", ",", "wenn", "die\u00b7se", "wald\u00b7ge\u00b7bor\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Wenn diese freien Str\u00f6me sich vereinen", "tokens": ["Wenn", "die\u00b7se", "frei\u00b7en", "Str\u00f6\u00b7me", "sich", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Und m\u00e4chtig von den Alpen niedergehn.", "tokens": ["Und", "m\u00e4ch\u00b7tig", "von", "den", "Al\u00b7pen", "nie\u00b7der\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Was haben wir als Damm, sie abzuwehren?", "tokens": ["Was", "ha\u00b7ben", "wir", "als", "Damm", ",", "sie", "ab\u00b7zu\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KOUS", "NN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Den Ruhm der V\u00e4ter und der Enkel Wahn!", "tokens": ["Den", "Ruhm", "der", "V\u00e4\u00b7ter", "und", "der", "En\u00b7kel", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Mir aber sei's verg\u00f6nnt, vorher zu sterben!", "tokens": ["Mir", "a\u00b7ber", "sei's", "ver\u00b7g\u00f6nnt", ",", "vor\u00b7her", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Mich ekelt dieser faulgewordnen Zeit,", "tokens": ["Mich", "e\u00b7kelt", "die\u00b7ser", "faul\u00b7ge\u00b7word\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und oft beschleicht mich qualvoll der Gedanke:", "tokens": ["Und", "oft", "be\u00b7schleicht", "mich", "qual\u00b7voll", "der", "Ge\u00b7dan\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Die G\u00f6tter achten dieser Erde nur,", "tokens": ["Die", "G\u00f6t\u00b7ter", "ach\u00b7ten", "die\u00b7ser", "Er\u00b7de", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Um uns zu strafen, nicht um uns zu helfen.", "tokens": ["Um", "uns", "zu", "stra\u00b7fen", ",", "nicht", "um", "uns", "zu", "hel\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Nicht unter diesen Menschen will ich leben:", "tokens": ["Nicht", "un\u00b7ter", "die\u00b7sen", "Men\u00b7schen", "will", "ich", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Aufrollen will ich mir der Zeiten Buch,", "tokens": ["Auf\u00b7rol\u00b7len", "will", "ich", "mir", "der", "Zei\u00b7ten", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Und Gro\u00dfes schau'n, das andre Tage schufen", "tokens": ["Und", "Gro\u00b7\u00dfes", "schau'n", ",", "das", "and\u00b7re", "Ta\u00b7ge", "schu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Doch dieser Zeit will ich empfindungslos,", "tokens": ["Doch", "die\u00b7ser", "Zeit", "will", "ich", "emp\u00b7fin\u00b7dungs\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Ein Demantspiegel, gegen\u00fcber stehn", "tokens": ["Ein", "De\u00b7mant\u00b7spie\u00b7gel", ",", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "stehn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Und zeigen ihr das ungeheure Bild", "tokens": ["Und", "zei\u00b7gen", "ihr", "das", "un\u00b7ge\u00b7heu\u00b7re", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Der eignen Torheit und der eignen Schuld.", "tokens": ["Der", "eig\u00b7nen", "Tor\u00b7heit", "und", "der", "eig\u00b7nen", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "O w\u00fcrd' es ihnen zum Gorgonenhaupt,", "tokens": ["O", "w\u00fcrd'", "es", "ih\u00b7nen", "zum", "Gor\u00b7go\u00b7nen\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Das sie entsetzte und versteinerte:", "tokens": ["Das", "sie", "ent\u00b7setz\u00b7te", "und", "ver\u00b7stei\u00b7ner\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.p