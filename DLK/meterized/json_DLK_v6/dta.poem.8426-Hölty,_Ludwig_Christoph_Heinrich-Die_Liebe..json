{"dta.poem.8426": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Gott dem Menschengeschlecht; aber der lastende", "tokens": ["Gott", "dem", "Men\u00b7schen\u00b7ge\u00b7schlecht", ";", "a\u00b7ber", "der", "las\u00b7ten\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ADV", "ART", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kummer senket die Schale;", "tokens": ["Kum\u00b7mer", "sen\u00b7ket", "die", "Scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Immer hebet die andre sich.", "tokens": ["Im\u00b7mer", "he\u00b7bet", "die", "and\u00b7re", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Irr und trauriges Tritts wanken wir unsern Weg", "tokens": ["Irr", "und", "trau\u00b7ri\u00b7ges", "Tritts", "wan\u00b7ken", "wir", "un\u00b7sern", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Durch das Leben hinab, bis sich die Liebe naht,", "tokens": ["Durch", "das", "Le\u00b7ben", "hin\u00b7ab", ",", "bis", "sich", "die", "Lie\u00b7be", "naht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Eine F\u00fclle der Freuden", "tokens": ["Ei\u00b7ne", "F\u00fcl\u00b7le", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In die steigende Schale geusst.", "tokens": ["In", "die", "stei\u00b7gen\u00b7de", "Scha\u00b7le", "geusst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie dem Pilger der Quell silbern entgegen rinnt,", "tokens": ["Wie", "dem", "Pil\u00b7ger", "der", "Quell", "sil\u00b7bern", "ent\u00b7ge\u00b7gen", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie der Regen des Mais \u00fcber die Bl\u00fcten tr\u00e4uft,", "tokens": ["Wie", "der", "Re\u00b7gen", "des", "Mais", "\u00fc\u00b7ber", "die", "Bl\u00fc\u00b7ten", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Naht die Liebe: des J\u00fcnglings", "tokens": ["Naht", "die", "Lie\u00b7be", ":", "des", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seele zittert, und huldigt ihr!", "tokens": ["See\u00b7le", "zit\u00b7tert", ",", "und", "hul\u00b7digt", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "N\u00e4hm' er Kronen und Gold, misste der Liebe? Gold", "tokens": ["N\u00e4hm'", "er", "Kro\u00b7nen", "und", "Gold", ",", "miss\u00b7te", "der", "Lie\u00b7be", "?", "Gold"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "VVFIN", "ART", "NN", "$.", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ist ihm fliegende Spreu; Kronen ein Flittertand;", "tokens": ["Ist", "ihm", "flie\u00b7gen\u00b7de", "Spreu", ";", "Kro\u00b7nen", "ein", "Flit\u00b7ter\u00b7tand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Alle Hoheit der Erde,", "tokens": ["Al\u00b7le", "Ho\u00b7heit", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sonder herzliche Liebe, Staub!", "tokens": ["Son\u00b7der", "herz\u00b7li\u00b7che", "Lie\u00b7be", ",", "Staub", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Loos der Engel! Kein Sturm tr\u00fcbet die Heiterkeit", "tokens": ["Loos", "der", "En\u00b7gel", "!", "Kein", "Sturm", "tr\u00fc\u00b7bet", "die", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seiner Seele! Der Tag h\u00fcllt sich in lichter Blau;", "tokens": ["Sei\u00b7ner", "See\u00b7le", "!", "Der", "Tag", "h\u00fcllt", "sich", "in", "lich\u00b7ter", "Blau", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "--+--++-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kuss und Fl\u00fcstern und L\u00e4cheln", "tokens": ["Kuss", "und", "Fl\u00fcs\u00b7tern", "und", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Fl\u00fcgelt Stunden an Stunden fort!", "tokens": ["Fl\u00fc\u00b7gelt", "Stun\u00b7den", "an", "Stun\u00b7den", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Herscher neideten ihn, kosteten sie des Gl\u00fccks,", "tokens": ["Her\u00b7scher", "nei\u00b7de\u00b7ten", "ihn", ",", "kos\u00b7te\u00b7ten", "sie", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das dem Liebenden ward; w\u00fcrfen den K\u00f6nigsstab", "tokens": ["Das", "dem", "Lie\u00b7ben\u00b7den", "ward", ";", "w\u00fcr\u00b7fen", "den", "K\u00f6\u00b7nigs\u00b7stab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VAFIN", "$.", "VVFIN", "ART", "NN"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus den H\u00e4nden, und suchten", "tokens": ["Aus", "den", "H\u00e4n\u00b7den", ",", "und", "such\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich ein friedliches H\u00fcttendach.", "tokens": ["Sich", "ein", "fried\u00b7li\u00b7ches", "H\u00fct\u00b7ten\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Unter Rosengestr\u00e4uch lispelt ein Quell, und mischt", "tokens": ["Un\u00b7ter", "Ro\u00b7sen\u00b7ge\u00b7str\u00e4uch", "lis\u00b7pelt", "ein", "Quell", ",", "und", "mischt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Zum begegnenden Bach Silber. So str\u00f6men flugs", "tokens": ["Zum", "be\u00b7ge\u00b7gnen\u00b7den", "Bach", "Sil\u00b7ber", ".", "So", "str\u00f6\u00b7men", "flugs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN", "$.", "ADV", "VVFIN", "ADV"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Seel' und Seele zusammen,", "tokens": ["Seel'", "und", "See\u00b7le", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn allm\u00e4chtige Liebe naht.", "tokens": ["Wenn", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Lie\u00b7be", "naht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}