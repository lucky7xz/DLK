{"textgrid.poem.49387": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der B\u00fc\u00dfer", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdorrt der Mund und trocken die Ged\u00e4rme", "tokens": ["Ver\u00b7dorrt", "der", "Mund", "und", "tro\u00b7cken", "die", "Ge\u00b7d\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So tanze ich um meinen eignen Schatten.", "tokens": ["So", "tan\u00b7ze", "ich", "um", "mei\u00b7nen", "eig\u00b7nen", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus meinem Bette spiel ich mit den Ratten", "tokens": ["Aus", "mei\u00b7nem", "Bet\u00b7te", "spiel", "ich", "mit", "den", "Rat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sauge aus den Fingern mir die W\u00e4rme.", "tokens": ["Und", "sau\u00b7ge", "aus", "den", "Fin\u00b7gern", "mir", "die", "W\u00e4r\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Damit ich mich nicht allzu bitter h\u00e4rme", "tokens": ["Da\u00b7mit", "ich", "mich", "nicht", "all\u00b7zu", "bit\u00b7ter", "h\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "PTKA", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df eine g\u00fct'ge Macht mein Herz ermatten.", "tokens": ["Lie\u00df", "ei\u00b7ne", "g\u00fct'\u00b7ge", "Macht", "mein", "Herz", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sehnsucht starb. Nur meine nimmersatten", "tokens": ["Die", "Sehn\u00b7sucht", "starb", ".", "Nur", "mei\u00b7ne", "nim\u00b7mer\u00b7sat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verflogenen Ohren h\u00e4ngen noch am L\u00e4rme.", "tokens": ["Ver\u00b7flo\u00b7ge\u00b7nen", "Oh\u00b7ren", "h\u00e4n\u00b7gen", "noch", "am", "L\u00e4r\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da ich mich also in mir selbst verfangen", "tokens": ["Da", "ich", "mich", "al\u00b7so", "in", "mir", "selbst", "ver\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich auch meinen H\u00e4schern nicht entgangen", "tokens": ["Bin", "ich", "auch", "mei\u00b7nen", "H\u00e4\u00b7schern", "nicht", "ent\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und teile die Gemeinschaft schriller K\u00e4uze.", "tokens": ["Und", "tei\u00b7le", "die", "Ge\u00b7mein\u00b7schaft", "schril\u00b7ler", "K\u00e4u\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Lappenkleide und bedeckt mit Schorfen", "tokens": ["Im", "Lap\u00b7pen\u00b7klei\u00b7de", "und", "be\u00b7deckt", "mit", "Schor\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Werd t\u00e4glich ich den W\u00e4rtern vorgeworfen,", "tokens": ["Werd", "t\u00e4g\u00b7lich", "ich", "den", "W\u00e4r\u00b7tern", "vor\u00b7ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die striegeln mich mit einem Eisenkreuze.", "tokens": ["Die", "strie\u00b7geln", "mich", "mit", "ei\u00b7nem", "Ei\u00b7sen\u00b7kreu\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}