{"dta.poem.9554": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie speiset seinen vogel aus ihrem munde.  \n J. v. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein zei\u00dfgen nehrte sich bi\u00dfher von hanff und r\u00fcben/", "tokens": ["Mein", "zei\u00df\u00b7gen", "nehr\u00b7te", "sich", "bi\u00df\u00b7her", "von", "hanff", "und", "r\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und liesse seinen tranck ein schlechtes wasser seyn.", "tokens": ["Und", "lies\u00b7se", "sei\u00b7nen", "tranck", "ein", "schlech\u00b7tes", "was\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es lie\u00df den irrdnen napff ihm mehr als gold belieben/", "tokens": ["Es", "lie\u00df", "den", "irrd\u00b7nen", "napff", "ihm", "mehr", "als", "gold", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bildet sich ein schlo\u00df von seinem k\u00e4ficht ein.", "tokens": ["Und", "bil\u00b7det", "sich", "ein", "schlo\u00df", "von", "sei\u00b7nem", "k\u00e4\u00b7ficht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "VVFIN", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun merckt der vogel erst/ da\u00df gla\u00df chrystallen weichet/", "tokens": ["Nun", "merckt", "der", "vo\u00b7gel", "erst", "/", "da\u00df", "gla\u00df", "chrys\u00b7tal\u00b7len", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$(", "KOUS", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ros\u2019 ein nessel-strauch/ dem bisame zibeth/", "tokens": ["Der", "ros'", "ein", "nes\u00b7sel\u00b7strauch", "/", "dem", "bi\u00b7sa\u00b7me", "zi\u00b7beth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "VVFIN", "$("], "meter": "-+--+-+-++-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Nachdem sein neuer stand so wenig jenem gleichet/", "tokens": ["Nach\u00b7dem", "sein", "neu\u00b7er", "stand", "so", "we\u00b7nig", "je\u00b7nem", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVFIN", "ADV", "PIS", "PDAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als wenig schnecken-blut nach andern farben geht.", "tokens": ["Als", "we\u00b7nig", "schne\u00b7cken\u00b7blut", "nach", "an\u00b7dern", "far\u00b7ben", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es hat sein altes hau\u00df (von holtze zubereitet/", "tokens": ["Es", "hat", "sein", "al\u00b7tes", "hau\u00df", "(", "von", "holt\u00b7ze", "zu\u00b7be\u00b7rei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit tannen ausgeziert) um eine hand vertauscht/", "tokens": ["Mit", "tan\u00b7nen", "aus\u00b7ge\u00b7ziert", ")", "um", "ei\u00b7ne", "hand", "ver\u00b7tauscht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die selbst der kreide trotzt/ und mit der wolle streitet/", "tokens": ["Die", "selbst", "der", "krei\u00b7de", "trotzt", "/", "und", "mit", "der", "wol\u00b7le", "strei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "APPR", "ART", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Darin der lose gast auff seidnen polstern lauscht.", "tokens": ["Da\u00b7rin", "der", "lo\u00b7se", "gast", "auff", "seid\u00b7nen", "pols\u00b7tern", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein ietzigs trinckgeschirr ist ein rubinen-becher/", "tokens": ["Sein", "ie\u00b7tzigs", "trinck\u00b7ge\u00b7schirr", "ist", "ein", "ru\u00b7bi\u00b7nen\u00b7be\u00b7cher", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den rings-um die natur mit perlen ausgesetzt/", "tokens": ["Den", "rings\u00b7um", "die", "na\u00b7tur", "mit", "per\u00b7len", "aus\u00b7ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In dessen wunder-safft zu seinem liebes-k\u00f6cher", "tokens": ["In", "des\u00b7sen", "wun\u00b7der\u00b7\u00b7safft", "zu", "sei\u00b7nem", "lie\u00b7bes\u00b7k\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Cupido allemahl die g\u00fcldne pfeile netzt.", "tokens": ["Cu\u00b7pi\u00b7do", "al\u00b7le\u00b7mahl", "die", "g\u00fcld\u00b7ne", "pfei\u00b7le", "netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein vogel speist allhier nichts als nur amber-kuchen/", "tokens": ["Mein", "vo\u00b7gel", "speist", "all\u00b7hier", "nichts", "als", "nur", "am\u00b7ber\u00b7ku\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PIS", "KOKOM", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und trinckt den nectar/ der aus purpur-rosen quillt.", "tokens": ["Und", "trinckt", "den", "nec\u00b7tar", "/", "der", "aus", "pur\u00b7pur\u00b7ro\u00b7sen", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "ART", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Der s\u00fcsse seelen-thau/ den viele geister suchen/", "tokens": ["Der", "s\u00fcs\u00b7se", "see\u00b7len\u00b7\u00b7thau", "/", "den", "vie\u00b7le", "geis\u00b7ter", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat diesen gl\u00fccklichen zum \u00f6fftern angef\u00fcllt.", "tokens": ["Hat", "die\u00b7sen", "gl\u00fcck\u00b7li\u00b7chen", "zum", "\u00f6ff\u00b7tern", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach wie verschwendrisch ist bi\u00dfweilen das gel\u00fccke!", "tokens": ["Ach", "wie", "ver\u00b7schwend\u00b7risch", "ist", "bi\u00df\u00b7wei\u00b7len", "das", "ge\u00b7l\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hier wirfft es alles zu dem/ der gar nichts verdient/", "tokens": ["Hier", "wirfft", "es", "al\u00b7les", "zu", "dem", "/", "der", "gar", "nichts", "ver\u00b7dient", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "$(", "ART", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der nichts erkennen kan/ und der die holden blicke", "tokens": ["Der", "nichts", "er\u00b7ken\u00b7nen", "kan", "/", "und", "der", "die", "hol\u00b7den", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$(", "KON", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wol um ein korn vertauscht/ daraus ein hanff-stiel gr\u00fcnt.", "tokens": ["Wol", "um", "ein", "korn", "ver\u00b7tauscht", "/", "da\u00b7raus", "ein", "hanf\u00b7fstiel", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$(", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Ach wolte mir einmahl ein solcher gl\u00fccks-stern scheinen/", "tokens": ["Ach", "wol\u00b7te", "mir", "ein\u00b7mahl", "ein", "sol\u00b7cher", "gl\u00fccks\u00b7stern", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich wolt empfindlicher/ als du mein zei\u00dfgen/ seyn/", "tokens": ["Ich", "wolt", "emp\u00b7find\u00b7li\u00b7cher", "/", "als", "du", "mein", "zei\u00df\u00b7gen", "/", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "$(", "KOUS", "PPER", "PPOSAT", "NN", "$(", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich wei\u00df den wahren werth von solchen edel-steinen/", "tokens": ["Ich", "wei\u00df", "den", "wah\u00b7ren", "werth", "von", "sol\u00b7chen", "e\u00b7del\u00b7stei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJD", "APPR", "PIAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Man nimt die perlen nicht wie r\u00fcben-saamen ein.", "tokens": ["Man", "nimt", "die", "per\u00b7len", "nicht", "wie", "r\u00fc\u00b7ben\u00b7saa\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein solcher g\u00f6tter-safft mit rosen \u00fcberdecket", "tokens": ["Ein", "sol\u00b7cher", "g\u00f6t\u00b7ter\u00b7\u00b7safft", "mit", "ro\u00b7sen", "\u00fc\u00b7berd\u00b7e\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Reimt sich/ mein armes thier/ f\u00fcr deinen schnabel nicht;", "tokens": ["Reimt", "sich", "/", "mein", "ar\u00b7mes", "thier", "/", "f\u00fcr", "dei\u00b7nen", "schna\u00b7bel", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach diese zucker-kost/ die nach je\u00dfminen schmecket/", "tokens": ["Ach", "die\u00b7se", "zu\u00b7cke\u00b7rkost", "/", "die", "nach", "je\u00df\u00b7mi\u00b7nen", "schme\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDAT", "NN", "$(", "ART", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ist eintzig und allein den seelen zugericht!", "tokens": ["Ist", "eint\u00b7zig", "und", "al\u00b7lein", "den", "see\u00b7len", "zu\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Komm tausch einmahl mit mir: Ich schwer bey meinem leben/", "tokens": ["Komm", "tausch", "ein\u00b7mahl", "mit", "mir", ":", "Ich", "schwer", "bey", "mei\u00b7nem", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "$.", "PPER", "ADJD", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich will weit mehr vergn\u00fcgt von deiner taffel gehn.", "tokens": ["Ich", "will", "weit", "mehr", "ver\u00b7gn\u00fcgt", "von", "dei\u00b7ner", "taf\u00b7fel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du armer vogel nimst/ und kanst nicht wieder geben;", "tokens": ["Du", "ar\u00b7mer", "vo\u00b7gel", "nimst", "/", "und", "kanst", "nicht", "wie\u00b7der", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$(", "KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich kans/ und mu\u00df dennoch dich vorgezogen sehn.", "tokens": ["Ich", "kans", "/", "und", "mu\u00df", "den\u00b7noch", "dich", "vor\u00b7ge\u00b7zo\u00b7gen", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KON", "VMFIN", "ADV", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}