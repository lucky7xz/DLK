{"textgrid.poem.63188": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon hebt die tanzende Charite", "tokens": ["Schon", "hebt", "die", "tan\u00b7zen\u00b7de", "Cha\u00b7ri\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die selige Syrinx,", "tokens": ["Die", "se\u00b7li\u00b7ge", "Sy\u00b7rinx", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Und dem gel\u00f6sten Haar entf\u00e4llt", "tokens": ["Und", "dem", "ge\u00b7l\u00f6s\u00b7ten", "Haar", "ent\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein B\u00fcschel Mohn.", "tokens": ["Ein", "B\u00fc\u00b7schel", "Mohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Im Wasser spiegelt sich erstaunt", "tokens": ["Im", "Was\u00b7ser", "spie\u00b7gelt", "sich", "er\u00b7staunt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der heilige Frosch.", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Frosch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die letzte Schwalbe", "tokens": ["Die", "letz\u00b7te", "Schwal\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Verweht nach S\u00fcden.", "tokens": ["Ver\u00b7weht", "nach", "S\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ins brechende Blumenauge", "tokens": ["Ins", "bre\u00b7chen\u00b7de", "Blu\u00b7men\u00b7au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Blickt der verwunderte J\u00fcngling,", "tokens": ["Blickt", "der", "ver\u00b7wun\u00b7der\u00b7te", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Unwissend, dass er die Blume brach am Taumorgen,", "tokens": ["Un\u00b7wis\u00b7send", ",", "dass", "er", "die", "Blu\u00b7me", "brach", "am", "Tau\u00b7mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da er die Freundin streichelte.", "tokens": ["Da", "er", "die", "Freun\u00b7din", "strei\u00b7chel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er schreitet,", "tokens": ["Er", "schrei\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der marmorne Henker,", "tokens": ["Der", "mar\u00b7mor\u00b7ne", "Hen\u00b7ker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nackt", "tokens": ["Nackt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "In die stygische Nacht.", "tokens": ["In", "die", "sty\u00b7gi\u00b7sche", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}