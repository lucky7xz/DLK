{"textgrid.poem.45855": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[kehret Gedanken doch heimw\u00e4rts]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kehret Gedanken doch heimw\u00e4rts, eilet den Tempel zu ordnen,", "tokens": ["Keh\u00b7ret", "Ge\u00b7dan\u00b7ken", "doch", "heim\u00b7w\u00e4rts", ",", "ei\u00b7let", "den", "Tem\u00b7pel", "zu", "ord\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADV", "$,", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schafft mir im Herzen Gebet, eh' es in Sehnsucht mir bricht,", "tokens": ["Schafft", "mir", "im", "Her\u00b7zen", "Ge\u00b7bet", ",", "eh'", "es", "in", "Sehn\u00b7sucht", "mir", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Drei sind ihrer, der Teuern, die weit in der Fremde mir weilen;", "tokens": ["Drei", "sind", "ih\u00b7rer", ",", "der", "Teu\u00b7ern", ",", "die", "weit", "in", "der", "Frem\u00b7de", "mir", "wei\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PPOSAT", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Zwei dem Tode geweiht gr\u00fc\u00dfe noch einmal mein Blick,", "tokens": ["Zwei", "dem", "To\u00b7de", "ge\u00b7weiht", "gr\u00fc\u00b7\u00dfe", "noch", "ein\u00b7mal", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "VVPP", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Da\u00df ich friedlich entsage, dem was die Fremde begehrt.", "tokens": ["Da\u00df", "ich", "fried\u00b7lich", "ent\u00b7sa\u00b7ge", ",", "dem", "was", "die", "Frem\u00b7de", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "PRELS", "PWS", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dann umfasse mich Leben, \u2013 denn eine noch weilet, \u2013 ich f\u00fchle,", "tokens": ["Dann", "um\u00b7fas\u00b7se", "mich", "Le\u00b7ben", ",", "\u2013", "denn", "ei\u00b7ne", "noch", "wei\u00b7let", ",", "\u2013", "ich", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "$(", "KON", "ART", "ADV", "VVFIN", "$,", "$(", "PPER", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Da\u00df sie das einzige ist: Leben und Liebe und Zukunft. \u2013", "tokens": ["Da\u00df", "sie", "das", "ein\u00b7zi\u00b7ge", "ist", ":", "Le\u00b7ben", "und", "Lie\u00b7be", "und", "Zu\u00b7kunft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "$.", "NN", "KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wie mir's im Herzen, \u2013 das hat ihr der Gott in den Busen geschrieben,", "tokens": ["Wie", "mir's", "im", "Her\u00b7zen", ",", "\u2013", "das", "hat", "ihr", "der", "Gott", "in", "den", "Bu\u00b7sen", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "$,", "$(", "PDS", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wie in der Seele es mir, schrieb ihr der Gott in das Aug'. \u2013", "tokens": ["Wie", "in", "der", "See\u00b7le", "es", "mir", ",", "schrieb", "ihr", "der", "Gott", "in", "das", "Aug'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Schweigend spricht sie das Wort, was meine Lippe nicht redet;", "tokens": ["Schwei\u00b7gend", "spricht", "sie", "das", "Wort", ",", "was", "mei\u00b7ne", "Lip\u00b7pe", "nicht", "re\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Flieh' ich so ist sie die Flucht; ruh' ich so ruht sie in mir.", "tokens": ["Flieh'", "ich", "so", "ist", "sie", "die", "Flucht", ";", "ruh'", "ich", "so", "ruht", "sie", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "$.", "VVFIN", "PPER", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Suchst Du sie? \u2013 dort in den Schatten des Waldes wo sich das Dunkel", "tokens": ["Suchst", "Du", "sie", "?", "\u2013", "dort", "in", "den", "Schat\u00b7ten", "des", "Wal\u00b7des", "wo", "sich", "das", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "$(", "ADV", "APPR", "ART", "NN", "ART", "NN", "PWAV", "PRF", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Tiefer Begeisterung l\u00f6st, stiller der Himmel sich senkt,", "tokens": ["Tie\u00b7fer", "Be\u00b7geis\u00b7te\u00b7rung", "l\u00f6st", ",", "stil\u00b7ler", "der", "Him\u00b7mel", "sich", "senkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$,", "ADJA", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Wo an der liebenden Brust, dem Gestade des brausenden Lebens,", "tokens": ["Wo", "an", "der", "lie\u00b7ben\u00b7den", "Brust", ",", "dem", "Ge\u00b7sta\u00b7de", "des", "brau\u00b7sen\u00b7den", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Des unendlichen Meeres Woge melodisch sich bricht.", "tokens": ["Des", "un\u00b7end\u00b7li\u00b7chen", "Mee\u00b7res", "Wo\u00b7ge", "me\u00b7lo\u00b7disch", "sich", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Dort weilt sie, dichtet fromm, was Ihr Geister sie lehret,", "tokens": ["Dort", "weilt", "sie", ",", "dich\u00b7tet", "fromm", ",", "was", "Ihr", "Geis\u00b7ter", "sie", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Begierig Geheimnis zu fassen,", "tokens": ["Be\u00b7gie\u00b7rig", "Ge\u00b7heim\u00b7nis", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Und Euch Ihr G\u00f6tter in mir, schuf nur des Kindes Gebet.", "tokens": ["Und", "Euch", "Ihr", "G\u00f6t\u00b7ter", "in", "mir", ",", "schuf", "nur", "des", "Kin\u00b7des", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "APPR", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Tr\u00f6sterin! \u2013 Freundliche! \u2013 Dein Seherauge entsiegelt dem Tode", "tokens": ["Tr\u00f6s\u00b7te\u00b7rin", "!", "\u2013", "Freund\u00b7li\u00b7che", "!", "\u2013", "Dein", "Se\u00b7her\u00b7au\u00b7ge", "ent\u00b7sie\u00b7gelt", "dem", "To\u00b7de"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "NN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+--+---+-+--+--+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "Der Dich als Leben umgiebt, selbst den geschlossenen Blick. \u2013", "tokens": ["Der", "Dich", "als", "Le\u00b7ben", "um\u00b7giebt", ",", "selbst", "den", "ge\u00b7schlos\u00b7se\u00b7nen", "Blick", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Alles ", "tokens": ["Al\u00b7les"], "token_info": ["word"], "pos": ["PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Was Deine segnende Hand, was Dein Gedanke ber\u00fchrt,", "tokens": ["Was", "Dei\u00b7ne", "seg\u00b7nen\u00b7de", "Hand", ",", "was", "Dein", "Ge\u00b7dan\u00b7ke", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Bl\u00fchet sch\u00f6ner ein Freiheit verkl\u00e4rendes Leben.", "tokens": ["Bl\u00fc\u00b7het", "sch\u00f6\u00b7ner", "ein", "Frei\u00b7heit", "ver\u00b7kl\u00e4\u00b7ren\u00b7des", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Bilde in mir Deine Welt, Du die den Zweifel nicht kennt,", "tokens": ["Bil\u00b7de", "in", "mir", "Dei\u00b7ne", "Welt", ",", "Du", "die", "den", "Zwei\u00b7fel", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "$,", "PPER", "ART", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.25": {"text": "Die aus dem Busen mir zog den vergifteten Pfeil.", "tokens": ["Die", "aus", "dem", "Bu\u00b7sen", "mir", "zog", "den", "ver\u00b7gif\u00b7te\u00b7ten", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Alles was der Genius zu bilden mich dr\u00e4ngt,", "tokens": ["Al\u00b7les", "was", "der", "Ge\u00b7nius", "zu", "bil\u00b7den", "mich", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "ART", "NN", "PTKZU", "VVINF", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Bilde ich Schwacher es nicht, weilt schon gestaltet in Dir.", "tokens": ["Bil\u00b7de", "ich", "Schwa\u00b7cher", "es", "nicht", ",", "weilt", "schon", "ge\u00b7stal\u00b7tet", "in", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PPER", "PTKNEG", "$,", "VVFIN", "ADV", "VVPP", "APPR", "PPER", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Sch\u00fctzend will ich Dir folgen, Du Leben, das, wo ich zage mich sch\u00fctzt;", "tokens": ["Sch\u00fct\u00b7zend", "will", "ich", "Dir", "fol\u00b7gen", ",", "Du", "Le\u00b7ben", ",", "das", ",", "wo", "ich", "za\u00b7ge", "mich", "sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PPER", "NN", "$,", "PDS", "$,", "PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-++--+-++-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.29": {"text": "Das, wo ich welke erbl\u00fcht, gern mir die Jugend ersetzt.", "tokens": ["Das", ",", "wo", "ich", "wel\u00b7ke", "er\u00b7bl\u00fcht", ",", "gern", "mir", "die", "Ju\u00b7gend", "er\u00b7setzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "VVFIN", "VVPP", "$,", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Verwechselt im Herzen, schreitest Du k\u00fchn auf tobender Woge", "tokens": ["Ver\u00b7wech\u00b7selt", "im", "Her\u00b7zen", ",", "schrei\u00b7test", "Du", "k\u00fchn", "auf", "to\u00b7ben\u00b7der", "Wo\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Die aufbraust in mir und s\u00e4nftigst sie, da\u00df sie heller melodischer klingt.", "tokens": ["Die", "auf\u00b7braust", "in", "mir", "und", "s\u00e4nf\u00b7tigst", "sie", ",", "da\u00df", "sie", "hel\u00b7ler", "me\u00b7lo\u00b7di\u00b7scher", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.32": {"text": "In Dir weile ich flammend, Du giebst die lindernden \u00d6le", "tokens": ["In", "Dir", "wei\u00b7le", "ich", "flam\u00b7mend", ",", "Du", "giebst", "die", "lin\u00b7dern\u00b7den", "\u00d6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.33": {"text": "Und so s\u00fchnt sich in Dir, opfernd den G\u00f6ttern, der Sturm.", "tokens": ["Und", "so", "s\u00fchnt", "sich", "in", "Dir", ",", "op\u00b7fernd", "den", "G\u00f6t\u00b7tern", ",", "der", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "PPER", "$,", "ADJD", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}