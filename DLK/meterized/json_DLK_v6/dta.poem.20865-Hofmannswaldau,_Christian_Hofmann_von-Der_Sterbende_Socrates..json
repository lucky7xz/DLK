{"dta.poem.20865": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Du glaubest aber di\u00df/ da\u00df nach den letzten Stunden/", "tokens": ["Du", "glau\u00b7best", "a\u00b7ber", "di\u00df", "/", "da\u00df", "nach", "den", "letz\u00b7ten", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "$(", "KOUS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann nun der reine Geist dem Fleische wird entbunden/", "tokens": ["Wann", "nun", "der", "rei\u00b7ne", "Geist", "dem", "Flei\u00b7sche", "wird", "ent\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wir erstarret sind/", "tokens": ["Und", "wir", "er\u00b7star\u00b7ret", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der C\u00f6rper etwas noch in seinem Wesen bleibe/", "tokens": ["Der", "C\u00f6r\u00b7per", "et\u00b7was", "noch", "in", "sei\u00b7nem", "We\u00b7sen", "blei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und man das faule Grab genug bem\u00fchet find/", "tokens": ["Und", "man", "das", "fau\u00b7le", "Grab", "ge\u00b7nug", "be\u00b7m\u00fc\u00b7het", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "ADV", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eh\u2019 es den schlechten Klo\u00df des Leibes recht zutreibe.", "tokens": ["Eh'", "es", "den", "schlech\u00b7ten", "Klo\u00df", "des", "Lei\u00b7bes", "recht", "zu\u00b7trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja wenn ein schneller Fall durch unverhoffte Macht", "tokens": ["Ja", "wenn", "ein", "schnel\u00b7ler", "Fall", "durch", "un\u00b7ver\u00b7hoff\u00b7te", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat einen frischen Leib in ihre Bande bracht/", "tokens": ["Hat", "ei\u00b7nen", "fri\u00b7schen", "Leib", "in", "ih\u00b7re", "Ban\u00b7de", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wei\u00df man noch zu haben", "tokens": ["So", "wei\u00df", "man", "noch", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKZU", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Mittel so den Leib fast unversehrt erh\u00e4lt/", "tokens": ["Ein", "Mit\u00b7tel", "so", "den", "Leib", "fast", "un\u00b7ver\u00b7sehrt", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es lehrt Aegyptien den Kunstgrieff zubegraben/", "tokens": ["Es", "lehrt", "A\u00b7e\u00b7gyp\u00b7tien", "den", "Kunst\u00b7grieff", "zu\u00b7be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df in viel lange Zeit die Leiche nicht zuf\u00e4lt.", "tokens": ["Da\u00df", "in", "viel", "lan\u00b7ge", "Zeit", "die", "Lei\u00b7che", "nicht", "zu\u00b7f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wiewol nun unser Fleisch das F\u00e4ulni\u00df kan zutreiben/", "tokens": ["Wie\u00b7wol", "nun", "un\u00b7ser", "Fleisch", "das", "F\u00e4ul\u00b7ni\u00df", "kan", "zu\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und als ein zartes Ding nicht ewig wei\u00df zubleiben/", "tokens": ["Und", "als", "ein", "zar\u00b7tes", "Ding", "nicht", "e\u00b7wig", "wei\u00df", "zu\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So f\u00e4lt doch alles nicht/", "tokens": ["So", "f\u00e4lt", "doch", "al\u00b7les", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann Sehne/ Knorpel/ Bein/ kan nicht so bald verschwinden/", "tokens": ["Dann", "Seh\u00b7ne", "/", "Knor\u00b7pel", "/", "Bein", "/", "kan", "nicht", "so", "bald", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "NE", "$(", "NN", "$(", "VMFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df also auch der Todt nicht alle Glieder bricht/", "tokens": ["Da\u00df", "al\u00b7so", "auch", "der", "Todt", "nicht", "al\u00b7le", "Glie\u00b7der", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "PTKNEG", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wiewol in allem dem nichts himmlisches zu finden.", "tokens": ["Wie\u00b7wol", "in", "al\u00b7lem", "dem", "nichts", "himm\u00b7li\u00b7sches", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "ART", "PIS", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}