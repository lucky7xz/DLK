{"dta.poem.22539": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "91. Die geheimbe Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ros\u2019 ist meine Seel\u2019/ der Dorn de\u00df Fleisches-", "tokens": ["Die", "Ros'", "ist", "mei\u00b7ne", "Seel'", "/", "der", "Dorn", "de\u00df", "Flei\u00b7sches"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "lust/", "tokens": ["lust", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der Fr\u00fchling Gottes gunst/ sein Zorn ist K\u00e4lt und", "tokens": ["Der", "Fr\u00fch\u00b7ling", "Got\u00b7tes", "gunst", "/", "sein", "Zorn", "ist", "K\u00e4lt", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "$(", "PPOSAT", "NN", "VAFIN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Frost:", "tokens": ["Frost", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Jhr bl\u00fchn ist guttes thun/ den Dorn jhr Fleisch nicht", "tokens": ["Ihr", "bl\u00fchn", "ist", "gut\u00b7tes", "thun", "/", "den", "Dorn", "jhr", "Fleisch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VAFIN", "ADJA", "VVINF", "$(", "ART", "NN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "achten/", "tokens": ["ach\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit Tugenden sich ziehrn/ un\u0303 nach dem Him\u0303el trachte\u0303:", "tokens": ["Mit", "Tu\u00b7gen\u00b7den", "sich", "ziehrn", "/", "u\u00f1", "nach", "dem", "Him\u0303el", "tracht\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nim\u0303t sie die Zeit wol war/ un\u0303 bl\u00fcht weils Fr\u00fchling ist/", "tokens": ["Nim\u0303t", "sie", "die", "Zeit", "wol", "war", "/", "u\u00f1", "bl\u00fcht", "weils", "Fr\u00fch\u00b7ling", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "VAFIN", "$(", "KON", "VVFIN", "ADV", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wird sie ewiglich f\u00fcr GOttes Ros\u2019 erkiest.", "tokens": ["So", "wird", "sie", "e\u00b7wig\u00b7lich", "f\u00fcr", "Got\u00b7tes", "Ros'", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}