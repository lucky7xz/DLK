{"textgrid.poem.47033": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Blick ist matt, wie wenn mit bl\u00f6den Augen", "tokens": ["Dein", "Blick", "ist", "matt", ",", "wie", "wenn", "mit", "bl\u00f6\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne dreinsieht in die Winterstunde;", "tokens": ["Die", "Son\u00b7ne", "drein\u00b7sieht", "in", "die", "Win\u00b7ter\u00b7stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Ku\u00df ist welk, wie wenn das todeswunde", "tokens": ["Dein", "Ku\u00df", "ist", "welk", ",", "wie", "wenn", "das", "to\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herbstblatt den letzten Tropfen Tau will saugen.", "tokens": ["Herbst\u00b7blatt", "den", "letz\u00b7ten", "Trop\u00b7fen", "Tau", "will", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbkann ich davor, wenn Aug' und Mund nicht taugen?\u00ab", "tokens": ["\u00bb", "kann", "ich", "da\u00b7vor", ",", "wenn", "Aug'", "und", "Mund", "nicht", "tau\u00b7gen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "PAV", "$,", "KOUS", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, nicht am Auge liegt es noch am Munde,", "tokens": ["Ach", ",", "nicht", "am", "Au\u00b7ge", "liegt", "es", "noch", "am", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "APPRART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sind ein tot Gef\u00e4\u00df, wenn nicht vom Grunde", "tokens": ["Die", "sind", "ein", "tot", "Ge\u00b7f\u00e4\u00df", ",", "wenn", "nicht", "vom", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,", "KOUS", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Seele steigt herauf in Mund und Augen.", "tokens": ["Die", "See\u00b7le", "steigt", "her\u00b7auf", "in", "Mund", "und", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbso werd' ich keine Seel' im Grunde haben.\u00ab", "tokens": ["\u00bb", "so", "werd'", "ich", "kei\u00b7ne", "Seel'", "im", "Grun\u00b7de", "ha\u00b7ben", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja wohl, entweder hast du keine Seele,", "tokens": ["Ja", "wohl", ",", "ent\u00b7we\u00b7der", "hast", "du", "kei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "KON", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oder du hast zu Blick und Ku\u00df mir keine.", "tokens": ["O\u00b7der", "du", "hast", "zu", "Blick", "und", "Ku\u00df", "mir", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Heil ihm, der einst damit dich wird begaben,", "tokens": ["Heil", "ihm", ",", "der", "einst", "da\u00b7mit", "dich", "wird", "be\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "ADV", "KOUS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da\u00df ihn ja dann Eifersucht nicht qu\u00e4le", "tokens": ["Und", "da\u00df", "ihn", "ja", "dann", "Ei\u00b7fer\u00b7sucht", "nicht", "qu\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf den, der einst gekost mit einem Steine.", "tokens": ["Auf", "den", ",", "der", "einst", "ge\u00b7kost", "mit", "ei\u00b7nem", "Stei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}