{"textgrid.poem.25163": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: O komm, Georgiana! Die Rosen schau an,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O komm, Georgiana! Die Rosen schau an,", "tokens": ["O", "komm", ",", "Ge\u00b7or\u00b7gi\u00b7a\u00b7na", "!", "Die", "Ro\u00b7sen", "schau", "an", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "$.", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Den blumigen Teppich, den Flora rings spann;", "tokens": ["Den", "blu\u00b7mi\u00b7gen", "Tep\u00b7pich", ",", "den", "Flo\u00b7ra", "rings", "spann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Luft ist voll S\u00fc\u00dfe, das Wasser voll Glanz.", "tokens": ["Die", "Luft", "ist", "voll", "S\u00fc\u00b7\u00dfe", ",", "das", "Was\u00b7ser", "voll", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der West schwebt mit funkelndster Sonne zum Tanz.", "tokens": ["Der", "West", "schwebt", "mit", "fun\u00b7kelnds\u00b7ter", "Son\u00b7ne", "zum", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "O komm! La\u00df uns ziehn ins erfrischende Gr\u00fcn,", "tokens": ["O", "komm", "!", "La\u00df", "uns", "ziehn", "ins", "er\u00b7fri\u00b7schen\u00b7de", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVIMP", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durch Schatten und Matten, die duften und spr\u00fchn,", "tokens": ["Durch", "Schat\u00b7ten", "und", "Mat\u00b7ten", ",", "die", "duf\u00b7ten", "und", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zur Waldlichtung hin, wo die Feen sich drehn", "tokens": ["Zur", "Wald\u00b7lich\u00b7tung", "hin", ",", "wo", "die", "Feen", "sich", "drehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "PRF", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und Sylphen wie lichtester Sonnenglanz gehn.", "tokens": ["Und", "Syl\u00b7phen", "wie", "lich\u00b7tes\u00b7ter", "Son\u00b7nen\u00b7glanz", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und bist du dann m\u00fcde, so bett ich dich sacht", "tokens": ["Und", "bist", "du", "dann", "m\u00fc\u00b7de", ",", "so", "bett", "ich", "dich", "sacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf Moos und auf Blumen mit liebem Bedacht;", "tokens": ["Auf", "Moos", "und", "auf", "Blu\u00b7men", "mit", "lie\u00b7bem", "Be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dort lieg ich, Georgiana, zu F\u00fc\u00dfen dir nah,", "tokens": ["Dort", "lieg", "ich", ",", "Ge\u00b7or\u00b7gi\u00b7a\u00b7na", ",", "zu", "F\u00fc\u00b7\u00dfen", "dir", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "$,", "APPR", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mein M\u00e4rchen von Liebe erz\u00e4hl ich dir da.", "tokens": ["Mein", "M\u00e4r\u00b7chen", "von", "Lie\u00b7be", "er\u00b7z\u00e4hl", "ich", "dir", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und atme so z\u00e4rtlich und seufze so lind,", "tokens": ["Und", "at\u00b7me", "so", "z\u00e4rt\u00b7lich", "und", "seuf\u00b7ze", "so", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als seufze von Liebe der Fr\u00fchsommerwind;", "tokens": ["Als", "seuf\u00b7ze", "von", "Lie\u00b7be", "der", "Fr\u00fch\u00b7som\u00b7mer\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein sch\u00f6nes Knie pre\u00df ich und atme so tief \u2013", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Knie", "pre\u00df", "ich", "und", "at\u00b7me", "so", "tief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da f\u00fchlst du, da\u00df ich's war, der seufzend dich rief.", "tokens": ["Da", "f\u00fchlst", "du", ",", "da\u00df", "ich's", "war", ",", "der", "seuf\u00b7zend", "dich", "rief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VAFIN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Warum, liebstes M\u00e4dchen, entbehren dies Gl\u00fcck?", "tokens": ["Wa\u00b7rum", ",", "liebs\u00b7tes", "M\u00e4d\u00b7chen", ",", "ent\u00b7beh\u00b7ren", "dies", "Gl\u00fcck", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,", "VVFIN", "PDS", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Narr nur weist soviel Begl\u00fcckung zur\u00fcck:", "tokens": ["Ein", "Narr", "nur", "weist", "so\u00b7viel", "Be\u00b7gl\u00fc\u00b7ckung", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So l\u00e4chle Gew\u00e4hrung und gib deine Hand", "tokens": ["So", "l\u00e4ch\u00b7le", "Ge\u00b7w\u00e4h\u00b7rung", "und", "gib", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "VVIMP", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ein z\u00e4rtliches Wort, das dein Herz f\u00fcr mich fand.", "tokens": ["Und", "ein", "z\u00e4rt\u00b7li\u00b7ches", "Wort", ",", "das", "dein", "Herz", "f\u00fcr", "mich", "fand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "--+--+-++--+", "measure": "anapaest.di.plus"}}}}}