{"textgrid.poem.65278": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erl\u00f6sung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meines M\u00e4dchens Fenster,", "tokens": ["Vor", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schwing' ich meinen Hut,", "tokens": ["Da", "schwing'", "ich", "mei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schwing' ihn in die L\u00fcfte", "tokens": ["Ich", "schwing'", "ihn", "in", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit freiem, leichtem Muth.", "tokens": ["Mit", "frei\u00b7em", ",", "leich\u00b7tem", "Muth", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, sieh die gr\u00fcne Flagge,", "tokens": ["Sieh", ",", "sieh", "die", "gr\u00fc\u00b7ne", "Flag\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die von dem Hut mir weht!", "tokens": ["Die", "von", "dem", "Hut", "mir", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Band weht in die Weite,", "tokens": ["Das", "Band", "weht", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Weg von dannen geht.", "tokens": ["Mein", "Weg", "von", "dan\u00b7nen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kind, hast in deinem K\u00e4fich", "tokens": ["Kind", ",", "hast", "in", "dei\u00b7nem", "K\u00e4\u00b7fich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gequ\u00e4lt mich lang' genug.", "tokens": ["Ge\u00b7qu\u00e4lt", "mich", "lang'", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' den Stab zerbrochen,", "tokens": ["Ich", "hab'", "den", "Stab", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' wieder freien Flug.", "tokens": ["Hab'", "wie\u00b7der", "frei\u00b7en", "Flug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Juchhe, ihr Berg' und W\u00e4lder!", "tokens": ["Juch\u00b7he", ",", "ihr", "Ber\u00b7g'", "und", "W\u00e4l\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Juchhe, nun bin ich frei,", "tokens": ["Juch\u00b7he", ",", "nun", "bin", "ich", "frei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und schlage froh ein Schnippchen", "tokens": ["Und", "schla\u00b7ge", "froh", "ein", "Schnipp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der harten Liebestreu'!", "tokens": ["Der", "har\u00b7ten", "Lie\u00b7bes\u00b7treu'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun h\u00fcpft und springt, ihr Heerden!", "tokens": ["Nun", "h\u00fcpft", "und", "springt", ",", "ihr", "Heer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In's Freie geht's hinaus.", "tokens": ["In's", "Frei\u00b7e", "geht's", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollt nicht mehr Stoppeln suchen", "tokens": ["Sollt", "nicht", "mehr", "Stop\u00b7peln", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor meines M\u00e4dchens Haus.", "tokens": ["Vor", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich treib' euch auf die Weide", "tokens": ["Ich", "treib'", "euch", "auf", "die", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach frischen Felsenh\u00f6hn,", "tokens": ["Nach", "fri\u00b7schen", "Fel\u00b7sen\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo thauig ist der Rasen,", "tokens": ["Wo", "thau\u00b7ig", "ist", "der", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo k\u00fchle B\u00e4chlein gehn.", "tokens": ["Wo", "k\u00fch\u00b7le", "B\u00e4ch\u00b7lein", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Rupft im Vor\u00fcbergehen", "tokens": ["Rupft", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch noch ein H\u00e4lmchen aus,", "tokens": ["Euch", "noch", "ein", "H\u00e4lm\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inde\u00df mein Abschiedsliedchen", "tokens": ["In\u00b7de\u00df", "mein", "Ab\u00b7schieds\u00b7lied\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich singe vor dem Haus.", "tokens": ["Ich", "sin\u00b7ge", "vor", "dem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}