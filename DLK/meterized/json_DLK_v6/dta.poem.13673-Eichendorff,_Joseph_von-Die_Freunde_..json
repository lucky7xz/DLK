{"dta.poem.13673": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Seh ich des Tages wirrendes Beginnen,", "tokens": ["Seh", "ich", "des", "Ta\u00b7ges", "wir\u00b7ren\u00b7des", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Die bunten Bilder flieh'n und sich vereinen,", "tokens": ["Die", "bun\u00b7ten", "Bil\u00b7der", "flieh'n", "und", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6cht' ich das sch\u00f6ne Schattenspiel beweinen,", "tokens": ["M\u00f6cht'", "ich", "das", "sch\u00f6\u00b7ne", "Schat\u00b7ten\u00b7spiel", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn eitel ist, was jeder will gewinnen.", "tokens": ["Denn", "ei\u00b7tel", "ist", ",", "was", "je\u00b7der", "will", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}