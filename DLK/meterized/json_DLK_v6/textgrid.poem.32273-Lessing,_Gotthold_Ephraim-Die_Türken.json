{"textgrid.poem.32273": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die T\u00fcrken", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die T\u00fcrken haben sch\u00f6ne T\u00f6chter,", "tokens": ["Die", "T\u00fcr\u00b7ken", "ha\u00b7ben", "sch\u00f6\u00b7ne", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diese scharfe Keuschheitsw\u00e4chter;", "tokens": ["Und", "die\u00b7se", "schar\u00b7fe", "Keuschheits\u00b7w\u00e4ch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer will, kann mehr als Eine frein:", "tokens": ["Wer", "will", ",", "kann", "mehr", "als", "Ei\u00b7ne", "frein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "VMFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich m\u00f6chte schon ein T\u00fcrke sein.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "schon", "ein", "T\u00fcr\u00b7ke", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie wollt' ich mich der Lieb' ergeben!", "tokens": ["Wie", "wollt'", "ich", "mich", "der", "Lieb'", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wollt' ich liebend ruhig leben,", "tokens": ["Wie", "wollt'", "ich", "lie\u00b7bend", "ru\u00b7hig", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u2013 \u2013 Doch sie trinken keinen Wein;", "tokens": ["Und", "\u2013", "\u2013", "Doch", "sie", "trin\u00b7ken", "kei\u00b7nen", "Wein", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "$(", "KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, nein, ich mag kein T\u00fcrke sein.", "tokens": ["Nein", ",", "nein", ",", "ich", "mag", "kein", "T\u00fcr\u00b7ke", "sein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}