{"dta.poem.7717": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben mich gelobt, und mich dadurch besch\u00e4mt;", "tokens": ["Sie", "ha\u00b7ben", "mich", "ge\u00b7lobt", ",", "und", "mich", "da\u00b7durch", "be\u00b7sch\u00e4mt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "KON", "PRF", "PAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getadelt haben sie, und meinen Muth gel\u00e4hmt.", "tokens": ["Ge\u00b7ta\u00b7delt", "ha\u00b7ben", "sie", ",", "und", "mei\u00b7nen", "Muth", "ge\u00b7l\u00e4hmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Entweder haben sie mir Lob und Tadel schlecht", "tokens": ["Ent\u00b7we\u00b7der", "ha\u00b7ben", "sie", "mir", "Lob", "und", "Ta\u00b7del", "schlecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gegeben, oder ich genommen es nicht recht.", "tokens": ["Ge\u00b7ge\u00b7ben", ",", "o\u00b7der", "ich", "ge\u00b7nom\u00b7men", "es", "nicht", "recht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VVPP", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein st\u00e4rkendes Gef\u00fchl soll Lob und Tadel geben,", "tokens": ["Ein", "st\u00e4r\u00b7ken\u00b7des", "Ge\u00b7f\u00fchl", "soll", "Lob", "und", "Ta\u00b7del", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df etwas ist erreicht, und mehr noch anzustreben.", "tokens": ["Da\u00df", "et\u00b7was", "ist", "er\u00b7reicht", ",", "und", "mehr", "noch", "an\u00b7zu\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "$,", "KON", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}