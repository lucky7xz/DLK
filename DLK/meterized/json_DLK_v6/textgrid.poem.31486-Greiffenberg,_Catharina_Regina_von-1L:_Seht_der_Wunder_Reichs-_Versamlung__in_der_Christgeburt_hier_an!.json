{"textgrid.poem.31486": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht der Wunder Reichs- Versamlung / in der Christgeburt hier an!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht der Wunder Reichs- Versamlung / in der Christgeburt hier an!", "tokens": ["Seht", "der", "Wun\u00b7der", "Reichs", "Ver\u00b7sam\u00b7lung", "/", "in", "der", "Christ\u00b7ge\u00b7burt", "hier", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "TRUNC", "NN", "$(", "APPR", "ART", "NE", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "der so mit der Erden-Kugel / wie mit einen Apfel / spielt /", "tokens": ["der", "so", "mit", "der", "Er\u00b7den\u00b7Ku\u00b7gel", "/", "wie", "mit", "ei\u00b7nen", "Ap\u00b7fel", "/", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "$(", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "wird im Scho\u00df der keuschen Mutter ein geraume Zeit verh\u00fcllt.", "tokens": ["wird", "im", "Scho\u00df", "der", "keu\u00b7schen", "Mut\u00b7ter", "ein", "ge\u00b7rau\u00b7me", "Zeit", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "In das Glas des schwachen Leibes wird der Gottheit Meer gethan.", "tokens": ["In", "das", "Glas", "des", "schwa\u00b7chen", "Lei\u00b7bes", "wird", "der", "Got\u00b7theit", "Meer", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Auf da\u00df wir das Leben h\u00e4tten / wandlet er die Todten-Bahn.", "tokens": ["Auf", "da\u00df", "wir", "das", "Le\u00b7ben", "h\u00e4t\u00b7ten", "/", "wand\u00b7let", "er", "die", "Tod\u00b7ten\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "VAFIN", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Das uns Vberflu\u00df ergetzet' / er hier lauter Mangel f\u00fchlt.", "tokens": ["Das", "uns", "Vberf\u00b7lu\u00df", "er\u00b7get\u00b7zet'", "/", "er", "hier", "lau\u00b7ter", "Man\u00b7gel", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "VVFIN", "$(", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "--++--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Kurz! di\u00df Wunder-Wunder alles hier auf unser Heil nur zielt.", "tokens": ["Kurz", "!", "di\u00df", "Wun\u00b7der\u00b7Wun\u00b7der", "al\u00b7les", "hier", "auf", "un\u00b7ser", "Heil", "nur", "zielt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PDS", "NN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dieses Gottes-Werk kan heissen aller Gnaden offner Plan.", "tokens": ["Die\u00b7ses", "Got\u00b7tes\u00b7Werk", "kan", "heis\u00b7sen", "al\u00b7ler", "Gna\u00b7den", "off\u00b7ner", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "VVINF", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Die Herzsch\u00f6nste Eden-Blume / bl\u00fcht in Winters-ungeheur.", "tokens": ["Die", "Herz\u00b7sch\u00f6ns\u00b7te", "E\u00b7den\u00b7Blu\u00b7me", "/", "bl\u00fcht", "in", "Win\u00b7ter\u00b7sun\u00b7ge\u00b7heur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "$."], "meter": "-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.10": {"text": "In dem gr\u00f6sten Schnee und K\u00e4lte / kommt das heissest Liebes-Feur:", "tokens": ["In", "dem", "gr\u00f6s\u00b7ten", "Schnee", "und", "K\u00e4l\u00b7te", "/", "kommt", "das", "heis\u00b7sest", "Lie\u00b7bes\u00b7Feur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "da\u00df es unsre kalte Herzen Lieb-und Andacht-hitzig macht.", "tokens": ["da\u00df", "es", "uns\u00b7re", "kal\u00b7te", "Her\u00b7zen", "Lie\u00b7b\u00b7und", "An\u00b7dacht\u00b7hit\u00b7zig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.12": {"text": "Davids-Zweig / hat nun zugleiche / Laub und Bl\u00fch' und Frucht gebracht.", "tokens": ["Da\u00b7vids\u00b7Zweig", "/", "hat", "nun", "zu\u00b7glei\u00b7che", "/", "Laub", "und", "Bl\u00fch'", "und", "Frucht", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "ADV", "ADJA", "$(", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Da\u00df der Mensch das Leben h\u00e4tte / wurd' ein Mensch der Lebens-F\u00fcrst.", "tokens": ["Da\u00df", "der", "Mensch", "das", "Le\u00b7ben", "h\u00e4t\u00b7te", "/", "wurd'", "ein", "Mensch", "der", "Le\u00b7bens\u00b7F\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$(", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Seht / wie sehr den Gnaden-Brunnen hat nach unsern Heil ged\u00fcrst!", "tokens": ["Seht", "/", "wie", "sehr", "den", "Gna\u00b7den\u00b7Brun\u00b7nen", "hat", "nach", "un\u00b7sern", "Heil", "ge\u00b7d\u00fcrst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}