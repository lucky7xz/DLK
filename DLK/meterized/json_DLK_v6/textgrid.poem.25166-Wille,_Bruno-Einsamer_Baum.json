{"textgrid.poem.25166": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "Einsamer Baum", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zersplissen ist mein Haupt", "tokens": ["Zer\u00b7splis\u00b7sen", "ist", "mein", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von schwarzem Wolkenwetter;", "tokens": ["Von", "schwar\u00b7zem", "Wol\u00b7ken\u00b7wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herbstwind und Regen raubt", "tokens": ["Herbst\u00b7wind", "und", "Re\u00b7gen", "raubt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die abgestorbenen Bl\u00e4tter:", "tokens": ["Die", "ab\u00b7ge\u00b7stor\u00b7be\u00b7nen", "Bl\u00e4t\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So rag' ich ganz allein", "tokens": ["So", "rag'", "ich", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aus \u00f6dem Haidekraut", "tokens": ["Aus", "\u00f6\u00b7dem", "Hai\u00b7de\u00b7kraut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tr\u00e4ume von dem Hain,", "tokens": ["Und", "tr\u00e4u\u00b7me", "von", "dem", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der weit verloren blaut.", "tokens": ["Der", "weit", "ver\u00b7lo\u00b7ren", "blaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft, wenn mit grimmer Wucht", "tokens": ["Oft", ",", "wenn", "mit", "grim\u00b7mer", "Wucht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich packt ein n\u00e4chtlich Brausen,", "tokens": ["Mich", "packt", "ein", "n\u00e4cht\u00b7lich", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Raff ich mit j\u00e4hem Grausen", "tokens": ["Raff", "ich", "mit", "j\u00e4\u00b7hem", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zusammen mich zur Flucht;", "tokens": ["Zu\u00b7sam\u00b7men", "mich", "zur", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch halten z\u00e4he Schollen", "tokens": ["Doch", "hal\u00b7ten", "z\u00e4\u00b7he", "Schol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich an den Wurzeln fest. \u2013", "tokens": ["Mich", "an", "den", "Wur\u00b7zeln", "fest", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da steh' ich nun mit Grollen,", "tokens": ["Da", "steh'", "ich", "nun", "mit", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wild sch\u00fcttelnd mein Ge\u00e4st ...", "tokens": ["Wild", "sch\u00fct\u00b7telnd", "mein", "Ge\u00b7\u00e4st", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}