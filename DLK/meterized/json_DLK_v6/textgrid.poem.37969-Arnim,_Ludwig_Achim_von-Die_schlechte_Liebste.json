{"textgrid.poem.37969": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die schlechte Liebste", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzunder geht mir mein Trauern an,", "tokens": ["Je\u00b7tzun\u00b7der", "geht", "mir", "mein", "Trau\u00b7ern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Zeit ist leider kommen,", "tokens": ["Die", "Zeit", "ist", "lei\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die mir vor'm Jahr die Liebste war,", "tokens": ["Die", "mir", "vor'm", "Jahr", "die", "Liebs\u00b7te", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist schlecht mir vorgekommen.", "tokens": ["Ist", "schlecht", "mir", "vor\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist von lauter Eisen und Stahl,", "tokens": ["Mein", "Herz", "ist", "von", "lau\u00b7ter", "Ei\u00b7sen", "und", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dazu von Edelsteinen.", "tokens": ["Da\u00b7zu", "von", "E\u00b7del\u00b7stei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach wenn doch das mein Schatzliebchen erf\u00fchr,", "tokens": ["Ach", "wenn", "doch", "das", "mein", "Schatz\u00b7lieb\u00b7chen", "er\u00b7f\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Es w\u00fcrde trauren und weinen.", "tokens": ["Es", "w\u00fcr\u00b7de", "trau\u00b7ren", "und", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es trauert mit mir die Sonne, der Mond,", "tokens": ["Es", "trau\u00b7ert", "mit", "mir", "die", "Son\u00b7ne", ",", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dazu die hellen Sterne,", "tokens": ["Da\u00b7zu", "die", "hel\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die haben den lebenden, schwebenden", "tokens": ["Die", "ha\u00b7ben", "den", "le\u00b7ben\u00b7den", ",", "schwe\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Lustgarten an dem Himmel.", "tokens": ["Lust\u00b7gar\u00b7ten", "an", "dem", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Garten von lauter Lust war erbaut,", "tokens": ["Mein", "Gar\u00b7ten", "von", "lau\u00b7ter", "Lust", "war", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf einem schwarzen Sumpfe,", "tokens": ["Auf", "ei\u00b7nem", "schwar\u00b7zen", "Sump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo ich lebend und schwebend vertraut,", "tokens": ["Und", "wo", "ich", "le\u00b7bend", "und", "schwe\u00b7bend", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da ist ein Irrlicht versunken.", "tokens": ["Da", "ist", "ein", "Irr\u00b7licht", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Wollt Gott, da\u00df fr\u00fch ich gestorben w\u00e4r,", "tokens": ["Wollt", "Gott", ",", "da\u00df", "fr\u00fch", "ich", "ge\u00b7stor\u00b7ben", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "KOUS", "ADJD", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "In meinen jungen Jahren,", "tokens": ["In", "mei\u00b7nen", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4re mir all mein Lebetag,", "tokens": ["So", "w\u00e4\u00b7re", "mir", "all", "mein", "Le\u00b7be\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein gr\u00f6\u00dfre Freud wiederfahren.", "tokens": ["Kein", "gr\u00f6\u00df\u00b7re", "Freud", "wie\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Es ist nicht hier ein k\u00fchler Brunn,", "tokens": ["Es", "ist", "nicht", "hier", "ein", "k\u00fch\u00b7ler", "Brunn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir mein Herz th\u00e4t laben,", "tokens": ["Der", "mir", "mein", "Herz", "th\u00e4t", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein k\u00fchler Brunn zu aller Stund,", "tokens": ["Ein", "k\u00fch\u00b7ler", "Brunn", "zu", "al\u00b7ler", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er fliest aus meinem Herzen.", "tokens": ["Er", "fliest", "aus", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}