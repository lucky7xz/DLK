{"textgrid.poem.38950": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[ein edles Ebenmaa\u00df sucht keine Winkel]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein edles Ebenmaa\u00df sucht keine Winkel,", "tokens": ["Ein", "ed\u00b7les", "E\u00b7ben\u00b7maa\u00df", "sucht", "kei\u00b7ne", "Win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit reiner Sch\u00f6nheit dort sich zu verbergen:", "tokens": ["Mit", "rei\u00b7ner", "Sch\u00f6n\u00b7heit", "dort", "sich", "zu", "ver\u00b7ber\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sind wohl Riesen neben kleinen Zwergen,", "tokens": ["Wir", "sind", "wohl", "Rie\u00b7sen", "ne\u00b7ben", "klei\u00b7nen", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei Riesen selbst vergeht uns dieser D\u00fcnkel.", "tokens": ["Bei", "Rie\u00b7sen", "selbst", "ver\u00b7geht", "uns", "die\u00b7ser", "D\u00fcn\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es eilt so manches sanfte Versgeklinkel", "tokens": ["Es", "eilt", "so", "man\u00b7ches", "sanf\u00b7te", "Ver\u00b7sge\u00b7klin\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit holder Eil zu den papiernen S\u00e4rgen,", "tokens": ["Mit", "hol\u00b7der", "Eil", "zu", "den", "pa\u00b7pier\u00b7nen", "S\u00e4r\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da kommen denn die \u00fcberm\u00fcthgen Schergen", "tokens": ["Da", "kom\u00b7men", "denn", "die", "\u00fc\u00b7ber\u00b7m\u00fcth\u00b7gen", "Scher\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ziehn sie wieder an des Lichts Geblinkel.", "tokens": ["Und", "ziehn", "sie", "wie\u00b7der", "an", "des", "Lichts", "Ge\u00b7blin\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die liebevolle G\u00fcte will nicht strahlen,", "tokens": ["Die", "lie\u00b7be\u00b7vol\u00b7le", "G\u00fc\u00b7te", "will", "nicht", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein still Geheimni\u00df pa\u00dft nicht auf den Markt;", "tokens": ["Ein", "still", "Ge\u00b7heim\u00b7ni\u00df", "pa\u00dft", "nicht", "auf", "den", "Markt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer geht gern vor der Menge wenn er hinkt?", "tokens": ["Wer", "geht", "gern", "vor", "der", "Men\u00b7ge", "wenn", "er", "hinkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ihr die Kinderchen also zerharkt,", "tokens": ["Wenn", "ihr", "die", "Kin\u00b7der\u00b7chen", "al\u00b7so", "zer\u00b7harkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und r\u00fchrt sie um zu wiederholten Mahlen,", "tokens": ["Und", "r\u00fchrt", "sie", "um", "zu", "wie\u00b7der\u00b7hol\u00b7ten", "Mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUI", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist es nicht Eigenlob allein das stinkt.", "tokens": ["Ist", "es", "nicht", "Ei\u00b7gen\u00b7lob", "al\u00b7lein", "das", "stinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}