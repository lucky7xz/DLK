{"textgrid.poem.42864": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Jene kleinsten ehrlichen Artisten", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jener kleinsten, ehrlichen Artisten", "tokens": ["Je\u00b7ner", "kleins\u00b7ten", ",", "ehr\u00b7li\u00b7chen", "Ar\u00b7tis\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denk ich, die kein Ruhm belohnt,", "tokens": ["Denk", "ich", ",", "die", "kein", "Ruhm", "be\u00b7lohnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr Dasein \u00e4rmlich, flei\u00dfig fristen,", "tokens": ["Die", "ihr", "Da\u00b7sein", "\u00e4rm\u00b7lich", ",", "flei\u00b7\u00dfig", "fris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und in denen nur die Zukunft wohnt.", "tokens": ["Und", "in", "de\u00b7nen", "nur", "die", "Zu\u00b7kunft", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In Programmen stehen sie bescheiden,", "tokens": ["In", "Pro\u00b7gram\u00b7men", "ste\u00b7hen", "sie", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das Publikum bleibt ihnen stumm.", "tokens": ["Und", "das", "Pub\u00b7li\u00b7kum", "bleibt", "ih\u00b7nen", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dennoch geben sie ihr Bestes und beneiden", "tokens": ["Den\u00b7noch", "ge\u00b7ben", "sie", "ihr", "Bes\u00b7tes", "und", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Gr\u00f6\u00dfre nicht. Und wissen nicht, warum.", "tokens": ["Gr\u00f6\u00df\u00b7re", "nicht", ".", "Und", "wis\u00b7sen", "nicht", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "KON", "VVFIN", "PTKNEG", "$,", "PWAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Grober D\u00fcnkel dr\u00fcckt sie in die Ecken.", "tokens": ["Gro\u00b7ber", "D\u00fcn\u00b7kel", "dr\u00fcckt", "sie", "in", "die", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre Grenze ist der Rampenschein.", "tokens": ["Ih\u00b7re", "Gren\u00b7ze", "ist", "der", "Ram\u00b7pen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber nachts vor kleinen M\u00e4dchen recken", "tokens": ["A\u00b7ber", "nachts", "vor", "klei\u00b7nen", "M\u00e4d\u00b7chen", "re\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie sich auf in K\u00fcnstlerschw\u00e4rmerein.", "tokens": ["Sie", "sich", "auf", "in", "K\u00fcnst\u00b7ler\u00b7schw\u00e4r\u00b7me\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Die ihr bleiben sollt, wo wir begonnen,", "tokens": ["Die", "ihr", "blei\u00b7ben", "sollt", ",", "wo", "wir", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00f6gt ihr ruhmlos sein und unbegabt,", "tokens": ["M\u00f6gt", "ihr", "ruh\u00b7mlos", "sein", "und", "un\u00b7be\u00b7gabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch euch tr\u00f6stet: Uns ist viel zerronnen,", "tokens": ["Doch", "euch", "tr\u00f6s\u00b7tet", ":", "Uns", "ist", "viel", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00f6nes, was ihr jetzt noch in euch habt.", "tokens": ["Sch\u00f6\u00b7nes", ",", "was", "ihr", "jetzt", "noch", "in", "euch", "habt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PWS", "PPER", "ADV", "ADV", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ehrlichkeit ist Kunst und derart selten,", "tokens": ["Ehr\u00b7lich\u00b7keit", "ist", "Kunst", "und", "der\u00b7art", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df es wenig Wichtigeres gibt.", "tokens": ["Da\u00df", "es", "we\u00b7nig", "Wich\u00b7ti\u00b7ge\u00b7res", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Euer Schicksal wird euch reich vergelten,", "tokens": ["Eu\u00b7er", "Schick\u00b7sal", "wird", "euch", "reich", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ihr euer Schicksal habt geliebt.", "tokens": ["Da\u00df", "ihr", "eu\u00b7er", "Schick\u00b7sal", "habt", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}