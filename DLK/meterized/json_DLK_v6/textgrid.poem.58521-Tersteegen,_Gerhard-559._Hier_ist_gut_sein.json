{"textgrid.poem.58521": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "559. Hier ist gut sein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist mir doch geschehn, wo bin ich hingef\u00fchrt,", "tokens": ["Wie", "ist", "mir", "doch", "ge\u00b7schehn", ",", "wo", "bin", "ich", "hin\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch eine neue Welt hat mich in sich genommen!", "tokens": ["Welch", "ei\u00b7ne", "neu\u00b7e", "Welt", "hat", "mich", "in", "sich", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Geist in seinem Grund die tiefste Stille sp\u00fcrt,", "tokens": ["Mein", "Geist", "in", "sei\u00b7nem", "Grund", "die", "tiefs\u00b7te", "Stil\u00b7le", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin ins Paradies, und es in mich gekommen.", "tokens": ["Ich", "bin", "ins", "Pa\u00b7ra\u00b7dies", ",", "und", "es", "in", "mich", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein vor bedr\u00e4ngter Geist, der kriegt nun weiten Raum,", "tokens": ["Mein", "vor", "be\u00b7dr\u00e4ng\u00b7ter", "Geist", ",", "der", "kriegt", "nun", "wei\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sch\u00f6pfet frische Luft und sanft empor sich hebet,", "tokens": ["Er", "sch\u00f6p\u00b7fet", "fri\u00b7sche", "Luft", "und", "sanft", "em\u00b7por", "sich", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJD", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie munter sieht er aus, der sonst sich regte kaum,", "tokens": ["Wie", "mun\u00b7ter", "sieht", "er", "aus", ",", "der", "sonst", "sich", "reg\u00b7te", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und frei im Element der reinsten Wonne lebet!", "tokens": ["Und", "frei", "im", "E\u00b7le\u00b7ment", "der", "reins\u00b7ten", "Won\u00b7ne", "le\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}