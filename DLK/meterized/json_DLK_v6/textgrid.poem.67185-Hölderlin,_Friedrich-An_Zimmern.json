{"textgrid.poem.67185": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Zimmern", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Linien des Lebens sind verschieden,", "tokens": ["Die", "Li\u00b7ni\u00b7en", "des", "Le\u00b7bens", "sind", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Wege sind, und wie der Berge Grenzen.", "tokens": ["Wie", "We\u00b7ge", "sind", ",", "und", "wie", "der", "Ber\u00b7ge", "Gren\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,", "KON", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was hier wir sind, kann dort ein Gott erg\u00e4nzen", "tokens": ["Was", "hier", "wir", "sind", ",", "kann", "dort", "ein", "Gott", "er\u00b7g\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "VAFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Harmonien und ewigem Lohn und Frieden.", "tokens": ["Mit", "Har\u00b7mo\u00b7ni\u00b7en", "und", "e\u00b7wi\u00b7gem", "Lohn", "und", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}