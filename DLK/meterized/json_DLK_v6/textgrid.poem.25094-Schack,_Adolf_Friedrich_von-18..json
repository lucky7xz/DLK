{"textgrid.poem.25094": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getrost! der Weg war hei\u00df und lang,", "tokens": ["Ge\u00b7trost", "!", "der", "Weg", "war", "hei\u00df", "und", "lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein der Abend kommt;", "tokens": ["Al\u00b7lein", "der", "A\u00b7bend", "kommt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesorgt ist, sei darum nicht bang,", "tokens": ["Ge\u00b7sorgt", "ist", ",", "sei", "da\u00b7rum", "nicht", "bang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VAFIN", "PAV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alles, was dir frommt.", "tokens": ["F\u00fcr", "al\u00b7les", ",", "was", "dir", "frommt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Schatten werden l\u00e4nger schon", "tokens": ["Die", "Schat\u00b7ten", "wer\u00b7den", "l\u00e4n\u00b7ger", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fchl're L\u00fcfte wehn;", "tokens": ["Und", "k\u00fchl'\u00b7re", "L\u00fcf\u00b7te", "wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Turme hallt der Glocke Ton", "tokens": ["Vom", "Tur\u00b7me", "hallt", "der", "Glo\u00b7cke", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mahnt zum Schlafengehn.", "tokens": ["Und", "mahnt", "zum", "Schla\u00b7fen\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald thut sich dir das Rasthaus auf,", "tokens": ["Bald", "thut", "sich", "dir", "das", "Rast\u00b7haus", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dem f\u00fcr alle Raum;", "tokens": ["In", "dem", "f\u00fcr", "al\u00b7le", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da labt dich nach dem Tageslauf", "tokens": ["Da", "labt", "dich", "nach", "dem", "Ta\u00b7ges\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schlummer ohne Traum.", "tokens": ["Ein", "Schlum\u00b7mer", "oh\u00b7ne", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}