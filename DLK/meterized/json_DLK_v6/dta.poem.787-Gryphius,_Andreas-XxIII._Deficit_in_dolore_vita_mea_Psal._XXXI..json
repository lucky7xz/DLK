{"dta.poem.787": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.  \n Deficit in dolore vita mea/ Psal. XXXI.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mjt thr\u00e4nen vnd mit ach/ mit arbeit weh vnd zagen ", "tokens": ["Mjt", "thr\u00e4\u00b7nen", "vnd", "mit", "ach", "/", "mit", "ar\u00b7beit", "weh", "vnd", "za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "APPR", "XY", "$(", "APPR", "NN", "PTKVZ", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschlie\u00df ich Stund vnd Tag/ der Feber grim\u0303es Leidt", "tokens": ["Ver\u00b7schlie\u00df", "ich", "Stund", "vnd", "Tag", "/", "der", "Fe\u00b7ber", "grim\u0303es", "Leidt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Nimmt mit dem Jahr mich hin/ die flucht der schnellen", "tokens": ["Nimmt", "mit", "dem", "Jahr", "mich", "hin", "/", "die", "flucht", "der", "schnel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$(", "ART", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zeit/", "tokens": ["zeit", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "L\u00e4st mich mein herbes Weh/ mein Elend kaum beklagen!", "tokens": ["L\u00e4st", "mich", "mein", "her\u00b7bes", "Weh", "/", "mein", "E\u00b7lend", "kaum", "be\u00b7kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So bald die Sonn vns bringt den lichten Tag getragen/", "tokens": ["So", "bald", "die", "Sonn", "vns", "bringt", "den", "lich\u00b7ten", "Tag", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rufft jammer mich zu Kampff. Dafern mir in dem streitt", "tokens": ["Rufft", "jam\u00b7mer", "mich", "zu", "Kampff", ".", "Da\u00b7fern", "mir", "in", "dem", "streitt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$.", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Hoffnung h\u00fclff einspricht/ f\u00e4lt der erhitzte Neidt/", "tokens": ["Die", "Hoff\u00b7nung", "h\u00fclff", "ein\u00b7spricht", "/", "f\u00e4lt", "der", "er\u00b7hitz\u00b7te", "Neidt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich mehr denn h\u00fcndisch an/ vnd l\u00e4st nicht ab zu nagen.", "tokens": ["Mich", "mehr", "denn", "h\u00fcn\u00b7disch", "an", "/", "vnd", "l\u00e4st", "nicht", "ab", "zu", "na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ist nichts/ das mich mit ruh\u2019 vnd rath ergetzen kan/", "tokens": ["Ist", "nichts", "/", "das", "mich", "mit", "ruh'", "vnd", "rath", "er\u00b7get\u00b7zen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$(", "PRELS", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie lange sol ich noch in diesem kummer stecken?", "tokens": ["Wie", "lan\u00b7ge", "sol", "ich", "noch", "in", "die\u00b7sem", "kum\u00b7mer", "ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich weynel doch vmbsonst. so offt die schwartze Nacht", "tokens": ["Ich", "wey\u00b7nel", "doch", "vmbsonst", ".", "so", "offt", "die", "schwart\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den tr\u00fcben Sternen rufft/ vnd wenn ", "tokens": ["Den", "tr\u00fc\u00b7ben", "Ster\u00b7nen", "rufft", "/", "vnd", "wenn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KON", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Abend deckt die Noth/ kein Tag kan trost erwecken.", "tokens": ["Kein", "A\u00b7bend", "deckt", "die", "Noth", "/", "kein", "Tag", "kan", "trost", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$(", "PIAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}