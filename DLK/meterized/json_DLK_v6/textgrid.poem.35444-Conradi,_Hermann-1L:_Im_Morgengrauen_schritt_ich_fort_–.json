{"textgrid.poem.35444": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Morgengrauen schritt ich fort \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Morgengrauen schritt ich fort \u2013", "tokens": ["Im", "Mor\u00b7gen\u00b7grau\u00b7en", "schritt", "ich", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nebel lag in den Gassen ...", "tokens": ["Ne\u00b7bel", "lag", "in", "den", "Gas\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "In Qualen war mir das Herz verdorrt \u2013", "tokens": ["In", "Qua\u00b7len", "war", "mir", "das", "Herz", "ver\u00b7dorrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Lippe sprach kein Abschiedswort \u2013", "tokens": ["Die", "Lip\u00b7pe", "sprach", "kein", "Ab\u00b7schieds\u00b7wort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie st\u00f6hnte nur leise: Verlassen!", "tokens": ["Sie", "st\u00f6hn\u00b7te", "nur", "lei\u00b7se", ":", "Ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Verlassen! Kennst du das Marterwort?", "tokens": ["Ver\u00b7las\u00b7sen", "!", "Kennst", "du", "das", "Mar\u00b7ter\u00b7wort", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das fri\u00dft wie verruchte Schande!", "tokens": ["Das", "fri\u00dft", "wie", "ver\u00b7ruch\u00b7te", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Qualen war mir das Herz verdorrt \u2013", "tokens": ["In", "Qua\u00b7len", "war", "mir", "das", "Herz", "ver\u00b7dorrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Morgengrauen ging ich fort \u2013", "tokens": ["Im", "Mor\u00b7gen\u00b7grau\u00b7en", "ging", "ich", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinaus in die d\u00e4mmernden Lande!", "tokens": ["Hin\u00b7aus", "in", "die", "d\u00e4m\u00b7mern\u00b7den", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Entgegen dem jungen Maientag:", "tokens": ["Ent\u00b7ge\u00b7gen", "dem", "jun\u00b7gen", "Mai\u00b7en\u00b7tag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das war ein seltsam Passen!", "tokens": ["Das", "war", "ein", "selt\u00b7sam", "Pas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00e4hlich wurde die Welt nun wach \u2013", "tokens": ["M\u00e4h\u00b7lich", "wur\u00b7de", "die", "Welt", "nun", "wach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Was war mir der prangende Fr\u00fchlingstag \u2013", "tokens": ["Was", "war", "mir", "der", "pran\u00b7gen\u00b7de", "Fr\u00fch\u00b7lings\u00b7tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich st\u00f6hnte nur leise: Verlassen! ...", "tokens": ["Ich", "st\u00f6hn\u00b7te", "nur", "lei\u00b7se", ":", "Ver\u00b7las\u00b7sen", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}