{"textgrid.poem.58052": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "90. Dem\u00fctig, doch getrost", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dir schwach, klein und blo\u00df,", "tokens": ["In", "dir", "schwach", ",", "klein", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Gottes St\u00e4rke gro\u00df,", "tokens": ["In", "Got\u00b7tes", "St\u00e4r\u00b7ke", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Demut tief geb\u00fccket,", "tokens": ["In", "De\u00b7mut", "tief", "ge\u00b7b\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Glauben unverr\u00fccket,", "tokens": ["Im", "Glau\u00b7ben", "un\u00b7ver\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Geist getrost und frei", "tokens": ["Im", "Geist", "ge\u00b7trost", "und", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In deinem Wandel sei!", "tokens": ["In", "dei\u00b7nem", "Wan\u00b7del", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}