", "ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Na, de Mamsell, de lep nu nah't Gericht.", "tokens": ["Na", ",", "de", "Mam\u00b7sell", ",", "de", "lep", "nu", "nah't", "Ge\u00b7richt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.19": {"text": "\u00bbhei slog mi \u00e4wer'n Puckel, \u00e4wer'n Bregen,", "tokens": ["\u00bb", "hei", "slog", "mi", "\u00e4\u00b7wer'n", "Pu\u00b7ckel", ",", "\u00e4\u00b7wer'n", "Bre\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Un 'n St\u00fcck'ner dr\u00fctteihn heww ick kregen\u00ab,", "tokens": ["Un", "'n", "St\u00fc\u00b7ck'\u00b7ner", "dr\u00fct\u00b7teihn", "heww", "ick", "kre\u00b7gen", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "VVFIN", "PPER", "VVINF", "$(", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "So sl\u00f6t sei ehre Klag'geschicht.", "tokens": ["So", "sl\u00f6t", "sei", "eh\u00b7re", "Klag'\u00b7ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "De Amtmann ded nu minen Str\u00f6ming fragen:", "tokens": ["De", "Amt\u00b7mann", "ded", "nu", "mi\u00b7nen", "Str\u00f6\u00b7ming", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "w