{"textgrid.poem.52287": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "8. De \u00dcmgang mit Damen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schriwerbengels, dat is wi\u00df,", "tokens": ["Schri\u00b7wer\u00b7ben\u00b7gels", ",", "dat", "is", "wi\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "De s\u00fcnd unplumpsch von Natur,", "tokens": ["De", "s\u00fcnd", "un\u00b7plumpsch", "von", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wat so 'n rechten Schriwer is,", "tokens": ["Wat", "so", "'n", "rech\u00b7ten", "Schri\u00b7wer", "is", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dat's 'ne grave Kreatur.", "tokens": ["Dat's", "'ne", "gra\u00b7ve", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Utnahm mak ick girn mit Freuden,", "tokens": ["Ut\u00b7nahm", "mak", "ick", "girn", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Un nich \u00fcmmer pa\u00dft de Fall!", "tokens": ["Un", "nich", "\u00fcm\u00b7mer", "pa\u00dft", "de", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4nnigein, de is bescheiden;", "tokens": ["M\u00e4n\u00b7ni\u00b7gein", ",", "de", "is", "be\u00b7schei\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Un ick mein sei ok nich ", "tokens": ["Un", "ick", "mein", "sei", "ok", "nich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PPOSAT", "VAFIN", "NE", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ne, ick mein de Grotmulsprekers,", "tokens": ["Ne", ",", "ick", "mein", "de", "Grot\u00b7mul\u00b7spre\u00b7kers", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "PPOSAT", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein de rechten, echten Sloms,", "tokens": ["Mein", "de", "rech\u00b7ten", ",", "ech\u00b7ten", "Sloms", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein de rechten, echten Br\u00e4kers;", "tokens": ["Mein", "de", "rech\u00b7ten", ",", "ech\u00b7ten", "Br\u00e4\u00b7kers", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00f6r gew\u00f6hnlich heiten s' Stroms.", "tokens": ["F\u00f6r", "ge\u00b7w\u00f6hn\u00b7lich", "hei\u00b7ten", "s'", "Stroms", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Wenn des' Ort so up en Hof is,", "tokens": ["Wenn", "des'", "Ort", "so", "up", "en", "Hof", "is", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "FM", "FM", "FM", "FM", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ach wat s\u00fcnd sei denn so d\u00fcnn!", "tokens": ["Ach", "wat", "s\u00fcnd", "sei", "denn", "so", "d\u00fcnn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADJD", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn de Herr man 'n beten groww is,", "tokens": ["Wenn", "de", "Herr", "man", "'n", "be\u00b7ten", "groww", "is", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "PIS", "ART", "ADJA", "FM", "FM", "$,"], "meter": "--+-+---+", "measure": "anapaest.init"}, "line.4": {"text": "Krupen s' nah en Muslock 'rin.", "tokens": ["Kru\u00b7pen", "s'", "nah", "en", "Mus\u00b7lock", "'rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "ADJA", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Ach, wo m\u00e4nnigein hett seten", "tokens": ["Ach", ",", "wo", "m\u00e4n\u00b7ni\u00b7gein", "hett", "se\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An 'n Bedeintendisch un et,", "tokens": ["An", "'n", "Be\u00b7dein\u00b7ten\u00b7disch", "un", "et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "FM", "FM", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ebenso as't Stuwenm\u00e4ten,", "tokens": ["E\u00b7ben\u00b7so", "as't", "Stu\u00b7wen\u00b7m\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wat de Herrschaft \u00e4wrig let.", "tokens": ["Wat", "de", "Herr\u00b7schaft", "\u00e4w\u00b7rig", "let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn hei \u00e4werst denn einmalen", "tokens": ["Wenn", "hei", "\u00e4\u00b7werst", "denn", "ein\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "FM", "FM", "KON", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "K\u00fcmmt tau Stadt 'rin, na, denn geiht't,", "tokens": ["K\u00fcmmt", "tau", "Stadt", "'rin", ",", "na", ",", "denn", "geiht't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "NE", "$,", "ITJ", "$,", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Grad as wenn so'n j\u00f6hrig Fahlen", "tokens": ["Grad", "as", "wenn", "so'n", "j\u00f6h\u00b7rig", "Fah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KOUS", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "V\u00f6rn un hinnen utslahn deiht.", "tokens": ["V\u00f6rn", "un", "hin\u00b7nen", "uts\u00b7lahn", "deiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbs\u00fch dor, Brauder, b\u00fcst du hir?", "tokens": ["\u00bb", "s\u00fch", "dor", ",", "Brau\u00b7der", ",", "b\u00fcst", "du", "hir", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Is de Vo\u00df dor buten ", "tokens": ["Is", "de", "Vo\u00df", "dor", "bu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heda! Hir! Mark\u00fcr! Mark\u00fcr!", "tokens": ["He\u00b7da", "!", "Hir", "!", "Mar\u00b7k\u00fcr", "!", "Mar\u00b7k\u00fcr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "NE", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring mi mal 'ne Buddel Win.\u00ab", "tokens": ["Bring", "mi", "mal", "'ne", "Bud\u00b7del", "Win", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit de Dalers smiten s' r\u00fcmmer,", "tokens": ["Mit", "de", "Da\u00b7lers", "smi\u00b7ten", "s'", "r\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Grad as wir dat Geld man Dreck,", "tokens": ["Grad", "as", "wir", "dat", "Geld", "man", "Dreck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ART", "NN", "PIS", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Un \u00bbMark\u00fcr!\u00ab, so geiht dat \u00fcmmer,", "tokens": ["Un", "\u00bb", "Mar\u00b7k\u00fcr", "!", "\u00ab", ",", "so", "geiht", "dat", "\u00fcm\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$.", "$(", "$,", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Un \u00bbMark\u00fcr!\u00ab in einem weg.", "tokens": ["Un", "\u00bb", "Mar\u00b7k\u00fcr", "!", "\u00ab", "in", "ei\u00b7nem", "weg", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$.", "$(", "APPR", "ART", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbbr\u00e4uding, kumm, nu will w' mal danzen!", "tokens": ["\u00bb", "br\u00e4u\u00b7ding", ",", "kumm", ",", "nu", "will", "w'", "mal", "dan\u00b7zen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJD", "$,", "ADV", "VMFIN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hir is baben h\u00fct en Ball.", "tokens": ["Hir", "is", "ba\u00b7ben", "h\u00fct", "en", "Ball", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will'n de Dirns mal 'r\u00fcmkuranzen,", "tokens": ["Will'n", "de", "Dirns", "mal", "'r\u00fcm\u00b7ku\u00b7ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dat ehr glik dat Wedder sall!\u00ab", "tokens": ["Dat", "ehr", "glik", "dat", "Wed\u00b7der", "sall", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "In den Saal, dor kamen s' 'rinner.", "tokens": ["In", "den", "Saal", ",", "dor", "ka\u00b7men", "s'", "'rin\u00b7ner", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "NE", "NE", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "(ach, wo n\u00fcdlich b\u00fcst du, Strom!)", "tokens": ["(", "ach", ",", "wo", "n\u00fcd\u00b7lich", "b\u00fcst", "du", ",", "Strom", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbna, Muskant, geswinn, geswinner!", "tokens": ["\u00bb", "na", ",", "Mus\u00b7kant", ",", "ges\u00b7winn", ",", "ges\u00b7win\u00b7ner", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dat geiht just as in en Drom.\u00ab", "tokens": ["Dat", "geiht", "just", "as", "in", "en", "Drom", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Wenn sei sick 'ne Dam denn halen,", "tokens": ["Wenn", "sei", "sick", "'ne", "Dam", "denn", "ha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "FM", "KON", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "S\u00fcnd sei zierlich as en O\u00df;", "tokens": ["S\u00fcnd", "sei", "zier\u00b7lich", "as", "en", "O\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "FM", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchren sei den Saal hendalen,", "tokens": ["F\u00fch\u00b7ren", "sei", "den", "Saal", "hen\u00b7da\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Just as leddten s' ehren Vo\u00df.", "tokens": ["Just", "as", "ledd\u00b7ten", "s'", "eh\u00b7ren", "Vo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Sei schenier'n sick nich en Happen,", "tokens": ["Sei", "sche\u00b7nier'n", "sick", "nich", "en", "Hap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehr geh\u00fcrt de ganze Saal,", "tokens": ["Ehr", "ge\u00b7h\u00fcrt", "de", "gan\u00b7ze", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Un sei springen 'r\u00fcm un trappen,", "tokens": ["Un", "sei", "sprin\u00b7gen", "'r\u00fcm", "un", "trap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Talpsen \u00fcmmer up un dal.", "tokens": ["Tal\u00b7psen", "\u00fcm\u00b7mer", "up", "un", "dal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.13": {"line.1": {"text": "In sin Fett is nu en jeder.", "tokens": ["In", "sin", "Fett", "is", "nu", "en", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "FM", "FM", "FM", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbbrauder, h\u00fct danz ick mi dod!\u00ab", "tokens": ["\u00bb", "brau\u00b7der", ",", "h\u00fct", "danz", "ick", "mi", "dod", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pedden de Damen up de Kleder,", "tokens": ["Ped\u00b7den", "de", "Da\u00b7men", "up", "de", "Kle\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "NE", "NE", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Geiten Win ehr in den Schot.", "tokens": ["Gei\u00b7ten", "Win", "ehr", "in", "den", "Schot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "\u00bbkumm, wi will'n mal drinken, Brauder!", "tokens": ["\u00bb", "kumm", ",", "wi", "will'n", "mal", "drin\u00b7ken", ",", "Brau\u00b7der", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PWAV", "VMFIN", "ADV", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Un Champagne m\u00f6t dat sin."