", "Amt\u00b7mann", ",", "ne", ",", "dat", "nenn'", "ick", "\u00e4\u00b7wer\u00b7dre\u00b7wen", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "Dat nenn ick utgestunk'ne L\u00e4gen!", "tokens": ["Dat", "nenn", "ick", "ut\u00b7ge\u00b7stunk'\u00b7ne", "L\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ick strid dat nich, ick heww sei slahn;", "tokens": ["Ick", "strid", "dat", "nich", ",", "ick", "heww", "sei", "slahn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PTKNEG", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Ick weit mit Damens \u00fcmtaugahn.\u00ab", "tokens": ["Ick", "weit", "mit", "Da\u00b7mens", "\u00fcm\u00b7tau\u00b7gahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}