{"textgrid.poem.41600": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf immer, freier Mensch, wirst lieben du das Meer,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf immer, freier Mensch, wirst lieben du das Meer,", "tokens": ["Auf", "im\u00b7mer", ",", "frei\u00b7er", "Mensch", ",", "wirst", "lie\u00b7ben", "du", "das", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ADJA", "NN", "$,", "VAFIN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Spiegel ist das Meer. Du schaust der Seele Bildnis", "tokens": ["Dein", "Spie\u00b7gel", "ist", "das", "Meer", ".", "Du", "schaust", "der", "See\u00b7le", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im weiten Wellenspiel der ungeheuren Wildnis,", "tokens": ["Im", "wei\u00b7ten", "Wel\u00b7len\u00b7spiel", "der", "un\u00b7ge\u00b7heu\u00b7ren", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich ihm ist deine Brust von Bitternissen schwer.", "tokens": ["Gleich", "ihm", "ist", "dei\u00b7ne", "Brust", "von", "Bit\u00b7ter\u00b7nis\u00b7sen", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gern schaust dein Bild du, das die Wellen dir enth\u00fcllen,", "tokens": ["Gern", "schaust", "dein", "Bild", "du", ",", "das", "die", "Wel\u00b7len", "dir", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "PRELS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Auge und mit Arm fa\u00dft du es, und dein Herz", "tokens": ["Mit", "Au\u00b7ge", "und", "mit", "Arm", "fa\u00dft", "du", "es", ",", "und", "dein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "PPER", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergi\u00dft wie trunken oft den eignen lauten Schmerz", "tokens": ["Ver\u00b7gi\u00dft", "wie", "trun\u00b7ken", "oft", "den", "eig\u00b7nen", "lau\u00b7ten", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJD", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei dieses Klagesangs unz\u00e4hmbar wildem Br\u00fcllen.", "tokens": ["Bei", "die\u00b7ses", "Kla\u00b7ge\u00b7sangs", "un\u00b7z\u00e4hm\u00b7bar", "wil\u00b7dem", "Br\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schweigsam und dunkel seid ihr beide allezeit:", "tokens": ["Schweig\u00b7sam", "und", "dun\u00b7kel", "seid", "ihr", "bei\u00b7de", "al\u00b7le\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "PIS", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mensch, noch drang keiner je in deine tiefsten Gr\u00fcnde,", "tokens": ["Mensch", ",", "noch", "drang", "kei\u00b7ner", "je", "in", "dei\u00b7ne", "tiefs\u00b7ten", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Meer, noch fand keiner je den Reichtum deiner Schl\u00fcnde,", "tokens": ["Meer", ",", "noch", "fand", "kei\u00b7ner", "je", "den", "Reich\u00b7tum", "dei\u00b7ner", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "So bergt ihr euren Hort in finstrer Heimlichkeit.", "tokens": ["So", "bergt", "ihr", "eu\u00b7ren", "Hort", "in", "finst\u00b7rer", "Heim\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jahrtausende hindurch rollt euer nimmerm\u00fcder", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "hin\u00b7durch", "rollt", "eu\u00b7er", "nim\u00b7mer\u00b7m\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mitleidsloser Kampf bar jeder Reue fort.", "tokens": ["Und", "mit\u00b7leids\u00b7lo\u00b7ser", "Kampf", "bar", "je\u00b7der", "Reu\u00b7e", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "PIAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So sehr liebt beide ihr die Schlachten und den Mord,", "tokens": ["So", "sehr", "liebt", "bei\u00b7de", "ihr", "die", "Schlach\u00b7ten", "und", "den", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O ewges K\u00e4mpferpaar, o nie vers\u00f6hnte Br\u00fcder!", "tokens": ["O", "ew\u00b7ges", "K\u00e4mp\u00b7fer\u00b7paar", ",", "o", "nie", "ver\u00b7s\u00f6hn\u00b7te", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}