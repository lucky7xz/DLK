{"textgrid.poem.46864": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[h\u00e4tt' ich um Gold und Edelstein]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' ich um Gold und Edelstein", "tokens": ["H\u00e4tt'", "ich", "um", "Gold", "und", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch hingegeben? wahrlich nein!", "tokens": ["Euch", "hin\u00b7ge\u00b7ge\u00b7ben", "?", "wahr\u00b7lich", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun statt Gold und Schatzgewichts", "tokens": ["Und", "nun", "statt", "Gold", "und", "Schatz\u00b7ge\u00b7wichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df ich euch geben hin um Nichts?", "tokens": ["Mu\u00df", "ich", "euch", "ge\u00b7ben", "hin", "um", "Nichts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich geb' euch ja um Nichts nicht hin,", "tokens": ["Ich", "geb'", "euch", "ja", "um", "Nichts", "nicht", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIS", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um einen reichlichen Gewinn,", "tokens": ["Um", "ei\u00b7nen", "reich\u00b7li\u00b7chen", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um Thr\u00e4nen und um Herzeleid,", "tokens": ["Um", "Thr\u00e4\u00b7nen", "und", "um", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um ein so schweres Glanzgeschmeid,", "tokens": ["Um", "ein", "so", "schwe\u00b7res", "Glanz\u00b7ge\u00b7schmeid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Womit ihr aufgewogen seid,", "tokens": ["Wo\u00b7mit", "ihr", "auf\u00b7ge\u00b7wo\u00b7gen", "seid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das hat mir zugewogen", "tokens": ["Das", "hat", "mir", "zu\u00b7ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Wag' am Himmelsbogen.", "tokens": ["Die", "Wag'", "am", "Him\u00b7mels\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}