{"dta.poem.18148": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Herrlichkeit und Verg\u00e4nglichkeit  \n der Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Theander ging einst hin zum Herrn", "tokens": ["The\u00b7an\u00b7der", "ging", "einst", "hin", "zum", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Gartenlieb,", "tokens": ["von", "Gar\u00b7ten\u00b7lieb", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Traf ihn im Garten an, wo er auch", "tokens": ["Traf", "ihn", "im", "Gar\u00b7ten", "an", ",", "wo", "er", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "bey ihm blieb;", "tokens": ["bey", "ihm", "blieb", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wo sie mit gleicher Lust an Blumen", "tokens": ["Wo", "sie", "mit", "glei\u00b7cher", "Lust", "an", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "sich erg\u00f6tzten,", "tokens": ["sich", "er\u00b7g\u00f6tz\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und ihrer Arten Pracht, nach ihrer Meinung sch\u00e4tzten.", "tokens": ["Und", "ih\u00b7rer", "Ar\u00b7ten", "Pracht", ",", "nach", "ih\u00b7rer", "Mei\u00b7nung", "sch\u00e4tz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die bunt vermischte Meng, der Farben mannigfalt,", "tokens": ["Die", "bunt", "ver\u00b7mischte", "Meng", ",", "der", "Far\u00b7ben", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NE", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Verdoppelte das Sch\u00f6n von jeglicher Gestalt,", "tokens": ["Ver\u00b7dop\u00b7pel\u00b7te", "das", "Sch\u00f6n", "von", "jeg\u00b7li\u00b7cher", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil sich die Mischungen so unterschiedner Arten,", "tokens": ["Weil", "sich", "die", "Misc\u00b7hun\u00b7gen", "so", "un\u00b7ter\u00b7schied\u00b7ner", "Ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nach ihrer Augenschein, recht woll zusammen paar-", "tokens": ["Nach", "ih\u00b7rer", "Au\u00b7gen\u00b7schein", ",", "recht", "woll", "zu\u00b7sam\u00b7men", "paa\u00b7r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ten.", "tokens": ["ten", "."], "token_info": ["word", "punct"], "pos": ["FM", "$."], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Die Beeten waren roth, gr\u00fcn, gelb und weis ver-", "tokens": ["Die", "Bee\u00b7ten", "wa\u00b7ren", "roth", ",", "gr\u00fcn", ",", "gelb", "und", "weis", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "PTKVZ", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "mahlt", "tokens": ["mahlt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Nachdem der Sonnenschein, der auf die Fl\u00e4che", "tokens": ["Nach\u00b7dem", "der", "Son\u00b7nen\u00b7schein", ",", "der", "auf", "die", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "strahlt,", "tokens": ["strahlt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Und Licht und Farbe giebt, durch die gebrochnen Blicke,", "tokens": ["Und", "Licht", "und", "Far\u00b7be", "giebt", ",", "durch", "die", "ge\u00b7broch\u00b7nen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sich von der Bl\u00e4tter Rand ins Auge warf zur\u00fccke;", "tokens": ["Sich", "von", "der", "Bl\u00e4t\u00b7ter", "Rand", "ins", "Au\u00b7ge", "warf", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie wunderbahr scheint das, wie lieblich und wie", "tokens": ["Wie", "wun\u00b7der\u00b7bahr", "scheint", "das", ",", "wie", "lieb\u00b7lich", "und", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PDS", "$,", "PWAV", "ADJD", "KON", "PWAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.20": {"text": "sch\u00f6n", "tokens": ["sch\u00f6n"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Sprach Herr von Gartenlieb, so viele Farben sehn", "tokens": ["Sprach", "Herr", "von", "Gar\u00b7ten\u00b7lieb", ",", "so", "vie\u00b7le", "Far\u00b7ben", "sehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN", "$,", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die ein bemerkend Aug bald hie bald dahin lenken,", "tokens": ["Die", "ein", "be\u00b7mer\u00b7kend", "Aug", "bald", "hie", "bald", "da\u00b7hin", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "ADV", "ADV", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und durch Verwunderung in die Entz\u00fckkung senken.", "tokens": ["Und", "durch", "Ver\u00b7wun\u00b7de\u00b7rung", "in", "die", "Ent\u00b7z\u00fck\u00b7kung", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich habe dieses offt, Theander schon bedacht,", "tokens": ["Ich", "ha\u00b7be", "die\u00b7ses", "offt", ",", "The\u00b7an\u00b7der", "schon", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "$,", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Woher entstehet das, das solche Farben macht?", "tokens": ["Wo\u00b7her", "ent\u00b7ste\u00b7het", "das", ",", "das", "sol\u00b7che", "Far\u00b7ben", "macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich habe sonst gemeint, da\u00df in der Blumen Saffte,", "tokens": ["Ich", "ha\u00b7be", "sonst", "ge\u00b7meint", ",", "da\u00df", "in", "der", "Blu\u00b7men", "Saff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Farbe eigentlich, wie ihre Nahrung haffte", "tokens": ["Die", "Far\u00b7be", "ei\u00b7gent\u00b7lich", ",", "wie", "ih\u00b7re", "Nah\u00b7rung", "haff\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "J\u00fcngst aber l\u00f6sete ich eine g\u00e4ntzlich auf,", "tokens": ["J\u00fcngst", "a\u00b7ber", "l\u00f6\u00b7se\u00b7te", "ich", "ei\u00b7ne", "g\u00e4ntz\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Durchforschte auch mit Fleis derselben R\u00f6hr und", "tokens": ["Durch\u00b7forschte", "auch", "mit", "Fleis", "der\u00b7sel\u00b7ben", "R\u00f6hr", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PDAT", "NN", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Knauf,", "tokens": ["Knauf", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Das Keimchen war noch weis, des Stengels Feuch-", "tokens": ["Das", "Keim\u00b7chen", "war", "noch", "weis", ",", "des", "Sten\u00b7gels", "Feuch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "tigkeiten,", "tokens": ["tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Die durch die Wurtzeln sich, bis zu der Spitze leiten,", "tokens": ["Die", "durch", "die", "Wurt\u00b7zeln", "sich", ",", "bis", "zu", "der", "Spit\u00b7ze", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.34": {"text": "Die waren fast wie Milch, mit Wasser untermischt,", "tokens": ["Die", "wa\u00b7ren", "fast", "wie", "Milch", ",", "mit", "Was\u00b7ser", "un\u00b7ter\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "KOKOM", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als ich sie ausgeprest und auf die Hand gewischt,", "tokens": ["Als", "ich", "sie", "aus\u00b7ge\u00b7prest", "und", "auf", "die", "Hand", "ge\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Es fand sich sichtbahrlich nichts von dergleichen Far-", "tokens": ["Es", "fand", "sich", "sicht\u00b7bahr\u00b7lich", "nichts", "von", "derg\u00b7lei\u00b7chen", "Fa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PRF", "PIS", "APPR", "PIS", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "ben,", "tokens": ["ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.38": {"text": "Die Blumen gleicher Art in kurtzer Zeit erwarben.", "tokens": ["Die", "Blu\u00b7men", "glei\u00b7cher", "Art", "in", "kurt\u00b7zer", "Zeit", "er\u00b7war\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Theander fing drauf an und zeigte wie das Licht,", "tokens": ["The\u00b7an\u00b7der", "fing", "drauf", "an", "und", "zeig\u00b7te", "wie", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "PTKVZ", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Auf dunkle Fl\u00e4chen f\u00e4llt und an denselben bricht.", "tokens": ["Auf", "dunk\u00b7le", "Fl\u00e4\u00b7chen", "f\u00e4llt", "und", "an", "den\u00b7sel\u00b7ben", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und in dem Augen sich mit solchen Farben stellet,", "tokens": ["Und", "in", "dem", "Au\u00b7gen", "sich", "mit", "sol\u00b7chen", "Far\u00b7ben", "stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Nachdem der K\u00f6rper ist davon es r\u00fcckwerts prellet.", "tokens": ["Nach\u00b7dem", "der", "K\u00f6r\u00b7per", "ist", "da\u00b7von", "es", "r\u00fcck\u00b7werts", "prel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "PAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So unbegreiflich dies, zuerst den Sinnen scheint;", "tokens": ["So", "un\u00b7be\u00b7greif\u00b7lich", "dies", ",", "zu\u00b7erst", "den", "Sin\u00b7nen", "scheint", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDS", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So schwerlich wird es doch durch die Vernunft ver-", "tokens": ["So", "schwer\u00b7lich", "wird", "es", "doch", "durch", "die", "Ver\u00b7nunft", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "neint:", "tokens": ["neint", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.46": {"text": "Weil die Erfahrung selbst sich eine Zeugin nennet,", "tokens": ["Weil", "die", "Er\u00b7fah\u00b7rung", "selbst", "sich", "ei\u00b7ne", "Zeu\u00b7gin", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Da\u00df wen kein Licht mehr scheint, man keine Farben", "tokens": ["Da\u00df", "wen", "kein", "Licht", "mehr", "scheint", ",", "man", "kei\u00b7ne", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIAT", "NN", "ADV", "VVFIN", "$,", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "kennet.", "tokens": ["ken\u00b7net", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.49": {"text": "Es ist doch wundervoll, da\u00df jedes Blumen Blat,", "tokens": ["Es", "ist", "doch", "wun\u00b7der\u00b7voll", ",", "da\u00df", "je\u00b7des", "Blu\u00b7men", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Nach unsren Augenschein auch solche Farben hat,", "tokens": ["Nach", "un\u00b7sren", "Au\u00b7gen\u00b7schein", "auch", "sol\u00b7che", "Far\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VAFIN", "