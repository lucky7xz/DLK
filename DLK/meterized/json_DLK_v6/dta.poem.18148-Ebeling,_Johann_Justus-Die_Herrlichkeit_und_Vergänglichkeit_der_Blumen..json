, "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.130": {"text": "Mit unsrer Horrligkeit, von euren Schauplatz\nfliehn.", "tokens": ["Mit", "uns\u00b7rer", "Horr\u00b7lig\u00b7keit", ",", "von", "eu\u00b7ren", "Schau\u00b7platz", "fliehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.131": {"text": "Damit ihr, wenn der Sin durch unsern\nGlantz erquikket,", "tokens": ["Da\u00b7mit", "ihr", ",", "wenn", "der", "Sin", "durch", "un\u00b7sern", "Glantz", "er\u00b7quik\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.132": {"text": "Jhr eur Gem\u00fcthe auch, zum Sch\u00f6pfer selbst\nentz\u00fckker.", "tokens": ["Ihr", "eur", "Ge\u00b7m\u00fc\u00b7the", "auch", ",", "zum", "Sch\u00f6p\u00b7fer", "selbst", "ent\u00b7z\u00fck\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "$,", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}