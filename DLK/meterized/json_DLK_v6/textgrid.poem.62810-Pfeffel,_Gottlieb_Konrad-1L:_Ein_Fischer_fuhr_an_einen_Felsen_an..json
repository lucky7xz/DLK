{"textgrid.poem.62810": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Fischer fuhr an einen Felsen an.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fischer fuhr an einen Felsen an.", "tokens": ["Ein", "Fi\u00b7scher", "fuhr", "an", "ei\u00b7nen", "Fel\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einmal barst sein kleiner Kahn", "tokens": ["Auf", "ein\u00b7mal", "barst", "sein", "klei\u00b7ner", "Kahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und splitterte, wie spr\u00f6des Glas, in St\u00fccken.", "tokens": ["Und", "split\u00b7ter\u00b7te", ",", "wie", "spr\u00f6\u00b7des", "Glas", ",", "in", "St\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er war dem b\u00e4ngsten Tode nah,", "tokens": ["Er", "war", "dem", "b\u00e4ngs\u00b7ten", "To\u00b7de", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als ihn ein frommer Delphin sah;", "tokens": ["Als", "ihn", "ein", "from\u00b7mer", "Del\u00b7phin", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er schwamm herbey, er lud ihn auf den R\u00fccken", "tokens": ["Er", "schwamm", "her\u00b7bey", ",", "er", "lud", "ihn", "auf", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und trug ihn gl\u00fccklich an den Strand.", "tokens": ["Und", "trug", "ihn", "gl\u00fcck\u00b7lich", "an", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schnell zog der Fischer ihn ans Land", "tokens": ["Schnell", "zog", "der", "Fi\u00b7scher", "ihn", "ans", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sprach mit gnadenreichen Blicken;", "tokens": ["Und", "sprach", "mit", "gna\u00b7den\u00b7rei\u00b7chen", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dein Schicksal ist in meiner Hand;", "tokens": ["Dein", "Schick\u00b7sal", "ist", "in", "mei\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch zum Beweis, da\u00df auch wir Menschen edel denken,", "tokens": ["Doch", "zum", "Be\u00b7weis", ",", "da\u00df", "auch", "wir", "Men\u00b7schen", "e\u00b7del", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ADV", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So will ich dir das Leben schenken.", "tokens": ["So", "will", "ich", "dir", "das", "Le\u00b7ben", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}