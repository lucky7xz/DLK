{"textgrid.poem.26385": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[es war im Herbst, man machte Wein]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war im Herbst, man machte Wein,", "tokens": ["Es", "war", "im", "Herbst", ",", "man", "mach\u00b7te", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PIS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6rt' ich nachts mein Kissen schrein,", "tokens": ["Da", "h\u00f6rt'", "ich", "nachts", "mein", "Kis\u00b7sen", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau K\u00f6nigin schlief still bei mir,", "tokens": ["Frau", "K\u00f6\u00b7ni\u00b7gin", "schlief", "still", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dacht: \u00bbDas Mohrle schreit nach dir.\u00ab", "tokens": ["Ich", "dacht", ":", "\u00bb", "Das", "Mohr\u00b7le", "schreit", "nach", "dir", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Abends ging ich den Weinbergweg,", "tokens": ["A\u00b7bends", "ging", "ich", "den", "Wein\u00b7berg\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den ich so gern zu gehen pfleg'.", "tokens": ["Den", "ich", "so", "gern", "zu", "ge\u00b7hen", "pfleg'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Stadt lag unten voll Gelichter,", "tokens": ["Die", "Stadt", "lag", "un\u00b7ten", "voll", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4user weinselig wie Gesichter,", "tokens": ["H\u00e4u\u00b7ser", "wein\u00b7se\u00b7lig", "wie", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Wie Phantasie gl\u00fchte der Flu\u00df,", "tokens": ["Wie", "Phan\u00b7ta\u00b7sie", "gl\u00fch\u00b7te", "der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wer darauf geht, ertrinken mu\u00df.", "tokens": ["Wer", "da\u00b7rauf", "geht", ",", "er\u00b7trin\u00b7ken", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Froh bei den Winzern traf ich sie,", "tokens": ["Froh", "bei", "den", "Win\u00b7zern", "traf", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und still sprach ich: Jetzt oder nie!", "tokens": ["Und", "still", "sprach", "ich", ":", "Jetzt", "o\u00b7der", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Mohrle lie\u00df aus allen Taschen", "tokens": ["Das", "Mohr\u00b7le", "lie\u00df", "aus", "al\u00b7len", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Lachen los wie aus Weinflaschen,", "tokens": ["Ihr", "La\u00b7chen", "los", "wie", "aus", "Wein\u00b7fla\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Wir lachten in den Weinberglauben,", "tokens": ["Wir", "lach\u00b7ten", "in", "den", "Wein\u00b7berg\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herzen g\u00e4rten wie die Trauben,", "tokens": ["Die", "Her\u00b7zen", "g\u00e4r\u00b7ten", "wie", "die", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Man lie\u00df Raketen gl\u00fchend steigen,", "tokens": ["Man", "lie\u00df", "Ra\u00b7ke\u00b7ten", "gl\u00fc\u00b7hend", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als m\u00fc\u00dft' man die Gef\u00fchle zeigen.", "tokens": ["Als", "m\u00fc\u00dft'", "man", "die", "Ge\u00b7f\u00fch\u00b7le", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Most nimmt manchen Gott im Sturm,", "tokens": ["Der", "Most", "nimmt", "man\u00b7chen", "Gott", "im", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel \u00f6fters noch den Erdenwurm,", "tokens": ["Viel", "\u00f6f\u00b7ters", "noch", "den", "Er\u00b7den\u00b7wurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wir knackten N\u00fcsse, tranken Most,", "tokens": ["Wir", "knack\u00b7ten", "N\u00fcs\u00b7se", ",", "tran\u00b7ken", "Most", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herz stie\u00df ans Herz und sagte: Prost!", "tokens": ["Herz", "stie\u00df", "ans", "Herz", "und", "sag\u00b7te", ":", "Prost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Als mit den Winzern heim wir zogen,", "tokens": ["Als", "mit", "den", "Win\u00b7zern", "heim", "wir", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist von der Hand mein Ring geflogen.", "tokens": ["Ist", "von", "der", "Hand", "mein", "Ring", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ein steinern Heil'genbild dort stand,", "tokens": ["Ein", "stei\u00b7nern", "Heil'\u00b7gen\u00b7bild", "dort", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da flog der Ehring von der Hand.", "tokens": ["Da", "flog", "der", "Eh\u00b7ring", "von", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.14": {"line.1": {"text": "Wei\u00df schien das Antlitz der Madonnen,", "tokens": ["Wei\u00df", "schien", "das", "Ant\u00b7litz", "der", "Ma\u00b7don\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Garten, wo nur Nonnen wohnen.", "tokens": ["Vom", "Gar\u00b7ten", ",", "wo", "nur", "Non\u00b7nen", "woh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Als t\u00e4t sie mir 'n vom Finger ziehn,", "tokens": ["Als", "t\u00e4t", "sie", "mir", "'n", "vom", "Fin\u00b7ger", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PPER", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So flog der Ring zum Garten hin.", "tokens": ["So", "flog", "der", "Ring", "zum", "Gar\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Den Liebestanz tat ich just lehren,", "tokens": ["Den", "Lie\u00b7be\u00b7stanz", "tat", "ich", "just", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel wollt' es mir verwehren,", "tokens": ["Der", "Him\u00b7mel", "wollt'", "es", "mir", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Denn pl\u00f6tzlich Regen runterrannte", "tokens": ["Denn", "pl\u00f6tz\u00b7lich", "Re\u00b7gen", "run\u00b7ter\u00b7rann\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und man sein Parapluie aufspannte.", "tokens": ["Und", "man", "sein", "Pa\u00b7ra\u00b7plu\u00b7ie", "auf\u00b7spann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-++-", "measure": "anapaest.di.plus"}}, "stanza.18": {"line.1": {"text": "Doch wir tanzten im Regen fort,", "tokens": ["Doch", "wir", "tanz\u00b7ten", "im", "Re\u00b7gen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die F\u00fc\u00dfe hatten froh das Wort,", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "hat\u00b7ten", "froh", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Das Herz schlug Takt hinter der Weste,", "tokens": ["Das", "Herz", "schlug", "Takt", "hin\u00b7ter", "der", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich hob den Arm mit sch\u00f6nster Geste.", "tokens": ["Ich", "hob", "den", "Arm", "mit", "sch\u00f6ns\u00b7ter", "Ges\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Da war's, da\u00df mir der Ring fortflog,", "tokens": ["Da", "wa\u00b7r's", ",", "da\u00df", "mir", "der", "Ring", "fort\u00b7flog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Er, der nicht gern mit mir betrog.", "tokens": ["Er", ",", "der", "nicht", "gern", "mit", "mir", "be\u00b7trog", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PTKNEG", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Ich suchte ihn in allen Falten,", "tokens": ["Ich", "such\u00b7te", "ihn", "in", "al\u00b7len", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nonnen haben ihn behalten.", "tokens": ["Die", "Non\u00b7nen", "ha\u00b7ben", "ihn", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Vielleicht, wenn er im Garten liegt", "tokens": ["Viel\u00b7leicht", ",", "wenn", "er", "im", "Gar\u00b7ten", "liegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kleine Eheringe kriegt,", "tokens": ["Und", "klei\u00b7ne", "E\u00b7he\u00b7rin\u00b7ge", "kriegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Verliebt werden die Nonnen werden", "tokens": ["Ver\u00b7liebt", "wer\u00b7den", "die", "Non\u00b7nen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Kl\u00f6ster schwinden von der Erden.", "tokens": ["Und", "Kl\u00f6s\u00b7ter", "schwin\u00b7den", "von", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "So scherzte ich dar\u00fcber hin,", "tokens": ["So", "scherz\u00b7te", "ich", "da\u00b7r\u00fc\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil's Mohrle mich zu lieben schien.", "tokens": ["Weil's", "Mohr\u00b7le", "mich", "zu", "lie\u00b7ben", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Zum Weinberg f\u00fchrten tausend Stufen,", "tokens": ["Zum", "Wein\u00b7berg", "f\u00fchr\u00b7ten", "tau\u00b7send", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Aufstieg manchem M\u00fch sie schufen,", "tokens": ["Beim", "Auf\u00b7stieg", "man\u00b7chem", "M\u00fch", "sie", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.26": {"line.1": {"text": "Ich hab' es umgekehrt gemacht,", "tokens": ["Ich", "hab'", "es", "um\u00b7ge\u00b7kehrt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abstieg hat mir M\u00fch' gebracht,", "tokens": ["Der", "Ab\u00b7stieg", "hat", "mir", "M\u00fch'", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-