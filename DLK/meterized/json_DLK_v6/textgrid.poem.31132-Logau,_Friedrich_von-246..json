{"textgrid.poem.31132": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "246.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einen guten Trunck vermag, hat er denn einen Ruhm?", "tokens": ["Wer", "ei\u00b7nen", "gu\u00b7ten", "Trunck", "ver\u00b7mag", ",", "hat", "er", "denn", "ei\u00b7nen", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ja, wann er trinckt, da\u00df doch Vernunfft behelt das Meisterthum.", "tokens": ["Ja", ",", "wann", "er", "trinckt", ",", "da\u00df", "doch", "Ver\u00b7nunfft", "be\u00b7helt", "das", "Meis\u00b7ter\u00b7thum", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "ADV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Bey Hofe n\u00fctzt ein solcher Kopff, der also trincken kan,", "tokens": ["Bey", "Ho\u00b7fe", "n\u00fctzt", "ein", "sol\u00b7cher", "Kopff", ",", "der", "al\u00b7so", "trin\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "PIAT", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Da\u00df er entdeckt sich selbsten nicht, vielmehr den fremden Mann.", "tokens": ["Da\u00df", "er", "ent\u00b7deckt", "sich", "selbs\u00b7ten", "nicht", ",", "viel\u00b7mehr", "den", "frem\u00b7den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-++--+-+", "measure": "iambic.septa.relaxed"}}}}}