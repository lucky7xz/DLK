{"dta.poem.11763": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum der Vogel steht im Schlaf auf Einem Bein?", "tokens": ["Wa\u00b7rum", "der", "Vo\u00b7gel", "steht", "im", "Schlaf", "auf", "Ei\u00b7nem", "Bein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihm die Schlange k\u00f6nn' umschlingen eins allein.", "tokens": ["Da\u00df", "ihm", "die", "Schlan\u00b7ge", "k\u00f6nn'", "um\u00b7schlin\u00b7gen", "eins", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie schlingt ums Eine sich; doch mit dem andern Fange,", "tokens": ["Sie", "schlingt", "ums", "Ei\u00b7ne", "sich", ";", "doch", "mit", "dem", "an\u00b7dern", "Fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "PIS", "PRF", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit dem Schnabel dann, entringt er sich der Schlange.", "tokens": ["Und", "mit", "dem", "Schna\u00b7bel", "dann", ",", "ent\u00b7ringt", "er", "sich", "der", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum der Vogel schl\u00e4ft, den Kopf in Fl\u00fcgeln schmiegend?", "tokens": ["Wa\u00b7rum", "der", "Vo\u00b7gel", "schl\u00e4ft", ",", "den", "Kopf", "in", "Fl\u00fc\u00b7geln", "schmie\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df den die Eule nicht abrei\u00dfe, n\u00e4chtlich fliegend.", "tokens": ["Da\u00df", "den", "die", "Eu\u00b7le", "nicht", "ab\u00b7rei\u00b7\u00dfe", ",", "n\u00e4cht\u00b7lich", "flie\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hinfahrend \u00fcber ihn, erwischt sie einen Schopf;", "tokens": ["Hin\u00b7fah\u00b7rend", "\u00fc\u00b7ber", "ihn", ",", "er\u00b7wischt", "sie", "ei\u00b7nen", "Schopf", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den l\u00e4\u00dft er ihr und fliegt davon mit seinem Kopf.", "tokens": ["Den", "l\u00e4\u00dft", "er", "ihr", "und", "fliegt", "da\u00b7von", "mit", "sei\u00b7nem", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}