{"dta.poem.8048": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Ring .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging an einem Morgen", "tokens": ["Es", "ging", "an", "ei\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ritter \u00fcber die Au.", "tokens": ["Ein", "Rit\u00b7ter", "\u00fc\u00b7ber", "die", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er dacht\u2019 in bangen Sorgen", "tokens": ["Er", "dacht'", "in", "ban\u00b7gen", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An die allersch\u00f6nste Frau.", "tokens": ["An", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201emein werthes Ringlein golden!", "tokens": ["\u201e", "mein", "wert\u00b7hes", "Rin\u00b7glein", "gol\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verk\u00fcnde du mir frei,", "tokens": ["Ver\u00b7k\u00fcn\u00b7de", "du", "mir", "frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Pfand von meiner Holden,", "tokens": ["Du", "Pfand", "von", "mei\u00b7ner", "Hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie steht es mit ihrer Treu?\u201c", "tokens": ["Wie", "steht", "es", "mit", "ih\u00b7rer", "Treu", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie er\u2019s betrachten wollte,", "tokens": ["Wie", "er's", "be\u00b7trach\u00b7ten", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Finger es ihm sprang,", "tokens": ["Vom", "Fin\u00b7ger", "es", "ihm", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Ringlein h\u00fcpft\u2019 und rollte", "tokens": ["Das", "Rin\u00b7glein", "h\u00fcpft'", "und", "roll\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Wiesenrain entlang.", "tokens": ["Den", "Wie\u00b7sen\u00b7rain", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er will mit schnellen H\u00e4nden", "tokens": ["Er", "will", "mit", "schnel\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es haschen auf der Au,", "tokens": ["Es", "ha\u00b7schen", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch goldne Blumen ihn blenden", "tokens": ["Doch", "gold\u00b7ne", "Blu\u00b7men", "ihn", "blen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Gr\u00e4ser, betropft von Thau.", "tokens": ["Und", "Gr\u00e4\u00b7ser", ",", "be\u00b7tropft", "von", "Thau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Falk\u2019 es gleich erlauschte,", "tokens": ["Ein", "Fal\u00b7k'", "es", "gleich", "er\u00b7lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der auf der Linde sa\u00df,", "tokens": ["Der", "auf", "der", "Lin\u00b7de", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Wipfel er niederrauschte,", "tokens": ["Vom", "Wip\u00b7fel", "er", "nie\u00b7der\u00b7rauschte", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er holt\u2019 es aus dem Gras.", "tokens": ["Er", "holt'", "es", "aus", "dem", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit m\u00e4chtigem Gefieder", "tokens": ["Mit", "m\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er in die Luft sich schwang.", "tokens": ["Er", "in", "die", "Luft", "sich", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wollten seine Br\u00fcder", "tokens": ["Da", "woll\u00b7ten", "sei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm rauben den goldnen Fang.", "tokens": ["Ihm", "rau\u00b7ben", "den", "gold\u00b7nen", "Fang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Doch keiner gewann\u2019s von allen,", "tokens": ["Doch", "kei\u00b7ner", "ge\u00b7wann's", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "APPR", "PIAT", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Ringlein fiel aus der H\u00f6h\u2019.", "tokens": ["Das", "Rin\u00b7glein", "fiel", "aus", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Ritter sah es fallen", "tokens": ["Der", "Rit\u00b7ter", "sah", "es", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einen tiefen See.", "tokens": ["In", "ei\u00b7nen", "tie\u00b7fen", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Fischlein h\u00fcpften munter,", "tokens": ["Die", "Fisc\u00b7hlein", "h\u00fcpf\u00b7ten", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu haschen den goldnen Tand;", "tokens": ["Zu", "ha\u00b7schen", "den", "gold\u00b7nen", "Tand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Ringlein sank hinunter,", "tokens": ["Das", "Rin\u00b7glein", "sank", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis es den Blicken schwand.", "tokens": ["Bis", "es", "den", "Bli\u00b7cken", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201eo Ringlein! auf den Triften,", "tokens": ["\u201e", "o", "Rin\u00b7glein", "!", "auf", "den", "Trif\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da \u00e4ffen dich Gras und Blum\u2019;", "tokens": ["Da", "\u00e4f\u00b7fen", "dich", "Gras", "und", "Blum'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Ringlein! in den L\u00fcften,", "tokens": ["O", "Rin\u00b7glein", "!", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da tragen die V\u00f6gel dich um.", "tokens": ["Da", "tra\u00b7gen", "die", "V\u00f6\u00b7gel", "dich", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "O Ringlein! in Wassers Grunde,", "tokens": ["O", "Rin\u00b7glein", "!", "in", "Was\u00b7sers", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da haschen die Fische dich frei.", "tokens": ["Da", "ha\u00b7schen", "die", "Fi\u00b7sche", "dich", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Ringlein! ist das die Kunde,", "tokens": ["Mein", "Rin\u00b7glein", "!", "ist", "das", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "ART", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Kunde von Liebchens Treu?\u201c", "tokens": ["Die", "Kun\u00b7de", "von", "Lieb\u00b7chens", "Treu", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}