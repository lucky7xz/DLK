{"dta.poem.11513": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Jenes war zum lezten Male,", "tokens": ["Je\u00b7nes", "war", "zum", "lez\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit dir ging, o Cl\u00e4rchen!", "tokens": ["Da\u00df", "ich", "mit", "dir", "ging", ",", "o", "Cl\u00e4r\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "FM", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, das war das lezte Mal,", "tokens": ["Ja", ",", "das", "war", "das", "lez\u00b7te", "Mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir uns wie Kinder freuten.", "tokens": ["Da\u00df", "wir", "uns", "wie", "Kin\u00b7der", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als wir durch die sonnenhellen,", "tokens": ["Als", "wir", "durch", "die", "son\u00b7nen\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regnerischen Stra\u00dfen liefen,", "tokens": ["Reg\u00b7ne\u00b7ri\u00b7schen", "Stra\u00b7\u00dfen", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterm seidnen Schirme eilend,", "tokens": ["Un\u00b7term", "seid\u00b7nen", "Schir\u00b7me", "ei\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide heimlich eingeschlossen,", "tokens": ["Bei\u00b7de", "heim\u00b7lich", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie in einem Feenst\u00fcbchen,", "tokens": ["Wie", "in", "ei\u00b7nem", "Feen\u00b7st\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.6": {"text": "Endlich einmal Arm in Arme!", "tokens": ["End\u00b7lich", "ein\u00b7mal", "Arm", "in", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenig wagten wir zu reden,", "tokens": ["We\u00b7nig", "wag\u00b7ten", "wir", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn das Herz schlug zu gewaltig,", "tokens": ["Denn", "das", "Herz", "schlug", "zu", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Beide merkten wir es schweigend,", "tokens": ["Bei\u00b7de", "merk\u00b7ten", "wir", "es", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Jedes schob im Stillen", "tokens": ["Und", "ein", "Je\u00b7des", "schob", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Des Gesichtes gl\u00fch'nde R\u00f6the", "tokens": ["Des", "Ge\u00b7sich\u00b7tes", "gl\u00fch'n\u00b7de", "R\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Widerschein des Schirmes.", "tokens": ["Auf", "den", "Wi\u00b7der\u00b7schein", "des", "Schir\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, ein Engel warst du da!", "tokens": ["Ach", ",", "ein", "En\u00b7gel", "warst", "du", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie du auf den Boden immer", "tokens": ["Wie", "du", "auf", "den", "Bo\u00b7den", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Blicktest, und die blonden Locken", "tokens": ["Blick\u00b7test", ",", "und", "die", "blon\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Um den hellen Nacken fielen.", "tokens": ["Um", "den", "hel\u00b7len", "Na\u00b7cken", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ejezt ist wohl ein Regenbogen", "tokens": ["\u201e", "jezt", "ist", "wohl", "ein", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Himmel,\u201c sagt' ich einmal:", "tokens": ["An", "dem", "Him\u00b7mel", ",", "\u201c", "sagt'", "ich", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Dann in meinem frohen Muthe", "tokens": ["Dann", "in", "mei\u00b7nem", "fro\u00b7hen", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprach ich weiter diese Worte:", "tokens": ["Sprach", "ich", "wei\u00b7ter", "die\u00b7se", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201ek\u00e4m' auch keiner mehr an Himmel,", "tokens": ["\u201e", "k\u00e4m'", "auch", "kei\u00b7ner", "mehr", "an", "Him\u00b7mel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4r' es gar nicht zu verwundern,", "tokens": ["W\u00e4r'", "es", "gar", "nicht", "zu", "ver\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn die Leute ziehn ja selber", "tokens": ["Denn", "die", "Leu\u00b7te", "ziehn", "ja", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine bunten Bogenstreifen", "tokens": ["Sei\u00b7ne", "bun\u00b7ten", "Bo\u00b7gen\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zu sich nieder auf die Gassen.", "tokens": ["Zu", "sich", "nie\u00b7der", "auf", "die", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sieh nur, wie sie sich beeilen!", "tokens": ["Sieh", "nur", ",", "wie", "sie", "sich", "be\u00b7ei\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jeder mit dem Regendache", "tokens": ["Je\u00b7der", "mit", "dem", "Re\u00b7gen\u00b7da\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fchret einen andern Farben-", "tokens": ["F\u00fch\u00b7ret", "ei\u00b7nen", "an\u00b7dern", "Fa\u00b7rben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Bogen \u00fcber seinem Haupte,", "tokens": ["Bo\u00b7gen", "\u00fc\u00b7ber", "sei\u00b7nem", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Jeder springt mit seinem Raube,", "tokens": ["Je\u00b7der", "springt", "mit", "sei\u00b7nem", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Blaue, rothe, violete, \u2014", "tokens": ["Blau\u00b7e", ",", "ro\u00b7the", ",", "vi\u00b7o\u00b7le\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alles nehmen sie mit fort.\u201c", "tokens": ["Al\u00b7les", "neh\u00b7men", "sie", "mit", "fort", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und du l\u00e4cheltest und bogest", "tokens": ["Und", "du", "l\u00e4\u00b7chel\u00b7test", "und", "bo\u00b7gest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit mir um die lezte Ecke.", "tokens": ["Mit", "mir", "um", "die", "lez\u00b7te", "E\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ich bat dich um ein R\u00f6slein,", "tokens": ["Und", "ich", "bat", "dich", "um", "ein", "R\u00f6s\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das du an der Brust getragen,", "tokens": ["Das", "du", "an", "der", "Brust", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du reichtest mir's im Gehen", "tokens": ["Und", "du", "reich\u00b7test", "mir's", "im", "Ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnelle hin, das s\u00fc\u00dfe R\u00f6slein;", "tokens": ["Schnel\u00b7le", "hin", ",", "das", "s\u00fc\u00b7\u00dfe", "R\u00f6s\u00b7lein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zitternd hob ich's an die Lippen,", "tokens": ["Zit\u00b7ternd", "hob", "ich's", "an", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fc\u00dft' es br\u00fcnstig zwei- und dreimal,", "tokens": ["K\u00fc\u00dft'", "es", "br\u00fcns\u00b7tig", "zwei", "und", "drei\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "TRUNC", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Niemand konnte dessen spotten,", "tokens": ["Nie\u00b7mand", "konn\u00b7te", "des\u00b7sen", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keine Seele hat's gesehen,", "tokens": ["Kei\u00b7ne", "See\u00b7le", "hat's", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und du selber sahst es nicht.", "tokens": ["Und", "du", "sel\u00b7ber", "sahst", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "An dem fremden Haus, wohin", "tokens": ["An", "dem", "frem\u00b7den", "Haus", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich dich zu begleiten hatte,", "tokens": ["Ich", "dich", "zu", "be\u00b7glei\u00b7ten", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Standen wir nun, wei\u00df'st, ich dr\u00fcckte", "tokens": ["Stan\u00b7den", "wir", "nun", ",", "wei\u00df'st", ",", "ich", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir die Hand und \u2014", "tokens": ["Dir", "die", "Hand", "und"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Dieses war zum lezten Male,", "tokens": ["Die\u00b7ses", "war", "zum", "lez\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit dir ging, o Cl\u00e4rchen!", "tokens": ["Da\u00df", "ich", "mit", "dir", "ging", ",", "o", "Cl\u00e4r\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "FM", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, das war das lezte Mal,", "tokens": ["Ja", ",", "das", "war", "das", "lez\u00b7te", "Mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir uns wie Kinder freuten.", "tokens": ["Da\u00df", "wir", "uns", "wie", "Kin\u00b7der", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}