{"textgrid.poem.66230": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Thraso", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Unterthanen Last erschweren,", "tokens": ["Der", "Un\u00b7ter\u00b7tha\u00b7nen", "Last", "er\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um seines F\u00fcrsten Schatz zu mehren;", "tokens": ["Um", "sei\u00b7nes", "F\u00fcrs\u00b7ten", "Schatz", "zu", "meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An keinen Jammer sich zu kehren,", "tokens": ["An", "kei\u00b7nen", "Jam\u00b7mer", "sich", "zu", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und St\u00e4dt' und L\u00e4nder zu verheeren:", "tokens": ["Und", "St\u00e4dt'", "und", "L\u00e4n\u00b7der", "zu", "ver\u00b7hee\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df ist die hohe Wissenschaft,", "tokens": ["Die\u00df", "ist", "die", "ho\u00b7he", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Thraso Ehr' und Reichthum schafft.", "tokens": ["Die", "Thra\u00b7so", "Ehr'", "und", "Reicht\u00b7hum", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er hat des Tigers Grausamkeit,", "tokens": ["Er", "hat", "des", "Ti\u00b7gers", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Wolfes Raubbegier, die List", "tokens": ["Des", "Wol\u00b7fes", "Raub\u00b7be\u00b7gier", ",", "die", "List"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des Fuchsen; eines Hundes Neid,", "tokens": ["Des", "Fuch\u00b7sen", ";", "ei\u00b7nes", "Hun\u00b7des", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht seine Treu und Tapferkeit; \u2013", "tokens": ["Nicht", "sei\u00b7ne", "Treu", "und", "Tap\u00b7fer\u00b7keit", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und keines Menschen Herz \u2013 er ist", "tokens": ["Und", "kei\u00b7nes", "Men\u00b7schen", "Herz", "\u2013", "er", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein trefflicher Kameralist.", "tokens": ["Ein", "treff\u00b7li\u00b7cher", "Ka\u00b7me\u00b7ra\u00b7list", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}