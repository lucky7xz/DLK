{"textgrid.poem.55348": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndlich", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall, sie war entfernt,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "sie", "war", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling lockt sie wieder;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "lockt", "sie", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Neues hat sie nicht gelernt,", "tokens": ["Was", "Neu\u00b7es", "hat", "sie", "nicht", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt alte liebe Lieder.", "tokens": ["Singt", "al\u00b7te", "lie\u00b7be", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00dcberm\u00fctig sieht's nicht aus,", "tokens": ["\u00dc\u00b7berm\u00b7\u00fc\u00b7tig", "sieht's", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses kleine Gartenhaus,", "tokens": ["Die\u00b7ses", "klei\u00b7ne", "Gar\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen, die sich drin gen\u00e4hrt,", "tokens": ["Al\u00b7len", ",", "die", "sich", "drin", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward ein guter Mut beschert.", "tokens": ["Ward", "ein", "gu\u00b7ter", "Mut", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gar manches artig ist geschehn", "tokens": ["Gar", "man\u00b7ches", "ar\u00b7tig", "ist", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch leichte Griffelspiele;", "tokens": ["Durch", "leich\u00b7te", "Grif\u00b7fel\u00b7spie\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch recht betrachtet, wohl besehn,", "tokens": ["Doch", "recht", "be\u00b7trach\u00b7tet", ",", "wohl", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fehlt immer Hain und M\u00fchle.", "tokens": ["Fehlt", "im\u00b7mer", "Hain", "und", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Erinnr' ich mich doch sp\u00e4t und fr\u00fch", "tokens": ["Er\u00b7inn\u00b7r'", "ich", "mich", "doch", "sp\u00e4t", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des lieblichsten Gesichts,", "tokens": ["Des", "lieb\u00b7lichs\u00b7ten", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie denkt an mich, ich denk an sie,", "tokens": ["Sie", "denkt", "an", "mich", ",", "ich", "denk", "an", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und beiden hilft es nichts.", "tokens": ["Und", "bei\u00b7den", "hilft", "es", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}