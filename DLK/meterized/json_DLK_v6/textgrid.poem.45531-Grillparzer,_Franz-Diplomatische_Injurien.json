{"textgrid.poem.45531": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Diplomatische Injurien", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schreit dir ein Gassenbube Schimpfworte nach,", "tokens": ["Schreit", "dir", "ein", "Gas\u00b7sen\u00b7bu\u00b7be", "Schimpf\u00b7wor\u00b7te", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So geh deines Weges fort gemach.", "tokens": ["So", "geh", "dei\u00b7nes", "We\u00b7ges", "fort", "ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Almosen ist wohl schon verzettelt,", "tokens": ["Das", "Al\u00b7mo\u00b7sen", "ist", "wohl", "schon", "ver\u00b7zet\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das er bei Novara erbettelt.", "tokens": ["Das", "er", "bei", "No\u00b7va\u00b7ra", "er\u00b7bet\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}