{"textgrid.poem.35166": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Leitung", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird ein Engel dir gesandt,", "tokens": ["Es", "wird", "ein", "En\u00b7gel", "dir", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dich durchs Leben zu begleiten.", "tokens": ["Um", "dich", "durchs", "Le\u00b7ben", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er nimmt dich liebend an der Hand", "tokens": ["Er", "nimmt", "dich", "lie\u00b7bend", "an", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibt bei dir zu allen Zeiten.", "tokens": ["Und", "bleibt", "bei", "dir", "zu", "al\u00b7len", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er kennt den Weg, den du zu gehen hast,", "tokens": ["Er", "kennt", "den", "Weg", ",", "den", "du", "zu", "ge\u00b7hen", "hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und tr\u00e4gt mit dir der Erde Leid und Last.", "tokens": ["Und", "tr\u00e4gt", "mit", "dir", "der", "Er\u00b7de", "Leid", "und", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es wird ein Engel dir gesandt,", "tokens": ["Es", "wird", "ein", "En\u00b7gel", "dir", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem sollst du dich gern anvertrauen.", "tokens": ["Dem", "sollst", "du", "dich", "gern", "an\u00b7ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihn soll stets und unverwandt", "tokens": ["Auf", "ihn", "soll", "stets", "und", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Auge deiner Seele schauen.", "tokens": ["Das", "Au\u00b7ge", "dei\u00b7ner", "See\u00b7le", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er tr\u00e4gt zu deinem Schutz das Schwert des Herrn", "tokens": ["Er", "tr\u00e4gt", "zu", "dei\u00b7nem", "Schutz", "das", "Schwert", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ist dir nie mit seiner H\u00fclfe fern.", "tokens": ["Und", "ist", "dir", "nie", "mit", "sei\u00b7ner", "H\u00fcl\u00b7fe", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es wird ein Engel dir gesandt,", "tokens": ["Es", "wird", "ein", "En\u00b7gel", "dir", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem sollst du niemals widerstreben,", "tokens": ["Dem", "sollst", "du", "nie\u00b7mals", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hast du ihn vielleicht verkannt,", "tokens": ["Und", "hast", "du", "ihn", "viel\u00b7leicht", "ver\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zwing ihn nicht, dich aufzugeben,", "tokens": ["So", "zwing", "ihn", "nicht", ",", "dich", "auf\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn bautest du auf deine Kraft allein,", "tokens": ["Denn", "bau\u00b7test", "du", "auf", "dei\u00b7ne", "Kraft", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es w\u00fcrde nur zu deinem Ungl\u00fcck sein.", "tokens": ["Es", "w\u00fcr\u00b7de", "nur", "zu", "dei\u00b7nem", "Un\u00b7gl\u00fcck", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}