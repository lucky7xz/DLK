{"dta.poem.13099": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evernehmt, erstaunt und h\u00f6rt was meine Kunst vermag,", "tokens": ["\u201e", "ver\u00b7nehmt", ",", "er\u00b7staunt", "und", "h\u00f6rt", "was", "mei\u00b7ne", "Kunst", "ver\u00b7mag", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eo nicht verge\u00dflicher mit Gold bemerckter Tag!", "tokens": ["\u201e", "o", "nicht", "ver\u00b7ge\u00df\u00b7li\u00b7cher", "mit", "Gold", "be\u00b7merck\u00b7ter", "Tag", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKNEG", "ADJD", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edurch meiner Cither Kraft lehrt\u2019 ich ein Volck zu siegen,", "tokens": ["\u201e", "durch", "mei\u00b7ner", "Cit\u00b7her", "Kraft", "lehrt'", "ich", "ein", "Volck", "zu", "sie\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edas Lanze, Schwert und Schild schon zwey Mahl mu\u00dfte schmiegen.", "tokens": ["\u201e", "das", "Lan\u00b7ze", ",", "Schwert", "und", "Schild", "schon", "zwey", "Mahl", "mu\u00df\u00b7te", "schmie\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "NN", "KON", "NN", "ADV", "CARD", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "345\u201dIch sunge dessen Furcht mit solcher Regung vor,", "tokens": ["\"", "Ich", "sun\u00b7ge", "des\u00b7sen", "Furcht", "mit", "sol\u00b7cher", "Re\u00b7gung", "vor", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PDS", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eda\u00df es die Rach ergriff, Zaghaftigkeit verlohr,", "tokens": ["\u201e", "da\u00df", "es", "die", "Rach", "er\u00b7griff", ",", "Zag\u00b7haf\u00b7tig\u00b7keit", "ver\u00b7lohr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eden Muth, die Stirn erhob, noch einen Kampf zu wagen,", "tokens": ["\u201e", "den", "Muth", ",", "die", "Stirn", "er\u00b7hob", ",", "noch", "ei\u00b7nen", "Kampf", "zu", "wa\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eich w\u00fcnscht\u2019 es, es geschah; der Gegner ward geschlagen.", "tokens": ["\u201e", "ich", "w\u00fcnscht'", "es", ",", "es", "ge\u00b7schah", ";", "der", "Geg\u00b7ner", "ward", "ge\u00b7schla\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}