{"textgrid.poem.55716": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Du kein Popanz", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Du kein Popanz", "tokens": ["Wenn", "Du", "kein", "Po\u00b7panz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Heuchelnder Pfaffen bist,", "tokens": ["Heu\u00b7cheln\u00b7der", "Pfaf\u00b7fen", "bist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Herr des Himmels,", "tokens": ["Herr", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "So h\u00f6r' meinen Schrei", "tokens": ["So", "h\u00f6r'", "mei\u00b7nen", "Schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "La\u00df Deiner Blitze", "tokens": ["La\u00df", "Dei\u00b7ner", "Blit\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zischende Schlangen", "tokens": ["Zi\u00b7schen\u00b7de", "Schlan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Niedersausen aufs schuldige Haupt mir,", "tokens": ["Nie\u00b7der\u00b7sau\u00b7sen", "aufs", "schul\u00b7di\u00b7ge", "Haupt", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PPER", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Tilge die S\u00fcnden durch s\u00fchnenden Tod;", "tokens": ["Til\u00b7ge", "die", "S\u00fcn\u00b7den", "durch", "s\u00fch\u00b7nen\u00b7den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Oder rei\u00dfe mir aus der Seele", "tokens": ["O\u00b7der", "rei\u00b7\u00dfe", "mir", "aus", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Mit nerviger Faust", "tokens": ["Mit", "ner\u00b7vi\u00b7ger", "Faust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Den Stachel der Lust;", "tokens": ["Den", "Sta\u00b7chel", "der", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Pflanze die Kraft zur Bu\u00dfe", "tokens": ["Pflan\u00b7ze", "die", "Kraft", "zur", "Bu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Tief in den zuckenden Leib,", "tokens": ["Tief", "in", "den", "zu\u00b7cken\u00b7den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Aber hilf mir, hilf mir, hilf mir!", "tokens": ["A\u00b7ber", "hilf", "mir", ",", "hilf", "mir", ",", "hilf", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "VVIMP", "PPER", "$,", "VVIMP", "PPER", "$."], "meter": "+------+", "measure": "dactylic.init"}, "line.15": {"text": "Oder es soll", "tokens": ["O\u00b7der", "es", "soll"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Wie rachebr\u00fcllendes Volksgewimmel", "tokens": ["Wie", "ra\u00b7che\u00b7br\u00fcl\u00b7len\u00b7des", "Volks\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Empor zu Deinem Thron", "tokens": ["Em\u00b7por", "zu", "Dei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Die Schar meiner Fl\u00fcche steigen,", "tokens": ["Die", "Schar", "mei\u00b7ner", "Fl\u00fc\u00b7che", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Die Flut meiner L\u00e4sterungen", "tokens": ["Die", "Flut", "mei\u00b7ner", "L\u00e4s\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Soll Deinen Thron verschwemmen", "tokens": ["Soll", "Dei\u00b7nen", "Thron", "ver\u00b7schwem\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und Dich selber, Herr des Himmels,", "tokens": ["Und", "Dich", "sel\u00b7ber", ",", "Herr", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Niederzerren in Nacht und Kot.", "tokens": ["Nie\u00b7der\u00b7zer\u00b7ren", "in", "Nacht", "und", "Kot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}