{"textgrid.poem.42894": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du alter Stachelkaks,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du alter Stachelkaks,", "tokens": ["Du", "al\u00b7ter", "Sta\u00b7chel\u00b7kaks", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du bist kein Bohnerwachs,", "tokens": ["Du", "bist", "kein", "Boh\u00b7ner\u00b7wachs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Gew\u00e4chs, das die Liebe sich pfl\u00fcckt,", "tokens": ["Kein", "Ge\u00b7w\u00e4chs", ",", "das", "die", "Lie\u00b7be", "sich", "pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sondern du bist nur ein bi\u00dfchen verr\u00fcckt.", "tokens": ["Son\u00b7dern", "du", "bist", "nur", "ein", "bi\u00df\u00b7chen", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "PIS", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, da\u00df du wenig trinkst.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "we\u00b7nig", "trinkst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du hast auch keinerlei Duft.", "tokens": ["Du", "hast", "auch", "kei\u00b7ner\u00b7lei", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aber, ohne da\u00df du selber stinkst,", "tokens": ["A\u00b7ber", ",", "oh\u00b7ne", "da\u00df", "du", "sel\u00b7ber", "stinkst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Saugst du Stubenmief ein wie Tropenluft.", "tokens": ["Saugst", "du", "Stu\u00b7ben\u00b7mief", "ein", "wie", "Tro\u00b7pen\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du springst niemals Menschen an oder Vieh.", "tokens": ["Du", "springst", "nie\u00b7mals", "Men\u00b7schen", "an", "o\u00b7der", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wer aber mit Absicht oder versehentlich", "tokens": ["Wer", "a\u00b7ber", "mit", "Ab\u00b7sicht", "o\u00b7der", "ver\u00b7se\u00b7hent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "ADJD"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sich einmal auf dich", "tokens": ["Sich", "ein\u00b7mal", "auf", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Setzte, vergi\u00dft dich nie.", "tokens": ["Setz\u00b7te", ",", "ver\u00b7gi\u00dft", "dich", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ein betrunkener, lachender Neger", "tokens": ["Ein", "be\u00b7trun\u00b7ke\u00b7ner", ",", "la\u00b7chen\u00b7der", "Ne\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schenkte dich mir, du lustiges Kleines,", "tokens": ["Schenk\u00b7te", "dich", "mir", ",", "du", "lus\u00b7ti\u00b7ges", "Klei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df ich den Vater ersetze dir kantigem Ableger", "tokens": ["Da\u00df", "ich", "den", "Va\u00b7ter", "er\u00b7set\u00b7ze", "dir", "kan\u00b7ti\u00b7gem", "Ab\u00b7le\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Eines verr\u00fcckten, stets starren Stachelschweines.", "tokens": ["Ei\u00b7nes", "ver\u00b7r\u00fcck\u00b7ten", ",", "stets", "star\u00b7ren", "Sta\u00b7chel\u00b7schwei\u00b7nes", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}}}}