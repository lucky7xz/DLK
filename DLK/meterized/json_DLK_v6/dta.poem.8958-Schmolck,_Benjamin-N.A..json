{"dta.poem.8958": {"metadata": {"author": {"name": "Schmolck, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1712", "urn": "urn:nbn:de:kobv:b4-20543-0", "language": ["de:0.99"], "booktitle": "Schmolck, Benjamin: Der Lustige Sabbath. Jauer u. a., 1712."}, "poem": {"stanza.1": {"line.1": {"text": "So nehmen Sie nunGn\u00e4dig\nG\u00fctigstan/", "tokens": ["So", "neh\u00b7men", "Sie", "nun", "Gn\u00e4\u00b7dig", "G\u00fc\u00b7tigs\u00b7tan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NE", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was meine Hand hier liefern kan.", "tokens": ["Was", "mei\u00b7ne", "Hand", "hier", "lie\u00b7fern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Danckbarkeit hat mich gezogen.", "tokens": ["Die", "Dan\u00b7ck\u00b7bar\u00b7keit", "hat", "mich", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich nenn es nun Jhr Eigenthum/", "tokens": ["Ich", "nenn", "es", "nun", "Ihr", "Ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie brauchen es zu Gottes Ruhm/", "tokens": ["Sie", "brau\u00b7chen", "es", "zu", "Got\u00b7tes", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bleiben mir hinfort gewogen.", "tokens": ["Und", "blei\u00b7ben", "mir", "hin\u00b7fort", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}