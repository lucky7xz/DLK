{"dta.poem.19029": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.  Sehnliche Gedancken am 72sten Ge-  \n burts-Tage der Frau Gro\u00df-Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Komm Ewigkeit Inbegriff innigster Wonne,", "tokens": ["Komm", "E\u00b7wig\u00b7keit", "In\u00b7be\u00b7griff", "in\u00b7nigs\u00b7ter", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bestrahle und heitere unser Gem\u00fcth:", "tokens": ["Be\u00b7strah\u00b7le", "und", "hei\u00b7te\u00b7re", "un\u00b7ser", "Ge\u00b7m\u00fcth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Erscheine, du helle durchdringende Sonne,", "tokens": ["Er\u00b7schei\u00b7ne", ",", "du", "hel\u00b7le", "durch\u00b7drin\u00b7gen\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Darunter der Seegen erw\u00e4chset und bl\u00fcth:", "tokens": ["Da\u00b7run\u00b7ter", "der", "See\u00b7gen", "er\u00b7w\u00e4ch\u00b7set", "und", "bl\u00fcth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wir schauen mit Sehnen,", "tokens": ["Wir", "schau\u00b7en", "mit", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erwarten mit Thr\u00e4nen", "tokens": ["Er\u00b7war\u00b7ten", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Auf deine unendliche Klarheit und Gl\u00e4ntzen,", "tokens": ["Auf", "dei\u00b7ne", "un\u00b7end\u00b7li\u00b7che", "Klar\u00b7heit", "und", "Gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und wallen mit Wehmuth in irrdischen Gr\u00e4ntzen.", "tokens": ["Und", "wal\u00b7len", "mit", "Weh\u00b7muth", "in", "irr\u00b7di\u00b7schen", "Gr\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Arbeit der S\u00fcnden macht Krafft-lo\u00df und m\u00fcde:", "tokens": ["Die", "Ar\u00b7beit", "der", "S\u00fcn\u00b7den", "macht", "Krafft\u00b7lo\u00df", "und", "m\u00fc\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie wird sie uns sauer die fleischliche Last!", "tokens": ["Wie", "wird", "sie", "uns", "sau\u00b7er", "die", "fleischli\u00b7che", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Wie s\u00fcsse hingegen, wie sch\u00f6ne klingst: Friede!", "tokens": ["Wie", "s\u00fcs\u00b7se", "hin\u00b7ge\u00b7gen", ",", "wie", "sch\u00f6\u00b7ne", "klingst", ":", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "PWAV", "ADJA", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und Ruhe von Arbeit und ewige Rast!", "tokens": ["Und", "Ru\u00b7he", "von", "Ar\u00b7beit", "und", "e\u00b7wi\u00b7ge", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "O seelige Schaaren,", "tokens": ["O", "see\u00b7li\u00b7ge", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Die dahin gefahren,", "tokens": ["Die", "da\u00b7hin", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PAV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Wo Christi Verlobte mit edlen Geschmeiden", "tokens": ["Wo", "Chris\u00b7ti", "Ver\u00b7lob\u00b7te", "mit", "ed\u00b7len", "Ge\u00b7schmei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.16": {"text": "Und k\u00f6stlichem Schmucke im Rosen-Busch weiden.", "tokens": ["Und", "k\u00f6st\u00b7li\u00b7chem", "Schmu\u00b7cke", "im", "Ro\u00b7sen\u00b7Busch", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+-++-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Verlauffet, ihr Zeiten, verschwindet, ihr Stunden,", "tokens": ["Ver\u00b7lauf\u00b7fet", ",", "ihr", "Zei\u00b7ten", ",", "ver\u00b7schwin\u00b7det", ",", "ihr", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.18": {"text": "Macht unserem Br\u00e4utigam Bahne und Platz;", "tokens": ["Macht", "un\u00b7se\u00b7rem", "Br\u00e4u\u00b7ti\u00b7gam", "Bah\u00b7ne", "und", "Platz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Wir haben den Ausgang des Jammerthals funden;", "tokens": ["Wir", "ha\u00b7ben", "den", "Aus\u00b7gang", "des", "Jam\u00b7mer\u00b7thals", "fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.20": {"text": "Wir graben nach einem verborgenen Schatz.", "tokens": ["Wir", "gra\u00b7ben", "nach", "ei\u00b7nem", "ver\u00b7bor\u00b7ge\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.21": {"text": "Die Nacht hindurch sorgen", "tokens": ["Die", "Nacht", "hin\u00b7durch", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Wir nur auf den Morgen.", "tokens": ["Wir", "nur", "auf", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Ach! k\u00e4me derselbe, was w\u00fcrde uns qv\u00e4len?", "tokens": ["Ach", "!", "k\u00e4\u00b7me", "der\u00b7sel\u00b7be", ",", "was", "w\u00fcr\u00b7de", "uns", "qv\u00e4\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PDAT", "$,", "PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.24": {"text": "Was w\u00fcrde uns mehr an der Seeligkeit fehlen?", "tokens": ["Was", "w\u00fcr\u00b7de", "uns", "mehr", "an", "der", "See\u00b7lig\u00b7keit", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Wohlan denn, Geliebter, du wirst ja erscheinen,", "tokens": ["Wo\u00b7hlan", "denn", ",", "Ge\u00b7lieb\u00b7ter", ",", "du", "wirst", "ja", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Dein Seiger geht langsam; so zeige dich doch!", "tokens": ["Dein", "Sei\u00b7ger", "geht", "lang\u00b7sam", ";", "so", "zei\u00b7ge", "dich", "doch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "Schau nun nach den Frommen, erfreue die Deinen;", "tokens": ["Schau", "nun", "nach", "den", "From\u00b7men", ",", "er\u00b7freu\u00b7e", "die", "Dei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "PPOSAT", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Sie hassen die Welt-Lust, sie lieben dein Joch.", "tokens": ["Sie", "has\u00b7sen", "die", "Welt\u00b7Lust", ",", "sie", "lie\u00b7ben", "dein", "Joch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Befeuchte den Garten:", "tokens": ["Be\u00b7feuch\u00b7te", "den", "Gar\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Er kan nicht mehr warten:", "tokens": ["Er", "kan", "nicht", "mehr", "war\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Die Mitternacht nahet: wir hoffen mit Schmertzen;", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "na\u00b7het", ":", "wir", "hof\u00b7fen", "mit", "Schmert\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Die Lampen sind fertig: auch brennen die Kertzen.", "tokens": ["Die", "Lam\u00b7pen", "sind", "fer\u00b7tig", ":", "auch", "bren\u00b7nen", "die", "Kert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVINF", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.33": {"text": "Wir h\u00f6ren Dich, Liebster; Du heissest uns warten:", "tokens": ["Wir", "h\u00f6\u00b7ren", "Dich", ",", "Liebs\u00b7ter", ";", "Du", "heis\u00b7sest", "uns", "war\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.34": {"text": "Man lauffet dir niemahls mit F\u00f6rderung vor:", "tokens": ["Man", "lauf\u00b7fet", "dir", "nie\u00b7mahls", "mit", "F\u00f6r\u00b7de\u00b7rung", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.35": {"text": "Doch dr\u00fcckt uns die B\u00fcrde auf mancherley Arten:", "tokens": ["Doch", "dr\u00fcckt", "uns", "die", "B\u00fcr\u00b7de", "auf", "man\u00b7cher\u00b7ley", "Ar\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Das Fleisch l\u00e4st die Geister nicht gerne empor.", "tokens": ["Das", "Fleisch", "l\u00e4st", "die", "Geis\u00b7ter", "nicht", "ger\u00b7ne", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.37": {"text": "O JEsu! gedencke,", "tokens": ["O", "Je\u00b7su", "!", "ge\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.38": {"text": "Wie sehr es uns kr\u00e4ncke,", "tokens": ["Wie", "sehr", "es", "uns", "kr\u00e4n\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Dir so nicht zu dienen, wie wir es begehren,", "tokens": ["Dir", "so", "nicht", "zu", "die\u00b7nen", ",", "wie", "wir", "es", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.40": {"text": "Aufs wenigste must du uns stille seyn lehren.", "tokens": ["Aufs", "we\u00b7nigs\u00b7te", "must", "du", "uns", "stil\u00b7le", "seyn", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VMFIN", "PPER", "PPER", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}