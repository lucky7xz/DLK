{"textgrid.poem.50798": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "19", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O teure Luft! Mit jedem Odemzug", "tokens": ["O", "teu\u00b7re", "Luft", "!", "Mit", "je\u00b7dem", "O\u00b7dem\u00b7zug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vergeud ich sie, die unentbehrlich ist!", "tokens": ["Ver\u00b7geud", "ich", "sie", ",", "die", "un\u00b7ent\u00b7behr\u00b7lich", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fern bin ich euch, Bergh\u00f6hen, Wolkenflug,", "tokens": ["Fern", "bin", "ich", "euch", ",", "Berg\u00b7h\u00f6\u00b7hen", ",", "Wol\u00b7ken\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo man dies Gut nicht achtet und nicht mi\u00dft.", "tokens": ["Wo", "man", "dies", "Gut", "nicht", "ach\u00b7tet", "und", "nicht", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PDS", "NN", "PTKNEG", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier eingeschlossen mit der Todesqual,", "tokens": ["Hier", "ein\u00b7ge\u00b7schlos\u00b7sen", "mit", "der", "To\u00b7des\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der unsichtbaren, Stirn an Stirn gepre\u00dft,", "tokens": ["Der", "un\u00b7sicht\u00b7ba\u00b7ren", ",", "Stirn", "an", "Stirn", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umschlingt sie mir Haupt, Glieder, Herz zumal", "tokens": ["Um\u00b7schlingt", "sie", "mir", "Haupt", ",", "Glie\u00b7der", ",", "Herz", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$,", "NN", "$,", "NN", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Schlangenringen, unbarmherzig fest.", "tokens": ["Mit", "Schlan\u00b7gen\u00b7rin\u00b7gen", ",", "un\u00b7barm\u00b7her\u00b7zig", "fest", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun geht's ans Sterben \u2013 strenge Seelenzucht,", "tokens": ["Nun", "geht's", "ans", "Ster\u00b7ben", "\u2013", "stren\u00b7ge", "See\u00b7len\u00b7zucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ich mich scheidend unterwerfen soll!", "tokens": ["Der", "ich", "mich", "schei\u00b7dend", "un\u00b7ter\u00b7wer\u00b7fen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Denken schwindet mir in dunkler Flucht \u2013", "tokens": ["Mein", "Den\u00b7ken", "schwin\u00b7det", "mir", "in", "dunk\u00b7ler", "Flucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Matt schl\u00e4gt das Herz \u2013 bald bricht's \u2013 erwartungsvoll \u2013", "tokens": ["Matt", "schl\u00e4gt", "das", "Herz", "\u2013", "bald", "bricht's", "\u2013", "er\u00b7war\u00b7tungs\u00b7voll", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$(", "ADV", "VVFIN", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}