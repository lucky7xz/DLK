{"textgrid.poem.48817": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "42. Auf die demantnen Armb\u00e4nder", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Arme scheint ihr zwar von au\u00dfen nur zu binden,", "tokens": ["Die", "Ar\u00b7me", "scheint", "ihr", "zwar", "von", "au\u00b7\u00dfen", "nur", "zu", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da ihr das Herze doch von innen mehr verstrickt.", "tokens": ["da", "ihr", "das", "Her\u00b7ze", "doch", "von", "in\u00b7nen", "mehr", "ver\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr seid der H\u00e4nde Pracht, als die ihr herlich schm\u00fcckt,", "tokens": ["Ihr", "seid", "der", "H\u00e4n\u00b7de", "Pracht", ",", "als", "die", "ihr", "her\u00b7lich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "KOUS", "ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "doch mehr der Seelen Qual, die schmerzlich mu\u00df empfinden,", "tokens": ["doch", "mehr", "der", "See\u00b7len", "Qual", ",", "die", "schmerz\u00b7lich", "mu\u00df", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wie eure kalte Glut das Eis auch kan entz\u00fcnden,", "tokens": ["wie", "eu\u00b7re", "kal\u00b7te", "Glut", "das", "Eis", "auch", "kan", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das manchen Sinn macht stolz. Ihr werdet ihr geschickt.", "tokens": ["das", "man\u00b7chen", "Sinn", "macht", "stolz", ".", "Ihr", "wer\u00b7det", "ihr", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie, das geliebte Mensch, wird selbst aus ihr entr\u00fcckt", "tokens": ["Sie", ",", "das", "ge\u00b7lieb\u00b7te", "Mensch", ",", "wird", "selbst", "aus", "ihr", "ent\u00b7r\u00fcckt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und, die euch stets besitzt, kan sich selbst nicht mehr finden.", "tokens": ["und", ",", "die", "euch", "stets", "be\u00b7sitzt", ",", "kan", "sich", "selbst", "nicht", "mehr", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "VMFIN", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Besch\u00f6nt euch, wie ihr wolt. Auch dieser Plitz sagt frei,", "tokens": ["Be\u00b7sch\u00f6nt", "euch", ",", "wie", "ihr", "wolt", ".", "Auch", "die\u00b7ser", "Plitz", "sagt", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$.", "ADV", "PDAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der euer Gold macht blind, da\u00df Lieb' ein Feuer sei,", "tokens": ["der", "eu\u00b7er", "Gold", "macht", "blind", ",", "da\u00df", "Lieb'", "ein", "Feu\u00b7er", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "ADJD", "$,", "KOUS", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das auch die Herzen fri\u00dft, eh' als es wird empfunden.", "tokens": ["das", "auch", "die", "Her\u00b7zen", "fri\u00dft", ",", "eh'", "als", "es", "wird", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Reich seid ihr und macht arm, macht finster, ob ihr glei\u00dft;", "tokens": ["Reich", "seid", "ihr", "und", "macht", "arm", ",", "macht", "fins\u00b7ter", ",", "ob", "ihr", "glei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "ihr seid doch, was ihr seid. Bleibt B\u00e4nder, wie ihr hei\u00dft.", "tokens": ["ihr", "seid", "doch", ",", "was", "ihr", "seid", ".", "Bleibt", "B\u00e4n\u00b7der", ",", "wie", "ihr", "hei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$.", "VVFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lieb' einer noch so frei, so ist er doch gebunden.", "tokens": ["Lieb'", "ei\u00b7ner", "noch", "so", "frei", ",", "so", "ist", "er", "doch", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}