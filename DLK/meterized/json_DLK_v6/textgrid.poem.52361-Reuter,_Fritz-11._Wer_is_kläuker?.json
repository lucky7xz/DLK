{"textgrid.poem.52361": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "11. Wer is kl\u00e4uker?", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi hadden hier tau Lan'n en Preister,", "tokens": ["Wi", "had\u00b7den", "hier", "tau", "Lan'n", "en", "Preis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dat was en sihr olt snurrig Mann,", "tokens": ["Dat", "was", "en", "sihr", "olt", "snur\u00b7rig", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch in sin Predigt was hei Meister,", "tokens": ["Doch", "in", "sin", "Pre\u00b7digt", "was", "hei", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hei predigt so, dat jedermann", "tokens": ["Hei", "pre\u00b7digt", "so", ",", "dat", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sick licht dorut vernemen k\u00fcnn;", "tokens": ["Sick", "licht", "do\u00b7rut", "ver\u00b7ne\u00b7men", "k\u00fcnn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Un, wat sin Text ok grad m\u00fcggt sin,", "tokens": ["Un", ",", "wat", "sin", "Text", "ok", "grad", "m\u00fcggt", "sin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Hei w\u00fc\u00dft sin Thema so tau dreihn,", "tokens": ["Hei", "w\u00fc\u00dft", "sin", "The\u00b7ma", "so", "tau", "dreihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dat hei kamm rinne in den Sadel,", "tokens": ["Dat", "hei", "kamm", "rin\u00b7ne", "in", "den", "Sa\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von wo ut hei sin leiw Gemein", "tokens": ["Von", "wo", "ut", "hei", "sin", "leiw", "Ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "FM", "FM", "FM", "FM", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Mit spitze W\u00fcrd' un dristen Tadel", "tokens": ["Mit", "spit\u00b7ze", "W\u00fcrd'", "un", "dris\u00b7ten", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "FM", "FM", "FM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So recht tau Water riden k\u00fcnn.", "tokens": ["So", "recht", "tau", "Wa\u00b7ter", "ri\u00b7den", "k\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "CARD", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Na, einmal w\u00fcrd sin Text denn sin,", "tokens": ["Na", ",", "ein\u00b7mal", "w\u00fcrd", "sin", "Text", "denn", "sin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wo uns' Herr Christus \u00e4wer't Water f\u00fchrt", "tokens": ["Wo", "un\u00b7s'", "Herr", "Chris\u00b7tus", "\u00e4\u00b7wer't", "Wa\u00b7ter", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "VVFIN", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Un wo de Sturm den See upr\u00fchrt.", "tokens": ["Un", "wo", "de", "Sturm", "den", "See", "u\u00b7pr\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "NE", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Dit deilt hei sick nu n\u00fcdlich in:", "tokens": ["Dit", "deilt", "hei", "sick", "nu", "n\u00fcd\u00b7lich", "in", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "De irste Deil was \u00bbGott im Sturm\u00ab,", "tokens": ["De", "irs\u00b7te", "Deil", "was", "\u00bb", "Gott", "im", "Sturm", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "PWS", "$(", "NN", "APPRART", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "De tweite was \u00bbDer Mensch im Sturm\u00ab,", "tokens": ["De", "twei\u00b7te", "was", "\u00bb", "Der", "Mensch", "im", "Sturm", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "$(", "ART", "NN", "APPRART", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "De dr\u00fcdd \u00bbDer Mensch des Morgens nach dem Sturm\u00ab.", "tokens": ["De", "dr\u00fcdd", "\u00bb", "Der", "Mensch", "des", "Mor\u00b7gens", "nach", "dem", "Sturm", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$(", "ART", "NN", "ART", "ADV", "APPR", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So kamm hei denn up't Bramwindrinken", "tokens": ["So", "kamm", "hei", "denn", "up't", "Bram\u00b7wind\u00b7rin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Un von den Bramwin up de Deiweri", "tokens": ["Un", "von", "den", "Bram\u00b7win", "up", "de", "Dei\u00b7we\u00b7ri"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NE", "NE", "NE"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Un w\u00fcrd denn sin Gemein \u2013 verstehst mir? Wie? \u2013", "tokens": ["Un", "w\u00fcrd", "denn", "sin", "Ge\u00b7mein", "\u2013", "ver\u00b7stehst", "mir", "?", "Wie", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "$.", "PWAV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Geh\u00fcrig mit den Tunpahl winken.", "tokens": ["Ge\u00b7h\u00fc\u00b7rig", "mit", "den", "Tun\u00b7pahl", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Sei hadden em sin Wisch uth\u00f6tt,", "tokens": ["Sei", "had\u00b7den", "em", "sin", "Wisch", "ut\u00b7h\u00f6tt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Un't w\u00fcrd ehr utenanner set't,", "tokens": ["Un't", "w\u00fcrd", "ehr", "u\u00b7ten\u00b7an\u00b7ner", "set't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dat nich allein des Nachts Inbreken", "tokens": ["Dat", "nich", "al\u00b7lein", "des", "Nachts", "In\u00b7bre\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "ART", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Un heimlich Musen Stehlen wir.", "tokens": ["Un", "heim\u00b7lich", "Mu\u00b7sen", "Steh\u00b7len", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "\u00bbnein, meine Freunde\u00ab, w\u00fcrd hei spreken,", "tokens": ["\u00bb", "nein", ",", "mei\u00b7ne", "Freun\u00b7de", "\u00ab", ",", "w\u00fcrd", "hei", "spre\u00b7ken", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPOSAT", "NN", "$(", "$,", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "\u00bb's ist Diebstahl auch, wenn einer mir", "tokens": ["\u00bb", "'s", "ist", "Dieb\u00b7stahl", "auch", ",", "wenn", "ei\u00b7ner", "mir"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "NN", "ADV", "$,", "KOUS", "PIS", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Die Wiese, ohn' es zu verg\u00fcten,", "tokens": ["Die", "Wie\u00b7se", ",", "ohn'", "es", "zu", "ver\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Mit seinen Pferden l\u00e4\u00dft aush\u00fcten.\u00ab", "tokens": ["Mit", "sei\u00b7nen", "Pfer\u00b7den", "l\u00e4\u00dft", "aus\u00b7h\u00fc\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Na, dat is gaud! De Predigt is nu ut,", "tokens": ["Na", ",", "dat", "is", "gaud", "!", "De", "Pre\u00b7digt", "is", "nu", "ut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.nl", "FM.nl", "FM.nl", "$.", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De L\u00fcd' gahn ut de Kirch herut,", "tokens": ["De", "L\u00fcd'", "gahn", "ut", "de", "Kirch", "he\u00b7rut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un de Herr Paster sitt recht sch\u00f6n", "tokens": ["Un", "de", "Herr", "Pas\u00b7ter", "sitt", "recht", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In'n Lehnstaul in sin Stuw', dunn kloppt dor wen.", "tokens": ["In'n", "Lehn\u00b7staul", "in", "sin", "Stu\u00b7w'", ",", "dunn", "kloppt", "dor", "wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "\u00bbherein!\u00ab \u2013 'rin k\u00fcmmt de Schult un s\u00e4ben Buren.", "tokens": ["\u00bb", "her\u00b7ein", "!", "\u00ab", "\u2013", "'rin", "k\u00fcmmt", "de", "Schult", "un", "s\u00e4\u00b7ben", "Bu\u00b7ren", "."], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "$(", "NE", "VVFIN", "NE", "NE", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwi kamen tau den Herrn Pasturen,", "tokens": ["\u00bb", "wi", "ka\u00b7men", "tau", "den", "Herrn", "Pas\u00b7tu\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "NE", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "De Predigt was uns doch tau hart. \u2013", "tokens": ["De", "Pre\u00b7digt", "was", "uns", "doch", "tau", "hart", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "PWS", "PPER", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.8": {"text": "Na, red nu du mal, Vadder Swart!\u00ab", "tokens": ["Na", ",", "red", "nu", "du", "mal", ",", "Vad\u00b7der", "Swart", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "PPER", "ADV", "$,", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbja, Herr Pastur\u00ab, seggt Swart, \u00bbSei s\u00e4den,", "tokens": ["\u00bb", "ja", ",", "Herr", "Pas\u00b7tur", "\u00ab", ",", "seggt", "Swart", ",", "\u00bb", "Sei", "s\u00e4\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "NN", "$(", "$,", "VVFIN", "NE", "$,", "$(", "VAIMP", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dat wi Ehr Wisch uth\u00e4uden deden.", "tokens": ["Dat", "wi", "Ehr", "Wisch", "ut\u00b7h\u00e4u\u00b7den", "de\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.11": {"text": "Dat's nich an dem, dat's woll nich so. \u2013", "tokens": ["Dat's", "nich", "an", "dem", ",", "dat's", "woll", "nich", "so", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "$,", "PRELS", "ADV", "PTKNEG", "ADV", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Na, nu red du mal, Ramelow!\u00ab", "tokens": ["Na", ",", "nu", "red", "du", "mal", ",", "Ra\u00b7me\u00b7low", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbja\u00ab, seggt denn Ram'low, \u00bbnich de M\u00e4hren,", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "denn", "Ram'\u00b7low", ",", "\u00bb", "nich", "de", "M\u00e4h\u00b7ren", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "wo