{"textgrid.poem.25815": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[am s\u00fc\u00dfen lila Kleefeld vorbei]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am s\u00fc\u00dfen lila Kleefeld vorbei,", "tokens": ["Am", "s\u00fc\u00b7\u00dfen", "li\u00b7la", "Klee\u00b7feld", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu den Tannen, den zwei,", "tokens": ["Zu", "den", "Tan\u00b7nen", ",", "den", "zwei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "CARD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit der Bank inmitten,", "tokens": ["Mit", "der", "Bank", "in\u00b7mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dort zieht wie ein weicher Fl\u00f6tenlaut", "tokens": ["Dort", "zieht", "wie", "ein", "wei\u00b7cher", "Fl\u00f6\u00b7ten\u00b7laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der sanfte Fjord,", "tokens": ["Der", "sanf\u00b7te", "Fjord", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Blau im Schilfgr\u00fcn ausgeschnitten.", "tokens": ["Blau", "im", "Schilf\u00b7gr\u00fcn", "aus\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gib mir die Hand.", "tokens": ["Gib", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die beiden Tannen stehen so still,", "tokens": ["Die", "bei\u00b7den", "Tan\u00b7nen", "ste\u00b7hen", "so", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich will dir sagen,", "tokens": ["Ich", "will", "dir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was die Stille rings verschweigen will.", "tokens": ["Was", "die", "Stil\u00b7le", "rings", "ver\u00b7schwei\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gib mir die Hand ...", "tokens": ["Gib", "mir", "die", "Hand", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Gib mir in deiner Hand dein Herz.", "tokens": ["Gib", "mir", "in", "dei\u00b7ner", "Hand", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}