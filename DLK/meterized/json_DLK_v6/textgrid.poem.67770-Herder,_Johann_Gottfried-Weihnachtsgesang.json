{"textgrid.poem.67770": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsgesang", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Friedensk\u00f6nig kommt heran;", "tokens": ["Der", "Frie\u00b7dens\u00b7k\u00f6\u00b7nig", "kommt", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereitet ihm den Weg!", "tokens": ["Be\u00b7rei\u00b7tet", "ihm", "den", "Weg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Streut Palmen auf die Siegesbahn", "tokens": ["Streut", "Pal\u00b7men", "auf", "die", "Sie\u00b7ges\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ebnet jeden Steg!", "tokens": ["Und", "eb\u00b7net", "je\u00b7den", "Steg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sanftm\u00fcthig kommt er; Ha\u00df und Streit", "tokens": ["Sanft\u00b7m\u00fct\u00b7hig", "kommt", "er", ";", "Ha\u00df", "und", "Streit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist fern von seinem Sinn.", "tokens": ["Ist", "fern", "von", "sei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem\u00fcthig kommt er; Demuth breit'", "tokens": ["De\u00b7m\u00fct\u00b7hig", "kommt", "er", ";", "De\u00b7muth", "breit'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "NN", "VVFIN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Kleider vor ihm hin!", "tokens": ["Die", "Klei\u00b7der", "vor", "ihm", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn wen noch wilder Zorn entflammt,", "tokens": ["Denn", "wen", "noch", "wil\u00b7der", "Zorn", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wen Rach' und Ha\u00df emp\u00f6rt,", "tokens": ["Wen", "Rach'", "und", "Ha\u00df", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer Andre k\u00fchn und stolz verdammt,", "tokens": ["Wer", "And\u00b7re", "k\u00fchn", "und", "stolz", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht des K\u00f6nigs werth.", "tokens": ["Ist", "nicht", "des", "K\u00f6\u00b7nigs", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er kommt zum Frieden. Fried' ern\u00e4hrt,", "tokens": ["Er", "kommt", "zum", "Frie\u00b7den", ".", "Fried'", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unfried' verheert die Welt.", "tokens": ["Un\u00b7fried'", "ver\u00b7heert", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der ist nicht dieses K\u00f6nigs werth,", "tokens": ["Der", "ist", "nicht", "die\u00b7ses", "K\u00f6\u00b7nigs", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bund und Treu nicht h\u00e4lt.", "tokens": ["Der", "Bund", "und", "Treu", "nicht", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Willkommen, Held f\u00fcr unser Heil!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Held", "f\u00fcr", "un\u00b7ser", "Heil", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschheit Retter Du!", "tokens": ["Der", "Menschheit", "Ret\u00b7ter", "Du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer Wahrheit liebt, hat an Dir Theil", "tokens": ["Wer", "Wahr\u00b7heit", "liebt", ",", "hat", "an", "Dir", "Theil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "APPR", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Freud' und Himmelsruh;", "tokens": ["Und", "Freud'", "und", "Him\u00b7mels\u00b7ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch wer f\u00fcr Menschenwohl und Gl\u00fcck", "tokens": ["Auch", "wer", "f\u00fcr", "Men\u00b7schen\u00b7wohl", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefahr und Noth nicht scheut", "tokens": ["Ge\u00b7fahr", "und", "Noth", "nicht", "scheut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, ruft der Himmel ihn zur\u00fcck,", "tokens": ["Und", ",", "ruft", "der", "Him\u00b7mel", "ihn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leben willig weiht.", "tokens": ["Sein", "Le\u00b7ben", "wil\u00b7lig", "weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihm t\u00f6net Segen nach und Dank,", "tokens": ["Ihm", "t\u00f6\u00b7net", "Se\u00b7gen", "nach", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich sein Auge schlie\u00dft,", "tokens": ["Wenn", "sich", "sein", "Au\u00b7ge", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inde\u00df ihn Himmelslobgesang", "tokens": ["In\u00b7de\u00df", "ihn", "Him\u00b7mels\u00b7lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbgesegnet sei!\u00ab begr\u00fc\u00dft.", "tokens": ["\u00bb", "ge\u00b7seg\u00b7net", "sei", "!", "\u00ab", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "$.", "$(", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wolauf, wir stimmen in den Chor", "tokens": ["Wo\u00b7lauf", ",", "wir", "stim\u00b7men", "in", "den", "Chor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hosianna ein!", "tokens": ["Das", "Ho\u00b7si\u00b7an\u00b7na", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Engel schwingt die Palm' empor,", "tokens": ["Ein", "En\u00b7gel", "schwingt", "die", "Palm'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sanftmuth uns zu weihn!", "tokens": ["Der", "Sanft\u00b7muth", "uns", "zu", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}