{"textgrid.poem.39083": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Sie s\u00e4hn es gern, ich w\u00fcrde kirre", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie s\u00e4hn es gern, ich w\u00fcrde kirre", "tokens": ["Sie", "s\u00e4hn", "es", "gern", ",", "ich", "w\u00fcr\u00b7de", "kir\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und beugete mich niederw\u00e4rts;", "tokens": ["und", "beu\u00b7ge\u00b7te", "mich", "nie\u00b7der\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "sie machten gern mein tapfres Herz", "tokens": ["sie", "mach\u00b7ten", "gern", "mein", "tapf\u00b7res", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in seinem stolzen Glauben irre.", "tokens": ["in", "sei\u00b7nem", "stol\u00b7zen", "Glau\u00b7ben", "ir\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sagten mir: Es ist vergebens,", "tokens": ["Sie", "sag\u00b7ten", "mir", ":", "Es", "ist", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du \u00e4nderst nicht den Lauf der Welt;", "tokens": ["du", "\u00e4n\u00b7derst", "nicht", "den", "Lauf", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Knecht bleibt sie doch! Und dir verg\u00e4llt", "tokens": ["Knecht", "bleibt", "sie", "doch", "!", "Und", "dir", "ver\u00b7g\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "hast du den Sommer deines Lebens.", "tokens": ["hast", "du", "den", "Som\u00b7mer", "dei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl, sei es so! Sich f\u00fcgen lerne,", "tokens": ["Wohl", ",", "sei", "es", "so", "!", "Sich", "f\u00fc\u00b7gen", "ler\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PPER", "ADV", "$.", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wem F\u00fcgsamkeit gen\u00fcgen kann,", "tokens": ["wem", "F\u00fcg\u00b7sam\u00b7keit", "ge\u00b7n\u00fc\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auch Demut schm\u00fccket ihren Mann:", "tokens": ["auch", "De\u00b7mut", "schm\u00fc\u00b7cket", "ih\u00b7ren", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber folge meinem Sterne!", "tokens": ["Ich", "a\u00b7ber", "fol\u00b7ge", "mei\u00b7nem", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da hilft kein Rat, da ist kein W\u00e4hlen,", "tokens": ["Da", "hilft", "kein", "Rat", ",", "da", "ist", "kein", "W\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich kann nicht anders, wollt' ich auch:", "tokens": ["ich", "kann", "nicht", "an\u00b7ders", ",", "wollt'", "ich", "auch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$,", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freiheit ist mein Lebenshauch,", "tokens": ["Die", "Frei\u00b7heit", "ist", "mein", "Le\u00b7bens\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie ist die Seele meiner Seelen!", "tokens": ["sie", "ist", "die", "See\u00b7le", "mei\u00b7ner", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So la\u00dft mich meine Bahn vollenden,", "tokens": ["So", "la\u00dft", "mich", "mei\u00b7ne", "Bahn", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie sie auch sei, mein Ziel ist mein;", "tokens": ["wie", "sie", "auch", "sei", ",", "mein", "Ziel", "ist", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ja, sollt' es auch ein Irrweg sein,", "tokens": ["ja", ",", "sollt'", "es", "auch", "ein", "Irr\u00b7weg", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich will ihn doch mit Ehren enden.", "tokens": ["ich", "will", "ihn", "doch", "mit", "Eh\u00b7ren", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}