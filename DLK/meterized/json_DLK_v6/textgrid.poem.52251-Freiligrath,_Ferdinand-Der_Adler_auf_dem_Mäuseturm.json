{"textgrid.poem.52251": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Adler auf dem M\u00e4useturm", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf wei\u00dfer Flagge weht ein Aar", "tokens": ["Auf", "wei\u00b7\u00dfer", "Flag\u00b7ge", "weht", "ein", "Aar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf dem M\u00e4useturm bei Bingen;", "tokens": ["Hoch", "auf", "dem", "M\u00e4u\u00b7se\u00b7turm", "bei", "Bin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er zeigt ein t\u00fcchtig Klauenpaar,", "tokens": ["Er", "zeigt", "ein", "t\u00fcch\u00b7tig", "Klau\u00b7en\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt eine Kron' und reckt die Schwingen.", "tokens": ["Tr\u00e4gt", "ei\u00b7ne", "Kron'", "und", "reckt", "die", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Sonnebrand und Schnee und Sturm", "tokens": ["Vom", "Son\u00b7ne\u00b7brand", "und", "Schnee", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind ihm die Federn glatt geschlichtet \u2013", "tokens": ["Sind", "ihm", "die", "Fe\u00b7dern", "glatt", "ge\u00b7schlich\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was Teufel, in den M\u00e4useturm,", "tokens": ["Was", "Teu\u00b7fel", ",", "in", "den", "M\u00e4u\u00b7se\u00b7turm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Adler, hast du dich gefl\u00fcchtet?", "tokens": ["O", "Ad\u00b7ler", ",", "hast", "du", "dich", "ge\u00b7fl\u00fcch\u00b7tet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du aus deiner F\u00fclle Horn", "tokens": ["Hast", "du", "aus", "dei\u00b7ner", "F\u00fcl\u00b7le", "Horn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Etwa gleich Hatto, jenem Alten,", "tokens": ["Et\u00b7wa", "gleich", "Hat\u00b7to", ",", "je\u00b7nem", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Mehl und Brot das teure Korn", "tokens": ["Zu", "Mehl", "und", "Brot", "das", "teu\u00b7re", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Mund des Volkes vorenthalten?", "tokens": ["Dem", "Mund", "des", "Vol\u00b7kes", "vor\u00b7ent\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Will dir ein r\u00e4chend M\u00e4useheer,", "tokens": ["Will", "dir", "ein", "r\u00e4\u00b7chend", "M\u00e4u\u00b7se\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie jenem Bischof einst, ans Leben?", "tokens": ["Wie", "je\u00b7nem", "Bi\u00b7schof", "einst", ",", "ans", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gereicht auch dir zu Schutz und Wehr", "tokens": ["Ge\u00b7reicht", "auch", "dir", "zu", "Schutz", "und", "Wehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hattos zerfallne Tr\u00fcmmer eben?", "tokens": ["Hat\u00b7tos", "zer\u00b7fall\u00b7ne", "Tr\u00fcm\u00b7mer", "e\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht doch! du geizest nicht mit Brot!", "tokens": ["Nicht", "doch", "!", "du", "gei\u00b7zest", "nicht", "mit", "Brot", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "J\u00fcngst noch", "tokens": ["J\u00fcngst", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Hast du ge\u00f6ffnet unsrer Not", "tokens": ["Hast", "du", "ge\u00b7\u00f6ff\u00b7net", "uns\u00b7rer", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All deiner Vorrathsh\u00e4user Segen!", "tokens": ["All", "dei\u00b7ner", "Vor\u00b7raths\u00b7h\u00e4u\u00b7ser", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du lie\u00dfest Hunsr\u00fcck, Eifel, Ahr", "tokens": ["Du", "lie\u00b7\u00dfest", "Huns\u00b7r\u00fcck", ",", "Ei\u00b7fel", ",", "Ahr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Brotkorn, soviel sie brauchten, fassen;", "tokens": ["Brot\u00b7korn", ",", "so\u00b7viel", "sie", "brauch\u00b7ten", ",", "fas\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast auch sonst manch schlechtes Jahr", "tokens": ["Du", "hast", "auch", "sonst", "manch", "schlech\u00b7tes", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Most die Steuer uns erlassen!", "tokens": ["Vom", "Most", "die", "Steu\u00b7er", "uns", "er\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum nicht als Wucherer am Rhein", "tokens": ["Drum", "nicht", "als", "Wu\u00b7che\u00b7rer", "am", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "KOUS", "NN", "APPRART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flohst du auf jene Mauerkronen!", "tokens": ["Flohst", "du", "auf", "je\u00b7ne", "Mau\u00b7er\u00b7kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch: \u2013 Brot aus Korne nicht allein", "tokens": ["Doch", ":", "\u2013", "Brot", "aus", "Kor\u00b7ne", "nicht", "al\u00b7lein"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$.", "$(", "NN", "APPR", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begehren heut die Nationen!", "tokens": ["Be\u00b7geh\u00b7ren", "heut", "die", "Na\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie wollen mehr, als was man kaut;", "tokens": ["Sie", "wol\u00b7len", "mehr", ",", "als", "was", "man", "kaut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie heben dreist den kr\u00e4ft'gen Nacken;", "tokens": ["Sie", "he\u00b7ben", "dreist", "den", "kr\u00e4ft'\u00b7gen", "Na\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie sehn sich um und rufen laut:", "tokens": ["Sie", "sehn", "sich", "um", "und", "ru\u00b7fen", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwo wird der Freiheit Brot gebacken?\u00ab", "tokens": ["\u00bb", "wo", "wird", "der", "Frei\u00b7heit", "Brot", "ge\u00b7ba\u00b7cken", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00e4ltst du mit allzu festen Krallen;", "tokens": ["H\u00e4ltst", "du", "mit", "all\u00b7zu", "fes\u00b7ten", "Kral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl lie\u00dfest du auch \u2013 wahr bleibt wahr! \u2013", "tokens": ["Wohl", "lie\u00b7\u00dfest", "du", "auch", "\u2013", "wahr", "bleibt", "wahr", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ADJD", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Freiheit j\u00fcngst ein W\u00f6rtchen fallen!", "tokens": ["Von", "Frei\u00b7heit", "j\u00fcngst", "ein", "W\u00f6rt\u00b7chen", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es schien des Volkes Hungerschrei", "tokens": ["Es", "schien", "des", "Vol\u00b7kes", "Hun\u00b7ger\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Recht in der Seele zu kr\u00e4nken;", "tokens": ["Recht", "in", "der", "See\u00b7le", "zu", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Du schienst an eine B\u00e4ckerei", "tokens": ["Du", "schienst", "an", "ei\u00b7ne", "B\u00e4\u00b7cke\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Freiheitsbrot im Ernst zu denken!", "tokens": ["Von", "Frei\u00b7heits\u00b7brot", "im", "Ernst", "zu", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du schienst \u2013 ja doch, es war nur Schein!", "tokens": ["Du", "schienst", "\u2013", "ja", "doch", ",", "es", "war", "nur", "Schein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Aar, du bist ein karger Reicher!", "tokens": ["O", "Aar", ",", "du", "bist", "ein", "kar\u00b7ger", "Rei\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schnell die Segel zogst du ein,", "tokens": ["Wie", "schnell", "die", "Se\u00b7gel", "zogst", "du", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schnell verschlossest du die Speicher!", "tokens": ["Wie", "schnell", "ver\u00b7schlos\u00b7sest", "du", "die", "Spei\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du gabst \u2013 doch gleich auch nahmst du \u2013 schier,", "tokens": ["Du", "gabst", "\u2013", "doch", "gleich", "auch", "nahmst", "du", "\u2013", "schier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$(", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um unsern Hunger noch zu sch\u00e4rfen;", "tokens": ["Um", "un\u00b7sern", "Hun\u00b7ger", "noch", "zu", "sch\u00e4r\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um doppelthei\u00dfe Qual und Gier", "tokens": ["Um", "dop\u00b7pel\u00b7thei\u00b7\u00dfe", "Qual", "und", "Gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In unser lechzend Herz zu werfen!", "tokens": ["In", "un\u00b7ser", "lech\u00b7zend", "Herz", "zu", "wer\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O, flieg nicht fort auf solcher Bahn!", "tokens": ["O", ",", "flieg", "nicht", "fort", "auf", "sol\u00b7cher", "Bahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brot f\u00fcr den Geist! o, woll' es brechen!", "tokens": ["Brot", "f\u00fcr", "den", "Geist", "!", "o", ",", "woll'", "es", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "FM", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gib. gib! Es k\u00f6nnte M\u00e4usezahn", "tokens": ["Gib", ".", "gib", "!", "Es", "k\u00f6nn\u00b7te", "M\u00e4u\u00b7se\u00b7zahn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "VVIMP", "$.", "PPER", "VMFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch diese Brotverweigrung r\u00e4chen!", "tokens": ["Auch", "die\u00b7se", "Brot\u00b7ver\u00b7wei\u00b7grung", "r\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, nimm die Sache nicht zu leicht!", "tokens": ["O", ",", "nimm", "die", "Sa\u00b7che", "nicht", "zu", "leicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00e4ttest du die Macht von Greifen \u2013", "tokens": ["Und", "h\u00e4t\u00b7test", "du", "die", "Macht", "von", "Grei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "APPR", "NE", "$("], "meter": "