{"textgrid.poem.61906": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach dem Walde zog der Ritter,", "tokens": ["Nach", "dem", "Wal\u00b7de", "zog", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch vor Tage zog er aus,", "tokens": ["Fr\u00fch", "vor", "Ta\u00b7ge", "zog", "er", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich ein Wildpret zu erjagen,", "tokens": ["Sich", "ein", "Wild\u00b7pret", "zu", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcg' es Klauen oder Flaum.", "tokens": ["Tr\u00fcg'", "es", "Klau\u00b7en", "o\u00b7der", "Flaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da erkannt' er auf der Heide", "tokens": ["Da", "er\u00b7kannt'", "er", "auf", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer W\u00f6lfin Spur im Tau,", "tokens": ["Ei\u00b7ner", "W\u00f6l\u00b7fin", "Spur", "im", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die frische Spur verfolgend", "tokens": ["Und", "die", "fri\u00b7sche", "Spur", "ver\u00b7fol\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch Geb\u00fcsch und Farrenkraut", "tokens": ["Durch", "Ge\u00b7b\u00fcsch", "und", "Far\u00b7ren\u00b7kraut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Fand er eine sch\u00f6ne Jungfrau", "tokens": ["Fand", "er", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Schlafend unterm Eichenbaum.", "tokens": ["Schla\u00b7fend", "un\u00b7term", "Ei\u00b7chen\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Von des Fr\u00fchrots ersten Strahlen", "tokens": ["Von", "des", "Fr\u00fch\u00b7rots", "ers\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Lag sie rosig angehaucht,", "tokens": ["Lag", "sie", "ro\u00b7sig", "an\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Nur in ihres Goldhaars Schleier", "tokens": ["Nur", "in", "ih\u00b7res", "Gold\u00b7ha\u00b7ars", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Eingeh\u00fcllt und gr\u00fcnes Laub.", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "und", "gr\u00fc\u00b7nes", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da sie reizend ihn bed\u00fcnkte,", "tokens": ["Da", "sie", "rei\u00b7zend", "ihn", "be\u00b7d\u00fcnk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Weckt' er sie mit K\u00fcssen auf,", "tokens": ["Weckt'", "er", "sie", "mit", "K\u00fcs\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Deckte sie mit seinem Mantel,", "tokens": ["Deck\u00b7te", "sie", "mit", "sei\u00b7nem", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hub sie auf sein Ro\u00df hinauf,", "tokens": ["Hub", "sie", "auf", "sein", "Ro\u00df", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und in seinen Armen f\u00fchrt' er", "tokens": ["Und", "in", "sei\u00b7nen", "Ar\u00b7men", "f\u00fchrt'", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Als Gemahl sie in sein Haus.", "tokens": ["Als", "Ge\u00b7mahl", "sie", "in", "sein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Sieben Monden dort in Freuden", "tokens": ["Sie\u00b7ben", "Mon\u00b7den", "dort", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wohnten sie als Mann und Frau,", "tokens": ["Wohn\u00b7ten", "sie", "als", "Mann", "und", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und es war umher im Lande", "tokens": ["Und", "es", "war", "um\u00b7her", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Kein begl\u00fcckter Paar zu schaun.", "tokens": ["Kein", "be\u00b7gl\u00fcck\u00b7ter", "Paar", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "N\u00e4chtens teilte sie sein Lager,", "tokens": ["N\u00e4ch\u00b7tens", "teil\u00b7te", "sie", "sein", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Tags versah sie Hof und Haus,", "tokens": ["Tags", "ver\u00b7sah", "sie", "Hof", "und", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Spann den Flachs und wob das Linnen,", "tokens": ["Spann", "den", "Flachs", "und", "wob", "das", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Sang dazu und schwatzte traut.", "tokens": ["Sang", "da\u00b7zu", "und", "schwatz\u00b7te", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "KON", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Nur befragt um ihre Herkunft,", "tokens": ["Nur", "be\u00b7fragt", "um", "ih\u00b7re", "Her\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sch\u00fcttelte sie stets das Haupt,", "tokens": ["Sch\u00fct\u00b7tel\u00b7te", "sie", "stets", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Und beschwor er sie zu reden,", "tokens": ["Und", "be\u00b7schwor", "er", "sie", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Brach sie laut in Weinen aus.", "tokens": ["Brach", "sie", "laut", "in", "Wei\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}