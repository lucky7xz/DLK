{"dta.poem.7450": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie wurden noch der Sylben mehr gemessen,", "tokens": ["Nie", "wur\u00b7den", "noch", "der", "Syl\u00b7ben", "mehr", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und glaubt man unserm kritischen Gelichter,", "tokens": ["Und", "glaubt", "man", "un\u00b7serm", "kri\u00b7ti\u00b7schen", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00e4re schier der dritte Mann ein Dichter", "tokens": ["So", "w\u00e4\u00b7re", "schier", "der", "drit\u00b7te", "Mann", "ein", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Thule bis zum Lande der Tscherkessen.", "tokens": ["Von", "Thu\u00b7le", "bis", "zum", "Lan\u00b7de", "der", "Tscher\u00b7kes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und Alle nur auf eitel Ruhm versessen,", "tokens": ["Und", "Al\u00b7le", "nur", "auf", "ei\u00b7tel", "Ruhm", "ver\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeglicher Poet begehret, spricht er", "tokens": ["Ein", "jeg\u00b7li\u00b7cher", "Po\u00b7et", "be\u00b7ge\u00b7hret", ",", "spricht", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwei Verse nur, gleich Publikum und Richter,", "tokens": ["Zwei", "Ver\u00b7se", "nur", ",", "gleich", "Pub\u00b7li\u00b7kum", "und", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00fcrd' sein Pfeifen anders bald vergessen.", "tokens": ["Und", "w\u00fcrd'", "sein", "Pfei\u00b7fen", "an\u00b7ders", "bald", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch mir d\u00e4ucht nur ein Dichter, der noch s\u00e4nge,", "tokens": ["Doch", "mir", "d\u00e4ucht", "nur", "ein", "Dich\u00b7ter", ",", "der", "noch", "s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der seinen Wohllaut noch verstr\u00f6men m\u00fc\u00dfte,", "tokens": ["Der", "sei\u00b7nen", "Wohl\u00b7laut", "noch", "ver\u00b7str\u00f6\u00b7men", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo keines Menschen Stimme zu ihm dr\u00e4nge:", "tokens": ["Wo", "kei\u00b7nes", "Men\u00b7schen", "Stim\u00b7me", "zu", "ihm", "dr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im stillen Meer an unwirtbarer K\u00fcste \u2014", "tokens": ["Im", "stil\u00b7len", "Meer", "an", "un\u00b7wirt\u00b7ba\u00b7rer", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zuh\u00f6rer nur die wilden Felsenh\u00e4nge \u2014", "tokens": ["Zu\u00b7h\u00f6\u00b7rer", "nur", "die", "wil\u00b7den", "Fel\u00b7sen\u00b7h\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in Arabiens grauenvoller W\u00fcste.", "tokens": ["Und", "in", "A\u00b7ra\u00b7biens", "grau\u00b7en\u00b7vol\u00b7ler", "W\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}