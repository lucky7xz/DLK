{"textgrid.poem.60732": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Steinen warf ein Narr nach einem weisen Mann.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Steinen warf ein Narr nach einem weisen Mann.", "tokens": ["Mit", "Stei\u00b7nen", "warf", "ein", "Narr", "nach", "ei\u00b7nem", "wei\u00b7sen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der kehrt sich jenem zu. \u00bbMein Freund,\u00ab spricht er ihn an,", "tokens": ["Der", "kehrt", "sich", "je\u00b7nem", "zu", ".", "\u00bb", "Mein", "Freund", ",", "\u00ab", "spricht", "er", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PDAT", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "$,", "$(", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u00bbdas hast du gut gemacht; nimm diesen Taler hier!", "tokens": ["\u00bb", "das", "hast", "du", "gut", "ge\u00b7macht", ";", "nimm", "die\u00b7sen", "Ta\u00b7ler", "hier", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "VVIMP", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer so wie du sich m\u00fcht, gewinne nach Geb\u00fchr,", "tokens": ["Wer", "so", "wie", "du", "sich", "m\u00fcht", ",", "ge\u00b7win\u00b7ne", "nach", "Ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOKOM", "PPER", "PRF", "ADJD", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn jede M\u00fche ist, so hei\u00dft's, des Lohnes wert.", "tokens": ["Denn", "je\u00b7de", "M\u00fc\u00b7he", "ist", ",", "so", "hei\u00dft's", ",", "des", "Loh\u00b7nes", "wert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$,", "ADV", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sieh dort den Herrn, er soll gar gro\u00dfen Reichtum haben,", "tokens": ["Sieh", "dort", "den", "Herrn", ",", "er", "soll", "gar", "gro\u00b7\u00dfen", "Reich\u00b7tum", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PPER", "VMFIN", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er zahlt dir's gl\u00e4nzend, zeigst auch ihm du deine Gaben.\u00ab", "tokens": ["Er", "zahlt", "dir's", "gl\u00e4n\u00b7zend", ",", "zeigst", "auch", "ihm", "du", "dei\u00b7ne", "Ga\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$,", "VVFIN", "ADV", "PPER", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verlockt durch den Gewinn, beehrt", "tokens": ["Ver\u00b7lockt", "durch", "den", "Ge\u00b7winn", ",", "be\u00b7ehrt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Narr auch ihn mit losen Steinen,", "tokens": ["Der", "Narr", "auch", "ihn", "mit", "lo\u00b7sen", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Geld erh\u00e4lt er diesmal nicht.", "tokens": ["Doch", "Geld", "er\u00b7h\u00e4lt", "er", "dies\u00b7mal", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Reiche ruft emp\u00f6rt nach seinen", "tokens": ["Der", "Rei\u00b7che", "ruft", "em\u00b7p\u00f6rt", "nach", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bedienten, und es regnen dicht", "tokens": ["Be\u00b7dien\u00b7ten", ",", "und", "es", "reg\u00b7nen", "dicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hiebe auf den Narren nieder.", "tokens": ["Die", "Hie\u00b7be", "auf", "den", "Nar\u00b7ren", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der warf sein Lebtag nicht mehr wieder.", "tokens": ["Der", "warf", "sein", "Leb\u00b7tag", "nicht", "mehr", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bei Hof sind manche, die's gleich jenem Narren machen.", "tokens": ["Bei", "Hof", "sind", "man\u00b7che", ",", "die's", "gleich", "je\u00b7nem", "Nar\u00b7ren", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIS", "$,", "PRELS", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Auf eure Kosten machen ihren Herrn sie lachen.", "tokens": ["Auf", "eu\u00b7re", "Kos\u00b7ten", "ma\u00b7chen", "ih\u00b7ren", "Herrn", "sie", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie abzuwehren, w\u00e4r nicht klug,", "tokens": ["Sie", "ab\u00b7zu\u00b7weh\u00b7ren", ",", "w\u00e4r", "nicht", "klug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr habt vielleicht nicht Macht genug;", "tokens": ["Ihr", "habt", "viel\u00b7leicht", "nicht", "Macht", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hetzet sie an den heran,", "tokens": ["Doch", "het\u00b7zet", "sie", "an", "den", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der sich geh\u00f6rig r\u00e4chen kann.", "tokens": ["Der", "sich", "ge\u00b7h\u00f6\u00b7rig", "r\u00e4\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}