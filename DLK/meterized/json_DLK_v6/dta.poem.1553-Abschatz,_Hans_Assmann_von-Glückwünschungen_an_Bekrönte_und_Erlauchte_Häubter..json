{"dta.poem.1553": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es war F\u00fcrst MICHAEL mein erster Br\u00e4utigam/", "tokens": ["Es", "war", "F\u00fcrst", "MiCHAEL", "mein", "ers\u00b7ter", "Br\u00e4u\u00b7ti\u00b7gam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NE", "PPOSAT", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Thron Sarmatiens f\u00fcr mich die Morgengabe.", "tokens": ["Der", "Thron", "Sar\u00b7ma\u00b7ti\u00b7ens", "f\u00fcr", "mich", "die", "Mor\u00b7gen\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich liebte wohl vergn\u00fcgt/ doch ohne Reich und Land/", "tokens": ["Mich", "lieb\u00b7te", "wohl", "ver\u00b7gn\u00fcgt", "/", "doch", "oh\u00b7ne", "Reich", "und", "Land", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Held von Lotthringen: Izt reichet mir die Hand", "tokens": ["Der", "Held", "von", "Lot\u00b7thrin\u00b7gen", ":", "Izt", "rei\u00b7chet", "mir", "die", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Der Ertz-F\u00fcrst Michael/ der F\u00fcrst aus Juda Stamm/", "tokens": ["Der", "Ertz\u00b7F\u00fcrst", "Mic\u00b7hael", "/", "der", "F\u00fcrst", "aus", "Ju\u00b7da", "Stamm", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "APPR", "NE", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wo ich um Kron und Thron nicht ferner Kummer habe.", "tokens": ["Wo", "ich", "um", "Kron", "und", "Thron", "nicht", "fer\u00b7ner", "Kum\u00b7mer", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}