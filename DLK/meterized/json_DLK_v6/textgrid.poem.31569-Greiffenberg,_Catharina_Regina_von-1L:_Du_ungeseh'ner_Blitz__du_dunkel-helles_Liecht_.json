{"textgrid.poem.31569": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du ungeseh'ner Blitz / du dunkel-helles Liecht /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ungeseh'ner Blitz / du dunkel-helles Liecht /", "tokens": ["Du", "un\u00b7ge\u00b7seh'\u00b7ner", "Blitz", "/", "du", "dun\u00b7kel\u00b7hel\u00b7les", "Liecht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du Herzerf\u00fcllte Krafft / doch unbegreifflichs Wesen", "tokens": ["du", "Her\u00b7zer\u00b7f\u00fcll\u00b7te", "Krafft", "/", "doch", "un\u00b7be\u00b7greif\u00b7flichs", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$(", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist was G\u00f6ttliches in meinem Geist gewesen /", "tokens": ["Es", "ist", "was", "G\u00f6tt\u00b7li\u00b7ches", "in", "mei\u00b7nem", "Geist", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df mich bewegt und regt: Ich sp\u00fcr ein seltnes Liecht.", "tokens": ["da\u00df", "mich", "be\u00b7wegt", "und", "regt", ":", "Ich", "sp\u00fcr", "ein", "selt\u00b7nes", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Seel ist von sich selbst nicht also l\u00f6blich liecht.", "tokens": ["Die", "Seel", "ist", "von", "sich", "selbst", "nicht", "al\u00b7so", "l\u00f6b\u00b7lich", "liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ein Wunder-Wind / ein Geist / ein webend Wesen /", "tokens": ["Es", "ist", "ein", "Wun\u00b7der\u00b7Wind", "/", "ein", "Geist", "/", "ein", "we\u00b7bend", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "die ewig' Athem-Krafft / das Erz-seyn selbst gewesen /", "tokens": ["die", "e\u00b7wig'", "A\u00b7them\u00b7Krafft", "/", "das", "Er\u00b7zseyn", "selbst", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das ihm in mir entz\u00fcnd di\u00df Himmel-flammend Liecht.", "tokens": ["das", "ihm", "in", "mir", "ent\u00b7z\u00fcnd", "di\u00df", "Him\u00b7mel\u00b7flam\u00b7mend", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "PDS", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Farben-Spiegel-Blick / du wunderbundtes Gl\u00e4nzen!", "tokens": ["Du", "Fa\u00b7rben\u00b7Spie\u00b7gel\u00b7Blick", "/", "du", "wun\u00b7der\u00b7bund\u00b7tes", "Gl\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "du schimmerst hin und her / bist unbegreiflich klar", "tokens": ["du", "schim\u00b7merst", "hin", "und", "her", "/", "bist", "un\u00b7be\u00b7greif\u00b7lich", "klar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$(", "VAFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die Geistes Taubenfl\u00fcg' in Warheits-Sonne gl\u00e4nzen.", "tokens": ["die", "Geis\u00b7tes", "Tau\u00b7ben\u00b7fl\u00fcg'", "in", "Wa\u00b7rheits\u00b7Son\u00b7ne", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Gott-bewegte Teich / ist auch getr\u00fcbet klar!", "tokens": ["Der", "Got\u00b7tbe\u00b7weg\u00b7te", "Teich", "/", "ist", "auch", "ge\u00b7tr\u00fc\u00b7bet", "klar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VAFIN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "es will erst gegen ihr die Geisies-Sonn begl\u00e4nzen", "tokens": ["es", "will", "erst", "ge\u00b7gen", "ihr", "die", "Gei\u00b7sies\u00b7Sonn", "be\u00b7gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "den Mond / dann dreht er sich / wird Erden-ab auch klar.", "tokens": ["den", "Mond", "/", "dann", "dreht", "er", "sich", "/", "wird", "Er\u00b7den\u00b7ab", "auch", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "VVFIN", "PPER", "PRF", "$(", "VAFIN", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}