{"dta.poem.3157": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df ist die Aehnlichkeit von Seel' und Schmetterling.", "tokens": ["Gro\u00df", "ist", "die", "A\u00b7ehn\u00b7lich\u00b7keit", "von", "Seel'", "und", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Doch die Verschiedenheit von beiden nicht gering.", "tokens": ["Doch", "die", "Ver\u00b7schie\u00b7den\u00b7heit", "von", "bei\u00b7den", "nicht", "ge\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Puppenmaske zeigt ein Todtenangesicht,", "tokens": ["Die", "Pup\u00b7pen\u00b7mas\u00b7ke", "zeigt", "ein", "Tod\u00b7ten\u00b7an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus dessen Ernst ein Strahl von h\u00f6herm Leben bricht;", "tokens": ["Aus", "des\u00b7sen", "Ernst", "ein", "Strahl", "von", "h\u00f6\u00b7herm", "Le\u00b7ben", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist das Gold, wovon die Chrysalide tr\u00e4gt", "tokens": ["Das", "ist", "das", "Gold", ",", "wo\u00b7von", "die", "Chry\u00b7sa\u00b7li\u00b7de", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Namen, darin ist Verkl\u00e4rung vorgepr\u00e4gt;", "tokens": ["Den", "Na\u00b7men", ",", "da\u00b7rin", "ist", "Ver\u00b7kl\u00e4\u00b7rung", "vor\u00b7ge\u00b7pr\u00e4gt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur da\u00df der Schmetterling noch in dem Sarge liegt,", "tokens": ["Nur", "da\u00df", "der", "Schmet\u00b7ter\u00b7ling", "noch", "in", "dem", "Sar\u00b7ge", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Inde\u00df vom Kerker frei die Psyche dr\u00fcber fliegt.", "tokens": ["In\u00b7de\u00df", "vom", "Ker\u00b7ker", "frei", "die", "Psy\u00b7che", "dr\u00fc\u00b7ber", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Psyche, die, wie sie sich unsichtbar gestaltet", "tokens": ["Die", "Psy\u00b7che", ",", "die", ",", "wie", "sie", "sich", "un\u00b7sicht\u00b7bar", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Leben, so im Tod unsichtbar sich entfaltet.", "tokens": ["Im", "Le\u00b7ben", ",", "so", "im", "Tod", "un\u00b7sicht\u00b7bar", "sich", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "APPRART", "NN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Schmetterling erhebt sein himmlisches Gefieder,", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", "er\u00b7hebt", "sein", "himm\u00b7li\u00b7sches", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Senkt nieder es, und heckt am Boden Raupen wieder.", "tokens": ["Senkt", "nie\u00b7der", "es", ",", "und", "heckt", "am", "Bo\u00b7den", "Rau\u00b7pen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "$,", "KON", "VVFIN", "APPRART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich aber hoffe, wenn mein Schmetterling sich hebt,", "tokens": ["Ich", "a\u00b7ber", "hof\u00b7fe", ",", "wenn", "mein", "Schmet\u00b7ter\u00b7ling", "sich", "hebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ewig erdenfrei er durch die Himmel schwebt.", "tokens": ["Da\u00df", "e\u00b7wig", "er\u00b7den\u00b7frei", "er", "durch", "die", "Him\u00b7mel", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Denn keine Blume bl\u00fcht hienieden, die aus L\u00fcften", "tokens": ["Denn", "kei\u00b7ne", "Blu\u00b7me", "bl\u00fcht", "hien\u00b7ie\u00b7den", ",", "die", "aus", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich locken k\u00f6nnte gleich dem Schmetterling mit D\u00fcften.", "tokens": ["Mich", "lo\u00b7cken", "k\u00f6nn\u00b7te", "gleich", "dem", "Schmet\u00b7ter\u00b7ling", "mit", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}