{"textgrid.poem.67791": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was bin ich, Gott? was, Herr, bin ich,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bin ich, Gott? was, Herr, bin ich,", "tokens": ["Was", "bin", "ich", ",", "Gott", "?", "was", ",", "Herr", ",", "bin", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$.", "PWS", "$,", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der's wagt, zu Dir zu singen?", "tokens": ["Der's", "wagt", ",", "zu", "Dir", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, st\u00e4rke mich! Herr l\u00e4utre mich,", "tokens": ["Herr", ",", "st\u00e4r\u00b7ke", "mich", "!", "Herr", "l\u00e4ut\u00b7re", "mich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz Dir zuzuschwingen!", "tokens": ["Mein", "Herz", "Dir", "zu\u00b7zu\u00b7schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Opfer, wie Du's nie verschm\u00e4ht,", "tokens": ["Ein", "Op\u00b7fer", ",", "wie", "Du's", "nie", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein schuldzerknirschtes Angstgebet,", "tokens": ["Ein", "schuld\u00b7zer\u00b7knirschtes", "Angst\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das noch, Herr, an Dich glaubet!", "tokens": ["Das", "noch", ",", "Herr", ",", "an", "Dich", "glau\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+++-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Wer war ich, da Du riefest mich", "tokens": ["Wer", "war", "ich", ",", "da", "Du", "rie\u00b7fest", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nanntest mich mit Namen?", "tokens": ["Und", "nann\u00b7test", "mich", "mit", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du riefst mich; Herr, ich preise Dich,", "tokens": ["Du", "riefst", "mich", ";", "Herr", ",", "ich", "prei\u00b7se", "Dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich, Dein Gesch\u00f6pf, Dein Samen!", "tokens": ["Ich", ",", "Dein", "Ge\u00b7sch\u00f6pf", ",", "Dein", "Sa\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vor Tausenden von Dir begl\u00fcckt,", "tokens": ["Vor", "Tau\u00b7sen\u00b7den", "von", "Dir", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Tausenden hat mich entz\u00fcckt", "tokens": ["Vor", "Tau\u00b7sen\u00b7den", "hat", "mich", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Wort, Herr, Deine Lehre!", "tokens": ["Dein", "Wort", ",", "Herr", ",", "Dei\u00b7ne", "Leh\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah, was, Herr, nicht Tausend sehn;", "tokens": ["Ich", "sah", ",", "was", ",", "Herr", ",", "nicht", "Tau\u00b7send", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "$,", "NN", "$,", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hast Du, Herr, zu fodern?", "tokens": ["Was", "hast", "Du", ",", "Herr", ",", "zu", "fo\u00b7dern", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich ging, wo Tausende nicht gehn;", "tokens": ["Ich", "ging", ",", "wo", "Tau\u00b7sen\u00b7de", "nicht", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr, soll mein Licht verlodern?", "tokens": ["Herr", ",", "soll", "mein", "Licht", "ver\u00b7lo\u00b7dern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein F\u00fcnklein in der Asch', es fleht,", "tokens": ["Mein", "F\u00fcn\u00b7klein", "in", "der", "Asch'", ",", "es", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es blinkt hinauf und will Gebet,", "tokens": ["Es", "blinkt", "hin\u00b7auf", "und", "will", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ach, es sinkt danieder!", "tokens": ["Und", "ach", ",", "es", "sinkt", "da\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, Jesus Christus, warst Du gleich", "tokens": ["Ach", ",", "Je\u00b7sus", "Chris\u00b7tus", ",", "warst", "Du", "gleich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "NE", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schw\u00e4chsten Deiner Br\u00fcder", "tokens": ["Dem", "Schw\u00e4chs\u00b7ten", "Dei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gingst aus Deines Vaters Reich", "tokens": ["Und", "gingst", "aus", "Dei\u00b7nes", "Va\u00b7ters", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sankst zur Erde nieder,", "tokens": ["Und", "sankst", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Aermsten, Schw\u00e4chsten gleich zu sein,", "tokens": ["Dem", "A\u00b7erms\u00b7ten", ",", "Schw\u00e4chs\u00b7ten", "gleich", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und f\u00fchltest Schw\u00e4che, M\u00fcde, Pein", "tokens": ["Und", "f\u00fchl\u00b7test", "Schw\u00e4\u00b7che", ",", "M\u00fc\u00b7de", ",", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und klagtest gottverlassen", "tokens": ["Und", "klag\u00b7test", "gott\u00b7ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und gingst hinauf in Vaters Reich,", "tokens": ["Und", "gingst", "hin\u00b7auf", "in", "Va\u00b7ters", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schw\u00e4chsten zu erh\u00f6ren!", "tokens": ["Den", "Schw\u00e4chs\u00b7ten", "zu", "er\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schw\u00e4chste soll Dir werden gleich", "tokens": ["Der", "Schw\u00e4chs\u00b7te", "soll", "Dir", "wer\u00b7den", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Sieg und Lohn und Ehren.", "tokens": ["An", "Sieg", "und", "Lohn", "und", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr, wo Du flehtest, fleh' auch ich!", "tokens": ["Herr", ",", "wo", "Du", "fleh\u00b7test", ",", "fleh'", "auch", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Erh\u00f6rter, ach! erh\u00f6r auch mich!", "tokens": ["Er\u00b7h\u00f6r\u00b7ter", ",", "ach", "!", "er\u00b7h\u00f6r", "auch", "mich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$.", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hilf mir zu Deinem Bilde!", "tokens": ["Hilf", "mir", "zu", "Dei\u00b7nem", "Bil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zu Deinem Bild, o Menschensohn", "tokens": ["Zu", "Dei\u00b7nem", "Bild", ",", "o", "Men\u00b7schen\u00b7sohn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gottes Sohn dort oben!", "tokens": ["Und", "Got\u00b7tes", "Sohn", "dort", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich, auch ich Dich k\u00f6nn' am Thron", "tokens": ["Da\u00df", "ich", ",", "auch", "ich", "Dich", "k\u00f6nn'", "am", "Thron"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "PPER", "PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schon im Staube loben!", "tokens": ["Und", "schon", "im", "Stau\u00b7be", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich, auch ich, schon Dich hier seh',", "tokens": ["Da\u00df", "ich", ",", "auch", "ich", ",", "schon", "Dich", "hier", "seh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "PPER", "$,", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon hier von Deinem Geiste weh',", "tokens": ["Schon", "hier", "von", "Dei\u00b7nem", "Geis\u00b7te", "weh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weh' in mich Kraft des Lebens!", "tokens": ["Weh'", "in", "mich", "Kraft", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Und meine Zunge singe Preis,", "tokens": ["Und", "mei\u00b7ne", "Zun\u00b7ge", "sin\u00b7ge", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Dank mein Herz Dir schlage,", "tokens": ["Und", "Dank", "mein", "Herz", "Dir", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Stirn in Todesschwei\u00df", "tokens": ["Und", "mei\u00b7ne", "Stirn", "in", "To\u00b7des\u00b7schwei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich nicht mehr, Herr, verklage,", "tokens": ["Dich", "nicht", "mehr", ",", "Herr", ",", "ver\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dir gl\u00e4nze, Herr, von Deinem Licht,", "tokens": ["Dir", "gl\u00e4n\u00b7ze", ",", "Herr", ",", "von", "Dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und all mein Todtenangesicht", "tokens": ["Und", "all", "mein", "Tod\u00b7ten\u00b7an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Licht, o Herr, belebe!", "tokens": ["Dein", "Licht", ",", "o", "Herr", ",", "be\u00b7le\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und all mein Todtenleichnam weh',", "tokens": ["Und", "all", "mein", "Tod\u00b7ten\u00b7leich\u00b7nam", "weh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weh' auf von Kraft des Lebens,", "tokens": ["Weh'", "auf", "von", "Kraft", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ach! mein bl\u00f6des Auge seh',", "tokens": ["Und", "ach", "!", "mein", "bl\u00f6\u00b7des", "Au\u00b7ge", "seh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh' nimmermehr vergebens", "tokens": ["Seh'", "nim\u00b7mer\u00b7mehr", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Gotteslicht! Es werde mir", "tokens": ["Dein", "Got\u00b7tes\u00b7licht", "!", "Es", "wer\u00b7de", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Flamme, die mich, Herr, vor Dir", "tokens": ["Zur", "Flam\u00b7me", ",", "die", "mich", ",", "Herr", ",", "vo