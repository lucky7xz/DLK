{"dta.poem.16952": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "53.  Was GOtt am liebsten ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist das GOtt so sehr als eine Jungfrau liebt/", "tokens": ["Nichts", "ist", "das", "Gott", "so", "sehr", "als", "ei\u00b7ne", "Jung\u00b7frau", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADV", "ADV", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er auch jhr sich selbst zur Frucht und Kind ergiebt:", "tokens": ["Da\u00df", "er", "auch", "jhr", "sich", "selbst", "zur", "Frucht", "und", "Kind", "er\u00b7giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PRF", "ADV", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wilstu seyn liebstes seyn noch hier auf dieser Erden/", "tokens": ["Wils\u00b7tu", "seyn", "liebs\u00b7tes", "seyn", "noch", "hier", "auf", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So darffstu anders nichts als seine Jungfrau werden.", "tokens": ["So", "darffs\u00b7tu", "an\u00b7ders", "nichts", "als", "sei\u00b7ne", "Jung\u00b7frau", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "PIS", "KOKOM", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}