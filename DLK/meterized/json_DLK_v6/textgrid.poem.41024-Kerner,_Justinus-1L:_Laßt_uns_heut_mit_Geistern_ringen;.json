{"textgrid.poem.41024": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft uns heut mit Geistern ringen;", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns heut mit Geistern ringen;", "tokens": ["La\u00dft", "uns", "heut", "mit", "Geis\u00b7tern", "rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt der Alte noch so klar,", "tokens": ["Blickt", "der", "Al\u00b7te", "noch", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringet jetzt den Neuen dar,", "tokens": ["Brin\u00b7get", "jetzt", "den", "Neu\u00b7en", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dem Kerker will entspringen!", "tokens": ["Der", "dem", "Ker\u00b7ker", "will", "ent\u00b7sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt sein unterirdisch Beben!", "tokens": ["H\u00f6rt", "sein", "un\u00b7ter\u00b7ir\u00b7disch", "Be\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Nacht will er hinaus,", "tokens": ["Aus", "der", "Nacht", "will", "er", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtig dringt sein Geist durchs Haus,", "tokens": ["M\u00e4ch\u00b7tig", "dringt", "sein", "Geist", "durchs", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir stehn von ihm umgeben.", "tokens": ["Da\u00df", "wir", "stehn", "von", "ihm", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Horcht! der wei\u00df von Jugendwonne", "tokens": ["Horcht", "!", "der", "wei\u00df", "von", "Ju\u00b7gend\u00b7won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch zu singen euch ein Lied:", "tokens": ["Noch", "zu", "sin\u00b7gen", "euch", "ein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er hat in Duft gebl\u00fcht,", "tokens": ["Wie", "er", "hat", "in", "Duft", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihn hat durchgl\u00fcht die Sonne;", "tokens": ["Wie", "ihn", "hat", "durch\u00b7gl\u00fcht", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie von hohen Bergen nieder", "tokens": ["Wie", "von", "ho\u00b7hen", "Ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei er sah die Welt entlang,", "tokens": ["Frei", "er", "sah", "die", "Welt", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter ihm der Flu\u00dfgott sang,", "tokens": ["Un\u00b7ter", "ihm", "der", "Flu\u00df\u00b7gott", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um ihn t\u00f6nten Vogellieder;", "tokens": ["Um", "ihn", "t\u00f6n\u00b7ten", "Vo\u00b7gel\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie mit Sonn' und Stern' im Bunde", "tokens": ["Wie", "mit", "Sonn'", "und", "Stern'", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4hlich seine Traube schwoll,", "tokens": ["M\u00e4h\u00b7lich", "sei\u00b7ne", "Trau\u00b7be", "schwoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sie war des Saftes voll;", "tokens": ["Bis", "sie", "war", "des", "Saf\u00b7tes", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der von Geistern nun gibt Kunde.", "tokens": ["Der", "von", "Geis\u00b7tern", "nun", "gibt", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcllet mutig bis zum Rande", "tokens": ["F\u00fcl\u00b7let", "mu\u00b7tig", "bis", "zum", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Pokal mit seiner Glut!", "tokens": ["Den", "Po\u00b7kal", "mit", "sei\u00b7ner", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sto\u00dfet an! Dem Jugendblut", "tokens": ["Sto\u00b7\u00dfet", "an", "!", "Dem", "Ju\u00b7gend\u00b7blut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil im weiten deutschen Lande!", "tokens": ["Heil", "im", "wei\u00b7ten", "deut\u00b7schen", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach! es liegt erstarrt, veraltet", "tokens": ["Ach", "!", "es", "liegt", "er\u00b7starrt", ",", "ver\u00b7al\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "VVPP", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher V\u00f6lker gro\u00dfes Herz,", "tokens": ["Man\u00b7cher", "V\u00f6l\u00b7ker", "gro\u00b7\u00dfes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jugendw\u00e4rme, Lust und Scherz", "tokens": ["Ju\u00b7gend\u00b7w\u00e4r\u00b7me", ",", "Lust", "und", "Scherz"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind in ihrer Brust erkaltet.", "tokens": ["Sind", "in", "ih\u00b7rer", "Brust", "er\u00b7kal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00dft der Jugend warmes Leben", "tokens": ["La\u00dft", "der", "Ju\u00b7gend", "war\u00b7mes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6men euch ins Herz hinein.", "tokens": ["Str\u00f6\u00b7men", "euch", "ins", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinkt in Lust den neuen Wein,", "tokens": ["Trinkt", "in", "Lust", "den", "neu\u00b7en", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den der neue Stern gegeben!", "tokens": ["Den", "der", "neu\u00b7e", "Stern", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}