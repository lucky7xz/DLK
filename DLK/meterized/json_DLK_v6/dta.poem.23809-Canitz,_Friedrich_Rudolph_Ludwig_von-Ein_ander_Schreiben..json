{"dta.poem.23809": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ein ander Schreiben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Bruder ich bin froh/ da\u00df deine werthe Schrifft", "tokens": ["Herr", "Bru\u00b7der", "ich", "bin", "froh", "/", "da\u00df", "dei\u00b7ne", "wert\u00b7he", "Schrifft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "VAFIN", "ADJD", "$(", "KOUS", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dem was mich erg\u00f6tzt/ so wol zusammen trifft", "tokens": ["Mit", "dem", "was", "mich", "er\u00b7g\u00f6tzt", "/", "so", "wol", "zu\u00b7sam\u00b7men", "trifft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PWS", "PPER", "VVPP", "$(", "ADV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich es wagen darf/ da ich aus dem Gedr\u00e4nge", "tokens": ["Da\u00df", "ich", "es", "wa\u00b7gen", "darf", "/", "da", "ich", "aus", "dem", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$(", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Hofes m\u00fc\u00dfig geh/ erbauliche Ges\u00e4nge", "tokens": ["Des", "Ho\u00b7fes", "m\u00fc\u00b7\u00dfig", "geh", "/", "er\u00b7bau\u00b7li\u00b7che", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit dir zu stimmen an/ und da\u00df in unserm Geist", "tokens": ["Mit", "dir", "zu", "stim\u00b7men", "an", "/", "und", "da\u00df", "in", "un\u00b7serm", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVFIN", "PTKVZ", "$(", "KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das alte Schrodt und Korn sich ohne Zusatz weist.", "tokens": ["Das", "al\u00b7te", "Schrodt", "und", "Korn", "sich", "oh\u00b7ne", "Zu\u00b7satz", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Begl\u00fccktes Vaterland das uns hat auferzogen/", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Va\u00b7ter\u00b7land", "das", "uns", "hat", "auf\u00b7er\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wir noch gl\u00fccklicher/ da\u00df uns nicht hat betrogen", "tokens": ["Und", "wir", "noch", "gl\u00fcck\u00b7li\u00b7cher", "/", "da\u00df", "uns", "nicht", "hat", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD", "$(", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das eitle Gauc", "tokens": ["Das", "eit\u00b7le", "Gauc"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das dir zum zweyten mahl mein Kiel gewidmet hat;", "tokens": ["Das", "dir", "zum", "zwey\u00b7ten", "mahl", "mein", "Kiel", "ge\u00b7wid\u00b7met", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "ADJA", "ADV", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der sol wenn du ihn wirst mit gleicher Lust erwecken/", "tokens": ["Der", "sol", "wenn", "du", "ihn", "wirst", "mit", "glei\u00b7cher", "Lust", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KOUS", "PPER", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dir meine Phantasey noch mehr und mehr entdecken/", "tokens": ["Dir", "mei\u00b7ne", "Phan\u00b7ta\u00b7sey", "noch", "mehr", "und", "mehr", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn du bist nicht ein Mann nach Art der neuen Welt/", "tokens": ["Denn", "du", "bist", "nicht", "ein", "Mann", "nach", "Art", "der", "neu\u00b7en", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der den Machiavell f\u00fcr sein Gebet-Buch h\u00e4lt/", "tokens": ["Der", "den", "Ma\u00b7chi\u00b7a\u00b7vell", "f\u00fcr", "sein", "Ge\u00b7bet\u00b7Buch", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.15": {"text": "Der sich blo\u00df auf die Kunst den Hof zu schmeicheln leget/", "tokens": ["Der", "sich", "blo\u00df", "auf", "die", "Kunst", "den", "Hof", "zu", "schmei\u00b7cheln", "le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und einen Juncker kaum Herr Ohm zu nennen pfleget/", "tokens": ["Und", "ei\u00b7nen", "Jun\u00b7cker", "kaum", "Herr", "Ohm", "zu", "nen\u00b7nen", "pfle\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Gl\u00fcck ist dir zu starck/ das dich bemeistern kan/", "tokens": ["Kein", "Gl\u00fcck", "ist", "dir", "zu", "starck", "/", "das", "dich", "be\u00b7meis\u00b7tern", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dir legt kein F\u00fcrsten-Blick die g\u00fcldne Fessel an;", "tokens": ["Dir", "legt", "kein", "F\u00fcrs\u00b7ten\u00b7Blick", "die", "g\u00fcld\u00b7ne", "Fes\u00b7sel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du lebst wie mancher nicht als Last-Vieh angebunden/", "tokens": ["Du", "lebst", "wie", "man\u00b7cher", "nicht", "als", "Last\u00b7Vieh", "an\u00b7ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PIS", "PTKNEG", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was du der Herrschafft stiehlst/ das sind vergn\u00fcgte", "tokens": ["Was", "du", "der", "Herr\u00b7schafft", "stiehlst", "/", "das", "sind", "ver\u00b7gn\u00fcg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$(", "PDS", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Kein fremdes Wohlergehn ists was dein Hertze nagt/", "tokens": ["Kein", "frem\u00b7des", "Woh\u00b7ler\u00b7gehn", "ists", "was", "dein", "Hert\u00b7ze", "nagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PIS", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mir ist nicht unbewust/ da\u00df dir ein Schertz behagt/", "tokens": ["Mir", "ist", "nicht", "un\u00b7be\u00b7wust", "/", "da\u00df", "dir", "ein", "Schertz", "be\u00b7hagt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn nur ein freyes Wort/ das uns die Zeit verk\u00fcrtzet/", "tokens": ["Wenn", "nur", "ein", "frey\u00b7es", "Wort", "/", "das", "uns", "die", "Zeit", "ver\u00b7k\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Richt seinen Honig-Seim mit Coloquinten w\u00fcrtzet/", "tokens": ["Richt", "sei\u00b7nen", "Ho\u00b7nig\u00b7Seim", "mit", "Co\u00b7loq\u00b7uin\u00b7ten", "w\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und nur kein heimlich Gifft den Nechsten sticht und schilt/", "tokens": ["Und", "nur", "kein", "heim\u00b7lich", "Gifft", "den", "Nechs\u00b7ten", "sticht", "und", "schilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df manchem Papagey der Kopff von Eyffer schwilt.", "tokens": ["Da\u00df", "man\u00b7chem", "Pa\u00b7pa\u00b7gey", "der", "Kopff", "von", "Eyf\u00b7fer", "schwilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du forderst keinen Pracht der k\u00f6stlichen Pancketen/", "tokens": ["Du", "for\u00b7derst", "kei\u00b7nen", "Pracht", "der", "k\u00f6st\u00b7li\u00b7chen", "Pan\u00b7cke\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "F\u00fcr dir darff keiner nicht mit schlechter Kost err\u00f6then;", "tokens": ["F\u00fcr", "dir", "darff", "kei\u00b7ner", "nicht", "mit", "schlech\u00b7ter", "Kost", "er\u00b7r\u00f6\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich wei\u00df da\u00df du die Zeit mit Wirthschafft oft vertreibst/", "tokens": ["Ich", "wei\u00df", "da\u00df", "du", "die", "Zeit", "mit", "Wirth\u00b7schafft", "oft", "ver\u00b7treibst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und selbst wie Plinius und Columella schreibst.", "tokens": ["Und", "selbst", "wie", "Pli\u00b7nius", "und", "Co\u00b7lu\u00b7mel\u00b7la", "schreibst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NE", "KON", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Wird doch kein B\u00fccher-Saal im Teutschen Reich ge-", "tokens": ["Wird", "doch", "kein", "B\u00fc\u00b7cher\u00b7Saal", "im", "Teut\u00b7schen", "Reich", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Da nicht Eusebius in Pergament gebunden;", "tokens": ["Da", "nicht", "Eu\u00b7se\u00b7bius", "in", "Per\u00b7ga\u00b7ment", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NE", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.33": {"text": "Durch Hohbergs treuen Flei\u00df die sp\u00e4te Nachwelt lehrt/", "tokens": ["Durch", "Hoh\u00b7bergs", "treu\u00b7en", "Flei\u00df", "die", "sp\u00e4\u00b7te", "Nach\u00b7welt", "lehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie die Murene sich in seinen Wassern mehrt.", "tokens": ["Wie", "die", "Mu\u00b7re\u00b7ne", "sich", "in", "sei\u00b7nen", "Was\u00b7sern", "mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So soll denn alle Frucht/ die mein Gehirn gebiehret/", "tokens": ["So", "soll", "denn", "al\u00b7le", "Frucht", "/", "die", "mein", "Ge\u00b7hirn", "ge\u00b7bieh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PIAT", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil uns doch gleicher Sinn zum gleichen Handwerck", "tokens": ["Weil", "uns", "doch", "glei\u00b7cher", "Sinn", "zum", "glei\u00b7chen", "Hand\u00b7werck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Dir k\u00fcnfftig eigen seyn/ wenn nur nicht Grie\u00df und Gicht", "tokens": ["Dir", "k\u00fcnff\u00b7tig", "ei\u00b7gen", "seyn", "/", "wenn", "nur", "nicht", "Grie\u00df", "und", "Gicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "VAINF", "$(", "KOUS", "ADV", "PTKNEG", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Unschuld-volle Lust zu zeitig unterbricht.", "tokens": ["Die", "Un\u00b7schuld\u00b7vol\u00b7le", "Lust", "zu", "zei\u00b7tig", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nimm dis zur Antwort hin auf die geehrte Zeilen/", "tokens": ["Nimm", "dis", "zur", "Ant\u00b7wort", "hin", "auf", "die", "ge\u00b7ehr\u00b7te", "Zei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "APPRART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die gestern dir beliebt mir wieder zu ertheilen/", "tokens": ["Die", "ge\u00b7stern", "dir", "be\u00b7liebt", "mir", "wie\u00b7der", "zu", "er\u00b7thei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Nun send ich werthster Freund den Danck der dir geb\u00fchrt/", "tokens": ["Nun", "send", "ich", "werths\u00b7ter", "Freund", "den", "Danck", "der", "dir", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PPER", "ADJA", "NN", "ART", "NN", "ART", "PPER", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.42": {"text": "Da\u00df schon dein muntrer Knecht die R\u00e4der hat geschmiert/", "tokens": ["Da\u00df", "schon", "dein", "mun\u00b7trer", "Knecht", "die", "R\u00e4\u00b7der", "hat", "ge\u00b7schmiert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Damit du desto eh\u2019/ mit den geliebten Deinen/", "tokens": ["Da\u00b7mit", "du", "des\u00b7to", "eh'", "/", "mit", "den", "ge\u00b7lieb\u00b7ten", "Dei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "$(", "APPR", "ART", "ADJA", "PPOSAT", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Auff meinem Meyerhof/ am Freytag kanst erscheinen.", "tokens": ["Auff", "mei\u00b7nem", "Me\u00b7yer\u00b7hof", "/", "am", "Frey\u00b7tag", "kanst", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Fort Gelben! bi\u00df der Trab euch das Gebi\u00df besch\u00e4umt/", "tokens": ["Fort", "Gel\u00b7ben", "!", "bi\u00df", "der", "Trab", "euch", "das", "Ge\u00b7bi\u00df", "be\u00b7sch\u00e4umt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Euch ist schon Kripp und Stall beyzeiten ausger\u00e4umt.", "tokens": ["Euch", "ist", "schon", "Kripp", "und", "Stall", "bey\u00b7zei\u00b7ten", "aus\u00