{"textgrid.poem.38910": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Holdes, holdes Sehnsuchtrufen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdes, holdes Sehnsuchtrufen", "tokens": ["Hol\u00b7des", ",", "hol\u00b7des", "Sehn\u00b7sucht\u00b7ru\u00b7fen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Wald, vom Thale herauf:", "tokens": ["Aus", "dem", "Wald", ",", "vom", "Tha\u00b7le", "her\u00b7auf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Klimm' herab die Felsenstufen,", "tokens": ["Klimm'", "her\u00b7ab", "die", "Fel\u00b7sen\u00b7stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folge diesem Locken, Rufen", "tokens": ["Fol\u00b7ge", "die\u00b7sem", "Lo\u00b7cken", ",", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PDAT", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hofnung thut sich, Gl\u00fcck dir auf.", "tokens": ["Hof\u00b7nung", "thut", "sich", ",", "Gl\u00fcck", "dir", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl seh' ich Gestalten wanken", "tokens": ["Wohl", "seh'", "ich", "Ge\u00b7stal\u00b7ten", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch des Waldes gr\u00fcne Nacht,", "tokens": ["Durch", "des", "Wal\u00b7des", "gr\u00fc\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die bewegten Zweige schwanken,", "tokens": ["Die", "be\u00b7weg\u00b7ten", "Zwei\u00b7ge", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie entschimmern wie Gedanken,", "tokens": ["Sie", "ent\u00b7schim\u00b7mern", "wie", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Schlaf hinweg gefacht.", "tokens": ["Die", "der", "Schlaf", "hin\u00b7weg", "ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APZR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm' Erinnrung, liebe Treue,", "tokens": ["Komm'", "E\u00b7rinn\u00b7rung", ",", "lie\u00b7be", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir oft im Arm geruht,", "tokens": ["Die", "mir", "oft", "im", "Arm", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe mir dein Lied, erfreue", "tokens": ["Sin\u00b7ge", "mir", "dein", "Lied", ",", "er\u00b7freu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses matte Herz, der Scheue", "tokens": ["Die\u00b7ses", "mat\u00b7te", "Herz", ",", "der", "Scheu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlt dann Kraft und Lebensmuth.", "tokens": ["F\u00fchlt", "dann", "Kraft", "und", "Le\u00b7bens\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kinder lieben ja die Scherze,", "tokens": ["Kin\u00b7der", "lie\u00b7ben", "ja", "die", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich bin ein th\u00f6richt Kind,", "tokens": ["Und", "ich", "bin", "ein", "th\u00f6\u00b7richt", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu verblieb dir doch mein Herze,", "tokens": ["Treu", "ver\u00b7blieb", "dir", "doch", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leichtsinn nur im frohen Scherze,", "tokens": ["Leicht\u00b7sinn", "nur", "im", "fro\u00b7hen", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bin noch so wie sonst gesinnt.", "tokens": ["Bin", "noch", "so", "wie", "sonst", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOKOM", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wald und Thal, ihr gr\u00fcne H\u00fcgel", "tokens": ["Wald", "und", "Thal", ",", "ihr", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennt die W\u00fcnsche meiner Brust,", "tokens": ["Kennt", "die", "W\u00fcn\u00b7sche", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich gern mit goldnem Fl\u00fcgel", "tokens": ["Wie", "ich", "gern", "mit", "gold\u00b7nem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Abendr\u00f6the H\u00fcgel", "tokens": ["Von", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6chte ziehn zu meiner Lust.", "tokens": ["M\u00f6ch\u00b7te", "ziehn", "zu", "mei\u00b7ner", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Erd' und Himmel nun in K\u00fcssen", "tokens": ["Erd'", "und", "Him\u00b7mel", "nun", "in", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit Liebesschaam entbrennt; \u2013", "tokens": ["Wie", "mit", "Lie\u00b7bes\u00b7schaam", "ent\u00b7brennt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ich mu\u00df den Frevel b\u00fcssen,", "tokens": ["Ach", "!", "ich", "mu\u00df", "den", "Fre\u00b7vel", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange noch die Holde missen", "tokens": ["Lan\u00b7ge", "noch", "die", "Hol\u00b7de", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mein ganzes Herze nennt.", "tokens": ["Die", "mein", "gan\u00b7zes", "Her\u00b7ze", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Morgenr\u00f6the kommt gegangen,", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7the", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht den Tag von Banden frei,", "tokens": ["Macht", "den", "Tag", "von", "Ban\u00b7den", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erd' und Himmel br\u00e4utlich prangen:", "tokens": ["Erd'", "und", "Him\u00b7mel", "br\u00e4ut\u00b7lich", "pran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ach! ich bin gefangen,", "tokens": ["A\u00b7ber", "ach", "!", "ich", "bin", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einsam hier im s\u00fc\u00dfen May.", "tokens": ["Ein\u00b7sam", "hier", "im", "s\u00fc\u00b7\u00dfen", "May", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lieb' und Mailust ist verschwunden,", "tokens": ["Lieb'", "und", "Mai\u00b7lust", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nur Mai in ihrem Blick,", "tokens": ["Ist", "nur", "Mai", "in", "ih\u00b7rem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Rose wird erfunden; \u2013", "tokens": ["Kei\u00b7ne", "Ro\u00b7se", "wird", "er\u00b7fun\u00b7den", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flieht und eilt ihr tr\u00e4gen Stunden,", "tokens": ["Flieht", "und", "eilt", "ihr", "tr\u00e4\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bringt die Braut mir bald zur\u00fcck!", "tokens": ["Bringt", "die", "Braut", "mir", "bald", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}