{"textgrid.poem.41540": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am ersten Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und hast du deinen Frieden denn gegeben", "tokens": ["Und", "hast", "du", "dei\u00b7nen", "Frie\u00b7den", "denn", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An alle, die sich sehnen um dein Heil,", "tokens": ["An", "al\u00b7le", ",", "die", "sich", "seh\u00b7nen", "um", "dein", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So will ich meine Stimme auch erheben:", "tokens": ["So", "will", "ich", "mei\u00b7ne", "Stim\u00b7me", "auch", "er\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier bin ich, Vater, gib auch mir mein Teil!", "tokens": ["Hier", "bin", "ich", ",", "Va\u00b7ter", ",", "gib", "auch", "mir", "mein", "Teil", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "VVIMP", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Warum sollt' ich, ein ausgeschlo\u00dfnes Kind,", "tokens": ["Wa\u00b7rum", "sollt'", "ich", ",", "ein", "aus\u00b7ge\u00b7schlo\u00df\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Allein verschmachtend um mein Erbe weinen?", "tokens": ["Al\u00b7lein", "ver\u00b7schmach\u00b7tend", "um", "mein", "Er\u00b7be", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Warum nicht sollte deine Sonne scheinen,", "tokens": ["Wa\u00b7rum", "nicht", "soll\u00b7te", "dei\u00b7ne", "Son\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo doch im Boden gute Keime sind?", "tokens": ["Wo", "doch", "im", "Bo\u00b7den", "gu\u00b7te", "Kei\u00b7me", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft mein' ich zwar, zum Beten sei genommen", "tokens": ["Oft", "mein'", "ich", "zwar", ",", "zum", "Be\u00b7ten", "sei", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir alles Recht, da es so tr\u00fcb und lau;", "tokens": ["Mir", "al\u00b7les", "Recht", ",", "da", "es", "so", "tr\u00fcb", "und", "lau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir k\u00f6nne nur geduldig Harren frommen,", "tokens": ["Mir", "k\u00f6n\u00b7ne", "nur", "ge\u00b7dul\u00b7dig", "Har\u00b7ren", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und starrer Aufblick zu des Himmels Blau;", "tokens": ["Und", "star\u00b7rer", "Auf\u00b7blick", "zu", "des", "Him\u00b7mels", "Blau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch, Herr! der du dem Z\u00f6llner dich gesellt,", "tokens": ["Doch", ",", "Herr", "!", "der", "du", "dem", "Z\u00f6ll\u00b7ner", "dich", "ge\u00b7sellt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "ART", "NE", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O la\u00df nicht zu, da\u00df ich in Nacht verschwimme,", "tokens": ["O", "la\u00df", "nicht", "zu", ",", "da\u00df", "ich", "in", "Nacht", "ver\u00b7schwim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dem irren Lamme ruft ja deine Stimme,", "tokens": ["Dem", "ir\u00b7ren", "Lam\u00b7me", "ruft", "ja", "dei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und um den S\u00fcnder kamst du in die Welt.", "tokens": ["Und", "um", "den", "S\u00fcn\u00b7der", "kamst", "du", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl wei\u00df ich, wie es steht in meiner Seelen,", "tokens": ["Wohl", "wei\u00df", "ich", ",", "wie", "es", "steht", "in", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie glaubensarm, wie trotzig und verwirrt,", "tokens": ["Wie", "glau\u00b7bens\u00b7arm", ",", "wie", "trot\u00b7zig", "und", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach da\u00df sich, da\u00df sich manches mochte hehlen;", "tokens": ["Ach", "da\u00df", "sich", ",", "da\u00df", "sich", "man\u00b7ches", "moch\u00b7te", "heh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PRF", "$,", "KOUS", "PRF", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich f\u00fchle, wie es durch die Nerven schwirrt,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "es", "durch", "die", "Ner\u00b7ven", "schwirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und kraftlos folg' ich seiner tr\u00fcben Spur.", "tokens": ["Und", "kraft\u00b7los", "fol\u00b7g'", "ich", "sei\u00b7ner", "tr\u00fc\u00b7ben", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mein Helfer, was ich nimmer mag ergr\u00fcnden,", "tokens": ["Mein", "Hel\u00b7fer", ",", "was", "ich", "nim\u00b7mer", "mag", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du kennst es wohl, du wei\u00dft es wohl zu finden,", "tokens": ["Du", "kennst", "es", "wohl", ",", "du", "wei\u00dft", "es", "wohl", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du bist der Arzt, ich bin der Kranke nur.", "tokens": ["Du", "bist", "der", "Arzt", ",", "ich", "bin", "der", "Kran\u00b7ke", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und hast du tief geschaut in meine S\u00fcnden,", "tokens": ["Und", "hast", "du", "tief", "ge\u00b7schaut", "in", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie nicht ein Menschenauge schauen kann;", "tokens": ["Wie", "nicht", "ein", "Men\u00b7schen\u00b7au\u00b7ge", "schau\u00b7en", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du gesehn, wie in den tiefsten Gr\u00fcnden", "tokens": ["Hast", "du", "ge\u00b7sehn", ",", "wie", "in", "den", "tiefs\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch schlummert mancher w\u00fcste, dunkle Wahn:", "tokens": ["Noch", "schlum\u00b7mert", "man\u00b7cher", "w\u00fcs\u00b7te", ",", "dunk\u00b7le", "Wahn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wei\u00df ich auch, da\u00df keine Tr\u00e4n' entschleicht,", "tokens": ["Doch", "wei\u00df", "ich", "auch", ",", "da\u00df", "kei\u00b7ne", "Tr\u00e4n'", "ent\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die deine treue Hand nicht hat gewogen,", "tokens": ["Die", "dei\u00b7ne", "treu\u00b7e", "Hand", "nicht", "hat", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und da\u00df kein Seufzer dieser Brust entflogen,", "tokens": ["Und", "da\u00df", "kein", "Seuf\u00b7zer", "die\u00b7ser", "Brust", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der dein barmherzig Ohr nicht hat erreicht.", "tokens": ["Der", "dein", "barm\u00b7her\u00b7zig", "Ohr", "nicht", "hat", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du, der verschlo\u00dfne T\u00fcren kann durchdringen,", "tokens": ["Du", ",", "der", "ver\u00b7schlo\u00df\u00b7ne", "T\u00fc\u00b7ren", "kann", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieh, meine Brust ist ein verschlo\u00dfnes Tor,", "tokens": ["Sieh", ",", "mei\u00b7ne", "Brust", "ist", "ein", "ver\u00b7schlo\u00df\u00b7nes", "Tor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu matt bin ich die Riegel zu bezwingen;", "tokens": ["Zu", "matt", "bin", "ich", "die", "Rie\u00b7gel", "zu", "be\u00b7zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch siehst du, wie ich angstvoll steh davor.", "tokens": ["Doch", "siehst", "du", ",", "wie", "ich", "angst\u00b7voll", "steh", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Brich ein! brich ein! o komm mit deiner Macht,", "tokens": ["Brich", "ein", "!", "brich", "ein", "!", "o", "komm", "mit", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADJD", "PTKVZ", "$.", "FM", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df Liebe gelten, da gering der Glaube,", "tokens": ["La\u00df", "Lie\u00b7be", "gel\u00b7ten", ",", "da", "ge\u00b7ring", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O la\u00df mich schauen deine Friedenstaube,", "tokens": ["O", "la\u00df", "mich", "schau\u00b7en", "dei\u00b7ne", "Frie\u00b7dens\u00b7tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "La\u00df fallen deinen Strahl in meine Nacht!", "tokens": ["La\u00df", "fal\u00b7len", "dei\u00b7nen", "Strahl", "in", "mei\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nicht weich' ich, eh ich einen Schein gesehen,", "tokens": ["Nicht", "weich'", "ich", ",", "eh", "ich", "ei\u00b7nen", "Schein", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00e4r' er schwach wie Wurmes Flimmer auch;", "tokens": ["Und", "w\u00e4r'", "er", "schwach", "wie", "Wur\u00b7mes", "Flim\u00b7mer", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht von dieser Schwelle will ich gehen,", "tokens": ["Und", "nicht", "von", "die\u00b7ser", "Schwel\u00b7le", "will", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis ich vernommen deiner Stimme Hauch,", "tokens": ["Bis", "ich", "ver\u00b7nom\u00b7men", "dei\u00b7ner", "Stim\u00b7me", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So sprich, mein Vater, sprich denn auch zu mir", "tokens": ["So", "sprich", ",", "mein", "Va\u00b7ter", ",", "sprich", "denn", "auch", "zu", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"te