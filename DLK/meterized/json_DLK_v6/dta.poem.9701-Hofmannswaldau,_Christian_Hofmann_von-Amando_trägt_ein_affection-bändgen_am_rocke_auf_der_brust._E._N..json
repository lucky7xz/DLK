{"dta.poem.9701": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Amando tr\u00e4gt ein affection-b\u00e4ndgen am  \n rocke auf der brust.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Amando trit nunmehr in einen ritter-orden/", "tokens": ["A\u00b7man\u00b7do", "trit", "nun\u00b7mehr", "in", "ei\u00b7nen", "rit\u00b7ter\u00b7or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wo lauter hasen seyn. Ein b\u00e4ndgen ist sein ruhm/", "tokens": ["Wo", "lau\u00b7ter", "ha\u00b7sen", "seyn", ".", "Ein", "b\u00e4nd\u00b7gen", "ist", "sein", "ruhm", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "VAINF", "$.", "ART", "ADJA", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ihm die liebste gab. Er nennts ein heiligthum.", "tokens": ["Das", "ihm", "die", "liebs\u00b7te", "gab", ".", "Er", "nennts", "ein", "hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist gr\u00f6\u00dfre fantasie auch ie erh\u00f6ret worden?", "tokens": ["Ist", "gr\u00f6\u00df\u00b7re", "fan\u00b7ta\u00b7sie", "auch", "ie", "er\u00b7h\u00f6\u00b7ret", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "ADV", "VVFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gehts. Er bauet nun auf dieses band sein heil:", "tokens": ["So", "gehts", ".", "Er", "bau\u00b7et", "nun", "auf", "die\u00b7ses", "band", "sein", "heil", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wenn mans recht besieht/ so ists ein narren-seil.", "tokens": ["Doch", "wenn", "mans", "recht", "be\u00b7sieht", "/", "so", "ists", "ein", "nar\u00b7ren\u00b7seil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VVFIN", "$(", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}