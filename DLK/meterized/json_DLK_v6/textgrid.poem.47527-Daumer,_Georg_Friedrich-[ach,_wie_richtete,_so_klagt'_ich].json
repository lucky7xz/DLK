{"textgrid.poem.47527": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach, wie richtete, so klagt' ich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie richtete, so klagt' ich,", "tokens": ["Ach", ",", "wie", "rich\u00b7te\u00b7te", ",", "so", "klagt'", "ich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Saure Weisheit, Alter, Tugend", "tokens": ["Sau\u00b7re", "Weis\u00b7heit", ",", "Al\u00b7ter", ",", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich so ganz und gar zu Grunde! \u2013", "tokens": ["Mich", "so", "ganz", "und", "gar", "zu", "Grun\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "ADV", "KON", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm und sauge, sprach mein Liebchen,", "tokens": ["Komm", "und", "sau\u00b7ge", ",", "sprach", "mein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfe Thorheit, S\u00fcnde, Jugend", "tokens": ["S\u00fc\u00b7\u00dfe", "Thor\u00b7heit", ",", "S\u00fcn\u00b7de", ",", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise mir vom Rosenmunde,", "tokens": ["Lei\u00b7se", "mir", "vom", "Ro\u00b7sen\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Linde mir vom Lilienbusen,", "tokens": ["Lin\u00b7de", "mir", "vom", "Li\u00b7li\u00b7en\u00b7bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und zu neuem Tag gesunde!", "tokens": ["Und", "zu", "neu\u00b7em", "Tag", "ge\u00b7sun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}