{"textgrid.poem.41953": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich im mondlichen Glanz, umw\u00f6lbt von Bl\u00fcthengeb\u00fcschen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich im mondlichen Glanz, umw\u00f6lbt von Bl\u00fcthengeb\u00fcschen,", "tokens": ["Als", "ich", "im", "mond\u00b7li\u00b7chen", "Glanz", ",", "um\u00b7w\u00f6lbt", "von", "Bl\u00fc\u00b7then\u00b7ge\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "J\u00fcngst im Kahne mit dir h\u00fcpfende Wellen durchschwamm,", "tokens": ["J\u00fcngst", "im", "Kah\u00b7ne", "mit", "dir", "h\u00fcp\u00b7fen\u00b7de", "Wel\u00b7len", "durch\u00b7schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Als du so innig und warm an des Liebenden Seite dich schmiegtest,", "tokens": ["Als", "du", "so", "in\u00b7nig", "und", "warm", "an", "des", "Lie\u00b7ben\u00b7den", "Sei\u00b7te", "dich", "schmieg\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dem rosigen Mund Liebesgelispel entflo\u00df,", "tokens": ["Und", "dem", "ro\u00b7si\u00b7gen", "Mund", "Lie\u00b7bes\u00b7ge\u00b7lis\u00b7pel", "ent\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ach da pochte mein gl\u00fchendes Herz voll k\u00fchnen Verlangens,", "tokens": ["Ach", "da", "poch\u00b7te", "mein", "gl\u00fc\u00b7hen\u00b7des", "Herz", "voll", "k\u00fch\u00b7nen", "Ver\u00b7lan\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Ueppiger hauchte der West hei\u00dferes Sehnen mir zu,", "tokens": ["Uep\u00b7pi\u00b7ger", "hauch\u00b7te", "der", "West", "hei\u00b7\u00dfe\u00b7res", "Seh\u00b7nen", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Heilig gelobt' ich es, Reizende, dir, dich einzig zu lieben,", "tokens": ["Hei\u00b7lig", "ge\u00b7lobt'", "ich", "es", ",", "Rei\u00b7zen\u00b7de", ",", "dir", ",", "dich", "ein\u00b7zig", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "PPER", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Und ein freundlicher Ku\u00df lohnte den z\u00e4rtlichen Schwur.", "tokens": ["Und", "ein", "freund\u00b7li\u00b7cher", "Ku\u00df", "lohn\u00b7te", "den", "z\u00e4rt\u00b7li\u00b7chen", "Schwur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Tag' entschwanden, es schwanden dahin unendliche Monden,", "tokens": ["Tag'", "ent\u00b7schwan\u00b7den", ",", "es", "schwan\u00b7den", "da\u00b7hin", "un\u00b7end\u00b7li\u00b7che", "Mon\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PPER", "VVFIN", "PAV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und ich erblickte dich nicht, weilte verlassen und fern;", "tokens": ["Und", "ich", "er\u00b7blick\u00b7te", "dich", "nicht", ",", "weil\u00b7te", "ver\u00b7las\u00b7sen", "und", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "VVINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Todt erschien mir die Welt und hart die gewohnten Gesch\u00e4fte,", "tokens": ["Todt", "er\u00b7schien", "mir", "die", "Welt", "und", "hart", "die", "ge\u00b7wohn\u00b7ten", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Denn den romantischen Sinn beugte die ernstere Pflicht.", "tokens": ["Denn", "den", "ro\u00b7man\u00b7ti\u00b7schen", "Sinn", "beug\u00b7te", "die", "erns\u00b7te\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Fruchtlos harrt' ich auf Kunde von dir; kein freundliches Briefchen", "tokens": ["Frucht\u00b7los", "harrt'", "ich", "auf", "Kun\u00b7de", "von", "dir", ";", "kein", "freund\u00b7li\u00b7ches", "Brief\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPER", "$.", "PIAT", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Und kein tr\u00f6stender Gru\u00df letzte das schmachtende Herz.", "tokens": ["Und", "kein", "tr\u00f6s\u00b7ten\u00b7der", "Gru\u00df", "letz\u00b7te", "das", "schmach\u00b7ten\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Deiner gedacht ich im Wachen und Schlaf, und l\u00e4chelnd, wohin ich", "tokens": ["Dei\u00b7ner", "ge\u00b7dacht", "ich", "im", "Wa\u00b7chen", "und", "Schlaf", ",", "und", "l\u00e4\u00b7chelnd", ",", "wo\u00b7hin", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "VVPP", "PPER", "APPRART", "NN", "KON", "NN", "$,", "KON", "ADJD", "$,", "PWAV", "PPER"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Irrte, durch Wiesen und Hain folgte dem Tr\u00e4umer dein Bild.", "tokens": ["Irr\u00b7te", ",", "durch", "Wie\u00b7sen", "und", "Hain", "folg\u00b7te", "dem", "Tr\u00e4u\u00b7mer", "dein", "Bild", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Jegliche zarte Gestalt, die des Zufalls Gunst mich ersp\u00e4hn lie\u00df,", "tokens": ["Jeg\u00b7li\u00b7che", "zar\u00b7te", "Ge\u00b7stalt", ",", "die", "des", "Zu\u00b7falls", "Gunst", "mich", "er\u00b7sp\u00e4hn", "lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Wandelt' in deine Gestalt schnell dem Verlangenden sich;", "tokens": ["Wan\u00b7delt'", "in", "dei\u00b7ne", "Ge\u00b7stalt", "schnell", "dem", "Ver\u00b7lan\u00b7gen\u00b7den", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "PRF", "$."], "meter": "+--+--+-+-+-++", "measure": "dactylic.di.plus"}, "line.19": {"text": "Du vereinest ja doch was nur einzeln Alle besitzen,", "tokens": ["Du", "ver\u00b7ei\u00b7nest", "ja", "doch", "was", "nur", "ein\u00b7zeln", "Al\u00b7le", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PWS", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "Und dein Bild es erscheint stets mit dem Sch\u00f6nen zugleich.", "tokens": ["Und", "dein", "Bild", "es", "er\u00b7scheint", "stets", "mit", "dem", "Sch\u00f6\u00b7nen", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Mu\u00dft' ich sie alle nicht lieben, um dich nur einzig zu lieben?", "tokens": ["Mu\u00dft'", "ich", "sie", "al\u00b7le", "nicht", "lie\u00b7ben", ",", "um", "dich", "nur", "ein\u00b7zig", "zu", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "PTKNEG", "VVINF", "$,", "KOUI", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "F\u00fchlt' ich wohl deinen Ku\u00df, wenn ich nicht Alle gek\u00fc\u00dft?", "tokens": ["F\u00fchlt'", "ich", "wohl", "dei\u00b7nen", "Ku\u00df", ",", "wenn", "ich", "nicht", "Al\u00b7le", "ge\u00b7k\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "PIS", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Was ich zu ihnen auch Z\u00e4rtliches sprach, ich sprach es zu dir nur:", "tokens": ["Was", "ich", "zu", "ih\u00b7nen", "auch", "Z\u00e4rt\u00b7li\u00b7ches", "sprach", ",", "ich", "sprach", "es", "zu", "dir", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "ADV", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Und doch z\u00fcrnest du mir, da\u00df ich die Treue verletzt?", "tokens": ["Und", "doch", "z\u00fcr\u00b7nest", "du", "mir", ",", "da\u00df", "ich", "die", "Treu\u00b7e", "ver\u00b7letzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Ist nicht Amor ein Kind, das stets mit regem Verlangen", "tokens": ["Ist", "nicht", "A\u00b7mor", "ein", "Kind", ",", "das", "stets", "mit", "re\u00b7gem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NE", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Neue Gespielen erw\u00e4hlt, wenn ihm die alten entflohn?", "tokens": ["Neu\u00b7e", "Ge\u00b7spie\u00b7len", "er\u00b7w\u00e4hlt", ",", "wenn", "ihm", "die", "al\u00b7ten", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Jenen umflattert er nur, der hold und freundlich ihm liebkost:", "tokens": ["Je\u00b7nen", "um\u00b7flat\u00b7tert", "er", "nur", ",", "der", "hold", "und", "freund\u00b7lich", "ihm", "lieb\u00b7kost", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Wer zu naschen ihm giebt, ist ihm der z\u00e4rtlichste Freund.", "tokens": ["Wer", "zu", "na\u00b7schen", "ihm", "giebt", ",", "ist", "ihm", "der", "z\u00e4rt\u00b7lichs\u00b7te", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Soll ich mit ewiger Treu' in deine Fesseln mich schmiegen,", "tokens": ["Soll", "ich", "mit", "e\u00b7wi\u00b7ger", "Treu'", "in", "dei\u00b7ne", "Fes\u00b7seln", "mich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "O dann fl\u00fcstere stets Worte der Liebe mir zu,", "tokens": ["O", "dann", "fl\u00fcs\u00b7te\u00b7re", "stets", "Wor\u00b7te", "der", "Lie\u00b7be", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ADV", "NN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "La\u00df mich nimmer entfliehn aus dem magischen Kreis der Umarmung,", "tokens": ["La\u00df", "mich", "nim\u00b7mer", "ent\u00b7fliehn", "aus", "dem", "ma\u00b7gi\u00b7schen", "Kreis", "der", "Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Und ein unendlicher Ku\u00df binde den fl\u00fcchtigen Geist.", "tokens": ["Und", "ein", "un\u00b7end\u00b7li\u00b7cher", "Ku\u00df", "bin\u00b7de", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}