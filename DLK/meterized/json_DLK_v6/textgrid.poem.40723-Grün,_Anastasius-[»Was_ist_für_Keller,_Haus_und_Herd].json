{"textgrid.poem.40723": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[\u00bbWas ist f\u00fcr Keller, Haus und Herd]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas ist f\u00fcr Keller, Haus und Herd", "tokens": ["\u00bb", "was", "ist", "f\u00fcr", "Kel\u00b7ler", ",", "Haus", "und", "Herd"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Kranz, was Kunst und Ruhm mir werth,", "tokens": ["Dein", "Kranz", ",", "was", "Kunst", "und", "Ruhm", "mir", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "NN", "KON", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ich nicht messen kann und w\u00e4gen!\u00ab", "tokens": ["Die", "ich", "nicht", "mes\u00b7sen", "kann", "und", "w\u00e4\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frau Agnes keift; doch D\u00fcrer spricht:", "tokens": ["Frau", "Ag\u00b7nes", "keift", ";", "doch", "D\u00fc\u00b7rer", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbden Lorbeer, Freundin, sollst du nicht", "tokens": ["\u00bb", "den", "Lor\u00b7beer", ",", "Freun\u00b7din", ",", "sollst", "du", "nicht"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "NN", "$,", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf deine K\u00fcchenwage legen,", "tokens": ["Auf", "dei\u00b7ne", "K\u00fc\u00b7chen\u00b7wa\u00b7ge", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht mit der Myrthe Stuben fegen.\u00ab", "tokens": ["Nicht", "mit", "der", "Myr\u00b7the", "Stu\u00b7ben", "fe\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}