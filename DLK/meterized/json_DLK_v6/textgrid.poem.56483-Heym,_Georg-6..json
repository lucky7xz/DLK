{"textgrid.poem.56483": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du zogst voraus nach dem geheimen Reiche.", "tokens": ["Du", "zogst", "vo\u00b7raus", "nach", "dem", "ge\u00b7hei\u00b7men", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich folge dir dereinst, du Trauerbild,", "tokens": ["Ich", "fol\u00b7ge", "dir", "de\u00b7reinst", ",", "du", "Trau\u00b7er\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und halte ewig deine Hand, die bleiche,", "tokens": ["Und", "hal\u00b7te", "e\u00b7wig", "dei\u00b7ne", "Hand", ",", "die", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die meiner K\u00fcsse blasse Lilie f\u00fcllt.", "tokens": ["Die", "mei\u00b7ner", "K\u00fcs\u00b7se", "blas\u00b7se", "Li\u00b7lie", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann \u00fcberschwemmen lange Ewigkeiten", "tokens": ["Dann", "\u00fc\u00b7bersc\u00b7hwem\u00b7men", "lan\u00b7ge", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel Mauern und das tote Land,", "tokens": ["Der", "Him\u00b7mel", "Mau\u00b7ern", "und", "das", "to\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die, gro\u00dfe Schatten, in den Westen schreiten,", "tokens": ["Die", ",", "gro\u00b7\u00dfe", "Schat\u00b7ten", ",", "in", "den", "Wes\u00b7ten", "schrei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ehern ruht der Horizonte Wand.", "tokens": ["Wo", "e\u00b7hern", "ruht", "der", "Ho\u00b7ri\u00b7zon\u00b7te", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}