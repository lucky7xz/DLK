{"dta.poem.2849": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast ein gleich Gef\u00fchl nicht immer deiner Kr\u00e4fte,", "tokens": ["Du", "hast", "ein", "gleich", "Ge\u00b7f\u00fchl", "nicht", "im\u00b7mer", "dei\u00b7ner", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "NN", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schaffen mu\u00dft du, was einmal ist dein Gesch\u00e4fte.", "tokens": ["Doch", "schaf\u00b7fen", "mu\u00dft", "du", ",", "was", "ein\u00b7mal", "ist", "dein", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "PRELS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn du bei deinem Werk nicht f\u00fchlst die frische Lust,", "tokens": ["Wenn", "du", "bei", "dei\u00b7nem", "Werk", "nicht", "f\u00fchlst", "die", "fri\u00b7sche", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch denke darum nicht, da\u00df du nichts rechtes thust.", "tokens": ["Doch", "den\u00b7ke", "da\u00b7rum", "nicht", ",", "da\u00df", "du", "nichts", "rech\u00b7tes", "thust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKNEG", "$,", "KOUS", "PPER", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vertrau dem guten Geist auch in der schlechten Stunde,", "tokens": ["Ver\u00b7trau", "dem", "gu\u00b7ten", "Geist", "auch", "in", "der", "schlech\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, ohne da\u00df du's wei\u00dft, doch ist mit dir im Bunde.", "tokens": ["Der", ",", "oh\u00b7ne", "da\u00df", "du's", "wei\u00dft", ",", "doch", "ist", "mit", "dir", "im", "Bun\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "KOUS", "PIS", "VVFIN", "$,", "ADV", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}