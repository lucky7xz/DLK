{"dta.poem.275": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An seine harte Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Soll Sch\u00f6nste denn mein Hertz in stum\u0303er Gluth verbrennen?", "tokens": ["Soll", "Sch\u00f6ns\u00b7te", "denn", "mein", "Hertz", "in", "stum\u0303er", "Gluth", "ver\u00b7bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und darf die Zunge nicht des Hertzens stille Quaal?", "tokens": ["Und", "darf", "die", "Zun\u00b7ge", "nicht", "des", "Hert\u00b7zens", "stil\u00b7le", "Qua\u00b7al", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu einer Linderung der Liebes-Pein bekennen?", "tokens": ["Zu", "ei\u00b7ner", "Lin\u00b7de\u00b7rung", "der", "Lie\u00b7bes\u00b7Pein", "be\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Auge/ das mich brennt/ verl\u00e4ugnet seinen Strahl.", "tokens": ["Dein", "Au\u00b7ge", "/", "das", "mich", "brennt", "/", "ver\u00b7l\u00e4ug\u00b7net", "sei\u00b7nen", "Strahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wilt ein Feuer seyn/ und keine Hitze haben/", "tokens": ["Du", "wilt", "ein", "Feu\u00b7er", "seyn", "/", "und", "kei\u00b7ne", "Hit\u00b7ze", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$(", "KON", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein spielend Augen-Blitz verwundet Muth und Hertz;", "tokens": ["Dein", "spie\u00b7lend", "Au\u00b7gen\u00b7Blitz", "ver\u00b7wun\u00b7det", "Muth", "und", "Hertz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch soll kein sanffter Blick das matte Hertze laben/", "tokens": ["Doch", "soll", "kein", "sanff\u00b7ter", "Blick", "das", "mat\u00b7te", "Hert\u00b7ze", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "ADJA", "NN", "ART", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Sterben macht dir Lust/ mein Trauren s\u00fcssen Schertz.", "tokens": ["Mein", "Ster\u00b7ben", "macht", "dir", "Lust", "/", "mein", "Trau\u00b7ren", "s\u00fcs\u00b7sen", "Schertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kan Gluth und kalter Frost nun wol beysammen stehen?", "tokens": ["Kan", "Gluth", "und", "kal\u00b7ter", "Frost", "nun", "wol", "bey\u00b7sam\u00b7men", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "ADJA", "NN", "ADV", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will das Augen-Paar nicht warm und kalt mir seyn?", "tokens": ["Und", "will", "das", "Au\u00b7gen\u00b7Paar", "nicht", "warm", "und", "kalt", "mir", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja/ Feur und Ey\u00df l\u00e4st sich im Berge ", "tokens": ["Ja", "/", "Feur", "und", "Ey\u00df", "l\u00e4st", "sich", "im", "Ber\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "NN", "KON", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Auge ist ein Ey\u00df/ und giebt doch heissen Schein.", "tokens": ["Dein", "Au\u00b7ge", "ist", "ein", "Ey\u00df", "/", "und", "giebt", "doch", "heis\u00b7sen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Br\u00fcste ", "tokens": ["Der", "Br\u00fcs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Die Augen spielen Gluth/ und sind mir dennoch kalt;", "tokens": ["Die", "Au\u00b7gen", "spie\u00b7len", "Gluth", "/", "und", "sind", "mir", "den\u00b7noch", "kalt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "KON", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald loben sie mein Thun/ bald wollen sie\u2019s verdammen/", "tokens": ["Bald", "lo\u00b7ben", "sie", "mein", "Thun", "/", "bald", "wol\u00b7len", "sie's", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und also foltert mich die himmlische Gestalt.", "tokens": ["Und", "al\u00b7so", "fol\u00b7tert", "mich", "die", "himm\u00b7li\u00b7sche", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}