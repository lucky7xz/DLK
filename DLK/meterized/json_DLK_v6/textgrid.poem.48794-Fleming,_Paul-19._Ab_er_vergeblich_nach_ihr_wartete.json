{"textgrid.poem.48794": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19. Ab er vergeblich nach ihr wartete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und t\u00f6test du mich gleich, so bist du doch mein Freund,", "tokens": ["Und", "t\u00f6\u00b7test", "du", "mich", "gleich", ",", "so", "bist", "du", "doch", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ob di\u00df Verlangen zwar, das \u00e4ngstliche, das schwere,", "tokens": ["ob", "di\u00df", "Ver\u00b7lan\u00b7gen", "zwar", ",", "das", "\u00e4ngst\u00b7li\u00b7che", ",", "das", "schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "ADV", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nichts Anders bald wird tun, als was ich so begehre.", "tokens": ["nichts", "An\u00b7ders", "bald", "wird", "tun", ",", "als", "was", "ich", "so", "be\u00b7geh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "VAFIN", "VVINF", "$,", "KOUS", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Leid dringt in die Luft, kein einigs Sternlein scheint.", "tokens": ["Mein", "Leid", "dringt", "in", "die", "Luft", ",", "kein", "ei\u00b7nigs", "Stern\u00b7lein", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Himmel treuft mir nach, was ich ihm vorgeweint,", "tokens": ["Der", "Him\u00b7mel", "treuft", "mir", "nach", ",", "was", "ich", "ihm", "vor\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Winde seufzen so, wie ich sie seufzen lehre.", "tokens": ["die", "Win\u00b7de", "seuf\u00b7zen", "so", ",", "wie", "ich", "sie", "seuf\u00b7zen", "leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hab' ich keinen Sinn, der dir zuwider w\u00e4re.", "tokens": ["Doch", "hab'", "ich", "kei\u00b7nen", "Sinn", ",", "der", "dir", "zu\u00b7wi\u00b7der", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hab' ich, Trost, dich nicht lieb, so bin ich mir selbst Feind.", "tokens": ["Hab'", "ich", ",", "Trost", ",", "dich", "nicht", "lieb", ",", "so", "bin", "ich", "mir", "selbst", "Feind", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPER", "PTKNEG", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier wart' ich, teures Blut, vor deiner tauben Schwellen,", "tokens": ["Hier", "wart'", "ich", ",", "teu\u00b7res", "Blut", ",", "vor", "dei\u00b7ner", "tau\u00b7ben", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nicht hoffend, da\u00df du itzt dich werdest noch einstellen,", "tokens": ["nicht", "hof\u00b7fend", ",", "da\u00df", "du", "itzt", "dich", "wer\u00b7dest", "noch", "ein\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "KOUS", "PPER", "ADV", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nein! sondern da\u00df mich hier der nahe Tod rei\u00df' hin.", "tokens": ["nein", "!", "son\u00b7dern", "da\u00df", "mich", "hier", "der", "na\u00b7he", "Tod", "rei\u00df'", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So wird es denn geschehn, da\u00df du, wenn du zu Morgen", "tokens": ["So", "wird", "es", "denn", "ge\u00b7schehn", ",", "da\u00df", "du", ",", "wenn", "du", "zu", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mich sehn wirst, da\u00df ich kalt und ganz gestorben bin,", "tokens": ["mich", "sehn", "wirst", ",", "da\u00df", "ich", "kalt", "und", "ganz", "ge\u00b7stor\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit neuem Leben mich zur Strafe wirst versorgen.", "tokens": ["mit", "neu\u00b7em", "Le\u00b7ben", "mich", "zur", "Stra\u00b7fe", "wirst", "ver\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}