{"dta.poem.7532": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Auge kann die Welt tr\u00fcb' oder hell dir machen;", "tokens": ["Dein", "Au\u00b7ge", "kann", "die", "Welt", "tr\u00fcb'", "o\u00b7der", "hell", "dir", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVFIN", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du sie ansiehst, wird sie weinen oder lachen.", "tokens": ["Wie", "du", "sie", "an\u00b7siehst", ",", "wird", "sie", "wei\u00b7nen", "o\u00b7der", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein \u00e4u\u00dfres Auge kannst du sch\u00e4rfen selbst und \u00fcben;", "tokens": ["Dein", "\u00e4u\u00df\u00b7res", "Au\u00b7ge", "kannst", "du", "sch\u00e4r\u00b7fen", "selbst", "und", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O h\u00fcte dich vielmehr, dein inneres zu tr\u00fcben!", "tokens": ["O", "h\u00fc\u00b7te", "dich", "viel\u00b7mehr", ",", "dein", "in\u00b7ne\u00b7res", "zu", "tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn rein dein innres schaut, das \u00e4u\u00dfre mag erblinden,", "tokens": ["Wenn", "rein", "dein", "inn\u00b7res", "schaut", ",", "das", "\u00e4u\u00df\u00b7re", "mag", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "VVFIN", "$,", "PDS", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst das helle Bild der Welt im Herzen finden.", "tokens": ["Du", "wirst", "das", "hel\u00b7le", "Bild", "der", "Welt", "im", "Her\u00b7zen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}