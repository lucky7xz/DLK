{"textgrid.poem.61724": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Klage", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das treibt das Blut mir hei\u00df ins Angesicht,", "tokens": ["Das", "treibt", "das", "Blut", "mir", "hei\u00df", "ins", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, wo ich schweifen mag im fremden Lande,", "tokens": ["Da\u00df", ",", "wo", "ich", "schwei\u00b7fen", "mag", "im", "frem\u00b7den", "Lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00f6ren mu\u00df des deutschen Namens Schande", "tokens": ["Ich", "h\u00f6\u00b7ren", "mu\u00df", "des", "deut\u00b7schen", "Na\u00b7mens", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und darf nicht sagen, da\u00df man L\u00fcge spricht,", "tokens": ["Und", "darf", "nicht", "sa\u00b7gen", ",", "da\u00df", "man", "L\u00fc\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob mir vor Gram und Scham das Herz darob zerbricht.", "tokens": ["Ob", "mir", "vor", "Gram", "und", "Scham", "das", "Herz", "da\u00b7rob", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn ach, der Mund, einst aller Treue Hort,", "tokens": ["Denn", "ach", ",", "der", "Mund", ",", "einst", "al\u00b7ler", "Treu\u00b7e", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "$,", "ADV", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der deutsche Mund, des Spruch gleich teuren Eiden,", "tokens": ["Der", "deut\u00b7sche", "Mund", ",", "des", "Spruch", "gleich", "teu\u00b7ren", "Ei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Zucht und Wahrheit lernt' er sich zu scheiden;", "tokens": ["Von", "Zucht", "und", "Wahr\u00b7heit", "lernt'", "er", "sich", "zu", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerbrechlich worden ist wie Glas sein Wort,", "tokens": ["Zer\u00b7brech\u00b7lich", "wor\u00b7den", "ist", "wie", "Glas", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAPP", "VAFIN", "KOKOM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und seine Schw\u00fcre taun wie Schnee um Ostern fort.", "tokens": ["Und", "sei\u00b7ne", "Schw\u00fc\u00b7re", "taun", "wie", "Schnee", "um", "Os\u00b7tern", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "KOKOM", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und du, o deutsches Schwert, das scharf gefegt", "tokens": ["Und", "du", ",", "o", "deut\u00b7sches", "Schwert", ",", "das", "scharf", "ge\u00b7fegt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch hundert Schlachten k\u00fchn sich Bahn gebrochen,", "tokens": ["Durch", "hun\u00b7dert", "Schlach\u00b7ten", "k\u00fchn", "sich", "Bahn", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was zagst du, in der Scheide nun verkrochen,", "tokens": ["Was", "zagst", "du", ",", "in", "der", "Schei\u00b7de", "nun", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00e4rst du Schilf, das keine Wunden schl\u00e4gt,", "tokens": ["Als", "w\u00e4rst", "du", "Schilf", ",", "das", "kei\u00b7ne", "Wun\u00b7den", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sobald nur Moskaus Zar die Stirn in Runzeln legt!", "tokens": ["So\u00b7bald", "nur", "Mos\u00b7kaus", "Zar", "die", "Stirn", "in", "Run\u00b7zeln", "legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "NE", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, da's um Treu' und Mut bei uns geschehn,", "tokens": ["Ach", ",", "da's", "um", "Treu'", "und", "Mut", "bei", "uns", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "APPR", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da neigt' ihr Haupt und starb die deutsche Ehre \u2013", "tokens": ["Da", "neigt'", "ihr", "Haupt", "und", "starb", "die", "deut\u00b7sche", "Eh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fragt nach bei Schleswig zwischen Meer und Meere!", "tokens": ["Fragt", "nach", "bei", "Schles\u00b7wig", "zwi\u00b7schen", "Meer", "und", "Mee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort liegt sie eingescharrt; die Winde gehn", "tokens": ["Dort", "liegt", "sie", "ein\u00b7ge\u00b7scharrt", ";", "die", "Win\u00b7de", "gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Pfeifen dr\u00fcber hin. Wann wird sie auferstehn!", "tokens": ["Mit", "Pfei\u00b7fen", "dr\u00fc\u00b7ber", "hin", ".", "Wann", "wird", "sie", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "PTKVZ", "$.", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}