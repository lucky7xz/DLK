{"dta.poem.1686": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4thsel aus dem Itali\u00e4nischen  \n \u00fcbersetzet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Sch\u00f6pffer nicht/ noch in Gesch\u00f6pffes Orden/ ", "tokens": ["Ich", "bin", "der", "Sch\u00f6pf\u00b7fer", "nicht", "/", "noch", "in", "Ge\u00b7sch\u00f6pf\u00b7fes", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$(", "ADV", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch unter Lebenden niemahls gesehen worden/", "tokens": ["Auch", "un\u00b7ter", "Le\u00b7ben\u00b7den", "nie\u00b7mahls", "ge\u00b7se\u00b7hen", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVPP", "VAPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bey Menschen sieht man mich/ bi\u00df sie erblichen seyn/", "tokens": ["Bey", "Men\u00b7schen", "sieht", "man", "mich", "/", "bi\u00df", "sie", "er\u00b7bli\u00b7chen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPER", "$(", "KOUS", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch schleust mich kein schlecht Grab/ wie sie/ nur Mar-", "tokens": ["Doch", "schleust", "mich", "kein", "schlecht", "Grab", "/", "wie", "sie", "/", "nur", "Ma\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$(", "PWAV", "PPER", "$(", "ADV", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Galante Mode-Welt gibt mir die erste Stelle/", "tokens": ["Ga\u00b7lan\u00b7te", "Mo\u00b7de\u00b7Welt", "gibt", "mir", "die", "ers\u00b7te", "Stel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erde tr\u00e4gt mich nicht/ mich hegt noch Flutt noch Welle/", "tokens": ["Die", "Er\u00b7de", "tr\u00e4gt", "mich", "nicht", "/", "mich", "hegt", "noch", "Flutt", "noch", "Wel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "PPER", "VVFIN", "ADV", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mag nicht in die Lufft noch in das Feuer gehn/", "tokens": ["Ich", "mag", "nicht", "in", "die", "Lufft", "noch", "in", "das", "Feu\u00b7er", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will im Mittel-Punct der Elementen stehn.", "tokens": ["Und", "will", "im", "Mit\u00b7tel\u00b7Punct", "der", "E\u00b7le\u00b7men\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Es wei\u00df noch Jahr noch Tag noch Stunde mich zu messen/", "tokens": ["Es", "wei\u00df", "noch", "Jahr", "noch", "Tag", "noch", "Stun\u00b7de", "mich", "zu", "mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "NN", "ADV", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beym Monat-Wechsel bleib ich dennoch unvergessen/", "tokens": ["Beym", "Mo\u00b7nat\u00b7Wech\u00b7sel", "bleib", "ich", "den\u00b7noch", "un\u00b7ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In gegenw\u00e4rtiger und in vergangner Zeit", "tokens": ["In", "ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ger", "und", "in", "ver\u00b7gang\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schweb ich so wenig als in k\u00fcnfftger Ewigkeit.", "tokens": ["Schweb", "ich", "so", "we\u00b7nig", "als", "in", "k\u00fcnfft\u00b7ger", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gebohren bin ich nicht wie andre/ welche leben/", "tokens": ["Ge\u00b7boh\u00b7ren", "bin", "ich", "nicht", "wie", "and\u00b7re", "/", "wel\u00b7che", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKNEG", "PWAV", "PIS", "$(", "PWS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mu\u00df mir ", "tokens": ["Doch", "mu\u00df", "mir"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "In Marter bin ich stets/ doch von der H\u00f6llen fern:", "tokens": ["In", "Mar\u00b7ter", "bin", "ich", "stets", "/", "doch", "von", "der", "H\u00f6l\u00b7len", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "$(", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Himmel bin ich auch/ doch ohne Sonn und Stern.", "tokens": ["Im", "Him\u00b7mel", "bin", "ich", "auch", "/", "doch", "oh\u00b7ne", "Sonn", "und", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "$(", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}