{"textgrid.poem.57562": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den alten Platanen fl\u00fcstert der Wind", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den alten Platanen fl\u00fcstert der Wind", "tokens": ["In", "den", "al\u00b7ten", "Pla\u00b7ta\u00b7nen", "fl\u00fcs\u00b7tert", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit m\u00fcdem, nachl\u00e4ssigem Wehen \u2013", "tokens": ["Mit", "m\u00fc\u00b7dem", ",", "nach\u00b7l\u00e4s\u00b7si\u00b7gem", "We\u00b7hen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich denke an dich, du totes Kind,", "tokens": ["Ich", "den\u00b7ke", "an", "dich", ",", "du", "to\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und da\u00df ich dich gestern gesehen.", "tokens": ["Und", "da\u00df", "ich", "dich", "ge\u00b7stern", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du schautest mich an so bittend und scheu,", "tokens": ["Du", "schau\u00b7test", "mich", "an", "so", "bit\u00b7tend", "und", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erflehend ein Zeichen der Liebe,", "tokens": ["Er\u00b7fle\u00b7hend", "ein", "Zei\u00b7chen", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich aber ging h\u00f6flich gr\u00fc\u00dfend vorbei", "tokens": ["Ich", "a\u00b7ber", "ging", "h\u00f6f\u00b7lich", "gr\u00fc\u00b7\u00dfend", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ADJD", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch das wogende Sonntagsgetriebe.", "tokens": ["Durch", "das", "wo\u00b7gen\u00b7de", "Sonn\u00b7tags\u00b7ge\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es war ein Traum, so wonnig und bang,", "tokens": ["Es", "war", "ein", "Traum", ",", "so", "won\u00b7nig", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich werde ihn niemals vergessen,", "tokens": ["Ich", "wer\u00b7de", "ihn", "nie\u00b7mals", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den kurzen Traum, wo mein Arm dich umschlang,", "tokens": ["Den", "kur\u00b7zen", "Traum", ",", "wo", "mein", "Arm", "dich", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo ich deine Liebe besessen.", "tokens": ["Wo", "ich", "dei\u00b7ne", "Lie\u00b7be", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich lieb' dich noch heut wie an jenem Tag,", "tokens": ["Ich", "lieb'", "dich", "noch", "heut", "wie", "an", "je\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch will ich es dir nicht mehr sagen,", "tokens": ["Doch", "will", "ich", "es", "dir", "nicht", "mehr", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seitdem du mit l\u00e4chelnd kokettem Schlag", "tokens": ["Seit\u00b7dem", "du", "mit", "l\u00e4\u00b7chelnd", "ko\u00b7ket\u00b7tem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meinen Glauben an dich hast erschlagen.", "tokens": ["Mei\u00b7nen", "Glau\u00b7ben", "an", "dich", "hast", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und blickst du auch noch so schmerzlich und lieb,", "tokens": ["Und", "blickst", "du", "auch", "noch", "so", "schmerz\u00b7lich", "und", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zertreten ist einmal der Samen,", "tokens": ["Zer\u00b7tre\u00b7ten", "ist", "ein\u00b7mal", "der", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In das Album meiner Erinnerung schrieb", "tokens": ["In", "das", "Al\u00b7bum", "mei\u00b7ner", "E\u00b7rin\u00b7ne\u00b7rung", "schrieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ich ein Kreuz dir hinter den Namen.", "tokens": ["Ich", "ein", "Kreuz", "dir", "hin\u00b7ter", "den", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich h\u00e4tte getr\u00e4umt ein sch\u00f6nes Gedicht:", "tokens": ["Ich", "h\u00e4t\u00b7te", "ge\u00b7tr\u00e4umt", "ein", "sch\u00f6\u00b7nes", "Ge\u00b7dicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dich als ehelich Weib zu umschlie\u00dfen,", "tokens": ["Dich", "als", "e\u00b7he\u00b7lich", "Weib", "zu", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Doch um Liebe betteln, das tue ich nicht,", "tokens": ["Doch", "um", "Lie\u00b7be", "bet\u00b7teln", ",", "das", "tue", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht einmal zu deinen F\u00fc\u00dfen.", "tokens": ["Nicht", "ein\u00b7mal", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}