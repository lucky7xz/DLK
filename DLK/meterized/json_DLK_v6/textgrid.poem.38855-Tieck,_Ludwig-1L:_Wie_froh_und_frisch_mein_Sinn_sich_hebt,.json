{"textgrid.poem.38855": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie froh und frisch mein Sinn sich hebt,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie froh und frisch mein Sinn sich hebt,", "tokens": ["Wie", "froh", "und", "frisch", "mein", "Sinn", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcckbleibt alles Bangen,", "tokens": ["Zu\u00b7r\u00fcck\u00b7bleibt", "al\u00b7les", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Brust mit neuem Muthe strebt,", "tokens": ["Die", "Brust", "mit", "neu\u00b7em", "Mu\u00b7the", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwacht ein neu Verlangen.", "tokens": ["Er\u00b7wacht", "ein", "neu", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sterne spiegeln sich im Meer,", "tokens": ["Die", "Ster\u00b7ne", "spie\u00b7geln", "sich", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und golden gl\u00e4nzt die Fluth. \u2013", "tokens": ["Und", "gol\u00b7den", "gl\u00e4nzt", "die", "Fluth", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich rannte taumelnd hin und her,", "tokens": ["Ich", "rann\u00b7te", "tau\u00b7melnd", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und war nicht schlimm nicht gut.", "tokens": ["Und", "war", "nicht", "schlimm", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch niedergezogen", "tokens": ["Doch", "nie\u00b7der\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sind Zweifel und wankender Sinn,", "tokens": ["Sind", "Zwei\u00b7fel", "und", "wan\u00b7ken\u00b7der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "O tragt mich, ihr schaukelnden Wogen,", "tokens": ["O", "tragt", "mich", ",", "ihr", "schau\u00b7keln\u00b7den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zur l\u00e4ngst ersehnten Heimath hin.", "tokens": ["Zur", "l\u00e4ngst", "er\u00b7sehn\u00b7ten", "Hei\u00b7math", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In lieber d\u00e4mmernder Ferne,", "tokens": ["In", "lie\u00b7ber", "d\u00e4m\u00b7mern\u00b7der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort rufen einheimische Lieder,", "tokens": ["Dort", "ru\u00b7fen", "ein\u00b7hei\u00b7mi\u00b7sche", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Aus jeglichem Sterne", "tokens": ["Aus", "jeg\u00b7li\u00b7chem", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Blickt sie mit sanftem Auge nieder.", "tokens": ["Blickt", "sie", "mit", "sanf\u00b7tem", "Au\u00b7ge", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ebne dich, du treue Welle,", "tokens": ["Eb\u00b7ne", "dich", ",", "du", "treu\u00b7e", "Wel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchre mich auf fernen Wegen", "tokens": ["F\u00fch\u00b7re", "mich", "auf", "fer\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der vielgeliebten Schwelle,", "tokens": ["Zu", "der", "viel\u00b7ge\u00b7lieb\u00b7ten", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich meinem Gl\u00fcck entgegen!", "tokens": ["End\u00b7lich", "mei\u00b7nem", "Gl\u00fcck", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}