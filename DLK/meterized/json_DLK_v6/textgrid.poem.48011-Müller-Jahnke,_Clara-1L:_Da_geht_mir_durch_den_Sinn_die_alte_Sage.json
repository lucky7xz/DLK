{"textgrid.poem.48011": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da geht mir durch den Sinn die alte Sage", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da geht mir durch den Sinn die alte Sage", "tokens": ["Da", "geht", "mir", "durch", "den", "Sinn", "die", "al\u00b7te", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von jenem K\u00f6nig, dem, was er ber\u00fchrt,", "tokens": ["von", "je\u00b7nem", "K\u00f6\u00b7nig", ",", "dem", ",", "was", "er", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu Golde ward: \u2013 das M\u00e4rchen meiner Tage.", "tokens": ["zu", "Gol\u00b7de", "ward", ":", "\u2013", "das", "M\u00e4r\u00b7chen", "mei\u00b7ner", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "$.", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als noch mein Geist, der Erdenfesseln frei,", "tokens": ["Als", "noch", "mein", "Geist", ",", "der", "Er\u00b7den\u00b7fes\u00b7seln", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in seliger Gefilde Wonne schwelgte,", "tokens": ["in", "se\u00b7li\u00b7ger", "Ge\u00b7fil\u00b7de", "Won\u00b7ne", "schwelg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "trat l\u00e4chelnd zu ihm der Erh\u00f6rung Fei.", "tokens": ["trat", "l\u00e4\u00b7chelnd", "zu", "ihm", "der", "Er\u00b7h\u00f6\u00b7rung", "Fei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr roter Mund verhie\u00df ihm reichen Segen", "tokens": ["Ihr", "ro\u00b7ter", "Mund", "ver\u00b7hie\u00df", "ihm", "rei\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf seiner Pilgerfahrt: \u00bbNun w\u00e4hle dir", "tokens": ["auf", "sei\u00b7ner", "Pil\u00b7ger\u00b7fahrt", ":", "\u00bb", "Nun", "w\u00e4h\u00b7le", "dir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "ADV", "VVIMP", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "den Leitstern selbst auf deinen Erdenwegen.\u00ab", "tokens": ["den", "Leits\u00b7tern", "selbst", "auf", "dei\u00b7nen", "Er\u00b7den\u00b7we\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vor meines Geistes Augen hingestreut", "tokens": ["Vor", "mei\u00b7nes", "Geis\u00b7tes", "Au\u00b7gen", "hin\u00b7ge\u00b7streut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "lag alles Gl\u00fcck und alle Herrlichkeiten,", "tokens": ["lag", "al\u00b7les", "Gl\u00fcck", "und", "al\u00b7le", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die seinen Lieblingen das Leben beut.", "tokens": ["die", "sei\u00b7nen", "Lieb\u00b7lin\u00b7gen", "das", "Le\u00b7ben", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Da funkelten des Reichtums Diamanten,", "tokens": ["Da", "fun\u00b7kel\u00b7ten", "des", "Reich\u00b7tums", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Liebe Rosen bl\u00fchten d\u00fcftereich,", "tokens": ["der", "Lie\u00b7be", "Ro\u00b7sen", "bl\u00fch\u00b7ten", "d\u00fcf\u00b7te\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Weltlust helle Opferfeuer brannten,", "tokens": ["der", "Welt\u00b7lust", "hel\u00b7le", "Op\u00b7fer\u00b7feu\u00b7er", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "der Freundschaft Perlen gl\u00e4nzten auf der Schnur;", "tokens": ["der", "Freund\u00b7schaft", "Per\u00b7len", "gl\u00e4nz\u00b7ten", "auf", "der", "Schnur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "doch meinen Geist vermochte nichts zu locken,", "tokens": ["doch", "mei\u00b7nen", "Geist", "ver\u00b7moch\u00b7te", "nichts", "zu", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als eines Himmelssternes Schimmer nur:", "tokens": ["als", "ei\u00b7nes", "Him\u00b7melss\u00b7ter\u00b7nes", "Schim\u00b7mer", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbob meinem Haupte strahlt der Stern der Lieder,", "tokens": ["\u00bb", "ob", "mei\u00b7nem", "Haup\u00b7te", "strahlt", "der", "Stern", "der", "Lie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nimm hin des Reichtums Glanz, der Liebe Lust:", "tokens": ["nimm", "hin", "des", "Reich\u00b7tums", "Glanz", ",", "der", "Lie\u00b7be", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "im Reich der Dichtung bl\u00fcht mir alles wieder.", "tokens": ["im", "Reich", "der", "Dich\u00b7tung", "bl\u00fcht", "mir", "al\u00b7les", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und was dein Mund mir sch\u00f6nes bieten mag,", "tokens": ["Und", "was", "dein", "Mund", "mir", "sch\u00f6\u00b7nes", "bie\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "PPER", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich geb es freudig hin um seinen Schimmer,", "tokens": ["ich", "geb", "es", "freu\u00b7dig", "hin", "um", "sei\u00b7nen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ins tiefste Dunkel tr\u00e4gt er lichten Tag.", "tokens": ["ins", "tiefs\u00b7te", "Dun\u00b7kel", "tr\u00e4gt", "er", "lich\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Nicht sorg ich mehr, da\u00df ich den Heimweg finde:", "tokens": ["Nicht", "sorg", "ich", "mehr", ",", "da\u00df", "ich", "den", "Heim\u00b7weg", "fin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "was meine Hand ber\u00fchrt, wird Poesie \u2013", "tokens": ["was", "mei\u00b7ne", "Hand", "be\u00b7r\u00fchrt", ",", "wird", "Poe\u00b7sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "o gib den Stern mir mit als Angebinde!\u00ab", "tokens": ["o", "gib", "den", "Stern", "mir", "mit", "als", "An\u00b7ge\u00b7bin\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "VVIMP", "ART", "NN", "PPER", "APPR", "KOUS", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und l\u00e4chelnd winkte der Erh\u00f6rung Fei. \u2013", "tokens": ["Und", "l\u00e4\u00b7chelnd", "wink\u00b7te", "der", "Er\u00b7h\u00f6\u00b7rung", "Fei", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da sank ein dumpfer Schlaf auf meine Lider;", "tokens": ["Da", "sank", "ein", "dum\u00b7pfer", "Schlaf", "auf", "mei\u00b7ne", "Li\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als ich erwachte, war der Traum vorbei.", "tokens": ["als", "ich", "er\u00b7wach\u00b7te", ",", "war", "der", "Traum", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Im dunklen Tal der Welt fand ich mich wieder;", "tokens": ["Im", "dunk\u00b7len", "Tal", "der", "Welt", "fand", "ich", "mich", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und Zentnerlasten beugen mir das Haupt,", "tokens": ["und", "Zent\u00b7ner\u00b7las\u00b7ten", "beu\u00b7gen", "mir", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Eisenfesseln dr\u00fccken mir die Glieder.", "tokens": ["und", "Ei\u00b7sen\u00b7fes\u00b7seln", "dr\u00fc\u00b7cken", "mir", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die gl\u00fchende Bilderpracht des Lebens blinkt", "tokens": ["Die", "gl\u00fc\u00b7hen\u00b7de", "Bil\u00b7der\u00b7pracht", "des", "Le\u00b7bens", "blinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "vor meiner Sehnsucht fieberhei\u00dfen Blicken,", "tokens": ["vor", "mei\u00b7ner", "Sehn\u00b7sucht", "fie\u00b7ber\u00b7hei\u00b7\u00dfen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie durch die W\u00fcste die Morgana winkt.", "tokens": ["wie", "durch", "die", "W\u00fcs\u00b7te", "die", "Mor\u00b7ga\u00b7na", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Der Freundschaft L\u00e4cheln hab ich nie empfangen;", "tokens": ["Der", "Freund\u00b7schaft", "L\u00e4\u00b7cheln", "hab", "ich", "nie", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wenn sie bei andern sich zu Gaste lud,", "tokens": ["wenn", "sie", "bei", "an\u00b7dern", "sich", "zu", "Gas\u00b7te", "lud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "an meiner T\u00fcr ist sie vorbeigegangen.", "tokens": ["an", "mei\u00b7ner", "T\u00fcr", "ist", "sie", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Und selbst die s\u00fc\u00dfe Liebe pochte nur", "tokens": ["Und", "selbst", "die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", "poch\u00b7te", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zum Spiele an mein Herz; als ich ge\u00f6ffnet,", "tokens": ["zum", "Spie\u00b7le", "an", "mein", "Herz", ";", "als", "ich", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$.", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da fand ich nicht mehr ihrer Tritte Spur.", "tokens": ["da", "fand", "ich", "nicht", "mehr", "ih\u00b7rer", "Trit\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und bange T\u00f6ne klingen mir im Haupte:", "tokens": ["Und", "ban\u00b7ge", "T\u00f6\u00b7ne", "klin\u00b7gen", "mir", "im", "Haup\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im Liede gl\u00fcht und bl\u00fcht mir alles neu,", "tokens": ["im", "Lie\u00b7de", "gl\u00fcht", "und", "bl\u00fcht", "mir", "al\u00b7les", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "was mir die Welt versagte oder raubte,", "tokens": ["was", "mir", "die", "Welt", "ver\u00b7sag\u00b7te", "o\u00b7der", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "der Winde Wehen klingt wie Melodie;", "tokens": ["der", "Win\u00b7de", "We\u00b7hen", "klingt", "wie", "Me\u00b7lo\u00b7die", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ob meinem Haupte strahlt der Stern der Lieder,", "tokens": ["ob", "mei\u00b7nem", "Haup\u00b7te", "strahlt", "der", "Stern", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KO