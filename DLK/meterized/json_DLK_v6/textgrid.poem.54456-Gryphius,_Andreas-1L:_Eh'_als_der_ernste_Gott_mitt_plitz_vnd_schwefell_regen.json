{"textgrid.poem.54456": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eh' als der ernste Gott mitt plitz vnd schwefell regen", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh' als der ernste Gott mitt plitz vnd schwefell regen", "tokens": ["Eh'", "als", "der", "erns\u00b7te", "Gott", "mitt", "plitz", "vnd", "schwe\u00b7fell", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "APPR", "NE", "KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mitt fewer pech vnd sturm hatt Sodom vmbgekehrt", "tokens": ["Mitt", "fe\u00b7wer", "pech", "vnd", "sturm", "hatt", "So\u00b7dom", "vmb\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Eh' erd vnd Himmel kracht vor seines eyvers schwerdt/", "tokens": ["Eh'", "erd", "vnd", "Him\u00b7mel", "kracht", "vor", "sei\u00b7nes", "ey\u00b7vers", "schwerdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh' als er Zeboim lies in die asche legen;", "tokens": ["Eh'", "als", "er", "Ze\u00b7bo\u00b7im", "lies", "in", "die", "asc\u00b7he", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "NE", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "++-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Eh' als die heisse lufft erklang von donnerschl\u00e4gen;", "tokens": ["Eh'", "als", "die", "heis\u00b7se", "lufft", "er\u00b7klang", "von", "don\u00b7ner\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eh' er auff Adama sein r\u00fcsthaus au\u00dfgelert", "tokens": ["Eh'", "er", "auff", "A\u00b7da\u00b7ma", "sein", "r\u00fcst\u00b7haus", "au\u00df\u00b7ge\u00b7lert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Eh' als Gomorrha noch von flammen auffgezehrt;", "tokens": ["Eh'", "als", "Go\u00b7morr\u00b7ha", "noch", "von", "flam\u00b7men", "auff\u00b7ge\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NE", "ADV", "APPR", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mus Loth mitt Weib vnd Kind von dannen sich bewegen.", "tokens": ["Mus", "Loth", "mitt", "Weib", "vnd", "Kind", "von", "dan\u00b7nen", "sich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "KON", "NN", "APPR", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In dem der brandt entsteht/ vnd sein besturtzt gemahl", "tokens": ["In", "dem", "der", "brandt", "ent\u00b7steht", "/", "vnd", "sein", "be\u00b7sturtzt", "ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$(", "KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mitt vmb gekehrten aug erblickt wie vber all", "tokens": ["Mitt", "vmb", "ge\u00b7kehr\u00b7ten", "aug", "er\u00b7blickt", "wie", "vber", "all"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "KOKOM", "APPR", "PIAT"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Mitt loh' vnd lichter glutt die h\u00e4user sind gekr\u00f6net;", "tokens": ["Mitt", "loh'", "vnd", "lich\u00b7ter", "glutt", "die", "h\u00e4u\u00b7ser", "sind", "ge\u00b7kr\u00f6\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "ADJD", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "F\u00fchlt sie das threnen saltz aus ihren augen rint/", "tokens": ["F\u00fchlt", "sie", "das", "thre\u00b7nen", "saltz", "aus", "ih\u00b7ren", "au\u00b7gen", "rint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd sie/ sie selbst wirdt saltz. Vnd ehr sie sich besinnt/", "tokens": ["Vnd", "sie", "/", "sie", "selbst", "wirdt", "saltz", ".", "Vnd", "ehr", "sie", "sich", "be\u00b7sinnt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "ADV", "VAFIN", "ADJD", "$.", "KON", "NN", "PPER", "PRF", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist durch die weise straff ihr vorwitz ausges\u00f6hnet.", "tokens": ["Ist", "durch", "die", "wei\u00b7se", "straff", "ihr", "vor\u00b7witz", "aus\u00b7ge\u00b7s\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}