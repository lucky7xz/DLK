{"textgrid.poem.58520": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "558. Vom Selbstvergessen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sich selbst vergessen und mit Dingen,", "tokens": ["Sich", "selbst", "ver\u00b7ges\u00b7sen", "und", "mit", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nichts sind, seine Zeit zubringen,", "tokens": ["Die", "nichts", "sind", ",", "sei\u00b7ne", "Zeit", "zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mensch, diese Torheit merk!", "tokens": ["Mensch", ",", "die\u00b7se", "Tor\u00b7heit", "merk", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich selbst vergessen, andre lehren,", "tokens": ["Sich", "selbst", "ver\u00b7ges\u00b7sen", ",", "and\u00b7re", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum selbst bekehrt, die Welt bekehren,", "tokens": ["Kaum", "selbst", "be\u00b7kehrt", ",", "die", "Welt", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist schlecht und mi\u00dflich Werk.", "tokens": ["Ist", "schlecht", "und", "mi\u00df\u00b7lich", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich selbst vergessen, Jesum sehen,", "tokens": ["Sich", "selbst", "ver\u00b7ges\u00b7sen", ",", "Je\u00b7sum", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich selbst nicht achten, Gott erh\u00f6hen,", "tokens": ["Sich", "selbst", "nicht", "ach\u00b7ten", ",", "Gott", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur dies gibt Mut und St\u00e4rk'.", "tokens": ["Nur", "dies", "gibt", "Mut", "und", "St\u00e4rk'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}