{"textgrid.poem.34884": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ja die verkehrte Welt,", "tokens": ["Das", "ist", "ja", "die", "ver\u00b7kehr\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir gehen auf den K\u00f6pfen!", "tokens": ["Wir", "ge\u00b7hen", "auf", "den", "K\u00f6p\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die J\u00e4ger werden dutzendweis'", "tokens": ["Die", "J\u00e4\u00b7ger", "wer\u00b7den", "dut\u00b7zend\u00b7weis'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschossen von den Schnepfen.", "tokens": ["Er\u00b7schos\u00b7sen", "von", "den", "Schnep\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die K\u00e4lber braten jetzt den Koch,", "tokens": ["Die", "K\u00e4l\u00b7ber", "bra\u00b7ten", "jetzt", "den", "Koch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Menschen reiten die G\u00e4ule;", "tokens": ["Auf", "Men\u00b7schen", "rei\u00b7ten", "die", "G\u00e4u\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcr Lehrfreiheit und Rechte des Lichts", "tokens": ["F\u00fcr", "Lehr\u00b7frei\u00b7heit", "und", "Rech\u00b7te", "des", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "K\u00e4mpft die katholische Eule.", "tokens": ["K\u00e4mpft", "die", "ka\u00b7tho\u00b7li\u00b7sche", "Eu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Der H\u00e4ring wird ein Sansculott',", "tokens": ["Der", "H\u00e4\u00b7ring", "wird", "ein", "Sans\u00b7cu\u00b7lott'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wahrheit sagt uns Bettine,", "tokens": ["Die", "Wahr\u00b7heit", "sagt", "uns", "Bet\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und ein gestiefelter Kater bringt", "tokens": ["Und", "ein", "ge\u00b7stie\u00b7fel\u00b7ter", "Ka\u00b7ter", "bringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Sophokles auf die B\u00fchne.", "tokens": ["Den", "So\u00b7phok\u00b7les", "auf", "die", "B\u00fch\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Affe l\u00e4\u00dft ein Pantheon", "tokens": ["Ein", "Af\u00b7fe", "l\u00e4\u00dft", "ein", "Pan\u00b7the\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbauen f\u00fcr deutsche Helden.", "tokens": ["Er\u00b7bau\u00b7en", "f\u00fcr", "deut\u00b7sche", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Ma\u00dfmann hat sich j\u00fcngst gek\u00e4mmt,", "tokens": ["Der", "Ma\u00df\u00b7mann", "hat", "sich", "j\u00fcngst", "ge\u00b7k\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie deutsche Bl\u00e4tter melden.", "tokens": ["Wie", "deut\u00b7sche", "Bl\u00e4t\u00b7ter", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Germanische B\u00e4ren glauben nicht mehr", "tokens": ["Ger\u00b7ma\u00b7ni\u00b7sche", "B\u00e4\u00b7ren", "glau\u00b7ben", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und werden Atheisten;", "tokens": ["Und", "wer\u00b7den", "A\u00b7theis\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Jedoch die franz\u00f6sischen Papagei'n,", "tokens": ["Je\u00b7doch", "die", "fran\u00b7z\u00f6\u00b7si\u00b7schen", "Pa\u00b7pa\u00b7gei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die werden gute Christen.", "tokens": ["Die", "wer\u00b7den", "gu\u00b7te", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im uckerm\u00e4rk'schen Moniteur,", "tokens": ["Im", "u\u00b7cker\u00b7m\u00e4rk'\u00b7schen", "Mo\u00b7ni\u00b7teur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hat man's am tollsten getrieben:", "tokens": ["Da", "hat", "man's", "am", "tolls\u00b7ten", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein Toter hat dem Lebenden dort", "tokens": ["Ein", "To\u00b7ter", "hat", "dem", "Le\u00b7ben\u00b7den", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die schn\u00f6deste Grabschrift geschrieben.", "tokens": ["Die", "schn\u00f6\u00b7des\u00b7te", "Grab\u00b7schrift", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "La\u00dft uns nicht schwimmen gegen den Strom,", "tokens": ["La\u00dft", "uns", "nicht", "schwim\u00b7men", "ge\u00b7gen", "den", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr Br\u00fcder! Es hilft uns wenig!", "tokens": ["Ihr", "Br\u00fc\u00b7der", "!", "Es", "hilft", "uns", "we\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00dft uns besteigen den Templower Berg", "tokens": ["La\u00dft", "uns", "be\u00b7stei\u00b7gen", "den", "Tem\u00b7plo\u00b7wer", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und rufen: \u00bbEs lebe der K\u00f6nig!\u00ab", "tokens": ["Und", "ru\u00b7fen", ":", "\u00bb", "Es", "le\u00b7be", "der", "K\u00f6\u00b7nig", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}