{"textgrid.poem.66903": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er ist nicht tot, der heute ruft. Er lebt.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist nicht tot, der heute ruft. Er lebt.", "tokens": ["Er", "ist", "nicht", "tot", ",", "der", "heu\u00b7te", "ruft", ".", "Er", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PRELS", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wagemut, der unsre Brust durchbebt,", "tokens": ["Der", "Wa\u00b7ge\u00b7mut", ",", "der", "uns\u00b7re", "Brust", "durch\u00b7bebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Stolz der Arbeit, unsrer Eintracht Wall,", "tokens": ["Der", "Stolz", "der", "Ar\u00b7beit", ",", "uns\u00b7rer", "Ein\u00b7tracht", "Wall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er lebt, er atmet \u2013 Ferdinand Lassalle!", "tokens": ["Er", "lebt", ",", "er", "at\u00b7met", "\u2013", "Fer\u00b7di\u00b7nand", "Las\u00b7sal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$(", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er redet \u2013 h\u00f6rt! Blank zieht er seine Worte.", "tokens": ["Er", "re\u00b7det", "\u2013", "h\u00f6rt", "!", "Blank", "zieht", "er", "sei\u00b7ne", "Wor\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$.", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Kolbenschlag \u2013 auf springt der Zukunft Pforte.", "tokens": ["Ein", "Kol\u00b7ben\u00b7schlag", "\u2013", "auf", "springt", "der", "Zu\u00b7kunft", "Pfor\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er redet Glut, und rastlos wallt der Brand", "tokens": ["Er", "re\u00b7det", "Glut", ",", "und", "rast\u00b7los", "wallt", "der", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der neuen Botschaft durch der Arbeit Land.", "tokens": ["Der", "neu\u00b7en", "Bot\u00b7schaft", "durch", "der", "Ar\u00b7beit", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwei Jahre nur! Zwei Jahre nur der Tat \u2013", "tokens": ["Zwei", "Jah\u00b7re", "nur", "!", "Zwei", "Jah\u00b7re", "nur", "der", "Tat", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$.", "CARD", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gebrochen war der Werkgenossen Pfad.", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "der", "Werk\u00b7ge\u00b7nos\u00b7sen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf seines F\u00fchrers Spur in sichren S\u00e4ulen", "tokens": ["Auf", "sei\u00b7nes", "F\u00fch\u00b7rers", "Spur", "in", "sich\u00b7ren", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vordrang das Volk mit seiner Schlagkraft Keulen.", "tokens": ["Vor\u00b7drang", "das", "Volk", "mit", "sei\u00b7ner", "Schlag\u00b7kraft", "Keu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir denken deiner, k\u00fchner Volkstribun,", "tokens": ["Wir", "den\u00b7ken", "dei\u00b7ner", ",", "k\u00fch\u00b7ner", "Volk\u00b7stri\u00b7bun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht, um beim Kranzeswinden auszuruhn.", "tokens": ["Nicht", ",", "um", "beim", "Kran\u00b7zes\u00b7win\u00b7den", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUI", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht, um dein Ich anbetend zu verg\u00f6ttern", "tokens": ["Nicht", ",", "um", "dein", "Ich", "an\u00b7be\u00b7tend", "zu", "ver\u00b7g\u00f6t\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUI", "PPOSAT", "PPER", "VVPP", "PTKZU", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Und deinen Ruhm ins Schmeichelhorn zu schmettern,", "tokens": ["Und", "dei\u00b7nen", "Ruhm", "ins", "Schmei\u00b7chel\u00b7horn", "zu", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht faul zu feiern, prahlend m\u00fc\u00dfiger Tro\u00df,", "tokens": ["Nicht", "faul", "zu", "fei\u00b7ern", ",", "prah\u00b7lend", "m\u00fc\u00b7\u00dfi\u00b7ger", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Wir denken k\u00e4mpfend deiner, Kampfgeno\u00df!", "tokens": ["Wir", "den\u00b7ken", "k\u00e4mp\u00b7fend", "dei\u00b7ner", ",", "Kampf\u00b7ge\u00b7no\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "An Wissen reich, ges\u00e4ttigt mit Genie,", "tokens": ["An", "Wis\u00b7sen", "reich", ",", "ge\u00b7s\u00e4t\u00b7tigt", "mit", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begriffst du, wie das Recht nach Geltung schrie.", "tokens": ["Be\u00b7griffst", "du", ",", "wie", "das", "Recht", "nach", "Gel\u00b7tung", "schrie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Leidenschaften sch\u00f6nster Katarakt,", "tokens": ["Der", "Lei\u00b7den\u00b7schaf\u00b7ten", "sch\u00f6ns\u00b7ter", "Ka\u00b7ta\u00b7rakt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Sturzflut der Idee hat dich gepackt,", "tokens": ["Die", "Sturz\u00b7flut", "der", "I\u00b7dee", "hat", "dich", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und sturmkeck schreitend durch Gewalt und L\u00fcge,", "tokens": ["Und", "sturm\u00b7keck", "schrei\u00b7tend", "durch", "Ge\u00b7walt", "und", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Rangst du das Recht der jungen Macht zum Siege.", "tokens": ["Rangst", "du", "das", "Recht", "der", "jun\u00b7gen", "Macht", "zum", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Verfolgter! Du hast gl\u00e4nzend dich gewehrt,", "tokens": ["Ver\u00b7folg\u00b7ter", "!", "Du", "hast", "gl\u00e4n\u00b7zend", "dich", "ge\u00b7wehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Triumph war deinen Strafen selbst beschert.", "tokens": ["Tri\u00b7umph", "war", "dei\u00b7nen", "Stra\u00b7fen", "selbst", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor deinem. R\u00e4cherblitz und Richtergrimme", "tokens": ["Vor", "dei\u00b7nem", ".", "R\u00e4\u00b7cher\u00b7blitz", "und", "Rich\u00b7ter\u00b7grim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verstummte schier der m\u00e4chtigen Kl\u00e4ger Stimme. \u2013", "tokens": ["Ver\u00b7stumm\u00b7te", "schier", "der", "m\u00e4ch\u00b7ti\u00b7gen", "Kl\u00e4\u00b7ger", "Stim\u00b7me", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du fielst abseits. Dein Werk kam nicht zu Fall.", "tokens": ["Du", "fielst", "ab\u00b7seits", ".", "Dein", "Werk", "kam", "nicht", "zu", "Fall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Du bist ger\u00e4cht. Du hast gesiegt, Lassalle!", "tokens": ["Du", "bist", "ge\u00b7r\u00e4cht", ".", "Du", "hast", "ge\u00b7siegt", ",", "Las\u00b7sal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}