{"textgrid.poem.41131": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sechzig Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer wird mir todesbang,", "tokens": ["Im\u00b7mer", "wird", "mir", "to\u00b7des\u00b7bang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich \u00fcberschlag',", "tokens": ["Wenn", "ich", "\u00fc\u00b7ber\u00b7schlag'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich sechzig Jahre lang", "tokens": ["Da\u00df", "ich", "sech\u00b7zig", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Nase trag'.", "tokens": ["Mei\u00b7ne", "Na\u00b7se", "trag'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df schon sechzig Jahre lang", "tokens": ["Da\u00df", "schon", "sech\u00b7zig", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "CARD", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blut durchs Herz mir flie\u00dft,", "tokens": ["Blut", "durchs", "Herz", "mir", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und dies Herz, so eng und bang,", "tokens": ["Und", "dies", "Herz", ",", "so", "eng", "und", "bang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht zersprungen ist.", "tokens": ["Nicht", "zer\u00b7sprun\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df mein Kopf, obgleich ich als", "tokens": ["Da\u00df", "mein", "Kopf", ",", "ob\u00b7gleich", "ich", "als"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4chtig ihn zerschellt,", "tokens": ["M\u00e4ch\u00b7tig", "ihn", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sechzig Jahr' auf meinem Hals", "tokens": ["Sech\u00b7zig", "Jahr'", "auf", "mei\u00b7nem", "Hals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Turmknopf h\u00e4lt.", "tokens": ["Wie", "ein", "Turm\u00b7knopf", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn ich jetzt zusammenbrech',", "tokens": ["Wenn", "ich", "jetzt", "zu\u00b7sam\u00b7men\u00b7brech'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klage nicht, mein Weib!", "tokens": ["Kla\u00b7ge", "nicht", ",", "mein", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aus B\u00fcffelhaut und Eisenblech", "tokens": ["Aus", "B\u00fcf\u00b7fel\u00b7haut", "und", "Ei\u00b7sen\u00b7blech"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besteht kein Menschenleib.", "tokens": ["Be\u00b7steht", "kein", "Men\u00b7schen\u00b7leib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}