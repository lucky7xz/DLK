{"textgrid.poem.59959": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie wohl ist der daran,", "genre": "verse", "period": "N.A.", "pub_year": 1653, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie wohl ist der daran,", "tokens": ["O", "wie", "wohl", "ist", "der", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "ART", "PAV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Der da kann", "tokens": ["Der", "da", "kann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sich begeistern,", "tokens": ["Sich", "be\u00b7geis\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Seine Feindin Sorge meistern,", "tokens": ["Sei\u00b7ne", "Fein\u00b7din", "Sor\u00b7ge", "meis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist bei Gl\u00fccke trotzig nicht,", "tokens": ["Ist", "bei", "Gl\u00fc\u00b7cke", "trot\u00b7zig", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zag im Leiden,", "tokens": ["Zag", "im", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Der beh\u00e4lt in Leid und Freuden", "tokens": ["Der", "be\u00b7h\u00e4lt", "in", "Leid", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lacht das Gl\u00fcck, er denkt bei sich:", "tokens": ["Lacht", "das", "Gl\u00fcck", ",", "er", "denkt", "bei", "sich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcte dich!", "tokens": ["H\u00fc\u00b7te", "dich", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sonnenscheinen", "tokens": ["Son\u00b7nen\u00b7schei\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Kehrt sich bald in Regenweinen.", "tokens": ["Kehrt", "sich", "bald", "in", "Re\u00b7gen\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute da auf stillem Meer", "tokens": ["Heu\u00b7te", "da", "auf", "stil\u00b7lem", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schiffe laufen,", "tokens": ["Schif\u00b7fe", "lau\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Morgen sieht man sie ersaufen", "tokens": ["Mor\u00b7gen", "sieht", "man", "sie", "er\u00b7sau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ungef\u00e4hr.", "tokens": ["Un\u00b7ge\u00b7f\u00e4hr", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Tobt das Gl\u00fcck, ihn trifft es nit:", "tokens": ["Tobt", "das", "Gl\u00fcck", ",", "ihn", "trifft", "es", "nit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Gem\u00fcth", "tokens": ["Sein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Felsengleiche", "tokens": ["Fel\u00b7sen\u00b7glei\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Weichet nie, wird niemals weiche.", "tokens": ["Wei\u00b7chet", "nie", ",", "wird", "nie\u00b7mals", "wei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VAFIN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er sitzt allzeit klippenfest,", "tokens": ["Er", "sitzt", "all\u00b7zeit", "klip\u00b7pen\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ob das Sausen,", "tokens": ["Ob", "das", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ob das tolle Nordenbrausen", "tokens": ["Ob", "das", "tol\u00b7le", "Nor\u00b7den\u00b7brau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf ihn bl\u00e4st.", "tokens": ["Auf", "ihn", "bl\u00e4st", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "La\u00df den Fels best\u00fcrmen sehr", "tokens": ["La\u00df", "den", "Fels", "be\u00b7st\u00fcr\u00b7men", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wind und Meer.", "tokens": ["Wind", "und", "Meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sturm und Wellen", "tokens": ["Sturm", "und", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "M\u00fcssen stets zur\u00fccke prellen.", "tokens": ["M\u00fcs\u00b7sen", "stets", "zu\u00b7r\u00fc\u00b7cke", "prel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es steht seiner Wurzeln Erz", "tokens": ["Es", "steht", "sei\u00b7ner", "Wur\u00b7zeln", "Erz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unverletzet;", "tokens": ["Un\u00b7ver\u00b7let\u00b7zet", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Nur sein Au\u00dfen wird benetzet,", "tokens": ["Nur", "sein", "Au\u00b7\u00dfen", "wird", "be\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht das Herz.", "tokens": ["Nicht", "das", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Tugend ist das rechte Gl\u00fcck,", "tokens": ["Tu\u00b7gend", "ist", "das", "rech\u00b7te", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zur\u00fcck", "tokens": ["Das", "zu\u00b7r\u00fcck"], "token_info": ["word", "word"], "pos": ["PDS", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nimmer weichet,", "tokens": ["Nim\u00b7mer", "wei\u00b7chet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Gl\u00fcck und Ungl\u00fcck wohl abgleichet.", "tokens": ["Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "wohl", "ab\u00b7glei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gro\u00dfmuth sich selbst Alles ist,", "tokens": ["Gro\u00df\u00b7muth", "sich", "selbst", "Al\u00b7les", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "PIS", "VAFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Wohnet innen,", "tokens": ["Woh\u00b7net", "in\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Macht, da\u00df du mit Stand der Sinnen", "tokens": ["Macht", ",", "da\u00df", "du", "mit", "Stand", "der", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00fccklich bist.", "tokens": ["Gl\u00fcck\u00b7lich", "bist", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}