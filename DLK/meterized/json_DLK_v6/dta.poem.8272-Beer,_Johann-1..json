{"dta.poem.8272": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Der Augen schwartze Sonnen", "tokens": ["Der", "Au\u00b7gen", "schwart\u00b7ze", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seynd nun fast ohne Licht", "tokens": ["Seynd", "nun", "fast", "oh\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil so viel Thr\u00e4nen-Saltz", "tokens": ["Weil", "so", "viel", "Thr\u00e4\u00b7nen\u00b7Saltz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch ihren Qvell geronnen;", "tokens": ["Durch", "ih\u00b7ren", "Qvell", "ge\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach! sie entz\u00fcnden nicht.", "tokens": ["Ach", "!", "sie", "ent\u00b7z\u00fcn\u00b7den", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die rothen Mund-Corallen", "tokens": ["Die", "ro\u00b7then", "Mun\u00b7dCoral\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Seynd gantz entf\u00e4rbt und bleich:", "tokens": ["Seynd", "gantz", "ent\u00b7f\u00e4rbt", "und", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch vieler Seuffzer Wind", "tokens": ["Durch", "vie\u00b7ler", "Seuff\u00b7zer", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist nun ihr Glantz verfallen:", "tokens": ["Ist", "nun", "ihr", "Glantz", "ver\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sehn sich nicht mehr gleich.", "tokens": ["Sie", "sehn", "sich", "nicht", "mehr", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}