{"dta.poem.14821": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die verlorene Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esolch' Augen hat das meine,", "tokens": ["\u201e", "solch'", "Au\u00b7gen", "hat", "das", "mei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach meines bist Du nicht,", "tokens": ["Ach", "mei\u00b7nes", "bist", "Du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ruht ja unter'm Steine,", "tokens": ["Das", "ruht", "ja", "un\u00b7ter'm", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den niemand mehr zerbricht.", "tokens": ["Den", "nie\u00b7mand", "mehr", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}