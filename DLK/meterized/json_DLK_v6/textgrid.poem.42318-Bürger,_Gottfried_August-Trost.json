{"textgrid.poem.42318": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Trost", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann dich die L\u00e4sterzunge sticht,", "tokens": ["Wann", "dich", "die", "L\u00e4s\u00b7ter\u00b7zun\u00b7ge", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df dir dies zum Troste sagen:", "tokens": ["So", "la\u00df", "dir", "dies", "zum", "Tros\u00b7te", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PDS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die schlechtsten Fr\u00fcchte sind es nicht,", "tokens": ["Die", "schlechts\u00b7ten", "Fr\u00fcch\u00b7te", "sind", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Woran die Wespen nagen.", "tokens": ["Wo\u00b7ran", "die", "Wes\u00b7pen", "na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}