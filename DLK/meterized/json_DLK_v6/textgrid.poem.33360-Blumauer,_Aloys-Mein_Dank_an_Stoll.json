{"textgrid.poem.33360": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Mein Dank an Stoll", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Leben, das uns geist- und weltliche Chikane", "tokens": ["Dies", "Le\u00b7ben", ",", "das", "uns", "geist", "und", "welt\u00b7li\u00b7che", "Chi\u00b7ka\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft zu verleiden sich so sehr bem\u00fcht,", "tokens": ["Oft", "zu", "ver\u00b7lei\u00b7den", "sich", "so", "sehr", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und das uns Armen doch zu schnell entflieht,", "tokens": ["Und", "das", "uns", "Ar\u00b7men", "doch", "zu", "schnell", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "NN", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das man, get\u00e4uscht von einem s\u00fcssen Wahne,", "tokens": ["Das", "man", ",", "ge\u00b7t\u00e4uscht", "von", "ei\u00b7nem", "s\u00fcs\u00b7sen", "Wah\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bald tropfenweis vom Nagel schl\u00fcrft,", "tokens": ["Bald", "trop\u00b7fen\u00b7weis", "vom", "Na\u00b7gel", "schl\u00fcrft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bald als eine B\u00fcrde von sich wirft,", "tokens": ["Und", "bald", "als", "ei\u00b7ne", "B\u00fcr\u00b7de", "von", "sich", "wirft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dies Leben, das du blos der Menschheit lebest,", "tokens": ["Dies", "Le\u00b7ben", ",", "das", "du", "blos", "der", "Menschheit", "le\u00b7best", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und jedem, der mit fleh'ndem Blick", "tokens": ["Und", "je\u00b7dem", ",", "der", "mit", "fleh'n\u00b7dem", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dir an sein Bette winkt, mit einem St\u00fcck", "tokens": ["Dir", "an", "sein", "Bet\u00b7te", "winkt", ",", "mit", "ei\u00b7nem", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Des Deinen zu verl\u00e4ngern strebest,", "tokens": ["Des", "Dei\u00b7nen", "zu", "ver\u00b7l\u00e4n\u00b7gern", "stre\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dies s\u00fc\u00dfe Leben, das auch mir", "tokens": ["Dies", "s\u00fc\u00b7\u00dfe", "Le\u00b7ben", ",", "das", "auch", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Noch werth ist, dank' ich, Theurer, dir!", "tokens": ["Noch", "werth", "ist", ",", "dank'", "ich", ",", "Theu\u00b7rer", ",", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADJD", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dir und den Edlen,", "tokens": ["Dir", "und", "den", "Ed\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Mich dem gewissen, nahen Tod entwand.", "tokens": ["Mich", "dem", "ge\u00b7wis\u00b7sen", ",", "na\u00b7hen", "Tod", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und sch\u00e4tzt' ich diese sonst zweideut'ge Gabe", "tokens": ["Und", "sch\u00e4tzt'", "ich", "die\u00b7se", "sonst", "zwei\u00b7deut'\u00b7ge", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Des Lebens gleich vordem nicht sehr,", "tokens": ["Des", "Le\u00b7bens", "gleich", "vor\u00b7dem", "nicht", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So freut es mich anjetzt um so viel mehr,", "tokens": ["So", "freut", "es", "mich", "an\u00b7jetzt", "um", "so", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADV", "ADV", "ADV", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Da ich von dir als ein Geschenk es habe.", "tokens": ["Da", "ich", "von", "dir", "als", "ein", "Ge\u00b7schenk", "es", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "KOUS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Du, Theurer, hast die Achtung mich gelehrt,", "tokens": ["Du", ",", "Theu\u00b7rer", ",", "hast", "die", "Ach\u00b7tung", "mich", "ge\u00b7lehrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die ich von nun an f\u00fcr mein Leben hege:", "tokens": ["Die", "ich", "von", "nun", "an", "f\u00fcr", "mein", "Le\u00b7ben", "he\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Denn ach, du fandest es so vieler Pflege,", "tokens": ["Denn", "ach", ",", "du", "fan\u00b7dest", "es", "so", "vie\u00b7ler", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "So vieler M\u00fch', so vieler Sorge werth! \u2013", "tokens": ["So", "vie\u00b7ler", "M\u00fch'", ",", "so", "vie\u00b7ler", "Sor\u00b7ge", "werth", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn also meine Denk- und Schreibmaschine", "tokens": ["Wenn", "al\u00b7so", "mei\u00b7ne", "Denk", "und", "Schreib\u00b7ma\u00b7schi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Gang noch bleibt, und ich damit bald der Vernunft", "tokens": ["Im", "Gang", "noch", "bleibt", ",", "und", "ich", "da\u00b7mit", "bald", "der", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,", "KON", "PPER", "PAV", "ADV", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zum Herold, bald der Fr\u00f6mmlerzunft", "tokens": ["Zum", "He\u00b7rold", ",", "bald", "der", "Fr\u00f6mm\u00b7ler\u00b7zunft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Aerger und Spektakel diene,", "tokens": ["Zum", "A\u00b7er\u00b7ger", "und", "Spek\u00b7ta\u00b7kel", "die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn mein Aeneas noch mit seiner Schafkopfsmiene,", "tokens": ["Wenn", "mein", "A\u00b7e\u00b7neas", "noch", "mit", "sei\u00b7ner", "Schaf\u00b7kopfs\u00b7mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit der er \u00fcber Narr'n und Schurken lacht,", "tokens": ["Mit", "der", "er", "\u00fc\u00b7ber", "Narr'n", "und", "Schur\u00b7ken", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So manchen Prediger auf seiner B\u00fchne,", "tokens": ["So", "man\u00b7chen", "Pre\u00b7di\u00b7ger", "auf", "sei\u00b7ner", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und manches Zwerchfell noch ert\u00f6nen macht,", "tokens": ["Und", "man\u00b7ches", "Zwerch\u00b7fell", "noch", "er\u00b7t\u00f6\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wenn am Ende Rom, durch so viel Lob ger\u00fchret,", "tokens": ["Und", "wenn", "am", "En\u00b7de", "Rom", ",", "durch", "so", "viel", "Lob", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "NE", "$,", "APPR", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mich etwa gar kanonisiret,", "tokens": ["Mich", "et\u00b7wa", "gar", "ka\u00b7no\u00b7ni\u00b7si\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "So ist's dein Werk! Denn ohne dich", "tokens": ["So", "ist's", "dein", "Werk", "!", "Denn", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "KON", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "W\u00e4r' ich, o Theurer, sicherlich", "tokens": ["W\u00e4r'", "ich", ",", "o", "Theu\u00b7rer", ",", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Trotz dem Verbot, aus Oestreich emigriret,", "tokens": ["Trotz", "dem", "Ver\u00b7bot", ",", "aus", "O\u00b7e\u00b7streich", "e\u00b7mi\u00b7gri\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Und h\u00e4tte dort in jener Welt", "tokens": ["Und", "h\u00e4t\u00b7te", "dort", "in", "je\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Virgilen schon bereits erz\u00e4hlt,", "tokens": ["Vir\u00b7gi\u00b7len", "schon", "be\u00b7reits", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df, so wie Rom sich metamorphosirte,", "tokens": ["Da\u00df", ",", "so", "wie", "Rom", "sich", "me\u00b7ta\u00b7mor\u00b7phos\u00b7ir\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "KOKOM", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.17": {"text": "Ich auch das Bild von seiner Hand,", "tokens": ["Ich", "auch", "das", "Bild", "von", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das l\u00e4ngst kein Mensch mehr \u00e4hnlich fand,", "tokens": ["Das", "l\u00e4ngst", "kein", "Mensch", "mehr", "\u00e4hn\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nun in ein and'res travestirte,", "tokens": ["Nun", "in", "ein", "an\u00b7d'\u00b7res", "tra\u00b7ves\u00b7tir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "PIS", "VVFIN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.20": {"text": "Und ich daher im Grunde nichts gethan,", "tokens": ["Und", "ich", "da\u00b7her", "im", "Grun\u00b7de", "nichts", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "APPRART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als was der gute, brave Mann", "tokens": ["Als", "was", "der", "gu\u00b7te", ",", "bra\u00b7ve", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PWS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "An meiner Stelle selbst \u2013 ich wette \u2013", "tokens": ["An", "mei\u00b7ner", "Stel\u00b7le", "selbst", "\u2013", "ich", "wet\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Gewi\u00df nicht unterlassen h\u00e4tte.", "tokens": ["Ge\u00b7wi\u00df", "nicht", "un\u00b7ter\u00b7las\u00b7sen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Du aber f\u00fchrtest mich zur\u00fcck in diese Welt", "tokens": ["Du", "a\u00b7ber", "f\u00fchr\u00b7test", "mich", "zu\u00b7r\u00fcck", "in", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In der es mir, trotz all' den Plagen,", "tokens": ["In", "der", "es", "mir", ",", "trotz", "all'", "den", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "$,", "APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die unsere Geduld parforcejagen,", "tokens": ["Die", "un\u00b7se\u00b7re", "Ge\u00b7duld", "par\u00b7for\u00b7ce\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Noch immer ziemlich wohl gef\u00e4llt.", "tokens": ["Noch", "im\u00b7mer", "ziem\u00b7lich", "wohl", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Du bist, o Theurer, all' den Kranken,", "tokens": ["Du", "bist", ",", "o", "Theu\u00b7rer", ",", "all'", "den", "Kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NN", "$,", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die ihres Daseyns Dau'r, wie ich, dir danken,", "tokens": ["Die", "ih\u00b7res", "Da\u00b7seyns", "Dau'r", ",", "wie", "ich", ",", "dir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "$,", "PWAV", "PPER", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Nicht blo\u00df der Arzt, der Arzenei verschreibt,", "tokens": ["Nicht", "blo\u00df", "der", "Arzt", ",", "der", "Ar\u00b7ze\u00b7nei", "ver\u00b7schreibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und unger\u00fchrt bei ihren Leiden bleibt,", "tokens": ["Und", "un\u00b7ge\u00b7r\u00fchrt", "bei", "ih\u00b7ren", "Lei\u00b7den", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Du bist zugleich ihr Freund, und theilest,", "tokens": ["Du", "bist", "zu\u00b7gleich", "ihr", "Freund", ",", "und", "thei\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Mit ihnen redlich jeden Schmerz,", "tokens": ["Mit", "ih\u00b7nen", "red\u00b7lich", "je\u00b7den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und wenn du sie von ihren Plagen heilest,", "tokens": ["Und", "wenn", "du", "sie", "von", "ih\u00b7ren", "Pla\u00b7gen", "hei\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "So heilest du zugleich \u2013 dein Herz.", "tokens": ["So", "hei\u00b7lest", "du", "zu\u00b7gleich", "\u2013", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sch\u00f6n ist's und gro\u00df, so vieler Menschen Leben", "tokens": ["Sch\u00f6n", "ist's", "und", "gro\u00df", ",", "so", "vie\u00b7ler", "Men\u00b7schen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "ADJD", "$,", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Zu retten, doch noch sch\u00f6ner, wenn daneben", "tokens": ["Zu", "ret\u00b7ten", ",", "doch", "noch", "sch\u00f6\u00b7ner", ",", "wenn", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "ADV", "ADJD", "$,", "KOUS", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Der Arzt zugleich, als seines Kranken Freund,", "tokens": ["Der", "Arzt", "zu\u00b7gleich", ",", "als", "sei\u00b7nes", "Kran\u00b7ken", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Auch seine Mitleids Thr\u00e4ne weint! \u2013", "tokens": ["Auch", "sei\u00b7ne", "Mit\u00b7leids", "Thr\u00e4\u00b7ne", "weint", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und kommt zu Schmerzen, die im K\u00f6rper w\u00fchlen,", "tokens": ["Und", "kommt", "zu", "Schmer\u00b7zen", ",", "die", "im", "K\u00f6r\u00b7per", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Erst noch ein Seelenleid hinzu,", "tokens": ["Erst", "noch", "ein", "See\u00b7len\u00b7leid", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "O wer versteht dann so, wie du,", "tokens": ["O", "wer", "ver\u00b7steht", "dann", "so", ",", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der kranken Seele selbst den Puls zu f\u00fchlen!", "tokens": ["Der", "kran\u00b7ken", "See\u00b7le", "selbst", "den", "Puls", "zu", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "D'rum nimm, o edler, theurer Mann!", "tokens": ["D'\u00b7rum", "nimm", ",", "o", "ed\u00b7ler", ",", "theu\u00b7rer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "FM", "ADJA", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Hier meinen Dank f\u00fcr deine Sorgfalt an;", "tokens": ["Hier", "mei\u00b7nen", "Dank", "f\u00fcr", "dei\u00b7ne", "Sorg\u00b7falt", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und wenn ich von der Nachwelt je gelesen", "tokens": ["Und", "wenn", "ich", "von", "der", "Nach\u00b7welt", "je", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Zu werden mir gew\u00fcnscht, so sey's, ob diesem Blatt,", "tokens": ["Zu", "wer\u00b7den", "mir", "ge\u00b7w\u00fcnscht", ",", "so", "sey's", ",", "ob", "die\u00b7sem", "Blatt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "$,", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Worauf ich kund den Zeitgenossen that,", "tokens": ["Wo\u00b7rauf", "ich", "kund", "den", "Zeit\u00b7ge\u00b7nos\u00b7sen", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Da\u00df Stoll mein Retter und mei