{"textgrid.poem.65037": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war der Hauch von deinem Munde,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war der Hauch von deinem Munde,", "tokens": ["Es", "war", "der", "Hauch", "von", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, was bewu\u00dftlos, still und tief", "tokens": ["Der", ",", "was", "be\u00b7wu\u00dft\u00b7los", ",", "still", "und", "tief"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geruht in meines Wesens Grunde,", "tokens": ["Ge\u00b7ruht", "in", "mei\u00b7nes", "We\u00b7sens", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor an's Licht des Tages rief.", "tokens": ["Her\u00b7vor", "an's", "Licht", "des", "Ta\u00b7ges", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn als dein Lied voll heil'ger Schmerzen", "tokens": ["Denn", "als", "dein", "Lied", "voll", "heil'\u00b7ger", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen Strahlen mich durchdrang,", "tokens": ["Mit", "sei\u00b7nen", "Strah\u00b7len", "mich", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Enthallte tr\u00e4umend meinem Herzen,", "tokens": ["Ent\u00b7hall\u00b7te", "tr\u00e4u\u00b7mend", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Memnonsbild, der erste Klang!", "tokens": ["Dem", "Mem\u00b7nons\u00b7bild", ",", "der", "ers\u00b7te", "Klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da nannte ich, mit Stolz und Wonne", "tokens": ["Da", "nann\u00b7te", "ich", ",", "mit", "Stolz", "und", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fc\u00dfend deine Herrlichkeit", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfend", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich meines Geistesmorgens Sonne,", "tokens": ["Dich", "mei\u00b7nes", "Geis\u00b7tes\u00b7mor\u00b7gens", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Fr\u00fchling meiner Jugendzeit! \u2013", "tokens": ["Den", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Ju\u00b7gend\u00b7zeit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der sch\u00f6ne Lenz, er ist vor\u00fcber,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Lenz", ",", "er", "ist", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr immer, ohne Wiederkehr!", "tokens": ["F\u00fcr", "im\u00b7mer", ",", "oh\u00b7ne", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich verh\u00fcllend, tr\u00fcb und tr\u00fcber,", "tokens": ["Und", "sich", "ver\u00b7h\u00fcl\u00b7lend", ",", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sinkt meine Sonne in das Meer!", "tokens": ["Sinkt", "mei\u00b7ne", "Son\u00b7ne", "in", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Memnonss\u00e4ule t\u00f6net wieder", "tokens": ["Die", "Mem\u00b7nons\u00b7s\u00e4u\u00b7le", "t\u00f6\u00b7net", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' sie f\u00fcr stets verstummen mu\u00df; \u2013", "tokens": ["Eh'", "sie", "f\u00fcr", "stets", "ver\u00b7stum\u00b7men", "mu\u00df", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bringen diese d\u00fcstern Lieder,", "tokens": ["Es", "brin\u00b7gen", "die\u00b7se", "d\u00fcs\u00b7tern", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlorner! dir den letzten Gru\u00df.", "tokens": ["Ver\u00b7lor\u00b7ner", "!", "dir", "den", "letz\u00b7ten", "Gru\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den letzten Gru\u00df, den letzten Segen", "tokens": ["Den", "letz\u00b7ten", "Gru\u00df", ",", "den", "letz\u00b7ten", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzens, welches treu und still,", "tokens": ["Des", "Her\u00b7zens", ",", "wel\u00b7ches", "treu", "und", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dir's gefolgt auf deinen Wegen,", "tokens": ["Wie", "dir's", "ge\u00b7folgt", "auf", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An deinem Weh verbluten will!", "tokens": ["An", "dei\u00b7nem", "Weh", "ver\u00b7blu\u00b7ten", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}