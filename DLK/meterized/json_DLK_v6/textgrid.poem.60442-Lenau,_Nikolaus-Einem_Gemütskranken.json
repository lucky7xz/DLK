{"textgrid.poem.60442": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Gem\u00fctskranken", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seitdem du mit den h\u00f6chsten M\u00e4chten", "tokens": ["Seit\u00b7dem", "du", "mit", "den", "h\u00f6chs\u00b7ten", "M\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begannst zu hadern und zu rechten,", "tokens": ["Be\u00b7gannst", "zu", "ha\u00b7dern", "und", "zu", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kann dir der kleinste, stillste Wurm", "tokens": ["Kann", "dir", "der", "kleins\u00b7te", ",", "stills\u00b7te", "Wurm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen wecken einen Sturm,", "tokens": ["Im", "Her\u00b7zen", "we\u00b7cken", "ei\u00b7nen", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie einst in jenen Fr\u00fchlingstagen,", "tokens": ["Wie", "einst", "in", "je\u00b7nen", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die dir kein Gott zur\u00fcck mehr ruft,", "tokens": ["Die", "dir", "kein", "Gott", "zu\u00b7r\u00fcck", "mehr", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "PTKVZ", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gr\u00fcnes Blatt, ein Hauch der Luft", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Blatt", ",", "ein", "Hauch", "der", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir oft gebracht ein seliges Behagen.", "tokens": ["Dir", "oft", "ge\u00b7bracht", "ein", "se\u00b7li\u00b7ges", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}