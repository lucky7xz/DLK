{"dta.poem.21260": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Wiederkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Ich, Kalliopens oft heimlich entflohener", "tokens": ["Ich", ",", "Kal\u00b7li\u00b7o\u00b7pens", "oft", "heim\u00b7lich", "ent\u00b7flo\u00b7he\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "ADV", "ADJD", "ADJA"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "J\u00fcnger, der ich, zu lange! dir,", "tokens": ["J\u00fcn\u00b7ger", ",", "der", "ich", ",", "zu", "lan\u00b7ge", "!", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "$,", "APPR", "ADV", "$.", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Strenge Kritika, dir, Schwester der eitelen", "tokens": ["Stren\u00b7ge", "Kri\u00b7ti\u00b7ka", ",", "dir", ",", "Schwes\u00b7ter", "der", "ei\u00b7te\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "$,", "NN", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Pansophia, gefolget bin,", "tokens": ["Pan\u00b7so\u00b7phia", ",", "ge\u00b7fol\u00b7get", "bin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kehre reuevoll um, eile voll Sehnsucht der", "tokens": ["Keh\u00b7re", "reu\u00b7e\u00b7voll", "um", ",", "ei\u00b7le", "voll", "Sehn\u00b7sucht", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "VVFIN", "ADJD", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Allgef\u00e4lligen G\u00f6ttinn zu.", "tokens": ["All\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7gen", "G\u00f6t\u00b7tinn", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Denn mein Tadel ", "tokens": ["Denn", "mein", "Ta\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Honig eingetaucht, schmerzete", "tokens": ["Ho\u00b7nig", "ein\u00b7ge\u00b7taucht", ",", "schmer\u00b7ze\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Meinen Selim; und noch schw\u00e4ret sein", "tokens": ["Mei\u00b7nen", "Se\u00b7lim", ";", "und", "noch", "schw\u00e4\u00b7ret", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$.", "KON", "ADV", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Ja! nun weih ich mich ewig der", "tokens": ["Ja", "!", "nun", "weih", "ich", "mich", "e\u00b7wig", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "ART"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Holden Muse! Mit ihr sang ich der W\u00e4lder", "tokens": ["Hol\u00b7den", "Mu\u00b7se", "!", "Mit", "ihr", "sang", "ich", "der", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Sang Ly\u00e4ens und Amors Lob:", "tokens": ["Sang", "Ly\u00b7\u00e4\u00b7ens", "und", "A\u00b7mors", "Lob", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NE", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Und mich liebte mein Freund. \u2014 O! sich", "tokens": ["Und", "mich", "lieb\u00b7te", "mein", "Freund", ".", "O", "!", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$(", "ITJ", "$.", "PRF"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Welche Seligkeit! Liebe, dich", "tokens": ["Wel\u00b7che", "Se\u00b7lig\u00b7keit", "!", "Lie\u00b7be", ",", "dich"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAT", "NN", "$.", "NN", "$,", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Tauscht mein trunkener Geist nicht um", "tokens": ["Tauscht", "mein", "trun\u00b7ke\u00b7ner", "Geist", "nicht", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Fingern, um der Versammelung", "tokens": ["Fin\u00b7gern", ",", "um", "der", "Ver\u00b7sam\u00b7me\u00b7lung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUI", "ART", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.17": {"text": "H\u00e4ndeklatschen, des Volks ehrebezeu-", "tokens": ["H\u00e4n\u00b7de\u00b7klat\u00b7schen", ",", "des", "Volks", "eh\u00b7re\u00b7be\u00b7zeu"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Aufstehn; dich um Gespr\u00e4che mit", "tokens": ["Auf\u00b7stehn", ";", "dich", "um", "Ge\u00b7spr\u00e4\u00b7che", "mit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "$.", "PRF", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Grossen K\u00f6nigen nicht, noch um die", "tokens": ["Gros\u00b7sen", "K\u00f6\u00b7ni\u00b7gen", "nicht", ",", "noch", "um", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKNEG", "$,", "ADV", "APPR", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Tafel ihrer Gewaltigen.", "tokens": ["Ta\u00b7fel", "ih\u00b7rer", "Ge\u00b7wal\u00b7ti\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}