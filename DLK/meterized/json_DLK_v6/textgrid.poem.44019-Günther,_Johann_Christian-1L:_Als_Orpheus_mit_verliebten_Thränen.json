{"textgrid.poem.44019": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Orpheus mit verliebten Thr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Orpheus mit verliebten Thr\u00e4nen", "tokens": ["Als", "Or\u00b7pheus", "mit", "ver\u00b7lieb\u00b7ten", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Abschied seiner Liebsten sang,", "tokens": ["Den", "Ab\u00b7schied", "sei\u00b7ner", "Liebs\u00b7ten", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewog des armen Dichters Sehnen", "tokens": ["Be\u00b7wog", "des", "ar\u00b7men", "Dich\u00b7ters", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sogar den todten Widerklang;", "tokens": ["So\u00b7gar", "den", "tod\u00b7ten", "Wi\u00b7der\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Thiere weinten in die Saythen,", "tokens": ["Die", "Thie\u00b7re", "wein\u00b7ten", "in", "die", "Say\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Steine starrten mehr als Stein,", "tokens": ["Die", "Stei\u00b7ne", "starr\u00b7ten", "mehr", "als", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sein Verdru\u00df bedr\u00e4ngter Zeiten", "tokens": ["Und", "sein", "Ver\u00b7dru\u00df", "be\u00b7dr\u00e4ng\u00b7ter", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nahm Feld und Wald mit Unmuth ein.", "tokens": ["Nahm", "Feld", "und", "Wald", "mit", "Un\u00b7muth", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So sah es jezo um Myrtillen", "tokens": ["So", "sah", "es", "je\u00b7zo", "um", "Myr\u00b7til\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und um die fetten Triften aus;", "tokens": ["Und", "um", "die", "fet\u00b7ten", "Trif\u00b7ten", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Zuspruch wust ihn mehr zu stillen,", "tokens": ["Kein", "Zu\u00b7spruch", "wust", "ihn", "mehr", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er lies sein weites Sch\u00e4ferhaus,", "tokens": ["Er", "lies", "sein", "wei\u00b7tes", "Sch\u00e4\u00b7fer\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begab sich taumelnd in die Heide,", "tokens": ["Be\u00b7gab", "sich", "tau\u00b7melnd", "in", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey der sich schon sein Vieh zerstreut,", "tokens": ["Bey", "der", "sich", "schon", "sein", "Vieh", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und klagte viel von seinem Leide", "tokens": ["Und", "klag\u00b7te", "viel", "von", "sei\u00b7nem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der hier verschwiegnen Einsamkeit.", "tokens": ["Der", "hier", "ver\u00b7schwieg\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was mu\u00df doch mancher Mensch nicht tragen!", "tokens": ["Was", "mu\u00df", "doch", "man\u00b7cher", "Mensch", "nicht", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun kommt das dritte Jahr ins Land,", "tokens": ["Nun", "kommt", "das", "drit\u00b7te", "Jahr", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit dem das Wachsthum meiner Plagen", "tokens": ["Seit", "dem", "das", "Wach\u00b7sthum", "mei\u00b7ner", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir allen Rath und Trost entwand.", "tokens": ["Mir", "al\u00b7len", "Rath", "und", "Trost", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gl\u00fccke greift mich allenthalben", "tokens": ["Das", "Gl\u00fc\u00b7cke", "greift", "mich", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zwar mit allen Pfeilen an;", "tokens": ["Und", "zwar", "mit", "al\u00b7len", "Pfei\u00b7len", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O da\u00df ich jezt nicht mit den Schwalben", "tokens": ["O", "da\u00df", "ich", "jezt", "nicht", "mit", "den", "Schwal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verschlafen oder fl\u00fcchten kan!", "tokens": ["Ver\u00b7schla\u00b7fen", "o\u00b7der", "fl\u00fcch\u00b7ten", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich will mein Creuz in Rechnung bringen,", "tokens": ["Ich", "will", "mein", "Creuz", "in", "Rech\u00b7nung", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menge l\u00e4st es nicht geschehn;", "tokens": ["Die", "Men\u00b7ge", "l\u00e4st", "es", "nicht", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will mich durch Verzweiflung zwingen,", "tokens": ["Ich", "will", "mich", "durch", "Ver\u00b7zwei\u00b7flung", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, d\u00fcrft ich keinen Himmel sehn.", "tokens": ["Ja", ",", "d\u00fcrft", "ich", "kei\u00b7nen", "Him\u00b7mel", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewohnheit macht die Noth ertr\u00e4glich,", "tokens": ["Ge\u00b7wohn\u00b7heit", "macht", "die", "Noth", "er\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jedoch nicht mir, sie ist stets neu,", "tokens": ["Je\u00b7doch", "nicht", "mir", ",", "sie", "ist", "stets", "neu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Himmel aber unbeweglich;", "tokens": ["Der", "Him\u00b7mel", "a\u00b7ber", "un\u00b7be\u00b7weg\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer sagt, wie mir zu Muthe sey?", "tokens": ["Wer", "sagt", ",", "wie", "mir", "zu", "Mu\u00b7the", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df mir's selber nicht zu sagen;", "tokens": ["Ich", "wei\u00df", "mir's", "sel\u00b7ber", "nicht", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer etwas davon wi\u00dfen will,", "tokens": ["Wer", "et\u00b7was", "da\u00b7von", "wi\u00b7\u00dfen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der geh nur hin, den Wald zu fragen,", "tokens": ["Der", "geh", "nur", "hin", ",", "den", "Wald", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und steh bey mancher Fichte still.", "tokens": ["Und", "steh", "bey", "man\u00b7cher", "Fich\u00b7te", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Kummer zeigt sich an den Heerden,", "tokens": ["Mein", "Kum\u00b7mer", "zeigt", "sich", "an", "den", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man sieht ihn selbst den Triften an,", "tokens": ["Man", "sieht", "ihn", "selbst", "den", "Trif\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn da\u00df sie beide mager werden,", "tokens": ["Denn", "da\u00df", "sie", "bei\u00b7de", "ma\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das hat mein fauler Gram gethan.", "tokens": ["Das", "hat", "mein", "fau\u00b7ler", "Gram", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich selbst verfalle vor den Jahren", "tokens": ["Ich", "selbst", "ver\u00b7fal\u00b7le", "vor", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zehre mich fast st\u00fcndlich ab", "tokens": ["Und", "zeh\u00b7re", "mich", "fast", "st\u00fcnd\u00b7lich", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dencke bey den grauen Haaren:", "tokens": ["Und", "den\u00b7cke", "bey", "den", "grau\u00b7en", "Haa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott geb, jezund erscheint das Grab.", "tokens": ["Gott", "geb", ",", "je\u00b7zund", "er\u00b7scheint", "das", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erschein ich einmahl auf den Festen,", "tokens": ["Er\u00b7schein", "ich", "ein\u00b7mahl", "auf", "den", "Fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So fragt mich jede Sch\u00e4ferin,", "tokens": ["So", "fragt", "mich", "je\u00b7de", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Warum ich bey so sch\u00f6nen G\u00e4sten", "tokens": ["Wa\u00b7rum", "ich", "bey", "so", "sch\u00f6\u00b7nen", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht aufger\u00e4umt und munter bin.", "tokens": ["Nicht", "auf\u00b7ge\u00b7r\u00e4umt", "und", "mun\u00b7ter", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mich selbst verdriest mein murrisch Wesen,", "tokens": ["Mich", "selbst", "ver\u00b7driest", "mein", "mur\u00b7risch", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gleichwohl \u00e4ndert mich kein Zwang;", "tokens": ["Und", "gleich\u00b7wohl", "\u00e4n\u00b7dert", "mich", "kein", "Zwang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Gl\u00fcck ist einmahl schon verlesen", "tokens": ["Mein", "Gl\u00fcck", "ist", "ein\u00b7mahl", "schon", "ver\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weis der Welt wohl wenig Danck.", "tokens": ["Und", "weis", "der", "Welt", "wohl", "we\u00b7nig", "Danck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kan bey keiner Arbeit bleiben,", "tokens": ["Ich", "kan", "bey", "kei\u00b7ner", "Ar\u00b7beit", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die unser Feldbau mit sich bringt,", "tokens": ["Die", "un\u00b7ser", "Feld\u00b7bau", "mit", "sich", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und weis vor Unmuth nichts zu treiben,", "tokens": ["Und", "weis", "vor", "Un\u00b7muth", "nichts", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das nur so obenhin gelingt.", "tokens": ["Das", "nur", "so", "o\u00b7ben\u00b7hin", "ge\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bald schniz ich etwan bunte St\u00e4be,", "tokens": ["Bald", "schniz", "ich", "et\u00b7wan", "bun\u00b7te", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da martert mich sogar das Bast,", "tokens": ["Da", "mar\u00b7tert", "mich", "so\u00b7gar", "das", "Bast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich abends Futter gebe,", "tokens": ["Und", "wenn", "ich", "a\u00b7bends", "Fut\u00b7ter", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird mir oft die Hand zur Last.", "tokens": ["So", "wird", "mir", "oft", "die", "Hand", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich deucht, die liebe Morgenr\u00f6the", "tokens": ["Mich", "deucht", ",", "die", "lie\u00b7be", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Steht \u00f6fters aus Erbarmung still,", "tokens": ["Steht", "\u00f6f\u00b7ters", "aus", "Er\u00b7bar\u00b7mung", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn nun die sonst getreue Fl\u00f6the", "tokens": ["Wenn", "nun", "die", "sonst", "ge\u00b7treu\u00b7e", "Fl\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kunst nicht mehr gehorchen will.", "tokens": ["Der", "Kunst", "nicht", "mehr", "ge\u00b7hor\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das Ungl\u00fcck kommt mir in Gedancken", "tokens": ["Das", "Un\u00b7gl\u00fcck", "kommt", "mir", "in", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn Ordnung und in Menge vor,", "tokens": ["Ohn", "Ord\u00b7nung", "und", "in", "Men\u00b7ge", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es heist mich auch in Tr\u00e4umen zancken", "tokens": ["Es", "heist", "mich", "auch", "in", "Tr\u00e4u\u00b7men", "zan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schw\u00e4cht mir t\u00e4glich Aug und Ohr;", "tokens": ["Und", "schw\u00e4cht", "mir", "t\u00e4g\u00b7lich", "Aug", "und", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald schmeist mich Filindrenens Leiche", "tokens": ["Bald", "schmeist", "mich", "Fi\u00b7lind\u00b7re\u00b7nens", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mit neuer Ohnmacht in den Staub,", "tokens": ["Mit", "neu\u00b7er", "Ohn\u00b7macht", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da zeigt mir Roschkowiz die Eiche,", "tokens": ["Da", "zeigt", "mir", "Roschko\u00b7wiz", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da denck ich an den s\u00fc\u00dfen Raub.", "tokens": ["Da", "denck", "ich", "an", "den", "s\u00fc\u00b7\u00dfen", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach Schweidniz, k\u00f6nt ich dich verge\u00dfen,", "tokens": ["Ach", "Schweid\u00b7niz", ",", "k\u00f6nt", "ich", "dich", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O was entbehrt ich jezt vor Gram!", "tokens": ["O", "was", "ent\u00b7behrt", "ich", "jezt", "vor", "Gram", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich habe deine Milch gege\u00dfen,", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7ne", "Milch", "ge\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit diesem acht ich keinen Ram.", "tokens": ["Seit", "die\u00b7sem", "acht", "ich", "kei\u00b7nen", "Ram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "CARD", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebt wohl und gr\u00fcnt, ihr fetten Auen,", "tokens": ["Lebt", "wohl", "und", "gr\u00fcnt", ",", "ihr", "fet\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und weidet Leonorens Brust,", "tokens": ["Und", "wei\u00b7det", "Le\u00b7o\u00b7no\u00b7rens", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich werd euch wohl nicht wiederschauen,", "tokens": ["Ich", "werd", "euch", "wohl", "nicht", "wie\u00b7der\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es machte denn ein Traum die Lust.", "tokens": ["Es", "mach\u00b7te", "denn", "ein", "Traum", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Albine war mir schlecht gewogen", "tokens": ["Al\u00b7bi\u00b7ne", "war", "mir", "schlecht", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hies der Anfang meiner Qual,", "tokens": ["Und", "hies", "der", "An\u00b7fang", "mei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch seit ich von ihr weggezogen,", "tokens": ["Doch", "seit", "ich", "von", "ihr", "weg\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedaur ich sie wohl tausendmahl,", "tokens": ["Be\u00b7daur", "ich", "sie", "wohl", "tau\u00b7send\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies macht die Freundschaft zweener Hirten;", "tokens": ["Dies", "macht", "die", "Freund\u00b7schaft", "zwee\u00b7ner", "Hir\u00b7ten", ";"], "token_i