{"textgrid.poem.28779": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilstu F\u00fcrsten Regeln geben,", "tokens": ["Wils\u00b7tu", "F\u00fcrs\u00b7ten", "Re\u00b7geln", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib der andren F\u00fcrsten Leben;", "tokens": ["Gib", "der", "an\u00b7dren", "F\u00fcrs\u00b7ten", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heb sie \u00fcber B\u00f6s empor,", "tokens": ["Heb", "sie", "\u00fc\u00b7ber", "B\u00f6s", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeuch nicht ihnen Be\u00dfre vor.", "tokens": ["Zeuch", "nicht", "ih\u00b7nen", "Be\u00df\u00b7re", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}