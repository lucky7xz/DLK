{"textgrid.poem.46047": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00dfgesang eines zerknirschten Herzens", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn abends uns die braune Nacht", "tokens": ["Wenn", "a\u00b7bends", "uns", "die", "brau\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Schatten schwarz verkleidet,", "tokens": ["In", "Schat\u00b7ten", "schwarz", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn dann ich meine Schuld betracht',", "tokens": ["Wenn", "dann", "ich", "mei\u00b7ne", "Schuld", "be\u00b7tracht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz in \u00c4ngsten streitet,", "tokens": ["Mein", "Herz", "in", "\u00c4ngs\u00b7ten", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Tr\u00e4nen, Leid, und Traurigkeit", "tokens": ["In", "Tr\u00e4\u00b7nen", ",", "Leid", ",", "und", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Augen mir zerrinnen,", "tokens": ["Die", "Au\u00b7gen", "mir", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zum Himmel auf, zum Sternenlauf,", "tokens": ["Zum", "Him\u00b7mel", "auf", ",", "zum", "Ster\u00b7nen\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schau' ich mit tr\u00fcben Sinnen!", "tokens": ["Schau'", "ich", "mit", "tr\u00fc\u00b7ben", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verweilt ihr Perlen schimmernd klar,", "tokens": ["Ver\u00b7weilt", "ihr", "Per\u00b7len", "schim\u00b7mernd", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr makellosen Lichter,", "tokens": ["Ihr", "ma\u00b7kel\u00b7lo\u00b7sen", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verweil' du Fackeltr\u00e4gerschar,", "tokens": ["Ver\u00b7weil'", "du", "Fa\u00b7ckel\u00b7tr\u00e4\u00b7ger\u00b7schar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hellflammend vor dem Richter!", "tokens": ["Hell\u00b7flam\u00b7mend", "vor", "dem", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sternhimmel, \u00fcber dem Er thront,", "tokens": ["Stern\u00b7him\u00b7mel", ",", "\u00fc\u00b7ber", "dem", "Er", "thront", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vernehme meine Klagen,", "tokens": ["Ver\u00b7neh\u00b7me", "mei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du Mond, der Ihm zu F\u00fc\u00dfen wohnt,", "tokens": ["Du", "Mond", ",", "der", "Ihm", "zu", "F\u00fc\u00b7\u00dfen", "wohnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00f6r' an mein Leid und Zagen!", "tokens": ["H\u00f6r'", "an", "mein", "Leid", "und", "Za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weh' mir! o Angst und Herzeleid!", "tokens": ["Weh'", "mir", "!", "o", "Angst", "und", "Her\u00b7ze\u00b7leid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$.", "FM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schuld bin ich umfangen;", "tokens": ["Mit", "Schuld", "bin", "ich", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf, auf, ihr hei\u00dfen Br\u00fcnnlein beid',", "tokens": ["Auf", ",", "auf", ",", "ihr", "hei\u00b7\u00dfen", "Br\u00fcnn\u00b7lein", "beid'", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun str\u00f6mt mir von den Wangen!", "tokens": ["Nun", "str\u00f6mt", "mir", "von", "den", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Klagt, sch\u00f6ne Sterne, meine Not,", "tokens": ["Klagt", ",", "sch\u00f6\u00b7ne", "Ster\u00b7ne", ",", "mei\u00b7ne", "Not", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Gott bin ich gewichen!", "tokens": ["Von", "Gott", "bin", "ich", "ge\u00b7wi\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach sch\u00f6ner Mond, in S\u00fcnde Tod", "tokens": ["Ach", "sch\u00f6\u00b7ner", "Mond", ",", "in", "S\u00fcn\u00b7de", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist meine Seel' erblichen!", "tokens": ["Ist", "mei\u00b7ne", "Seel'", "er\u00b7bli\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Str\u00f6m' ab, str\u00f6m' ab, du Tr\u00e4nenbad,", "tokens": ["Str\u00f6m'", "ab", ",", "str\u00f6m'", "ab", ",", "du", "Tr\u00e4\u00b7nen\u00b7bad", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leid kann dich nicht halten,", "tokens": ["Mein", "Leid", "kann", "dich", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rein wasche mich von Missetat,", "tokens": ["Rein", "wa\u00b7sche", "mich", "von", "Mis\u00b7se\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz ist mir zerspalten!", "tokens": ["Das", "Herz", "ist", "mir", "zer\u00b7spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O treuer Gott, all Dein Gebot", "tokens": ["O", "treu\u00b7er", "Gott", ",", "all", "Dein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hab' ich in Wind geschlagen!", "tokens": ["Hab'", "ich", "in", "Wind", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Fern von dem Herrn, zum S\u00fcndentod", "tokens": ["Fern", "von", "dem", "Herrn", ",", "zum", "S\u00fcn\u00b7den\u00b7tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat mich die Schuld getragen!", "tokens": ["Hat", "mich", "die", "Schuld", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie wird mir's nun vor Dir ergehn?", "tokens": ["Wie", "wird", "mir's", "nun", "vor", "Dir", "er\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Recht kann mich besch\u00f6nen!", "tokens": ["Kein", "Recht", "kann", "mich", "be\u00b7sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie soll ich nun vor Dir bestehn", "tokens": ["Wie", "soll", "ich", "nun", "vor", "Dir", "be\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Angesicht vers\u00f6hnen?", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "ver\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich war verkehrt, o Sch\u00f6pfer, wert!", "tokens": ["Ich", "war", "ver\u00b7kehrt", ",", "o", "Sch\u00f6p\u00b7fer", ",", "wert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "FM", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stumm mu\u00df ich vor Dir knieen,", "tokens": ["Stumm", "mu\u00df", "ich", "vor", "Dir", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wohl bin ich wert, da\u00df Feur und Schwert,", "tokens": ["Wohl", "bin", "ich", "wert", ",", "da\u00df", "Feur", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Recht an mir vollziehen!", "tokens": ["Das", "Recht", "an", "mir", "voll\u00b7zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr, stelle nicht in Eifermut", "tokens": ["Herr", ",", "stel\u00b7le", "nicht", "in", "Ei\u00b7fer\u00b7mut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir meine S\u00fcnd' entgegen,", "tokens": ["Dir", "mei\u00b7ne", "S\u00fcnd'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df nicht in des Gerichtes Glut", "tokens": ["La\u00df", "nicht", "in", "des", "Ge\u00b7rich\u00b7tes", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Strafe mich belegen.", "tokens": ["Mit", "Stra\u00b7fe", "mich", "be\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Gnadenbund macht mich gesund;", "tokens": ["Dein", "Gna\u00b7den\u00b7bund", "macht", "mich", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr! Nicht der S\u00fcnd' gedenke,", "tokens": ["Herr", "!", "Nicht", "der", "S\u00fcnd'", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach! jetzt zur Stund' zum Meeresgrund", "tokens": ["Ach", "!", "jetzt", "zur", "Stund'", "zum", "Mee\u00b7res\u00b7grund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "All meine Schuld versenke!", "tokens": ["All", "mei\u00b7ne", "Schuld", "ver\u00b7sen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr, gib, da\u00df ich mit Z\u00e4hren hei\u00df", "tokens": ["Herr", ",", "gib", ",", "da\u00df", "ich", "mit", "Z\u00e4h\u00b7ren", "hei\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir Deinen Zorn beg\u00fcte,", "tokens": ["Dir", "Dei\u00b7nen", "Zorn", "be\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mach mich recht schnee- und schwanenwei\u00df,", "tokens": ["Mach", "mich", "recht", "schnee", "und", "schwa\u00b7nen\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "TRUNC", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verleih' mir neue Bl\u00fcte!", "tokens": ["Ver\u00b7leih'", "mir", "neu\u00b7e", "Bl\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach! was geschehn, wer kann's umgehn?", "tokens": ["Ach", "!", "was", "ge\u00b7schehn", ",", "wer", "kann's", "um\u00b7gehn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVPP", "$,", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr, sieh mein Herz in Schmerze", "tokens": ["Herr", ",", "sieh", "mein", "Herz", "in", "Schmer\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Entflammet stehn, sieh! Tr\u00e4n' auf Tr\u00e4n'", "tokens": ["Ent\u00b7flam\u00b7met", "stehn", ",", "sieh", "!", "Tr\u00e4n'", "auf", "Tr\u00e4n'"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "VVIMP", "$.", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zerrinnt's gleich einer Kerze!", "tokens": ["Zer\u00b7rinnt's", "gleich", "ei\u00b7ner", "Ker\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach, d\u00fcrft' ich zu den Augen Dein,", "tokens": ["Ach", ",", "d\u00fcrft'", "ich", "zu", "den", 