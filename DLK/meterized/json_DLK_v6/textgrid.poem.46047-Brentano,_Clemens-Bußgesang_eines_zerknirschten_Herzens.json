, "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir bald Dein Herz durchbrechen!", "tokens": ["Dir", "bald", "Dein", "Herz", "durch\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da w\u00fcrd' Dein mildes Eingeweid',", "tokens": ["Da", "w\u00fcrd'", "Dein", "mil\u00b7des", "Ein\u00b7ge\u00b7weid'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Wachs im Feuer flie\u00dfen,", "tokens": ["Wie", "Wachs", "im", "Feu\u00b7er", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du w\u00fcrdest mich mit Armen beid", "tokens": ["Du", "w\u00fcr\u00b7dest", "mich", "mit", "Ar\u00b7men", "beid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Deine Wangen schlie\u00dfen,", "tokens": ["An", "Dei\u00b7ne", "Wan\u00b7gen", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr, spr\u00e4ch' ich dann, ach! nimm nur an,", "tokens": ["Herr", ",", "spr\u00e4ch'", "ich", "dann", ",", "ach", "!", "nimm", "nur", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "ITJ", "$.", "VVIMP", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Deiner gro\u00dfen Milde,", "tokens": ["Nach", "Dei\u00b7ner", "gro\u00b7\u00dfen", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nimm an geschwind, Dein armes Kind,", "tokens": ["Nimm", "an", "ge\u00b7schwind", ",", "Dein", "ar\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verirrt war's in der Wilde.", "tokens": ["Ver\u00b7irrt", "wa\u00b7r's", "in", "der", "Wil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Du w\u00fcrdest den verlornen Sohn", "tokens": ["Du", "w\u00fcr\u00b7dest", "den", "ver\u00b7lor\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Freuden gro\u00df empfangen,", "tokens": ["Mit", "Freu\u00b7den", "gro\u00df", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du g\u00e4bst ihm die verlorne Kron',", "tokens": ["Du", "g\u00e4bst", "ihm", "die", "ver\u00b7lor\u00b7ne", "Kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Kleinod reich umhangen.", "tokens": ["Mit", "Klei\u00b7nod", "reich", "um\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem neuen Kind lie\u00dft Du geschwind", "tokens": ["Dem", "neu\u00b7en", "Kind", "lie\u00dft", "Du", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Freudenmahl anr\u00fcsten,", "tokens": ["Ein", "Freu\u00b7den\u00b7mahl", "an\u00b7r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die bei Dir sind, Dein Hofgesind',", "tokens": ["Die", "bei", "Dir", "sind", ",", "Dein", "Hof\u00b7ge\u00b7sind'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "All mit Dir jubeln m\u00fc\u00dften.", "tokens": ["All", "mit", "Dir", "ju\u00b7beln", "m\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Nun bin ich's ja mitnichten wert!", "tokens": ["Nun", "bin", "ich's", "ja", "mit\u00b7nich\u00b7ten", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darf Dich nicht Vater nennen,", "tokens": ["Darf", "Dich", "nicht", "Va\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirst mich, der alles hat verzehrt,", "tokens": ["Wirst", "mich", ",", "der", "al\u00b7les", "hat", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr als Sohn erkennen!", "tokens": ["Nicht", "mehr", "als", "Sohn", "er\u00b7ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie soll ich's dann nur greifen an,", "tokens": ["Wie", "soll", "ich's", "dann", "nur", "grei\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wem, wie dann soll ich's klagen?", "tokens": ["Wem", ",", "wie", "dann", "soll", "ich's", "kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, ach! wer r\u00e4t, zwar ist's schon sp\u00e4t,", "tokens": ["Ach", ",", "ach", "!", "wer", "r\u00e4t", ",", "zwar", "ist's", "schon", "sp\u00e4t", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PWS", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch will ich nicht verzagen.", "tokens": ["Doch", "will", "ich", "nicht", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O stiller Mond, o Sterne still,", "tokens": ["O", "stil\u00b7ler", "Mond", ",", "o", "Ster\u00b7ne", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft euch mein Elend dauern!", "tokens": ["La\u00dft", "euch", "mein", "E\u00b7lend", "dau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bis mir mein Gott verzeihen will,", "tokens": ["Bis", "mir", "mein", "Gott", "ver\u00b7zei\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Helft klagen mir und trauern.", "tokens": ["Helft", "kla\u00b7gen", "mir", "und", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Sternenpracht, die winkt und lacht,", "tokens": ["O", "Ster\u00b7nen\u00b7pracht", ",", "die", "winkt", "und", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df Trauer dich umfalten,", "tokens": ["La\u00df", "Trau\u00b7er", "dich", "um\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und halt zur Nacht