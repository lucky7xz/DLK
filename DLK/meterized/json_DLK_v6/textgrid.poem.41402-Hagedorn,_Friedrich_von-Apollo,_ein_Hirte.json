{"textgrid.poem.41402": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Apollo, ein Hirte", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz gleicht den zufriednen Herzen,", "tokens": ["Mein", "Herz", "gleicht", "den", "zu\u00b7fried\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die Lieb' und freier Muth belebt,", "tokens": ["Die", "Lieb'", "und", "frei\u00b7er", "Muth", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die gern in sichrer Ruhe scherzen,", "tokens": ["Die", "gern", "in", "sich\u00b7rer", "Ru\u00b7he", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann rauschend Gl\u00fcck den Stolz erhebt.", "tokens": ["Wann", "rau\u00b7schend", "Gl\u00fcck", "den", "Stolz", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ehre g\u00f6nn' ich gr\u00f6\u00dfern Leuten,", "tokens": ["Die", "Eh\u00b7re", "g\u00f6nn'", "ich", "gr\u00f6\u00b7\u00dfern", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00fcnsche mir auf dieser Welt", "tokens": ["Und", "w\u00fcn\u00b7sche", "mir", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur den Genu\u00df der Z\u00e4rtlichkeiten,", "tokens": ["Nur", "den", "Ge\u00b7nu\u00df", "der", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Neid und Argwohn nicht verg\u00e4llt.", "tokens": ["Die", "Neid", "und", "Arg\u00b7wohn", "nicht", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was liebensw\u00fcrdig ist, zu lieben,", "tokens": ["Was", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "ist", ",", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat uns die paarende Natur", "tokens": ["Hat", "uns", "die", "paa\u00b7ren\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit unserm Blut ins Herz geschrieben,", "tokens": ["Mit", "un\u00b7serm", "Blut", "ins", "Herz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das entf\u00e4llt dem Alter nur.", "tokens": ["Und", "das", "ent\u00b7f\u00e4llt", "dem", "Al\u00b7ter", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erfinder weiser Schwermuthsgr\u00fcnde!", "tokens": ["Er\u00b7fin\u00b7der", "wei\u00b7ser", "Schwer\u00b7muths\u00b7gr\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man bei eurem Kl\u00fcgeln lacht,", "tokens": ["Wenn", "man", "bei", "eu\u00b7rem", "Kl\u00fc\u00b7geln", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So rechnet's der Natur zur S\u00fcnde,", "tokens": ["So", "rech\u00b7net's", "der", "Na\u00b7tur", "zur", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Da\u00df sie die Lust so reizend macht.", "tokens": ["Da\u00df", "sie", "die", "Lust", "so", "rei\u00b7zend", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verdru\u00df und Tadel zu verh\u00fcten,", "tokens": ["Ver\u00b7dru\u00df", "und", "Ta\u00b7del", "zu", "ver\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich mich unbemerkt erfreun;", "tokens": ["Will", "ich", "mich", "un\u00b7be\u00b7merkt", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht viel gehorchen noch gebieten,", "tokens": ["Nicht", "viel", "ge\u00b7hor\u00b7chen", "noch", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Sklav' und auch kein K\u00f6nig sein;", "tokens": ["Kein", "Skla\u00b7v'", "und", "auch", "kein", "K\u00f6\u00b7nig", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht blos mit Schein und Farben prangen,", "tokens": ["Nicht", "blos", "mit", "Schein", "und", "Far\u00b7ben", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die nur der P\u00f6bel trefflich hei\u00dft;", "tokens": ["Die", "nur", "der", "P\u00f6\u00b7bel", "treff\u00b7lich", "hei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kurz: wenig f\u00fcrchten und verlangen,", "tokens": ["Kurz", ":", "we\u00b7nig", "f\u00fcrch\u00b7ten", "und", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIS", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies ganz allein r\u00fchrt meinen Geist.", "tokens": ["Dies", "ganz", "al\u00b7lein", "r\u00fchrt", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als einsten Ph\u00f6bus von dem Himmel", "tokens": ["Als", "eins\u00b7ten", "Ph\u00f6\u00b7bus", "von", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gezwungen seinen Abschied nahm,", "tokens": ["Ge\u00b7zwun\u00b7gen", "sei\u00b7nen", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus der Oberwelt Get\u00fcmmel", "tokens": ["Und", "aus", "der", "O\u00b7ber\u00b7welt", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Freund Admetus kam;", "tokens": ["Zu", "sei\u00b7nem", "Freund", "Ad\u00b7me\u00b7tus", "kam", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da w\u00e4hlt' er sich ein freies Leben,", "tokens": ["Da", "w\u00e4hlt'", "er", "sich", "ein", "frei\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den angenehmen Sch\u00e4ferstand,", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Sch\u00e4\u00b7fer\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Sicherheit und Fried' umgeben,", "tokens": ["Den", "Si\u00b7cher\u00b7heit", "und", "Fried'", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Neid und Herrschsucht nie gekannt.", "tokens": ["Der", "Neid", "und", "Herrschsucht", "nie", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Hier konnt' er, zwischen Wald und Fl\u00fcssen,", "tokens": ["Hier", "konnt'", "er", ",", "zwi\u00b7schen", "Wald", "und", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ruhe Herz und Lieder weihn.", "tokens": ["Der", "Ru\u00b7he", "Herz", "und", "Lie\u00b7der", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er konnte dichten, lachen, k\u00fcssen:", "tokens": ["Er", "konn\u00b7te", "dich\u00b7ten", ",", "la\u00b7chen", ",", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedarf man mehr, vergn\u00fcgt zu sein?", "tokens": ["Be\u00b7darf", "man", "mehr", ",", "ver\u00b7gn\u00fcgt", "zu", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Gott verga\u00df, bei muntern Ch\u00f6ren,", "tokens": ["Der", "Gott", "ver\u00b7ga\u00df", ",", "bei", "mun\u00b7tern", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wann ihm ein holder Mund gefiel,", "tokens": ["Wann", "ihm", "ein", "hol\u00b7der", "Mund", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die stolze Harmonie der Sph\u00e4ren,", "tokens": ["Die", "stol\u00b7ze", "Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch nicht sein sanftes Saitenspiel.", "tokens": ["Doch", "nicht", "sein", "sanf\u00b7tes", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die besten L\u00e4mmer auf den Feldern,", "tokens": ["Die", "bes\u00b7ten", "L\u00e4m\u00b7mer", "auf", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die s\u00fc\u00df'ste Milch, den sch\u00f6nsten Strau\u00df,", "tokens": ["Die", "s\u00fc\u00df'\u00b7ste", "Milch", ",", "den", "sch\u00f6ns\u00b7ten", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die erste Frucht aus nahen W\u00e4ldern", "tokens": ["Die", "ers\u00b7te", "Frucht", "aus", "na\u00b7hen", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Las man f\u00fcr diesen Fremdling aus.", "tokens": ["Las", "man", "f\u00fcr", "die\u00b7sen", "Fremd\u00b7ling", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man fodert ihn zu allen Reihen;", "tokens": ["Man", "fo\u00b7dert", "ihn", "zu", "al\u00b7len", "Rei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Tanz schien artiger geziert,", "tokens": ["Kein", "Tanz", "schien", "ar\u00b7ti\u00b7ger", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als den er nach den Feldschalmeien", "tokens": ["Als", "den", "er", "nach", "den", "Feld\u00b7schal\u00b7mei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einer Hirtin aufgef\u00fchrt.", "tokens": ["Mit", "ei\u00b7ner", "Hir\u00b7tin", "auf\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Oft ward im Busch, bei ihren Schafen,", "tokens": ["Oft", "ward", "im", "Busch", ",", "bei", "ih\u00b7ren", "Scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein m\u00fcdes Kind von ihm entdeckt,", "tokens": ["Ein", "m\u00fc\u00b7des", "Kind", "von", "ihm", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wann sie l\u00e4chelnd eingeschlafen,", "tokens": ["Und", ",", "wann", "sie", "l\u00e4\u00b7chelnd", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihm bewacht, von ihm geweckt.", "tokens": ["Von", "ihm", "be\u00b7wacht", ",", "von", "ihm", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Oft wollten, um ihn zu gewinnen,", "tokens": ["Oft", "woll\u00b7ten", ",", "um", "ihn", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm andre froh entgegen gehn,", "tokens": ["Ihm", "and\u00b7re", "froh", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "wor