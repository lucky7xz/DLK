{"dta.poem.5966": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch einem st\u00e4rckern Stern, wird immer der, so schwach,", "tokens": ["Durch", "ei\u00b7nem", "st\u00e4r\u00b7ckern", "Stern", ",", "wird", "im\u00b7mer", "der", ",", "so", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "ART", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sammt seiner eigenen Bewegung, fortgerissen:", "tokens": ["Sammt", "sei\u00b7ner", "ei\u00b7ge\u00b7nen", "Be\u00b7we\u00b7gung", ",", "fort\u00b7ge\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie, wenn in seiner Wuht, ein aufgeschwollner Bach", "tokens": ["Wie", ",", "wenn", "in", "sei\u00b7ner", "Wuht", ",", "ein", "auf\u00b7ge\u00b7schwoll\u00b7ner", "Bach"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht, da\u00df durch schnellen Lauff die Fluhten sch\u00e4umen m\u00fcssen;", "tokens": ["Macht", ",", "da\u00df", "durch", "schnel\u00b7len", "Lauff", "die", "Fluh\u00b7ten", "sch\u00e4u\u00b7men", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Man Wirbel rollen sieht,", "tokens": ["Man", "Wir\u00b7bel", "rol\u00b7len", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wovon ein jeglicher sich um sich selbsten drehet,", "tokens": ["Wo\u00b7von", "ein", "jeg\u00b7li\u00b7cher", "sich", "um", "sich", "selbs\u00b7ten", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "PRF", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die aber doch der Bach, als der best\u00e4ndig gehet,", "tokens": ["Die", "a\u00b7ber", "doch", "der", "Bach", ",", "als", "der", "be\u00b7st\u00e4n\u00b7dig", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "$,", "KOUS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In dem besch\u00e4umten Schoo\u00df best\u00e4ndig mit sich zieht.", "tokens": ["In", "dem", "be\u00b7sch\u00e4um\u00b7ten", "Schoo\u00df", "be\u00b7st\u00e4n\u00b7dig", "mit", "sich", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}