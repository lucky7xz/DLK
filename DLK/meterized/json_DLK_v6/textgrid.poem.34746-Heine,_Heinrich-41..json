{"textgrid.poem.34746": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Greisenantlitz droben", "tokens": ["Wie", "ein", "Grei\u00b7sen\u00b7ant\u00b7litz", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Himmel anzuschauen,", "tokens": ["Ist", "der", "Him\u00b7mel", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rotein\u00e4ugig und umwoben", "tokens": ["Ro\u00b7tei\u00b7n\u00e4u\u00b7gig", "und", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Wolkenhaar, dem grauen.", "tokens": ["Von", "dem", "Wol\u00b7ken\u00b7haar", ",", "dem", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blickt er auf die Erde nieder,", "tokens": ["Blickt", "er", "auf", "die", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcssen welken Blum' und Bl\u00fcte,", "tokens": ["M\u00fcs\u00b7sen", "wel\u00b7ken", "Blum'", "und", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen welken Lieb' und Lieder", "tokens": ["M\u00fcs\u00b7sen", "wel\u00b7ken", "Lieb'", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem menschlichen Gem\u00fcte.", "tokens": ["In", "dem", "menschli\u00b7chen", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}