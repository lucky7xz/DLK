{"textgrid.poem.55440": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "20", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Berge denke gern, auch des Gesteins,", "tokens": ["Der", "Ber\u00b7ge", "den\u00b7ke", "gern", ",", "auch", "des", "Ge\u00b7steins", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie waren Zeugen freundlichsten Vereins,", "tokens": ["Sie", "wa\u00b7ren", "Zeu\u00b7gen", "freund\u00b7lichs\u00b7ten", "Ver\u00b7eins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.3": {"text": "Zutrauen, schnell gegeben, schnell gefunden,", "tokens": ["Zu\u00b7trau\u00b7en", ",", "schnell", "ge\u00b7ge\u00b7ben", ",", "schnell", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beschleunigte das Gl\u00fcck gez\u00e4hlter Stunden.", "tokens": ["Be\u00b7schleu\u00b7nig\u00b7te", "das", "Gl\u00fcck", "ge\u00b7z\u00e4hl\u00b7ter", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Behagen schaut nicht vorw\u00e4rts, nicht zur\u00fcck,", "tokens": ["Be\u00b7ha\u00b7gen", "schaut", "nicht", "vor\u00b7w\u00e4rts", ",", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und so verewigt sich der Augenblick!", "tokens": ["Und", "so", "ve\u00b7re\u00b7wigt", "sich", "der", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}