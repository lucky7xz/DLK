{"dta.poem.15584": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kannst in deinem Haus, dem n\u00e4chsten Tempel, beten,", "tokens": ["Du", "kannst", "in", "dei\u00b7nem", "Haus", ",", "dem", "n\u00e4chs\u00b7ten", "Tem\u00b7pel", ",", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und brauchst zum fernsten nicht die Wandrung anzutreten.", "tokens": ["Und", "brauchst", "zum", "ferns\u00b7ten", "nicht", "die", "Wand\u00b7rung", "an\u00b7zu\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "PTKNEG", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch zeugt dein Tempelgang, noch mehr die Pilgerschaft,", "tokens": ["Doch", "zeugt", "dein", "Tem\u00b7pel\u00b7gang", ",", "noch", "mehr", "die", "Pil\u00b7ger\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deiner Andacht Drang ist von besondrer Kraft.", "tokens": ["Da\u00df", "dei\u00b7ner", "An\u00b7dacht", "Drang", "ist", "von", "be\u00b7sond\u00b7rer", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT",