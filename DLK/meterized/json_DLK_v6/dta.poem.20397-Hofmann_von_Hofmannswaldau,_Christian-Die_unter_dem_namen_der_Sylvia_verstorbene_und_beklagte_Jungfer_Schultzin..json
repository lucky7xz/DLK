{"dta.poem.20397": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die unter dem namen der Sylvia  \n verstorbene und beklagte Jungfer  \n Schultzin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die muntre Sylvia/ ein spiegel seltner tugend/", "tokens": ["Die", "mun\u00b7tre", "Syl\u00b7via", "/", "ein", "spie\u00b7gel", "selt\u00b7ner", "tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein auszug der natur/ und bildni\u00df frischer jugend/", "tokens": ["Ein", "aus\u00b7zug", "der", "na\u00b7tur", "/", "und", "bild\u00b7ni\u00df", "fri\u00b7scher", "ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "KON", "NN", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die noch vor kurtzer zeit in unsrer Nymphen schaar", "tokens": ["Die", "noch", "vor", "kurt\u00b7zer", "zeit", "in", "uns\u00b7rer", "Nym\u00b7phen", "schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein pr\u00e4chtiger begriff der sch\u00f6nsten anmuth war.", "tokens": ["Ein", "pr\u00e4ch\u00b7ti\u00b7ger", "be\u00b7griff", "der", "sch\u00f6ns\u00b7ten", "an\u00b7muth", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach unsre Sylvia! die wir zu fr\u00fch vermissen/", "tokens": ["Ach", "uns\u00b7re", "Syl\u00b7via", "!", "die", "wir", "zu", "fr\u00fch", "ver\u00b7mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NE", "$.", "PRELS", "PPER", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wird durch des todes arm ins kalte grab gerissen/", "tokens": ["Wird", "durch", "des", "to\u00b7des", "arm", "ins", "kal\u00b7te", "grab", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und die/ als sonne/ sich erst auffzukl\u00e4ren schien/", "tokens": ["Und", "die", "/", "als", "son\u00b7ne", "/", "sich", "erst", "auff\u00b7zu\u00b7kl\u00e4\u00b7ren", "schien", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "KOUS", "NN", "$(", "PRF", "ADV", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mu\u00df ihrer strahlen gold schon unter wolcken ziehn.", "tokens": ["Mu\u00df", "ih\u00b7rer", "strah\u00b7len", "gold", "schon", "un\u00b7ter", "wol\u00b7cken", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie stirbet: aber wie? im morgen ihrer jahre;", "tokens": ["Sie", "stir\u00b7bet", ":", "a\u00b7ber", "wie", "?", "im", "mor\u00b7gen", "ih\u00b7rer", "jah\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PWAV", "$.", "APPRART", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihr braut-bett wird verkehrt in eine todten-bahre/", "tokens": ["Ihr", "braut\u00b7bett", "wird", "ver\u00b7kehrt", "in", "ei\u00b7ne", "tod\u00b7ten\u00b7bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die krone wandelt sich in einen leichen-krantz/", "tokens": ["Die", "kro\u00b7ne", "wan\u00b7delt", "sich", "in", "ei\u00b7nen", "lei\u00b7chen\u00b7krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das zimmer in den sarg; der hochzeitliche glantz", "tokens": ["Das", "zim\u00b7mer", "in", "den", "sarg", ";", "der", "hoch\u00b7zeit\u00b7li\u00b7che", "glantz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.13": {"text": "In eine dunckle nacht; der schmuck in sterbekittel/", "tokens": ["In", "ei\u00b7ne", "dunck\u00b7le", "nacht", ";", "der", "schmuck", "in", "ster\u00b7be\u00b7kit\u00b7tel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ART", "ADJD", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das frohe lust-geschrey in lauter klage-tittel;", "tokens": ["Das", "fro\u00b7he", "lust\u00b7ge\u00b7schrey", "in", "lau\u00b7ter", "kla\u00b7ge\u00b7tit\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das lachen in ein ach/ das jauchzen in geheul/", "tokens": ["Das", "la\u00b7chen", "in", "ein", "ach", "/", "das", "jauch\u00b7zen", "in", "ge\u00b7heul", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "XY", "$(", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und bey gemeiner noth tr\u00e4gt iederman sein theil.", "tokens": ["Und", "bey", "ge\u00b7mei\u00b7ner", "noth", "tr\u00e4gt", "ie\u00b7der\u00b7man", "sein", "theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.17": {"text": "Verworffener aprill! du anfang unsrer plage!", "tokens": ["Ver\u00b7worf\u00b7fe\u00b7ner", "ap\u00b7rill", "!", "du", "an\u00b7fang", "uns\u00b7rer", "pla\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein erster wird/ o leid! zum letzten ihrer tage;", "tokens": ["Dein", "ers\u00b7ter", "wird", "/", "o", "leid", "!", "zum", "letz\u00b7ten", "ih\u00b7rer", "ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "$(", "FM", "ADJD", "$.", "APPRART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein schein/ der sonsten nichts denn unbestand verspricht/", "tokens": ["Dein", "schein", "/", "der", "sons\u00b7ten", "nichts", "denn", "un\u00b7be\u00b7stand", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADJA", "PIS", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Scheint blo\u00df nur wider uns und unsre lust gericht.", "tokens": ["Scheint", "blo\u00df", "nur", "wi\u00b7der", "uns", "und", "uns\u00b7re", "lust", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du bist derselben itzt ein frecher st\u00f6hrer worden/", "tokens": ["Du", "bist", "der\u00b7sel\u00b7ben", "itzt", "ein", "fre\u00b7cher", "st\u00f6h\u00b7rer", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "ART", "ADJA", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wilt/ was uns ergetzt/ in Sylvien ermorden.", "tokens": ["Und", "wilt", "/", "was", "uns", "er\u00b7getzt", "/", "in", "Syl\u00b7vi\u00b7en", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "PWS", "PPER", "VVPP", "$(", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du nimmst den sonntag noch dir zum geh\u00fclffen ein/", "tokens": ["Du", "nimmst", "den", "sonn\u00b7tag", "noch", "dir", "zum", "ge\u00b7h\u00fclf\u00b7fen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PPER", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und dieser mu\u00df zu nechst beym schwartzen sonntag seyn/", "tokens": ["Und", "die\u00b7ser", "mu\u00df", "zu", "nechst", "beym", "schwart\u00b7zen", "sonn\u00b7tag", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PTKZU", "VVFIN", "APPRART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der doch mit besserm recht ein schwartzer tag zu nennen;", "tokens": ["Der", "doch", "mit", "bes\u00b7serm", "recht", "ein", "schwart\u00b7zer", "tag", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn ieder kan ihn ja an seiner w\u00fcrckung kennen/", "tokens": ["Denn", "ie\u00b7der", "kan", "ihn", "ja", "an", "sei\u00b7ner", "w\u00fcr\u00b7ckung", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er ist es/ der uns itzt ein schwartzes leid gebiehrt/", "tokens": ["Er", "ist", "es", "/", "der", "uns", "itzt", "ein", "schwart\u00b7zes", "leid", "ge\u00b7biehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PRELS", "PPER", "ADV", "ART", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der unsern sonnenschein zum schwartzen grabe f\u00fchrt.", "tokens": ["Der", "un\u00b7sern", "son\u00b7nen\u00b7schein", "zum", "schwart\u00b7zen", "gra\u00b7be", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der durch sein schwartzes bild uns allenthalben schrecket/", "tokens": ["Der", "durch", "sein", "schwart\u00b7zes", "bild", "uns", "al\u00b7len\u00b7thal\u00b7ben", "schre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und so viel klagende in schwartzen flohr verstecket.", "tokens": ["Und", "so", "viel", "kla\u00b7gen\u00b7de", "in", "schwart\u00b7zen", "flohr", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Man sagt/ die sonne selbst hab ihr erbla\u00dft gesicht", "tokens": ["Man", "sagt", "/", "die", "son\u00b7ne", "selbst", "hab", "ihr", "er\u00b7bla\u00dft", "ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "ART", "NN", "ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denselben tag verh\u00fcllt/ und sey vor kummer nicht/", "tokens": ["Den\u00b7sel\u00b7ben", "tag", "ver\u00b7h\u00fcllt", "/", "und", "sey", "vor", "kum\u00b7mer", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$(", "KON", "VAFIN", "APPR", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da unsre sonn entwich/ aus ihrer kammer kommen/", "tokens": ["Da", "uns\u00b7re", "sonn", "ent\u00b7wich", "/", "aus", "ih\u00b7rer", "kam\u00b7mer", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJD", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und Flora/ da sie hat die trauer-post vernommen/", "tokens": ["Und", "Flo\u00b7ra", "/", "da", "sie", "hat", "die", "trau\u00b7e\u00b7rpost", "ver\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "KOUS", "PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df Sylvia verf\u00e4llt im fr\u00fchling ihrer zeit/", "tokens": ["Da\u00df", "Syl\u00b7via", "ver\u00b7f\u00e4llt", "im", "fr\u00fch\u00b7ling", "ih\u00b7rer", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPRART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und einer blumen gleich vom stocke wird gemeyht.", "tokens": ["Und", "ei\u00b7ner", "blu\u00b7men", "gleich", "vom", "sto\u00b7cke", "wird", "ge\u00b7meyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPRART", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hat sie den gantzen tag die g\u00e4rten nicht beschicket/", "tokens": ["Hat", "sie", "den", "gant\u00b7zen", "tag", "die", "g\u00e4r\u00b7ten", "nicht", "be\u00b7schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und ihre kinder auch in der geburt ersticket/", "tokens": ["Und", "ih\u00b7re", "kin\u00b7der", "auch", "in", "der", "ge\u00b7burt", "er\u00b7sti\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Den Hyacinth hat man gantz einsam und verstellt/", "tokens": ["Den", "Hya\u00b7cin\u00b7th", "hat", "man", "gantz", "ein\u00b7sam", "und", "ver\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als eine leiche stehn/ und pl\u00f6tzlich abgef\u00e4llt/", "tokens": ["Als", "ei\u00b7ne", "lei\u00b7che", "stehn", "/", "und", "pl\u00f6tz\u00b7lich", "ab\u00b7ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVINF", "$(", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die veilgen aber sich sehr kl\u00e4glich sehen handeln/", "tokens": ["Die", "veil\u00b7gen", "a\u00b7ber", "sich", "sehr", "kl\u00e4g\u00b7lich", "se\u00b7hen", "han\u00b7deln", "/"], "token_info": ["word", "word", "