, "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Es ist die letzte pflicht/ die man der sel\u2019gen reicht/", "tokens": ["Es", "ist", "die", "letz\u00b7te", "pflicht", "/", "die", "man", "der", "sel'\u00b7gen", "reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PIS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der schmertz ist auch zu gro\u00df/ dem nichts gemeines gleicht;", "tokens": ["Der", "schmertz", "ist", "auch", "zu", "gro\u00df", "/", "dem", "nichts", "ge\u00b7mei\u00b7nes", "gleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "PTKA", "ADJD", "$(", "ART", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die wunden aber/ die der tod so tieff geschlagen/", "tokens": ["Die", "wun\u00b7den", "a\u00b7ber", "/", "die", "der", "tod", "so", "tieff", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$(", "PRELS", "ART", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Kan man/ so sehr man klagt/ doch nicht genung beklagen.", "tokens": ["Kan", "man", "/", "so", "sehr", "man", "klagt", "/", "doch", "nicht", "ge\u00b7nung", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$(", "ADV", "ADV", "PIS", "VVFIN", "$(", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ein mensch ist nur ein mensch/ nicht aber stahl und stein/", "tokens": ["Ein", "mensch", "ist", "nur", "ein", "mensch", "/", "nicht", "a\u00b7ber", "stahl", "und", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ART", "NN", "$(", "PTKNEG", "ADV", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und kan bey solchen ri\u00df nicht ohne regung seyn.", "tokens": ["Und", "kan", "bey", "sol\u00b7chen", "ri\u00df", "nicht", "oh\u00b7ne", "re\u00b7gung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "VVFIN", "PTKNEG", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ja/ da der himmel weint/ da so viel augen weinen/", "tokens": ["Ja", "/", "da", "der", "him\u00b7mel", "weint", "/", "da", "so", "viel", "au\u00b7gen", "wei\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "ADV", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wer wolto doch allein hier ohne thr\u00e4nen scheinen?", "tokens": ["Wer", "wol\u00b7to", "doch", "al\u00b7lein", "hier", "oh\u00b7ne", "thr\u00e4\u00b7nen", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "So weine/ wer da kan/ itzt ist es weinens-zeit;", "tokens": ["So", "wei\u00b7ne", "/", "wer", "da", "kan", "/", "itzt", "ist", "es", "wei\u0