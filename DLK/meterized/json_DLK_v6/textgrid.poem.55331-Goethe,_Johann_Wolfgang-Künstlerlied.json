{"textgrid.poem.55331": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstlerlied", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu erfinden, zu beschlie\u00dfen Bleibe,", "tokens": ["Zu", "er\u00b7fin\u00b7den", ",", "zu", "be\u00b7schlie\u00b7\u00dfen", "Blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fcnstler, oft allein,", "tokens": ["K\u00fcnst\u00b7ler", ",", "oft", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deines Wirkens zu genie\u00dfen,", "tokens": ["Dei\u00b7nes", "Wir\u00b7kens", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eile freudig zum Verein!", "tokens": ["Ei\u00b7le", "freu\u00b7dig", "zum", "Ver\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort im Ganzen schau, erfahre", "tokens": ["Dort", "im", "Gan\u00b7zen", "schau", ",", "er\u00b7fah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinen eignen Lebenslauf,", "tokens": ["Dei\u00b7nen", "eig\u00b7nen", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Taten mancher Jahre", "tokens": ["Und", "die", "Ta\u00b7ten", "man\u00b7cher", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gehn dir in dem Nachbar auf.", "tokens": ["Gehn", "dir", "in", "dem", "Nach\u00b7bar", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Gedanke, das Entwerfen,", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", ",", "das", "Ent\u00b7wer\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gestalten, ihr Bezug,", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", ",", "ihr", "Be\u00b7zug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines wird das andre sch\u00e4rfen,", "tokens": ["Ei\u00b7nes", "wird", "das", "and\u00b7re", "sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Ende sei's genug!", "tokens": ["Und", "am", "En\u00b7de", "sei's", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl erfunden, klug ersonnen,", "tokens": ["Wohl", "er\u00b7fun\u00b7den", ",", "klug", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6n gebildet, zart vollbracht,", "tokens": ["Sch\u00f6n", "ge\u00b7bil\u00b7det", ",", "zart", "voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So von jeher hat gewonnen", "tokens": ["So", "von", "je\u00b7her", "hat", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fcnstler kunstreich seine Macht.", "tokens": ["K\u00fcnst\u00b7ler", "kuns\u00b7treich", "sei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie Natur im Vielgebilde", "tokens": ["Wie", "Na\u00b7tur", "im", "Viel\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Gott nur offenbart,", "tokens": ["Ei\u00b7nen", "Gott", "nur", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So im weiten Kunstgefilde", "tokens": ["So", "im", "wei\u00b7ten", "Kunst\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Webt ein Sinn der ew'gen Art;", "tokens": ["Webt", "ein", "Sinn", "der", "ew'\u00b7gen", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses ist der Sinn der Wahrheit,", "tokens": ["Die\u00b7ses", "ist", "der", "Sinn", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sich nur mit Sch\u00f6nem schm\u00fcckt", "tokens": ["Der", "sich", "nur", "mit", "Sch\u00f6\u00b7nem", "schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und getrost der h\u00f6chsten Klarheit", "tokens": ["Und", "ge\u00b7trost", "der", "h\u00f6chs\u00b7ten", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hellsten Tags entgegenblickt.", "tokens": ["Hells\u00b7ten", "Tags", "ent\u00b7ge\u00b7gen\u00b7blickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie beherzt in Reim und Prose", "tokens": ["Wie", "be\u00b7herzt", "in", "Reim", "und", "Pro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Redner, Dichter sich ergehn,", "tokens": ["Red\u00b7ner", ",", "Dich\u00b7ter", "sich", "er\u00b7gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll des Lebens heitre Rose", "tokens": ["Soll", "des", "Le\u00b7bens", "heit\u00b7re", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frisch auf Malertafel stehn,", "tokens": ["Frisch", "auf", "Ma\u00b7ler\u00b7ta\u00b7fel", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Geschwistern reich umgeben,", "tokens": ["Mit", "Ge\u00b7schwis\u00b7tern", "reich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit des Herbstes Frucht umlegt,", "tokens": ["Mit", "des", "Herbs\u00b7tes", "Frucht", "um\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie von geheimem Leben", "tokens": ["Da\u00df", "sie", "von", "ge\u00b7hei\u00b7mem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Offenbaren Sinn erregt.", "tokens": ["Of\u00b7fen\u00b7ba\u00b7ren", "Sinn", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tausendfach und sch\u00f6n entflie\u00dfe", "tokens": ["Tau\u00b7send\u00b7fach", "und", "sch\u00f6n", "ent\u00b7flie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Form aus Formen deiner Hand,", "tokens": ["Form", "aus", "For\u00b7men", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Menschenbild genie\u00dfe,", "tokens": ["Und", "im", "Men\u00b7schen\u00b7bild", "ge\u00b7nie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ein Gott sich hergewandt.", "tokens": ["Da\u00df", "ein", "Gott", "sich", "her\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welch ein Werkzeug ihr gebrauchet,", "tokens": ["Welch", "ein", "Werk\u00b7zeug", "ihr", "ge\u00b7brau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stellet euch als Br\u00fcder dar;", "tokens": ["Stel\u00b7let", "euch", "als", "Br\u00fc\u00b7der", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und gesangweis flammt und rauchet", "tokens": ["Und", "ge\u00b7sang\u00b7weis", "flammt", "und", "rau\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Opfers\u00e4ule vom Altar.", "tokens": ["Op\u00b7fer\u00b7s\u00e4u\u00b7le", "vom", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}