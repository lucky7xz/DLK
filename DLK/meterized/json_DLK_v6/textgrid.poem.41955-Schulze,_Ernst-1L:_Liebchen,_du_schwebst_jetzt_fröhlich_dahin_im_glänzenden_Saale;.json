{"textgrid.poem.41955": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen, du schwebst jetzt fr\u00f6hlich dahin im gl\u00e4nzenden Saale;", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, du schwebst jetzt fr\u00f6hlich dahin im gl\u00e4nzenden Saale;", "tokens": ["Lieb\u00b7chen", ",", "du", "schwebst", "jetzt", "fr\u00f6h\u00b7lich", "da\u00b7hin", "im", "gl\u00e4n\u00b7zen\u00b7den", "Saa\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Leicht im fl\u00fcchtigen Tanz regst du den zierlichen Leib;", "tokens": ["Leicht", "im", "fl\u00fcch\u00b7ti\u00b7gen", "Tanz", "regst", "du", "den", "zier\u00b7li\u00b7chen", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "H\u00f6heres Roth durchrieselt die Wang', und es hebt der Begeistrung", "tokens": ["H\u00f6\u00b7he\u00b7res", "Roth", "durc\u00b7hrie\u00b7selt", "die", "Wang'", ",", "und", "es", "hebt", "der", "Be\u00b7geis\u00b7trung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ueppiger Rausch hochauf wogend die gl\u00fchende Brust.", "tokens": ["Uep\u00b7pi\u00b7ger", "Rausch", "hoch\u00b7auf", "wo\u00b7gend", "die", "gl\u00fc\u00b7hen\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Doch dein Freund, fern trauert er jetzt im stillen Gemache,", "tokens": ["Doch", "dein", "Freund", ",", "fern", "trau\u00b7ert", "er", "jetzt", "im", "stil\u00b7len", "Ge\u00b7ma\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wild um den br\u00fctenden Geist tobt ihm der Sorgen Gew\u00fchl.", "tokens": ["Wild", "um", "den", "br\u00fc\u00b7ten\u00b7den", "Geist", "tobt", "ihm", "der", "Sor\u00b7gen", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Ach wohl denkst du nicht mehr des Liebenden, welcher von dir nur", "tokens": ["Ach", "wohl", "denkst", "du", "nicht", "mehr", "des", "Lie\u00b7ben\u00b7den", ",", "wel\u00b7cher", "von", "dir", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Lernte die Lust, von dir, Einziggeliebte, den Schmerz;", "tokens": ["Lern\u00b7te", "die", "Lust", ",", "von", "dir", ",", "Ein\u00b7zig\u00b7ge\u00b7lieb\u00b7te", ",", "den", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "PPER", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "L\u00e4ngst wohl schwand im beth\u00f6renden Rausch des frohen Get\u00fcmmels", "tokens": ["L\u00e4ngst", "wohl", "schwand", "im", "be\u00b7th\u00f6\u00b7ren\u00b7den", "Rausch", "des", "fro\u00b7hen", "Ge\u00b7t\u00fcm\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Sein hind\u00e4mmerndes Bild ganz aus der Seele dir fort!", "tokens": ["Sein", "hin\u00b7d\u00e4m\u00b7mern\u00b7des", "Bild", "ganz", "aus", "der", "See\u00b7le", "dir", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Amor spannet so gern im Tanz die verstohlenen Netze,", "tokens": ["A\u00b7mor", "span\u00b7net", "so", "gern", "im", "Tanz", "die", "ver\u00b7stoh\u00b7le\u00b7nen", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Reichliche Beute belohnt immer den listigen Gott.", "tokens": ["Reich\u00b7li\u00b7che", "Beu\u00b7te", "be\u00b7lohnt", "im\u00b7mer", "den", "lis\u00b7ti\u00b7gen", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Lieblich bist du, wie nimmer ein anderes M\u00e4dchen der Erde;", "tokens": ["Lieb\u00b7lich", "bist", "du", ",", "wie", "nim\u00b7mer", "ein", "an\u00b7de\u00b7res", "M\u00e4d\u00b7chen", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Wer dir nahete, bleibt gern in der Fessel zur\u00fcck:", "tokens": ["Wer", "dir", "na\u00b7he\u00b7te", ",", "bleibt", "gern", "in", "der", "Fes\u00b7sel", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Doch du bist fl\u00fcchtig und leicht, wie die h\u00fcpfende Woge des Meeres,", "tokens": ["Doch", "du", "bist", "fl\u00fcch\u00b7tig", "und", "leicht", ",", "wie", "die", "h\u00fcp\u00b7fen\u00b7de", "Wo\u00b7ge", "des", "Mee\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Neues allein nur reizt immer den gaukelnden Sinn.", "tokens": ["Neu\u00b7es", "al\u00b7lein", "nur", "reizt", "im\u00b7mer", "den", "gau\u00b7keln\u00b7den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Wie mit B\u00e4llen das Kind, so spielst du mit Herzen; gelobst gern", "tokens": ["Wie", "mit", "B\u00e4l\u00b7len", "das", "Kind", ",", "so", "spielst", "du", "mit", "Her\u00b7zen", ";", "ge\u00b7lobst", "gern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$.", "ADV", "ADV"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Jeglichem, doch kein Gott, w\u00e4hnst du, bestrafe den Trug.", "tokens": ["Jeg\u00b7li\u00b7chem", ",", "doch", "kein", "Gott", ",", "w\u00e4hnst", "du", ",", "be\u00b7stra\u00b7fe", "den", "Trug", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "PIAT", "NN", "$,", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Oft schon nannt' ich dich falsch, und auf ewig wollt' ich dich meiden,", "tokens": ["Oft", "schon", "nannt'", "ich", "dich", "falsch", ",", "und", "auf", "e\u00b7wig", "wollt'", "ich", "dich", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,", "KON", "APPR", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Aber die z\u00f6gernde Flucht brachte mir neue Gefahr;", "tokens": ["A\u00b7ber", "die", "z\u00f6\u00b7gern\u00b7de", "Flucht", "brach\u00b7te", "mir", "neu\u00b7e", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Eifers\u00fcchtig schaltest du mich mit Lachen und th\u00f6richt,", "tokens": ["Ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "schal\u00b7test", "du", "mich", "mit", "La\u00b7chen", "und", "th\u00f6\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Und ein gl\u00fchender Ku\u00df machte von S\u00fcnden dich rein.", "tokens": ["Und", "ein", "gl\u00fc\u00b7hen\u00b7der", "Ku\u00df", "mach\u00b7te", "von", "S\u00fcn\u00b7den", "dich", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PPER", "ADJD", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Ach, jetzt windet ein Anderer wohl, aufwallend in Sehnsucht,", "tokens": ["Ach", ",", "jetzt", "win\u00b7det", "ein", "An\u00b7de\u00b7rer", "wohl", ",", "auf\u00b7wal\u00b7lend", "in", "Sehn\u00b7sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+--+--+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Rings um den zierlichen Leib leise den zitternden Arm,", "tokens": ["Rings", "um", "den", "zier\u00b7li\u00b7chen", "Leib", "lei\u00b7se", "den", "zit\u00b7tern\u00b7den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Lispelt mit kosendem Fl\u00fcstern im Sturm des wogenden Tanzes", "tokens": ["Lis\u00b7pelt", "mit", "ko\u00b7sen\u00b7dem", "Fl\u00fcs\u00b7tern", "im", "Sturm", "des", "wo\u00b7gen\u00b7den", "Tan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Manches verstohlene Wort liebeverlangend dir zu!", "tokens": ["Man\u00b7ches", "ver\u00b7stoh\u00b7le\u00b7ne", "Wort", "lie\u00b7be\u00b7ver\u00b7lan\u00b7gend", "dir", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "H\u00f6r' ihn nicht, er betr\u00fcgt dich nur, falsch ist der Verr\u00e4ther!", "tokens": ["H\u00f6r'", "ihn", "nicht", ",", "er", "be\u00b7tr\u00fcgt", "dich", "nur", ",", "falsch", "ist", "der", "Ver\u00b7r\u00e4\u00b7ther", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-++-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Ach, sein flatternder Sinn gleichet dem deinen, entflieh!", "tokens": ["Ach", ",", "sein", "flat\u00b7tern\u00b7der", "Sinn", "glei\u00b7chet", "dem", "dei\u00b7nen", ",", "ent\u00b7flieh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "PPOSAT", "$,", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Reicht er nicht jetzt dir die Hand? O hinweg mit ihr, sie ist giftig!", "tokens": ["Reicht", "er", "nicht", "jetzt", "dir", "die", "Hand", "?", "O", "hin\u00b7weg", "mit", "ihr", ",", "sie", "ist", "gif\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PPER", "ART", "NN", "$.", "NE", "APZR", "APPR", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Und Basiliskengewalt wohnt ihm im schmeichelnden Blick!", "tokens": ["Und", "Ba\u00b7si\u00b7lis\u00b7ken\u00b7ge\u00b7walt", "wohnt", "ihm", "im", "schmei\u00b7cheln\u00b7den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "O, verh\u00fcllte dich doch ein undurchdringlicher Schleier,", "tokens": ["O", ",", "ver\u00b7h\u00fcll\u00b7te", "dich", "doch", "ein", "un\u00b7durch\u00b7dring\u00b7li\u00b7cher", "Schlei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "K\u00f6nnte nur ich allein sicher die Reizende sehn,", "tokens": ["K\u00f6nn\u00b7te", "nur", "ich", "al\u00b7lein", "si\u00b7cher", "die", "Rei\u00b7zen\u00b7de", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.33": {"text": "W\u00e4r' es doch mir allein nur verg\u00f6nnt an der Brust dir zu ruhen,", "tokens": ["W\u00e4r'", "es", "doch", "mir", "al\u00b7lein", "nur", "ver\u00b7g\u00f6nnt", "an", "der", "Brust", "dir", "zu", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "D\u00fcrfte nur ich allein k\u00fcssen den rosigen Mund!", "tokens": ["D\u00fcrf\u00b7te", "nur", "ich", "al\u00b7lein", "k\u00fcs\u00b7sen", "den", "ro\u00b7si\u00b7gen", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Aber ich selber erschuf mir die qu\u00e4lende Sorg' in dem Herzen,", "tokens": ["A\u00b7ber", "ich", "sel\u00b7ber", "er\u00b7schuf", "mir", "die", "qu\u00e4\u00b7len\u00b7de", "Sor\u00b7g'", "in", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+---+-", "measure": "dactylic.tetra.plus"}, "line.36": {"text": "Und mein eignes Vergehn raubte mir heute die Ruh.", "tokens": ["Und", "mein", "eig\u00b7nes", "Ver\u00b7gehn", "raub\u00b7te", "mir", "heu\u00b7te", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Wehe, Warum auch z\u00fcrnet' ich gleich, als den ersten der T\u00e4nze", "tokens": ["We\u00b7he", ",", "Wa\u00b7rum", "auch", "z\u00fcr\u00b7net'", "ich", "gleich", ",", "als", "den", "ers\u00b7ten", "der", "T\u00e4n\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "ADJA", "ART", "NN"], "meter": "+--+-+--+--+--+-", 