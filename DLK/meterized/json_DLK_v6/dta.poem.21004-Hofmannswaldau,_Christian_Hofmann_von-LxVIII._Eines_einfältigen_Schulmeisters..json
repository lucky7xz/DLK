{"dta.poem.21004": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxVIII.  \n Eines einf\u00e4ltigen Schulmeisters.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ", "tokens": ["Hier", "liegt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Er wolte F\u00fchrer seyn/ und war doch selbsten blind.", "tokens": ["Er", "wol\u00b7te", "F\u00fch\u00b7rer", "seyn", "/", "und", "war", "doch", "selbs\u00b7ten", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$(", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer auff den Grabestein sich nur wird wollen se-", "tokens": ["Wer", "auff", "den", "Gra\u00b7be\u00b7stein", "sich", "nur", "wird", "wol\u00b7len", "se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "ADV", "VAFIN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "tzen/", "tokens": ["tzen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Dem wird die d\u00fcrre Faust den Sessel weidlich fetzen.", "tokens": ["Dem", "wird", "die", "d\u00fcr\u00b7re", "Faust", "den", "Ses\u00b7sel", "weid\u00b7lich", "fet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}