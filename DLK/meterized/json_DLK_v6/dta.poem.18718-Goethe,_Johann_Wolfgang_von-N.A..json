{"dta.poem.18718": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Und wer franzet oder brittet,", "tokens": ["Und", "wer", "fran\u00b7zet", "o\u00b7der", "brit\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Itali\u00e4nert oder teutschet,", "tokens": ["I\u00b7ta\u00b7li\u00b7\u00e4\u00b7nert", "o\u00b7der", "teut\u00b7schet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einer will nur wie der andre", "tokens": ["Ei\u00b7ner", "will", "nur", "wie", "der", "and\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "KOKOM", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die Eigenliebe heischet.", "tokens": ["Was", "die", "Ei\u00b7gen\u00b7lie\u00b7be", "hei\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn es ist kein Anerkennen,", "tokens": ["Denn", "es", "ist", "kein", "An\u00b7er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weder vieler, noch des einen,", "tokens": ["We\u00b7der", "vie\u00b7ler", ",", "noch", "des", "ei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "ADV", "ART", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn es nicht am Tage f\u00f6rdert", "tokens": ["Wenn", "es", "nicht", "am", "Ta\u00b7ge", "f\u00f6r\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man selbst was m\u00f6chte scheinen.", "tokens": ["Wo", "man", "selbst", "was", "m\u00f6ch\u00b7te", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PWS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Morgen habe denn das Rechte", "tokens": ["Mor\u00b7gen", "ha\u00b7be", "denn", "das", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Freunde wohlgesinnet,", "tokens": ["Sei\u00b7ne", "Freun\u00b7de", "wohl\u00b7ge\u00b7sin\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn nur heute noch das Schlechte", "tokens": ["Wenn", "nur", "heu\u00b7te", "noch", "das", "Schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vollen Platz und Gunst gewinnet.", "tokens": ["Vol\u00b7len", "Platz", "und", "Gunst", "ge\u00b7win\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer nicht von dreytausend Jahren", "tokens": ["Wer", "nicht", "von", "drey\u00b7tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich weiss Rechenschaft zu geben,", "tokens": ["Sich", "weiss", "Re\u00b7chen\u00b7schaft", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleib im Dunkeln unerfahren,", "tokens": ["Bleib", "im", "Dun\u00b7keln", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag von Tag zu Tage leben.", "tokens": ["Mag", "von", "Tag", "zu", "Ta\u00b7ge", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}