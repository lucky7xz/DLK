{"textgrid.poem.39226": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie hat den ganzen Tag getobt", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hat den ganzen Tag getobt", "tokens": ["Sie", "hat", "den", "gan\u00b7zen", "Tag", "ge\u00b7tobt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie in Zorn und Pein,", "tokens": ["Als", "wie", "in", "Zorn", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun bettet sich, nun gl\u00e4ttet sich", "tokens": ["Nun", "bet\u00b7tet", "sich", ",", "nun", "gl\u00e4t\u00b7tet", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "$,", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die See und schlummert ein.", "tokens": ["Die", "See", "und", "schlum\u00b7mert", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und dr\u00fcber zittert der Abendwind,", "tokens": ["Und", "dr\u00fc\u00b7ber", "zit\u00b7tert", "der", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein mildes, heiliges Wehn,", "tokens": ["Ein", "mil\u00b7des", ",", "hei\u00b7li\u00b7ges", "Wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das ist der Atem Gottes,", "tokens": ["Das", "ist", "der", "A\u00b7tem", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schwebet ob den See'n.", "tokens": ["Der", "schwe\u00b7bet", "ob", "den", "See'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VVFIN", "KOUS", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es k\u00fc\u00dft der Herr aufs Lockenhaupt", "tokens": ["Es", "k\u00fc\u00dft", "der", "Herr", "aufs", "Lo\u00b7cken\u00b7haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schlummernde See gelind", "tokens": ["Die", "schlum\u00b7mern\u00b7de", "See", "ge\u00b7lind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und spricht mit s\u00e4uselndem Segen:", "tokens": ["Und", "spricht", "mit", "s\u00e4u\u00b7seln\u00b7dem", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlaf' ruhig, wildes Kind!", "tokens": ["Schlaf'", "ru\u00b7hig", ",", "wil\u00b7des", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}