{"textgrid.poem.40940": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jez stelletich! \u2013 Du doher, hani gseit!", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jez stelletich! \u2013 Du doher, hani gseit!", "tokens": ["Jez", "stel\u00b7le\u00b7tich", "!", "\u2013", "Du", "do\u00b7her", ",", "ha\u00b7ni", "gseit", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "$(", "PPER", "ADJA", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und du d\u00f6rt mit dim gro\u00dfe Dreispitz links!", "tokens": ["Und", "du", "d\u00f6rt", "mit", "dim", "gro\u00b7\u00dfe", "Drei\u00b7spitz", "links", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und neig si jeds, und bettet lisli no!", "tokens": ["Und", "neig", "si", "jeds", ",", "und", "bet\u00b7tet", "lis\u00b7li", "no", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$,", "KON", "VVFIN", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Do bringi, liebi gn\u00e4digi F\u00fcrstefrau,", "tokens": ["Do", "brin\u00b7gi", ",", "lie\u00b7bi", "gn\u00e4\u00b7di\u00b7gi", "F\u00fcrs\u00b7te\u00b7frau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "ne ganzi Hochzit usem Hauestei", "tokens": ["ne", "gan\u00b7zi", "Hoch\u00b7zit", "u\u00b7sem", "Hau\u00b7es\u00b7tei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "vo Herischwand. Vor vierzeh Johre hen", "tokens": ["vo", "He\u00b7rischwand", ".", "Vor", "vier\u00b7zeh", "Joh\u00b7re", "hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "si alli 's ABC no bi mer glehrt", "tokens": ["si", "al\u00b7li", "'s", "AbC", "no", "bi", "mer", "glehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und treui F\u00fcrsteliebi. \u2013 Der do het", "tokens": ["und", "treu\u00b7i", "F\u00fcr\u00b7ste\u00b7lie\u00b7bi", ".", "\u2013", "Der", "do", "het"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "$(", "ART", "ADV", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "scho in der Schuel gern 's Mariannli gseh", "tokens": ["scho", "in", "der", "Schu\u00b7el", "gern", "'s", "Ma\u00b7ri\u00b7ann\u00b7li", "gseh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "PPER", "NE", "NE"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "und T\u00f6pli ghobe f\u00fcr's. Drum, d'Liebi het", "tokens": ["und", "T\u00f6p\u00b7li", "gho\u00b7be", "f\u00fcr'", "s.", "Drum", ",", "d'\u00b7Lie\u00b7bi", "het"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "punct", "word", "word"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "kei Zit. Jez endli vor Michelitag", "tokens": ["kei", "Zit", ".", "Jez", "end\u00b7li", "vor", "Mi\u00b7che\u00b7li\u00b7tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "NE", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "hen's d'V\u00e4tter usgmacht. \u2013 \u00bbLoset\u00ab, hani gseit,", "tokens": ["hen's", "d'\u00b7V\u00e4t\u00b7ter", "us\u00b7gmacht", ".", "\u2013", "\u00bb", "Lo\u00b7set", "\u00ab", ",", "ha\u00b7ni", "gseit", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$(", "$(", "NE", "$(", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "\u00bbl\u00f6nt's mittem Chilchgang, mittem Freudesprung", "tokens": ["\u00bb", "l\u00f6nt's", "mit\u00b7tem", "Chilch\u00b7gang", ",", "mit\u00b7tem", "Freu\u00b7des\u00b7prung"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "NE", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "no Zit ha bis zum heilige Stephanstag!", "tokens": ["no", "Zit", "ha", "bis", "zum", "hei\u00b7li\u00b7ge", "Ste\u00b7phans\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Mer g\u00f6hn go Carlisrueh! Wer wei\u00df, es macht", "tokens": ["Mer", "g\u00f6hn", "go", "Car\u00b7lis\u00b7rueh", "!", "Wer", "wei\u00df", ",", "es", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "FM", "FM", "FM", "$.", "PWS", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "der liebi F\u00fcrstin au ne chleini Freud.", "tokens": ["der", "lie\u00b7bi", "F\u00fcrs\u00b7tin", "au", "ne", "chlei\u00b7ni", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sie isch jo au zu \u00fcs cho. Gro\u00dfi Freud", "tokens": ["Sie", "isch", "jo", "au", "zu", "\u00fcs", "cho", ".", "Gro\u00b7\u00dfi", "Freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NE", "NE", "NE", "APPR", "NE", "NE", "$.", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "isch's gsi im Land.\u00ab \u2013 O gn\u00e4digi F\u00fcrstefrau,", "tokens": ["isch's", "gsi", "im", "Land", ".", "\u00ab", "\u2013", "O", "gn\u00e4\u00b7di\u00b7gi", "F\u00fcrs\u00b7te\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$.", "$(", "$(", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "mer ch\u00f6nnes nie vergesse. D'Muetter seit's", "tokens": ["mer", "ch\u00f6n\u00b7nes", "nie", "ver\u00b7ges\u00b7se", ".", "D'\u00b7Mu\u00b7et\u00b7ter", "seit's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "im Chindli uffem Scho\u00df, und 's Chindli lacht", "tokens": ["im", "Chind\u00b7li", "uf\u00b7fem", "Scho\u00df", ",", "und", "'s", "Chind\u00b7li", "lacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "KON", "PPER", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "und zuckt vor Freude. Dankich Gott der Her", "tokens": ["und", "zuckt", "vor", "Freu\u00b7de", ".", "Dan\u00b7kich", "Gott", "der", "Her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "f\u00fcr Eui Liebi, und was Euer Herz", "tokens": ["f\u00fcr", "Eu\u00b7i", "Lie\u00b7bi", ",", "und", "was", "Eu\u00b7er", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "$,", "KON", "PWS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "erfreue mag, das gebich Gott! \u2013 's erfreut", "tokens": ["er\u00b7freu\u00b7e", "mag", ",", "das", "ge\u00b7bich", "Gott", "!", "\u2013", "'s", "er\u00b7freut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "VMFIN", "$,", "PRELS", "ADJD", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "viel tausig, tausig Herze. \u00dcsereis", "tokens": ["viel", "tau\u00b7sig", ",", "tau\u00b7sig", "Her\u00b7ze", ".", "\u00dc\u00b7se\u00b7reis"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "cha's nit so sagen, au ne Schuelher nit.", "tokens": ["cha's", "nit", "so", "sa\u00b7gen", ",", "au", "ne", "Schu\u00b7el\u00b7her", "nit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "VVINF", "$,", "NE", "NE", "NN", "PTKNEG", "$."], "meter": "-+-+-+----+", "measure": "unknown.measure.tetra"}, "line.26": {"text": "\u2013 's isch viel gseit. \u2013 Bring der lieb Gott gsund und froh", "tokens": ["\u2013", "'s", "isch", "viel", "gseit", ".", "\u2013", "Bring", "der", "lieb", "Gott", "gsund", "und", "froh"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADJD", "PIAT", "NN", "$.", "$(", "VVFIN", "ART", "ADJD", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "bald wieder \u00fcse Heren in sein Schlo\u00df,", "tokens": ["bald", "wie\u00b7der", "\u00fc\u00b7se", "He\u00b7ren", "in", "sein", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "und segne seine Kronen und sein Haus", "tokens": ["und", "seg\u00b7ne", "sei\u00b7ne", "Kro\u00b7nen", "und", "sein", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "auf sp\u00e4te Zeit! \u2013 Sin Eui Chinder brav?", "tokens": ["auf", "sp\u00e4\u00b7te", "Zeit", "!", "\u2013", "Sin", "Eu\u00b7i", "Chin\u00b7der", "brav", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$(", "NE", "NE", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "'s gr\u00f6\u00dft wird jez bald in d'Schuel go, denki wol.", "tokens": ["'s", "gr\u00f6\u00dft", "wird", "jez", "bald", "in", "d'\u00b7Schuel", "go", ",", "den\u00b7ki", "wol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "ADV", "ADV", "APPR", "NE", "NE", "$,", "PRELS", "ADV", "$."], "meter": "-+-+--+-++-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Erhalt Gott ihri B\u00e4ckli frisch und rot,", "tokens": ["Er\u00b7halt", "Gott", "ih\u00b7ri", "B\u00e4ck\u00b7li", "frisch", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "und schenkene der Muetter ch\u00f6stlig Herz,", "tokens": ["und", "schen\u00b7ke\u00b7ne", "der", "Mu\u00b7et\u00b7ter", "ch\u00f6st\u00b7lig", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.33": {"text": "und bald e Br\u00fcederli. \u2013 Jez weihet au", "tokens": ["und", "bald", "e", "Br\u00fce\u00b7der\u00b7li", ".", "\u2013", "Jez", "wei\u00b7het", "au"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "NE", "NE", "$.", "$(", "NE", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "mi P\u00e4rli do mit Euem liebe Blick,", "tokens": ["mi", "P\u00e4r\u00b7li", "do", "mit", "Eu\u00b7em", "lie\u00b7be", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "und ch\u00f6mmet, wenn der Maie wieder gr\u00fcent,", "tokens": ["und", "ch\u00f6m\u00b7met", ",", "wenn", "der", "Mai\u00b7e", "wie\u00b7der", "gr\u00fcent", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "und Bluest zue neue Freudechr\u00e4nze bringt,", "tokens": ["und", "Blu\u00b7est", "zue", "neu\u00b7e", "Freu\u00b7dech\u00b7r\u00e4n\u00b7ze", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "au wieder ufe! \u2013 's g'rotet Frucht und Wi", "tokens": ["au", "wie\u00b7der", "u\u00b7fe", "!", "\u2013", "'s", "g'\u00b7ro\u00b7tet", "Frucht", "und", "Wi"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "$.", "$(", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "nit, bis der wieder in der N\u00f6chi sind,", "tokens": ["nit", ",", "bis", "der", "wie\u00b7der", "in", "der", "N\u00f6\u00b7chi", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "APPR", "ART", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "und Sege bringet, wie im Johrgang \u00d6lf.", "tokens": ["und", "Se\u00b7ge", "brin\u00b7get", ",", "wie", "im", "Johr\u00b7gang", "\u00d6lf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PWAV", "APPRART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "'s isch Sege, wo der sind. \u2013 Jez, Mariann,", "tokens": ["'s", "isch", "Se\u00b7ge", ",", "wo", "der", "sind", ".", "\u2013", "Jez", ",", "Ma\u00b7ri\u00b7ann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PWAV", "ART", "VAFIN", "$.", "$(", "NE", "$,", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.41": {"text": "gang, gib's Papierli umme! Bisch nit sch\u00fcch,", "tokens": ["gang", ",", "gib's", "Pa\u00b7pier\u00b7li", "um\u00b7me", "!", "Bisch", "nit", "sch\u00fcch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "VVFIN", "$.", "NE", "PTKNEG", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.42": {"text": "und neig di zimpfer! Zeig!", "tokens": ["und", "neig", "di", "zimp\u00b7fer", "!", "Zeig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}