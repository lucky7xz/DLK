{"textgrid.poem.44081": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, der du dich zu aller Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, der du dich zu aller Zeit", "tokens": ["Gott", ",", "der", "du", "dich", "zu", "al\u00b7ler", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir, was du bist, erwiesen,", "tokens": ["Mir", ",", "was", "du", "bist", ",", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verzeih der faulen Danckbarkeit,", "tokens": ["Ver\u00b7zeih", "der", "fau\u00b7len", "Dan\u00b7ck\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie hat es schlecht gepriesen,", "tokens": ["Sie", "hat", "es", "schlecht", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und gieb den Fehler mit Gedult", "tokens": ["Und", "gieb", "den", "Feh\u00b7ler", "mit", "Ge\u00b7dult"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem unerfahrnen Alter Schuld,", "tokens": ["Dem", "un\u00b7er\u00b7fahr\u00b7nen", "Al\u00b7ter", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In dem wir th\u00f6richt handeln", "tokens": ["In", "dem", "wir", "th\u00f6\u00b7richt", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wegen Mangel an Verstand", "tokens": ["Und", "we\u00b7gen", "Man\u00b7gel", "an", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So wie ein Schatten an der Wand", "tokens": ["So", "wie", "ein", "Schat\u00b7ten", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nach jedem Scheine wandeln.", "tokens": ["Nach", "je\u00b7dem", "Schei\u00b7ne", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Blendwerck sch\u00f6n- und eitler Lust", "tokens": ["Das", "Blend\u00b7werck", "sch\u00f6n", "und", "eit\u00b7ler", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4llt den jungen Jahren;", "tokens": ["Ge\u00b7f\u00e4llt", "den", "jun\u00b7gen", "Jah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Feuer wallt uns in der Brust,", "tokens": ["Das", "Feu\u00b7er", "wallt", "uns", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mit der Welt zu paaren,", "tokens": ["Sich", "mit", "der", "Welt", "zu", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es reizt uns Wollust, Trunck und Pracht,", "tokens": ["Es", "reizt", "uns", "Wol\u00b7lust", ",", "Trunck", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und weil es keiner be\u00dfer macht,", "tokens": ["Und", "weil", "es", "kei\u00b7ner", "be\u00b7\u00dfer", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So folgt ein Schaaf dem Haufen;", "tokens": ["So", "folgt", "ein", "Schaaf", "dem", "Hau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja, eh sich Fleisch und Blut bequemt", "tokens": ["Ja", ",", "eh", "sich", "Fleisch", "und", "Blut", "be\u00b7quemt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PRF", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und der Verstand den Willen z\u00e4hmt,", "tokens": ["Und", "der", "Ver\u00b7stand", "den", "Wil\u00b7len", "z\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mu\u00df mancher Strom entlaufen.", "tokens": ["Mu\u00df", "man\u00b7cher", "Strom", "ent\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Darum, mein Gott, verk\u00fcrze nicht", "tokens": ["Da\u00b7rum", ",", "mein", "Gott", ",", "ver\u00b7k\u00fcr\u00b7ze", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00e4lfte meiner Tage", "tokens": ["Die", "H\u00e4lf\u00b7te", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und las mir deiner Wei\u00dfheit Licht,", "tokens": ["Und", "las", "mir", "dei\u00b7ner", "Wei\u00df\u00b7heit", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wornach ich eifrig jage.", "tokens": ["Wor\u00b7nach", "ich", "eif\u00b7rig", "ja\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jezt komm ich erstlich auf die Spur,", "tokens": ["Jezt", "komm", "ich", "erst\u00b7lich", "auf", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Worauf mir Einsehn und Natur", "tokens": ["Wo\u00b7rauf", "mir", "Ein\u00b7sehn", "und", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein reifer K\u00e4ntn\u00fc\u00df geben;", "tokens": ["Ein", "rei\u00b7fer", "K\u00e4nt\u00b7n\u00fc\u00df", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich seh die S\u00fcnd- und Tugendbahn", "tokens": ["Ich", "seh", "die", "S\u00fcn\u00b7d", "und", "Tu\u00b7gend\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mit ungleich sch\u00e4rfern Augen an", "tokens": ["Mit", "un\u00b7gleich", "sch\u00e4r\u00b7fern", "Au\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und denck erst recht zu leben.", "tokens": ["Und", "denck", "erst", "recht", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}