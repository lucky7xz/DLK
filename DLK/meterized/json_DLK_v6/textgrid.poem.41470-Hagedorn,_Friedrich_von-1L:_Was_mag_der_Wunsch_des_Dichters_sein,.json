{"textgrid.poem.41470": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was mag der Wunsch des Dichters sein,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mag der Wunsch des Dichters sein,", "tokens": ["Was", "mag", "der", "Wunsch", "des", "Dich\u00b7ters", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der den geweihten Phoebus bittet?", "tokens": ["Der", "den", "ge\u00b7weih\u00b7ten", "Phoe\u00b7bus", "bit\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ruft er ihn an, da er den neuen Wein", "tokens": ["Und", "was", "ruft", "er", "ihn", "an", ",", "da", "er", "den", "neu\u00b7en", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus seiner Opferschale sch\u00fcttet?", "tokens": ["Aus", "sei\u00b7ner", "Op\u00b7fer\u00b7scha\u00b7le", "sch\u00fct\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird den Reichthum voller Aehren", "tokens": ["Er", "wird", "den", "Reicht\u00b7hum", "vol\u00b7ler", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht aus der feisten Flur Sardiniens begehren,", "tokens": ["Nicht", "aus", "der", "feis\u00b7ten", "Flur", "Sar\u00b7di\u00b7ni\u00b7ens", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch nicht um den Besitz der sch\u00f6nen Heerden flehn,", "tokens": ["Auch", "nicht", "um", "den", "Be\u00b7sitz", "der", "sch\u00f6\u00b7nen", "Heer\u00b7den", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die in Calabriens erhitzten Triften gehn.", "tokens": ["Die", "in", "Ca\u00b7lab\u00b7ri\u00b7ens", "er\u00b7hitz\u00b7ten", "Trif\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein indisch Elfenbein noch Gold", "tokens": ["Kein", "in\u00b7disch", "El\u00b7fen\u00b7bein", "noch", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind das, warum er Bitten waget,", "tokens": ["Sind", "das", ",", "wa\u00b7rum", "er", "Bit\u00b7ten", "wa\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch Felder nicht, um die der stumme Liris rollt,", "tokens": ["Auch", "Fel\u00b7der", "nicht", ",", "um", "die", "der", "stum\u00b7me", "Li\u00b7ris", "rollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKNEG", "$,", "KOUI", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sie mit stillem Wasser naget.", "tokens": ["Der", "sie", "mit", "stil\u00b7lem", "Was\u00b7ser", "na\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der, dem ein g\u00fcnstig Gl\u00fcck bei Cales Wein gegeben,", "tokens": ["Der", ",", "dem", "ein", "g\u00fcns\u00b7tig", "Gl\u00fcck", "bei", "Ca\u00b7les", "Wein", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "ADJD", "NN", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beschneid' und keltre sich die ihm geg\u00f6nnten Reben!", "tokens": ["Be\u00b7schneid'", "und", "kelt\u00b7re", "sich", "die", "ihm", "ge\u00b7g\u00f6nn\u00b7ten", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die g\u00fcldnen Kelche leer' ein reicher Handelsmann", "tokens": ["Die", "g\u00fcld\u00b7nen", "Kel\u00b7che", "leer'", "ein", "rei\u00b7cher", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von Weinen, die sein Tausch in Syrien gewann!", "tokens": ["Von", "Wei\u00b7nen", ",", "die", "sein", "Tausch", "in", "Sy\u00b7ri\u00b7en", "ge\u00b7wann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der G\u00f6tter Liebling sei nur Er!", "tokens": ["Der", "G\u00f6t\u00b7ter", "Lieb\u00b7ling", "sei", "nur", "Er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df drei- ja viermal alle Jahre", "tokens": ["Da\u00df", "drei", "ja", "vier\u00b7mal", "al\u00b7le", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "TRUNC", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er straffrei und verschont des Atlas breites Meer", "tokens": ["Er", "straf\u00b7frei", "und", "ver\u00b7schont", "des", "At\u00b7las", "brei\u00b7tes", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit sichern Frachten \u00fcberfahre!", "tokens": ["Mit", "si\u00b7chern", "Frach\u00b7ten", "\u00fc\u00b7berf\u00b7ah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Mir sind Cichorien, mir sind des Oelbaums Fr\u00fcchte", "tokens": ["Mir", "sind", "Cic\u00b7ho\u00b7ri\u00b7en", ",", "mir", "sind", "des", "O\u00b7el\u00b7baums", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und leichte Malven stets vergn\u00fcgende Gerichte.", "tokens": ["Und", "leich\u00b7te", "Mal\u00b7ven", "stets", "ver\u00b7gn\u00fc\u00b7gen\u00b7de", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gib mir, Latonens Sohn, bis zu des Lebens Schlu\u00df,", "tokens": ["Gib", "mir", ",", "La\u00b7to\u00b7nens", "Sohn", ",", "bis", "zu", "des", "Le\u00b7bens", "Schlu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NE", "NN", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum Gegenw\u00e4rtigen Gesundheit und Genu\u00df.", "tokens": ["Zum", "Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", "Ge\u00b7sund\u00b7heit", "und", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur etwas w\u00fcnsch' ich mir dabei,", "tokens": ["Nur", "et\u00b7was", "w\u00fcn\u00b7sch'", "ich", "mir", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PRF", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verweil' ich l\u00e4nger auf der Erde:", "tokens": ["Ver\u00b7weil'", "ich", "l\u00e4n\u00b7ger", "auf", "der", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auch mein Alter noch ein Stand der Ehre sei", "tokens": ["Da\u00df", "auch", "mein", "Al\u00b7ter", "noch", "ein", "Stand", "der", "Eh\u00b7re", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mir zu keinem Vorwurf werde.", "tokens": ["Und", "mir", "zu", "kei\u00b7nem", "Vor\u00b7wurf", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Alsdann vermindre mir kein Kummer, kein Gesch\u00e4fte,", "tokens": ["Als\u00b7dann", "ver\u00b7mind\u00b7re", "mir", "kein", "Kum\u00b7mer", ",", "kein", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und keiner Krankheit Gift die mindern Seelenkr\u00e4fte,", "tokens": ["Und", "kei\u00b7ner", "Krank\u00b7heit", "Gift", "die", "min\u00b7dern", "See\u00b7len\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und, wie der Dichter Kunst mir immer wohlgefiel,", "tokens": ["Und", ",", "wie", "der", "Dich\u00b7ter", "Kunst", "mir", "im\u00b7mer", "wohl\u00b7ge\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So sei der Saiten Scherz auch meines Alters Spiel.", "tokens": ["So", "sei", "der", "Sai\u00b7ten", "Scherz", "auch", "mei\u00b7nes", "Al\u00b7ters", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}