{"dta.poem.12872": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre unempfindligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die augen meiner Caroline", "tokens": ["Die", "au\u00b7gen", "mei\u00b7ner", "Ca\u00b7ro\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind von saphyr: die lippen zwey rubine;", "tokens": ["Sind", "von", "sa\u00b7phyr", ":", "die", "lip\u00b7pen", "zwey", "ru\u00b7bi\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "$.", "ART", "NN", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was mach ich aber aus der hand?", "tokens": ["Was", "mach", "ich", "a\u00b7ber", "aus", "der", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als ein werck von glattem elffenbeine.", "tokens": ["Nichts", "als", "ein", "werck", "von", "glat\u00b7tem", "elf\u00b7fen\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist nicht die brust aus wei\u00dfem marmelsteine?", "tokens": ["Ist", "nicht", "die", "brust", "aus", "wei\u00b7\u00dfem", "mar\u00b7mel\u00b7stei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihres hertzens zeug ein bloser diamant?", "tokens": ["Und", "ih\u00b7res", "hert\u00b7zens", "zeug", "ein", "blo\u00b7ser", "di\u00b7a\u00b7mant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was wunder\u2019 ich mich denn, da\u00df ieder pfeil zerspringet,", "tokens": ["Was", "wun\u00b7der'", "ich", "mich", "denn", ",", "da\u00df", "ie\u00b7der", "pfeil", "zer\u00b7sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Amor bey ihr angebracht?", "tokens": ["Den", "A\u00b7mor", "bey", "ihr", "an\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und da\u00df mein treues ach nicht in ihr hertze dringet,", "tokens": ["Und", "da\u00df", "mein", "treu\u00b7es", "ach", "nicht", "in", "ihr", "hert\u00b7ze", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nachdem sie die natur aus bein und stein gemacht.", "tokens": ["Nach\u00b7dem", "sie", "die", "na\u00b7tur", "aus", "bein", "und", "stein", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "VVIMP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}