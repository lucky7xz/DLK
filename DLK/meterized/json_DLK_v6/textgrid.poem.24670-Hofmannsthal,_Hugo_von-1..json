{"textgrid.poem.24670": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der \u00e4rmsten kleinen Geige liegt die Harmonie des Alls verborgen,", "tokens": ["In", "der", "\u00e4rms\u00b7ten", "klei\u00b7nen", "Gei\u00b7ge", "liegt", "die", "Har\u00b7mo\u00b7nie", "des", "Alls", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Liegt ekstatisch tiefstes St\u00f6hnen, Jauchzen s\u00fc\u00dfen Schalls verborgen;", "tokens": ["Liegt", "eks\u00b7ta\u00b7tisch", "tiefs\u00b7tes", "St\u00f6h\u00b7nen", ",", "Jauch\u00b7zen", "s\u00fc\u00b7\u00dfen", "Schalls", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$,", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "In dem Stein am Wege liegt der Funke, der die Welt entz\u00fcndet,", "tokens": ["In", "dem", "Stein", "am", "We\u00b7ge", "liegt", "der", "Fun\u00b7ke", ",", "der", "die", "Welt", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Liegt die Wucht des f\u00fcrchterlichen, blitzesgleichen Pralls verborgen.", "tokens": ["Liegt", "die", "Wucht", "des", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", ",", "blit\u00b7zes\u00b7glei\u00b7chen", "Pralls", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "In dem Wort, dem abgegriffnen, liegt was mancher sinnend suchet:", "tokens": ["In", "dem", "Wort", ",", "dem", "ab\u00b7ge\u00b7griff\u00b7nen", ",", "liegt", "was", "man\u00b7cher", "sin\u00b7nend", "su\u00b7chet", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Eine Wahrheit, mit der Klarheit leuchtenden Kristalls verborgen ...", "tokens": ["Ei\u00b7ne", "Wahr\u00b7heit", ",", "mit", "der", "Klar\u00b7heit", "leuch\u00b7ten\u00b7den", "Kris\u00b7talls", "ver\u00b7bor\u00b7gen", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Lockt die T\u00f6ne, sucht die Wahrheit, werft den Stein mit Riesenkr\u00e4ften!", "tokens": ["Lockt", "die", "T\u00f6\u00b7ne", ",", "sucht", "die", "Wahr\u00b7heit", ",", "werft", "den", "Stein", "mit", "Rie\u00b7sen\u00b7kr\u00e4f\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Unsern Blicken ist Vollkommnes seit dem Tag des S\u00fcndenfalls verborgen.", "tokens": ["Un\u00b7sern", "Bli\u00b7cken", "ist", "Voll\u00b7komm\u00b7nes", "seit", "dem", "Tag", "des", "S\u00fcn\u00b7den\u00b7falls", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}