{"dta.poem.11834": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du die Pflanze wirst mit k\u00fchler Flut besprengen;", "tokens": ["Wenn", "du", "die", "Pflan\u00b7ze", "wirst", "mit", "k\u00fch\u00b7ler", "Flut", "be\u00b7spren\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tropfen dunsten weg, die an den Bl\u00e4ttern h\u00e4ngen.", "tokens": ["Die", "Trop\u00b7fen", "duns\u00b7ten", "weg", ",", "die", "an", "den", "Bl\u00e4t\u00b7tern", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur was zu Fu\u00dfe flie\u00dft und bis zur Wurzel nieder,", "tokens": ["Nur", "was", "zu", "Fu\u00b7\u00dfe", "flie\u00dft", "und", "bis", "zur", "Wur\u00b7zel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "NN", "VVFIN", "KON", "APPR", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchdringt als Lebenssaft von dort der Pflanze Glieder.", "tokens": ["Durch\u00b7dringt", "als", "Le\u00b7bens\u00b7saft", "von", "dort", "der", "Pflan\u00b7ze", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "APPR", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So was von au\u00dfen sich mit Lust an dich mag dr\u00e4ngen,", "tokens": ["So", "was", "von", "au\u00b7\u00dfen", "sich", "mit", "Lust", "an", "dich", "mag", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "ADV", "PRF", "APPR", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Reize schwinden weg, die an den Sinnen h\u00e4ngen.", "tokens": ["Die", "Rei\u00b7ze", "schwin\u00b7den", "weg", ",", "die", "an", "den", "Sin\u00b7nen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur was zur Wurzel dringt und bis zum Herzen nieder,", "tokens": ["Nur", "was", "zur", "Wur\u00b7zel", "dringt", "und", "bis", "zum", "Her\u00b7zen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPRART", "NN", "VVFIN", "KON", "APPR", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erfrischt als Nahrungssaft von dort des Lebens Glieder.", "tokens": ["Er\u00b7frischt", "als", "Nah\u00b7rungs\u00b7saft", "von", "dort", "des", "Le\u00b7bens", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "APPR", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}