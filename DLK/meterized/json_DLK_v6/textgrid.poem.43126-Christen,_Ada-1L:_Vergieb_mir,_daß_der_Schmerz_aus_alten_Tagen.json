{"textgrid.poem.43126": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergieb mir, da\u00df der Schmerz aus alten Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergieb mir, da\u00df der Schmerz aus alten Tagen", "tokens": ["Ver\u00b7gieb", "mir", ",", "da\u00df", "der", "Schmerz", "aus", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das kranke Herz mir konnte wild verbittern", "tokens": ["Das", "kran\u00b7ke", "Herz", "mir", "konn\u00b7te", "wild", "ver\u00b7bit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seine r\u00fchrend kindlich-bangen Klagen", "tokens": ["Und", "sei\u00b7ne", "r\u00fch\u00b7rend", "kind\u00b7lich\u00b7ban\u00b7gen", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In heiser-schrilles Lachen mir zersplittern.", "tokens": ["In", "hei\u00b7ser\u00b7schril\u00b7les", "La\u00b7chen", "mir", "zer\u00b7split\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich liebte Dich und w\u00e4hnte Dich zu hassen,", "tokens": ["Ich", "lieb\u00b7te", "Dich", "und", "w\u00e4hn\u00b7te", "Dich", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als all' die Andern Dir zu F\u00fc\u00dfen lagen;", "tokens": ["Als", "all'", "die", "An\u00b7dern", "Dir", "zu", "F\u00fc\u00b7\u00dfen", "la\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun da Du alt geworden und verlassen,", "tokens": ["Nun", "da", "Du", "alt", "ge\u00b7wor\u00b7den", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "VAPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erfasset mich ein unerkl\u00e4rbar Zagen.", "tokens": ["Er\u00b7fas\u00b7set", "mich", "ein", "un\u00b7er\u00b7kl\u00e4r\u00b7bar", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O w\u00fcrdest Du wie einst, voll trotz'gem Wagen,", "tokens": ["O", "w\u00fcr\u00b7dest", "Du", "wie", "einst", ",", "voll", "trotz'\u00b7gem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KOKOM", "ADV", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Jugend-Uebermuth mein Herz zerfleischen,", "tokens": ["Voll", "Ju\u00b7gend\u00b7Ue\u00b7ber\u00b7muth", "mein", "Herz", "zer\u00b7flei\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel leichter als die Blicke w\u00fcrd' ich's tragen,", "tokens": ["Viel", "leich\u00b7ter", "als", "die", "Bli\u00b7cke", "w\u00fcrd'", "ich's", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unbewu\u00dft nur tiefes Mitleid heischen.", "tokens": ["Die", "un\u00b7be\u00b7wu\u00dft", "nur", "tie\u00b7fes", "Mit\u00b7leid", "hei\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}