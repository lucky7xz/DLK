{"textgrid.poem.55732": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Antlitz ist bleich und rein", "tokens": ["Dein", "Ant\u00b7litz", "ist", "bleich", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie Mondlicht.", "tokens": ["Wie", "Mond\u00b7licht", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nur Deine Lippen lodern", "tokens": ["Nur", "Dei\u00b7ne", "Lip\u00b7pen", "lo\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In brennendem Rosenrot.", "tokens": ["In", "bren\u00b7nen\u00b7dem", "Ro\u00b7sen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Weich und tief,", "tokens": ["Weich", "und", "tief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vers\u00f6hnung singende Orgellaute,", "tokens": ["Ver\u00b7s\u00f6h\u00b7nung", "sin\u00b7gen\u00b7de", "Or\u00b7gel\u00b7lau\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Quellen Worte des Herzens", "tokens": ["Quel\u00b7len", "Wor\u00b7te", "des", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Von Deinen s\u00fcndhaft sch\u00f6nen Lippen,", "tokens": ["Von", "Dei\u00b7nen", "s\u00fcnd\u00b7haft", "sch\u00f6\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und in meine zuckende Seele", "tokens": ["Und", "in", "mei\u00b7ne", "zu\u00b7cken\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Tr\u00e4ufelst Du", "tokens": ["Tr\u00e4u\u00b7felst", "Du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Lindernden Balsam.", "tokens": ["Lin\u00b7dern\u00b7den", "Bal\u00b7sam", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}