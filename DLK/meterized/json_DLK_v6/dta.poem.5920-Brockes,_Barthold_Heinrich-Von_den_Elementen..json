{"dta.poem.5920": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das dritte Element besteht", "tokens": ["Das", "drit\u00b7te", "E\u00b7le\u00b7ment", "be\u00b7steht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der Materie gebrochnen gr\u00f6bern St\u00fccken.", "tokens": ["Aus", "der", "Ma\u00b7te\u00b7rie", "ge\u00b7broch\u00b7nen", "gr\u00f6\u00b7bern", "St\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "In diesen nun erblicken", "tokens": ["In", "die\u00b7sen", "nun", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und finden wir die Theilung gantz verschieden.", "tokens": ["Und", "fin\u00b7den", "wir", "die", "Thei\u00b7lung", "gantz", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Derselbigen Figur ist gantz nicht gleich gedreht;", "tokens": ["Der\u00b7sel\u00b7bi\u00b7gen", "Fi\u00b7gur", "ist", "gantz", "nicht", "gleich", "ge\u00b7dreht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie sind verschiedentlich vermengt,", "tokens": ["Sie", "sind", "ver\u00b7schie\u00b7dent\u00b7lich", "ver\u00b7mengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verwirrt, vereint und in sich selbst gedr\u00e4ngt:", "tokens": ["Ver\u00b7wirrt", ",", "ver\u00b7eint", "und", "in", "sich", "selbst", "ge\u00b7dr\u00e4ngt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und da sie nicht recht fest verbunden seyn;", "tokens": ["Und", "da", "sie", "nicht", "recht", "fest", "ver\u00b7bun\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So nehmen ihre leere Stellen", "tokens": ["So", "neh\u00b7men", "ih\u00b7re", "lee\u00b7re", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die andern Elementen ein.", "tokens": ["Die", "an\u00b7dern", "E\u00b7le\u00b7men\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}