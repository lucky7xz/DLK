{"textgrid.poem.66711": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer sucht das Gl\u00fcck in leeren Wahnesr\u00e4umen?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sucht das Gl\u00fcck in leeren Wahnesr\u00e4umen?", "tokens": ["Wer", "sucht", "das", "Gl\u00fcck", "in", "lee\u00b7ren", "Wah\u00b7nes\u00b7r\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer \u00fcberhebt sich ob der Erde Rund?", "tokens": ["Wer", "\u00fc\u00b7ber\u00b7hebt", "sich", "ob", "der", "Er\u00b7de", "Rund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer m\u00f6chte diese Lebensstatt vers\u00e4umen,", "tokens": ["Wer", "m\u00f6ch\u00b7te", "die\u00b7se", "Le\u00b7bens\u00b7statt", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer l\u00f6sen sich aus urbestimmtem Bund?", "tokens": ["Wer", "l\u00f6\u00b7sen", "sich", "aus", "ur\u00b7be\u00b7stimm\u00b7tem", "Bund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mu\u00dft' ich oft in wildem Schmerz mich b\u00e4umen,", "tokens": ["Und", "mu\u00dft'", "ich", "oft", "in", "wil\u00b7dem", "Schmerz", "mich", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ward des Lebens tiefes Leid mir kund \u2013", "tokens": ["Und", "ward", "des", "Le\u00b7bens", "tie\u00b7fes", "Leid", "mir", "kund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Wehes Schl\u00fcnden, Wonne\u00fcbersch\u00e4umen", "tokens": ["In", "We\u00b7hes", "Schl\u00fcn\u00b7den", ",", "Won\u00b7ne\u00b7\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verleugn' ich nicht den m\u00fctterlichen Grund.", "tokens": ["Ver\u00b7leugn'", "ich", "nicht", "den", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Sang vom Jammertal soll mir verderben", "tokens": ["Kein", "Sang", "vom", "Jam\u00b7mer\u00b7tal", "soll", "mir", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den wurzelfest vertrauten Aufenthalt \u2013", "tokens": ["Den", "wur\u00b7zel\u00b7fest", "ver\u00b7trau\u00b7ten", "Auf\u00b7ent\u00b7halt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und br\u00e4ch' auch all mein Lebensgl\u00fcck in Scherben,", "tokens": ["Und", "br\u00e4ch'", "auch", "all", "mein", "Le\u00b7bens\u00b7gl\u00fcck", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich zu sterben w\u00fcnschte faustgeballt \u2013", "tokens": ["Da\u00df", "ich", "zu", "ster\u00b7ben", "w\u00fcnschte", "faust\u00b7ge\u00b7ballt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir, Erde, soll mein letzter Laut nicht fluchen,", "tokens": ["Dir", ",", "Er\u00b7de", ",", "soll", "mein", "letz\u00b7ter", "Laut", "nicht", "flu\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPOSAT", "ADJA", "APPR", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht im \u00bbJenseits\u00ab will ich Rettung suchen.", "tokens": ["Und", "nicht", "im", "\u00bb", "Jen\u00b7seits", "\u00ab", "will", "ich", "Ret\u00b7tung", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "$(", "ADV", "$(", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}