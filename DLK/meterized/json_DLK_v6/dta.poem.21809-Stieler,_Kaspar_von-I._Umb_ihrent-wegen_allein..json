{"dta.poem.21809": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Umb ihrent-wegen allein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Man mag mich loben oder schimfen/", "tokens": ["Man", "mag", "mich", "lo\u00b7ben", "o\u00b7der", "schim\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "man seh mich s\u00fc\u00df und sauer an:", "tokens": ["man", "seh", "mich", "s\u00fc\u00df", "und", "sau\u00b7er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Genug/ da\u00df ich der sch\u00f6nsten Nymfen/", "tokens": ["Ge\u00b7nug", "/", "da\u00df", "ich", "der", "sch\u00f6ns\u00b7ten", "Nym\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rosillens Ruhm beschreiben kan.", "tokens": ["Ro\u00b7sil\u00b7lens", "Ruhm", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sprecht/ stolze Neider/ sprecht nur immer", "tokens": ["Sprecht", "/", "stol\u00b7ze", "Nei\u00b7der", "/", "sprecht", "nur", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ich sey veracht und ungelehrt/", "tokens": ["ich", "sey", "ver\u00b7acht", "und", "un\u00b7ge\u00b7lehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wo mich ein eitels Frauenzimmer", "tokens": ["wo", "mich", "ein", "ei\u00b7tels", "Frau\u00b7en\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "nicht machte bey der Welt geh\u00f6rt.", "tokens": ["nicht", "mach\u00b7te", "bey", "der", "Welt", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Recht. Durch Sie wil ich sein erhoben", "tokens": ["Recht", ".", "Durch", "Sie", "wil", "ich", "sein", "er\u00b7ho\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPER", "VMFIN", "PPER", "PPOSAT", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durch ihren glanz der Freundligkeit/", "tokens": ["durch", "ih\u00b7ren", "glanz", "der", "Freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "durch ihrer reiffen Tugend Loben", "tokens": ["durch", "ih\u00b7rer", "reif\u00b7fen", "Tu\u00b7gend", "Lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "durch das/ wormit Sie mich erfreut.", "tokens": ["durch", "das", "/", "wor\u00b7mit", "Sie", "mich", "er\u00b7freut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Es wird mir nicht zum Nachteil dienen/", "tokens": ["Es", "wird", "mir", "nicht", "zum", "Nach\u00b7teil", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df ich ihr Wesen hier benennt.", "tokens": ["da\u00df", "ich", "ihr", "We\u00b7sen", "hier", "be\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr Nachruhm wird mit meinem gr\u00fcnen.", "tokens": ["Ihr", "Nach\u00b7ruhm", "wird", "mit", "mei\u00b7nem", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "weil man der Liebe Hoheit kennt.", "tokens": ["weil", "man", "der", "Lie\u00b7be", "Ho\u00b7heit", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Rosille/ la\u00df dich nicht bek\u00fcmmern", "tokens": ["Ro\u00b7sil\u00b7le", "/", "la\u00df", "dich", "nicht", "be\u00b7k\u00fcm\u00b7mern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df mancher B\u00f6ses von uns h\u00e4lt.", "tokens": ["da\u00df", "man\u00b7cher", "B\u00f6\u00b7ses", "von", "uns", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "des Neides Anschlag geht zu tr\u00fcmmern", "tokens": ["des", "Nei\u00b7des", "An\u00b7schlag", "geht", "zu", "tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ihn f\u00e4ngt der Strikk den er uns stellt.", "tokens": ["ihn", "f\u00e4ngt", "der", "Strikk", "den", "er", "uns", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ganze Reich der Pafirillen", "tokens": ["Das", "gan\u00b7ze", "Reich", "der", "Pa\u00b7fi\u00b7ril\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kupid\u2019 und Venus stehn uns bey/", "tokens": ["Ku\u00b7pid'", "und", "Ve\u00b7nus", "stehn", "uns", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die Musen sind uns selbst zu willen.", "tokens": ["die", "Mu\u00b7sen", "sind", "uns", "selbst", "zu", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Uns sch\u00fczzt der F\u00fcrst der Dichterey.", "tokens": ["Uns", "sch\u00fczzt", "der", "F\u00fcrst", "der", "Dich\u00b7te\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der blaue Basilisken-Drache", "tokens": ["Der", "blau\u00b7e", "Ba\u00b7si\u00b7lis\u00b7ken\u00b7Dra\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhn st\u00fcrzt die eingebildte Rache/", "tokens": ["Jhn", "st\u00fcrzt", "die", "ein\u00b7ge\u00b7bild\u00b7te", "Ra\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wird der Neider auch zerspringen/", "tokens": ["So", "wird", "der", "Nei\u00b7der", "auch", "zer\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wenn er uns l\u00e4nger lieben sicht/", "tokens": ["wenn", "er", "uns", "l\u00e4n\u00b7ger", "lie\u00b7ben", "sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wenn er mich ferner h\u00f6ret singen", "tokens": ["wenn", "er", "mich", "fer\u00b7ner", "h\u00f6\u00b7ret", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und er es kan verwehren nicht.", "tokens": ["und", "er", "es", "kan", "ver\u00b7weh\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VMFIN", "VVINF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}