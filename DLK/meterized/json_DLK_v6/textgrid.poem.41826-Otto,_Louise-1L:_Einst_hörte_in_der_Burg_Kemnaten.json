{"textgrid.poem.41826": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst h\u00f6rte in der Burg Kemnaten", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst h\u00f6rte in der Burg Kemnaten", "tokens": ["Einst", "h\u00f6r\u00b7te", "in", "der", "Burg", "Kem\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00fctgarde, da es drau\u00dfen st\u00fcrmte,", "tokens": ["L\u00fct\u00b7gar\u00b7de", ",", "da", "es", "drau\u00b7\u00dfen", "st\u00fcrm\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter mit dem Vogt beraten,", "tokens": ["Die", "Mut\u00b7ter", "mit", "dem", "Vogt", "be\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie man zwei fremde Reiter schirmte,", "tokens": ["Wie", "man", "zwei", "frem\u00b7de", "Rei\u00b7ter", "schirm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die wohl verirrt, als sank der Tag,", "tokens": ["Die", "wohl", "ver\u00b7irrt", ",", "als", "sank", "der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Einla\u00df begehrt, auf Werawag.", "tokens": ["Ein\u00b7la\u00df", "be\u00b7gehrt", ",", "auf", "We\u00b7ra\u00b7wag", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum nennt der Aeltere der beiden,", "tokens": ["Kaum", "nennt", "der", "A\u00b7el\u00b7te\u00b7re", "der", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "PIAT", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den j\u00fcngeren: Rudolf, den S\u00e4nger,", "tokens": ["Den", "j\u00fcn\u00b7ge\u00b7ren", ":", "Ru\u00b7dolf", ",", "den", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "NE", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da t\u00f6nten bald der Harfen Saiten.", "tokens": ["Da", "t\u00f6n\u00b7ten", "bald", "der", "Har\u00b7fen", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit s\u00fc\u00dfen Kl\u00e4ngen, eng und enger", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Kl\u00e4n\u00b7gen", ",", "eng", "und", "en\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umwob in T\u00f6nen Zauberbann", "tokens": ["Um\u00b7wob", "in", "T\u00f6\u00b7nen", "Zau\u00b7ber\u00b7bann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jungfrau und den fremden Mann \u2013", "tokens": ["Die", "Jung\u00b7frau", "und", "den", "frem\u00b7den", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da er geschieden, ging die M\u00e4re:", "tokens": ["Da", "er", "ge\u00b7schie\u00b7den", ",", "ging", "die", "M\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es sei der Kaiser selbst gewesen,", "tokens": ["Es", "sei", "der", "Kai\u00b7ser", "selbst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der hier verirrt von ungef\u00e4hre", "tokens": ["Der", "hier", "ver\u00b7irrt", "von", "un\u00b7ge\u00b7f\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Solch stillen Aufenthalt erlesen.", "tokens": ["Solch", "stil\u00b7len", "Auf\u00b7ent\u00b7halt", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch bald verk\u00fcndet ward mit Hohn:", "tokens": ["Doch", "bald", "ver\u00b7k\u00fcn\u00b7det", "ward", "mit", "Hohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fremde war ein B\u00fcrgerssohn.", "tokens": ["Der", "Frem\u00b7de", "war", "ein", "B\u00fcr\u00b7gers\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit L\u00e4cheln h\u00f6rte es ", "tokens": ["Mit", "L\u00e4\u00b7cheln", "h\u00f6r\u00b7te", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer also hold die Saiten r\u00fchrte", "tokens": ["Wer", "al\u00b7so", "hold", "die", "Sai\u00b7ten", "r\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "War ihr ein gottgesandter Barde.", "tokens": ["War", "ihr", "ein", "gott\u00b7ge\u00b7sand\u00b7ter", "Bar\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob er ein stolzes Wappen f\u00fchrte,", "tokens": ["Ob", "er", "ein", "stol\u00b7zes", "Wap\u00b7pen", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Krone trug \u2013: ihr galts nicht mehr \u2013", "tokens": ["Die", "Kro\u00b7ne", "trug", "\u2013", ":", "ihr", "galts", "nicht", "mehr", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem S\u00e4nger nur gab sie die Ehr'.", "tokens": ["Dem", "S\u00e4n\u00b7ger", "nur", "gab", "sie", "die", "Ehr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Dem S\u00e4nger nur gab sie die Seele \u2013", "tokens": ["Dem", "S\u00e4n\u00b7ger", "nur", "gab", "sie", "die", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie wies zur\u00fccke jedes Werben:", "tokens": ["Sie", "wies", "zu\u00b7r\u00fc\u00b7cke", "je\u00b7des", "Wer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df sie niemals sich verm\u00e4hle", "tokens": ["Und", "da\u00df", "sie", "nie\u00b7mals", "sich", "ver\u00b7m\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel lieber wollt als Jungfrau sterben", "tokens": ["Viel", "lie\u00b7ber", "wollt", "als", "Jung\u00b7frau", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gab stets zur Antwort jeder Frag',", "tokens": ["Gab", "stets", "zur", "Ant\u00b7wort", "je\u00b7der", "Frag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachtigall von Werawag. \u2013", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "von", "We\u00b7ra\u00b7wag", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da einstens ist der Tag gekommen:", "tokens": ["Da", "eins\u00b7tens", "ist", "der", "Tag", "ge\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Freiburgs Mauern sieht man's wallen,", "tokens": ["Zu", "Frei\u00b7burgs", "Mau\u00b7ern", "sieht", "man's", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein herrlich Paar zieht gl\u00fcckumschwommen", "tokens": ["Ein", "herr\u00b7lich", "Paar", "zieht", "gl\u00fc\u00b7ckum\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In die geweihten hohen Hallen.", "tokens": ["In", "die", "ge\u00b7weih\u00b7ten", "ho\u00b7hen", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist der frohe Hochzeitstag", "tokens": ["Das", "ist", "der", "fro\u00b7he", "Hoch\u00b7zeits\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Nachtigall von Werawag.", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "von", "We\u00b7ra\u00b7wag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der B\u00fcrgerssohn im Dienst der Minne,", "tokens": ["Der", "B\u00fcr\u00b7gers\u00b7sohn", "im", "Dienst", "der", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dienst des Sanges und der Seinen,", "tokens": ["Im", "Dienst", "des", "San\u00b7ges", "und", "der", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den B\u00fcrgern all', die im Gewinne", "tokens": ["Den", "B\u00fcr\u00b7gern", "all'", ",", "die", "im", "Ge\u00b7win\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ersehnter Freiheit ihm sich einen,", "tokens": ["Er\u00b7sehn\u00b7ter", "Frei\u00b7heit", "ihm", "sich", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PRF", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem S\u00e4nger, der die Holde freit,", "tokens": ["Dem", "S\u00e4n\u00b7ger", ",", "der", "die", "Hol\u00b7de", "freit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachtigall, die ihm sich weiht.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "die", "ihm", "sich", "weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie lie\u00df das Raubschlo\u00df ihrer Ahnen", "tokens": ["Sie", "lie\u00df", "das", "Raub\u00b7schlo\u00df", "ih\u00b7rer", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem blut'gen Heldenruhme", "tokens": ["Mit", "sei\u00b7nem", "blut'\u00b7gen", "Hel\u00b7den\u00b7ruh\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wandte sich auf neuen Bahnen,", "tokens": ["Und", "wand\u00b7te", "sich", "auf", "neu\u00b7en", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu des Geliebten B\u00fcrgertume,", "tokens": ["Zu", "des", "Ge\u00b7lieb\u00b7ten", "B\u00fcr\u00b7ger\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nachtigall von Werawag,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "von", "We\u00b7ra\u00b7wag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vereint mit ihm des Sanges pflag.", "tokens": ["Ver\u00b7eint", "mit", "ihm", "des", "San\u00b7ges", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}