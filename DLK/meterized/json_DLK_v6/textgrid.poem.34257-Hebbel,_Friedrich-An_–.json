{"textgrid.poem.34257": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' dein Haupt mit Lorbeer'n reich bekr\u00e4nzt,", "tokens": ["Ich", "seh'", "dein", "Haupt", "mit", "Lor\u00b7beer'n", "reich", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch auch vom Schnee des Alters wei\u00df umgl\u00e4nzt.", "tokens": ["Doch", "auch", "vom", "Schnee", "des", "Al\u00b7ters", "wei\u00df", "um\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O, kauftest du, der Welt, wie dir, zum Gl\u00fcck,", "tokens": ["O", ",", "kauf\u00b7test", "du", ",", "der", "Welt", ",", "wie", "dir", ",", "zum", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt f\u00fcr den Kranz die Locken dir zur\u00fcck!", "tokens": ["Jetzt", "f\u00fcr", "den", "Kranz", "die", "Lo\u00b7cken", "dir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du w\u00fcrdest durch den Ruhm, der dich verkl\u00e4rt,", "tokens": ["Du", "w\u00fcr\u00b7dest", "durch", "den", "Ruhm", ",", "der", "dich", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Lebens, das er kostet, doppelt werth:", "tokens": ["Des", "Le\u00b7bens", ",", "das", "er", "kos\u00b7tet", ",", "dop\u00b7pelt", "werth", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Warum versagt dir die Natur den Preis?", "tokens": ["Wa\u00b7rum", "ver\u00b7sagt", "dir", "die", "Na\u00b7tur", "den", "Preis", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Welch einen J\u00fcngling g\u00e4be solch ein Greis!", "tokens": ["Welch", "ei\u00b7nen", "J\u00fcng\u00b7ling", "g\u00e4\u00b7be", "solch", "ein", "Greis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}