{"dta.poem.21642": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Psalm der Trauer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Klagen wein\u2019 ich des Menschen Loos,", "tokens": ["Mit", "Kla\u00b7gen", "wein'", "ich", "des", "Men\u00b7schen", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn mit Schmerzen geb\u00e4rt ihn der Mutter Leib", "tokens": ["Denn", "mit", "Schmer\u00b7zen", "ge\u00b7b\u00e4rt", "ihn", "der", "Mut\u00b7ter", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und Schmerzen geleiten zum Grab ihn.", "tokens": ["Und", "Schmer\u00b7zen", "ge\u00b7lei\u00b7ten", "zum", "Grab", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "PPER", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und es heulen, dem Armen, von hohler Noth", "tokens": ["Und", "es", "heu\u00b7len", ",", "dem", "Ar\u00b7men", ",", "von", "hoh\u00b7ler", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Blasend die St\u00fcrme des Lebens ihm;", "tokens": ["Bla\u00b7send", "die", "St\u00fcr\u00b7me", "des", "Le\u00b7bens", "ihm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.3": {"text": "Was er schaffend zur That vollbringt,", "tokens": ["Was", "er", "schaf\u00b7fend", "zur", "That", "voll\u00b7bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Schl\u00e4gt mit Leid der Genossen Brust,", "tokens": ["Schl\u00e4gt", "mit", "Leid", "der", "Ge\u00b7nos\u00b7sen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und es fa\u00dft ihn des Todes Faust,", "tokens": ["Und", "es", "fa\u00dft", "ihn", "des", "To\u00b7des", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Rei\u00dft ihn krallend herab zum Grund,", "tokens": ["Rei\u00dft", "ihn", "kral\u00b7lend", "her\u00b7ab", "zum", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Modernd im schweigenden Grabesdunst", "tokens": ["Mo\u00b7dernd", "im", "schwei\u00b7gen\u00b7den", "Gra\u00b7bes\u00b7dunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Schlummert der Staub", "tokens": ["Schlum\u00b7mert", "der", "Staub"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Beim Staube.", "tokens": ["Beim", "Stau\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Und was der Staub", "tokens": ["Und", "was", "der", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jubelnd psalmierte, verzweifelnd schrie,", "tokens": ["Ju\u00b7belnd", "psal\u00b7mier\u00b7te", ",", "ver\u00b7zwei\u00b7felnd", "schrie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVPP", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Herrlicher Geister schaffender Traum", "tokens": ["Herr\u00b7li\u00b7cher", "Geis\u00b7ter", "schaf\u00b7fen\u00b7der", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist verronnen in L\u00fcfte der Zeit,", "tokens": ["Ist", "ver\u00b7ron\u00b7nen", "in", "L\u00fcf\u00b7te", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wie sich von Blumen verhaucht ein Duft,", "tokens": ["Wie", "sich", "von", "Blu\u00b7men", "ver\u00b7haucht", "ein", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "S\u00fc\u00dfer Ton in der Ferne verhallt", "tokens": ["S\u00fc\u00b7\u00dfer", "Ton", "in", "der", "Fer\u00b7ne", "ver\u00b7hallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und du schaust nicht, wohin! Wohin?!", "tokens": ["Und", "du", "schaust", "nicht", ",", "wo\u00b7hin", "!", "Wo\u00b7hin", "?!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was qu\u00e4lt den Staub,", "tokens": ["Was", "qu\u00e4lt", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da\u00df er sich baut zum Tempel des Geistes,", "tokens": ["Da\u00df", "er", "sich", "baut", "zum", "Tem\u00b7pel", "des", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Darinnen G\u00f6ttergef\u00fchle entfacht", "tokens": ["Da\u00b7rin\u00b7nen", "G\u00f6t\u00b7ter\u00b7ge\u00b7f\u00fch\u00b7le", "ent\u00b7facht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Zehren am Erdenstoffe des Leib\u2019s?", "tokens": ["Zeh\u00b7ren", "am", "Er\u00b7den\u00b7stof\u00b7fe", "des", "Leib's", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ach, es seufzet das Leben nach Tod.", "tokens": ["Ach", ",", "es", "seuf\u00b7zet", "das", "Le\u00b7ben", "nach", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und der Tod", "tokens": ["Und", "der", "Tod"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.3": {"text": "W\u00fcrget in Ewigkeit. \u2014", "tokens": ["W\u00fcr\u00b7get", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}