{"textgrid.poem.33853": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das V\u00f6glein", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein vom Zweig", "tokens": ["V\u00f6\u00b7glein", "vom", "Zweig"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gaukelt hernieder;", "tokens": ["Gau\u00b7kelt", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lustig sogleich", "tokens": ["Lus\u00b7tig", "sog\u00b7leich"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schwingt es sich wieder.", "tokens": ["Schwingt", "es", "sich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt dir so nah',", "tokens": ["Jetzt", "dir", "so", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jetzt sich versteckend;", "tokens": ["Jetzt", "sich", "ver\u00b7ste\u00b7ckend", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Abermals da,", "tokens": ["A\u00b7ber\u00b7mals", "da", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Scherzend und neckend.", "tokens": ["Scher\u00b7zend", "und", "ne\u00b7ckend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Tastest du zu,", "tokens": ["Tas\u00b7test", "du", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Bist du betrogen,", "tokens": ["Bist", "du", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Spottend im Nu", "tokens": ["Spot\u00b7tend", "im", "Nu"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ist es entflogen.", "tokens": ["Ist", "es", "ent\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Still! Bis zur Hand", "tokens": ["Still", "!", "Bis", "zur", "Hand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "APPR", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wird's dir noch h\u00fcpfen,", "tokens": ["Wird's", "dir", "noch", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bist du gewandt,", "tokens": ["Bist", "du", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Kann's nicht entschl\u00fcpfen.", "tokens": ["Kann's", "nicht", "ent\u00b7schl\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ist's denn so schwer", "tokens": ["Ist's", "denn", "so", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das zu erwarten?", "tokens": ["Das", "zu", "er\u00b7war\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schau' um dich her:", "tokens": ["Schau'", "um", "dich", "her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bl\u00fchender Garten!", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ei, du verzagst?", "tokens": ["Ei", ",", "du", "ver\u00b7zagst", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "La\u00df es gew\u00e4hren,", "tokens": ["La\u00df", "es", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bis du's erjagst,", "tokens": ["Bis", "du's", "er\u00b7jagst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Kannst du's entbehren.", "tokens": ["Kannst", "du's", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wird's doch auch dann", "tokens": ["Wird's", "doch", "auch", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wenig nur bringen,", "tokens": ["We\u00b7nig", "nur", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Aber es kann", "tokens": ["A\u00b7ber", "es", "kann"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VMFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "S\u00fc\u00dfestes singen.", "tokens": ["S\u00fc\u00b7\u00dfes\u00b7tes", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}