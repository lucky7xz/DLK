{"dta.poem.10292": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Sursum rorda?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Was hemmst du, o Held, den Lauf deines Hengstes.             ", "tokens": ["Was", "hemmst", "du", ",", "o", "Held", ",", "den", "Lauf", "dei\u00b7nes", "Hengs\u00b7tes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An den Sattelgurten rinnt ihm der Schwei\u00df,", "tokens": ["An", "den", "Sat\u00b7tel\u00b7gur\u00b7ten", "rinnt", "ihm", "der", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sein Hals ist na\u00df, die Flanken fliegen.", "tokens": ["Sein", "Hals", "ist", "na\u00df", ",", "die", "Flan\u00b7ken", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs Kreuz ihm st\u00fctzt du die Hand,", "tokens": ["Aufs", "Kreuz", "ihm", "st\u00fctzt", "du", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und schaust zur\u00fcck.", "tokens": ["Und", "schaust", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Feinde folgten dir wie die W\u00f6lfe dem Schlitten,", "tokens": ["Die", "Fein\u00b7de", "folg\u00b7ten", "dir", "wie", "die", "W\u00f6l\u00b7fe", "dem", "Schlit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Schon sind sie nahe.", "tokens": ["Schon", "sind", "sie", "na\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Was schaust du nach vorn,", "tokens": ["Was", "schaust", "du", "nach", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die Feinde kommen wie die Welle der Springflut,", "tokens": ["Die", "Fein\u00b7de", "kom\u00b7men", "wie", "die", "Wel\u00b7le", "der", "Spring\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schon sind sie nah\u2019.", "tokens": ["Schon", "sind", "sie", "nah'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was schaust du nach allen Seiten hin,", "tokens": ["Was", "schaust", "du", "nach", "al\u00b7len", "Sei\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Feinde blies der Wind aus allen Richtungen auf dich,", "tokens": ["Die", "Fein\u00b7de", "blies", "der", "Wind", "aus", "al\u00b7len", "Rich\u00b7tun\u00b7gen", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Schon sind sie nahe mit funkelnden Augen,", "tokens": ["Schon", "sind", "sie", "na\u00b7he", "mit", "fun\u00b7keln\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Siehst auf der Aegis du Gorgos schreckliches Haupt \u2014", "tokens": ["Siehst", "auf", "der", "A\u00b7e\u00b7gis", "du", "Gor\u00b7gos", "schreck\u00b7li\u00b7ches", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "NE", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Und kein Ausweg.", "tokens": ["Und", "kein", "Aus\u00b7weg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "H\u00f6rst du sie heulen, h\u00f6rst du das Donnern der Hufe?", "tokens": ["H\u00f6rst", "du", "sie", "heu\u00b7len", ",", "h\u00f6rst", "du", "das", "Don\u00b7nern", "der", "Hu\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und eh\u2019 einmal der gierige Geier \u00fcber dir", "tokens": ["Und", "eh'", "ein\u00b7mal", "der", "gie\u00b7ri\u00b7ge", "Gei\u00b7er", "\u00fc\u00b7ber", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Den tr\u00e4gen Fl\u00fcgel schl\u00e4gt,", "tokens": ["Den", "tr\u00e4\u00b7gen", "Fl\u00fc\u00b7gel", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Haben dich tausend Pfeile durchbohrt,", "tokens": ["Ha\u00b7ben", "dich", "tau\u00b7send", "Pfei\u00b7le", "durch\u00b7bohrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Haben tausend Speere dein Herz zersto\u00dfen.", "tokens": ["Ha\u00b7ben", "tau\u00b7send", "Spee\u00b7re", "dein", "Herz", "zer\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.3": {"line.1": {"text": "Was h\u00e4ltst du, o Freund, die Hand deines Weibes.", "tokens": ["Was", "h\u00e4ltst", "du", ",", "o", "Freund", ",", "die", "Hand", "dei\u00b7nes", "Wei\u00b7bes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ruht wei\u00df und kalt und tot, und so schwer,", "tokens": ["Sie", "ruht", "wei\u00df", "und", "kalt", "und", "tot", ",", "und", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dein Kind liegt neben ihr im Sterben.", "tokens": ["Dein", "Kind", "liegt", "ne\u00b7ben", "ihr", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du st\u00fctzt das Haupt in die Hand,", "tokens": ["Du", "st\u00fctzt", "das", "Haupt", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Verzweiflungsvoll.", "tokens": ["Ver\u00b7zwei\u00b7flungs\u00b7voll", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wer wagt in deinen Kisten und Kasten zu w\u00fchlen.", "tokens": ["Wer", "wagt", "in", "dei\u00b7nen", "Kis\u00b7ten", "und", "Kas\u00b7ten", "zu", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wehe dir Armen,", "tokens": ["We\u00b7he", "dir", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Gl\u00e4ubiger sind\u2019s,", "tokens": ["Die", "Gl\u00e4u\u00b7bi\u00b7ger", "sin\u00b7d's", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die ohn\u2019 Erbarmen Alles pf\u00e4nden und nehmen,", "tokens": ["Die", "ohn'", "Er\u00b7bar\u00b7men", "Al\u00b7les", "pf\u00e4n\u00b7den", "und", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Nichts bleibt zur\u00fcck,", "tokens": ["Nichts", "bleibt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ach, kleinste Erinnerungen selbst.", "tokens": ["Ach", ",", "kleins\u00b7te", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "selbst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Hat Hochmut, Eitelkeit, hat Schuld und Ungl\u00fcck gest\u00fcrzt", "tokens": ["Hat", "Hoch\u00b7mut", ",", "Ei\u00b7tel\u00b7keit", ",", "hat", "Schuld", "und", "Un\u00b7gl\u00fcck", "ge\u00b7st\u00fcrzt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "VAFIN", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Weltklug, das Eiseswort, kanntest du nimmer,", "tokens": ["Welt\u00b7klug", ",", "das", "Ei\u00b7ses\u00b7wort", ",", "kann\u00b7test", "du", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-++-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Doch, ohne weltklug zu sein, Freund, kommst du", "tokens": ["Doch", ",", "oh\u00b7ne", "welt\u00b7klug", "zu", "sein", ",", "Freund", ",", "kommst", "du"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$,", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und kein Ausweg.", "tokens": ["Und", "kein", "Aus\u00b7weg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "H\u00f6rst du sie zischeln, h\u00f6rst du das Lachen der Menschen,", "tokens": ["H\u00f6rst", "du", "sie", "zi\u00b7scheln", ",", "h\u00f6rst", "du", "das", "La\u00b7chen", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Und eh\u2019 einmal der erzene K\u00fcnder \u00fcber dir", "tokens": ["Und", "eh'", "ein\u00b7mal", "der", "er\u00b7ze\u00b7ne", "K\u00fcn\u00b7der", "\u00fc\u00b7ber", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Vom Turm die Stunde ruft,", "tokens": ["Vom", "Turm", "die", "Stun\u00b7de", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Haben dich tausend Siebe zerspellt,", "tokens": ["Ha\u00b7ben", "dich", "tau\u00b7send", "Sie\u00b7be", "zer\u00b7spellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Haben tausend Zungen dein Herz zersto\u00dfen.", "tokens": ["Ha\u00b7ben", "tau\u00b7send", "Zun\u00b7gen", "dein", "Herz", "zer\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}