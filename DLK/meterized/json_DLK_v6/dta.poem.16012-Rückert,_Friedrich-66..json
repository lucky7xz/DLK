{"dta.poem.16012": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kind, lerne was du kannst, und frage nicht, wozu", "tokens": ["Kind", ",", "ler\u00b7ne", "was", "du", "kannst", ",", "und", "fra\u00b7ge", "nicht", ",", "wo\u00b7zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "KON", "VVFIN", "PTKNEG", "$,", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einst das Gelernte dient, f\u00fcr jetzo lerne du.", "tokens": ["Einst", "das", "Ge\u00b7lern\u00b7te", "dient", ",", "f\u00fcr", "jet\u00b7zo", "ler\u00b7ne", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "APPR", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist der Vorzug den die Jugend hat im Lernen,", "tokens": ["Das", "ist", "der", "Vor\u00b7zug", "den", "die", "Ju\u00b7gend", "hat", "im", "Ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihr das Was steht nah, und das Wozu im Fernen.", "tokens": ["Da\u00df", "ihr", "das", "Was", "steht", "nah", ",", "und", "das", "Wo\u00b7zu", "im", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADJD", "$,", "KON", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dem Alter nachundnach mu\u00df dieser Muth verrauchen,", "tokens": ["Dem", "Al\u00b7ter", "na\u00b7chund\u00b7nach", "mu\u00df", "die\u00b7ser", "Muth", "ver\u00b7rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu lernen ohne Zweck, wozu es sei zu brauchen.", "tokens": ["Zu", "ler\u00b7nen", "oh\u00b7ne", "Zweck", ",", "wo\u00b7zu", "es", "sei", "zu", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$,", "PWAV", "PPER", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}