{"textgrid.poem.45712": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in das Album der Frau Bertha Prey\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat dir Schiller gefallen,", "tokens": ["Hat", "dir", "Schil\u00b7ler", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Teilst du den Beifall mit vielen, mit allen;", "tokens": ["Teilst", "du", "den", "Bei\u00b7fall", "mit", "vie\u00b7len", ",", "mit", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "$,", "APPR", "PIAT", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Doch wenn du Goethe liebst,", "tokens": ["Doch", "wenn", "du", "Goe\u00b7the", "liebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Empf\u00e4ngst du nur, weil du gibst.", "tokens": ["Emp\u00b7f\u00e4ngst", "du", "nur", ",", "weil", "du", "gibst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}