{"textgrid.poem.39851": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was thu' ich, deine Thorheit auszureuten!", "tokens": ["Was", "thu'", "ich", ",", "dei\u00b7ne", "Thor\u00b7heit", "aus\u00b7zu\u00b7reu\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Ernst, ich wei\u00df es, nicht ist er f\u00fcr Alle,", "tokens": ["Der", "Ernst", ",", "ich", "wei\u00df", "es", ",", "nicht", "ist", "er", "f\u00fcr", "Al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PTKNEG", "VAFIN", "PPER", "APPR", "PIS", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch du sprichst redlich, da\u00df er dir mi\u00dffalle;", "tokens": ["Auch", "du", "sprichst", "red\u00b7lich", ",", "da\u00df", "er", "dir", "mi\u00df\u00b7fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So la\u00df dir mit der Schellenkappe l\u00e4uten!", "tokens": ["So", "la\u00df", "dir", "mit", "der", "Schel\u00b7len\u00b7kap\u00b7pe", "l\u00e4u\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will dein Herz mit einem Gleichni\u00df deuten:", "tokens": ["Ich", "will", "dein", "Herz", "mit", "ei\u00b7nem", "Gleich\u00b7ni\u00df", "deu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir kommt es vor, wie eines Gasthofs Halle;", "tokens": ["Mir", "kommt", "es", "vor", ",", "wie", "ei\u00b7nes", "Gast\u00b7hofs", "Hal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Haus ert\u00f6nt von mannichfachem Schalle", "tokens": ["Das", "Haus", "er\u00b7t\u00f6nt", "von", "man\u00b7nich\u00b7fa\u00b7chem", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Herrn und Knechten, Dirnen wallt's und Br\u00e4uten.", "tokens": ["Von", "Herrn", "und", "Knech\u00b7ten", ",", "Dir\u00b7nen", "wallt's", "und", "Br\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ausgeh\u00e4ngt als Schild hast eine Sonne", "tokens": ["Und", "aus\u00b7ge\u00b7h\u00e4ngt", "als", "Schild", "hast", "ei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von blankem Blech, die Lust da drin zu malen,", "tokens": ["Von", "blan\u00b7kem", "Blech", ",", "die", "Lust", "da", "drin", "zu", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herbei zu locken vieler Zecher Schw\u00e4rme;", "tokens": ["Her\u00b7bei", "zu", "lo\u00b7cken", "vie\u00b7ler", "Ze\u00b7cher", "Schw\u00e4r\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann einmal \u00fcber's and're rufst du: Wonne!", "tokens": ["Dann", "ein\u00b7mal", "\u00fc\u00b7ber's", "an\u00b7d'\u00b7re", "rufst", "du", ":", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "PIS", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Legst drunter dich, als ob sie leucht' und w\u00e4rme,", "tokens": ["Legst", "drun\u00b7ter", "dich", ",", "als", "ob", "sie", "leucht'", "und", "w\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPER", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, pflegst ein Blumenbeet mit ihren Stralen.", "tokens": ["Ja", ",", "pflegst", "ein", "Blu\u00b7men\u00b7beet", "mit", "ih\u00b7ren", "Stra\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}