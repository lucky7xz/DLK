{"textgrid.poem.39881": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Gleichen bei G\u00f6ttingen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat die Gleichen sich beschaut?", "tokens": ["Wer", "hat", "die", "Glei\u00b7chen", "sich", "be\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind am gleichen Tag gebaut,", "tokens": ["Sie", "sind", "am", "glei\u00b7chen", "Tag", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf dem Doppelh\u00fcgel", "tokens": ["Und", "auf", "dem", "Dop\u00b7pel\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwingt ", "tokens": ["Schwingt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Jetzt liegen sie in Schutt und Rauch,", "tokens": ["Jetzt", "lie\u00b7gen", "sie", "in", "Schutt", "und", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch kommt heran des Liedes Hauch", "tokens": ["Doch", "kommt", "he\u00b7ran", "des", "Lie\u00b7des", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und webt zur rechten Stelle", "tokens": ["Und", "webt", "zur", "rech\u00b7ten", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Burgen hoch und helle.", "tokens": ["Die", "Bur\u00b7gen", "hoch", "und", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zwei Br\u00fcder bauten rasch daran", "tokens": ["Zwei", "Br\u00fc\u00b7der", "bau\u00b7ten", "rasch", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nach gleichem Sinn und gleichem Plan,", "tokens": ["Nach", "glei\u00b7chem", "Sinn", "und", "glei\u00b7chem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Mauern gr\u00fc\u00dften zusammen", "tokens": ["Die", "Mau\u00b7ern", "gr\u00fc\u00df\u00b7ten", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Des Abendrotes Flammen.", "tokens": ["Des", "A\u00b7ben\u00b7dro\u00b7tes", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Thore w\u00f6lbten sich zugleich,", "tokens": ["Die", "Tho\u00b7re", "w\u00f6lb\u00b7ten", "sich", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Maurer f\u00fchrten gleichen Streich,", "tokens": ["Die", "Mau\u00b7rer", "f\u00fchr\u00b7ten", "glei\u00b7chen", "Streich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bis beider Th\u00fcrme Spitzen", "tokens": ["Bis", "bei\u00b7der", "Th\u00fcr\u00b7me", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und wo die W\u00e4nde br\u00fcderlich", "tokens": ["Und", "wo", "die", "W\u00e4n\u00b7de", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die eine kehrt zur andern sich,", "tokens": ["Die", "ei\u00b7ne", "kehrt", "zur", "an\u00b7dern", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "ADJA", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sie lie\u00dfen zu beiden Seiten", "tokens": ["Sie", "lie\u00b7\u00dfen", "zu", "bei\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Sich den Altan bereiten.", "tokens": ["Sich", "den", "Al\u00b7tan", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Dann mit der Sonne fr\u00fchstem Stral", "tokens": ["Dann", "mit", "der", "Son\u00b7ne", "fr\u00fchs\u00b7tem", "Stral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die guten Br\u00fcder jedesmal", "tokens": ["Die", "gu\u00b7ten", "Br\u00fc\u00b7der", "je\u00b7des\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie gr\u00fc\u00dften sich quer\u00fcber", "tokens": ["Sie", "gr\u00fc\u00df\u00b7ten", "sich", "quer\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und hatten sich desto lieber.", "tokens": ["Und", "hat\u00b7ten", "sich", "des\u00b7to", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Und mit dem letzten Abendlicht", "tokens": ["Und", "mit", "dem", "letz\u00b7ten", "A\u00b7bend\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht lie\u00dfen sie die s\u00fc\u00dfe Pflicht,", "tokens": ["Nicht", "lie\u00b7\u00dfen", "sie", "die", "s\u00fc\u00b7\u00dfe", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sie winkten sich wie Kinder,", "tokens": ["Sie", "wink\u00b7ten", "sich", "wie", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und schliefen um so linder.", "tokens": ["Und", "schlie\u00b7fen", "um", "so", "lin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Auch ihre S\u00f6hne hielten's so;", "tokens": ["Auch", "ih\u00b7re", "S\u00f6h\u00b7ne", "hiel\u00b7ten's", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dar\u00fcber Anger und Wald war froh,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "An\u00b7ger", "und", "Wald", "war", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Th\u00e4t sch\u00f6ner, als in ganz Sachsen,", "tokens": ["Th\u00e4t", "sch\u00f6\u00b7ner", ",", "als", "in", "ganz", "Sach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "KOUS", "APPR", "ADV", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "In solcher Eintracht wachsen.", "tokens": ["In", "sol\u00b7cher", "Ein\u00b7tracht", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und auch der S\u00f6hne S\u00f6hne noch,", "tokens": ["Und", "auch", "der", "S\u00f6h\u00b7ne", "S\u00f6h\u00b7ne", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Sie gr\u00fc\u00dften sich wie Br\u00fcder doch", "tokens": ["Sie", "gr\u00fc\u00df\u00b7ten", "sich", "wie", "Br\u00fc\u00b7der", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mit Ku\u00df und Liebeszeichen,", "tokens": ["Mit", "Ku\u00df", "und", "Lie\u00b7bes\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Dort vom Altan der Gleichen.", "tokens": ["Dort", "vom", "Al\u00b7tan", "der", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.36": {"text": "So ging's in's zehnte, zw\u00f6lfte Glied,", "tokens": ["So", "ging's", "in's", "zehn\u00b7te", ",", "zw\u00f6lf\u00b7te", "Glied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Bis Einer sonder Erben schied;", "tokens": ["Bis", "Ei\u00b7ner", "son\u00b7der", "Er\u00b7ben", "schied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Doch, welcher es war von Beiden,", "tokens": ["Doch", ",", "wel\u00b7cher", "es", "war", "von", "Bei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VAFIN", "APPR", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Die Sage will's nicht entscheiden.", "tokens": ["Die", "Sa\u00b7ge", "will's", "nicht", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Wie dieser f\u00fchlt sein Ende nah'n,", "tokens": ["Wie", "die\u00b7ser", "f\u00fchlt", "sein", "En\u00b7de", "nah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "L\u00e4\u00dft er sich tragen zum Altan,", "tokens": ["L\u00e4\u00dft", "er", "sich", "tra\u00b7gen", "zum", "Al\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Er ruft von dr\u00fcben vor Sterben", "tokens": ["Er", "ruft", "von", "dr\u00fc\u00b7ben", "vor", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Den einen Sohn zum Erben.", "tokens": ["Den", "ei\u00b7nen", "Sohn", "zum", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Von Lieb' und Eintracht predigt er", "tokens": ["Von", "Lieb'", "und", "Ein\u00b7tracht", "pre\u00b7digt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Den beiden Gleichen theure M\u00e4r;", "tokens": ["Den", "bei\u00b7den", "Glei\u00b7chen", "theu\u00b7re", "M\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Sturmwolken trieb der Winter,", "tokens": ["Sturm\u00b7wol\u00b7ken", "trieb", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Ein Sp\u00e4trot stand dahinter.", "tokens": ["Ein", "Sp\u00e4t\u00b7rot", "stand", "da\u00b7hin\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Drauf schlief der alte Gleichen ein,", "tokens": ["Drauf", "schlief", "der", "al\u00b7te", "Glei\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Bald dr\u00fcben auch der Vetter sein,", "tokens": ["Bald", "dr\u00fc\u00b7ben", "auch", "der", "Vet\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und von den Schl\u00f6ssern nieder", "tokens": ["Und", "von", "den", "Schl\u00f6s\u00b7sern", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Da schauten Br\u00fcder wieder.", "tokens": ["Da", "schau\u00b7ten", "Br\u00fc\u00b7der", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Doch war nicht Fried' und Freude seit,", "tokens": ["Doch", "war", "nicht", "Fried'", "und", "Freu\u00b7de", "seit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Die Erbschaft zeugte b\u00f6sen Streit;", "tokens": ["Die", "Erb\u00b7schaft", "zeug\u00b7te", "b\u00f6\u00b7sen", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Da f\u00fchrten ihre Bahnen", "tokens": ["Da", "f\u00fchr\u00b7ten", "ih\u00b7re", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Sie nicht zu den Altanen.", "tokens": ["Sie", "nicht", "zu", "den", "Al\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Der eine zog gen S\u00fcden aus,", "tokens": ["Der", "ei\u00b7ne", "zog", "gen", "S\u00fc\u00b7den", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Vom Norden kam der andr' in's Haus,", "tokens": ["Vom", "Nor\u00b7den", "kam", "der", "andr'", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Sie suchten sich Gen\u00fcgen", "tokens": ["Sie", "such\u00b7ten", "sich", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "In wilden Fehdez\u00fcgen.", "tokens": ["In", "wil\u00b7den", "Feh\u00b7de\u00b7z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Der 