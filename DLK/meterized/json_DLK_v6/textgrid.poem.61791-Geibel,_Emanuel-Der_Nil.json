{"textgrid.poem.61791": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Nil", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Verborgenen quillt das Heilige. Keiner ist jemals", "tokens": ["Aus", "dem", "Ver\u00b7bor\u00b7ge\u00b7nen", "quillt", "das", "Hei\u00b7li\u00b7ge", ".", "Kei\u00b7ner", "ist", "je\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "$.", "PIS", "VAFIN", "ADV"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Seinem Brunnen genaht, noch kennt er die R\u00e4tsel des Ursprungs,", "tokens": ["Sei\u00b7nem", "Brun\u00b7nen", "ge\u00b7naht", ",", "noch", "kennt", "er", "die", "R\u00e4t\u00b7sel", "des", "Ur\u00b7sprungs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Welchen die Sage verh\u00fcllt in goldene Wundergew\u00f6lke;", "tokens": ["Wel\u00b7chen", "die", "Sa\u00b7ge", "ver\u00b7h\u00fcllt", "in", "gol\u00b7de\u00b7ne", "Wun\u00b7der\u00b7ge\u00b7w\u00f6l\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber es str\u00f6mt Jahrtausende durch und erquickt die Geschlechter.", "tokens": ["A\u00b7ber", "es", "str\u00f6mt", "Jahr\u00b7tau\u00b7sen\u00b7de", "durch", "und", "er\u00b7quickt", "die", "Ge\u00b7schlech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "APPR", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Also, m\u00e4chtiger Nil, umwallt vom Dufte der Fabel,", "tokens": ["Al\u00b7so", ",", "m\u00e4ch\u00b7ti\u00b7ger", "Nil", ",", "um\u00b7wallt", "vom", "Duf\u00b7te", "der", "Fa\u00b7bel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Steigst auch du zu den V\u00f6lkern herab und bewahrst das Geheimnis", "tokens": ["Steigst", "auch", "du", "zu", "den", "V\u00f6l\u00b7kern", "her\u00b7ab", "und", "be\u00b7wahrst", "das", "Ge\u00b7heim\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "ADV", "KON", "VVFIN", "ART", "NN"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Deiner Geburt in verschlossener Brust. Wir fragen vergebens,", "tokens": ["Dei\u00b7ner", "Ge\u00b7burt", "in", "ver\u00b7schlos\u00b7se\u00b7ner", "Brust", ".", "Wir", "fra\u00b7gen", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Ob du gigantischen Seen dicht unter der Sonne des Gleichers", "tokens": ["Ob", "du", "gi\u00b7gan\u00b7ti\u00b7schen", "Seen", "dicht", "un\u00b7ter", "der", "Son\u00b7ne", "des", "Glei\u00b7chers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Selbst ein Gigant entstiegst, ob tausend h\u00fcpfende Quellen", "tokens": ["Selbst", "ein", "Gi\u00b7gant", "ent\u00b7stiegst", ",", "ob", "tau\u00b7send", "h\u00fcp\u00b7fen\u00b7de", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dir, von G\u00fcssen geschwellt, vielarmig die Wiege bereitet.", "tokens": ["Dir", ",", "von", "G\u00fcs\u00b7sen", "ge\u00b7schwellt", ",", "viel\u00b7ar\u00b7mig", "die", "Wie\u00b7ge", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.11": {"text": "Schweigsam wandelst du her durch Urwaldnacht, in das Brausen", "tokens": ["Schweig\u00b7sam", "wan\u00b7delst", "du", "her", "durch", "Ur\u00b7wald\u00b7nacht", ",", "in", "das", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Riesiger Wipfel vertieft und das Lied weissagender V\u00f6gel,", "tokens": ["Rie\u00b7si\u00b7ger", "Wip\u00b7fel", "ver\u00b7tieft", "und", "das", "Lied", "weis\u00b7sa\u00b7gen\u00b7der", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Mit breitbl\u00e4ttriger Blumen Geflecht schwerm\u00fctig dich kr\u00e4nzend.", "tokens": ["Mit", "breit\u00b7bl\u00e4tt\u00b7ri\u00b7ger", "Blu\u00b7men", "Ge\u00b7flecht", "schwer\u00b7m\u00fc\u00b7tig", "dich", "kr\u00e4n\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Aber es wirft sich dir jetzt vom Aufgang kommend der wilde", "tokens": ["A\u00b7ber", "es", "wirft", "sich", "dir", "jetzt", "vom", "Auf\u00b7gang", "kom\u00b7mend", "der", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPER", "ADV", "APPRART", "NN", "ADJD", "ART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Zwillingsbruder ans Herz, und froh der Vereinigung fl\u00fcgelst", "tokens": ["Zwil\u00b7lings\u00b7bru\u00b7der", "ans", "Herz", ",", "und", "froh", "der", "Ver\u00b7ei\u00b7ni\u00b7gung", "fl\u00fc\u00b7gelst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KON", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+--+-+---+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Du den gemessenen Schritt und bezwingst nicht l\u00e4nger die Sehnsucht,", "tokens": ["Du", "den", "ge\u00b7mes\u00b7se\u00b7nen", "Schritt", "und", "be\u00b7zwingst", "nicht", "l\u00e4n\u00b7ger", "die", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "KON", "VVFIN", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Die allm\u00e4chtig den J\u00fcngling ergreift, in die Ferne zu schweifen.", "tokens": ["Die", "all\u00b7m\u00e4ch\u00b7tig", "den", "J\u00fcng\u00b7ling", "er\u00b7greift", ",", "in", "die", "Fer\u00b7ne", "zu", "schwei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Ob ins untere Tal des Gebirgs Felsriegel die Pforte", "tokens": ["Ob", "ins", "un\u00b7te\u00b7re", "Tal", "des", "Ge\u00b7birgs", "Fel\u00b7srie\u00b7gel", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ART", "NN", "NN", "ART", "NN"], "meter": "--+--+--++-+-+-", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Dir zu sperren versucht, du zersprengst ihn jauchzend und ruhst nicht,", "tokens": ["Dir", "zu", "sper\u00b7ren", "ver\u00b7sucht", ",", "du", "zer\u00b7sprengst", "ihn", "jauch\u00b7zend", "und", "ruhst", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVPP", "$,", "PPER", "VVFIN", "PPER", "VVPP", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Bis du den Arm um Mero\u00eb schlingst, wie ein f\u00fcrstlicher Sieger", "tokens": ["Bis", "du", "den", "Arm", "um", "Me\u00b7ro\u00eb", "schlingst", ",", "wie", "ein", "f\u00fcrst\u00b7li\u00b7cher", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Um die gewonnene Braut, die hold ihm l\u00e4chelt, zu weilen.", "tokens": ["Um", "die", "ge\u00b7won\u00b7ne\u00b7ne", "Braut", ",", "die", "hold", "ihm", "l\u00e4\u00b7chelt", ",", "zu", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Doch sie l\u00e4chelt umsonst; du entrei\u00dfest dich ihr, und beharrlich", "tokens": ["Doch", "sie", "l\u00e4\u00b7chelt", "um\u00b7sonst", ";", "du", "ent\u00b7rei\u00b7\u00dfest", "dich", "ihr", ",", "und", "be\u00b7harr\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "PPER", "$,", "KON", "ADJD"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.23": {"text": "\u00dcber der Klippen Gestuf durch unendlicher Strudel und F\u00e4lle", "tokens": ["\u00dc\u00b7ber", "der", "Klip\u00b7pen", "Ge\u00b7stuf", "durch", "un\u00b7end\u00b7li\u00b7cher", "Stru\u00b7del", "und", "F\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "M\u00fchsal schreitest du fort, der erhabneren Pflichten gedenkend.", "tokens": ["M\u00fch\u00b7sal", "schrei\u00b7test", "du", "fort", ",", "der", "er\u00b7hab\u00b7ne\u00b7ren", "Pflich\u00b7ten", "ge\u00b7den\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Denn schon wartet das Tiefland dein, und verschwenderisch sollst du", "tokens": ["Denn", "schon", "war\u00b7tet", "das", "Tief\u00b7land", "dein", ",", "und", "ver\u00b7schwen\u00b7de\u00b7risch", "sollst", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "$,", "KON", "ADJD", "VMFIN", "PPER"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "\u00dcber das weite Gebiet bis hinunter ans Meer, wie ein K\u00f6nig,", "tokens": ["\u00dc\u00b7ber", "das", "wei\u00b7te", "Ge\u00b7biet", "bis", "hin\u00b7un\u00b7ter", "ans", "Meer", ",", "wie", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Deine Gaben verstreun und das Horn aussch\u00fctten des Segens. \u2013", "tokens": ["Dei\u00b7ne", "Ga\u00b7ben", "ver\u00b7streun", "und", "das", "Horn", "aus\u00b7sch\u00fct\u00b7ten", "des", "Se\u00b7gens", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}}}}