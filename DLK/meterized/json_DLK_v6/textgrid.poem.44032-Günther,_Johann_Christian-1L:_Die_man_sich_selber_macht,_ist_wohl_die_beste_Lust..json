{"textgrid.poem.44032": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die man sich selber macht, ist wohl die beste Lust.", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die man sich selber macht, ist wohl die beste Lust.", "tokens": ["Die", "man", "sich", "sel\u00b7ber", "macht", ",", "ist", "wohl", "die", "bes\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies f\u00fchlet meine Brust", "tokens": ["Dies", "f\u00fch\u00b7let", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey innerlicher Angst und eu\u00dferlichen Plagen.", "tokens": ["Bey", "in\u00b7ner\u00b7li\u00b7cher", "Angst", "und", "eu\u00b7\u00dfer\u00b7li\u00b7chen", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn f\u00e4llt mir Zeit und Flei\u00df und aller Umgang schwer,", "tokens": ["Denn", "f\u00e4llt", "mir", "Zeit", "und", "Flei\u00df", "und", "al\u00b7ler", "Um\u00b7gang", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So komm ich ohngefehr", "tokens": ["So", "komm", "ich", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf etwas, das mich st\u00e4rckt, die Grillen wegzuschlagen.", "tokens": ["Auf", "et\u00b7was", ",", "das", "mich", "st\u00e4rckt", ",", "die", "Gril\u00b7len", "weg\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Betr\u00fcbt mich auf der Welt das Leben obenhin,", "tokens": ["Be\u00b7tr\u00fcbt", "mich", "auf", "der", "Welt", "das", "Le\u00b7ben", "o\u00b7ben\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will der blinde Sinn,", "tokens": ["So", "will", "der", "blin\u00b7de", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der fleischlich denckt und schliest, fast mit dem Sch\u00f6pfer schmollen,", "tokens": ["Der", "fleischlich", "denckt", "und", "schliest", ",", "fast", "mit", "dem", "Sch\u00f6p\u00b7fer", "schmol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Als h\u00e4tt er, da er l\u00e4ngst mein Leiden und mein Flehn", "tokens": ["Als", "h\u00e4tt", "er", ",", "da", "er", "l\u00e4ngst", "mein", "Lei\u00b7den", "und", "mein", "Flehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gut voraus gesehn,", "tokens": ["So", "gut", "vo\u00b7raus", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Wesen und Natur nur m\u00f6glich la\u00dfen sollen.", "tokens": ["Mein", "We\u00b7sen", "und", "Na\u00b7tur", "nur", "m\u00f6g\u00b7lich", "la\u00b7\u00dfen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn h\u00e4tt er dieses nicht zur W\u00fcrcklichkeit gebracht,", "tokens": ["Denn", "h\u00e4tt", "er", "die\u00b7ses", "nicht", "zur", "W\u00fcrck\u00b7lich\u00b7keit", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00e4g ich in der Nacht,", "tokens": ["So", "l\u00e4g", "ich", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da, wo Gefahr und Angst das todte Volck nicht dr\u00e4ngen,", "tokens": ["Da", ",", "wo", "Ge\u00b7fahr", "und", "Angst", "das", "tod\u00b7te", "Volck", "nicht", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fchlt ich jezt auch nicht Verfolgung, Gram und Noth,", "tokens": ["So", "f\u00fchlt", "ich", "jezt", "auch", "nicht", "Ver\u00b7fol\u00b7gung", ",", "Gram", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die mir so Tranck als Brodt,", "tokens": ["Die", "mir", "so", "Tranck", "als", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Darum ich dienen mu\u00df, mit Gift und Galle mengen.", "tokens": ["Da\u00b7rum", "ich", "die\u00b7nen", "mu\u00df", ",", "mit", "Gift", "und", "Gal\u00b7le", "men\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So schlie\u00dfet Fleisch und Blut, nicht aber der Verstand,", "tokens": ["So", "schlie\u00b7\u00dfet", "Fleisch", "und", "Blut", ",", "nicht", "a\u00b7ber", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der l\u00e4st die Allmachtshand,", "tokens": ["Der", "l\u00e4st", "die", "All\u00b7machts\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die nichts umsonst erbaut, ein danckbar Herze sp\u00fcren,", "tokens": ["Die", "nichts", "um\u00b7sonst", "er\u00b7baut", ",", "ein", "dan\u00b7ck\u00b7bar", "Her\u00b7ze", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$,", "ART", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Indem ihr weiser Schlu\u00df mich schlechten Theil der Welt", "tokens": ["In\u00b7dem", "ihr", "wei\u00b7ser", "Schlu\u00df", "mich", "schlech\u00b7ten", "Theil", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So werth und w\u00fcrdig h\u00e4lt,", "tokens": ["So", "werth", "und", "w\u00fcr\u00b7dig", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Ungl\u00fcck und Gedult den gro\u00dfen Bau zu zieren.", "tokens": ["Durch", "Un\u00b7gl\u00fcck", "und", "Ge\u00b7dult", "den", "gro\u00b7\u00dfen", "Bau", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du unvergn\u00fcgtes Herz verstehst dein Gl\u00fccke nicht,", "tokens": ["Du", "un\u00b7ver\u00b7gn\u00fcg\u00b7tes", "Herz", "ver\u00b7stehst", "dein", "Gl\u00fc\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wofern dein scheel Gesicht", "tokens": ["Wo\u00b7fern", "dein", "scheel", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf andre Nechsten f\u00e4llt, die hoch und pr\u00e4chtig leben.", "tokens": ["Auf", "and\u00b7re", "Nechs\u00b7ten", "f\u00e4llt", ",", "die", "hoch", "und", "pr\u00e4ch\u00b7tig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach, lidtest du auch mehr als jezo in der That,", "tokens": ["Ach", ",", "lid\u00b7test", "du", "auch", "mehr", "als", "je\u00b7zo", "in", "der", "That", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "PIAT", "KOKOM", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der heilgen W\u00e4chter Rath", "tokens": ["Der", "heil\u00b7gen", "W\u00e4ch\u00b7ter", "Rath"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4r dennoch gegen dich noch viel zu reich im Geben.", "tokens": ["W\u00e4r", "den\u00b7noch", "ge\u00b7gen", "dich", "noch", "viel", "zu", "reich", "im", "Ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV", "ADV", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Komm, zehle mir einmahl die Lichter in der Luft,", "tokens": ["Komm", ",", "zeh\u00b7le", "mir", "ein\u00b7mahl", "die", "Lich\u00b7ter", "in", "der", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die St\u00e4ubchen in der Gruft,", "tokens": ["Die", "St\u00e4ub\u00b7chen", "in", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fehler, so du hast, und aller Menschen S\u00fcnden;", "tokens": ["Die", "Feh\u00b7ler", ",", "so", "du", "hast", ",", "und", "al\u00b7ler", "Men\u00b7schen", "S\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VAFIN", "$,", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hastu denn davon die Rechnung aufgebracht,", "tokens": ["Und", "has\u00b7tu", "denn", "da\u00b7von", "die", "Rech\u00b7nung", "auf\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So rechne mit Bedacht:", "tokens": ["So", "rech\u00b7ne", "mit", "Be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du wirst an dir von Gott mehr G\u00fct und Wunder finden.", "tokens": ["Du", "wirst", "an", "dir", "von", "Gott", "mehr", "G\u00fct", "und", "Wun\u00b7der", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "NN", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Erwege doch nur recht die gl\u00fcckliche Natur,", "tokens": ["Er\u00b7we\u00b7ge", "doch", "nur", "recht", "die", "gl\u00fcck\u00b7li\u00b7che", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch du auf die Spur", "tokens": ["Wo\u00b7durch", "du", "auf", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der rechten Wei\u00dfheit kommst, bedencke dein Gem\u00fcthe,", "tokens": ["Der", "rech\u00b7ten", "Wei\u00df\u00b7heit", "kommst", ",", "be\u00b7den\u00b7cke", "dein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist im Fallen schwach, jedoch an Liebe starck", "tokens": ["Es", "ist", "im", "Fal\u00b7len", "schwach", ",", "je\u00b7doch", "an", "Lie\u00b7be", "starck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVFIN", "$,", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und theilte Blut und Marck,", "tokens": ["Und", "theil\u00b7te", "Blut", "und", "Marck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn de\u00dfen Treu dadurch dem \u00e4rmsten Menschen riethe.", "tokens": ["Wenn", "de\u00b7\u00dfen", "Treu", "da\u00b7durch", "dem", "\u00e4rms\u00b7ten", "Men\u00b7schen", "rie\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Ach, so ein ehrlich Herz, was ist das vor ein Schaz!", "tokens": ["Ach", ",", "so", "ein", "ehr\u00b7lich", "Herz", ",", "was", "ist", "das", "vor", "ein", "Schaz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ART", "ADJD", "NN", "$,", "PWS", "VAFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da hat kein Ungl\u00fcck Plaz,", "tokens": ["Da", "hat", "kein", "Un\u00b7gl\u00fcck", "Plaz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Hang, Geburth und Zucht den Geist begierig machen,", "tokens": ["Wo", "Hang", ",", "Ge\u00b7burth", "und", "Zucht", "den", "Geist", "be\u00b7gie\u00b7rig", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+