{"dta.poem.21639": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das Butterbrod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Schaum entst\u00fcrzt der M\u00fchle der Bach", "tokens": ["Im", "Schaum", "ent\u00b7st\u00fcrzt", "der", "M\u00fch\u00b7le", "der", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wirft die Fluth den Fluthen nach.", "tokens": ["Und", "wirft", "die", "Fluth", "den", "Flut\u00b7hen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eerbarm\u2019 dich Himmel der Herzensnoth", "tokens": ["\u201e", "er\u00b7barm'", "dich", "Him\u00b7mel", "der", "Her\u00b7zens\u00b7noth"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Kindlein ringt im Fluthentod!\u201c", "tokens": ["Mein", "Kin\u00b7dlein", "ringt", "im", "Flut\u00b7hen\u00b7tod", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es kommt ein Wandersmann gegangen,", "tokens": ["Es", "kommt", "ein", "Wan\u00b7ders\u00b7mann", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Handwerksbursch mit bleichen Wangen.", "tokens": ["Ein", "Hand\u00b7werks\u00b7bursch", "mit", "blei\u00b7chen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sieht\u2019s; Felleisen wirft er ab,", "tokens": ["Er", "sieht's", ";", "Fell\u00b7ei\u00b7sen", "wirft", "er", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Springt wortlos in das Wellengrab.", "tokens": ["Springt", "wort\u00b7los", "in", "das", "Wel\u00b7len\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie er triefend sucht und ringt,", "tokens": ["Und", "wie", "er", "trie\u00b7fend", "sucht", "und", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gerettet das M\u00e4gdlein ans Ufer sinkt.", "tokens": ["Ge\u00b7ret\u00b7tet", "das", "M\u00e4gd\u00b7lein", "ans", "U\u00b7fer", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Da h\u00e4lt im H\u00e4ndchen noch fest und klein", "tokens": ["Da", "h\u00e4lt", "im", "H\u00e4nd\u00b7chen", "noch", "fest", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Butterbr\u00f6dchen das M\u00e4gdelein.", "tokens": ["Sein", "But\u00b7ter\u00b7br\u00f6d\u00b7chen", "das", "M\u00e4g\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201egerettet das Kind! Das Br\u00f6dchen gar!", "tokens": ["\u201e", "ge\u00b7ret\u00b7tet", "das", "Kind", "!", "Das", "Br\u00f6d\u00b7chen", "gar", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "ART", "NN", "$.", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Edler, wir bringen dir Goldlohn dar!\u201c", "tokens": ["Du", "Ed\u00b7ler", ",", "wir", "brin\u00b7gen", "dir", "Gold\u00b7lohn", "dar", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "\u201eich leide schwere Hungersnoth \u2014", "tokens": ["\u201e", "ich", "lei\u00b7de", "schwe\u00b7re", "Hun\u00b7gers\u00b7noth"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenkt mir zum Lohn das kleine Brod!\u201c", "tokens": ["Schenkt", "mir", "zum", "Lohn", "das", "klei\u00b7ne", "Brod", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und spricht\u2019s und wandelt die Stra\u00dfe weiter", "tokens": ["Und", "spricht's", "und", "wan\u00b7delt", "die", "Stra\u00b7\u00dfe", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und i\u00dft sein Br\u00f6dchen still und heiter.", "tokens": ["Und", "i\u00dft", "sein", "Br\u00f6d\u00b7chen", "still", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ihn segne die Sonne, so weit sie scheint \u2014", "tokens": ["Ihn", "seg\u00b7ne", "die", "Son\u00b7ne", ",", "so", "weit", "sie", "scheint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich habe vor Freuden und Schmerz geweint.", "tokens": ["Ich", "ha\u00b7be", "vor", "Freu\u00b7den", "und", "Schmerz", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}