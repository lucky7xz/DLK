{"textgrid.poem.46146": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von vorhergehender materi", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor und Mars je mehr und mehr", "tokens": ["A\u00b7mor", "und", "Mars", "je", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "mir allzeit widerstehen,", "tokens": ["mir", "all\u00b7zeit", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nichts, was ich auf der welt begehr,", "tokens": ["Nichts", ",", "was", "ich", "auf", "der", "welt", "be\u00b7gehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mir lassen sie fortgehen;", "tokens": ["mir", "las\u00b7sen", "sie", "fort\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "darum mein lust soll sein", "tokens": ["da\u00b7rum", "mein", "lust", "soll", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "VMFIN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "schmerz und pein.", "tokens": ["schmerz", "und", "pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ein got allein ist sunst stark gnug", "tokens": ["Ein", "got", "al\u00b7lein", "ist", "sunst", "stark", "gnug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den menschen umzubringen,", "tokens": ["den", "men\u00b7schen", "um\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Leid, liegen, falschheit und betrug", "tokens": ["Leid", ",", "lie\u00b7gen", ",", "falschheit", "und", "be\u00b7trug"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "KON", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "zween brauchen, mich zu zwingen;", "tokens": ["zween", "brau\u00b7chen", ",", "mich", "zu", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "daher ihr list allein", "tokens": ["da\u00b7her", "ihr", "list", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "meine pein.", "tokens": ["mei\u00b7ne", "pein", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Amor mir eine Nymf bracht f\u00fcr,", "tokens": ["A\u00b7mor", "mir", "ei\u00b7ne", "Nymf", "bracht", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVFIN", "APPR", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "die mir mein herz entwendet,", "tokens": ["die", "mir", "mein", "herz", "ent\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mars sich f\u00f6rchtet selbs f\u00fcr mir", "tokens": ["Der", "Mars", "sich", "f\u00f6rch\u00b7tet", "selbs", "f\u00fcr", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "darum er den krieg endet;", "tokens": ["da\u00b7rum", "er", "den", "krieg", "en\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ist nicht ihr list allein", "tokens": ["ist", "nicht", "ihr", "list", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mein pein?", "tokens": ["mein", "pein", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Geduld, begird, trost, hofnung, lust", "tokens": ["Ge\u00b7duld", ",", "be\u00b7gird", ",", "trost", ",", "hof\u00b7nung", ",", "lust"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "fort! ich mu\u00df euch aufgeben;", "tokens": ["fort", "!", "ich", "mu\u00df", "euch", "auf\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Angst und schmerz f\u00fcllet meine brust", "tokens": ["Angst", "und", "schmerz", "f\u00fcl\u00b7let", "mei\u00b7ne", "brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "und endet bald mein leben!", "tokens": ["und", "en\u00b7det", "bald", "mein", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "mein trost und heil mu\u00df sein", "tokens": ["mein", "trost", "und", "heil", "mu\u00df", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "VMFIN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "qual und pein.", "tokens": ["qual", "und", "pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Mein herz mu\u00df nu aus Amors rach", "tokens": ["Mein", "herz", "mu\u00df", "nu", "aus", "A\u00b7mors", "rach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr meiner liebsten brennen;", "tokens": ["f\u00fcr", "mei\u00b7ner", "liebs\u00b7ten", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, doch nichts fraget sie darnach", "tokens": ["Ach", ",", "doch", "nichts", "fra\u00b7get", "sie", "dar\u00b7nach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PIS", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und will es nicht erkennen;", "tokens": ["und", "will", "es", "nicht", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ihr herz so hart als stein", "tokens": ["ihr", "herz", "so", "hart", "als", "stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ist mein pein.", "tokens": ["ist", "mein", "pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Ihr, ihr! o d\u00fcrft ich sagen mein!", "tokens": ["Ihr", ",", "ihr", "!", "o", "d\u00fcrft", "ich", "sa\u00b7gen", "mein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$.", "FM", "VMFIN", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ach, thut die sach ergr\u00fcnden;", "tokens": ["ach", ",", "thut", "die", "sach", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "VVFIN", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Recht richtend sprechet, ist es fein,", "tokens": ["Recht", "rich\u00b7tend", "spre\u00b7chet", ",", "ist", "es", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df ich bei euch soll finden", "tokens": ["da\u00df", "ich", "bei", "euch", "soll", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "f\u00fcr meine treu so rein", "tokens": ["f\u00fcr", "mei\u00b7ne", "treu", "so", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "nichts dan pein?", "tokens": ["nichts", "dan", "pein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ach, ihr solt vil mehr, edles herz,", "tokens": ["Ach", ",", "ihr", "solt", "vil", "mehr", ",", "ed\u00b7les", "herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mich g\u00fcnstig zu gewehren,", "tokens": ["mich", "g\u00fcns\u00b7tig", "zu", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit freindlichkeit, mit schimpf und scherz", "tokens": ["Mit", "freind\u00b7lich\u00b7keit", ",", "mit", "schimpf", "und", "scherz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "verzuckern und verkehren", "tokens": ["ver\u00b7zu\u00b7ckern", "und", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVINF", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "durch s\u00fc\u00dfe schm\u00e4tzelein", "tokens": ["durch", "s\u00fc\u00b7\u00dfe", "schm\u00e4t\u00b7zel\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "all mein pein.", "tokens": ["all", "mein", "pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}