{"textgrid.poem.24988": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Schlucht beim Abendd\u00e4mmern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Schlucht beim Abendd\u00e4mmern", "tokens": ["In", "der", "Schlucht", "beim", "A\u00b7bend\u00b7d\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreit' ich durch den d\u00fcstern Wald.", "tokens": ["Schreit'", "ich", "durch", "den", "d\u00fcs\u00b7tern", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stille ringsum in den Zweigen;", "tokens": ["Stil\u00b7le", "ring\u00b7sum", "in", "den", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur da\u00df leise durch das Schweigen", "tokens": ["Nur", "da\u00df", "lei\u00b7se", "durch", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den fernen Eisenh\u00e4mmern", "tokens": ["Von", "den", "fer\u00b7nen", "Ei\u00b7sen\u00b7h\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An mein Ohr ein Pochen schallt.", "tokens": ["An", "mein", "Ohr", "ein", "Po\u00b7chen", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und auf viel verschlungnen Wegen", "tokens": ["Und", "auf", "viel", "ver\u00b7schlung\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Gedankens irrt mein Geist,", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "irrt", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinnt dem R\u00e4tsel nach, dem alten,", "tokens": ["Sinnt", "dem", "R\u00e4t\u00b7sel", "nach", ",", "dem", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher Macht geheimes Walten", "tokens": ["Wel\u00b7cher", "Macht", "ge\u00b7hei\u00b7mes", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Finstern Zielen uns entgegen", "tokens": ["Fins\u00b7tern", "Zie\u00b7len", "uns", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch Geburt und Sterben rei\u00dft.", "tokens": ["Durch", "Ge\u00b7burt", "und", "Ster\u00b7ben", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O der Mensch mit seinem Wollen,", "tokens": ["O", "der", "Mensch", "mit", "sei\u00b7nem", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er ringt und wie er strebt!", "tokens": ["Wie", "er", "ringt", "und", "wie", "er", "strebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine W\u00fcnsche unermessen;", "tokens": ["Sei\u00b7ne", "W\u00fcn\u00b7sche", "un\u00b7er\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann zu ewigem Vergessen", "tokens": ["Dann", "zu", "e\u00b7wi\u00b7gem", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruht er unter kalten Schollen,", "tokens": ["Ruht", "er", "un\u00b7ter", "kal\u00b7ten", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich als h\u00e4tt' er nie gelebt!", "tokens": ["Gleich", "als", "h\u00e4tt'", "er", "nie", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Seele f\u00fchl' ich schwanken", "tokens": ["Und", "die", "See\u00b7le", "f\u00fchl'", "ich", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter schwerer Zweifel Wucht;", "tokens": ["Un\u00b7ter", "schwe\u00b7rer", "Zwei\u00b7fel", "Wucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder aus der Felsenenge", "tokens": ["Wie\u00b7der", "aus", "der", "Fel\u00b7sen\u00b7en\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winden sich ans Licht die G\u00e4nge;", "tokens": ["Win\u00b7den", "sich", "ans", "Licht", "die", "G\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, o Abgrund der Gedanken,", "tokens": ["Doch", ",", "o", "Ab\u00b7grund", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchrt ein Pfad aus ", "tokens": ["F\u00fchrt", "ein", "Pfad", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}}}}}