{"textgrid.poem.67191": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr W\u00e4lder sch\u00f6n an der Seite,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr W\u00e4lder sch\u00f6n an der Seite,", "tokens": ["Ihr", "W\u00e4l\u00b7der", "sch\u00f6n", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am gr\u00fcnen Abhang gemalt,", "tokens": ["Am", "gr\u00fc\u00b7nen", "Ab\u00b7hang", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo ich umher mich leite,", "tokens": ["Wo", "ich", "um\u00b7her", "mich", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch s\u00fc\u00dfe Ruhe bezahlt", "tokens": ["Durch", "s\u00fc\u00b7\u00dfe", "Ru\u00b7he", "be\u00b7zahlt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "F\u00fcr jeden Stachel im Herzen,", "tokens": ["F\u00fcr", "je\u00b7den", "Sta\u00b7chel", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn dunkel mir ist der Sinn,", "tokens": ["Wenn", "dun\u00b7kel", "mir", "ist", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den Kunst und Sinnen hat Schmerzen", "tokens": ["Den", "Kunst", "und", "Sin\u00b7nen", "hat", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gekostet von Anbeginn.", "tokens": ["Ge\u00b7kos\u00b7tet", "von", "An\u00b7be\u00b7ginn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ihr lieblichen Bilder im Tale,", "tokens": ["Ihr", "lieb\u00b7li\u00b7chen", "Bil\u00b7der", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Zum Beispiel G\u00e4rten und Baum,", "tokens": ["Zum", "Bei\u00b7spiel", "G\u00e4r\u00b7ten", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Und dann der Steg, der schmale,", "tokens": ["Und", "dann", "der", "Steg", ",", "der", "schma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der Bach zu sehen kaum,", "tokens": ["Der", "Bach", "zu", "se\u00b7hen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie sch\u00f6n aus heiterer Ferne", "tokens": ["Wie", "sch\u00f6n", "aus", "hei\u00b7te\u00b7rer", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Gl\u00e4nzt einem das herrliche Bild", "tokens": ["Gl\u00e4nzt", "ei\u00b7nem", "das", "herr\u00b7li\u00b7che", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Der Landschaft, die ich gerne", "tokens": ["Der", "Land\u00b7schaft", ",", "die", "ich", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Besuch in Witterung mild.", "tokens": ["Be\u00b7such", "in", "Wit\u00b7te\u00b7rung", "mild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Die Gottheit freundlich geleitet", "tokens": ["Die", "Got\u00b7theit", "freund\u00b7lich", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Uns erstlich mit Blau,", "tokens": ["Uns", "erst\u00b7lich", "mit", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Hernach mit Wolken bereitet,", "tokens": ["Her\u00b7nach", "mit", "Wol\u00b7ken", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Gebildet w\u00f6lbig und grau,", "tokens": ["Ge\u00b7bil\u00b7det", "w\u00f6l\u00b7big", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Mit sengenden Blitzen und Rollen", "tokens": ["Mit", "sen\u00b7gen\u00b7den", "Blit\u00b7zen", "und", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Des Donners, mit Reiz des Gefilds,", "tokens": ["Des", "Don\u00b7ners", ",", "mit", "Reiz", "des", "Ge\u00b7filds", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.23": {"text": "Mit Sch\u00f6nheit, die gequollen", "tokens": ["Mit", "Sch\u00f6n\u00b7heit", ",", "die", "ge\u00b7quol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Vom Quell urspr\u00fcnglichen Bilds.", "tokens": ["Vom", "Quell", "ur\u00b7spr\u00fcng\u00b7li\u00b7chen", "Bilds", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}