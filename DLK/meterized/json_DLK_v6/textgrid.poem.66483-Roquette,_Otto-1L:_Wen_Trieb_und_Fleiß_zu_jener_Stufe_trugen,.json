{"textgrid.poem.66483": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wen Trieb und Flei\u00df zu jener Stufe trugen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen Trieb und Flei\u00df zu jener Stufe trugen,", "tokens": ["Wen", "Trieb", "und", "Flei\u00df", "zu", "je\u00b7ner", "Stu\u00b7fe", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Stoff und Form sich beugen unwillk\u00fcrlich,", "tokens": ["Wo", "Stoff", "und", "Form", "sich", "beu\u00b7gen", "un\u00b7will\u00b7k\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der baut sein Kunstgebild in festen Fugen,", "tokens": ["Der", "baut", "sein", "Kunst\u00b7ge\u00b7bild", "in", "fes\u00b7ten", "Fu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf deren Grund kein H\u00f6chstes unausf\u00fchrlich.", "tokens": ["Auf", "de\u00b7ren", "Grund", "kein", "H\u00f6chs\u00b7tes", "un\u00b7aus\u00b7f\u00fchr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und die mit Blicken staunend \u00fcberschlugen", "tokens": ["Und", "die", "mit", "Bli\u00b7cken", "stau\u00b7nend", "\u00fc\u00b7bersc\u00b7hlu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Ganzen Ordnung, finden nur geb\u00fchrlich", "tokens": ["Des", "Gan\u00b7zen", "Ord\u00b7nung", ",", "fin\u00b7den", "nur", "ge\u00b7b\u00fchr\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Schmuck und Schimmer, der bis zur Verschwendung", "tokens": ["Den", "Schmuck", "und", "Schim\u00b7mer", ",", "der", "bis", "zur", "Ver\u00b7schwen\u00b7dung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRELS", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Werk umgiebt in festlicher Vollendung.", "tokens": ["Das", "Werk", "um\u00b7giebt", "in", "fest\u00b7li\u00b7cher", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Am schwersten wird des Dichters Form die Massen", "tokens": ["Am", "schwers\u00b7ten", "wird", "des", "Dich\u00b7ters", "Form", "die", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VAFIN", "ART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergreifen, die doch sonst so leicht bezwingbar,", "tokens": ["Er\u00b7grei\u00b7fen", ",", "die", "doch", "sonst", "so", "leicht", "be\u00b7zwing\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn H\u00f6rer auf ein Tongeriesel passen,", "tokens": ["Wenn", "H\u00f6\u00b7rer", "auf", "ein", "Ton\u00b7ge\u00b7rie\u00b7sel", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das nur des Geigers Zauberhand erschwingbar;", "tokens": ["Das", "nur", "des", "Gei\u00b7gers", "Zau\u00b7ber\u00b7hand", "er\u00b7schwing\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn Stimmen trillernd sich vernehmen lassen", "tokens": ["Wenn", "Stim\u00b7men", "tril\u00b7lernd", "sich", "ver\u00b7neh\u00b7men", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PRF", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In einem Kreuzgewirr, das kaum noch singbar,", "tokens": ["In", "ei\u00b7nem", "Kreuz\u00b7ge\u00b7wirr", ",", "das", "kaum", "noch", "sing\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann zuckt's und kribbelt's in den H\u00e4nden Allen,", "tokens": ["Dann", "zuckt's", "und", "krib\u00b7belt's", "in", "den", "H\u00e4n\u00b7den", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem wohlverdienten Beifall beizufallen.", "tokens": ["Dem", "wohl\u00b7ver\u00b7dien\u00b7ten", "Bei\u00b7fall", "bei\u00b7zu\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie nennen's Virtuosit\u00e4t, und schelten,", "tokens": ["Sie", "nen\u00b7nen's", "Vir\u00b7tu\u00b7o\u00b7si\u00b7t\u00e4t", ",", "und", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und loben auch, wenn Einer Virtuos ist.", "tokens": ["Und", "lo\u00b7ben", "auch", ",", "wenn", "Ei\u00b7ner", "Vir\u00b7tu\u00b7os", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Man schelte den, der sonst in allen Welten", "tokens": ["Man", "schel\u00b7te", "den", ",", "der", "sonst", "in", "al\u00b7len", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "$,", "PRELS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts ist, wenn er die Prunkeffekte los ist.", "tokens": ["Nichts", "ist", ",", "wenn", "er", "die", "Prun\u00b7kef\u00b7fek\u00b7te", "los", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "PTKVZ", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Doch lassen gern das Krauseste wir gelten,", "tokens": ["Doch", "las\u00b7sen", "gern", "das", "Krau\u00b7ses\u00b7te", "wir", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn unterm Schmuck der Inhalt sch\u00f6n und gro\u00df ist.", "tokens": ["Wenn", "un\u00b7term", "Schmuck", "der", "In\u00b7halt", "sch\u00f6n", "und", "gro\u00df", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Um freies Spiel hat nie umsonst geworben", "tokens": ["Um", "frei\u00b7es", "Spiel", "hat", "nie", "um\u00b7sonst", "ge\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Kunst, bei der das K\u00f6nnen unverdorben.", "tokens": ["Die", "Kunst", ",", "bei", "der", "das", "K\u00f6n\u00b7nen", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Dichter freilich, der sich in den Kopf setzt", "tokens": ["Der", "Dich\u00b7ter", "frei\u00b7lich", ",", "der", "sich", "in", "den", "Kopf", "setzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Form und Reimspiel sich einmal zu putzen,", "tokens": ["Mit", "Form", "und", "Reim\u00b7spiel", "sich", "ein\u00b7mal", "zu", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat Wenige nur f\u00fcr sich. Jeder Tropf setzt", "tokens": ["Hat", "We\u00b7ni\u00b7ge", "nur", "f\u00fcr", "sich", ".", "Je\u00b7der", "Tropf", "setzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ADV", "APPR", "PRF", "$.", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich gleich parat, den Putz ihm aufzumutzen,", "tokens": ["Sich", "gleich", "pa\u00b7rat", ",", "den", "Putz", "ihm", "auf\u00b7zu\u00b7mut\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKVZ", "$,", "ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sagt, wer an den Rock den simpeln Knopf setzt", "tokens": ["Und", "sagt", ",", "wer", "an", "den", "Rock", "den", "sim\u00b7peln", "Knopf", "setzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Braucht nicht mit Nestelband das Kleid zu stutzen.", "tokens": ["Braucht", "nicht", "mit", "Nes\u00b7tel\u00b7band", "das", "Kleid", "zu", "stut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Kenner nur folgt mit geheimer Sp\u00fcrung,", "tokens": ["Der", "Ken\u00b7ner", "nur", "folgt", "mit", "ge\u00b7hei\u00b7mer", "Sp\u00fc\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vielleicht vergn\u00fcgt, dem Gang der Formenf\u00fchrung.", "tokens": ["Viel\u00b7leicht", "ver\u00b7gn\u00fcgt", ",", "dem", "Gang", "der", "For\u00b7men\u00b7f\u00fch\u00b7rung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn stets mit der gewohnten Reime Ticktack", "tokens": ["Denn", "stets", "mit", "der", "ge\u00b7wohn\u00b7ten", "Rei\u00b7me", "Tick\u00b7tack"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eint\u00f6nig klingt zu oft der Strophen Uhrwerk.", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "klingt", "zu", "oft", "der", "Stro\u00b7phen", "Uhr\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Recht hat der Dichter, wenn in lust'gem Tricktrack", "tokens": ["Recht", "hat", "der", "Dich\u00b7ter", ",", "wenn", "in", "lust'\u00b7gem", "Trick\u00b7track"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er spielend zwingt des spr\u00f6den Reims Naturwerk.", "tokens": ["Er", "spie\u00b7lend", "zwingt", "des", "spr\u00f6\u00b7den", "Reims", "Na\u00b7tur\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer k\u00fchnlich wagt, der f\u00e4hrt auch wohl im Zickzack,", "tokens": ["Wer", "k\u00fchn\u00b7lich", "wagt", ",", "der", "f\u00e4hrt", "auch", "wohl", "im", "Zick\u00b7zack", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und bringt zum Ziele sein poetisch Fuhrwerk.", "tokens": ["Und", "bringt", "zum", "Zie\u00b7le", "sein", "po\u00b7e\u00b7tisch", "Fuhr\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So widm' ich Dir, o Freund, dies B\u00fcndel Schnickschnack,", "tokens": ["So", "widm'", "ich", "Dir", ",", "o", "Freund", ",", "dies", "B\u00fcn\u00b7del", "Schnick\u00b7schnack", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$,", "PDS", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der formend Du so viel gebracht in Schick, Schack!", "tokens": ["Der", "for\u00b7mend", "Du", "so", "viel", "ge\u00b7bracht", "in", "Schick", ",", "Schack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "VVPP", "PPER", "ADV", "ADV", "VVPP", "APPR", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}