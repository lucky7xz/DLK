{"textgrid.poem.39504": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst war die heil'ge Schrift sammt den Legenden", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst war die heil'ge Schrift sammt den Legenden", "tokens": ["Einst", "war", "die", "heil'\u00b7ge", "Schrift", "sammt", "den", "Le\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Thespis-Karrn der rohen neuern B\u00fchnen;", "tokens": ["Der", "Thes\u00b7pis\u00b7Karrn", "der", "ro\u00b7hen", "neu\u00b7ern", "B\u00fch\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Volk und Spielern, gleich an Einfalt, schienen", "tokens": ["Dem", "Volk", "und", "Spie\u00b7lern", ",", "gleich", "an", "Ein\u00b7falt", ",", "schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "ADV", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Possen nicht das Heiligste zu sch\u00e4nden.", "tokens": ["Die", "Pos\u00b7sen", "nicht", "das", "Hei\u00b7ligs\u00b7te", "zu", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch als die Kunst entwuchs den frommen H\u00e4nden,", "tokens": ["Doch", "als", "die", "Kunst", "ent\u00b7wuchs", "den", "from\u00b7men", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da wollt' im Schauspiel niemand Gott mehr dienen,", "tokens": ["Da", "wollt'", "im", "Schau\u00b7spiel", "nie\u00b7mand", "Gott", "mehr", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "VVFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und stolze Geister durften sich erk\u00fchnen", "tokens": ["Und", "stol\u00b7ze", "Geis\u00b7ter", "durf\u00b7ten", "sich", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Spott \u00fcber jene Wunder auszusenden.", "tokens": ["Spott", "\u00fc\u00b7ber", "je\u00b7ne", "Wun\u00b7der", "aus\u00b7zu\u00b7sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, in der Dichterbildung reicher Bl\u00fcthe,", "tokens": ["Du", ",", "in", "der", "Dich\u00b7ter\u00b7bil\u00b7dung", "rei\u00b7cher", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bringst uns verwandelt wieder jene Zeiten,", "tokens": ["Bringst", "uns", "ver\u00b7wan\u00b7delt", "wie\u00b7der", "je\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Adam auf der B\u00fchn' erschien und Eva.", "tokens": ["Wo", "A\u00b7dam", "auf", "der", "B\u00fchn'", "er\u00b7schien", "und", "E\u00b7va", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, Dank sei deinem kindlichen Gem\u00fcthe,", "tokens": ["Ja", ",", "Dank", "sei", "dei\u00b7nem", "kind\u00b7li\u00b7chen", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heiligst die Kunst, versch\u00f6nerst Heiligkeiten,", "tokens": ["Hei\u00b7ligst", "die", "Kunst", ",", "ver\u00b7sch\u00f6\u00b7nerst", "Hei\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und machst zum Lied das Leid der Genoveva.", "tokens": ["Und", "machst", "zum", "Lied", "das", "Leid", "der", "Ge\u00b7no\u00b7ve\u00b7va", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}