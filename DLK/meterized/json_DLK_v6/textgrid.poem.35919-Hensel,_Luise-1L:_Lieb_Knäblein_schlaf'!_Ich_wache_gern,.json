{"textgrid.poem.35919": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lieb Kn\u00e4blein schlaf'! Ich wache gern,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb Kn\u00e4blein schlaf'! Ich wache gern,", "tokens": ["Lieb", "Kn\u00e4\u00b7blein", "schlaf'", "!", "Ich", "wa\u00b7che", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O schlaf', mein armes Kind!", "tokens": ["O", "schlaf'", ",", "mein", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Himmel steht der Abendstern,", "tokens": ["Am", "Him\u00b7mel", "steht", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der sieht recht lieb und lind.", "tokens": ["Der", "sieht", "recht", "lieb", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sehn ja alle Sterne", "tokens": ["Es", "sehn", "ja", "al\u00b7le", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein bleiches Kindlein gerne.", "tokens": ["Mein", "blei\u00b7ches", "Kin\u00b7dlein", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schlaf' ein, mein frommes Kind!", "tokens": ["Schlaf'", "ein", ",", "mein", "from\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, schlaf' in Gottes Namen ein,", "tokens": ["Ja", ",", "schlaf'", "in", "Got\u00b7tes", "Na\u00b7men", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aeuglein schlie\u00dfe zu;", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "schlie\u00b7\u00dfe", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann sehn die lichten Engelein", "tokens": ["Dann", "sehn", "die", "lich\u00b7ten", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab auf Deine Ruh. \u2013", "tokens": ["Her\u00b7ab", "auf", "Dei\u00b7ne", "Ruh", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da drau\u00dfen wehn die B\u00e4ume,", "tokens": ["Da", "drau\u00b7\u00dfen", "wehn", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie rauschen bunte Tr\u00e4ume. \u2013", "tokens": ["Sie", "rau\u00b7schen", "bun\u00b7te", "Tr\u00e4u\u00b7me", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, thu' die Aeuglein zu! \u2013", "tokens": ["Ach", ",", "thu'", "die", "A\u00b7e\u00b7u\u00b7glein", "zu", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Lang ist's schon, da\u00df mein armes Herz", "tokens": ["Lang", "ist's", "schon", ",", "da\u00df", "mein", "ar\u00b7mes", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der s\u00fc\u00dfe Schlummer flieht", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Schlum\u00b7mer", "flieht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df auf meinen stillen Schmerz", "tokens": ["Und", "da\u00df", "auf", "mei\u00b7nen", "stil\u00b7len", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond hernieder sieht.", "tokens": ["Der", "Mond", "her\u00b7nie\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Waislein, bleib doch liegen,", "tokens": ["Mein", "Wais\u00b7lein", ",", "bleib", "doch", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will Dich als Mutter wiegen;", "tokens": ["Will", "Dich", "als", "Mut\u00b7ter", "wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Horch' auf ein neues Lied!", "tokens": ["Horch'", "auf", "ein", "neu\u00b7es", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aus theurem Grabe wuchs ein Reis,", "tokens": ["Aus", "theu\u00b7rem", "Gra\u00b7be", "wuchs", "ein", "Reis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war so zart und fein;", "tokens": ["Das", "war", "so", "zart", "und", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich pflanzt' es in mein Beet mit Flei\u00df", "tokens": ["Ich", "pflanzt'", "es", "in", "mein", "Beet", "mit", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah es sch\u00f6n gedeih'n;", "tokens": ["Und", "sah", "es", "sch\u00f6n", "ge\u00b7deih'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun nagt an seinem Herzen", "tokens": ["Nun", "nagt", "an", "sei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein b\u00f6ser Wurm mit Schmerzen,", "tokens": ["Ein", "b\u00f6\u00b7ser", "Wurm", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun welkt es mir zur Pein.", "tokens": ["Nun", "welkt", "es", "mir", "zur", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wol tr\u00e4umt ich manchen sch\u00f6nen Traum", "tokens": ["Wol", "tr\u00e4umt", "ich", "man\u00b7chen", "sch\u00f6\u00b7nen", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem lieben Reis:", "tokens": ["Von", "mei\u00b7nem", "lie\u00b7ben", "Reis", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hofft', es werd' ein hoher Baum", "tokens": ["Ich", "hofft'", ",", "es", "werd'", "ein", "ho\u00b7her", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Gottes Ehr' und Preis,", "tokens": ["Zu", "Got\u00b7tes", "Ehr'", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der, dacht' ich, wird in St\u00fcrmen", "tokens": ["Der", ",", "dacht'", "ich", ",", "wird", "in", "St\u00fcr\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "VVFIN", "PPER", "$,", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel schwache B\u00e4umlein schirmen", "tokens": ["Viel", "schwa\u00b7che", "B\u00e4um\u00b7lein", "schir\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Umher im weiten Kreis. \u2013", "tokens": ["Um\u00b7her", "im", "wei\u00b7ten", "Kreis", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mein Hoffen seh' ich nun vergehn:", "tokens": ["Mein", "Hof\u00b7fen", "seh'", "ich", "nun", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es welkt mein Zweigelein,", "tokens": ["Es", "welkt", "mein", "Zwei\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seh doch andre Reislein stehn,", "tokens": ["Und", "seh", "doch", "and\u00b7re", "Reis\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nicht so lieb und fein. \u2013", "tokens": ["Die", "nicht", "so", "lieb", "und", "fein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O, schlaf', mein armer Knabe!", "tokens": ["O", ",", "schlaf'", ",", "mein", "ar\u00b7mer", "Kna\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Mutter schl\u00e4ft im Grabe", "tokens": ["Die", "Mut\u00b7ter", "schl\u00e4ft", "im", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und denkt im Himmel Dein.", "tokens": ["Und", "denkt", "im", "Him\u00b7mel", "Dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}