{"dta.poem.14774": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die falsche Schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne ist untergegangen", "tokens": ["Die", "Son\u00b7ne", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und der Mond im tiefen Meer,", "tokens": ["Und", "der", "Mond", "im", "tie\u00b7fen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Es dunkelt schon \u00fcber dem Lande,", "tokens": ["Es", "dun\u00b7kelt", "schon", "\u00fc\u00b7ber", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gute Nacht! seh' Dich nimmermehr!", "tokens": ["Gu\u00b7te", "Nacht", "!", "seh'", "Dich", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}