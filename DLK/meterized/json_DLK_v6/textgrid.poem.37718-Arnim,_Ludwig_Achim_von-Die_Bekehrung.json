{"textgrid.poem.37718": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Bekehrung", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nie mit wilder Faust", "tokens": ["Wer", "nie", "mit", "wil\u00b7der", "Faust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An die eherne Glocke geschlagen,", "tokens": ["An", "die", "e\u00b7her\u00b7ne", "Glo\u00b7cke", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Worin der Geist gefangen haust,", "tokens": ["Wo\u00b7rin", "der", "Geist", "ge\u00b7fan\u00b7gen", "haust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wird nimmermehr Ruhe zusagen,", "tokens": ["Dem", "wird", "nim\u00b7mer\u00b7mehr", "Ru\u00b7he", "zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Der h\u00f6rt noch nicht,", "tokens": ["Der", "h\u00f6rt", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der sieht kein Licht,", "tokens": ["Der", "sieht", "kein", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er w\u00e4hnt sich Gott,", "tokens": ["Er", "w\u00e4hnt", "sich", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wei\u00df viel von sich zu sagen.", "tokens": ["Wei\u00df", "viel", "von", "sich", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wem nie das Herz zu schnell", "tokens": ["Wem", "nie", "das", "Herz", "zu", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In den forschenden Geist eingeschlagen,", "tokens": ["In", "den", "for\u00b7schen\u00b7den", "Geist", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Der sieht am lichten Tag nicht hell,", "tokens": ["Der", "sieht", "am", "lich\u00b7ten", "Tag", "nicht", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird \u00fcber die Zeiten hinjagen,", "tokens": ["Der", "wird", "\u00fc\u00b7ber", "die", "Zei\u00b7ten", "hin\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Der h\u00f6rt noch nichts,", "tokens": ["Der", "h\u00f6rt", "noch", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der sieht noch nichts,", "tokens": ["Der", "sieht", "noch", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er w\u00e4hnt sich Gott,", "tokens": ["Er", "w\u00e4hnt", "sich", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bis er sich \u00fcberschlagen.", "tokens": ["Bis", "er", "sich", "\u00fc\u00b7bersc\u00b7hla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wem nie ging aus die Luft,", "tokens": ["Wem", "nie", "ging", "aus", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn er fliehend viel Tausend mitrissen", "tokens": ["Wenn", "er", "flie\u00b7hend", "viel", "Tau\u00b7send", "mit\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PIAT", "NN", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wo Leichtsinn zu den Waffen ruft,", "tokens": ["Wo", "Leicht\u00b7sinn", "zu", "den", "Waf\u00b7fen", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der bleibt immerdar ohne Gewissen;", "tokens": ["Der", "bleibt", "im\u00b7mer\u00b7dar", "oh\u00b7ne", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der h\u00f6rt nur sich,", "tokens": ["Der", "h\u00f6rt", "nur", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der sieht nur sich,", "tokens": ["Der", "sieht", "nur", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der w\u00e4hnt sich Gott,", "tokens": ["Der", "w\u00e4hnt", "sich", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Hat k\u00fcssend sich zerbissen.", "tokens": ["Hat", "k\u00fcs\u00b7send", "sich", "zer\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wem nie mit Liebesmacht", "tokens": ["Wem", "nie", "mit", "Lie\u00b7bes\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Beide gl\u00fchende Arme gezogen,", "tokens": ["Bei\u00b7de", "gl\u00fc\u00b7hen\u00b7de", "Ar\u00b7me", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bis sie entwichen, er verlacht", "tokens": ["Bis", "sie", "ent\u00b7wi\u00b7chen", ",", "er", "ver\u00b7lacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von stockfinsteren N\u00e4chten umzogen,", "tokens": ["Von", "stock\u00b7fins\u00b7te\u00b7ren", "N\u00e4ch\u00b7ten", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Der h\u00f6rt mich nicht", "tokens": ["Der", "h\u00f6rt", "mich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Aus Zuversicht,", "tokens": ["Aus", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der meint sich Gott", "tokens": ["Der", "meint", "sich", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und hat sich Lieb' gelogen.", "tokens": ["Und", "hat", "sich", "Lieb'", "ge\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer sonst der Welten Lauf", "tokens": ["Wer", "sonst", "der", "Wel\u00b7ten", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf der eigenen F\u00e4hrte sich dachte,", "tokens": ["Auf", "der", "ei\u00b7ge\u00b7nen", "F\u00e4hr\u00b7te", "sich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sieht nun verwundert auf,", "tokens": ["Sieht", "nun", "ver\u00b7wun\u00b7dert", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wieviel gr\u00f6\u00dfer sich alles rings machte.", "tokens": ["Wie\u00b7viel", "gr\u00f6\u00b7\u00dfer", "sich", "al\u00b7les", "rings", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "PIS", "ADV", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Der h\u00f6rte nicht,", "tokens": ["Der", "h\u00f6r\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der s\u00e4he nicht,", "tokens": ["Der", "s\u00e4\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der meinte Gott,", "tokens": ["Der", "mein\u00b7te", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Da\u00df er das Gl\u00fcck verachte.", "tokens": ["Da\u00df", "er", "das", "Gl\u00fcck", "ver\u00b7ach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die blinde Leidenschaft", "tokens": ["Die", "blin\u00b7de", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ehret der klagende Mensch in dem Staube;", "tokens": ["Eh\u00b7ret", "der", "kla\u00b7gen\u00b7de", "Mensch", "in", "dem", "Stau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie f\u00fchrt dich an mit deiner Kraft", "tokens": ["Sie", "f\u00fchrt", "dich", "an", "mit", "dei\u00b7ner", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Klippen den V\u00f6geln zum Raube,", "tokens": ["Auf", "Klip\u00b7pen", "den", "V\u00f6\u00b7geln", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Du h\u00f6rst dich nicht,", "tokens": ["Du", "h\u00f6rst", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Du siehst dich nicht,", "tokens": ["Du", "siehst", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du f\u00fchlest Gott", "tokens": ["Du", "f\u00fch\u00b7lest", "Gott"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und betest nun mit Glauben.", "tokens": ["Und", "be\u00b7test", "nun", "mit", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer lernen kann, der lebt,", "tokens": ["Wer", "ler\u00b7nen", "kann", ",", "der", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der nur immerdar leben wird bleiben,", "tokens": ["Der", "nur", "im\u00b7mer\u00b7dar", "le\u00b7ben", "wird", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VAFIN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und die in allem wieder lebt,", "tokens": ["Und", "die", "in", "al\u00b7lem", "wie\u00b7der", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonn' wird ihn nun h\u00f6her noch treiben,", "tokens": ["Die", "Sonn'", "wird", "ihn", "nun", "h\u00f6\u00b7her", "noch", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Er h\u00f6rt in sich", "tokens": ["Er", "h\u00f6rt", "in", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nur dich, nur dich!", "tokens": ["Nur", "dich", ",", "nur", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er schauet Gott", "tokens": ["Er", "schau\u00b7et", "Gott"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und wird in Gott verbleiben.", "tokens": ["Und", "wird", "in", "Gott", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}