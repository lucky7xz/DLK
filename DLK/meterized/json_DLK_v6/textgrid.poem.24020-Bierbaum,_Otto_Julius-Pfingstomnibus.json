{"textgrid.poem.24020": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Pfingstomnibus", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei dicke Isabellen,", "tokens": ["Zwei", "di\u00b7cke", "I\u00b7sa\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ziehn das R\u00e4derhaus,", "tokens": ["Die", "ziehn", "das", "R\u00e4\u00b7der\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darinnen sieht's von hellen", "tokens": ["Da\u00b7rin\u00b7nen", "sieht's", "von", "hel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pfingstkleidern lustig aus.", "tokens": ["Pfingst\u00b7klei\u00b7dern", "lus\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kutscher auf dem Bocke", "tokens": ["Der", "Kut\u00b7scher", "auf", "dem", "Bo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sitzt zwischen Zweigen gr\u00fcn;", "tokens": ["Sitzt", "zwi\u00b7schen", "Zwei\u00b7gen", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wunder! An seinem Rocke", "tokens": ["Wun\u00b7der", "!", "An", "sei\u00b7nem", "Ro\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Zwei Fliederb\u00fcsche bl\u00fchn.", "tokens": ["Zwei", "Flie\u00b7der\u00b7b\u00fc\u00b7sche", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Peitsche l\u00e4\u00dft er wehen", "tokens": ["Die", "Peit\u00b7sche", "l\u00e4\u00dft", "er", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie linden Wimpelschwung,", "tokens": ["Wie", "lin\u00b7den", "Wim\u00b7pel\u00b7schwung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die dicken Gelben gehen", "tokens": ["Die", "di\u00b7cken", "Gel\u00b7ben", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut wie zwei Fohlen jung.", "tokens": ["Heut", "wie", "zwei", "Foh\u00b7len", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als wenn sie heut zu Ehren", "tokens": ["Als", "wenn", "sie", "heut", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Fr\u00fchlingsfeiertag", "tokens": ["Dem", "Fr\u00fch\u00b7lings\u00b7fei\u00b7er\u00b7tag"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Silberbeschlagen w\u00e4ren,", "tokens": ["Sil\u00b7ber\u00b7be\u00b7schla\u00b7gen", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Klingt ihrer Hufe Schlag.", "tokens": ["Klingt", "ih\u00b7rer", "Hu\u00b7fe", "Schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In hellen Resonnanzen", "tokens": ["In", "hel\u00b7len", "Re\u00b7son\u00b7nan\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "T\u00f6nts wider der Asphalt,", "tokens": ["T\u00f6nts", "wi\u00b7der", "der", "As\u00b7phalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klipp-klapp von Liebe und Tanzen", "tokens": ["Klipp\u00b7klapp", "von", "Lie\u00b7be", "und", "Tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ein Lied empor mir schallt:", "tokens": ["Ein", "Lied", "em\u00b7por", "mir", "schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein lieber Junge ist der Mai,", "tokens": ["Ein", "lie\u00b7ber", "Jun\u00b7ge", "ist", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sitzt mit gr\u00fcnem Kranze", "tokens": ["Er", "sitzt", "mit", "gr\u00fc\u00b7nem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf einer buschigen Linde srei", "tokens": ["Auf", "ei\u00b7ner", "bu\u00b7schi\u00b7gen", "Lin\u00b7de", "srei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "CARD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und spielt uns auf zum Tanze.", "tokens": ["Und", "spielt", "uns", "auf", "zum", "Tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hat Augen grade so wie du,", "tokens": ["Hat", "Au\u00b7gen", "gra\u00b7de", "so", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wie zwei Sonnen scheinen,", "tokens": ["Die", "wie", "zwei", "Son\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er spielt und schwingt den Takt dazu", "tokens": ["Er", "spielt", "und", "schwingt", "den", "Takt", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinen nackten Beinen.", "tokens": ["Mit", "sei\u00b7nen", "nack\u00b7ten", "Bei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Komm, M\u00e4del, gieb mir deine Hand,", "tokens": ["Komm", ",", "M\u00e4\u00b7del", ",", "gieb", "mir", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen einen drehen,", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nen", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie ihn der Mai, der Musikant,", "tokens": ["Wie", "ihn", "der", "Mai", ",", "der", "Mu\u00b7si\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Lebtag nicht gesehen.", "tokens": ["Sein", "Leb\u00b7tag", "nicht", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nicht nach der Ueberzarten Art", "tokens": ["Nicht", "nach", "der", "Ue\u00b7berz\u00b7ar\u00b7ten", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wolln wir im Kreise schleichen,", "tokens": ["Wolln", "wir", "im", "Krei\u00b7se", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir tanzen heute Himmelfahrt", "tokens": ["Wir", "tan\u00b7zen", "heu\u00b7te", "Him\u00b7mel\u00b7fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach des Maien Geigen.", "tokens": ["Und", "nach", "des", "Mai\u00b7en", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum fassen wir uns fest und warm", "tokens": ["Drum", "fas\u00b7sen", "wir", "uns", "fest", "und", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirbeln uns verwegen,", "tokens": ["Und", "wir\u00b7beln", "uns", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hopp, M\u00e4del, komm! In meinen Arm", "tokens": ["Hopp", ",", "M\u00e4\u00b7del", ",", "komm", "!", "In", "mei\u00b7nen", "Arm"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kannst du dich ruhig legen.", "tokens": ["Kannst", "du", "dich", "ru\u00b7hig", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So hoch des Maien Geige singt,", "tokens": ["So", "hoch", "des", "Mai\u00b7en", "Gei\u00b7ge", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hoch will ich dich heben:", "tokens": ["So", "hoch", "will", "ich", "dich", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer tanzend in die Liebe springt,", "tokens": ["Wer", "tan\u00b7zend", "in", "die", "Lie\u00b7be", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der springt ins ewige Leben.", "tokens": ["Der", "springt", "ins", "e\u00b7wi\u00b7ge", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}