{"textgrid.poem.28345": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Plumpart meint, er hat die K\u00fcnste, da\u00df ihn niemand sehen kan;", "tokens": ["Plum\u00b7part", "meint", ",", "er", "hat", "die", "K\u00fcns\u00b7te", ",", "da\u00df", "ihn", "nie\u00b7mand", "se\u00b7hen", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wann ihm gleich nun zwantzig F\u00e4uste Maul und Nase treffen an,", "tokens": ["Wann", "ihm", "gleich", "nun", "zwant\u00b7zig", "F\u00e4us\u00b7te", "Maul", "und", "Na\u00b7se", "tref\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "CARD", "NN", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Traut er dennoch seinen K\u00fcnsten, die da so sind eingericht,", "tokens": ["Traut", "er", "den\u00b7noch", "sei\u00b7nen", "K\u00fcns\u00b7ten", ",", "die", "da", "so", "sind", "ein\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df sie nur gehn auff das sehen, auff das f\u00fchlen aber nicht.", "tokens": ["Da\u00df", "sie", "nur", "gehn", "auff", "das", "se\u00b7hen", ",", "auff", "das", "f\u00fch\u00b7len", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "APPR", "ART", "VVINF", "$,", "APPR", "PDS", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}