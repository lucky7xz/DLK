{"textgrid.poem.35745": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der weise Diogenes war", "tokens": ["Der", "wei\u00b7se", "Dio\u00b7ge\u00b7nes", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VAFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Liebhaber ambrosischer Klarheit,", "tokens": ["Lieb\u00b7ha\u00b7ber", "am\u00b7bro\u00b7si\u00b7scher", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Und sang in der zechenden Schar:", "tokens": ["Und", "sang", "in", "der", "ze\u00b7chen\u00b7den", "Schar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trinkt, Br\u00fcder! im Wein ist die Wahrheit!", "tokens": ["Trinkt", ",", "Br\u00fc\u00b7der", "!", "im", "Wein", "ist", "die", "Wahr\u00b7heit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und kam er betrunken vom Schmaus,", "tokens": ["Und", "kam", "er", "be\u00b7trun\u00b7ken", "vom", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Dann w\u00e4hlte der Alte, so hei\u00dft es,", "tokens": ["Dann", "w\u00e4hl\u00b7te", "der", "Al\u00b7te", ",", "so", "hei\u00dft", "es", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ein lediges Oxhoft zum Haus,", "tokens": ["Ein", "le\u00b7di\u00b7ges", "Ox\u00b7hoft", "zum", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und freute sich atmend des Geistes.", "tokens": ["Und", "freu\u00b7te", "sich", "at\u00b7mend", "des", "Geis\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}