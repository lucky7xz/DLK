{"textgrid.poem.52197": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: 's ist im Sabinerland ein Kirchentor \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s ist im Sabinerland ein Kirchentor \u2013", "tokens": ["'s", "ist", "im", "Sa\u00b7bi\u00b7ner\u00b7land", "ein", "Kir\u00b7chen\u00b7tor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mir war ein Reisejugendtag erf\u00fcllt \u2013", "tokens": ["Mir", "war", "ein", "Rei\u00b7se\u00b7ju\u00b7gend\u00b7tag", "er\u00b7f\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sa\u00df auf einer Bank von Stein davor,", "tokens": ["Ich", "sa\u00df", "auf", "ei\u00b7ner", "Bank", "von", "Stein", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einen langen Mantel eingeh\u00fcllt,", "tokens": ["In", "ei\u00b7nen", "lan\u00b7gen", "Man\u00b7tel", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus dem Gebirge blies ein harscher Wind \u2013", "tokens": ["Aus", "dem", "Ge\u00b7bir\u00b7ge", "blies", "ein", "har\u00b7scher", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vor\u00fcber schritt ein Weib mit einem Kind,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "schritt", "ein", "Weib", "mit", "ei\u00b7nem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das, zu der Mutter fl\u00fcsternd, scheu begann:", "tokens": ["Das", ",", "zu", "der", "Mut\u00b7ter", "fl\u00fcs\u00b7ternd", ",", "scheu", "be\u00b7gann", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbda sitzt ein Pilgerim und Wandersmann!\u00ab", "tokens": ["\u00bb", "da", "sitzt", "ein", "Pil\u00b7ge\u00b7rim", "und", "Wan\u00b7ders\u00b7mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir blieb das Wort des Kindes eingepr\u00e4gt,", "tokens": ["Mir", "blieb", "das", "Wort", "des", "Kin\u00b7des", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wo ich neues Land und Meer erschaut,", "tokens": ["Und", "wo", "ich", "neu\u00b7es", "Land", "und", "Meer", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Wanderstecken neben mich gelegt,", "tokens": ["Den", "Wan\u00b7der\u00b7ste\u00b7cken", "ne\u00b7ben", "mich", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo das Geheimnis einer Ferne blaut,", "tokens": ["Wo", "das", "Ge\u00b7heim\u00b7nis", "ei\u00b7ner", "Fer\u00b7ne", "blaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ergriff mich uners\u00e4ttlich Lebenslust", "tokens": ["Er\u00b7griff", "mich", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "Le\u00b7bens\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und f\u00fcllte mir die Augen und die Brust,", "tokens": ["Und", "f\u00fcll\u00b7te", "mir", "die", "Au\u00b7gen", "und", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hell in die L\u00fcfte jubelnd rief ich dann:", "tokens": ["Hell", "in", "die", "L\u00fcf\u00b7te", "ju\u00b7belnd", "rief", "ich", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbich bin ein Pilgerim und Wandersmann!\u00ab", "tokens": ["\u00bb", "ich", "bin", "ein", "Pil\u00b7ge\u00b7rim", "und", "Wan\u00b7ders\u00b7mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es war am Comer- oder Langensee,", "tokens": ["Es", "war", "am", "Co\u00b7mer", "o\u00b7der", "Lan\u00b7gen\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf lichter Tiefe trug das Boot mich hin", "tokens": ["Auf", "lich\u00b7ter", "Tie\u00b7fe", "trug", "das", "Boot", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entgegen meinem ew'gen, stillen Schnee", "tokens": ["Ent\u00b7ge\u00b7gen", "mei\u00b7nem", "ew'\u00b7gen", ",", "stil\u00b7len", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einer andern lieben Pilgerin \u2013", "tokens": ["Mit", "ei\u00b7ner", "an\u00b7dern", "lie\u00b7ben", "Pil\u00b7ge\u00b7rin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Rasch zog mir meine Schwester aus dem Haar,", "tokens": ["Rasch", "zog", "mir", "mei\u00b7ne", "Schwes\u00b7ter", "aus", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem braungelockten, eins, das silbern war,", "tokens": ["Dem", "braun\u00b7ge\u00b7lock\u00b7ten", ",", "eins", ",", "das", "sil\u00b7bern", "war", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und es betrachtend, seufzt ich leis und sann:", "tokens": ["Und", "es", "be\u00b7trach\u00b7tend", ",", "seufzt", "ich", "leis", "und", "sann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbdu bist ein Pilgerim und Wandersmann.\u00ab", "tokens": ["\u00bb", "du", "bist", "ein", "Pil\u00b7ge\u00b7rim", "und", "Wan\u00b7ders\u00b7mann", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit Weib und Kind an meinem eignen Herd", "tokens": ["Mit", "Weib", "und", "Kind", "an", "mei\u00b7nem", "eig\u00b7nen", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einer h\u00e4uslich trauten Flamme Schein", "tokens": ["In", "ei\u00b7ner", "h\u00e4us\u00b7lich", "trau\u00b7ten", "Flam\u00b7me", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00fcnkt keine Ferne mir begehrenswert,", "tokens": ["D\u00fcnkt", "kei\u00b7ne", "Fer\u00b7ne", "mir", "be\u00b7geh\u00b7rens\u00b7wert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So ist es gut! So sollt es ewig sein...", "tokens": ["So", "ist", "es", "gut", "!", "So", "sollt", "es", "e\u00b7wig", "sein", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt f\u00e4llt das Wort mir pl\u00f6tzlich in den Sinn", "tokens": ["Jetzt", "f\u00e4llt", "das", "Wort", "mir", "pl\u00f6tz\u00b7lich", "in", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der kleinen furchtsamen Sabinerin,", "tokens": ["Der", "klei\u00b7nen", "furcht\u00b7sa\u00b7men", "Sa\u00b7bi\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Wort, das nimmer ich vergessen kann:", "tokens": ["Das", "Wort", ",", "das", "nim\u00b7mer", "ich", "ver\u00b7ges\u00b7sen", "kann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbda sitzt ein Pilgerim und Wandersmann\u00ab", "tokens": ["\u00bb", "da", "sitzt", "ein", "Pil\u00b7ge\u00b7rim", "und", "Wan\u00b7ders\u00b7mann", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}