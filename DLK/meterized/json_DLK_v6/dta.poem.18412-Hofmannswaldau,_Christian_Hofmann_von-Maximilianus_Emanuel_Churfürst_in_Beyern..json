{"dta.poem.18412": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Maximilianus Emanuel Churf\u00fcrst in  \n Beyern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seit Donau/ Rhein und Po mit blut vermenget fliessen/", "tokens": ["Seit", "Do\u00b7nau", "/", "Rhein", "und", "Po", "mit", "blut", "ver\u00b7men\u00b7get", "flies\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "NE", "KON", "NE", "APPR", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das deiner helden-faust die feinde geben m\u00fcssen/", "tokens": ["Das", "dei\u00b7ner", "hel\u00b7den\u00b7faust", "die", "fein\u00b7de", "ge\u00b7ben", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lauffen sie damit in manch entferntes land/", "tokens": ["So", "lauf\u00b7fen", "sie", "da\u00b7mit", "in", "manch", "ent\u00b7fern\u00b7tes", "land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und machen deinen ruhm in Ost und West bekand.", "tokens": ["Und", "ma\u00b7chen", "dei\u00b7nen", "ruhm", "in", "Ost", "und", "West", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "