{"textgrid.poem.61181": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist in leere N\u00fcchternheit die ganze Welt versunken,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist in leere N\u00fcchternheit die ganze Welt versunken,", "tokens": ["Es", "ist", "in", "lee\u00b7re", "N\u00fcch\u00b7tern\u00b7heit", "die", "gan\u00b7ze", "Welt", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und keine Zunge redet mehr vom Heil'gen Geiste trunken;", "tokens": ["Und", "kei\u00b7ne", "Zun\u00b7ge", "re\u00b7det", "mehr", "vom", "Heil'\u00b7gen", "Geis\u00b7te", "trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die Poesie, das fromme Kind, ist scheu von uns gewichen,", "tokens": ["Die", "Poe\u00b7sie", ",", "das", "from\u00b7me", "Kind", ",", "ist", "scheu", "von", "uns", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Himmel d\u00fcnkt uns tr\u00fcb und grau, und Sonn' und Mond verblichen;", "tokens": ["Der", "Him\u00b7mel", "d\u00fcnkt", "uns", "tr\u00fcb", "und", "grau", ",", "und", "Sonn'", "und", "Mond", "ver\u00b7bli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Die gro\u00df geschaut und gro\u00df gebaut, sie schlummern in den S\u00e4rgen,", "tokens": ["Die", "gro\u00df", "ge\u00b7schaut", "und", "gro\u00df", "ge\u00b7baut", ",", "sie", "schlum\u00b7mern", "in", "den", "S\u00e4r\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Auf ihren Gr\u00e4bern kriechen wir als ein Geschlecht von Zwergen,", "tokens": ["Auf", "ih\u00b7ren", "Gr\u00e4\u00b7bern", "krie\u00b7chen", "wir", "als", "ein", "Ge\u00b7schlecht", "von", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Nichts blieb uns als die schlimmste Kunst, zu zweifeln und zu richten,", "tokens": ["Nichts", "blieb", "uns", "als", "die", "schlimms\u00b7te", "Kunst", ",", "zu", "zwei\u00b7feln", "und", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Und wenn sich ein Gigant erhebt, so ist er's im Vernichten.", "tokens": ["Und", "wenn", "sich", "ein", "Gi\u00b7gant", "er\u00b7hebt", ",", "so", "ist", "er's", "im", "Ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wohl gr\u00fcbelt ihr und m\u00f6chtet gern das gro\u00dfe R\u00e4tsel l\u00f6sen,", "tokens": ["Wohl", "gr\u00fc\u00b7belt", "ihr", "und", "m\u00f6ch\u00b7tet", "gern", "das", "gro\u00b7\u00dfe", "R\u00e4t\u00b7sel", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Aus welchem tiefverborgnen Quell der Strom sich w\u00e4lzt des B\u00f6sen,", "tokens": ["Aus", "wel\u00b7chem", "tief\u00b7ver\u00b7borg\u00b7nen", "Quell", "der", "Strom", "sich", "w\u00e4lzt", "des", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ihr eilt gesch\u00e4ftig hin und her, um Wust auf Wust zu t\u00fcrmen,", "tokens": ["Ihr", "eilt", "ge\u00b7sch\u00e4f\u00b7tig", "hin", "und", "her", ",", "um", "Wust", "auf", "Wust", "zu", "t\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,", "KOUI", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und meint mit eures Witzes Rat den Himmel zu erst\u00fcrmen,", "tokens": ["Und", "meint", "mit", "eu\u00b7res", "Wit\u00b7zes", "Rat", "den", "Him\u00b7mel", "zu", "er\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Doch seht, nur eines Donners Schlag, nur eines Blitzes Flammen,", "tokens": ["Doch", "seht", ",", "nur", "ei\u00b7nes", "Don\u00b7ners", "Schlag", ",", "nur", "ei\u00b7nes", "Blit\u00b7zes", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ART", "NN", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und eurer Weisheit Pelion und Ossa st\u00fcrzt zusammen.", "tokens": ["Und", "eu\u00b7rer", "Weis\u00b7heit", "Pe\u00b7li\u00b7on", "und", "Os\u00b7sa", "st\u00fcrzt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ich aber sage euch: F\u00fcrwahr, es wird nicht anders werden,", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "euch", ":", "F\u00fcr\u00b7wahr", ",", "es", "wird", "nicht", "an\u00b7ders", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Bis ihr den Blick nicht himmelw\u00e4rts erhebt vom Staub der Erden,", "tokens": ["Bis", "ihr", "den", "Blick", "nicht", "him\u00b7mel\u00b7w\u00e4rts", "er\u00b7hebt", "vom", "Staub", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Bis ihr dem Geist der Liebe nicht, dem gro\u00dfen \u00dcberwinder,", "tokens": ["Bis", "ihr", "dem", "Geist", "der", "Lie\u00b7be", "nicht", ",", "dem", "gro\u00b7\u00dfen", "\u00dc\u00b7berw\u00b7in\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ART", "NN", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Dem\u00fctig euer Herz erschlie\u00dft und werdet wie die Kinder;", "tokens": ["De\u00b7m\u00fc\u00b7tig", "eu\u00b7er", "Herz", "er\u00b7schlie\u00dft", "und", "wer\u00b7det", "wie", "die", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "KON", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Denn wo die Liebe wohnt, da hat ein ew'ger Lenz begonnen,", "tokens": ["Denn", "wo", "die", "Lie\u00b7be", "wohnt", ",", "da", "hat", "ein", "ew'\u00b7ger", "Lenz", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Da gr\u00fcnen alle W\u00e4lder auf, und rauschen alle Bronnen,", "tokens": ["Da", "gr\u00fc\u00b7nen", "al\u00b7le", "W\u00e4l\u00b7der", "auf", ",", "und", "rau\u00b7schen", "al\u00b7le", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Ihr offenbart sich, was dem Blick der klugen Welt verborgen,", "tokens": ["Ihr", "of\u00b7fen\u00b7bart", "sich", ",", "was", "dem", "Blick", "der", "klu\u00b7gen", "Welt", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "In tr\u00fcber D\u00e4mmrung sieht sie schon den rosenroten Morgen,", "tokens": ["In", "tr\u00fc\u00b7ber", "D\u00e4mm\u00b7rung", "sieht", "sie", "schon", "den", "ro\u00b7sen\u00b7ro\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Das Brausen wird ihr zur Musik, zum Reigen das Gewimmel,", "tokens": ["Das", "Brau\u00b7sen", "wird", "ihr", "zur", "Mu\u00b7sik", ",", "zum", "Rei\u00b7gen", "das", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Helljauchzend steigt ihr Lied empor auf Fl\u00fcgeln in den Himmel,", "tokens": ["Hell\u00b7jauch\u00b7zend", "steigt", "ihr", "Lied", "em\u00b7por", "auf", "Fl\u00fc\u00b7geln", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Sie ist ein Kind und doch ein Held mit unbesiegten Waffen,", "tokens": ["Sie", "ist", "ein", "Kind", "und", "doch", "ein", "Held", "mit", "un\u00b7be\u00b7sieg\u00b7ten", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Und weil sie noch an Wunder glaubt, so kann sie Wunder schaffen.", "tokens": ["Und", "weil", "sie", "noch", "an", "Wun\u00b7der", "glaubt", ",", "so", "kann", "sie", "Wun\u00b7der", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}