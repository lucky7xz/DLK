{"textgrid.poem.47050": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht doch! Sie steht in ihrer stillen sch\u00f6nen", "tokens": ["Nicht", "doch", "!", "Sie", "steht", "in", "ih\u00b7rer", "stil\u00b7len", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleichg\u00fclt'gen Unbefangenheit noch immer!", "tokens": ["Gleich\u00b7g\u00fclt'\u00b7gen", "Un\u00b7be\u00b7fan\u00b7gen\u00b7heit", "noch", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O lern' von ihr, nimm ohne Klaggewimmer", "tokens": ["O", "lern'", "von", "ihr", ",", "nimm", "oh\u00b7ne", "Klag\u00b7ge\u00b7wim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "VVIMP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Abschied, geh' und nimm ihn ohne H\u00f6hnen.", "tokens": ["Den", "Ab\u00b7schied", ",", "geh'", "und", "nimm", "ihn", "oh\u00b7ne", "H\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sprich ruhig: Uns zusammen zu gew\u00f6hnen", "tokens": ["Sprich", "ru\u00b7hig", ":", "Uns", "zu\u00b7sam\u00b7men", "zu", "ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$.", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf l\u00e4ng're Zeit in deinem engen Zimmer,", "tokens": ["Auf", "l\u00e4ng'\u00b7re", "Zeit", "in", "dei\u00b7nem", "en\u00b7gen", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie ging es gut, nun geht es immer schlimmer;", "tokens": ["Nie", "ging", "es", "gut", ",", "nun", "geht", "es", "im\u00b7mer", "schlim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leb' wohl! und la\u00df die Trennung uns vers\u00f6hnen.", "tokens": ["Leb'", "wohl", "!", "und", "la\u00df", "die", "Tren\u00b7nung", "uns", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "KON", "VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich habe dir einmal ein Lied gegeben,", "tokens": ["Ich", "ha\u00b7be", "dir", "ein\u00b7mal", "ein", "Lied", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Behalt's und denk' dabei zu Zeiten meiner,", "tokens": ["Be\u00b7halt's", "und", "denk'", "da\u00b7bei", "zu", "Zei\u00b7ten", "mei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PAV", "APPR", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn du einst einen hast, der keine singet.", "tokens": ["Wenn", "du", "einst", "ei\u00b7nen", "hast", ",", "der", "kei\u00b7ne", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "PRELS", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du gabest mir nach kurzem Widerstreben", "tokens": ["Du", "ga\u00b7best", "mir", "nach", "kur\u00b7zem", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst diesen Ring; gedenken will ich deiner,", "tokens": ["Einst", "die\u00b7sen", "Ring", ";", "ge\u00b7den\u00b7ken", "will", "ich", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$.", "VVINF", "VMFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich damit wo ansto\u00df' und er klinget.", "tokens": ["Wenn", "ich", "da\u00b7mit", "wo", "an\u00b7sto\u00df'", "und", "er", "klin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PWAV", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}