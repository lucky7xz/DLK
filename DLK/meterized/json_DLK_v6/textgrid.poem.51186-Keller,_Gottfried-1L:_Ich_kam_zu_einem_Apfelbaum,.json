{"textgrid.poem.51186": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kam zu einem Apfelbaum,", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam zu einem Apfelbaum,", "tokens": ["Ich", "kam", "zu", "ei\u00b7nem", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dessen gr\u00fcnen \u00c4sten", "tokens": ["In", "des\u00b7sen", "gr\u00fc\u00b7nen", "\u00c4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein krummer Zwerg den frischen Schaum", "tokens": ["Ein", "krum\u00b7mer", "Zwerg", "den", "fri\u00b7schen", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00c4pfel sog, der besten.", "tokens": ["Der", "\u00c4p\u00b7fel", "sog", ",", "der", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Um einen Apfel bat ich ihn,", "tokens": ["Um", "ei\u00b7nen", "Ap\u00b7fel", "bat", "ich", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fing er an zu r\u00fctteln", "tokens": ["Da", "fing", "er", "an", "zu", "r\u00fct\u00b7teln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und toll und wild und her und hin", "tokens": ["Und", "toll", "und", "wild", "und", "her", "und", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "KON", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So Frucht wie Laub zu sch\u00fctteln.", "tokens": ["So", "Frucht", "wie", "Laub", "zu", "sch\u00fct\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich a\u00df, wie ein begier'ger Mann,", "tokens": ["Ich", "a\u00df", ",", "wie", "ein", "be\u00b7gier'\u00b7ger", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df es mich gel\u00fcsten,", "tokens": ["Und", "lie\u00df", "es", "mich", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht achtend, wie der Zwerg begann", "tokens": ["Nicht", "ach\u00b7tend", ",", "wie", "der", "Zwerg", "be\u00b7gann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krone zu verw\u00fcsten.", "tokens": ["Die", "Kro\u00b7ne", "zu", "ver\u00b7w\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da sang ein Vogel: \u00bbI\u00df, du Held!", "tokens": ["Da", "sang", "ein", "Vo\u00b7gel", ":", "\u00bb", "I\u00df", ",", "du", "Held", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast den Witz gefunden:", "tokens": ["Du", "hast", "den", "Witz", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Laub, das mit daneben f\u00e4llt,", "tokens": ["Das", "Laub", ",", "das", "mit", "da\u00b7ne\u00b7ben", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PAV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Bedeutet deine Stunden!\u00ab", "tokens": ["Be\u00b7deu\u00b7tet", "dei\u00b7ne", "Stun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da jagt ich Kobold Unverstand", "tokens": ["Da", "jagt", "ich", "Ko\u00b7bold", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herunter aus den Zweigen", "tokens": ["Her\u00b7un\u00b7ter", "aus", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und unternahm, mit Fu\u00df und Hand", "tokens": ["Und", "un\u00b7ter\u00b7nahm", ",", "mit", "Fu\u00df", "und", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedacht hinanzusteigen.", "tokens": ["Be\u00b7dacht", "hi\u00b7nan\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun sa\u00df ich selber auf dem Baum,", "tokens": ["Nun", "sa\u00df", "ich", "sel\u00b7ber", "auf", "dem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach \u00c4pfeln auszusp\u00e4hen,", "tokens": ["Nach", "\u00c4p\u00b7feln", "aus\u00b7zu\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ich geno\u00df den s\u00fc\u00dfen Schaum,", "tokens": ["Und", "ich", "ge\u00b7no\u00df", "den", "s\u00fc\u00b7\u00dfen", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00e4tter lie\u00df ich stehen.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "lie\u00df", "ich", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}