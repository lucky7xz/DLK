{"textgrid.poem.33764": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Priesterin der Isis in Rom", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heucheln soll ich Zauberk\u00fcnste,", "tokens": ["Heu\u00b7cheln", "soll", "ich", "Zau\u00b7ber\u00b7k\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Flammen tr\u00fcber D\u00fcnste", "tokens": ["In", "den", "Flam\u00b7men", "tr\u00fc\u00b7ber", "D\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00e4hen nach verborgnem Sinn;", "tokens": ["Sp\u00e4\u00b7hen", "nach", "ver\u00b7borg\u00b7nem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der V\u00f6gel hohen Fl\u00fcgen", "tokens": ["Aus", "der", "V\u00f6\u00b7gel", "ho\u00b7hen", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich Prophezeiung l\u00fcgen", "tokens": ["Soll", "ich", "Pro\u00b7phe\u00b7zei\u00b7ung", "l\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um verha\u00dften Goldgewinn.", "tokens": ["Um", "ver\u00b7ha\u00df\u00b7ten", "Gold\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob nicht bald ein Freier werbe,", "tokens": ["Ob", "nicht", "bald", "ein", "Frei\u00b7er", "wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob ein grauer Schurke sterbe,", "tokens": ["Ob", "ein", "grau\u00b7er", "Schur\u00b7ke", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welch ein Frevler ihn beerbe,", "tokens": ["Welch", "ein", "Frev\u00b7ler", "ihn", "be\u00b7er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fr\u00e4gt man die \u00c4gypterin.", "tokens": ["Fr\u00e4gt", "man", "die", "\u00c4\u00b7gyp\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "V\u00f6lkern hier ein Licht zu schenken", "tokens": ["V\u00f6l\u00b7kern", "hier", "ein", "Licht", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zur Wahrheit sie zu lenken,", "tokens": ["Und", "zur", "Wahr\u00b7heit", "sie", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hnte die Prophetenbraut;", "tokens": ["W\u00e4hn\u00b7te", "die", "Pro\u00b7phe\u00b7ten\u00b7braut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weh, nun mu\u00df ich hier bei Kesseln", "tokens": ["Weh", ",", "nun", "mu\u00df", "ich", "hier", "bei", "Kes\u00b7seln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauen, wie mit Lolch und Nesseln", "tokens": ["Schau\u00b7en", ",", "wie", "mit", "Lolch", "und", "Nes\u00b7seln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlangenhaut und Kr\u00f6te braut,", "tokens": ["Schlan\u00b7gen\u00b7haut", "und", "Kr\u00f6\u00b7te", "braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df mein edles Wissen sch\u00e4nden,", "tokens": ["Mu\u00df", "mein", "ed\u00b7les", "Wis\u00b7sen", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hohes sehn aus Sklavenh\u00e4nden", "tokens": ["Ho\u00b7hes", "sehn", "aus", "Skla\u00b7ven\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und am Dreiweg Feuerbr\u00e4nden", "tokens": ["Und", "am", "Drei\u00b7weg", "Feu\u00b7er\u00b7br\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Niederstreu'n das Lorbeerkraut.", "tokens": ["Nie\u00b7der\u00b7streu'n", "das", "Lor\u00b7beer\u00b7kraut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich, des gro\u00dfen Landes Tochter,", "tokens": ["Ich", ",", "des", "gro\u00b7\u00dfen", "Lan\u00b7des", "Toch\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo zuerst auf unterjochter", "tokens": ["Wo", "zu\u00b7erst", "auf", "un\u00b7ter\u00b7joch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erdkraft sich der Geist vernahm,", "tokens": ["Erd\u00b7kraft", "sich", "der", "Geist", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenes Landes, dessen Lehre", "tokens": ["Je\u00b7nes", "Lan\u00b7des", ",", "des\u00b7sen", "Leh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leuchtend \u00fcber Land und Meere", "tokens": ["Leuch\u00b7tend", "\u00fc\u00b7ber", "Land", "und", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einst an alle V\u00f6lker kam.", "tokens": ["Einst", "an", "al\u00b7le", "V\u00f6l\u00b7ker", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, wir waren's, die am fr\u00fchsten", "tokens": ["Ja", ",", "wir", "wa\u00b7ren's", ",", "die", "am", "fr\u00fchs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$,", "PRELS", "APPRART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Halt geboten Meer und W\u00fcsten,", "tokens": ["Halt", "ge\u00b7bo\u00b7ten", "Meer", "und", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit Gesang die Sterne gr\u00fc\u00dften,", "tokens": ["Mit", "Ge\u00b7sang", "die", "Ster\u00b7ne", "gr\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tiere zogen fromm und zahm!", "tokens": ["Tie\u00b7re", "zo\u00b7gen", "fromm", "und", "zahm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jene Weisheit ist verloren,", "tokens": ["Je\u00b7ne", "Weis\u00b7heit", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Gaukler, unter Toren", "tokens": ["Un\u00b7ter", "Gauk\u00b7ler", ",", "un\u00b7ter", "To\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00f6\u00dft ein fremder P\u00f6bel mich;", "tokens": ["St\u00f6\u00dft", "ein", "frem\u00b7der", "P\u00f6\u00b7bel", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weh, was lie\u00df ich den geliebten", "tokens": ["Weh", ",", "was", "lie\u00df", "ich", "den", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Strand des Nils, o dich, \u00c4gypten,", "tokens": ["Strand", "des", "Nils", ",", "o", "dich", ",", "\u00c4\u00b7gyp\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "FM", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Grab der K\u00f6nige und dich!", "tokens": ["Grab", "der", "K\u00f6\u00b7ni\u00b7ge", "und", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo beim Staub der Pharaonen", "tokens": ["Wo", "beim", "Staub", "der", "Pha\u00b7ra\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Teure Seelenwandrer wohnen,", "tokens": ["Teu\u00b7re", "See\u00b7len\u00b7wand\u00b7rer", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "K\u00f6nnt' auch ich im Schatten thronen", "tokens": ["K\u00f6nnt'", "auch", "ich", "im", "Schat\u00b7ten", "thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hochgeehrt und priesterlich.", "tokens": ["Hoch\u00b7geehrt", "und", "pries\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier ist alles wie zerrissen;", "tokens": ["Hier", "ist", "al\u00b7les", "wie", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nirgends kn\u00fcpft ein Allmachtwissen", "tokens": ["Nir\u00b7gends", "kn\u00fcpft", "ein", "All\u00b7macht\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erdennacht und Himmelspol.", "tokens": ["Er\u00b7den\u00b7nacht", "und", "Him\u00b7mel\u00b7spol", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch bedeutungslose Ferne", "tokens": ["Durch", "be\u00b7deu\u00b7tungs\u00b7lo\u00b7se", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Irren tonlos hier die Sterne,", "tokens": ["Ir\u00b7ren", "ton\u00b7los", "hier", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles t\u00f6nt mir leer und hohl.", "tokens": ["Al\u00b7les", "t\u00f6nt", "mir", "leer", "und", "hohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr zerrei\u00dft den Isisschleier,", "tokens": ["Ihr", "zer\u00b7rei\u00dft", "den", "I\u00b7sis\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber saget nun, Entweiher:", "tokens": ["A\u00b7ber", "sa\u00b7get", "nun", ",", "Ent\u00b7wei\u00b7her", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sehet die Natur ihr freier,", "tokens": ["Se\u00b7het", "die", "Na\u00b7tur", "ihr", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seit zertreten ihr Symbol?", "tokens": ["Seit", "zer\u00b7tre\u00b7ten", "ihr", "Sym\u00b7bol", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fromme V\u00f6gel seh' ich schweben", "tokens": ["From\u00b7me", "V\u00f6\u00b7gel", "seh'", "ich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem tempelreichen Theben,", "tokens": ["Nach", "dem", "tem\u00b7pel\u00b7rei\u00b7chen", "The\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald fliegt meine Seele mit;", "tokens": ["Bald", "fliegt", "mei\u00b7ne", "See\u00b7le", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn der Sternenkreis vollendet,", "tokens": ["Wenn", "der", "Ster\u00b7nen\u00b7kreis", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn zum Tal der Nil sich wendet,", "tokens": ["Wenn", "zum", "Tal", "der", "Nil", "sich", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Siegeshoch im Segensschritt,", "tokens": ["Sie\u00b7ges\u00b7hoch", "im", "Se\u00b7gens\u00b7schritt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer weil' ich dann hienieden;", "tokens": ["Nim\u00b7mer", "weil'", "ich", "dann", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hohe Nacht der Pyramiden,", "tokens": ["Ho\u00b7he", "Nacht", "der", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nimm mich auf, wenn ich um Frieden", "tokens": ["Nimm", "mich", "auf", ",", "wenn", "ich", "um", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deine goldnen Tore bitt'!", "tokens": ["Dei\u00b7ne", "gold\u00b7nen", "To\u00b7re", "bitt'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}