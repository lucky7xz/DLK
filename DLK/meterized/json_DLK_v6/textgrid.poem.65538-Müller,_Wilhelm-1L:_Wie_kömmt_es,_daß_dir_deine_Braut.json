{"textgrid.poem.65538": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie k\u00f6mmt es, da\u00df dir deine Braut", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6mmt es, da\u00df dir deine Braut", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "da\u00df", "dir", "dei\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sch\u00f6nen Kleidern wird getraut?", "tokens": ["In", "sch\u00f6\u00b7nen", "Klei\u00b7dern", "wird", "ge\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird nach langer Werbung Qual", "tokens": ["So", "wird", "nach", "lan\u00b7ger", "Wer\u00b7bung", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hohe Weisheit dein Gemahl.", "tokens": ["Die", "ho\u00b7he", "Weis\u00b7heit", "dein", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hast du sie in deinem Haus,", "tokens": ["Und", "hast", "du", "sie", "in", "dei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ziehst du ihr die Kleider aus.", "tokens": ["So", "ziehst", "du", "ihr", "die", "Klei\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann sieht der Doktor ohne Frist,", "tokens": ["Dann", "sieht", "der", "Dok\u00b7tor", "oh\u00b7ne", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie viel er hat, wie viel er ist.", "tokens": ["Wie", "viel", "er", "hat", ",", "wie", "viel", "er", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VAFIN", "$,", "PWAV", "PIS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}