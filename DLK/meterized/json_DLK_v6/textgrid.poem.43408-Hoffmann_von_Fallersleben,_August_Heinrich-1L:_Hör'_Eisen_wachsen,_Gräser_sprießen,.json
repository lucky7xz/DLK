{"textgrid.poem.43408": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6r' Eisen wachsen, Gr\u00e4ser sprie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' Eisen wachsen, Gr\u00e4ser sprie\u00dfen,", "tokens": ["H\u00f6r'", "Ei\u00b7sen", "wach\u00b7sen", ",", "Gr\u00e4\u00b7ser", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lies aus der Menschen Angesicht,", "tokens": ["Lies", "aus", "der", "Men\u00b7schen", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wisse Zukunft zu erschlie\u00dfen!", "tokens": ["Und", "wis\u00b7se", "Zu\u00b7kunft", "zu", "er\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Vorurtheil ist, wei\u00dft du nicht.", "tokens": ["Was", "Vor\u00b7urt\u00b7heil", "ist", ",", "wei\u00dft", "du", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zerhau den Wind auf seinem Gange,", "tokens": ["Zer\u00b7hau", "den", "Wind", "auf", "sei\u00b7nem", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Greif mit der Hand das Sonnenlicht,", "tokens": ["Greif", "mit", "der", "Hand", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zertritt den Schatten wie die Schlange!", "tokens": ["Zer\u00b7tritt", "den", "Schat\u00b7ten", "wie", "die", "Schlan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Vorurtheil bezwingst du nicht.", "tokens": ["Das", "Vor\u00b7urt\u00b7heil", "be\u00b7zwingst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}