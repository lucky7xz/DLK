{"dta.poem.14683": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger und J\u00e4gerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Im dunkelgr\u00fcnen Waldrevier,", "tokens": ["Im", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da blitzt der Liebste rosenroth,", "tokens": ["Da", "blitzt", "der", "Liebs\u00b7te", "ro\u00b7sen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gef\u00e4llt so sehr dem armen Thier,", "tokens": ["Ge\u00b7f\u00e4llt", "so", "sehr", "dem", "ar\u00b7men", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Hirschlein w\u00fcnscht, es l\u00e4ge todt.", "tokens": ["Das", "Hirsc\u00b7hlein", "w\u00fcnscht", ",", "es", "l\u00e4\u00b7ge", "todt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}