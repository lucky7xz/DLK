{"dta.poem.3197": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00e4lt den Vogel, der in L\u00fcften schwebt, am Band,", "tokens": ["Was", "h\u00e4lt", "den", "Vo\u00b7gel", ",", "der", "in", "L\u00fcf\u00b7ten", "schwebt", ",", "am", "Band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er zur Erde nicht herabf\u00e4llt? Gottes Hand.", "tokens": ["Da\u00df", "er", "zur", "Er\u00b7de", "nicht", "her\u00b7ab\u00b7f\u00e4llt", "?", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "VVPP", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dieselbe Gottes Hand h\u00e4lt auch am Band dein Leben,", "tokens": ["Die\u00b7sel\u00b7be", "Got\u00b7tes", "Hand", "h\u00e4lt", "auch", "am", "Band", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An welchem Abgrund auch es der Gefahr mag schweben.", "tokens": ["An", "wel\u00b7chem", "Ab\u00b7grund", "auch", "es", "der", "Ge\u00b7fahr", "mag", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mach', wie der Vogel, des Vertrauens Fittig fest!", "tokens": ["Mach'", ",", "wie", "der", "Vo\u00b7gel", ",", "des", "Ver\u00b7trau\u00b7ens", "Fit\u00b7tig", "fest", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NE", "$,", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Vom Irrflug tr\u00e4gt er dich noch heut' in's sichre Nest.", "tokens": ["Vom", "Irr\u00b7flug", "tr\u00e4gt", "er", "dich", "noch", "heut'", "in's", "sich\u00b7re", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}