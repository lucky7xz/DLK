{"textgrid.poem.25391": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn in den Gew\u00f6lben abendlich", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn in den Gew\u00f6lben abendlich", "tokens": ["Wenn", "in", "den", "Ge\u00b7w\u00f6l\u00b7ben", "a\u00b7bend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die blauen Kugelschalen", "tokens": ["die", "blau\u00b7en", "Ku\u00b7gel\u00b7scha\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aufd\u00e4mmern, gl\u00e4nzt ihr Licht in die Nacht hin\u00fcber", "tokens": ["Auf\u00b7d\u00e4m\u00b7mern", ",", "gl\u00e4nzt", "ihr", "Licht", "in", "die", "Nacht", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "gleich dem Feuer von Signalen.", "tokens": ["gleich", "dem", "Feu\u00b7er", "von", "Sig\u00b7na\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie Lichtoasen ruhen in der st\u00e4hlernen Hut", "tokens": ["Wie", "Lich\u00b7to\u00b7a\u00b7sen", "ru\u00b7hen", "in", "der", "st\u00e4h\u00b7ler\u00b7nen", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "die geschwungenen Hallen", "tokens": ["die", "ge\u00b7schwun\u00b7ge\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Und warten. Und dann sind sie", "tokens": ["Und", "war\u00b7ten", ".", "Und", "dann", "sind", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "KON", "ADV", "VAFIN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "mit einem Mal von Abenteuer \u00fcberfallen,", "tokens": ["mit", "ei\u00b7nem", "Mal", "von", "A\u00b7bent\u00b7eu\u00b7er", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und alle erzne Kraft", "tokens": ["Und", "al\u00b7le", "erz\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "ist in ihren riesigen Leib verstaut,", "tokens": ["ist", "in", "ih\u00b7ren", "rie\u00b7si\u00b7gen", "Leib", "ver\u00b7staut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Und der wilde Atem der Maschine, die wie ein Tier", "tokens": ["Und", "der", "wil\u00b7de", "A\u00b7tem", "der", "Ma\u00b7schi\u00b7ne", ",", "die", "wie", "ein", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN"], "meter": "--+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "auf der Flucht stille steht und um sich schaut,", "tokens": ["auf", "der", "Flucht", "stil\u00b7le", "steht", "und", "um", "sich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "VVFIN", "KON", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.13": {"text": "Und es ist,", "tokens": ["Und", "es", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "als ob sich das Schicksal vieler hundert Menschen", "tokens": ["als", "ob", "sich", "das", "Schick\u00b7sal", "vie\u00b7ler", "hun\u00b7dert", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PRF", "ART", "NN", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "in ihr erzitterndes Bett ergossen h\u00e4tte,", "tokens": ["in", "ihr", "er\u00b7zit\u00b7tern\u00b7des", "Bett", "er\u00b7gos\u00b7sen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und die Luft ist kriegerisch erf\u00fcllt", "tokens": ["Und", "die", "Luft", "ist", "krie\u00b7ge\u00b7risch", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "von den Balladen s\u00fcdlicher Meere", "tokens": ["von", "den", "Bal\u00b7la\u00b7den", "s\u00fcd\u00b7li\u00b7cher", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "und gr\u00fcner K\u00fcsten und der gro\u00dfen St\u00e4dte.", "tokens": ["und", "gr\u00fc\u00b7ner", "K\u00fcs\u00b7ten", "und", "der", "gro\u00b7\u00dfen", "St\u00e4d\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und dann zieht das Wunder weiter.", "tokens": ["Und", "dann", "zieht", "das", "Wun\u00b7der", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und schon ist wieder Stille und Licht", "tokens": ["Und", "schon", "ist", "wie\u00b7der", "Stil\u00b7le", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "wie ein Sternhimmel aufgegangen,", "tokens": ["wie", "ein", "Stern\u00b7him\u00b7mel", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.22": {"text": "Aber noch lange halten die aufgeschreckten W\u00e4nde,", "tokens": ["A\u00b7ber", "noch", "lan\u00b7ge", "hal\u00b7ten", "die", "auf\u00b7ge\u00b7schreck\u00b7ten", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.23": {"text": "wie Muscheln Meerget\u00f6n, die verklingende Musik", "tokens": ["wie", "Mu\u00b7scheln", "Meer\u00b7ge\u00b7t\u00f6n", ",", "die", "ver\u00b7klin\u00b7gen\u00b7de", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "eines wilden Abenteuers gefangen.", "tokens": ["ei\u00b7nes", "wil\u00b7den", "A\u00b7bent\u00b7eu\u00b7ers", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}