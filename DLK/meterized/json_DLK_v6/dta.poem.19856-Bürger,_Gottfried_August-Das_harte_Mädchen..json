{"dta.poem.19856": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  harte M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah so frei und wonnereich               ", "tokens": ["Ich", "sah", "so", "frei", "und", "won\u00b7ne\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst meine Tag\u2019 entschl\u00fcpfen,", "tokens": ["Einst", "mei\u00b7ne", "Tag'", "ent\u00b7schl\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie V\u00f6gelchen, von Zweig auf Zweig,", "tokens": ["Wie", "V\u00f6\u00b7gel\u00b7chen", ",", "von", "Zweig", "auf", "Zweig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Morgenliede h\u00fcpfen.", "tokens": ["Beim", "Mor\u00b7gen\u00b7lie\u00b7de", "h\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fragt jeden Sommerwind, der hier", "tokens": ["Fragt", "je\u00b7den", "Som\u00b7mer\u00b7wind", ",", "der", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumenau erfrischet:", "tokens": ["Die", "Blu\u00b7me\u00b7nau", "er\u00b7fri\u00b7schet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob je ein Seufzer sich von mir", "tokens": ["Ob", "je", "ein", "Seuf\u00b7zer", "sich", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen Hauch gemischet?", "tokens": ["In", "sei\u00b7nen", "Hauch", "ge\u00b7mi\u00b7schet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fragt nur den stillen Bach im Klee:", "tokens": ["Fragt", "nur", "den", "stil\u00b7len", "Bach", "im", "Klee", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob er mich klagen h\u00f6rte?", "tokens": ["Ob", "er", "mich", "kla\u00b7gen", "h\u00f6r\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ob von mir ein Thr\u00e4nchen je", "tokens": ["Und", "ob", "von", "mir", "ein", "Thr\u00e4n\u00b7chen", "je"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die kleinen Wellen mehrte?", "tokens": ["Die", "klei\u00b7nen", "Wel\u00b7len", "mehr\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Auge schaute falkenhell,", "tokens": ["Mein", "Au\u00b7ge", "schau\u00b7te", "fal\u00b7ken\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch meilenlange R\u00e4ume,", "tokens": ["Durch", "mei\u00b7len\u00b7lan\u00b7ge", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Gems und Eichhorn, sprang ich schnell", "tokens": ["Wie", "Gems", "und", "Eich\u00b7horn", ",", "sprang", "ich", "schnell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Felsen und auf B\u00e4ume.", "tokens": ["Auf", "Fel\u00b7sen", "und", "auf", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So bald ich auf mein Lager sank,", "tokens": ["So", "bald", "ich", "auf", "mein", "La\u00b7ger", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entschlief ich ungest\u00f6ret.", "tokens": ["Ent\u00b7schlief", "ich", "un\u00b7ge\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des W\u00e4chters Horn und Nachtgesang", "tokens": ["Des", "W\u00e4ch\u00b7ters", "Horn", "und", "Nacht\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat nie mein Ohr geh\u00f6ret.", "tokens": ["Hat", "nie", "mein", "Ohr", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun aber ist mir Lust und Scherz", "tokens": ["Nun", "a\u00b7ber", "ist", "mir", "Lust", "und", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Mut und Kraft vergangen.", "tokens": ["Und", "Mut", "und", "Kraft", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein hartes M\u00e4dchen h\u00e4lt mein Herz,", "tokens": ["Ein", "har\u00b7tes", "M\u00e4d\u00b7chen", "h\u00e4lt", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein armes Herz gefangen.", "tokens": ["Mein", "ar\u00b7mes", "Herz", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun hauch\u2019 ich meine Seele schier", "tokens": ["Nun", "hauch'", "ich", "mei\u00b7ne", "See\u00b7le", "schier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erseufzend in die Winde,", "tokens": ["Er\u00b7seuf\u00b7zend", "in", "die", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und girre kl\u00e4glich hin nach ihr,", "tokens": ["Und", "gir\u00b7re", "kl\u00e4g\u00b7lich", "hin", "nach", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich einem kranken Kinde.", "tokens": ["Gleich", "ei\u00b7nem", "kran\u00b7ken", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun m\u00fcssen Bach und Klee genung", "tokens": ["Nun", "m\u00fcs\u00b7sen", "Bach", "und", "Klee", "ge\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NE", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliebter Z\u00e4ren saugen,", "tokens": ["Ver\u00b7lieb\u00b7ter", "Z\u00e4\u00b7ren", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und graue Nebeld\u00e4mmerung", "tokens": ["Und", "grau\u00b7e", "Ne\u00b7bel\u00b7d\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Umw\u00f6lkt die muntern Augen.", "tokens": ["Um\u00b7w\u00f6lkt", "die", "mun\u00b7tern", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun h\u00e4rm\u2019 ich ganze N\u00e4chte lang,", "tokens": ["Nun", "h\u00e4rm'", "ich", "gan\u00b7ze", "N\u00e4ch\u00b7te", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf schlummerlosem Lager,", "tokens": ["Auf", "schlum\u00b7mer\u00b7lo\u00b7sem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die leichten Glieder mat und krank,", "tokens": ["Die", "leich\u00b7ten", "Glie\u00b7der", "mat", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die vollen Wangen hager.", "tokens": ["Die", "vol\u00b7len", "Wan\u00b7gen", "ha\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "An meinem Leben nagt die Wut", "tokens": ["An", "mei\u00b7nem", "Le\u00b7ben", "nagt", "die", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Grausamer Seelengeier;", "tokens": ["Grau\u00b7sa\u00b7mer", "See\u00b7len\u00b7gei\u00b7er", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nagt Eifersucht auf fremde Glut,", "tokens": ["Nagt", "Ei\u00b7fer\u00b7sucht", "auf", "frem\u00b7de", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nagt mein verschm\u00e4htes Feuer.", "tokens": ["Nagt", "mein", "ver\u00b7schm\u00e4h\u00b7tes", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das harte M\u00e4dchen sieht den Schmerz,", "tokens": ["Das", "har\u00b7te", "M\u00e4d\u00b7chen", "sieht", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mehrt ihn dennoch st\u00fcndlich.", "tokens": ["Und", "mehrt", "ihn", "den\u00b7noch", "st\u00fcnd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Liebe, kenst du noch ein Herz,", "tokens": ["O", "Lie\u00b7be", ",", "kenst", "du", "noch", "ein", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie dieses, unempfindlich? \u2014", "tokens": ["Wie", "die\u00b7ses", ",", "un\u00b7emp\u00b7find\u00b7lich", "?"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "PDS", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ein einzig L\u00e4cheln voller Huld", "tokens": ["Ein", "ein\u00b7zig", "L\u00e4\u00b7cheln", "vol\u00b7ler", "Huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00fcrd\u2019 allen Kummer lindern,", "tokens": ["W\u00fcrd'", "al\u00b7len", "Kum\u00b7mer", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre nicht erkante Schuld", "tokens": ["Und", "ih\u00b7re", "nicht", "er\u00b7kan\u00b7te", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "PTKNEG", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flugs tilgen, oder mindern.", "tokens": ["Flugs", "til\u00b7gen", ",", "o\u00b7der", "min\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mich wekte wol ihr s\u00fcsser Ton", "tokens": ["Mich", "wek\u00b7te", "wol", "ihr", "s\u00fcs\u00b7ser", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch aus dem Grabe wieder;", "tokens": ["Noch", "aus", "dem", "Gra\u00b7be", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja, w\u00e4r\u2019 ich auch im Himmel schon,", "tokens": ["Ja", ",", "w\u00e4r'", "ich", "auch", "im", "Him\u00b7mel", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er lokte mich hernieder.", "tokens": ["Er", "lok\u00b7te", "mich", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}