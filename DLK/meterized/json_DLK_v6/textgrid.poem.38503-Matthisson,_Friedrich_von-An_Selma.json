{"textgrid.poem.38503": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Selma", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte! weine nicht, da\u00df unser tr\u00fcbes", "tokens": ["Ge\u00b7lieb\u00b7te", "!", "wei\u00b7ne", "nicht", ",", "da\u00df", "un\u00b7ser", "tr\u00fc\u00b7bes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschick mit Thr\u00e4nen mehr als Freud' hienieden", "tokens": ["Ge\u00b7schick", "mit", "Thr\u00e4\u00b7nen", "mehr", "als", "Freud'", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PIAT", "KOKOM", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unsres Erdenlebens rauhe", "tokens": ["Uns\u00b7res", "Er\u00b7den\u00b7le\u00b7bens", "rau\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfade bezeichnet!", "tokens": ["Pfa\u00b7de", "be\u00b7zeich\u00b7net", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Gedenke jener Wonne, die der Tugend", "tokens": ["Ge\u00b7den\u00b7ke", "je\u00b7ner", "Won\u00b7ne", ",", "die", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Eden einst, durch Ewigkeiten harret!", "tokens": ["In", "E\u00b7den", "einst", ",", "durch", "E\u00b7wig\u00b7kei\u00b7ten", "har\u00b7ret", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wandellose Freude bl\u00fcht nur", "tokens": ["Wan\u00b7del\u00b7lo\u00b7se", "Freu\u00b7de", "bl\u00fcht", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenseit des Grabes!", "tokens": ["Jen\u00b7seit", "des", "Gra\u00b7bes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}