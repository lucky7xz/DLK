{"dta.poem.5608": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie elend ist Spinosens Blindheit nicht,", "tokens": ["Wie", "e\u00b7lend", "ist", "Spi\u00b7no\u00b7sens", "Blind\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er von dieser Sache spricht:", "tokens": ["Wenn", "er", "von", "die\u00b7ser", "Sa\u00b7che", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df die Materie und GOTT nur einerley,", "tokens": ["Da\u00df", "die", "Ma\u00b7te\u00b7rie", "und", "GoTT", "nur", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NE", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und da\u00df der C\u00f6rper, der sich reget,", "tokens": ["Und", "da\u00df", "der", "C\u00f6r\u00b7per", ",", "der", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Materialisch gantz, durch sich belebt, beweget,", "tokens": ["Ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "gantz", ",", "durch", "sich", "be\u00b7lebt", ",", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPR", "PRF", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das allgemeine Wesen sey.", "tokens": ["Das", "all\u00b7ge\u00b7mei\u00b7ne", "We\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein\u2019 Ordnung soll zugleich allein", "tokens": ["Sein'", "Ord\u00b7nung", "soll", "zu\u00b7gleich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Ursach und die W\u00fcrckung seyn.", "tokens": ["Die", "Ur\u00b7sach", "und", "die", "W\u00fcr\u00b7ckung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er sey zu aller Zeit", "tokens": ["Er", "sey", "zu", "al\u00b7ler", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Bewegend, und zugleich bewegt von Ewigkeit.", "tokens": ["Be\u00b7we\u00b7gend", ",", "und", "zu\u00b7gleich", "be\u00b7wegt", "von", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Stoff zusammt der Krafft, die sie zusammen h\u00e4ngt,", "tokens": ["Der", "Stoff", "zu\u00b7sammt", "der", "Krafft", ",", "die", "sie", "zu\u00b7sam\u00b7men", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die zeigen in dem Klumpen sich,", "tokens": ["Die", "zei\u00b7gen", "in", "dem", "Klum\u00b7pen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der unerm\u00e4\u00dflich ist, vermengt.", "tokens": ["Der", "un\u00b7er\u00b7m\u00e4\u00df\u00b7lich", "ist", ",", "ver\u00b7mengt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hier ist der C\u00f6rper Geist; der Geist ist c\u00f6rperlich.", "tokens": ["Hier", "ist", "der", "C\u00f6r\u00b7per", "Geist", ";", "der", "Geist", "ist", "c\u00f6r\u00b7per\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}