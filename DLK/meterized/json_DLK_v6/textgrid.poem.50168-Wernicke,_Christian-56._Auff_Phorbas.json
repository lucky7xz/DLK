{"textgrid.poem.50168": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "56. Auff Phorbas", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Er fragt nichts nach der ", "tokens": ["Er", "fragt", "nichts", "nach", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es gehn ihn ", "tokens": ["Es", "gehn", "ihn"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wenn er sie ", "tokens": ["Wenn", "er", "sie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Er denckt: vor ", "tokens": ["Er", "denckt", ":", "vor"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Die nur die ", "tokens": ["Die", "nur", "die"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Und kennt den ", "tokens": ["Und", "kennt", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}