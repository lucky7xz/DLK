{"dta.poem.10969": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dank-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott! wie gro\u00df ist doch die Menge", "tokens": ["Mein", "Gott", "!", "wie", "gro\u00df", "ist", "doch", "die", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gaben, die du mir geschenkt,", "tokens": ["Der", "Ga\u00b7ben", ",", "die", "du", "mir", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zumalen, wenn mein Geist der Zeiten L\u00e4nge,", "tokens": ["Zu\u00b7ma\u00b7len", ",", "wenn", "mein", "Geist", "der", "Zei\u00b7ten", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In welcher ich sie hab, erweget, und bedenkt.", "tokens": ["In", "wel\u00b7cher", "ich", "sie", "hab", ",", "er\u00b7we\u00b7get", ",", "und", "be\u00b7denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VAFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie mancher Tag, wie manche Nacht,", "tokens": ["Wie", "man\u00b7cher", "Tag", ",", "wie", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie manchen Theil der Zeit, wie viele Stunden,", "tokens": ["Wie", "man\u00b7chen", "Theil", "der", "Zeit", ",", "wie", "vie\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie viel Minuten und Secunden", "tokens": ["Wie", "viel", "Mi\u00b7nu\u00b7ten", "und", "Se\u00b7cun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Besitz ich sie! Und nicht allein", "tokens": ["Be\u00b7sitz", "ich", "sie", "!", "Und", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "$.", "KON", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur ich, so gar auch in den Meinen,", "tokens": ["Nur", "ich", ",", "so", "gar", "auch", "in", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der abermal so viele seyn,", "tokens": ["Der", "a\u00b7ber\u00b7mal", "so", "vie\u00b7le", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die gleichsam sich in mir vereinen,", "tokens": ["Die", "gleich\u00b7sam", "sich", "in", "mir", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Besitz ich ebenfalls die Gaben,", "tokens": ["Be\u00b7sitz", "ich", "e\u00b7ben\u00b7falls", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die sie, von deiner G\u00fctigkeit,", "tokens": ["Die", "sie", ",", "von", "dei\u00b7ner", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und zwar auch seit so langer Zeit,", "tokens": ["Und", "zwar", "auch", "seit", "so", "lan\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Empfunden und genossen haben.", "tokens": ["Emp\u00b7fun\u00b7den", "und", "ge\u00b7nos\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ich danke denn, wie billig, dir,", "tokens": ["Ich", "dan\u00b7ke", "denn", ",", "wie", "bil\u00b7lig", ",", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "O Brunnqvell alles Heils, daf\u00fcr.", "tokens": ["O", "Brunn\u00b7qvell", "al\u00b7les", "Heils", ",", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$,", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach la\u00df mich doch, auf dieser Welt,", "tokens": ["Ach", "la\u00df", "mich", "doch", ",", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nebst allen Meinen, so zu leben,", "tokens": ["Nebst", "al\u00b7len", "Mei\u00b7nen", ",", "so", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wie es dir, o mein Gott, gef\u00e4llt,", "tokens": ["Wie", "es", "dir", ",", "o", "mein", "Gott", ",", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Aus allen Kr\u00e4ften mich bestreben!", "tokens": ["Aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", "mich", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}