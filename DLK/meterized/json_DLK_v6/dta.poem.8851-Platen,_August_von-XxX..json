{"dta.poem.8851": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fc\u00dft' ich, wem ich mich vertraute:", "tokens": ["W\u00fc\u00dft'", "ich", ",", "wem", "ich", "mich", "ver\u00b7trau\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dein Fenster w\u00fcrd' ich treten,", "tokens": ["Vor", "dein", "Fens\u00b7ter", "w\u00fcrd'", "ich", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich blasen auf der Flaute;", "tokens": ["K\u00f6nnt'", "ich", "bla\u00b7sen", "auf", "der", "Flau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Worte scheinen mir so n\u00fcchtern,", "tokens": ["Wor\u00b7te", "schei\u00b7nen", "mir", "so", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mir oft vor ihnen graute!", "tokens": ["Da\u00df", "mir", "oft", "vor", "ih\u00b7nen", "grau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Worte h\u00f6rt man nicht von Ferne", "tokens": ["Wor\u00b7te", "h\u00f6rt", "man", "nicht", "von", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die s\u00fc\u00dfen Fl\u00f6tenlaute;", "tokens": ["Wie", "die", "s\u00fc\u00b7\u00dfen", "Fl\u00f6\u00b7ten\u00b7lau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch soll die Welt erfahren", "tokens": ["Den\u00b7noch", "soll", "die", "Welt", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Was ich Holdes an dir schaute:", "tokens": ["Was", "ich", "Hol\u00b7des", "an", "dir", "schau\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schwarzes Auge! Goldne Locken!", "tokens": ["Schwar\u00b7zes", "Au\u00b7ge", "!", "Gold\u00b7ne", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Uepp'ge Glieder, sch\u00f6ngebaute!", "tokens": ["Uepp'\u00b7ge", "Glie\u00b7der", ",", "sch\u00f6n\u00b7ge\u00b7bau\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nach dem Vlie\u00dfe deiner Locken", "tokens": ["Nach", "dem", "Vlie\u00b7\u00dfe", "dei\u00b7ner", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "F\u00e4hrt mein Herz als Argonaute.", "tokens": ["F\u00e4hrt", "mein", "Herz", "als", "Ar\u00b7go\u00b7nau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}