{"textgrid.poem.24268": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Eisblumen zu Weihnachten", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das unfruchtbare Eis, kalt, panzerglatt,", "tokens": ["Das", "un\u00b7frucht\u00b7ba\u00b7re", "Eis", ",", "kalt", ",", "pan\u00b7zer\u00b7glatt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verh\u00e4rtet Leben, das dem Tode dient,", "tokens": ["Ver\u00b7h\u00e4r\u00b7tet", "Le\u00b7ben", ",", "das", "dem", "To\u00b7de", "dient", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sich, der Farblose, mit ihm umschient \u2013", "tokens": ["Der", "sich", ",", "der", "Farb\u00b7lo\u00b7se", ",", "mit", "ihm", "um\u00b7schient", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Eis, das keine Seele hat,", "tokens": ["Das", "Eis", ",", "das", "kei\u00b7ne", "See\u00b7le", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das unbewegte, allen Lebens Bann:", "tokens": ["Das", "un\u00b7be\u00b7weg\u00b7te", ",", "al\u00b7len", "Le\u00b7bens", "Bann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das starre Eis selbst ist nicht tot.", "tokens": ["Das", "star\u00b7re", "Eis", "selbst", "ist", "nicht", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihm auch wirkt gestaltendes Gebot,", "tokens": ["In", "ihm", "auch", "wirkt", "ge\u00b7stal\u00b7ten\u00b7des", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sch\u00f6nheit Triebkraft ward auch ihm:", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Trieb\u00b7kraft", "ward", "auch", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es setzt geheimnisvolle Bl\u00fcten an,", "tokens": ["Es", "setzt", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Bl\u00fc\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Schwingenrispen, wie dem Seraphim", "tokens": ["Und", "Schwin\u00b7gen\u00b7ris\u00b7pen", ",", "wie", "dem", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gefieder\u00fcppig sie aus Schulternrund,", "tokens": ["Ge\u00b7fie\u00b7de\u00b7r\u00fcp\u00b7pig", "sie", "aus", "Schul\u00b7tern\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gekraust, geschwungen, tausendf\u00f6rmig und", "tokens": ["Ge\u00b7kraust", ",", "ge\u00b7schwun\u00b7gen", ",", "tau\u00b7send\u00b7f\u00f6r\u00b7mig", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVPP", "$,", "ADJD", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In tausend Formen ", "tokens": ["In", "tau\u00b7send", "For\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Siehst du im Eis nach innerstem Gesetz,", "tokens": ["Siehst", "du", "im", "Eis", "nach", "in\u00b7ners\u00b7tem", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Ein wunderbares Bild, zusammenschie\u00dfen.", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7res", "Bild", ",", "zu\u00b7sam\u00b7men\u00b7schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die \u00e4rmste Scherbe tr\u00e4gt ein Wundernetz,", "tokens": ["Die", "\u00e4rms\u00b7te", "Scher\u00b7be", "tr\u00e4gt", "ein", "Wun\u00b7der\u00b7netz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und alles glei\u00dft von Wundersilberfliesen.", "tokens": ["Und", "al\u00b7les", "glei\u00dft", "von", "Wun\u00b7der\u00b7sil\u00b7ber\u00b7flie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh, Mensch, mit Andacht diesem Wunder zu", "tokens": ["Sieh", ",", "Mensch", ",", "mit", "An\u00b7dacht", "die\u00b7sem", "Wun\u00b7der", "zu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "APPR", "NN", "PDAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und glaub ans Leben! \u00dcberall sind Triebe.", "tokens": ["Und", "glaub", "ans", "Le\u00b7ben", "!", "\u00dc\u00b7be\u00b7rall", "sind", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist kein Wahn: Im Tode selbst ist Liebe,", "tokens": ["Es", "ist", "kein", "Wahn", ":", "Im", "To\u00b7de", "selbst", "ist", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$.", "APPRART", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und neues Werden und bewegte Ruh.", "tokens": ["Und", "neu\u00b7es", "Wer\u00b7den", "und", "be\u00b7weg\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}