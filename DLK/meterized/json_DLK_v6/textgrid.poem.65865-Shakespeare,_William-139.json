{"textgrid.poem.65865": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "139", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, nicht Besch\u00f6nigung des Unrechts brauche,", "tokens": ["O", ",", "nicht", "Be\u00b7sch\u00f6\u00b7ni\u00b7gung", "des", "Un\u00b7rechts", "brau\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du so lieblos meinem Herzen bist!", "tokens": ["Wenn", "du", "so", "lieb\u00b7los", "mei\u00b7nem", "Her\u00b7zen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwunde mit dem Mund, nicht mit dem Auge;", "tokens": ["Ver\u00b7wun\u00b7de", "mit", "dem", "Mund", ",", "nicht", "mit", "dem", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mi\u00df Kraft an Kraft; und triff mich nicht mit List!", "tokens": ["Mi\u00df", "Kraft", "an", "Kraft", ";", "und", "triff", "mich", "nicht", "mit", "List", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$.", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sprich: Andre lieb' ich; doch das Seitenwenden", "tokens": ["Sprich", ":", "And\u00b7re", "lieb'", "ich", ";", "doch", "das", "Sei\u00b7ten\u00b7wen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "PIS", "VVFIN", "PPER", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Augen meide, Liebchen, wo ich bin.", "tokens": ["Der", "Au\u00b7gen", "mei\u00b7de", ",", "Lieb\u00b7chen", ",", "wo", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wozu die K\u00fcnste? Reicht des M\u00fcden Los zu enden,", "tokens": ["Wo\u00b7zu", "die", "K\u00fcns\u00b7te", "?", "Reicht", "des", "M\u00fc\u00b7den", "Los", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$.", "NN", "ART", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir nicht die offne St\u00e4rke mehr als hin?", "tokens": ["Dir", "nicht", "die", "off\u00b7ne", "St\u00e4r\u00b7ke", "mehr", "als", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJA", "NN", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich will dein Anwalt sein: Wohl wei\u00df die Dirne,", "tokens": ["Ich", "will", "dein", "An\u00b7walt", "sein", ":", "Wohl", "wei\u00df", "die", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df ihre art'gen Augen mich bekriegt;", "tokens": ["Da\u00df", "ih\u00b7re", "art'\u00b7gen", "Au\u00b7gen", "mich", "be\u00b7kriegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Drum wendet sie den Feind mir von der Stirne,", "tokens": ["Drum", "wen\u00b7det", "sie", "den", "Feind", "mir", "von", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Damit sein Pfeil nach andern Zielen fliegt. \u2013", "tokens": ["Da\u00b7mit", "sein", "Pfeil", "nach", "an\u00b7dern", "Zie\u00b7len", "fliegt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch, la\u00df es gut sein! halb schon bin ich tot;", "tokens": ["Doch", ",", "la\u00df", "es", "gut", "sein", "!", "halb", "schon", "bin", "ich", "tot", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "PPER", "ADJD", "VAINF", "$.", "ADJD", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So blicke fort, und ende meine Not.", "tokens": ["So", "bli\u00b7cke", "fort", ",", "und", "en\u00b7de", "mei\u00b7ne", "Not", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}