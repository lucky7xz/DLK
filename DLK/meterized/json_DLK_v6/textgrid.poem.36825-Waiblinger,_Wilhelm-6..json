{"textgrid.poem.36825": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer h\u00e4tte je so schwesterlich verbunden", "tokens": ["Wer", "h\u00e4t\u00b7te", "je", "so", "schwes\u00b7ter\u00b7lich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kraft der ungesell'gen Elemente", "tokens": ["Die", "Kraft", "der", "un\u00b7ge\u00b7sell'\u00b7gen", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In einem einz'gen sch\u00f6nen Stern gefunden?", "tokens": ["In", "ei\u00b7nem", "einz'\u00b7gen", "sch\u00f6\u00b7nen", "Stern", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verkl\u00e4rt schien mir in seinem Glanz die Erde,", "tokens": ["Ver\u00b7kl\u00e4rt", "schien", "mir", "in", "sei\u00b7nem", "Glanz", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Irdische verewigt und vergeistigt,", "tokens": ["Das", "Ir\u00b7di\u00b7sche", "ve\u00b7re\u00b7wigt", "und", "ver\u00b7geis\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich w\u00e4hnte, da\u00df es nie vergehen werde.", "tokens": ["Ich", "w\u00e4hn\u00b7te", ",", "da\u00df", "es", "nie", "ver\u00b7ge\u00b7hen", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des ganzen Himmels Sch\u00f6ne lacht' in blauen,", "tokens": ["Des", "gan\u00b7zen", "Him\u00b7mels", "Sch\u00f6\u00b7ne", "lacht'", "in", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In offnen, undurchdringlich hellen Tiefen,", "tokens": ["In", "off\u00b7nen", ",", "un\u00b7durch\u00b7dring\u00b7lich", "hel\u00b7len", "Tie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie konnt' ich bis zu ihrem Grunde schauen.", "tokens": ["Nie", "konnt'", "ich", "bis", "zu", "ih\u00b7rem", "Grun\u00b7de", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sein Licht, es galt mir mehr als Mond und Sonne,", "tokens": ["Sein", "Licht", ",", "es", "galt", "mir", "mehr", "als", "Mond", "und", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Fr\u00fchling bringen sie, mir brachte jenes", "tokens": ["Den", "Fr\u00fch\u00b7ling", "brin\u00b7gen", "sie", ",", "mir", "brach\u00b7te", "je\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die keuschen Rosen erster Liebeswonne.", "tokens": ["Die", "keu\u00b7schen", "Ro\u00b7sen", "ers\u00b7ter", "Lie\u00b7bes\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach denk' ich gar der s\u00fc\u00dfen, hei\u00dfen Fluthen,", "tokens": ["Ach", "denk'", "ich", "gar", "der", "s\u00fc\u00b7\u00dfen", ",", "hei\u00b7\u00dfen", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit der Schmerz, die Wehmuth es gefeuchtet,", "tokens": ["Wo\u00b7mit", "der", "Schmerz", ",", "die", "Weh\u00b7muth", "es", "ge\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00e4ngt mir's im tiefsten Herzen an zu bluten.", "tokens": ["F\u00e4ngt", "mir's", "im", "tiefs\u00b7ten", "Her\u00b7zen", "an", "zu", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "ADJA", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gen\u00fcgt dir Eines schon, der St\u00fcrme Wehen,", "tokens": ["Ge\u00b7n\u00fcgt", "dir", "Ei\u00b7nes", "schon", ",", "der", "St\u00fcr\u00b7me", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Macht des Meers, der Flammen und der Erde,", "tokens": ["Die", "Macht", "des", "Meers", ",", "der", "Flam\u00b7men", "und", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Eins, im Elemente zu vergehen,", "tokens": ["Nur", "Eins", ",", "im", "E\u00b7le\u00b7men\u00b7te", "zu", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Dann darf der Sterbliche f\u00fcrwahr nicht klagen,", "tokens": ["Dann", "darf", "der", "Sterb\u00b7li\u00b7che", "f\u00fcr\u00b7wahr", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der einst sie alle seelenvoll zerflossen", "tokens": ["Der", "einst", "sie", "al\u00b7le", "see\u00b7len\u00b7voll", "zer\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In eines Auges feuchtem Licht ertragen.", "tokens": ["In", "ei\u00b7nes", "Au\u00b7ges", "feuch\u00b7tem", "Licht", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}