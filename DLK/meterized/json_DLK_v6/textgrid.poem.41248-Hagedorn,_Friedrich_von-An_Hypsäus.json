{"textgrid.poem.41248": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Hyps\u00e4us", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man mu\u00df nicht allezeit was Hocherhabnes sagen:", "tokens": ["Man", "mu\u00df", "nicht", "al\u00b7le\u00b7zeit", "was", "Ho\u00b7cher\u00b7hab\u00b7nes", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "ADV", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der allgemeine Witz ist nicht der Hoheit Freund.", "tokens": ["Der", "all\u00b7ge\u00b7mei\u00b7ne", "Witz", "ist", "nicht", "der", "Ho\u00b7heit", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Weltlichts vollen Glanz kann mancher nicht ertragen,", "tokens": ["Des", "Welt\u00b7lichts", "vol\u00b7len", "Glanz", "kann", "man\u00b7cher", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der seinen Schimmer liebt, wann er in Wassern scheint.", "tokens": ["Der", "sei\u00b7nen", "Schim\u00b7mer", "liebt", ",", "wann", "er", "in", "Was\u00b7sern", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht jeder Wahrheit Bild kann helle Farben leiden,", "tokens": ["Nicht", "je\u00b7der", "Wahr\u00b7heit", "Bild", "kann", "hel\u00b7le", "Far\u00b7ben", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die reizt, wann um ihr Licht ein zarter Schatten spielt.", "tokens": ["Die", "reizt", ",", "wann", "um", "ihr", "Licht", "ein", "zar\u00b7ter", "Schat\u00b7ten", "spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Uns brennt der Sonne Glut auf unbepflanzten Haiden,", "tokens": ["Uns", "brennt", "der", "Son\u00b7ne", "Glut", "auf", "un\u00b7be\u00b7pflanz\u00b7ten", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die uns zur Anmuth strahlt, wenn sie ein Lustwald k\u00fchlt.", "tokens": ["Die", "uns", "zur", "An\u00b7muth", "strahlt", ",", "wenn", "sie", "ein", "Lust\u00b7wald", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}