{"textgrid.poem.56451": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie trampeln um den Hof im engen Kreis.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie trampeln um den Hof im engen Kreis.", "tokens": ["Sie", "tram\u00b7peln", "um", "den", "Hof", "im", "en\u00b7gen", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Blick schweift hin und her im kahlen Raum.", "tokens": ["Ihr", "Blick", "schweift", "hin", "und", "her", "im", "kah\u00b7len", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er sucht nach einem Feld, nach einem Baum,", "tokens": ["Er", "sucht", "nach", "ei\u00b7nem", "Feld", ",", "nach", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und prallt zur\u00fcck von kahler Mauern Wei\u00df.", "tokens": ["Und", "prallt", "zu\u00b7r\u00fcck", "von", "kah\u00b7ler", "Mau\u00b7ern", "Wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie in den M\u00fchlen dreht der R\u00e4dergang,", "tokens": ["Wie", "in", "den", "M\u00fch\u00b7len", "dreht", "der", "R\u00e4\u00b7der\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So dreht sich ihrer Schritte schwarze Spur.", "tokens": ["So", "dreht", "sich", "ih\u00b7rer", "Schrit\u00b7te", "schwar\u00b7ze", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie ein Sch\u00e4del mit der M\u00f6nchstonsur,", "tokens": ["Und", "wie", "ein", "Sch\u00e4\u00b7del", "mit", "der", "M\u00f6nchs\u00b7ton\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So liegt des Hofes Mitte kahl und blank.", "tokens": ["So", "liegt", "des", "Ho\u00b7fes", "Mit\u00b7te", "kahl", "und", "blank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es regnet d\u00fcnn auf ihren kurzen Rock.", "tokens": ["Es", "reg\u00b7net", "d\u00fcnn", "auf", "ih\u00b7ren", "kur\u00b7zen", "Rock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie schaun betr\u00fcbt die graue Wand empor,", "tokens": ["Sie", "schaun", "be\u00b7tr\u00fcbt", "die", "grau\u00b7e", "Wand", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo kleine Fenster sind, mit Kasten vor,", "tokens": ["Wo", "klei\u00b7ne", "Fens\u00b7ter", "sind", ",", "mit", "Kas\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie schwarze Waben in dem Bienenstock.", "tokens": ["Wie", "schwar\u00b7ze", "Wa\u00b7ben", "in", "dem", "Bie\u00b7nen\u00b7stock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man treibt sie ein, wie Schafe zu der Schur.", "tokens": ["Man", "treibt", "sie", "ein", ",", "wie", "Scha\u00b7fe", "zu", "der", "Schur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die grauen R\u00fccken dr\u00e4ngen in den Stall.", "tokens": ["Die", "grau\u00b7en", "R\u00fc\u00b7cken", "dr\u00e4n\u00b7gen", "in", "den", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und klappernd schallt heraus der Widerhall", "tokens": ["Und", "klap\u00b7pernd", "schallt", "he\u00b7raus", "der", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Holzpantoffeln auf dem Treppenflur.", "tokens": ["Der", "Holz\u00b7pan\u00b7tof\u00b7feln", "auf", "dem", "Trep\u00b7pen\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}