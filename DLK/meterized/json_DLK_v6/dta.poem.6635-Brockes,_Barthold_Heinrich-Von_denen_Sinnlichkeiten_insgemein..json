{"dta.poem.6635": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch, durch GOTTES Hand gebildet und formirt,", "tokens": ["Der", "Mensch", ",", "durch", "GoT\u00b7TES", "Hand", "ge\u00b7bil\u00b7det", "und", "for\u00b7mirt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eintzig unsre Welt bewohnet und regiert,", "tokens": ["Der", "eint\u00b7zig", "uns\u00b7re", "Welt", "be\u00b7woh\u00b7net", "und", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Enthielt\u2019 in seinem Schoo\u00df die Nachwelt auf einmal.", "tokens": ["Ent\u00b7hielt'", "in", "sei\u00b7nem", "Schoo\u00df", "die", "Nach\u00b7welt", "auf", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die W\u00fcsten f\u00fcllete sein Saame, sonder Zahl,", "tokens": ["Die", "W\u00fcs\u00b7ten", "f\u00fcl\u00b7le\u00b7te", "sein", "Saa\u00b7me", ",", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bebauete das Feld, beschiffete das Meer.", "tokens": ["Be\u00b7bau\u00b7e\u00b7te", "das", "Feld", ",", "be\u00b7schif\u00b7fe\u00b7te", "das", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Dingen, die sich \u00e4ndern und vergehen,", "tokens": ["Von", "Din\u00b7gen", ",", "die", "sich", "\u00e4n\u00b7dern", "und", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kan man in Menschen recht den Auszug sehen.", "tokens": ["Kan", "man", "in", "Men\u00b7schen", "recht", "den", "Aus\u00b7zug", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es findet sich in seiner ersten Zeit,", "tokens": ["Es", "fin\u00b7det", "sich", "in", "sei\u00b7ner", "ers\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn er empfangen wird, kein Unterscheid.", "tokens": ["Wenn", "er", "emp\u00b7fan\u00b7gen", "wird", ",", "kein", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie eine grosse Zahl von Thier-und Pflantzen,", "tokens": ["Wie", "ei\u00b7ne", "gros\u00b7se", "Zahl", "von", "Thier\u00b7\u00b7und", "Pflant\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die bald aus einem Korn, bald einem Ey entspringt,", "tokens": ["Die", "bald", "aus", "ei\u00b7nem", "Korn", ",", "bald", "ei\u00b7nem", "Ey", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu seiner Vollenkommenheit", "tokens": ["Zu", "sei\u00b7ner", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sich Staffel-Weise dringt;", "tokens": ["Sich", "Staf\u00b7fel\u00b7Wei\u00b7se", "dringt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Empf\u00e4nget auch der Mensch gar bald", "tokens": ["Emp\u00b7f\u00e4n\u00b7get", "auch", "der", "Mensch", "gar", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein\u2019 Art von Bildung und Gestalt:", "tokens": ["Ein'", "Art", "von", "Bil\u00b7dung", "und", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Es scheint, ob werd\u2019 er in der Mutter Schoo\u00df", "tokens": ["Es", "scheint", ",", "ob", "werd'", "er", "in", "der", "Mut\u00b7ter", "Schoo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Recht auf die Art, als eine Pflantze, gro\u00df:", "tokens": ["Recht", "auf", "die", "Art", ",", "als", "ei\u00b7ne", "Pflant\u00b7ze", ",", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}