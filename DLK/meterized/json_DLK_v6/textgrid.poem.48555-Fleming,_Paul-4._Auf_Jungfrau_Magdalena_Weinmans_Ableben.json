{"textgrid.poem.48555": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf Jungfrau Magdalena Weinmans Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du dennoch hin,", "tokens": ["So", "bist", "du", "den\u00b7noch", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "du sch\u00f6ne ", "tokens": ["du", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "von welcher man so ofte", "tokens": ["von", "wel\u00b7cher", "man", "so", "of\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "den blassen Tod erfuhr,", "tokens": ["den", "blas\u00b7sen", "Tod", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "bald die gew\u00fcndschte Kur", "tokens": ["bald", "die", "ge\u00b7w\u00fcnd\u00b7schte", "Kur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und Leben hoffte?", "tokens": ["und", "Le\u00b7ben", "hoff\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du Tugendsame, du,", "tokens": ["Du", "Tu\u00b7gend\u00b7sa\u00b7me", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "tust du die Augen zu,", "tokens": ["tust", "du", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "die mehr nicht wollen glimmen?", "tokens": ["die", "mehr", "nicht", "wol\u00b7len", "glim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du wolberedter Mund,", "tokens": ["Du", "wol\u00b7be\u00b7red\u00b7ter", "Mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "erstummen dir itzund", "tokens": ["er\u00b7stum\u00b7men", "dir", "it\u00b7zund"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die s\u00fc\u00dfen Stimmen?", "tokens": ["die", "s\u00fc\u00b7\u00dfen", "Stim\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wo ist der Stirnen Schein,", "tokens": ["Wo", "ist", "der", "Stir\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die klaren \u00c4ugelein?", "tokens": ["die", "kla\u00b7ren", "\u00c4u\u00b7ge\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wei\u00dfen Z\u00e4hne blecken,", "tokens": ["Die", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7ne", "ble\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der Wangen Milch und Blut", "tokens": ["der", "Wan\u00b7gen", "Milch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "verf\u00e4rbt die sch\u00f6ne Flut", "tokens": ["ver\u00b7f\u00e4rbt", "die", "sch\u00f6\u00b7ne", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit toten Flecken.", "tokens": ["mit", "to\u00b7ten", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du lustigs Miethaus, du,", "tokens": ["Du", "lus\u00b7tigs", "Miet\u00b7haus", ",", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Leib, lege dich zur Ruh'!", "tokens": ["Leib", ",", "le\u00b7ge", "dich", "zur", "Ruh'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dein hat man satt gepflogen.", "tokens": ["Dein", "hat", "man", "satt", "ge\u00b7pflo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Geist, dein werter Gast,", "tokens": ["Dein", "Geist", ",", "dein", "wer\u00b7ter", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "den du bewirtet hast,", "tokens": ["den", "du", "be\u00b7wir\u00b7tet", "hast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ist ausgezogen.", "tokens": ["ist", "aus\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Er schiffte Himmel an;", "tokens": ["Er", "schiff\u00b7te", "Him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Glaube war sein Kahn,", "tokens": ["der", "Glau\u00b7be", "war", "sein", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die W\u00fcndsche Botsgesellen,", "tokens": ["die", "W\u00fcnd\u00b7sche", "Bots\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Ruder, Segel, Mast", "tokens": ["die", "Ru\u00b7der", ",", "Se\u00b7gel", ",", "Mast"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "sein Beten, er die Last,", "tokens": ["sein", "Be\u00b7ten", ",", "er", "die", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die Tr\u00e4nen Wellen.", "tokens": ["die", "Tr\u00e4\u00b7nen", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die Lieb' ist sein Kompa\u00df,", "tokens": ["Die", "Lieb'", "ist", "sein", "Kom\u00b7pa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die Gegenliebe das,", "tokens": ["die", "Ge\u00b7gen\u00b7lie\u00b7be", "das", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "was Helene bei Nachte,", "tokens": ["was", "He\u00b7le\u00b7ne", "bei", "Nach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "die Seufzer guter Wind.", "tokens": ["die", "Seuf\u00b7zer", "gu\u00b7ter", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So segelt' er geschwind,", "tokens": ["So", "se\u00b7gelt'", "er", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "wohin er dachte.", "tokens": ["wo\u00b7hin", "er", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Des Ankers darf er nicht,", "tokens": ["Des", "An\u00b7kers", "darf", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der ihm zwar nicht gebricht.", "tokens": ["der", "ihm", "zwar", "nicht", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nichts macht den Himmel tr\u00fcbe.", "tokens": ["Nichts", "macht", "den", "Him\u00b7mel", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Br\u00e4utgam geht zum Furt'", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "geht", "zum", "Furt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und wartet an dem Port'", "tokens": ["und", "war\u00b7tet", "an", "dem", "Port'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "auf seine Liebe.", "tokens": ["auf", "sei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Steig aus, du herzer Gast,", "tokens": ["Steig", "aus", ",", "du", "her\u00b7zer", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "spricht er, und sei umfa\u00dft", "tokens": ["spricht", "er", ",", "und", "sei", "um\u00b7fa\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VAFIN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "von deines Buhlen Armen!", "tokens": ["von", "dei\u00b7nes", "Buh\u00b7len", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, Seele, keusche Braut!", "tokens": ["Komm", ",", "See\u00b7le", ",", "keu\u00b7sche", "Braut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich hab ich mir vertraut", "tokens": ["Dich", "hab", "ich", "mir", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "durch mein Erbarmen.", "tokens": ["durch", "mein", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Dein Malschatz bin selbst ich,", "tokens": ["Dein", "Mal\u00b7schatz", "bin", "selbst", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "du meiner. Meine mich,", "tokens": ["du", "mei\u00b7ner", ".", "Mei\u00b7ne", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "$.", "PPOSAT", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wie ich dich herzlich meine!", "tokens": ["wie", "ich", "dich", "herz\u00b7lich", "mei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So solstu ewig sein", "tokens": ["So", "sols\u00b7tu", "e\u00b7wig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "was ich bin. Du bist mein',", "tokens": ["was", "ich", "bin", ".", "Du", "bist", "mein'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ich allzeit deine.", "tokens": ["ich", "all\u00b7zeit", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Dein Zierrat und Geschmeid'", "tokens": ["Dein", "Zier\u00b7rat", "und", "Ge\u00b7schmeid'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ist Zucht und Ehrligkeit,", "tokens": ["ist", "Zucht", "und", "Ehr\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Keuschheit deine Krone,", "tokens": ["die", "Keuschheit", "dei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Scham dein Ehrenkranz:", "tokens": ["die", "Scham", "dein", "Eh\u00b7ren\u00b7kranz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "so gehst du an den Tanz", "tokens": ["so", "gehst", "du", "an", "den", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit Gottes Sohne.", "tokens": ["mit", "Got\u00b7tes", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Der Schauplatz ist ein Feld", "tokens": ["Der", "Schau\u00b7platz", "ist", "ein", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der g\u00fcldnen Sternenwelt.", "tokens": ["der", "g\u00fcld\u00b7nen", "Ster\u00b7nen\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lichten Cherubinen", "tokens": ["Die", "lich\u00b7ten", "Che\u00b7ru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "sind fertig zu den Rei'n;", "tokens": ["sind", "fer\u00b7tig", "zu", "den", "Rei'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hingegen\u00fcber schrein", "tokens": ["hin\u00b7ge\u00b7gen\u00b7\u00fc\u00b7ber", "schrein"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die Seraphinen.", "tokens": ["die", "Se\u00b7ra\u00b7phi\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Das Brautmal ist bereit:", "tokens": ["Das", "Braut\u00b7mal", "ist", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die Speis' ist Ewigkeit,", "tokens": ["die", "Speis'", "ist", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Trachten lauter Leben,", "tokens": ["die", "Trach\u00b7ten", "lau\u00b7ter", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der Trank ein stetes Nun.", "tokens": ["der", "Trank", "ein", "ste\u00b7tes", "Nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit solchem lieben Tun", "tokens": ["Mit", "sol\u00b7chem", "lie\u00b7ben", "Tun"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "bist du umgeben.", "tokens": ["bist", "du", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "So laufen um den Saal", "tokens": ["So", "lau\u00b7fen", "um", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die Engel ohne Zahl", "tokens": ["die", "En\u00b7gel", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und r\u00fcsten auf ein Lager.", "tokens": ["und", "r\u00fcs\u00b7ten", "auf", "ein", "La\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Vater nennt ihn schon,", "tokens": ["Dein", "Va\u00b7ter", "nennt", "ihn", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "wie auch die Mutter, Sohn,", "tokens": ["wie", "auch", "die", "Mut\u00b7ter", ",", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die Schwester Schwager.", "tokens": ["die", "Schwes\u00b7ter", "Schwa\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Wol, keusche Seele, dir!", "tokens": ["Wol", ",", "keu\u00b7sche", "See\u00b7le", ",", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wo bleiben aber wir?", "tokens": ["Wo", "blei\u00b7ben", "a\u00b7ber", "wir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Furcht wacht an allen Enden,", "tokens": ["Furcht", "wacht", "an", "al\u00b7len", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dortnaus ist Krieg und Not,", "tokens": ["dort\u00b7naus", "ist", "Krieg", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hierinnen Pest und Tod.", "tokens": ["hie\u00b7rin\u00b7nen", "Pest", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wer wird di\u00df wenden?", "tokens": ["Wer", "wird", "di\u00df", "wen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Sprich deinen Br\u00e4utgam an!", "tokens": ["Sprich", "dei\u00b7nen", "Br\u00e4ut\u00b7gam", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er ists, der retten kan,", "tokens": ["Er", "ists", ",", "der", "ret\u00b7ten", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "er wird sich noch erbarmen,", "tokens": ["er", "wird", "sich", "noch", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "er ists, der H\u00fclfe hat.", "tokens": ["er", "ists", ",", "der", "H\u00fcl\u00b7fe", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns mangelt Rat und Tat,", "tokens": ["Uns", "man\u00b7gelt", "Rat", "und", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "uns armen Armen!", "tokens": ["uns", "ar\u00b7men", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di