{"textgrid.poem.63500": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "[dat s\u00fcnd de Wile-G\u00f6schen]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dat s\u00fcnd de Wile-G\u00f6schen,", "tokens": ["Dat", "s\u00fcnd", "de", "Wi\u00b7le\u00b7G\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "de hewwt all rode H\u00f6schen,", "tokens": ["de", "hewwt", "all", "ro\u00b7de", "H\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "grag Kittels an un witte Jop,", "tokens": ["grag", "Kit\u00b7tels", "an", "un", "wit\u00b7te", "Jop", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "FM", "VVFIN", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "de praten di wat Sch\u00f6ns tohop.", "tokens": ["de", "pra\u00b7ten", "di", "wat", "Sch\u00f6ns", "to\u00b7hop", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ehr Schnabels s\u00fcnd Trumpeten,", "tokens": ["Ehr", "Schna\u00b7bels", "s\u00fcnd", "Trum\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dor don se all mit st\u00f6ten.", "tokens": ["dor", "don", "se", "all", "mit", "st\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Se snatern all in eenschen f\u00fcrt", "tokens": ["Se", "sna\u00b7tern", "all", "in", "e\u00b7en\u00b7schen", "f\u00fcrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "un hewwt een grausam grotes Wurt.", "tokens": ["un", "hewwt", "e\u00b7en", "grau\u00b7sam", "gro\u00b7tes", "Wurt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Se kawweln sick un schelln.", "tokens": ["Se", "kaw\u00b7weln", "sick", "un", "schelln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wen schall dat meist woll gelln...", "tokens": ["Wen", "schall", "dat", "meist", "woll", "gelln", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADV", "ADV", "VVINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}