{"dta.poem.10542": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Berge steht die H\u00fctte,", "tokens": ["Auf", "dem", "Ber\u00b7ge", "steht", "die", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der alte Bergmann wohnt;", "tokens": ["Wo", "der", "al\u00b7te", "Berg\u00b7mann", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorten rauscht die gr\u00fcne Tanne,", "tokens": ["Dor\u00b7ten", "rauscht", "die", "gr\u00fc\u00b7ne", "Tan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ergl\u00e4nzt der gold'ne Mond.", "tokens": ["Und", "er\u00b7gl\u00e4nzt", "der", "gold'\u00b7ne", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der H\u00fctte steht ein Lehnstuhl,", "tokens": ["In", "der", "H\u00fct\u00b7te", "steht", "ein", "Lehn\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich geschnitzt und wunderlich,", "tokens": ["Reich", "ge\u00b7schnitzt", "und", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der darauf sitzt, der ist gl\u00fccklich,", "tokens": ["Der", "da\u00b7rauf", "sitzt", ",", "der", "ist", "gl\u00fcck\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "VVFIN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und der Gl\u00fcckliche bin Ich!", "tokens": ["Und", "der", "Gl\u00fcck\u00b7li\u00b7che", "bin", "Ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf dem Schemel sitzt die Kleine,", "tokens": ["Auf", "dem", "Sche\u00b7mel", "sitzt", "die", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fctzt den Arm auf meinen Schoo\u00df;", "tokens": ["St\u00fctzt", "den", "Arm", "auf", "mei\u00b7nen", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aeuglein wie zwei blaue Sterne,", "tokens": ["A\u00b7e\u00b7u\u00b7glein", "wie", "zwei", "blau\u00b7e", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "CARD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "M\u00fcndlein wie die Purpurros'.", "tokens": ["M\u00fcnd\u00b7lein", "wie", "die", "Pur\u00b7pur\u00b7ros'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die lieben, blauen Sterne", "tokens": ["Und", "die", "lie\u00b7ben", ",", "blau\u00b7en", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau'n mich an so himmelgro\u00df,", "tokens": ["Schau'n", "mich", "an", "so", "him\u00b7mel\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie legt den Lilienfinger", "tokens": ["Und", "sie", "legt", "den", "Li\u00b7li\u00b7en\u00b7fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schalkhaft auf die Purpurros'.", "tokens": ["Schalk\u00b7haft", "auf", "die", "Pur\u00b7pur\u00b7ros'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein, es sieht uns nicht die Mutter,", "tokens": ["Nein", ",", "es", "sieht", "uns", "nicht", "die", "Mut\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sie spinnt mit gro\u00dfem Flei\u00df,", "tokens": ["Denn", "sie", "spinnt", "mit", "gro\u00b7\u00dfem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Vater spielt die Zitter,", "tokens": ["Und", "der", "Va\u00b7ter", "spielt", "die", "Zit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er singt die alte Weis'.", "tokens": ["Und", "er", "singt", "die", "al\u00b7te", "Weis'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die Kleine fl\u00fcstert leise,", "tokens": ["Und", "die", "Klei\u00b7ne", "fl\u00fcs\u00b7tert", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise, mit ged\u00e4mpftem Laut;", "tokens": ["Lei\u00b7se", ",", "mit", "ge\u00b7d\u00e4mpf\u00b7tem", "Laut", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches wichtige Geheimni\u00df", "tokens": ["Man\u00b7ches", "wich\u00b7ti\u00b7ge", "Ge\u00b7heim\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sie mir schon ", "tokens": ["Hat", "sie", "mir", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "\u201eaber seit die Muhme todt ist,", "tokens": ["\u201e", "a\u00b7ber", "seit", "die", "Muh\u00b7me", "todt", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "K\u00f6nnen wir ja nicht mehr geh'n", "tokens": ["K\u00f6n\u00b7nen", "wir", "ja", "nicht", "mehr", "geh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Sch\u00fctzenhof zu Goslar,", "tokens": ["Nach", "dem", "Sch\u00fct\u00b7zen\u00b7hof", "zu", "Gos\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dort ist es gar zu sch\u00f6n.", "tokens": ["Und", "dort", "ist", "es", "gar", "zu", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "\u201ehier dagegen ist es einsam,", "tokens": ["\u201e", "hier", "da\u00b7ge\u00b7gen", "ist", "es", "ein\u00b7sam", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der kalten Bergesh\u00f6h',", "tokens": ["Auf", "der", "kal\u00b7ten", "Ber\u00b7ges\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Winters sind wir g\u00e4nzlich", "tokens": ["Und", "des", "Win\u00b7ters", "sind", "wir", "g\u00e4nz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wie vergraben in dem Schnee.", "tokens": ["Wie", "ver\u00b7gra\u00b7ben", "in", "dem", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201eund ich bin ein banges M\u00e4dchen,", "tokens": ["\u201e", "und", "ich", "bin", "ein", "ban\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich f\u00fcrcht' mich wie ein Kind", "tokens": ["Und", "ich", "f\u00fcrcht'", "mich", "wie", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor den b\u00f6sen Bergesgeistern,", "tokens": ["Vor", "den", "b\u00f6\u00b7sen", "Ber\u00b7ges\u00b7geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die des Nachts gesch\u00e4ftig sind.\u201c", "tokens": ["Die", "des", "Nachts", "ge\u00b7sch\u00e4f\u00b7tig", "sind", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "ADV", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Pl\u00f6tzlich schweigt die liebe Kleine,", "tokens": ["Pl\u00f6tz\u00b7lich", "schweigt", "die", "lie\u00b7be", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie vom eignen Wort erschreckt,", "tokens": ["Wie", "vom", "eig\u00b7nen", "Wort", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie hat mit beiden H\u00e4ndchen", "tokens": ["Und", "sie", "hat", "mit", "bei\u00b7den", "H\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Aeugelein bedeckt.", "tokens": ["Ih\u00b7re", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Lauter rauscht die Tanne drau\u00dfen,", "tokens": ["Lau\u00b7ter", "rauscht", "die", "Tan\u00b7ne", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Spinnrad schnarrt und brummt,", "tokens": ["Und", "das", "Spinn\u00b7rad", "schnarrt", "und", "brummt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Zither klingt dazwischen,", "tokens": ["Und", "die", "Zi\u00b7ther", "klingt", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die alte Weise summt:", "tokens": ["Und", "die", "al\u00b7te", "Wei\u00b7se", "summt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "F\u00fcrcht' dich nicht, du liebes Kindchen,", "tokens": ["F\u00fcrcht'", "dich", "nicht", ",", "du", "lie\u00b7bes", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der b\u00f6sen Geister Macht;", "tokens": ["Vor", "der", "b\u00f6\u00b7sen", "Geis\u00b7ter", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tag und Nacht, du liebes Kindchen,", "tokens": ["Tag", "und", "Nacht", ",", "du", "lie\u00b7bes", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halten Englein bei dir Wacht!\u201c", "tokens": ["Hal\u00b7ten", "En\u00b7glein", "bei", "dir", "Wacht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "APPR", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}