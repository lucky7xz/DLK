{"dta.poem.7515": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn es dir \u00fcbel geht, nimm es f\u00fcr gut nur immer;", "tokens": ["Wenn", "es", "dir", "\u00fc\u00b7bel", "geht", ",", "nimm", "es", "f\u00fcr", "gut", "nur", "im\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VVIMP", "PPER", "APPR", "ADJD", "ADV", "ADV", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn du es \u00fcbel nimmst, so geht es dir noch schlimmer.", "tokens": ["Wenn", "du", "es", "\u00fc\u00b7bel", "nimmst", ",", "so", "geht", "es", "dir", "noch", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn der Freund dich kr\u00e4nkt, verzeih's ihm, und versteh:", "tokens": ["Und", "wenn", "der", "Freund", "dich", "kr\u00e4nkt", ",", "ver\u00b7zeih's", "ihm", ",", "und", "ver\u00b7steh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ihm selbst nicht wohl, sonst th\u00e4t' er dir nicht weh.", "tokens": ["Es", "ist", "ihm", "selbst", "nicht", "wohl", ",", "sonst", "th\u00e4t'", "er", "dir", "nicht", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und kr\u00e4nkt die Liebe dich, sei dir's zur Lieb' ein Sporn;", "tokens": ["Und", "kr\u00e4nkt", "die", "Lie\u00b7be", "dich", ",", "sei", "dir's", "zur", "Lieb'", "ein", "Sporn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "VAFIN", "PIS", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du die Rose hast, das merkst du erst am Dorn.", "tokens": ["Da\u00df", "du", "die", "Ro\u00b7se", "hast", ",", "das", "merkst", "du", "erst", "am", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}