{"dta.poem.3787": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nste aus der weissen schwestern zug", "tokens": ["Die", "sch\u00f6ns\u00b7te", "aus", "der", "weis\u00b7sen", "schwes\u00b7tern", "zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie f\u00fcrstenmantel hing die schlichte wolle", "tokens": ["Wie", "f\u00fcrs\u00b7ten\u00b7man\u00b7tel", "hing", "die", "schlich\u00b7te", "wol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um ihres nackens ihrer schulter bug", "tokens": ["Um", "ih\u00b7res", "na\u00b7ckens", "ih\u00b7rer", "schul\u00b7ter", "bug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im schauspiel dann als sich die opfer mehrten", "tokens": ["Im", "schau\u00b7spiel", "dann", "als", "sich", "die", "op\u00b7fer", "mehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVFIN", "ADV", "KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und z\u00fcgellos die menge beifall rief", "tokens": ["Und", "z\u00fc\u00b7gel\u00b7los", "die", "men\u00b7ge", "bei\u00b7fall", "rief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die todberufenen den c\u00e4sar ehrten:", "tokens": ["Die", "tod\u00b7be\u00b7ru\u00b7fe\u00b7nen", "den", "c\u00e4\u00b7sar", "ehr\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ihr auge blieb gelassen streng und tief", "tokens": ["Ihr", "au\u00b7ge", "blieb", "ge\u00b7las\u00b7sen", "streng", "und", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich der kurzen werbung rausch bedenke!", "tokens": ["Wenn", "ich", "der", "kur\u00b7zen", "wer\u00b7bung", "rausch", "be\u00b7den\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich riss die priesterin von dem altar", "tokens": ["Ich", "riss", "die", "pries\u00b7te\u00b7rin", "von", "dem", "al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alle l\u00e4nder brachten brautgeschenke", "tokens": ["Und", "al\u00b7le", "l\u00e4n\u00b7der", "brach\u00b7ten", "braut\u00b7ge\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bot in b\u00e4chen gold und balsam dar", "tokens": ["Ich", "bot", "in", "b\u00e4\u00b7chen", "gold", "und", "bal\u00b7sam", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und zweiflend ob das neue gl\u00fcck mir werde", "tokens": ["Und", "zwei\u00b7flend", "ob", "das", "neu\u00b7e", "gl\u00fcck", "mir", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOUS", "ART", "ADJA", "NN", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erfand ich nur den quell der neuen qual...", "tokens": ["Er\u00b7fand", "ich", "nur", "den", "quell", "der", "neu\u00b7en", "qual", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sandte sie zur\u00fcck zu ihrem herde", "tokens": ["Ich", "sand\u00b7te", "sie", "zu\u00b7r\u00fcck", "zu", "ih\u00b7rem", "her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie hatte wie die anderen ein mal", "tokens": ["Sie", "hat\u00b7te", "wie", "die", "an\u00b7de\u00b7ren", "ein", "mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "ART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}