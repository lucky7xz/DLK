{"textgrid.poem.57087": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging einmal des abends, den du kennst, den Weg,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging einmal des abends, den du kennst, den Weg,", "tokens": ["Ich", "ging", "ein\u00b7mal", "des", "a\u00b7bends", ",", "den", "du", "kennst", ",", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit einem Freund, der mir von seinen Pl\u00e4nen sprach.", "tokens": ["mit", "ei\u00b7nem", "Freund", ",", "der", "mir", "von", "sei\u00b7nen", "Pl\u00e4\u00b7nen", "sprach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da ward mir seltsam: Wie ich schweigend neben ihm", "tokens": ["Da", "ward", "mir", "selt\u00b7sam", ":", "Wie", "ich", "schwei\u00b7gend", "ne\u00b7ben", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PWAV", "PPER", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und halb ihm lauschend ging im D\u00e4mmerlicht, geschah's,", "tokens": ["und", "halb", "ihm", "lau\u00b7schend", "ging", "im", "D\u00e4m\u00b7mer\u00b7licht", ",", "ge\u00b7scha\u00b7h's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da\u00df ich mich selbst als Dich empfand, als gingest Du", "tokens": ["da\u00df", "ich", "mich", "selbst", "als", "Dich", "emp\u00b7fand", ",", "als", "gin\u00b7gest", "Du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "in mir und lauschtest, wie ich seinem, meinem Wort ...", "tokens": ["in", "mir", "und", "lauschtest", ",", "wie", "ich", "sei\u00b7nem", ",", "mei\u00b7nem", "Wort", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und leise nickt' und murmelt' ich ihm zu,", "tokens": ["Und", "lei\u00b7se", "nickt'", "und", "mur\u00b7melt'", "ich", "ihm", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "mein Augenaufschlag war der Deine, Dein mein Leib", "tokens": ["mein", "Au\u00b7gen\u00b7auf\u00b7schlag", "war", "der", "Dei\u00b7ne", ",", "Dein", "mein", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "PPOSAT", "$,", "PPOSAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "in jeglicher Bewegung bis ins Innerste ...", "tokens": ["in", "jeg\u00b7li\u00b7cher", "Be\u00b7we\u00b7gung", "bis", "ins", "In\u00b7ners\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.10": {"text": "Und deine scheue Jungfraunseele liebte mich aus mir ...", "tokens": ["Und", "dei\u00b7ne", "scheu\u00b7e", "Jung\u00b7fraun\u00b7see\u00b7le", "lieb\u00b7te", "mich", "aus", "mir", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}