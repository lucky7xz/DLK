{"textgrid.poem.25697": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Anakreons Erbschaft", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollt' Anakreon itzt sterben,", "tokens": ["Sollt'", "A\u00b7nak\u00b7re\u00b7on", "itzt", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wir w\u00e4ren seine Erben:", "tokens": ["Und", "wir", "w\u00e4\u00b7ren", "sei\u00b7ne", "Er\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcge seine Leyer billig", "tokens": ["Tr\u00fc\u00b7ge", "sei\u00b7ne", "Le\u00b7yer", "bil\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor uns \u00fcbrigen davon.", "tokens": ["Vor", "uns", "\u00fcb\u00b7ri\u00b7gen", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Gabe zum Beneiden:", "tokens": ["Sei\u00b7ne", "Ga\u00b7be", "zum", "Be\u00b7nei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lebensweisheit einzukleiden", "tokens": ["Le\u00b7bens\u00b7weis\u00b7heit", "ein\u00b7zu\u00b7klei\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Spiele leichte Tracht,", "tokens": ["In", "der", "Spie\u00b7le", "leich\u00b7te", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat zu seinem Eigenthume", "tokens": ["Hat", "zu", "sei\u00b7nem", "Ei\u00b7gen\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seine Kunst, vergn\u00fcgt zu scherzen,", "tokens": ["Sei\u00b7ne", "Kunst", ",", "ver\u00b7gn\u00fcgt", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aller Frauenzimmer-Herzen", "tokens": ["Al\u00b7ler", "Frau\u00b7en\u00b7zim\u00b7mer\u00b7Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sichre Ueberwinderin!", "tokens": ["Sich\u00b7re", "Ue\u00b7ber\u00b7win\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "N\u00e4hme wohl mit Fug und Rechte", "tokens": ["N\u00e4h\u00b7me", "wohl", "mit", "Fug", "und", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Unser Freund ", "tokens": ["Un\u00b7ser", "Freund"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Sein geheimes Schmachten, Sehnen", "tokens": ["Sein", "ge\u00b7hei\u00b7mes", "Schmach\u00b7ten", ",", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und Frohlocken, erst der Sch\u00f6nen", "tokens": ["Und", "Froh\u00b7lo\u00b7cken", ",", "erst", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ADV", "ART", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Hartes Herz, durch Amors Pfeil", "tokens": ["Har\u00b7tes", "Herz", ",", "durch", "A\u00b7mors", "Pfeil"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Zu verwunden, dann zu heilen,", "tokens": ["Zu", "ver\u00b7wun\u00b7den", ",", "dann", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "W\u00fcrde dir, o ", "tokens": ["W\u00fcr\u00b7de", "dir", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "FM"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Wein verlie\u00df er nicht, der Zecher!", "tokens": ["Wein", "ver\u00b7lie\u00df", "er", "nicht", ",", "der", "Ze\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Aber seine Kr\u00e4nz' und Becher,", "tokens": ["A\u00b7ber", "sei\u00b7ne", "Kr\u00e4nz'", "und", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und den sorgenlosen Sinn", "tokens": ["Und", "den", "sor\u00b7gen\u00b7lo\u00b7sen", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Ihres vorigen Besitzers,", "tokens": ["Ih\u00b7res", "vo\u00b7ri\u00b7gen", "Be\u00b7sit\u00b7zers", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "N\u00e4hmest du, ", "tokens": ["N\u00e4h\u00b7mest", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Alles wollt' ich gern Euch g\u00f6nnen,", "tokens": ["Al\u00b7les", "wollt'", "ich", "gern", "Euch", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "M\u00f6cht' ich Eins nur erben k\u00f6nnen: \u2013", "tokens": ["M\u00f6cht'", "ich", "Eins", "nur", "er\u00b7ben", "k\u00f6n\u00b7nen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADV", "VVINF", "VMINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Seine Taube w\u00fcnscht' ich mir!", "tokens": ["Sei\u00b7ne", "Tau\u00b7be", "w\u00fcnscht'", "ich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Ach! mich liebt ein holdes M\u00e4dchen,", "tokens": ["Ach", "!", "mich", "liebt", "ein", "hol\u00b7des", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Aber weit ist sie von hier.", "tokens": ["A\u00b7ber", "weit", "ist", "sie", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und das M\u00e4dchen liebt die Tauben,", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "liebt", "die", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Aber diese, sollt' ich glauben,", "tokens": ["A\u00b7ber", "die\u00b7se", ",", "sollt'", "ich", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Liebte sie vor allen wohl,", "tokens": ["Lieb\u00b7te", "sie", "vor", "al\u00b7len", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Wenn sie k\u00e4me, beide Krallen", "tokens": ["Wenn", "sie", "k\u00e4\u00b7me", ",", "bei\u00b7de", "Kral\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Von des Senders Briefen voll!", "tokens": ["Von", "des", "Sen\u00b7ders", "Brie\u00b7fen", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}