{"dta.poem.8202": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Flohn die St\u00f6rche \u00fcber's Meer;", "tokens": ["Flohn", "die", "St\u00f6r\u00b7che", "\u00fc\u00b7ber's", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwalbenflug ist l\u00e4ngst geschieden,", "tokens": ["Schwal\u00b7ben\u00b7flug", "ist", "l\u00e4ngst", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Lerche singt nicht mehr.", "tokens": ["Auch", "die", "Ler\u00b7che", "singt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seufzend in geheimer Klage", "tokens": ["Seuf\u00b7zend", "in", "ge\u00b7hei\u00b7mer", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streift der Wind das letzte Gr\u00fcn;", "tokens": ["Streift", "der", "Wind", "das", "letz\u00b7te", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die s\u00fc\u00dfen Sommertage", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfen", "Som\u00b7mer\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach sie sind dahin, dahin!", "tokens": ["Ach", "sie", "sind", "da\u00b7hin", ",", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "PAV", "$,", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nebel hat den Wald verschlungen,", "tokens": ["Ne\u00b7bel", "hat", "den", "Wald", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dein stillstes Gl\u00fcck gesehn;", "tokens": ["Der", "dein", "stills\u00b7tes", "Gl\u00fcck", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz in Duft und D\u00e4mmerungen", "tokens": ["Ganz", "in", "Duft", "und", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will die sch\u00f6ne Welt vergehn.", "tokens": ["Will", "die", "sch\u00f6\u00b7ne", "Welt", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur noch einmal bricht die Sonne", "tokens": ["Nur", "noch", "ein\u00b7mal", "bricht", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unaufhaltsam durch den Duft,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "durch", "den", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Strahl der alten Wonne", "tokens": ["Und", "ein", "Strahl", "der", "al\u00b7ten", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rieselt \u00fcber Thal und Kluft.", "tokens": ["Rie\u00b7selt", "\u00fc\u00b7ber", "Thal", "und", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und es leuchten Wald und Haide,", "tokens": ["Und", "es", "leuch\u00b7ten", "Wald", "und", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man sicher glauben mag,", "tokens": ["Da\u00df", "man", "si\u00b7cher", "glau\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter allem Winterleide", "tokens": ["Hin\u00b7ter", "al\u00b7lem", "Win\u00b7ter\u00b7lei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieg' ein ferner Fr\u00fchlingstag.", "tokens": ["Lieg'", "ein", "fer\u00b7ner", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}