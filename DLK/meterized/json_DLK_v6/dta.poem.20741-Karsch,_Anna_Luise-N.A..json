{"dta.poem.20741": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was seh' ich? Friedrichs stark erk\u00e4mpfte Siege,                 ", "tokens": ["Was", "seh'", "ich", "?", "Fried\u00b7richs", "stark", "er\u00b7k\u00e4mpf\u00b7te", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gezeichnet bei Planetenlauf,", "tokens": ["Ge\u00b7zeich\u00b7net", "bei", "Pla\u00b7ne\u00b7ten\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und ihn, den Helden, der die Lorbern seiner Kriege", "tokens": ["Und", "ihn", ",", "den", "Hel\u00b7den", ",", "der", "die", "Lor\u00b7bern", "sei\u00b7ner", "Krie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Ph\u00f6bus opfert auf?", "tokens": ["Dem", "Ph\u00f6\u00b7bus", "op\u00b7fert", "auf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun wuchert Er mit Stunden, mit Minuten,", "tokens": ["Nun", "wu\u00b7chert", "Er", "mit", "Stun\u00b7den", ",", "mit", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn seine Weisheit f\u00fcr uns wacht,", "tokens": ["Wenn", "sei\u00b7ne", "Weis\u00b7heit", "f\u00fcr", "uns", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun wird nicht mehr sein Herz um seine L\u00e4nder bluten,", "tokens": ["Nun", "wird", "nicht", "mehr", "sein", "Herz", "um", "sei\u00b7ne", "L\u00e4n\u00b7der", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Befreit von Feindes Macht.", "tokens": ["Be\u00b7freit", "von", "Fein\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er z\u00fcrnet auf den Stoff zu Iliaden,", "tokens": ["Er", "z\u00fcr\u00b7net", "auf", "den", "Stoff", "zu", "I\u00b7lia\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und seine Menschenfreundschaft hei\u00dft", "tokens": ["Und", "sei\u00b7ne", "Men\u00b7schen\u00b7freund\u00b7schaft", "hei\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tyrann den Kriegesgott, der einer Welt zum Schaden", "tokens": ["Ty\u00b7rann", "den", "Krie\u00b7ges\u00b7gott", ",", "der", "ei\u00b7ner", "Welt", "zum", "Scha\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich wundersam beweis\u2019t.", "tokens": ["Sich", "wun\u00b7der\u00b7sam", "be\u00b7weis'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PRF", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gl\u00fcckseligkeit wird seinen Thron erh\u00f6hen,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "wird", "sei\u00b7nen", "Thron", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Er blickt herunter, und verschlie\u00dft", "tokens": ["Er", "blickt", "her\u00b7un\u00b7ter", ",", "und", "ver\u00b7schlie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verborgner Feinde Mund, die ganz verstummt gestehen,", "tokens": ["Ver\u00b7borg\u00b7ner", "Fein\u00b7de", "Mund", ",", "die", "ganz", "ver\u00b7stummt", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Friedrich furchtbar ist.", "tokens": ["Da\u00df", "Fried\u00b7rich", "furcht\u00b7bar", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vergeblich seinem Leben nachge-trachtet", "tokens": ["Ver\u00b7geb\u00b7lich", "sei\u00b7nem", "Le\u00b7ben", "nach\u00b7ge\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward von der Schlacht, die um sich fra\u00df,", "tokens": ["Ward", "von", "der", "Schlacht", ",", "die", "um", "sich", "fra\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Er, ganz Feldherr und ganz Held, nur uns geachtet", "tokens": ["Wenn", "Er", ",", "ganz", "Feld\u00b7herr", "und", "ganz", "Held", ",", "nur", "uns", "ge\u00b7ach\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "KON", "ADV", "NN", "$,", "ADV", "PPER", "VVPP"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und fechtend sich verga\u00df.", "tokens": ["Und", "fech\u00b7tend", "sich", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er wird uns neu gegeben von den G\u00f6ttern", "tokens": ["Er", "wird", "uns", "neu", "ge\u00b7ge\u00b7ben", "von", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Lobgesang ist unsre Pflicht,", "tokens": ["Und", "Lob\u00b7ge\u00b7sang", "ist", "uns\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Roms und Griechenlands triumphischen Errettern", "tokens": ["In", "Roms", "und", "Grie\u00b7chen\u00b7lands", "tri\u00b7um\u00b7phi\u00b7schen", "Er\u00b7ret\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Find\u2019 ich Sein Urbild nicht.", "tokens": ["Find'", "ich", "Sein", "Ur\u00b7bild", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}