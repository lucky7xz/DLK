{"textgrid.poem.59969": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Avis in betreff etwaiger Druckfehler", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll von Fehlern ist dies Buch;", "tokens": ["Voll", "von", "Feh\u00b7lern", "ist", "dies", "Buch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VAFIN", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit steht auf jeder Seite;", "tokens": ["Frei\u00b7heit", "steht", "auf", "je\u00b7der", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleichviel \u2013 gebt ihm euern Fluch", "tokens": ["Gleich\u00b7viel", "\u2013", "gebt", "ihm", "eu\u00b7ern", "Fluch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Segen zum Geleite!", "tokens": ["O\u00b7der", "Se\u00b7gen", "zum", "Ge\u00b7lei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr das S\u00fcndenregister", "tokens": ["F\u00fcr", "das", "S\u00fcn\u00b7den\u00b7re\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Sorge der deutsche Philister.", "tokens": ["Sor\u00b7ge", "der", "deut\u00b7sche", "Phi\u00b7lis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}