{"textgrid.poem.58415": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "453. Der abgeschiedene Kreuztr\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geb' den \u00e4u\u00dfern Menschen hin", "tokens": ["Ich", "geb'", "den", "\u00e4u\u00b7\u00dfern", "Men\u00b7schen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den bittern Kreuzesschmerzen,", "tokens": ["Den", "bit\u00b7tern", "Kreu\u00b7zes\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es liegt der abgeschiedne Sinn", "tokens": ["Es", "liegt", "der", "ab\u00b7ge\u00b7schied\u00b7ne", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz still in Gottes Herzen;", "tokens": ["Ganz", "still", "in", "Got\u00b7tes", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie stark und selig ist der Mann,", "tokens": ["Wie", "stark", "und", "se\u00b7lig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der so sein Leiden tragen kann!", "tokens": ["Der", "so", "sein", "Lei\u00b7den", "tra\u00b7gen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}