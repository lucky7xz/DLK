{"textgrid.poem.67057": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Vollendung", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vollendung! Vollendung! \u2013", "tokens": ["Vol\u00b7len\u00b7dung", "!", "Vol\u00b7len\u00b7dung", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$.", "NN", "$.", "$("], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "O du der Geister heiliges Ziel!", "tokens": ["O", "du", "der", "Geis\u00b7ter", "hei\u00b7li\u00b7ges", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wann werd ich siegestrunken", "tokens": ["Wann", "werd", "ich", "sie\u00b7ge\u00b7strun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich umfahen und ewig ruhn?", "tokens": ["Dich", "um\u00b7fa\u00b7hen", "und", "e\u00b7wig", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Und frei und gro\u00df", "tokens": ["Und", "frei", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Entgegenl\u00e4cheln der Heerschar,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7l\u00e4\u00b7cheln", "der", "Heer\u00b7schar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die zahllos aus den Welten", "tokens": ["Die", "zahl\u00b7los", "aus", "den", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In den Scho\u00df dir str\u00f6mt?", "tokens": ["In", "den", "Scho\u00df", "dir", "str\u00f6mt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ach ferne, ferne von dir!", "tokens": ["Ach", "fer\u00b7ne", ",", "fer\u00b7ne", "von", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mein g\u00f6ttlichster, sch\u00f6nster Gedanke", "tokens": ["Mein", "g\u00f6tt\u00b7lichs\u00b7ter", ",", "sch\u00f6ns\u00b7ter", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "War, wie der Welten", "tokens": ["War", ",", "wie", "der", "Wel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fernstes Ende, ferne von dir!", "tokens": ["Ferns\u00b7tes", "En\u00b7de", ",", "fer\u00b7ne", "von", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Und fleugt auf des Sturmes Fl\u00fcgeln", "tokens": ["Und", "fleugt", "auf", "des", "Stur\u00b7mes", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aeonen lang die Liebe dir zu,", "tokens": ["A\u00b7e\u00b7o\u00b7nen", "lang", "die", "Lie\u00b7be", "dir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Noch schmachtet sie ferne von dir,", "tokens": ["Noch", "schmach\u00b7tet", "sie", "fer\u00b7ne", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ach! ferne, ferne von dir!", "tokens": ["Ach", "!", "fer\u00b7ne", ",", "fer\u00b7ne", "von", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Doch k\u00fchner gewaltiger", "tokens": ["Doch", "k\u00fch\u00b7ner", "ge\u00b7wal\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Unaufhaltbarer immer", "tokens": ["Un\u00b7auf\u00b7halt\u00b7ba\u00b7rer", "im\u00b7mer"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fleugt durch Myriaden Aeonen", "tokens": ["Fleugt", "durch", "My\u00b7ria\u00b7den", "A\u00b7e\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir zu die gl\u00fchende Liebe.", "tokens": ["Dir", "zu", "die", "gl\u00fc\u00b7hen\u00b7de", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Voll hoher Einfalt,", "tokens": ["Voll", "ho\u00b7her", "Ein\u00b7falt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Einf\u00e4ltig still und gro\u00df", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "still", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rangen des Siegs gewi\u00df,", "tokens": ["Ran\u00b7gen", "des", "Siegs", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Rangen dir zu die V\u00e4ter.", "tokens": ["Ran\u00b7gen", "dir", "zu", "die", "V\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Ihre H\u00fclle verschlang die Zeit,", "tokens": ["Ih\u00b7re", "H\u00fcl\u00b7le", "ver\u00b7schlang", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Verwest, zerstreut ist der Staub,", "tokens": ["Ver\u00b7west", ",", "zer\u00b7streut", "ist", "der", "Staub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch rang des Sieges gewi\u00df", "tokens": ["Doch", "rang", "des", "Sie\u00b7ges", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der Funke Gottes, ihr Geist, dir zu.", "tokens": ["Der", "Fun\u00b7ke", "Got\u00b7tes", ",", "ihr", "Geist", ",", "dir", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sind sie eingegangen zu dir,", "tokens": ["Sind", "sie", "ein\u00b7ge\u00b7gan\u00b7gen", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die da lebten im Anbeginn?", "tokens": ["Die", "da", "leb\u00b7ten", "im", "An\u00b7be\u00b7ginn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ruhen, ruhen sie nun,", "tokens": ["Ru\u00b7hen", ",", "ru\u00b7hen", "sie", "nun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die frommen V\u00e4ter?", "tokens": ["Die", "from\u00b7men", "V\u00e4\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Vollendung! Vollendung!", "tokens": ["Vol\u00b7len\u00b7dung", "!", "Vol\u00b7len\u00b7dung", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Geister heiliges Ziel!", "tokens": ["Der", "Geis\u00b7ter", "hei\u00b7li\u00b7ges", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wann werd ich siegestrunken", "tokens": ["Wann", "werd", "ich", "sie\u00b7ge\u00b7strun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich umfahen und ewig ruhn?", "tokens": ["Dich", "um\u00b7fa\u00b7hen", "und", "e\u00b7wig", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}