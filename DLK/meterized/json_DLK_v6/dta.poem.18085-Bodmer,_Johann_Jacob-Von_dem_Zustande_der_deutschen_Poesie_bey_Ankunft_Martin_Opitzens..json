{"dta.poem.18085": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Zustande der deutschen  \n Poesie bey Ankunft Martin Opitzens.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Jupiter die Welt hat g\u00e4ntzlich ausgemachet,", "tokens": ["Als", "Ju\u00b7pi\u00b7ter", "die", "Welt", "hat", "g\u00e4ntz\u00b7lich", "aus\u00b7ge\u00b7ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf dem Erden-Krei\u00df schon alles gr\u00fcnt und lachet,", "tokens": ["Und", "auf", "dem", "Er\u00b7den\u00b7Krei\u00df", "schon", "al\u00b7les", "gr\u00fcnt", "und", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wand er sich dreymal um, und schauet hin und her,", "tokens": ["Wand", "er", "sich", "drey\u00b7mal", "um", ",", "und", "schau\u00b7et", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ob in dem grossen Hau\u00df irgend ein Mangel w\u00e4r.", "tokens": ["Ob", "in", "dem", "gros\u00b7sen", "Hau\u00df", "ir\u00b7gend", "ein", "Man\u00b7gel", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es fehlet noch ein Ding: Er lie\u00df ein Thier furkommen,", "tokens": ["Es", "feh\u00b7let", "noch", "ein", "Ding", ":", "Er", "lie\u00df", "ein", "Thier", "fur\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das nun fast \u00fcberall die Welt hat eingenommen,", "tokens": ["Das", "nun", "fast", "\u00fc\u00b7be\u00b7rall", "die", "Welt", "hat", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein artiges Gespenst, darnach ein jeder rennt,", "tokens": ["Ein", "ar\u00b7ti\u00b7ges", "Ge\u00b7spenst", ",", "dar\u00b7nach", "ein", "je\u00b7der", "rennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Welches in unserm Land ein Jungfrau wird genennt.", "tokens": ["Wel\u00b7ches", "in", "un\u00b7serm", "Land", "ein", "Jung\u00b7frau", "wird", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Ein Thier das um den Mund, vornemlich in der Zungen", "tokens": ["Ein", "Thier", "das", "um", "den", "Mund", ",", "vor\u00b7nem\u00b7lich", "in", "der", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Tr\u00e4gt ein verborgen Gifft, damit es Alt und Jungen", "tokens": ["Tr\u00e4gt", "ein", "ver\u00b7bor\u00b7gen", "Gifft", ",", "da\u00b7mit", "es", "Alt", "und", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Anstecket und verblendt, und mit eim s\u00fcssen Schmertz", "tokens": ["An\u00b7ste\u00b7cket", "und", "ver\u00b7blendt", ",", "und", "mit", "eim", "s\u00fcs\u00b7sen", "Schmertz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVPP", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kommt ungewarnter Sach gekrochen in das Hertz.", "tokens": ["Kommt", "un\u00b7ge\u00b7warn\u00b7ter", "Sach", "ge\u00b7kro\u00b7chen", "in", "das", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das uns je mehr nachzieht, je mehr wir von ihm fliehen,", "tokens": ["Das", "uns", "je", "mehr", "nach\u00b7zieht", ",", "je", "mehr", "wir", "von", "ihm", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADV", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und je mehr von uns flieht, je mehr wir ihm nachziehen.", "tokens": ["Und", "je", "mehr", "von", "uns", "flieht", ",", "je", "mehr", "wir", "ihm", "nach\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein freundliche Feindin, ein feindliche Freundin,", "tokens": ["Ein", "freund\u00b7li\u00b7che", "Fein\u00b7din", ",", "ein", "feind\u00b7li\u00b7che", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.16": {"text": "Die ohne Zauberey verzaubert unsern Sinn.", "tokens": ["Die", "oh\u00b7ne", "Zau\u00b7be\u00b7rey", "ver\u00b7zau\u00b7bert", "un\u00b7sern", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O wunderlich Gespenst, das uns ohn Feur entz\u00fcndet,", "tokens": ["O", "wun\u00b7der\u00b7lich", "Ge\u00b7spenst", ",", "das", "uns", "ohn", "Feur", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und ohne Strick und Band Gem\u00fcth und Seele bindet,", "tokens": ["Und", "oh\u00b7ne", "Strick", "und", "Band", "Ge\u00b7m\u00fcth", "und", "See\u00b7le", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Welches Band nicht zerrei\u00dft, und z\u00fcgen tausend dran,", "tokens": ["Wel\u00b7ches", "Band", "nicht", "zer\u00b7rei\u00dft", ",", "und", "z\u00fc\u00b7gen", "tau\u00b7send", "dran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Welch Feuer Mayn und Rhein nimmer verleschen kan.", "tokens": ["Welch", "Feu\u00b7er", "Mayn", "und", "Rhein", "nim\u00b7mer", "ver\u00b7le\u00b7schen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "KON", "NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Herr Br\u00e4utigam ihr k\u00f6nnt mir solches helffen zeugen,", "tokens": ["Herr", "Br\u00e4u\u00b7ti\u00b7gam", "ihr", "k\u00f6nnt", "mir", "sol\u00b7ches", "helf\u00b7fen", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVFIN", "PPER", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den ein so kleines Feur so bald hat k\u00f6nnen beugen,", "tokens": ["Den", "ein", "so", "klei\u00b7nes", "Feur", "so", "bald", "hat", "k\u00f6n\u00b7nen", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJA", "NN", "ADV", "ADV", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein bitters\u00fcsses Thier hat euch niedergef\u00e4llt,", "tokens": ["Ein", "bit\u00b7ter\u00b7s\u00fcs\u00b7ses", "Thier", "hat", "euch", "nie\u00b7der\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und euch in leidig Freud, in freudig Leid gestellt.", "tokens": ["Und", "euch", "in", "lei\u00b7dig", "Freud", ",", "in", "freu\u00b7dig", "Leid", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJD", "NN", "$,", "APPR", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jungfrau Anna die scho\u00df die hellgl\u00e4ntzende Stralen", "tokens": ["Jung\u00b7frau", "An\u00b7na", "die", "scho\u00df", "die", "hell\u00b7gl\u00e4nt\u00b7zen\u00b7de", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ART", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Von ihrer Augen Sonn, \u00fcber des Sandes Thalen,", "tokens": ["Von", "ih\u00b7rer", "Au\u00b7gen", "Sonn", ",", "\u00fc\u00b7ber", "des", "San\u00b7des", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ueber des Tragheims Berg, \u00fcber des Bobers Flu\u00df,", "tokens": ["Ue\u00b7ber", "des", "Trag\u00b7heims", "Berg", ",", "\u00fc\u00b7ber", "des", "Bo\u00b7bers", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Das Liebes-Fieber euch von diesem Schein anstie\u00df.", "tokens": ["Das", "Lie\u00b7bes\u00b7Fie\u00b7ber", "euch", "von", "die\u00b7sem", "Schein", "an\u00b7stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Brand kam in das Hertz, all eur Gedancken schwommen", "tokens": ["Der", "Brand", "kam", "in", "das", "Hertz", ",", "all", "eur", "Ge\u00b7dan\u00b7cken", "schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PIAT", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mitten in diesem Feur, ihr wu\u00dft nicht zu bekommen", "tokens": ["Mit\u00b7ten", "in", "die\u00b7sem", "Feur", ",", "ihr", "wu\u00dft", "nicht", "zu", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Zu euer Kranckheit Hilff, noch Trost zu euer Pein,", "tokens": ["Zu", "eu\u00b7er", "Kran\u00b7ck\u00b7heit", "Hilff", ",", "noch", "Trost", "zu", "eu\u00b7er", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Weil alles beydes war zu tieff gewurtzelt ein.", "tokens": ["Weil", "al\u00b7les", "bey\u00b7des", "war", "zu", "tieff", "ge\u00b7wurt\u00b7zelt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "VAFIN", "PTKA", "ADJD", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wolan Herr Br\u00e4utigam, wolt ihr werden curiret,", "tokens": ["Wo\u00b7lan", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", ",", "wolt", "ihr", "wer\u00b7den", "cu\u00b7ri\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "$,", "VMFIN", "PPER", "VAFIN", "NE", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "So schickt nach der die euch in di\u00df Elend gef\u00fchret,", "tokens": ["So", "schickt", "nach", "der", "die", "euch", "in", "di\u00df", "E\u00b7lend", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "PRELS", "PPER", "APPR", "PDS", "NN", "VVPP", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Euer Kranckheit ich gleich Achillis Wunden acht,", "tokens": ["Eu\u00b7er", "Kran\u00b7ck\u00b7heit", "ich", "gleich", "A\u00b7chil\u00b7lis", "Wun\u00b7den", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "NE", "NN", "CARD", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.36": {"text": "Die niemand heilen konnt, als der sie hat gemacht.", "tokens": ["Die", "nie\u00b7mand", "hei\u00b7len", "konnt", ",", "als", "der", "sie", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "KOUS", "ART", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und ihr o Jungfrau Braut, wendet das grosse Klagen,", "tokens": ["Und", "ihr", "o", "Jung\u00b7frau", "Braut", ",", "wen\u00b7det", "das", "gros\u00b7se", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "FM", "NN", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.38": {"text": "K\u00fchlt was ihr habt gebrennt, heilt was ihr habt geschlagen;", "tokens": ["K\u00fchlt", "was", "ihr", "habt", "ge\u00b7brennt", ",", "heilt", "was", "ihr", "habt", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VAFIN", "VVPP", "$,", "VVFIN", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wo ihr nicht selber seyd des Krancken Doctorin,", "tokens": ["Wo", "ihr", "nicht", "sel\u00b7ber", "seyd", "des", "Kran\u00b7cken", "Doc\u00b7to\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So f\u00e4hret er dahin ohn H\u00fclff und Medicin.", "tokens": ["So", "f\u00e4h\u00b7ret", "er", "da\u00b7hin", "ohn", "H\u00fclff", "und", "Me\u00b7di\u00b7cin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die sch\u00f6ne Nacht kommt an, der Mond sitzt auf den Wagen,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Nacht", "kommt", "an", ",", "der", "Mond", "sitzt", "auf", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und thut mit vollem Lauff des Himmels Feld durchjagen,", "tokens": ["Und", "thut", "mit", "vol\u00b7lem", "Lauff", "des", "Him\u00b7mels", "Feld", "durch\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die g\u00fcldnen Lichter hat der Himmel aufgesteckt,", "tokens": ["Die", "g\u00fcld\u00b7nen", "Lich\u00b7ter", "hat", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Weil sich die Sonn schon l\u00e4ngst zu Bette hat gelegt.", "tokens": ["Weil", "sich", "die", "Sonn", "schon", "l\u00e4ngst", "zu", "Bet\u00b7te", "hat", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wolan es ist nun Zeit, da\u00df ihr eur Kranckheit stillet,", "tokens": ["Wo\u00b7lan", "es", "ist", "nun", "Zeit", ",", "da\u00df", "ihr", "eur", "Kran\u00b7ck\u00b7heit", "stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "p