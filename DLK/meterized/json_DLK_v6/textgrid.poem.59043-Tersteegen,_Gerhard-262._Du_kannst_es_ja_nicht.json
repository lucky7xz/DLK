{"textgrid.poem.59043": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "262. Du kannst es ja nicht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gr\u00e4mst du dich in deinem Mut,", "tokens": ["Was", "gr\u00e4mst", "du", "dich", "in", "dei\u00b7nem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn kein Jesus w\u00e4r' zu finden?", "tokens": ["Als", "wenn", "kein", "Je\u00b7sus", "w\u00e4r'", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIAT", "NE", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm la\u00df dich, schau sein kostbar Blut!", "tokens": ["Ihm", "la\u00df", "dich", ",", "schau", "sein", "kost\u00b7bar", "Blut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "VAINF", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht du, nein, er hilft dir von S\u00fcnden.", "tokens": ["Nicht", "du", ",", "nein", ",", "er", "hilft", "dir", "von", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}