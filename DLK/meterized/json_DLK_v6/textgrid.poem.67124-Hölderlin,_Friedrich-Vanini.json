{"textgrid.poem.67124": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vanini", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Gottver\u00e4chter schalten sie dich? mit Fluch", "tokens": ["Den", "Gott\u00b7ver\u00b7\u00e4ch\u00b7ter", "schal\u00b7ten", "sie", "dich", "?", "mit", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "$.", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beschwerten sie dein Herz dir und banden dich", "tokens": ["Be\u00b7schwer\u00b7ten", "sie", "dein", "Herz", "dir", "und", "ban\u00b7den", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und \u00fcbergaben dich den Flammen,", "tokens": ["Und", "\u00fc\u00b7berg\u00b7a\u00b7ben", "dich", "den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiliger Mann! o warum nicht kamst du", "tokens": ["Hei\u00b7li\u00b7ger", "Mann", "!", "o", "wa\u00b7rum", "nicht", "kamst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "FM", "PWAV", "PTKNEG", "VVFIN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Vom Himmel her in Flammen zur\u00fcck, das Haupt", "tokens": ["Vom", "Him\u00b7mel", "her", "in", "Flam\u00b7men", "zu\u00b7r\u00fcck", ",", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "APZR", "APPR", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der L\u00e4sterer zu treffen und riefst dem Sturm;", "tokens": ["Der", "L\u00e4s\u00b7te\u00b7rer", "zu", "tref\u00b7fen", "und", "riefst", "dem", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df er die Asche der Barbaren", "tokens": ["Da\u00df", "er", "die", "A\u00b7sche", "der", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fort aus der Erd, aus der Heimat werfe!", "tokens": ["Fort", "aus", "der", "Erd", ",", "aus", "der", "Hei\u00b7mat", "wer\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch die du lebend liebtest, die dich empfing,", "tokens": ["Doch", "die", "du", "le\u00b7bend", "lieb\u00b7test", ",", "die", "dich", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Sterbenden, die heilge Natur vergi\u00dft", "tokens": ["Den", "Ster\u00b7ben\u00b7den", ",", "die", "heil\u00b7ge", "Na\u00b7tur", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Menschen Tun und deine Feinde", "tokens": ["Der", "Men\u00b7schen", "Tun", "und", "dei\u00b7ne", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kehrten, wie du, in den alten Frieden.", "tokens": ["Kehr\u00b7ten", ",", "wie", "du", ",", "in", "den", "al\u00b7ten", "Frie\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}