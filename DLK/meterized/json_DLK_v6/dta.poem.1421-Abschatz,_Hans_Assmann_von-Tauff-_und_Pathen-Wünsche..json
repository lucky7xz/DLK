{"dta.poem.1421": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Geht GOttes Huld vorher/ so folgen seine Gaben/", "tokens": ["Geht", "Got\u00b7tes", "Huld", "vor\u00b7her", "/", "so", "fol\u00b7gen", "sei\u00b7ne", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "ADV", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Hold-benahmter Tag f\u00fchrt dich ins Leben ein/", "tokens": ["Ein", "Hold\u00b7be\u00b7nahm\u00b7ter", "Tag", "f\u00fchrt", "dich", "ins", "Le\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heut wird der Unschuld Schmuck dein Tauff-Geschencke", "tokens": ["Heut", "wird", "der", "Un\u00b7schuld", "Schmuck", "dein", "Tauff\u00b7Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "seyn/", "tokens": ["seyn", "/"], "token_info": ["word", "punct"], "pos": ["VAINF", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Von beyden kanst du so ein freudig Denck-Mahl haben.", "tokens": ["Von", "bey\u00b7den", "kanst", "du", "so", "ein", "freu\u00b7dig", "Den\u00b7ck\u00b7Mahl", "ha\u00b7ben", "."], "token_info": ["word"