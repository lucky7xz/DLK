{"textgrid.poem.34098": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Meister und Pfuscher", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht den Character bewundert, wenn echte Talente verschm\u00e4hen,", "tokens": ["Nicht", "den", "Cha\u00b7rac\u00b7ter", "be\u00b7wun\u00b7dert", ",", "wenn", "ech\u00b7te", "Ta\u00b7len\u00b7te", "ver\u00b7schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVPP", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Um den erb\u00e4rmlichen Preis fl\u00fcchtigen Gauklererfolgs", "tokens": ["Um", "den", "er\u00b7b\u00e4rm\u00b7li\u00b7chen", "Preis", "fl\u00fcch\u00b7ti\u00b7gen", "Gauk\u00b7le\u00b7rer\u00b7folgs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Alle Gesetze der Kunst mit F\u00fc\u00dfen zu treten und alle", "tokens": ["Al\u00b7le", "Ge\u00b7set\u00b7ze", "der", "Kunst", "mit", "F\u00fc\u00b7\u00dfen", "zu", "tre\u00b7ten", "und", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "KON", "PIAT"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zu verh\u00f6hnen, verehrt ruhig die gr\u00f6\u00df're Natur.", "tokens": ["Zu", "ver\u00b7h\u00f6h\u00b7nen", ",", "ver\u00b7ehrt", "ru\u00b7hig", "die", "gr\u00f6\u00df'\u00b7re", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Pfuschern setztet ihr oft schon deutsche Preise, wof\u00fcr ihr", "tokens": ["Pfu\u00b7schern", "setz\u00b7tet", "ihr", "oft", "schon", "deut\u00b7sche", "Prei\u00b7se", ",", "wo\u00b7f\u00fcr", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,", "PWAV", "PPER"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Meisterst\u00fccke bedangt, setzt sie den Meistern einmal", "tokens": ["Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "be\u00b7dangt", ",", "setzt", "sie", "den", "Meis\u00b7tern", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und bedingt euch Gesudel, sie werden euch schwerlich gen\u00fcgen,", "tokens": ["Und", "be\u00b7dingt", "euch", "Ge\u00b7su\u00b7del", ",", "sie", "wer\u00b7den", "euch", "schwer\u00b7lich", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Denn ein harmonischer Geist hat im Gemeinen sein Maa\u00df.", "tokens": ["Denn", "ein", "har\u00b7mo\u00b7ni\u00b7scher", "Geist", "hat", "im", "Ge\u00b7mei\u00b7nen", "sein", "Maa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Einfach ist das Gesetz, es ruht die Kraft nicht im Wurme,", "tokens": ["Ein\u00b7fach", "ist", "das", "Ge\u00b7setz", ",", "es", "ruht", "die", "Kraft", "nicht", "im", "Wur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Eh' sie den Punct erreicht, wo sie sich selber gen\u00fcgt,", "tokens": ["Eh'", "sie", "den", "Punct", "er\u00b7reicht", ",", "wo", "sie", "sich", "sel\u00b7ber", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Und es triebe kein Gott sie weiter, wenn er auch wollte,", "tokens": ["Und", "es", "trie\u00b7be", "kein", "Gott", "sie", "wei\u00b7ter", ",", "wenn", "er", "auch", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Me\u00dft denn ehrlich den Weg, und es ergiebt sich der Grad.", "tokens": ["Me\u00dft", "denn", "ehr\u00b7lich", "den", "Weg", ",", "und", "es", "er\u00b7giebt", "sich", "der", "Grad", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}