{"textgrid.poem.66266": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Theons Nachtgesang", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eines Eichenwaldes schw\u00e4rzester Nacht", "tokens": ["In", "ei\u00b7nes", "Ei\u00b7chen\u00b7wal\u00b7des", "schw\u00e4r\u00b7zes\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Stand vor seines Gekl\u00fcftes Eingang", "tokens": ["Stand", "vor", "sei\u00b7nes", "Ge\u00b7kl\u00fcf\u00b7tes", "Ein\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Unsichtbar schwebten Geister des Himmels um ihn;", "tokens": ["Un\u00b7sicht\u00b7bar", "schweb\u00b7ten", "Geis\u00b7ter", "des", "Him\u00b7mels", "um", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da begann er seinen Nachtgesang:", "tokens": ["Da", "be\u00b7gann", "er", "sei\u00b7nen", "Nacht\u00b7ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbgro\u00df bist du, Riesin Natur.", "tokens": ["\u00bb", "gro\u00df", "bist", "du", ",", "Rie\u00b7sin", "Na\u00b7tur", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$,", "NN", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.2": {"text": "Deine Sohle ruht auf der Erde,", "tokens": ["Dei\u00b7ne", "Soh\u00b7le", "ruht", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dein Scheitel bestreift die Sterne!", "tokens": ["Dein", "Schei\u00b7tel", "be\u00b7streift", "die", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gro\u00df bist du, Riesin Natur!\u00ab", "tokens": ["Gro\u00df", "bist", "du", ",", "Rie\u00b7sin", "Na\u00b7tur", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "NN", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}