{"textgrid.poem.62992": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Lottod\u00e4mon hart geplagt,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Lottod\u00e4mon hart geplagt,", "tokens": ["Vom", "Lot\u00b7to\u00b7d\u00e4\u00b7mon", "hart", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschlo\u00dfen vier Pariser Damen,", "tokens": ["Be\u00b7schlo\u00b7\u00dfen", "vier", "Pa\u00b7ri\u00b7ser", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die niemals den Gewinst bekamen,", "tokens": ["Die", "nie\u00b7mals", "den", "Ge\u00b7winst", "be\u00b7ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem sie schon lange nachgejagt,", "tokens": ["Dem", "sie", "schon", "lan\u00b7ge", "nach\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Aberwitz um Rath zu fragen.", "tokens": ["Den", "A\u00b7ber\u00b7witz", "um", "Rath", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hatten sie schon oft gethan", "tokens": ["Das", "hat\u00b7ten", "sie", "schon", "oft", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wu\u00dftens nicht. Der goldne Wagen", "tokens": ["Und", "wu\u00df\u00b7tens", "nicht", ".", "Der", "gold\u00b7ne", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hielt mit Ger\u00e4usch am Tollhaus an.", "tokens": ["Hielt", "mit", "Ge\u00b7r\u00e4usch", "am", "Toll\u00b7haus", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie steigen aus; die Th\u00fcren knarren;", "tokens": ["Sie", "stei\u00b7gen", "aus", ";", "die", "Th\u00fc\u00b7ren", "knar\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie schaudern und beym ersten Narren,", "tokens": ["Sie", "schau\u00b7dern", "und", "beym", "ers\u00b7ten", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es war ein grauer Alchymist,", "tokens": ["Es", "war", "ein", "grau\u00b7er", "Al\u00b7chy\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Verweilen sie. Die Sch\u00f6nen baten", "tokens": ["Ver\u00b7wei\u00b7len", "sie", ".", "Die", "Sch\u00f6\u00b7nen", "ba\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den d\u00fcstern Geist beym Trismegist,", "tokens": ["Den", "d\u00fcs\u00b7tern", "Geist", "beym", "Tris\u00b7me\u00b7gist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vier Nummern ihnen anzurathen,", "tokens": ["Vier", "Num\u00b7mern", "ih\u00b7nen", "an\u00b7zu\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die als Quaterne dann ihr Gl\u00fcck", "tokens": ["Die", "als", "Qua\u00b7ter\u00b7ne", "dann", "ihr", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "ADV", "PPOSAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Zum Lottorad versuchen sollen.", "tokens": ["Zum", "Lot\u00b7to\u00b7rad", "ver\u00b7su\u00b7chen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Alte thut mit heiterm Blick,", "tokens": ["Der", "Al\u00b7te", "thut", "mit", "hei\u00b7term", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Was die geputzten Damen wollen,", "tokens": ["Was", "die", "ge\u00b7putz\u00b7ten", "Da\u00b7men", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die dem Papier mit frohem Drang", "tokens": ["Die", "dem", "Pa\u00b7pier", "mit", "fro\u00b7hem", "Drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die reichen Zahlen anvertrauen.", "tokens": ["Die", "rei\u00b7chen", "Zah\u00b7len", "an\u00b7ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "La\u00dft, rief der Greis, mich sie beschauen!", "tokens": ["La\u00dft", ",", "rief", "der", "Greis", ",", "mich", "sie", "be\u00b7schau\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Man gab sie ihm und er verschlang", "tokens": ["Man", "gab", "sie", "ihm", "und", "er", "ver\u00b7schlang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die Ziffern flugs wie Mandelkerne.", "tokens": ["Die", "Zif\u00b7fern", "flugs", "wie", "Man\u00b7del\u00b7ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nun, sprach er nach vollbrachtem Schmaus,", "tokens": ["Nun", ",", "sprach", "er", "nach", "voll\u00b7brach\u00b7tem", "Schmaus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Verziehet hier; denn die Quaterne", "tokens": ["Ver\u00b7zie\u00b7het", "hier", ";", "denn", "die", "Qua\u00b7ter\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "KON", "ART", "NN"], "meter": "-+-++-+--", "measure": "unknown.measure.tetra"}, "line.26": {"text": "K\u00f6mmt heute noch gewi\u00df heraus.", "tokens": ["K\u00f6mmt", "heu\u00b7te", "noch", "ge\u00b7wi\u00df", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}