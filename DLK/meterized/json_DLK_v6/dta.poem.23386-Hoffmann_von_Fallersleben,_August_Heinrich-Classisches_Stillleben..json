{"dta.poem.23386": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Classisches Stillleben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "St\u00f6rt doch nicht die alten Jungen!", "tokens": ["St\u00f6rt", "doch", "nicht", "die", "al\u00b7ten", "Jun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sie lesen eben jetzt", "tokens": ["Denn", "sie", "le\u00b7sen", "e\u00b7ben", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Homeros hat gesungen", "tokens": ["Was", "Ho\u00b7me\u00b7ros", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NE", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wie's Vo\u00df hat \u00fcbersetzt.", "tokens": ["Und", "wie's", "Vo\u00df", "hat", "\u00fc\u00b7bers\u00b7etzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Besser l\u00e4\u00dft es sich doch sitzen", "tokens": ["Bes\u00b7ser", "l\u00e4\u00dft", "es", "sich", "doch", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oben in dem G\u00f6tterrath,", "tokens": ["O\u00b7ben", "in", "dem", "G\u00f6t\u00b7ter\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als dereinst die Zeit verschwitzen", "tokens": ["Als", "de\u00b7reinst", "die", "Zeit", "ver\u00b7schwit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Actenmatt im Magistrat.", "tokens": ["Ac\u00b7ten\u00b7matt", "im", "Ma\u00b7gist\u00b7rat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Besser klingen doch die Sagen", "tokens": ["Bes\u00b7ser", "klin\u00b7gen", "doch", "die", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der G\u00f6tter Ha\u00df und Groll,", "tokens": ["Von", "der", "G\u00f6t\u00b7ter", "Ha\u00df", "und", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der B\u00fcrger ew'ge Klagen", "tokens": ["Als", "der", "B\u00fcr\u00b7ger", "ew'\u00b7ge", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Steuern, Mauth und Zoll.", "tokens": ["Ue\u00b7ber", "Steu\u00b7ern", ",", "Mauth", "und", "Zoll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Besser klingt das Schiffregister", "tokens": ["Bes\u00b7ser", "klingt", "das", "Schiff\u00b7re\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so mancher Schlachtbericht,", "tokens": ["Und", "so", "man\u00b7cher", "Schlacht\u00b7be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn uns ein Stockphilister", "tokens": ["Als", "wenn", "uns", "ein", "Stock\u00b7phi\u00b7lis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem letzten Budget spricht.", "tokens": ["Von", "dem", "letz\u00b7ten", "Bud\u00b7get", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Besser, wenn Thersites grimmig", "tokens": ["Bes\u00b7ser", ",", "wenn", "Ther\u00b7si\u00b7tes", "grim\u00b7mig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "NE", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ueber seinen K\u00f6nig schreit,", "tokens": ["Ue\u00b7ber", "sei\u00b7nen", "K\u00f6\u00b7nig", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn unser Land einstimmig", "tokens": ["Als", "wenn", "un\u00b7ser", "Land", "ein\u00b7stim\u00b7mig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweiget von der Pre\u00dffreiheit.", "tokens": ["Schwei\u00b7get", "von", "der", "Pre\u00df\u00b7frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Besser klinget Priams Jammer,", "tokens": ["Bes\u00b7ser", "klin\u00b7get", "Pri\u00b7ams", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sein Sohn im Kampf erlag,", "tokens": ["Da\u00df", "sein", "Sohn", "im", "Kampf", "er\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn unsre zweite Kammer", "tokens": ["Als", "wenn", "uns\u00b7re", "zwei\u00b7te", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreibet an den Bundestag.", "tokens": ["Schrei\u00b7bet", "an", "den", "Bun\u00b7des\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Besser klingt's, wenn nun im Feuer", "tokens": ["Bes\u00b7ser", "klingt's", ",", "wenn", "nun", "im", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich Trojas Feste steht,", "tokens": ["End\u00b7lich", "Tro\u00b7jas", "Fes\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn unser Landtag heuer", "tokens": ["Als", "wenn", "un\u00b7ser", "Land\u00b7tag", "heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhig auseinander geht.", "tokens": ["Ru\u00b7hig", "aus\u00b7ein\u00b7an\u00b7der", "geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}