{"textgrid.poem.58664": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie fertig ist die Eigenheit, sich alles anzuma\u00dfen!", "tokens": ["Wie", "fer\u00b7tig", "ist", "die", "Ei\u00b7gen\u00b7heit", ",", "sich", "al\u00b7les", "an\u00b7zu\u00b7ma\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRF", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Hat man den Unflat dieser Welt in erster Bu\u00df' verlassen,", "tokens": ["Hat", "man", "den", "Un\u00b7flat", "die\u00b7ser", "Welt", "in", "ers\u00b7ter", "Bu\u00df'", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "PDAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So nennt man sich in seinem Sinn", "tokens": ["So", "nennt", "man", "sich", "in", "sei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon Christi Braut und K\u00f6nigin", "tokens": ["Schon", "Chris\u00b7ti", "Braut", "und", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit dem Hut der Freiheit deckt", "tokens": ["Und", "mit", "dem", "Hut", "der", "Frei\u00b7heit", "deckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Greuel, der im Grund noch steckt.", "tokens": ["Den", "Greu\u00b7el", ",", "der", "im", "Grund", "noch", "steckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, diese ist es lange nicht, heb ab die Kron' zur Erden,", "tokens": ["Ach", ",", "die\u00b7se", "ist", "es", "lan\u00b7ge", "nicht", ",", "heb", "ab", "die", "Kron'", "zur", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "$,", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Du mu\u00dft zunicht, zunicht, zunicht und tief geniedrigt werden!", "tokens": ["Du", "mu\u00dft", "zu\u00b7nicht", ",", "zu\u00b7nicht", ",", "zu\u00b7nicht", "und", "tief", "ge\u00b7nied\u00b7rigt", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Wer's meint zu sein, von dem Gott spricht:", "tokens": ["Wer's", "meint", "zu", "sein", ",", "von", "dem", "Gott", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKZU", "VAINF", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbauch diese Seele ist es nicht\u00ab;", "tokens": ["\u00bb", "auch", "die\u00b7se", "See\u00b7le", "ist", "es", "nicht", "\u00ab", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PDAT", "NN", "VAFIN", "PPER", "PTKNEG", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer ganz entbl\u00f6\u00dft und klein und schlecht,", "tokens": ["Wer", "ganz", "ent\u00b7bl\u00f6\u00dft", "und", "klein", "und", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der hat allein zur Krone Recht.", "tokens": ["Der", "hat", "al\u00b7lein", "zur", "Kro\u00b7ne", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}