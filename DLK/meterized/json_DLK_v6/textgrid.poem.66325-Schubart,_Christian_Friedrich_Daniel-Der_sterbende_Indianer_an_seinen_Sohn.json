{"textgrid.poem.66325": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende Indianer an seinen Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sterbe, Sohn! Nimm diesen Kranz;", "tokens": ["Ich", "ster\u00b7be", ",", "Sohn", "!", "Nimm", "die\u00b7sen", "Kranz", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "NE", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Christenhaaren flocht' ich ihn;", "tokens": ["Von", "Chris\u00b7ten\u00b7haa\u00b7ren", "flocht'", "ich", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt Diamanten spielen drin", "tokens": ["Statt", "Di\u00b7a\u00b7man\u00b7ten", "spie\u00b7len", "drin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADV"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Erschlagner Christen Z\u00e4hne. Sohn,", "tokens": ["Er\u00b7schlag\u00b7ner", "Chris\u00b7ten", "Z\u00e4h\u00b7ne", ".", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sterbe arm; der Christen Geiz", "tokens": ["Ich", "ster\u00b7be", "arm", ";", "der", "Chris\u00b7ten", "Geiz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lie\u00df mir die\u00df Stroh, worauf ich sterbe,", "tokens": ["Lie\u00df", "mir", "die\u00df", "Stroh", ",", "wo\u00b7rauf", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dort den Bogen \u2013 Ha, den Pfeil,", "tokens": ["Und", "dort", "den", "Bo\u00b7gen", "\u2013", "Ha", ",", "den", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$(", "ITJ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der fliegt und singt und trifft und t\u00f6dtet!", "tokens": ["Der", "fliegt", "und", "singt", "und", "trifft", "und", "t\u00f6d\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O Sohn, sieh deinen Vater an", "tokens": ["O", "Sohn", ",", "sieh", "dei\u00b7nen", "Va\u00b7ter", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schw\u00f6re, mir ihm gleich zu sein!", "tokens": ["Und", "schw\u00f6\u00b7re", ",", "mir", "ihm", "gleich", "zu", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "PPER", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sei kalt und keck und frei und gut,", "tokens": ["Sei", "kalt", "und", "keck", "und", "frei", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und hasse den, der seinen Gott", "tokens": ["Und", "has\u00b7se", "den", ",", "der", "sei\u00b7nen", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Entehrt! Dort unter jenem Baum", "tokens": ["Ent\u00b7ehrt", "!", "Dort", "un\u00b7ter", "je\u00b7nem", "Baum"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist ein Altar, dort bete an!", "tokens": ["Ist", "ein", "Al\u00b7tar", ",", "dort", "be\u00b7te", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des Cocusbaumes Wipfel s\u00e4uselt", "tokens": ["Des", "Co\u00b7cus\u00b7bau\u00b7mes", "Wip\u00b7fel", "s\u00e4u\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dein Flehn zum Vater der Natur", "tokens": ["Dein", "Flehn", "zum", "Va\u00b7ter", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dem Himmel vor! Ich sterbe gern;", "tokens": ["Dem", "Him\u00b7mel", "vor", "!", "Ich", "ster\u00b7be", "gern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nun wirft kein Sturm den Fischerkahn", "tokens": ["Nun", "wirft", "kein", "Sturm", "den", "Fi\u00b7scher\u00b7kahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Auf hohen Wogen hin und her.", "tokens": ["Auf", "ho\u00b7hen", "Wo\u00b7gen", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein ew'ger Fr\u00fchling bl\u00fchet dort.", "tokens": ["Ein", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "bl\u00fc\u00b7het", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mein Weib, ach, deine Mutter reicht", "tokens": ["Mein", "Weib", ",", "ach", ",", "dei\u00b7ne", "Mut\u00b7ter", "reicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Auf goldner Schal' mir Ananas.", "tokens": ["Auf", "gold\u00b7ner", "Schal'", "mir", "A\u00b7na\u00b7nas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Aus Christensch\u00e4deln trink' ich dort", "tokens": ["Aus", "Chris\u00b7ten\u00b7sch\u00e4\u00b7deln", "trink'", "ich", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der G\u00f6tter Wein! O ", "tokens": ["Der", "G\u00f6t\u00b7ter", "Wein", "!", "O"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$.", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Der gro\u00dfe Kapak winkt mir schon!", "tokens": ["Der", "gro\u00b7\u00dfe", "Ka\u00b7pak", "winkt", "mir", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Leg' deine Hand auf meine Brust,", "tokens": ["Leg'", "dei\u00b7ne", "Hand", "auf", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und schw\u00f6re mir! Begrabe mich,", "tokens": ["Und", "schw\u00f6\u00b7re", "mir", "!", "Be\u00b7gra\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wo deine Mutter liegt! Leb' wohl!", "tokens": ["Wo", "dei\u00b7ne", "Mut\u00b7ter", "liegt", "!", "Leb'", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$.", "VVIMP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}