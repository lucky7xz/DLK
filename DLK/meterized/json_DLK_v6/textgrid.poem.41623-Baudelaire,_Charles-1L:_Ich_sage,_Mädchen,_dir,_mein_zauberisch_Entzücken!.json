{"textgrid.poem.41623": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sage, M\u00e4dchen, dir, mein zauberisch Entz\u00fccken!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sage, M\u00e4dchen, dir, mein zauberisch Entz\u00fccken!", "tokens": ["Ich", "sa\u00b7ge", ",", "M\u00e4d\u00b7chen", ",", "dir", ",", "mein", "zau\u00b7be\u00b7risch", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Reize mannigfalt, die deine Jugend schm\u00fccken,", "tokens": ["Die", "Rei\u00b7ze", "man\u00b7nig\u00b7falt", ",", "die", "dei\u00b7ne", "Ju\u00b7gend", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und malen will ich deine Pracht,", "tokens": ["Und", "ma\u00b7len", "will", "ich", "dei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Zartheit eines Kinds aus reifer Sch\u00f6nheit lacht.", "tokens": ["Wo", "Zar\u00b7theit", "ei\u00b7nes", "Kinds", "aus", "rei\u00b7fer", "Sch\u00f6n\u00b7heit", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann sacht du gehst, die Luft mit weiten R\u00f6cken fegend,", "tokens": ["Wann", "sacht", "du", "gehst", ",", "die", "Luft", "mit", "wei\u00b7ten", "R\u00f6\u00b7cken", "fe\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du ein sch\u00f6nes Schiff, das langsam sich bewegend", "tokens": ["Bist", "du", "ein", "sch\u00f6\u00b7nes", "Schiff", ",", "das", "lang\u00b7sam", "sich", "be\u00b7we\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aussegelt in der See Geroll,", "tokens": ["Aus\u00b7se\u00b7gelt", "in", "der", "See", "Ge\u00b7roll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Takt gewiegt, der tr\u00e4g und ruhevoll.", "tokens": ["In", "ei\u00b7nem", "Takt", "ge\u00b7wiegt", ",", "der", "tr\u00e4g", "und", "ru\u00b7he\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf deinem runden Hals, auf deiner Schulter Prangen", "tokens": ["Auf", "dei\u00b7nem", "run\u00b7den", "Hals", ",", "auf", "dei\u00b7ner", "Schul\u00b7ter", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gst du dein stolzes Haupt, von seltnem Reiz umfangen.", "tokens": ["Tr\u00e4gst", "du", "dein", "stol\u00b7zes", "Haupt", ",", "von", "selt\u00b7nem", "Reiz", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Triumph im Blick und sanfte Ruh,", "tokens": ["Tri\u00b7umph", "im", "Blick", "und", "sanf\u00b7te", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kind voller Majest\u00e4t, gehst deines Weges du.", "tokens": ["Kind", "vol\u00b7ler", "Ma\u00b7jes\u00b7t\u00e4t", ",", "gehst", "dei\u00b7nes", "We\u00b7ges", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Ich sage, M\u00e4dchen, dir, mein zauberisch Entz\u00fccken!", "tokens": ["Ich", "sa\u00b7ge", ",", "M\u00e4d\u00b7chen", ",", "dir", ",", "mein", "zau\u00b7be\u00b7risch", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Reize mannigfalt, die deine Jugend schm\u00fccken,", "tokens": ["Die", "Rei\u00b7ze", "man\u00b7nig\u00b7falt", ",", "die", "dei\u00b7ne", "Ju\u00b7gend", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und malen will ich deine Pracht,", "tokens": ["Und", "ma\u00b7len", "will", "ich", "dei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Zartheit eines Kinds aus reifer Sch\u00f6nheit lacht.", "tokens": ["Wo", "Zar\u00b7theit", "ei\u00b7nes", "Kinds", "aus", "rei\u00b7fer", "Sch\u00f6n\u00b7heit", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dein Busen, der sich hebt, geengt von seidnem Flimmer,", "tokens": ["Dein", "Bu\u00b7sen", ",", "der", "sich", "hebt", ",", "ge\u00b7engt", "von", "seid\u00b7nem", "Flim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist einer Lade gleich in seiner Reize Schimmer,", "tokens": ["Ist", "ei\u00b7ner", "La\u00b7de", "gleich", "in", "sei\u00b7ner", "Rei\u00b7ze", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit blanker W\u00f6lbung, wo das Licht", "tokens": ["Mit", "blan\u00b7ker", "W\u00f6l\u00b7bung", ",", "wo", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auf metallnem Schild in hellem Glanz sich bricht.", "tokens": ["Wie", "auf", "me\u00b7tall\u00b7nem", "Schild", "in", "hel\u00b7lem", "Glanz", "sich", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Verlockend Schilderpaar, mit ros'gen Spitzen prahlend,", "tokens": ["Ver\u00b7lo\u00b7ckend", "Schil\u00b7der\u00b7paar", ",", "mit", "ros'\u00b7gen", "Spit\u00b7zen", "prah\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich einer Lade, voll von Sch\u00e4tzen, s\u00fc\u00df und strahlend,", "tokens": ["Gleich", "ei\u00b7ner", "La\u00b7de", ",", "voll", "von", "Sch\u00e4t\u00b7zen", ",", "s\u00fc\u00df", "und", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Voll starken Tranks, voll Duft und Wein,", "tokens": ["Voll", "star\u00b7ken", "Tranks", ",", "voll", "Duft", "und", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00f6\u00dft selge Trunkenheit er Herz und Sinnen ein.", "tokens": ["Fl\u00f6\u00dft", "sel\u00b7ge", "Trun\u00b7ken\u00b7heit", "er", "Herz", "und", "Sin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Wann sacht du gehst, die Luft mit weiten R\u00f6cken fegend,", "tokens": ["Wann", "sacht", "du", "gehst", ",", "die", "Luft", "mit", "wei\u00b7ten", "R\u00f6\u00b7cken", "fe\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du ein sch\u00f6nes Schiff, das langsam sich bewegend", "tokens": ["Bist", "du", "ein", "sch\u00f6\u00b7nes", "Schiff", ",", "das", "lang\u00b7sam", "sich", "be\u00b7we\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aussegelt in der See Geroll,", "tokens": ["Aus\u00b7se\u00b7gelt", "in", "der", "See", "Ge\u00b7roll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Takt gewiegt, der trag und ruhevoll.", "tokens": ["In", "ei\u00b7nem", "Takt", "ge\u00b7wiegt", ",", "der", "trag", "und", "ru\u00b7he\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ART", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die edlen Beine, die des Kleides reiche Zierden", "tokens": ["Die", "ed\u00b7len", "Bei\u00b7ne", ",", "die", "des", "Klei\u00b7des", "rei\u00b7che", "Zier\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewegen, stacheln auf die dunkelen Begierden.", "tokens": ["Be\u00b7we\u00b7gen", ",", "sta\u00b7cheln", "auf", "die", "dun\u00b7ke\u00b7len", "Be\u00b7gier\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwei Zauberinnen gleich zu schaun,", "tokens": ["Zwei", "Zau\u00b7be\u00b7rin\u00b7nen", "gleich", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die einen schwarzen Trank in tiefer Urne braun.", "tokens": ["Die", "ei\u00b7nen", "schwar\u00b7zen", "Trank", "in", "tie\u00b7fer", "Ur\u00b7ne", "braun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Arme w\u00fcrden nicht vor jungen H\u00fcnen bangen,", "tokens": ["Die", "Ar\u00b7me", "w\u00fcr\u00b7den", "nicht", "vor", "jun\u00b7gen", "H\u00fc\u00b7nen", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wetteifernd leicht an Kraft mit glatten Riesenschlangen,", "tokens": ["Wett\u00b7ei\u00b7fernd", "leicht", "an", "Kraft", "mit", "glat\u00b7ten", "Rie\u00b7sen\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschaffen, den Geliebten fest", "tokens": ["Ge\u00b7schaf\u00b7fen", ",", "den", "Ge\u00b7lieb\u00b7ten", "fest"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ans Herz zu dr\u00fccken, das ihn nie mehr l\u00e4\u00dft.", "tokens": ["Ans", "Herz", "zu", "dr\u00fc\u00b7cken", ",", "das", "ihn", "nie", "mehr", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Auf deinem runden Hals, auf deiner Schultern Prangen,", "tokens": ["Auf", "dei\u00b7nem", "run\u00b7den", "Hals", ",", "auf", "dei\u00b7ner", "Schul\u00b7tern", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gst du dein stolzes Haupt, von seltnem Reiz umfangen.", "tokens": ["Tr\u00e4gst", "du", "dein", "stol\u00b7zes", "Haupt", ",", "von", "selt\u00b7nem", "Reiz", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "wor