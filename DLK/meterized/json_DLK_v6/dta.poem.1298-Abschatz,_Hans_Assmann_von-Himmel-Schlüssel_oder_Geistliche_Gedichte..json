{"dta.poem.1298": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich deinen Ruhm/ O GOtt/ zur Gn\u00fcg erheben/", "tokens": ["Wie", "soll", "ich", "dei\u00b7nen", "Ruhm", "/", "O", "Gott", "/", "zur", "Gn\u00fcg", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "$(", "NE", "NN", "$(", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben/ die Vernunfft/ O H\u00f6chster/ schenckst du", "tokens": ["Das", "Le\u00b7ben", "/", "die", "Ver\u00b7nunfft", "/", "O", "H\u00f6chs\u00b7ter", "/", "schenckst", "du"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "NE", "NN", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mir/", "tokens": ["mir", "/"], "token_info": ["word", "punct"], "pos": ["PPER", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Verlangte Mittel hab ich einig auch von dir/", "tokens": ["Ver\u00b7lang\u00b7te", "Mit\u00b7tel", "hab", "ich", "ei\u00b7nig", "auch", "von", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch die ich diesen Tag vergn\u00fcget kan erleben/", "tokens": ["Durch", "die", "ich", "die\u00b7sen", "Tag", "ver\u00b7gn\u00fc\u00b7get", "kan", "er\u00b7le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PDAT", "NN", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was werd ich dir davor aus treuem Hertzen geben/", "tokens": ["Was", "werd", "ich", "dir", "da\u00b7vor", "aus", "treu\u00b7em", "Hert\u00b7zen", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "PAV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das izt verhandne Fest h\u00e4lt meinen Augen hier", "tokens": ["Das", "izt", "ver\u00b7hand\u00b7ne", "Fest", "h\u00e4lt", "mei\u00b7nen", "Au\u00b7gen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Eyd-verbundne Pflicht/ den Schlu\u00df der Tauffe f\u00fcr/", "tokens": ["Die", "Ey\u00b7dver\u00b7bund\u00b7ne", "Pflicht", "/", "den", "Schlu\u00df", "der", "Tauf\u00b7fe", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und dort umschr\u00e4ncket mich des Fleisches Widerstreben.", "tokens": ["Und", "dort", "um\u00b7schr\u00e4n\u00b7cket", "mich", "des", "Flei\u00b7sches", "Wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Willen geb ich dir/ der so viel m\u00f6glich/ gutt/", "tokens": ["Den", "Wil\u00b7len", "geb", "ich", "dir", "/", "der", "so", "viel", "m\u00f6g\u00b7lich", "/", "gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$(", "ART", "ADV", "ADV", "ADJD", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Herr/ der du alles kanst/ bez\u00e4hme Fleisch und Blutt/", "tokens": ["Herr", "/", "der", "du", "al\u00b7les", "kanst", "/", "be\u00b7z\u00e4h\u00b7me", "Fleisch", "und", "Blutt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PIS", "VMFIN", "$(", "ADJA", "NN", "KON", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Vergieb die Schuld/ mit der ich S\u00fcnder bin beladen/", "tokens": ["Ver\u00b7gieb", "die", "Schuld", "/", "mit", "der", "ich", "S\u00fcn\u00b7der", "bin", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gieb wahre S\u00fcnden-Neu/ gieb einen neuen Geist/", "tokens": ["Gieb", "wah\u00b7re", "S\u00fcn\u00b7den\u00b7Neu", "/", "gieb", "ei\u00b7nen", "neu\u00b7en", "Geist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$(", "VVIMP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der von der Erde sich und ihrem Kothe reist/", "tokens": ["Der", "von", "der", "Er\u00b7de", "sich", "und", "ih\u00b7rem", "Ko\u00b7the", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wohne ferner bey den Deinen in Genaden.", "tokens": ["Und", "woh\u00b7ne", "fer\u00b7ner", "bey", "den", "Dei\u00b7nen", "in", "Ge\u00b7na\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}