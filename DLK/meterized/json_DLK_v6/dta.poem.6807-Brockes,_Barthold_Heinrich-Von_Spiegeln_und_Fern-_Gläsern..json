{"dta.poem.6807": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alt best\u00e4nbtes Buch zeigt uns auf seinem Bande,", "tokens": ["Ein", "alt", "be\u00b7st\u00e4nb\u00b7tes", "Buch", "zeigt", "uns", "auf", "sei\u00b7nem", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In feuchter F\u00e4ulni\u00df weichem Wust,", "tokens": ["In", "feuch\u00b7ter", "F\u00e4ul\u00b7ni\u00df", "wei\u00b7chem", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer gantz besondern Lust,", "tokens": ["Mit", "ei\u00b7ner", "gantz", "be\u00b7son\u00b7dern", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den allersch\u00f6nsten Schmeltz von einem Garten-Lande,", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Sch\u00b7meltz", "von", "ei\u00b7nem", "Gar\u00b7ten\u00b7Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "In bunter Blumen Glantz, in angenehmen Gr\u00fcnen.", "tokens": ["In", "bun\u00b7ter", "Blu\u00b7men", "Glantz", ",", "in", "an\u00b7ge\u00b7neh\u00b7men", "Gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es zeiget uns verkleint den Pinsel und die Spur,", "tokens": ["Es", "zei\u00b7get", "uns", "ver\u00b7kleint", "den", "Pin\u00b7sel", "und", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wodurch die weise Hand der bildenden Natur,", "tokens": ["Wo\u00b7durch", "die", "wei\u00b7se", "Hand", "der", "bil\u00b7den\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die wundervolle Pracht", "tokens": ["Die", "wun\u00b7der\u00b7vol\u00b7le", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Von ihren sch\u00f6nsten Wercken macht.", "tokens": ["Von", "ih\u00b7ren", "sch\u00f6ns\u00b7ten", "Wer\u00b7cken", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}