{"dta.poem.7375": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Geyerpfiff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun still! \u2014 Du an den Dohnenschlag!", "tokens": ["\u201e", "nun", "still", "!", "Du", "an", "den", "Doh\u00b7nen\u00b7schlag", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "$(", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Du links an den gespaltnen Baum!", "tokens": ["Du", "links", "an", "den", "ge\u00b7spalt\u00b7nen", "Baum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hier der faule Fetzer mag", "tokens": ["Und", "hier", "der", "fau\u00b7le", "Fet\u00b7zer", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich lagern an der Klippe Saum:", "tokens": ["Sich", "la\u00b7gern", "an", "der", "Klip\u00b7pe", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da seht fein offen \u00fcber's Land", "tokens": ["Da", "seht", "fein", "of\u00b7fen", "\u00fc\u00b7ber's", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kutsche ihr heran spazieren:", "tokens": ["Die", "Kut\u00b7sche", "ihr", "he\u00b7ran", "spa\u00b7zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Rieder dort der H\u00f6llenbrand,", "tokens": ["Und", "Rie\u00b7der", "dort", "der", "H\u00f6l\u00b7len\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mag in den Steinbruch sich postiren!\u201c", "tokens": ["Mag", "in", "den", "Stein\u00b7bruch", "sich", "pos\u00b7ti\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PRF", "VVINF", "$.", "$("], "meter": "+--+--+--", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "\u201edann aufgepa\u00dft mit Aug' und Ohr,", "tokens": ["\u201e", "dann", "auf\u00b7ge\u00b7pa\u00dft", "mit", "Aug'", "und", "Ohr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bei dem ersten R\u00e4derhall", "tokens": ["Und", "bei", "dem", "ers\u00b7ten", "R\u00e4\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Eulenschrei! und tritt hervor", "tokens": ["Den", "Eu\u00b7len\u00b7schrei", "!", "und", "tritt", "her\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fracht, dann wiederholt den Schall:", "tokens": ["Die", "Fracht", ",", "dann", "wie\u00b7der\u00b7holt", "den", "Schall", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch naht Gefahr \u2014 Patrouillen gehn, \u2014", "tokens": ["Doch", "naht", "Ge\u00b7fahr", "Pat\u00b7rou\u00b7il\u00b7len", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NN", "$(", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Seht ihr die Landdragoner streifen,", "tokens": ["Seht", "ihr", "die", "Land\u00b7dra\u00b7go\u00b7ner", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann dreimal, wie von Riffesh\u00f6hn,", "tokens": ["Dann", "drei\u00b7mal", ",", "wie", "von", "Rif\u00b7fes\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00dft ihr den ", "tokens": ["La\u00dft", "ihr", "den"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "\u201enun, Rieder, noch ein Wort zu dir:", "tokens": ["\u201e", "nun", ",", "Rie\u00b7der", ",", "noch", "ein", "Wort", "zu", "dir", ":"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Recht hei\u00dft du der H\u00f6llenbrand;", "tokens": ["Mit", "Recht", "hei\u00dft", "du", "der", "H\u00f6l\u00b7len\u00b7brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein St\u00fcckchen \u2014 ich verbitt' es mir \u2014", "tokens": ["Kein", "St\u00fcck\u00b7chen", "ich", "ver\u00b7bitt'", "es", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie neulich mit der kalten Hand!\u201c", "tokens": ["Wie", "neu\u00b7lich", "mit", "der", "kal\u00b7ten", "Hand", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hauptmann spricht es; durch den Kreis", "tokens": ["Der", "Haupt\u00b7mann", "spricht", "es", ";", "durch", "den", "Kreis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Rauschen geht und feines Schwirren,", "tokens": ["Ein", "Rau\u00b7schen", "geht", "und", "fei\u00b7nes", "Schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als sie die B\u00fcchsen schultern leis,", "tokens": ["Als", "sie", "die", "B\u00fcch\u00b7sen", "schul\u00b7tern", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und in den Gurt die Messer klirren.", "tokens": ["Und", "in", "den", "Gurt", "die", "Mes\u00b7ser", "klir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seltsamer Tro\u00df! hier Riesenbau", "tokens": ["Selt\u00b7sa\u00b7mer", "Tro\u00df", "!", "hier", "Rie\u00b7sen\u00b7bau"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und hiebgespaltnes Angesicht,", "tokens": ["Und", "hieb\u00b7ge\u00b7spalt\u00b7nes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dort ein B\u00fcbchen wie 'ne Frau,", "tokens": ["Und", "dort", "ein", "B\u00fcb\u00b7chen", "wie", "'ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein zierliches Spelunkenlicht;", "tokens": ["Ein", "zier\u00b7li\u00b7ches", "Spe\u00b7lun\u00b7ken\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der dr\u00fcben an dem Scheitelhaar", "tokens": ["Der", "dr\u00fc\u00b7ben", "an", "dem", "Schei\u00b7tel\u00b7haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sachte streift den blanken F\u00e4nger,", "tokens": ["So", "sach\u00b7te", "streift", "den", "blan\u00b7ken", "F\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schaut aus den blauen Augen gar", "tokens": ["Schaut", "aus", "den", "blau\u00b7en", "Au\u00b7gen", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ein verarmter Minnes\u00e4nger.", "tokens": ["Wie", "ein", "ver\u00b7arm\u00b7ter", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "'s ist lichter Tag! die Bande scheut", "tokens": ["'s", "ist", "lich\u00b7ter", "Tag", "!", "die", "Ban\u00b7de", "scheut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vor keiner Stunde \u2014 Alles gleich; \u2014", "tokens": ["Vor", "kei\u00b7ner", "Stun\u00b7de", "Al\u00b7les", "gleich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "PIS", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist die rothe Bande, weit", "tokens": ["Es", "ist", "die", "ro\u00b7the", "Ban\u00b7de", ",", "weit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschrien, gef\u00fcrchtet in dem Reich;", "tokens": ["Ver\u00b7schri\u00b7en", ",", "ge\u00b7f\u00fcrch\u00b7tet", "in", "dem", "Reich", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das Kn\u00e4bchen kauert unter'm Stier", "tokens": ["Das", "Kn\u00e4b\u00b7chen", "kau\u00b7ert", "un\u00b7ter'm", "Stier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und betet, raschelt es im Walde,", "tokens": ["Und", "be\u00b7tet", ",", "ra\u00b7schelt", "es", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und manches Weib verschlie\u00dft die Th\u00fcr,", "tokens": ["Und", "man\u00b7ches", "Weib", "ver\u00b7schlie\u00dft", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schreit nur ein Kukuk an der Halde.", "tokens": ["Schreit", "nur", "ein", "Ku\u00b7kuk", "an", "der", "Hal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Posten haben sich zerstreut,", "tokens": ["Die", "Pos\u00b7ten", "ha\u00b7ben", "sich", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in die H\u00fctte schl\u00fcpft der Tro\u00df \u2014", "tokens": ["Und", "in", "die", "H\u00fct\u00b7te", "schl\u00fcpft", "der", "Tro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wildh\u00fcters Obdach, zu der Zeit,", "tokens": ["Wild\u00b7h\u00fc\u00b7ters", "Ob\u00b7dach", ",", "zu", "der", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Als jene Tr\u00fcmmer war ein Schlo\u00df:", "tokens": ["Als", "je\u00b7ne", "Tr\u00fcm\u00b7mer", "war", "ein", "Schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Ritter vor der Ahnengruft,", "tokens": ["Wie", "Rit\u00b7ter", "vor", "der", "Ah\u00b7nen\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchlt sich der R\u00e4uber stolz gehoben", "tokens": ["F\u00fchlt", "sich", "der", "R\u00e4u\u00b7ber", "stolz", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Am Schutte, dran ein gleicher Schuft", "tokens": ["Am", "Schut\u00b7te", ",", "dran", "ein", "glei\u00b7cher", "Schuft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor Jahren einst den Brand geschoben.", "tokens": ["Vor", "Jah\u00b7ren", "einst", "den", "Brand", "ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und als der letzte Schritt verhallt,", "tokens": ["Und", "als", "der", "letz\u00b7te", "Schritt", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der letzte Zweig zur\u00fcck gerauscht,", "tokens": ["Der", "letz\u00b7te", "Zweig", "zu\u00b7r\u00fcck", "ge\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wird es einsam in dem Wald,", "tokens": ["Da", "wird", "es", "ein\u00b7sam", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo \u00fcber'm Ast die Sonne lauscht;", "tokens": ["Wo", "\u00fc\u00b7ber'm", "Ast", "die", "Son\u00b7ne", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und als es drinnen noch geklirrt,", "tokens": ["Und", "als", "es", "drin\u00b7nen", "noch", "ge\u00b7klirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und noch ein Weilchen sich geschoben,", "tokens": ["Und", "noch", "ein", "Weil\u00b7chen", "sich", "ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure"