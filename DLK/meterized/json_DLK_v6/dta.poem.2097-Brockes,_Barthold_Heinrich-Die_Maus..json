{"dta.poem.2097": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Maus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df derselben mehrer\u2019 Arten noch gefunden werden", "tokens": ["Da\u00df", "der\u00b7sel\u00b7ben", "meh\u00b7rer'", "Ar\u00b7ten", "noch", "ge\u00b7fun\u00b7den", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "PIAT", "NN", "ADV", "VVPP", "VAINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "m\u00fcssen.", "tokens": ["m\u00fcs\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Werden wir von diesen Thieren nun gleich oftermals be-", "tokens": ["Wer\u00b7den", "wir", "von", "die\u00b7sen", "Thie\u00b7ren", "nun", "gleich", "of\u00b7ter\u00b7mals", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "schwert,", "tokens": ["schwert", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sind sie doch, in ihrer Art, unserer Bewundrung", "tokens": ["Sind", "sie", "doch", ",", "in", "ih\u00b7rer", "Art", ",", "un\u00b7se\u00b7rer", "Be\u00b7wund\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "werth;", "tokens": ["werth", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Auch wird, durch derselben Menge, manches andre Thier", "tokens": ["Auch", "wird", ",", "durch", "der\u00b7sel\u00b7ben", "Men\u00b7ge", ",", "man\u00b7ches", "and\u00b7re", "Thier"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PDAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "ern\u00e4hrt.", "tokens": ["er\u00b7n\u00e4hrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}