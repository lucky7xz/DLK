{"textgrid.poem.43624": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mairegen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solltest doch lieber ins H\u00e4uschen gehn!", "tokens": ["Soll\u00b7test", "doch", "lie\u00b7ber", "ins", "H\u00e4usc\u00b7hen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wirst ja am Ende ganz na\u00df.", "tokens": ["Wirst", "ja", "am", "En\u00b7de", "ganz", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wozu doch willst du im Regen stehn?", "tokens": ["Wo\u00b7zu", "doch", "willst", "du", "im", "Re\u00b7gen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sag, wozu n\u00fctzet dir das?", "tokens": ["Sag", ",", "wo\u00b7zu", "n\u00fct\u00b7zet", "dir", "das", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbmairegen macht, da\u00df man gr\u00f6\u00dfer wird:", "tokens": ["\u00bb", "mai\u00b7re\u00b7gen", "macht", ",", "da\u00df", "man", "gr\u00f6\u00b7\u00dfer", "wird", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gr\u00f6\u00dfer doch m\u00f6cht' ich gern sein.", "tokens": ["Gr\u00f6\u00b7\u00dfer", "doch", "m\u00f6cht'", "ich", "gern", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r' ich, o M\u00fctterchen, gro\u00df genug,", "tokens": ["W\u00e4r'", "ich", ",", "o", "M\u00fct\u00b7ter\u00b7chen", ",", "gro\u00df", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "ADJD", "ADV", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ging ich gewi\u00df nicht hinein.\u00ab", "tokens": ["Ging", "ich", "ge\u00b7wi\u00df", "nicht", "hin\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}}}}}