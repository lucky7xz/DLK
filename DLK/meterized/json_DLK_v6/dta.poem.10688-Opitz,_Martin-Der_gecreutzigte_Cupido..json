{"dta.poem.10688": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der gecreutzigte  Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "In dem betr\u00fcbten Ort der schwartzen Trawerfelder/", "tokens": ["In", "dem", "be\u00b7tr\u00fcb\u00b7ten", "Ort", "der", "schwart\u00b7zen", "Tra\u00b7wer\u00b7fel\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen Maro sagt/ da grosse Myrtenw\u00e4lder", "tokens": ["Von", "de\u00b7nen", "Ma\u00b7ro", "sagt", "/", "da", "gros\u00b7se", "Myr\u00b7ten\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "NE", "VVFIN", "$(", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die armen bulerin vmbringen mit der Nacht/", "tokens": ["Die", "ar\u00b7men", "bu\u00b7le\u00b7rin", "vm\u00b7brin\u00b7gen", "mit", "der", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sah ich die Heldinnen die selbst sich vmbgebracht.", "tokens": ["Sah", "ich", "die", "Hel\u00b7din\u00b7nen", "die", "selbst", "sich", "vmb\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADV", "PRF", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich sah ein jegliche dasselbe thun vorgeben/", "tokens": ["Ich", "sah", "ein", "jeg\u00b7li\u00b7che", "das\u00b7sel\u00b7be", "thun", "vor\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "PDAT", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie sie jhr vor der Zeit genommen hett jhr Leben/", "tokens": ["Wie", "sie", "jhr", "vor", "der", "Zeit", "ge\u00b7nom\u00b7men", "hett", "jhr", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der dicke w\u00fcste Wald war Sonn vnd Mondes blo\u00df/", "tokens": ["Der", "di\u00b7cke", "w\u00fcs\u00b7te", "Wald", "war", "Sonn", "vnd", "Mon\u00b7des", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das schrecklich h\u00f6llensee grundlo\u00df ohn rauschen flo\u00df;", "tokens": ["Das", "schreck\u00b7lich", "h\u00f6l\u00b7len\u00b7see", "grund\u00b7lo\u00df", "ohn", "rau\u00b7schen", "flo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An dessen stillen Bach man kaum herf\u00fcr sicht schiessen", "tokens": ["An", "des\u00b7sen", "stil\u00b7len", "Bach", "man", "kaum", "her\u00b7f\u00fcr", "sicht", "schies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PIS", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Hiacynthen blum/ vnd fl\u00fcchtigen Narcissen/", "tokens": ["Die", "Hia\u00b7cyn\u00b7then", "blum", "/", "vnd", "fl\u00fcch\u00b7ti\u00b7gen", "Nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "KON", "ADJA", "NN", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "Adonis Purpurfarb/ vnd Crocus gelbes Kleidt/", "tokens": ["A\u00b7do\u00b7nis", "Pur\u00b7purf\u00b7arb", "/", "vnd", "Cro\u00b7cus", "gel\u00b7bes", "Kleidt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "KON", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch Aeas stund aldar verh\u00fcllt mit Trawrigkeit.", "tokens": ["Auch", "A\u00b7e\u00b7as", "stund", "al\u00b7dar", "ver\u00b7h\u00fcllt", "mit", "Traw\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Die sorgen ohne Ruh voll zehren/ leidt/ vnd krencken", "tokens": ["Die", "sor\u00b7gen", "oh\u00b7ne", "Ruh", "voll", "zeh\u00b7ren", "/", "leidt", "/", "vnd", "kren\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVINF", "$(", "ADJD", "$(", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bewegen widerumb die Heldinen zu dencken/", "tokens": ["Be\u00b7we\u00b7gen", "wi\u00b7de\u00b7rumb", "die", "Hel\u00b7di\u00b7nen", "zu", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Was sie zu vor alhier in solche Noth gest\u00fcrtzt/", "tokens": ["Was", "sie", "zu", "vor", "al\u00b7hier", "in", "sol\u00b7che", "Noth", "ge\u00b7st\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "APPR", "ADV", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df sie mit eygner Hand jhr Leben abgek\u00fcrtzt.", "tokens": ["Da\u00df", "sie", "mit", "eyg\u00b7ner", "Hand", "jhr", "Le\u00b7ben", "ab\u00b7ge\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Semele beklagt wie Juno sie betrogen/", "tokens": ["Die", "Se\u00b7me\u00b7le", "be\u00b7klagt", "wie", "Ju\u00b7no", "sie", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df sie durch falsch Geschenck im Fewer vffgeflogen/", "tokens": ["Da\u00df", "sie", "durch", "falsch", "Ge\u00b7schenck", "im", "Fe\u00b7wer", "vff\u00b7ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als Jupiter zu jhr mit Plitz vnd Donner kam/", "tokens": ["Als", "Ju\u00b7pi\u00b7ter", "zu", "jhr", "mit", "Plitz", "vnd", "Don\u00b7ner", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "APPR", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vnd Bacchum in sein H\u00fcfft au\u00df jhrem Leibe nam.", "tokens": ["Vnd", "Bac\u00b7chum", "in", "sein", "H\u00fcfft", "au\u00df", "jhrem", "Lei\u00b7be", "nam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Die Coenis so zum Mann au\u00df einer Jungfraw worden/", "tokens": ["Die", "Coe\u00b7nis", "so", "zum", "Mann", "au\u00df", "ei\u00b7ner", "Jung\u00b7fraw", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weint/ da\u00df sie widerumb sey in dem Weiber Orden.", "tokens": ["Weint", "/", "da\u00df", "sie", "wi\u00b7de\u00b7rumb", "sey", "in", "dem", "Wei\u00b7ber", "Or\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Procris trucknet noch die Wunden/ vnd gibt acht", "tokens": ["Die", "Pro\u00b7cris", "truck\u00b7net", "noch", "die", "Wun\u00b7den", "/", "vnd", "gibt", "acht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$(", "KON", "VVFIN", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auff jhren Cephalum/ der doch sie vmbgebracht.", "tokens": ["Auff", "jhren", "Ce\u00b7pha\u00b7lum", "/", "der", "doch", "sie", "vmb\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Die Hero hat noch jetzt die Lamp in jhren Henden/", "tokens": ["Die", "He\u00b7ro", "hat", "noch", "jetzt", "die", "Lamp", "in", "jhren", "Hen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Nach welcher sich bey Nacht Leander mu\u00dfte wenden.", "tokens": ["Nach", "wel\u00b7cher", "sich", "bey", "Nacht", "Le\u00b7an\u00b7der", "mu\u00df\u00b7te", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "APPR", "NN", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Sappho jmmer zu vom weissen Felsen springt/", "tokens": ["Die", "Sap\u00b7pho", "jm\u00b7mer", "zu", "vom", "weis\u00b7sen", "Fel\u00b7sen", "springt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Eriphyle auch noch mit dem Tode ringt.", "tokens": ["Die", "E\u00b7ri\u00b7phy\u00b7le", "auch", "noch", "mit", "dem", "To\u00b7de", "ringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was Minos hat gethan/ wie Creta sey gestanden/", "tokens": ["Was", "Mi\u00b7nos", "hat", "ge\u00b7than", "/", "wie", "Cre\u00b7ta", "sey", "ge\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$(", "KOKOM", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ist alles dunckler wei\u00df alda gemahlt vorhanden/", "tokens": ["Ist", "al\u00b7les", "dunck\u00b7ler", "wei\u00df", "al\u00b7da", "ge\u00b7mahlt", "vor\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "VVFIN", "PAV", "VVPP", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Pasiphae die laufft dem weissen Ochsen nach/", "tokens": ["Pa\u00b7si\u00b7phae", "die", "laufft", "dem", "weis\u00b7sen", "Och\u00b7sen", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "Die Ariadne klagt von Theseus vngemach:", "tokens": ["Die", "A\u00b7riad\u00b7ne", "klagt", "von", "The\u00b7seus", "vn\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.33": {"text": "Die Ph", "tokens": ["Die", "Ph"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Dardurch Sie zu der Hult wolt jhren S