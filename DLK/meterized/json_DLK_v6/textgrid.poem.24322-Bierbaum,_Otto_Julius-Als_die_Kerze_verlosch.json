{"textgrid.poem.24322": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Als die Kerze verlosch", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Licht, l\u00f6sch aus!", "tokens": ["Licht", ",", "l\u00f6sch", "aus", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Dunkel sei mein Haus.", "tokens": ["Dun\u00b7kel", "sei", "mein", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dunkel um mich,", "tokens": ["Dun\u00b7kel", "um", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dunkel in mir;", "tokens": ["Dun\u00b7kel", "in", "mir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Still, Herz: Dich", "tokens": ["Still", ",", "Herz", ":", "Dich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "NN", "$.", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Kennt niemand hier.", "tokens": ["Kennt", "nie\u00b7mand", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Niemand wei\u00df um deine,", "tokens": ["Nie\u00b7mand", "wei\u00df", "um", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Niemand wei\u00df um meine", "tokens": ["Nie\u00b7mand", "wei\u00df", "um", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tiefe Dunkelheit.", "tokens": ["Tie\u00b7fe", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie sehr wir beide verdorben,", "tokens": ["Wie", "sehr", "wir", "bei\u00b7de", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und da\u00df wir schon gestorben:", "tokens": ["Und", "da\u00df", "wir", "schon", "ge\u00b7stor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wem ist es bewu\u00dft? Wem w\u00e4r es leid?", "tokens": ["Wem", "ist", "es", "be\u00b7wu\u00dft", "?", "Wem", "w\u00e4r", "es", "leid", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "$.", "PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber wir leben ja! Leben", "tokens": ["A\u00b7ber", "wir", "le\u00b7ben", "ja", "!", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Klarer als je!", "tokens": ["Kla\u00b7rer", "als", "je", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das ist es eben:", "tokens": ["Das", "ist", "es", "e\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das tut zum Sterben weh:", "tokens": ["Das", "tut", "zum", "Ster\u00b7ben", "weh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wir, zu tr\u00e4umen so wohlgeboren,", "tokens": ["Da\u00df", "wir", ",", "zu", "tr\u00e4u\u00b7men", "so", "wohl\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PTKZU", "VVINF", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Gl\u00fcck des Traumes ach, verloren", "tokens": ["Das", "Gl\u00fcck", "des", "Trau\u00b7mes", "ach", ",", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "XY", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nichts daf\u00fcr gewonnen haben,", "tokens": ["Und", "nichts", "da\u00b7f\u00fcr", "ge\u00b7won\u00b7nen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als die verfluchteste von allen Erdengaben:", "tokens": ["Als", "die", "ver\u00b7fluch\u00b7tes\u00b7te", "von", "al\u00b7len", "Er\u00b7den\u00b7ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu sehen, da\u00df wir nicht zu Haus hier sind.", "tokens": ["Zu", "se\u00b7hen", ",", "da\u00df", "wir", "nicht", "zu", "Haus", "hier", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wohl, Herz, wir waren nicht dem Leben blind,", "tokens": ["Wohl", ",", "Herz", ",", "wir", "wa\u00b7ren", "nicht", "dem", "Le\u00b7ben", "blind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nun aber sind wir sehend tot", "tokens": ["Nun", "a\u00b7ber", "sind", "wir", "se\u00b7hend", "tot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und leiden bittre Heimwehnot", "tokens": ["Und", "lei\u00b7den", "bitt\u00b7re", "Heim\u00b7weh\u00b7not"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nach dem verlorenen Paradies,", "tokens": ["Nach", "dem", "ver\u00b7lo\u00b7re\u00b7nen", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Das wohl den andern Wahnsinn hie\u00df,", "tokens": ["Das", "wohl", "den", "an\u00b7dern", "Wahn\u00b7sinn", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Uns aber s\u00fc\u00dfe Heimat war;", "tokens": ["Uns", "a\u00b7ber", "s\u00fc\u00b7\u00dfe", "Hei\u00b7mat", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Land in Dunkelheiten klar.", "tokens": ["Ein", "Land", "in", "Dun\u00b7kel\u00b7hei\u00b7ten", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wolln wir nicht gehn, es wieder suchen?", "tokens": ["Wolln", "wir", "nicht", "gehn", ",", "es", "wie\u00b7der", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVINF", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei stark, Herz, komm: la\u00df uns selbsts\u00fcchtig sein!", "tokens": ["Sei", "stark", ",", "Herz", ",", "komm", ":", "la\u00df", "uns", "selbst\u00b7s\u00fcch\u00b7tig", "sein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "VVFIN", "$.", "VVIMP", "PPER", "ADJD", "VAINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur Gl\u00fcck ist Wert. Jetzt ist nichts mein und dein", "tokens": ["Nur", "Gl\u00fcck", "ist", "Wert", ".", "Jetzt", "ist", "nichts", "mein", "und", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "NE", "$.", "ADV", "VAFIN", "PIS", "PPOSAT", "KON", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als \u00f6des, widerliches Selbstverfluchen.", "tokens": ["Als", "\u00f6\u00b7des", ",", "wi\u00b7der\u00b7li\u00b7ches", "Selbst\u00b7ver\u00b7flu\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir sind am Leben krank, Herz. Auf ins Leere", "tokens": ["Wir", "sind", "am", "Le\u00b7ben", "krank", ",", "Herz", ".", "Auf", "ins", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "$,", "NN", "$.", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der holden L\u00fcge, deren Meister wir", "tokens": ["Der", "hol\u00b7den", "L\u00fc\u00b7ge", ",", "de\u00b7ren", "Meis\u00b7ter", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Diener sind. Das Wahre ist das Schwere,", "tokens": ["Und", "Die\u00b7ner", "sind", ".", "Das", "Wah\u00b7re", "ist", "das", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$.", "ART", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wir, Herz, wir sind nicht zum Tragen hier.", "tokens": ["Und", "wir", ",", "Herz", ",", "wir", "sind", "nicht", "zum", "Tra\u00b7gen", "hier", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir wollen schweben, schweben, bis wir fallen", "tokens": ["Wir", "wol\u00b7len", "schwe\u00b7ben", ",", "schwe\u00b7ben", ",", "bis", "wir", "fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum letzten Traum: dem seligsten von allen.", "tokens": ["Zum", "letz\u00b7ten", "Traum", ":", "dem", "se\u00b7ligs\u00b7ten", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ART", "ADJA", "APPR", "PIAT", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}