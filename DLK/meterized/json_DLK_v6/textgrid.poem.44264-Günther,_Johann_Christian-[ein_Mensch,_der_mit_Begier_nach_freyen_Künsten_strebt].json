{"textgrid.poem.44264": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ein Mensch, der mit Begier nach freyen K\u00fcnsten strebt]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch, der mit Begier nach freyen K\u00fcnsten strebt,", "tokens": ["Ein", "Mensch", ",", "der", "mit", "Be\u00b7gier", "nach", "frey\u00b7en", "K\u00fcns\u00b7ten", "strebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt wohl, so lang er noch auf hohen Schulen lebt,", "tokens": ["Tr\u00e4gt", "wohl", ",", "so", "lang", "er", "noch", "auf", "ho\u00b7hen", "Schu\u00b7len", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "ADJD", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor seinen Wechselbrief die allergr\u00f6sten Sorgen;", "tokens": ["Vor", "sei\u00b7nen", "Wech\u00b7sel\u00b7brief", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7ten", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Kummer, den er hat, ist seiner Eltern Geld,", "tokens": ["Der", "Kum\u00b7mer", ",", "den", "er", "hat", ",", "ist", "sei\u00b7ner", "El\u00b7tern", "Geld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zumahl wenn ihm der Muth aus leeren Beutheln f\u00e4llt", "tokens": ["Zu\u00b7mahl", "wenn", "ihm", "der", "Muth", "aus", "lee\u00b7ren", "Beut\u00b7heln", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Koch und Schneider ihm die lezte Nothdurft borgen.", "tokens": ["Und", "Koch", "und", "Schnei\u00b7der", "ihm", "die", "lez\u00b7te", "Noth\u00b7durft", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat er nunmehr verthan, was ihm der Mutter Hand,", "tokens": ["Hat", "er", "nun\u00b7mehr", "ver\u00b7than", ",", "was", "ihm", "der", "Mut\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "PWS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sie den lieben Sohn mit Thr\u00e4nen fortgesand,", "tokens": ["Als", "sie", "den", "lie\u00b7ben", "Sohn", "mit", "Thr\u00e4\u00b7nen", "fort\u00b7ge\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verstohlen auf der Flucht in alle Ficken steckte,", "tokens": ["Ver\u00b7stoh\u00b7len", "auf", "der", "Flucht", "in", "al\u00b7le", "Fi\u00b7cken", "steck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So geht die Noth an Mann, so st\u00fczt er Kopf und Arm,", "tokens": ["So", "geht", "die", "Noth", "an", "Mann", ",", "so", "st\u00fczt", "er", "Kopf", "und", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verriegelt Th\u00fcr und Schlo\u00df, macht Stuhl und Se\u00dfel warm", "tokens": ["Ver\u00b7rie\u00b7gelt", "Th\u00fcr", "und", "Schlo\u00df", ",", "macht", "Stuhl", "und", "Se\u00b7\u00dfel", "warm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sizt so finster da, als wenn er Grillen heckte.", "tokens": ["Und", "sizt", "so", "fins\u00b7ter", "da", ",", "als", "wenn", "er", "Gril\u00b7len", "heck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er lauft den Kutschen zu, die er beladen sp\u00fcrt,", "tokens": ["Er", "lauft", "den", "Kut\u00b7schen", "zu", ",", "die", "er", "be\u00b7la\u00b7den", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rennt t\u00e4glich auf die Post und list und buchstabirt", "tokens": ["Rennt", "t\u00e4g\u00b7lich", "auf", "die", "Post", "und", "list", "und", "buch\u00b7sta\u00b7birt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast vierundzwanzigmahl die Nahmen aller Charten.", "tokens": ["Fast", "vie\u00b7rund\u00b7zwan\u00b7zig\u00b7mahl", "die", "Nah\u00b7men", "al\u00b7ler", "Char\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwey Fest im Jahre sind, die ihn gewis erfreun:", "tokens": ["Zwey", "Fest", "im", "Jah\u00b7re", "sind", ",", "die", "ihn", "ge\u00b7wis", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er weis, da\u00df Michael und Ostern Tr\u00f6ster seyn;", "tokens": ["Er", "weis", ",", "da\u00df", "Mic\u00b7hael", "und", "Os\u00b7tern", "Tr\u00f6s\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "NE", "KON", "NN", "NN", "VAINF", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Nur dieses thut ihm weh: Er soll so lange warthen.", "tokens": ["Nur", "die\u00b7ses", "thut", "ihm", "weh", ":", "Er", "soll", "so", "lan\u00b7ge", "wart\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hingegen schickt es sich, da\u00df der verlangte Gast,", "tokens": ["Hin\u00b7ge\u00b7gen", "schickt", "es", "sich", ",", "da\u00df", "der", "ver\u00b7lang\u00b7te", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den er so wie ein B\u00e4r den Bienenzucker hast,", "tokens": ["Den", "er", "so", "wie", "ein", "B\u00e4r", "den", "Bie\u00b7nen\u00b7zu\u00b7cker", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "KOKOM", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Hause wieder kommt und wilde M\u00e4nner bringet,", "tokens": ["Von", "Hau\u00b7se", "wie\u00b7der", "kommt", "und", "wil\u00b7de", "M\u00e4n\u00b7ner", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wacht und lebt der Pursch, da zieht der Kummer aus,", "tokens": ["Da", "wacht", "und", "lebt", "der", "Pursch", ",", "da", "zieht", "der", "Kum\u00b7mer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und es erf\u00e4hrt es oft des sechsten Nachbars Haus,", "tokens": ["Und", "es", "er\u00b7f\u00e4hrt", "es", "oft", "des", "sechs\u00b7ten", "Nach\u00b7bars", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie seine Fr\u00f6hligkeit durch alle Fenster singet.", "tokens": ["Wie", "sei\u00b7ne", "Fr\u00f6h\u00b7lig\u00b7keit", "durch", "al\u00b7le", "Fens\u00b7ter", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir wi\u00dfen, Seeligster, da\u00df jezt die Tadelsucht", "tokens": ["Wir", "wi\u00b7\u00dfen", ",", "See\u00b7ligs\u00b7ter", ",", "da\u00df", "jezt", "die", "Ta\u00b7del\u00b7sucht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies ungereimte Blat, wie sie es nennt, verflucht,", "tokens": ["Dies", "un\u00b7ge\u00b7reim\u00b7te", "Blat", ",", "wie", "sie", "es", "nennt", ",", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil es bey deiner Gruft, um die es weinen sollte,", "tokens": ["Weil", "es", "bey", "dei\u00b7ner", "Gruft", ",", "um", "die", "es", "wei\u00b7nen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit halbem Scherz erscheint. Doch wi\u00dfen wir auch dies:", "tokens": ["Mit", "hal\u00b7bem", "Scherz", "er\u00b7scheint", ".", "Doch", "wi\u00b7\u00dfen", "wir", "auch", "dies", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "KON", "VVFIN", "PPER", "ADV", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du selber billigst es und straftest den gewis,", "tokens": ["Du", "sel\u00b7ber", "bil\u00b7ligst", "es", "und", "straf\u00b7test", "den", "ge\u00b7wis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der deinen Vortheil nun mit Thr\u00e4nen st\u00f6ren wollte.", "tokens": ["Der", "dei\u00b7nen", "Vor\u00b7theil", "nun", "mit", "Thr\u00e4\u00b7nen", "st\u00f6\u00b7ren", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn jezt, nachdem dein Fu\u00df, der hurtig Abschied nahm", "tokens": ["Denn", "jezt", ",", "nach\u00b7dem", "dein", "Fu\u00df", ",", "der", "hur\u00b7tig", "Ab\u00b7schied", "nahm"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohn Verhofen nechst geschw\u00e4cht zur\u00fccke kam,", "tokens": ["Und", "ohn", "Ver\u00b7ho\u00b7fen", "nechst", "ge\u00b7schw\u00e4cht", "zu\u00b7r\u00fc\u00b7cke", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Weg nach Halle nimmt, den Wechsel dort zu heben,", "tokens": ["Den", "Weg", "nach", "Hal\u00b7le", "nimmt", ",", "den", "Wech\u00b7sel", "dort", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So zahlt des Himmels Gunst dir durch des Todes Hand", "tokens": ["So", "zahlt", "des", "Him\u00b7mels", "Gunst", "dir", "durch", "des", "To\u00b7des", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch einen reichern aus, als deine Reise fand,", "tokens": ["Noch", "ei\u00b7nen", "rei\u00b7chern", "aus", ",", "als", "dei\u00b7ne", "Rei\u00b7se", "fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die dir Gelegenheit zur lezten Fahrt gegeben.", "tokens": ["Die", "dir", "Ge\u00b7le\u00b7gen\u00b7heit", "zur", "lez\u00b7ten", "Fahrt", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dies ist der Gnadenlohn der Zeiten ohne Zeit,", "tokens": ["Dies", "ist", "der", "Gna\u00b7den\u00b7lohn", "der", "Zei\u00b7ten", "oh\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Reichthum jener Welt, allwo die Eitelkeit", "tokens": ["Der", "Reicht\u00b7hum", "je\u00b7ner", "Welt", ",", "all\u00b7wo", "die", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht b\u00f6se Wahren f\u00fchrt, noch falsche M\u00fcnze pr\u00e4get.", "tokens": ["Nicht", "b\u00f6\u00b7se", "Wah\u00b7ren", "f\u00fchrt", ",", "noch", "fal\u00b7sche", "M\u00fcn\u00b7ze", "pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lage, so du giebst, ist warlich klein und schlecht,", "tokens": ["Die", "La\u00b7ge", ",", "so", "du", "giebst", ",", "ist", "war\u00b7lich", "klein", "und", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"