{"textgrid.poem.60783": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ohnmacht, Zerst\u00fccklung, jegliche herbe Schmach", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohnmacht, Zerst\u00fccklung, jegliche herbe Schmach", "tokens": ["Ohn\u00b7macht", ",", "Zer\u00b7st\u00fcck\u00b7lung", ",", "jeg\u00b7li\u00b7che", "her\u00b7be", "Schmach"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "War unser Los, seitdem du Germaniens", "tokens": ["War", "un\u00b7ser", "Los", ",", "seit\u00b7dem", "du", "Ger\u00b7ma\u00b7ni\u00b7ens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Reichsapfel nicht mehr wiegst in deiner", "tokens": ["Reichs\u00b7ap\u00b7fel", "nicht", "mehr", "wiegst", "in", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rechten, o Herr, und von uns verlassen,", "tokens": ["Rech\u00b7ten", ",", "o", "Herr", ",", "und", "von", "uns", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "KON", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Uns alle preisgabst schimpflichem Untergang!", "tokens": ["Uns", "al\u00b7le", "preis\u00b7gabst", "schimpf\u00b7li\u00b7chem", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wohl tat Erneurung unserem Reiche not,", "tokens": ["Wohl", "tat", "Er\u00b7neu\u00b7rung", "un\u00b7se\u00b7rem", "Rei\u00b7che", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch nicht Zerst\u00f6rung; tief im Busen", "tokens": ["Doch", "nicht", "Zer\u00b7st\u00f6\u00b7rung", ";", "tief", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "NN", "$.", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trug es den edelsten Keim der Freiheit.", "tokens": ["Trug", "es", "den", "e\u00b7dels\u00b7ten", "Keim", "der", "Frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Du zeihst des Abfalls uns, des Verrats mit Recht;", "tokens": ["Du", "zeihst", "des", "Ab\u00b7falls", "uns", ",", "des", "Ver\u00b7rats", "mit", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wir zeihen dich, da\u00df \u00fcber die Alpen stets", "tokens": ["Wir", "zei\u00b7hen", "dich", ",", "da\u00df", "\u00fc\u00b7ber", "die", "Al\u00b7pen", "stets"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Aug gekehrt war, da\u00df du V\u00f6lker,", "tokens": ["Dein", "Aug", "ge\u00b7kehrt", "war", ",", "da\u00df", "du", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deinem Germanien fremd, beherrschtest!", "tokens": ["Dei\u00b7nem", "Ger\u00b7ma\u00b7ni\u00b7en", "fremd", ",", "be\u00b7herrschtest", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Einst griff sogar nach spanischem Ehering", "tokens": ["Einst", "griff", "so\u00b7gar", "nach", "spa\u00b7ni\u00b7schem", "E\u00b7he\u00b7ring"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Habgierig \u00d6streich; doch es erwarb sich nur", "tokens": ["Hab\u00b7gie\u00b7rig", "\u00d6s\u00b7treich", ";", "doch", "es", "er\u00b7warb", "sich", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$.", "KON", "PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Deutschlands Verlust. Sein f\u00fcnfter Karl war", "tokens": ["Deutschlands", "Ver\u00b7lust", ".", "Sein", "f\u00fcnf\u00b7ter", "Karl", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPOSAT", "ADJA", "NE", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Verderben und ganz Europas!", "tokens": ["Un\u00b7ser", "Ver\u00b7der\u00b7ben", "und", "ganz", "Eu\u00b7ro\u00b7pas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Jedwedes Unheil, welches die Welt betraf,", "tokens": ["Jed\u00b7we\u00b7des", "Un\u00b7heil", ",", "wel\u00b7ches", "die", "Welt", "be\u00b7traf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Flo\u00df aus der Brust ehrs\u00fcchtiger K\u00f6nige,", "tokens": ["Flo\u00df", "aus", "der", "Brust", "ehr\u00b7s\u00fcch\u00b7ti\u00b7ger", "K\u00f6\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die unbefriedigt durch das Erbteil", "tokens": ["Die", "un\u00b7be\u00b7frie\u00b7digt", "durch", "das", "Erb\u00b7teil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihres Geschlechts in die Fremde schweiften.", "tokens": ["Ih\u00b7res", "Ge\u00b7schlechts", "in", "die", "Frem\u00b7de", "schweif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Vergebens hoffst du, da\u00df der Lombarde je", "tokens": ["Ver\u00b7ge\u00b7bens", "hoffst", "du", ",", "da\u00df", "der", "Lom\u00b7bar\u00b7de", "je"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dich lieben lernt, da\u00df je es der Pole lernt!", "tokens": ["Dich", "lie\u00b7ben", "lernt", ",", "da\u00df", "je", "es", "der", "Po\u00b7le", "lernt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl schleifte Mailand Barbarossa,", "tokens": ["Wohl", "schleif\u00b7te", "Mai\u00b7land", "Bar\u00b7ba\u00b7ros\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber es blutete Konradin auch.", "tokens": ["A\u00b7ber", "es", "blu\u00b7te\u00b7te", "Kon\u00b7ra\u00b7din", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Gieb deinem Deutschland wieder ein deutsches Herz!", "tokens": ["Gieb", "dei\u00b7nem", "Deutschland", "wie\u00b7der", "ein", "deut\u00b7sches", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann wird, f\u00fcrwahr, frohlockenden Jubelrufs", "tokens": ["Dann", "wird", ",", "f\u00fcr\u00b7wahr", ",", "froh\u00b7lo\u00b7cken\u00b7den", "Ju\u00b7bel\u00b7rufs"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein wahres Volk aufnehmen seinen", "tokens": ["Dein", "wah\u00b7res", "Volk", "auf\u00b7neh\u00b7men", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alten und kummergebeugten Kaiser!", "tokens": ["Al\u00b7ten", "und", "kum\u00b7mer\u00b7ge\u00b7beug\u00b7ten", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wer Sklave Moskaus w\u00fcnschte zu sein, er bleib's!", "tokens": ["Wer", "Skla\u00b7ve", "Mos\u00b7kaus", "w\u00fcnschte", "zu", "sein", ",", "er", "bleib's", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "NE", "VVFIN", "PTKZU", "VAINF", "$,", "PPER", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir m\u00f6chten frei sein, einig und gro\u00df; zu uns,", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "frei", "sein", ",", "ei\u00b7nig", "und", "gro\u00df", ";", "zu", "uns", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "ADJD", "KON", "ADJD", "$.", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die dein in Sehnsucht t\u00e4glich warten,", "tokens": ["Die", "dein", "in", "Sehn\u00b7sucht", "t\u00e4g\u00b7lich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kehre zur\u00fcck, o geliebter K\u00f6nig!", "tokens": ["Keh\u00b7re", "zu\u00b7r\u00fcck", ",", "o", "ge\u00b7lieb\u00b7ter", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Baschkireneinfall halte von uns entfernt;", "tokens": ["Baschki\u00b7ren\u00b7ein\u00b7fall", "hal\u00b7te", "von", "uns", "ent\u00b7fernt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dann beut in Freundschaft deinem erneuten Volk", "tokens": ["Dann", "beut", "in", "Freund\u00b7schaft", "dei\u00b7nem", "er\u00b7neu\u00b7ten", "Volk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das neue Frankreich auch den Handschlag", "tokens": ["Das", "neu\u00b7e", "Fran\u00b7kreich", "auch", "den", "Hand\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00dcber dem heiligen Sarg in Aachen.", "tokens": ["\u00dc\u00b7ber", "dem", "hei\u00b7li\u00b7gen", "Sarg", "in", "Aa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}