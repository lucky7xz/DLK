{"dta.poem.16207": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "261.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird mit Recht gesagt Markt der Literatur;", "tokens": ["Es", "wird", "mit", "Recht", "ge\u00b7sagt", "Markt", "der", "Li\u00b7te\u00b7ra\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn sie vergleichet sich mit einem Markte nur.", "tokens": ["Denn", "sie", "ver\u00b7glei\u00b7chet", "sich", "mit", "ei\u00b7nem", "Mark\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie auf dem Markte stehn zum Kaufe Waaren feil,", "tokens": ["Wie", "auf", "dem", "Mark\u00b7te", "stehn", "zum", "Kau\u00b7fe", "Waa\u00b7ren", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder nach Bedarf nimmt davon einen Theil:", "tokens": ["Und", "je\u00b7der", "nach", "Be\u00b7darf", "nimmt", "da\u00b7von", "ei\u00b7nen", "Theil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eine w\u00e4hlt sich dis, der andre das vom Haufen,", "tokens": ["Der", "ei\u00b7ne", "w\u00e4hlt", "sich", "dis", ",", "der", "and\u00b7re", "das", "vom", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "PDS", "$,", "PRELS", "PIS", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch keinem f\u00e4llt es ein, den ganzen Markt zu kaufen:", "tokens": ["Doch", "kei\u00b7nem", "f\u00e4llt", "es", "ein", ",", "den", "gan\u00b7zen", "Markt", "zu", "kau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So auch wer k\u00f6nnte jetzt sich noch einfallen lassen,", "tokens": ["So", "auch", "wer", "k\u00f6nn\u00b7te", "jetzt", "sich", "noch", "ein\u00b7fal\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWS", "VMFIN", "ADV", "PRF", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich mit Literatur der ganzen zu befassen?", "tokens": ["Sich", "mit", "Li\u00b7te\u00b7ra\u00b7tur", "der", "gan\u00b7zen", "zu", "be\u00b7fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Der greift sich hier ein St\u00fcck, der eines dort heraus,", "tokens": ["Der", "greift", "sich", "hier", "ein", "St\u00fcck", ",", "der", "ei\u00b7nes", "dort", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ART", "NN", "$,", "PRELS", "ART", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach eigenem Geschmack und zum Verbrauch im Haus.", "tokens": ["Nach", "ei\u00b7ge\u00b7nem", "Ge\u00b7schmack", "und", "zum", "Ver\u00b7brauch", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Der Zufall waltet, wo am Urteil es gebricht,", "tokens": ["Der", "Zu\u00b7fall", "wal\u00b7tet", ",", "wo", "am", "Ur\u00b7teil", "es", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Gew\u00fchl ist ganz unm\u00f6glich Uebersicht.", "tokens": ["Und", "im", "Ge\u00b7w\u00fchl", "ist", "ganz", "un\u00b7m\u00f6g\u00b7lich", "Ue\u00b7ber\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Unmerklich unter'm Glanz der ausgestellten G\u00fcter", "tokens": ["Un\u00b7mer\u00b7klich", "un\u00b7ter'm", "Glanz", "der", "aus\u00b7ge\u00b7stell\u00b7ten", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wird an den Mann gebracht auch mancher Ladenh\u00fcter.", "tokens": ["Wird", "an", "den", "Mann", "ge\u00b7bracht", "auch", "man\u00b7cher", "La\u00b7den\u00b7h\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Heut hat den Zulauf der, den andere beneiden,", "tokens": ["Heut", "hat", "den", "Zu\u00b7lauf", "der", ",", "den", "an\u00b7de\u00b7re", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die morgen am Verfall sich seines Krames weiden.", "tokens": ["Die", "mor\u00b7gen", "am", "Ver\u00b7fall", "sich", "sei\u00b7nes", "Kra\u00b7mes", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Es bietet kurzen Ruhm mit ungewissem Brode", "tokens": ["Es", "bie\u00b7tet", "kur\u00b7zen", "Ruhm", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Bro\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der \u00fcberf\u00fcllte Markt mit wechselhafter Mode.", "tokens": ["Der", "\u00fc\u00b7berf\u00b7\u00fcll\u00b7te", "Markt", "mit", "wech\u00b7sel\u00b7haf\u00b7ter", "Mo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}