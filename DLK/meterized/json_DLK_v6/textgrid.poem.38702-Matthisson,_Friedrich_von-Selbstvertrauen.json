{"textgrid.poem.38702": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Selbstvertrauen", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann gilts, da\u00df er mit Selbstvertraun sich r\u00fcste", "tokens": ["Dann", "gilts", ",", "da\u00df", "er", "mit", "Selbst\u00b7ver\u00b7traun", "sich", "r\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NE", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mann, wenn er allein im Kampfe steht,", "tokens": ["Der", "Mann", ",", "wenn", "er", "al\u00b7lein", "im", "Kamp\u00b7fe", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, als ob unverwundbar er sich w\u00fc\u00dfte,", "tokens": ["Und", ",", "als", "ob", "un\u00b7ver\u00b7wund\u00b7bar", "er", "sich", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOKOM", "KOUS", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Andrang der Gefahr entgegengeht;", "tokens": ["Dem", "An\u00b7drang", "der", "Ge\u00b7fahr", "ent\u00b7ge\u00b7gen\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn gleich, wie Abschiedswink von ferner K\u00fcste,", "tokens": ["Wenn", "gleich", ",", "wie", "Ab\u00b7schieds\u00b7wink", "von", "fer\u00b7ner", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PWAV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kaum sichtbar noch der Hoffnung Schleier weht.", "tokens": ["Kaum", "sicht\u00b7bar", "noch", "der", "Hoff\u00b7nung", "Schlei\u00b7er", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umringten, wie Gigantenschaaren,", "tokens": ["Um\u00b7ring\u00b7ten", ",", "wie", "Gi\u00b7gan\u00b7ten\u00b7schaa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dichtgeschlo\u00dfnem Phalanx, ihn", "tokens": ["In", "dicht\u00b7ge\u00b7schlo\u00df\u00b7nem", "Pha\u00b7lanx", ",", "ihn"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des ganzen Tartarus Gefahren,", "tokens": ["Des", "gan\u00b7zen", "Tar\u00b7ta\u00b7rus", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Er bliebe kalt, und fest und k\u00fchn:", "tokens": ["Er", "blie\u00b7be", "kalt", ",", "und", "fest", "und", "k\u00fchn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn Selbstvertraun, die\u00df G\u00f6tterkind, entschiede", "tokens": ["Denn", "Selbst\u00b7ver\u00b7traun", ",", "die\u00df", "G\u00f6t\u00b7ter\u00b7kind", ",", "ent\u00b7schie\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NE", "$,", "PDS", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gleich dem Gorgonenhaupte der Aegide!", "tokens": ["Gleich", "dem", "Gor\u00b7go\u00b7nen\u00b7haup\u00b7te", "der", "A\u00b7e\u00b7gi\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}