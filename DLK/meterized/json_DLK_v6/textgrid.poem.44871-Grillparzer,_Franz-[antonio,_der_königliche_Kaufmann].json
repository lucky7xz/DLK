{"textgrid.poem.44871": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[antonio, der k\u00f6nigliche Kaufmann]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Antonio, der k\u00f6nigliche Kaufmann,", "tokens": ["An\u00b7to\u00b7nio", ",", "der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Kauf\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Verlor ein Schiff im Sturm, und Shylock will sein Pfund.", "tokens": ["Ver\u00b7lor", "ein", "Schiff", "im", "Sturm", ",", "und", "Shy\u00b7lock", "will", "sein", "Pfund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "NE", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo ist der Richter, wo der Daniel,", "tokens": ["Wo", "ist", "der", "Rich\u00b7ter", ",", "wo", "der", "Da\u00b7ni\u00b7el", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Schein von Wahrheit trennt mit gotterf\u00fclltem Mund?", "tokens": ["Der", "Schein", "von", "Wahr\u00b7heit", "trennt", "mit", "got\u00b7ter\u00b7f\u00fcll\u00b7tem", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}