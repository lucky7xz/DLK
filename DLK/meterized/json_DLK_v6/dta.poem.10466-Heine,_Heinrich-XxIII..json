{"dta.poem.10466": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stand in dunkeln Tr\u00e4umen", "tokens": ["Ich", "stand", "in", "dun\u00b7keln", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und starrte ihr Bildni\u00df an,", "tokens": ["Und", "starr\u00b7te", "ihr", "Bild\u00b7ni\u00df", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und das geliebte Antlitz", "tokens": ["Und", "das", "ge\u00b7lieb\u00b7te", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heimlich zu leben begann.", "tokens": ["Heim\u00b7lich", "zu", "le\u00b7ben", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Um ihre Lippen zog sich", "tokens": ["Um", "ih\u00b7re", "Lip\u00b7pen", "zog", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein L\u00e4cheln wunderbar,", "tokens": ["Ein", "L\u00e4\u00b7cheln", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie von Wehmuthsthr\u00e4nen", "tokens": ["Und", "wie", "von", "Weh\u00b7muthst\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ergl\u00e4nzte ihr Augenpaar.", "tokens": ["Er\u00b7gl\u00e4nz\u00b7te", "ihr", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch meine Thr\u00e4nen flossen", "tokens": ["Auch", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir von den Wangen herab \u2014", "tokens": ["Mir", "von", "den", "Wan\u00b7gen", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und ach, ich kann es nicht glauben,", "tokens": ["Und", "ach", ",", "ich", "kann", "es", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich Dich verloren hab'!", "tokens": ["Da\u00df", "ich", "Dich", "ver\u00b7lo\u00b7ren", "hab'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}