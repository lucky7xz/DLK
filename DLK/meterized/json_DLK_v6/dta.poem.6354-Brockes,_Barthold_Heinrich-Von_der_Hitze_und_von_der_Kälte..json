{"dta.poem.6354": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich in solchen Gruben, Fl\u00fc\u00dfe", "tokens": ["Wenn", "sich", "in", "sol\u00b7chen", "Gru\u00b7ben", ",", "Fl\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PRF", "APPR", "PIAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gar zu enge R\u00f6hren finden.", "tokens": ["Und", "gar", "zu", "en\u00b7ge", "R\u00f6h\u00b7ren", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wasser, so daselbst ein Kercker fest verschleusst,", "tokens": ["Das", "Was\u00b7ser", ",", "so", "da\u00b7selbst", "ein", "Ker\u00b7cker", "fest", "ver\u00b7schleusst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PAV", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem es mit Gewalt der Bande sich entreisst;", "tokens": ["In\u00b7dem", "es", "mit", "Ge\u00b7walt", "der", "Ban\u00b7de", "sich", "en\u00b7treisst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So nimmt es, nebst den kleinen Theilen,", "tokens": ["So", "nimmt", "es", ",", "nebst", "den", "klei\u00b7nen", "Thei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Mineralien und rollenden Metallen,", "tokens": ["Von", "Mi\u00b7ne\u00b7ra\u00b7li\u00b7en", "und", "rol\u00b7len\u00b7den", "Me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch von den C\u00f6rpern mit, die durch ein stetes Wallen,", "tokens": ["Auch", "von", "den", "C\u00f6r\u00b7pern", "mit", ",", "die", "durch", "ein", "ste\u00b7tes", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch ein best\u00e4ndigs Regen,", "tokens": ["Durch", "ein", "be\u00b7st\u00e4n\u00b7digs", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und unaufh\u00f6rliches Bewegen,", "tokens": ["Und", "un\u00b7auf\u00b7h\u00f6r\u00b7li\u00b7ches", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich selber innerlich entz\u00fcnden,", "tokens": ["Sich", "sel\u00b7ber", "in\u00b7ner\u00b7lich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wodurch wir \u00e4usserlich viel Feuer-Brunnen finden,", "tokens": ["Wo\u00b7durch", "wir", "\u00e4us\u00b7ser\u00b7lich", "viel", "Feu\u00b7er\u00b7Brun\u00b7nen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wovon wir offt erstaunt, daselbst, wo sie entstehn,", "tokens": ["Wo\u00b7von", "wir", "offt", "er\u00b7staunt", ",", "da\u00b7selbst", ",", "wo", "sie", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,", "PAV", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Gluht und nebst dem Rauch ihr sprudelnd Kochen sehn.", "tokens": ["Die", "Gluht", "und", "nebst", "dem", "Rauch", "ihr", "spru\u00b7delnd", "Ko\u00b7chen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "ART", "NN", "PPER", "VVPP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}