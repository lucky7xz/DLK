{"textgrid.poem.58050": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "88. Das verborgene Leben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Gott verborgen leb in Abgeschiedenheit,", "tokens": ["In", "Gott", "ver\u00b7bor\u00b7gen", "leb", "in", "Ab\u00b7ge\u00b7schie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ganz fremd und tot der Welt und deiner Eigenheit!", "tokens": ["Ganz", "fremd", "und", "tot", "der", "Welt", "und", "dei\u00b7ner", "Ei\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Eig'ne bringt nur Qual; verlier, vergi\u00df dich gar,", "tokens": ["Das", "Eig'\u00b7ne", "bringt", "nur", "Qual", ";", "ver\u00b7lier", ",", "ver\u00b7gi\u00df", "dich", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$.", "ADJD", "$,", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Leben sei in dir als Christus offenbar!", "tokens": ["Kein", "Le\u00b7ben", "sei", "in", "dir", "als", "Chris\u00b7tus", "of\u00b7fen\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "KOUS", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}