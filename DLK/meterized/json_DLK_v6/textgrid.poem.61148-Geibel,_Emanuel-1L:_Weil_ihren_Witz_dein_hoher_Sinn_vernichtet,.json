{"textgrid.poem.61148": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil ihren Witz dein hoher Sinn vernichtet,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ihren Witz dein hoher Sinn vernichtet,", "tokens": ["Weil", "ih\u00b7ren", "Witz", "dein", "ho\u00b7her", "Sinn", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihre Schmeichelei f\u00fcr dich verloren,", "tokens": ["Und", "ih\u00b7re", "Schmei\u00b7che\u00b7lei", "f\u00fcr", "dich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So hei\u00dft dich marmorn dies Geschlecht von Toren,", "tokens": ["So", "hei\u00dft", "dich", "mar\u00b7morn", "dies", "Ge\u00b7schlecht", "von", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PDS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das frostig jede gro\u00dfe Seele richtet.", "tokens": ["Das", "fros\u00b7tig", "je\u00b7de", "gro\u00b7\u00dfe", "See\u00b7le", "rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch willig hast du auf ein Lob verzichtet,", "tokens": ["Doch", "wil\u00b7lig", "hast", "du", "auf", "ein", "Lob", "ver\u00b7zich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das f\u00fcr den Kern die Schale stets erkoren;", "tokens": ["Das", "f\u00fcr", "den", "Kern", "die", "Scha\u00b7le", "stets", "er\u00b7ko\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gleichst dem Wein, der, \u00e4u\u00dferlich gefroren,", "tokens": ["Du", "gleichst", "dem", "Wein", ",", "der", ",", "\u00e4u\u00b7\u00dfer\u00b7lich", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So Geist als Gut im Innersten verdichtet", "tokens": ["So", "Geist", "als", "Gut", "im", "In\u00b7ners\u00b7ten", "ver\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KOUS", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-----+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Heil aber jenem, der dich einst erkennt,", "tokens": ["Heil", "a\u00b7ber", "je\u00b7nem", ",", "der", "dich", "einst", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PDAT", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und, in der Seele stillen Reiz versunken,", "tokens": ["Und", ",", "in", "der", "See\u00b7le", "stil\u00b7len", "Reiz", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht eher rastet, bis er sein dich nennt!", "tokens": ["Nicht", "e\u00b7her", "ras\u00b7tet", ",", "bis", "er", "sein", "dich", "nennt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bei deinem Ku\u00df empfinden wird er trunken,", "tokens": ["Bei", "dei\u00b7nem", "Ku\u00df", "emp\u00b7fin\u00b7den", "wird", "er", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um wie viel hei\u00dfer heimlich Feuer brennt,", "tokens": ["Um", "wie", "viel", "hei\u00b7\u00dfer", "heim\u00b7lich", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOKOM", "PIAT", "ADJA", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als was f\u00fcr jeden sich verspr\u00fcht in Funken.", "tokens": ["Als", "was", "f\u00fcr", "je\u00b7den", "sich", "ver\u00b7spr\u00fcht", "in", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}