{"dta.poem.2909": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ro\u00df am Wagen merkt des Fuhrmanns Unbestand,", "tokens": ["Das", "Ro\u00df", "am", "Wa\u00b7gen", "merkt", "des", "Fuhr\u00b7manns", "Un\u00b7be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rei\u00dft widerspenstig ihm das Lenkseil aus der Hand.", "tokens": ["Rei\u00dft", "wi\u00b7der\u00b7spens\u00b7tig", "ihm", "das", "Lenk\u00b7seil", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sicher wird der Zucht dein Z\u00f6gling sich entziehn,", "tokens": ["Und", "si\u00b7cher", "wird", "der", "Zucht", "dein", "Z\u00f6g\u00b7ling", "sich", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuchtmeister, meisterst du mit Sicherheit nicht ihn.", "tokens": ["Zucht\u00b7meis\u00b7ter", ",", "meis\u00b7terst", "du", "mit", "Si\u00b7cher\u00b7heit", "nicht", "ihn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "PTKNEG", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure":