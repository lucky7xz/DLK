{"textgrid.poem.58228": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "266. Auf Gott nur richt stets dein Gesicht!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang ich meinen Freund recht zu allein ansehe,", "tokens": ["So\u00b7lang", "ich", "mei\u00b7nen", "Freund", "recht", "zu", "al\u00b7lein", "an\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sicher, unverr\u00fcckt und tief im Frieden stehe \u2013", "tokens": ["Ich", "si\u00b7cher", ",", "un\u00b7ver\u00b7r\u00fcckt", "und", "tief", "im", "Frie\u00b7den", "ste\u00b7he", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O reines Einfaltsaug, unschuldiges Gemerk!", "tokens": ["O", "rei\u00b7nes", "Ein\u00b7falt\u00b7saug", ",", "un\u00b7schul\u00b7di\u00b7ges", "Ge\u00b7merk", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sobald ich mich auf mich und Kreaturen kehre,", "tokens": ["So\u00b7bald", "ich", "mich", "auf", "mich", "und", "Kre\u00b7a\u00b7tu\u00b7ren", "keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bin ich bl\u00f6d und schwach und mich in allem st\u00f6re", "tokens": ["So", "bin", "ich", "bl\u00f6d", "und", "schwach", "und", "mich", "in", "al\u00b7lem", "st\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "PRF", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und was ich immer tu, ist ein beflecktes Werk.", "tokens": ["Und", "was", "ich", "im\u00b7mer", "tu", ",", "ist", "ein", "be\u00b7fleck\u00b7tes", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}