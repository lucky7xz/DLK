{"textgrid.poem.52647": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Reisefr\u00fcchtchen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sonderbar komm ich jetzt mir vor!", "tokens": ["Wie", "son\u00b7der\u00b7bar", "komm", "ich", "jetzt", "mir", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heb mich \u00fcber alle Welt empor,", "tokens": ["Heb", "mich", "\u00fc\u00b7ber", "al\u00b7le", "Welt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und bin vor der Welt vielleicht ein Thor.", "tokens": ["Und", "bin", "vor", "der", "Welt", "viel\u00b7leicht", "ein", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So gescheidt und so verkehrt ich bin,", "tokens": ["So", "ge\u00b7scheidt", "und", "so", "ver\u00b7kehrt", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mit der Gesundheit leichtem Sinn,", "tokens": ["Mit", "der", "Ge\u00b7sund\u00b7heit", "leich\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lauf ich \u00fcber Gr\u00e4ber und Blumen hin.", "tokens": ["Lauf", "ich", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ber", "und", "Blu\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Das ertrag ich nun weder gut noch lang:", "tokens": ["Das", "er\u00b7trag", "ich", "nun", "we\u00b7der", "gut", "noch", "lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Meine Stimmung l\u00f6st sich zumeist in Gesang;", "tokens": ["Mei\u00b7ne", "Stim\u00b7mung", "l\u00f6st", "sich", "zu\u00b7meist", "in", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und so viel ich wei\u00df, nie wird mir bang.", "tokens": ["Und", "so", "viel", "ich", "wei\u00df", ",", "nie", "wird", "mir", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Es treibt mich bald, was Gutes zu \u2013 thun,", "tokens": ["Es", "treibt", "mich", "bald", ",", "was", "Gu\u00b7tes", "zu", "\u2013", "thun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "NN", "PTKZU", "$(", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Zum Ziel zu kommen treibt michs nun,", "tokens": ["Zum", "Ziel", "zu", "kom\u00b7men", "treibt", "michs", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Meine Gedanken lassen mich nicht \u2013 ruhn.", "tokens": ["Mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "las\u00b7sen", "mich", "nicht", "\u2013", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}