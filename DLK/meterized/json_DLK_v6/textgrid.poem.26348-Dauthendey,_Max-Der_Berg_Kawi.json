{"textgrid.poem.26348": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Berg Kawi", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort im \u00f6stlichen Abendschein, der pfaublau,", "tokens": ["Dort", "im", "\u00f6st\u00b7li\u00b7chen", "A\u00b7bend\u00b7schein", ",", "der", "pfaub\u00b7lau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Liegt ein gewaltiger Berg, genannt die \u00bbliegende Frau\u00ab.", "tokens": ["Liegt", "ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Berg", ",", "ge\u00b7nannt", "die", "\u00bb", "lie\u00b7gen\u00b7de", "Frau", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVPP", "ART", "$(", "ADJA", "NN", "$(", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Frau ruht ausgestreckt, den Kopf seitlich gewandt.", "tokens": ["Die", "Frau", "ruht", "aus\u00b7ge\u00b7streckt", ",", "den", "Kopf", "seit\u00b7lich", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn die Himmelsgrenze abends braunrot verbrennt,", "tokens": ["Wenn", "die", "Him\u00b7mels\u00b7gren\u00b7ze", "a\u00b7bends", "braun\u00b7rot", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Sagt mein Blut, da\u00df es die \u00bbliegende Frau\u00ab erkennt,", "tokens": ["Sagt", "mein", "Blut", ",", "da\u00df", "es", "die", "\u00bb", "lie\u00b7gen\u00b7de", "Frau", "\u00ab", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "$(", "ADJA", "NN", "$(", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Wangenrundung, die volle H\u00fcfte und Brust,", "tokens": ["Die", "Wan\u00b7gen\u00b7run\u00b7dung", ",", "die", "vol\u00b7le", "H\u00fcf\u00b7te", "und", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Sehnsucht zeichnet mir dann deutlich der Sehnsucht Lust.", "tokens": ["Die", "Sehn\u00b7sucht", "zeich\u00b7net", "mir", "dann", "deut\u00b7lich", "der", "Sehn\u00b7sucht", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist kein toter Berg, es ist mein atmend Weib,", "tokens": ["Es", "ist", "kein", "to\u00b7ter", "Berg", ",", "es", "ist", "mein", "at\u00b7mend", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort liegt es und wartet mit ergebenem Leib.", "tokens": ["Dort", "liegt", "es", "und", "war\u00b7tet", "mit", "er\u00b7ge\u00b7be\u00b7nem", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die in der Sehnsucht warten, wachsen zu Riesen.", "tokens": ["Die", "in", "der", "Sehn\u00b7sucht", "war\u00b7ten", ",", "wach\u00b7sen", "zu", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ach, meine Schultern l\u00e4ngst an die Sterne stie\u00dfen.", "tokens": ["Ach", ",", "mei\u00b7ne", "Schul\u00b7tern", "l\u00e4ngst", "an", "die", "Ster\u00b7ne", "stie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}