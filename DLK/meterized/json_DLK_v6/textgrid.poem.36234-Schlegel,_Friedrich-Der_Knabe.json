{"textgrid.poem.36234": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich nur ein V\u00f6glein w\u00e4re,", "tokens": ["Wenn", "ich", "nur", "ein", "V\u00f6\u00b7glein", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wie wollt ich lustig fliegen,", "tokens": ["Ach", "wie", "wollt", "ich", "lus\u00b7tig", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle V\u00f6gel weit besiegen.", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "weit", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich so ein Vogel bin,", "tokens": ["Wenn", "ich", "so", "ein", "Vo\u00b7gel", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Darf ich alles, alles haschen,", "tokens": ["Darf", "ich", "al\u00b7les", ",", "al\u00b7les", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die h\u00f6chsten Kirschen naschen;", "tokens": ["Und", "die", "h\u00f6chs\u00b7ten", "Kir\u00b7schen", "na\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliege dann zur Mutter hin.", "tokens": ["Flie\u00b7ge", "dann", "zur", "Mut\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist sie b\u00f6s' in ihrem Sinn,", "tokens": ["Ist", "sie", "b\u00f6s'", "in", "ih\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann ich lieb mich an sie schmiegen,", "tokens": ["Kann", "ich", "lieb", "mich", "an", "sie", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihren Ernst gar bald besiegen.", "tokens": ["Ih\u00b7ren", "Ernst", "gar", "bald", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bunte Federn, leichte Fl\u00fcgel,", "tokens": ["Bun\u00b7te", "Fe\u00b7dern", ",", "leich\u00b7te", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrft' ich in der Sonne schwingen,", "tokens": ["D\u00fcrft'", "ich", "in", "der", "Son\u00b7ne", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die L\u00fcfte laut erklingen,", "tokens": ["Da\u00df", "die", "L\u00fcf\u00b7te", "laut", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df nichts mehr von Band und Z\u00fcgel.", "tokens": ["Wei\u00df", "nichts", "mehr", "von", "Band", "und", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "W\u00e4r ich \u00fcber jene H\u00fcgel,", "tokens": ["W\u00e4r", "ich", "\u00fc\u00b7ber", "je\u00b7ne", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach dann wollt' ich lustig fliegen,", "tokens": ["Ach", "dann", "wollt'", "ich", "lus\u00b7tig", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle V\u00f6gel weit besiegen.", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "weit", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}