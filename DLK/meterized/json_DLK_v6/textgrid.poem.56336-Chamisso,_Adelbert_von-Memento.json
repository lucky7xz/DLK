{"textgrid.poem.56336": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Memento", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nennt mir diesen Fl\u00fcchtling, diesen Alten,", "tokens": ["Wer", "nennt", "mir", "die\u00b7sen", "Fl\u00fccht\u00b7ling", ",", "die\u00b7sen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der zitternd f\u00fchrt den Wanderstab zur Hand,", "tokens": ["Der", "zit\u00b7ternd", "f\u00fchrt", "den", "Wan\u00b7der\u00b7stab", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bleich die Stirne zieht in d\u00fcstre Falten?", "tokens": ["Und", "bleich", "die", "Stir\u00b7ne", "zieht", "in", "d\u00fcst\u00b7re", "Fal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besudelt scheint mir Purpur sein Gewand,", "tokens": ["Be\u00b7su\u00b7delt", "scheint", "mir", "Pur\u00b7pur", "sein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und auf der Stirne, welch ein seltsam Mal?", "tokens": ["Und", "auf", "der", "Stir\u00b7ne", ",", "welch", "ein", "selt\u00b7sam", "Mal", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War der ein K\u00f6nig \u00fcber dieses Land?", "tokens": ["War", "der", "ein", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "die\u00b7ses", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er war es gestern, und zum dritten Mal", "tokens": ["Er", "war", "es", "ge\u00b7stern", ",", "und", "zum", "drit\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Entfleucht er, und zum letzten, seinen Reichen,", "tokens": ["Ent\u00b7fleucht", "er", ",", "und", "zum", "letz\u00b7ten", ",", "sei\u00b7nen", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPRART", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wor\u00fcber nicht mit Weisheit er befahl.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "nicht", "mit", "Weis\u00b7heit", "er", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und nun? \u2013 Er hofft die Fremde zu erreichen,", "tokens": ["Und", "nun", "?", "\u2013", "Er", "hofft", "die", "Frem\u00b7de", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das fremde Land, wo ihm des Fremden Gnade", "tokens": ["Das", "frem\u00b7de", "Land", ",", "wo", "ihm", "des", "Frem\u00b7den", "Gna\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das bittre Brod des Mitleids m\u00f6ge reichen.", "tokens": ["Das", "bitt\u00b7re", "Brod", "des", "Mit\u00b7leids", "m\u00f6\u00b7ge", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Gelangend an das Meer auf scheuem Pfade,", "tokens": ["Ge\u00b7lan\u00b7gend", "an", "das", "Meer", "auf", "scheu\u00b7em", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wo Schiffe, fremde Schiffe, seiner warten,", "tokens": ["Wo", "Schif\u00b7fe", ",", "frem\u00b7de", "Schif\u00b7fe", ",", "sei\u00b7ner", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Blickt er zur\u00fcck zur Heimat vom Gestade;", "tokens": ["Blickt", "er", "zu\u00b7r\u00fcck", "zur", "Hei\u00b7mat", "vom", "Ge\u00b7sta\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und lauscht \u2013 dem trunknen Freudenruf, dem harten,", "tokens": ["Und", "lauscht", "\u2013", "dem", "trunk\u00b7nen", "Freu\u00b7den\u00b7ruf", ",", "dem", "har\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der himmelangetragen widerhallt", "tokens": ["Der", "him\u00b7mel\u00b7an\u00b7ge\u00b7tra\u00b7gen", "wi\u00b7der\u00b7hallt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Inmitten neuerbl\u00fchtem Friedensgarten:", "tokens": ["In\u00b7mit\u00b7ten", "neu\u00b7er\u00b7bl\u00fch\u00b7tem", "Frie\u00b7dens\u00b7gar\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbzerri\u00df er den Vertrag doch selbst, da galt", "tokens": ["\u00bb", "zer\u00b7ri\u00df", "er", "den", "Ver\u00b7trag", "doch", "selbst", ",", "da", "galt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Es nur das Fest der Freiheit zu erneuen;", "tokens": ["Es", "nur", "das", "Fest", "der", "Frei\u00b7heit", "zu", "er\u00b7neu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Er stand allein, und drohte mit Gewalt!\u00ab", "tokens": ["Er", "stand", "al\u00b7lein", ",", "und", "droh\u00b7te", "mit", "Ge\u00b7walt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die Stimmen nur von wenigen Getreuen", "tokens": ["Die", "Stim\u00b7men", "nur", "von", "we\u00b7ni\u00b7gen", "Ge\u00b7treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Erheben sich, die, vor den freud'gen Scharen,", "tokens": ["Er\u00b7he\u00b7ben", "sich", ",", "die", ",", "vor", "den", "freu\u00b7d'\u00b7gen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PRELS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Sich seinen Stern nicht zu betrauern scheuen,", "tokens": ["Sich", "sei\u00b7nen", "Stern", "nicht", "zu", "be\u00b7trau\u00b7ern", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die Stimmen derer, mu\u00df er nun erfahren,", "tokens": ["Die", "Stim\u00b7men", "de\u00b7rer", ",", "mu\u00df", "er", "nun", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die er verstie\u00df mit Unbill und mit Schmach,", "tokens": ["Die", "er", "ver\u00b7stie\u00df", "mit", "Un\u00b7bill", "und", "mit", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Weil Toren nicht, weil Knechte nicht sie waren. \u2013 \u2013", "tokens": ["Weil", "To\u00b7ren", "nicht", ",", "weil", "Knech\u00b7te", "nicht", "sie", "wa\u00b7ren", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "$,", "KOUS", "NN", "PTKNEG", "PPER", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und solchem Bilde sinnt der Dichter nach,", "tokens": ["Und", "sol\u00b7chem", "Bil\u00b7de", "sinnt", "der", "Dich\u00b7ter", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Verstummt, von Gunst und Mi\u00dfgunst gleich entfernt;", "tokens": ["Ver\u00b7stummt", ",", "von", "Gunst", "und", "Mi\u00df\u00b7gunst", "gleich", "ent\u00b7fernt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Er sinnt und weint, sein Saitenspiel zerbrach.", "tokens": ["Er", "sinnt", "und", "weint", ",", "sein", "Sai\u00b7ten\u00b7spiel", "zer\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ihr M\u00e4chtigen der Erde! schaut und lernt!", "tokens": ["Ihr", "M\u00e4ch\u00b7ti\u00b7gen", "der", "Er\u00b7de", "!", "schaut", "und", "lernt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$.", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}