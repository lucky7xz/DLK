{"textgrid.poem.64461": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "83. Die vier Weltalter", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier Zeiten l\u00e4\u00dft uns ", "tokens": ["Vier", "Zei\u00b7ten", "l\u00e4\u00dft", "uns"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die erste, wie man glaubt, ist lauter Gold gewesen,", "tokens": ["Die", "ers\u00b7te", ",", "wie", "man", "glaubt", ",", "ist", "lau\u00b7ter", "Gold", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "PIS", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch Silber war einmal, nur da\u00df es bald verschwand,", "tokens": ["Auch", "Sil\u00b7ber", "war", "ein\u00b7mal", ",", "nur", "da\u00df", "es", "bald", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$,", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Kupfer haben wir vor kurzem noch gekannt;", "tokens": ["Das", "Kup\u00b7fer", "ha\u00b7ben", "wir", "vor", "kur\u00b7zem", "noch", "ge\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch aus der b\u00f6sen Welt ist Kupfer selbst entwichen,", "tokens": ["Doch", "aus", "der", "b\u00f6\u00b7sen", "Welt", "ist", "Kup\u00b7fer", "selbst", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Eisen herrscht anjetzt, nach ", "tokens": ["Und", "Ei\u00b7sen", "herrscht", "an\u00b7jetzt", ",", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Jude, der das h\u00f6rt, f\u00e4llt mir hier pl\u00f6tzlich ein:", "tokens": ["Ein", "Ju\u00b7de", ",", "der", "das", "h\u00f6rt", ",", "f\u00e4llt", "mir", "hier", "pl\u00f6tz\u00b7lich", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mei! war der Mann ein M\u00fcnzwardein?", "tokens": ["Mei", "!", "war", "der", "Mann", "ein", "M\u00fcnz\u00b7war\u00b7dein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}