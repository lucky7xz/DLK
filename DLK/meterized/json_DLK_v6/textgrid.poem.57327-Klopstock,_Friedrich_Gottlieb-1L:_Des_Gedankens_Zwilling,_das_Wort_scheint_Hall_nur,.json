{"textgrid.poem.57327": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Gedankens Zwilling, das Wort scheint Hall nur,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Gedankens Zwilling, das Wort scheint Hall nur,", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "Zwil\u00b7ling", ",", "das", "Wort", "scheint", "Hall", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "NE", "ADV", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der in die Luft hinfliesst: heiliges Band", "tokens": ["Der", "in", "die", "Luft", "hin\u00b7fliesst", ":", "hei\u00b7li\u00b7ges", "Band"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Sterblichen ist es, erhebt", "tokens": ["Des", "Sterb\u00b7li\u00b7chen", "ist", "es", ",", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die Vernunft ihm, und das Herz ihm!", "tokens": ["Die", "Ver\u00b7nunft", "ihm", ",", "und", "das", "Herz", "ihm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "KON", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und er weiss es; denn er erfand, durch Zeichen", "tokens": ["Und", "er", "weiss", "es", ";", "denn", "er", "er\u00b7fand", ",", "durch", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fest, wie den Fels, hinzuzaubern den Hall!", "tokens": ["Fest", ",", "wie", "den", "Fels", ",", "hin\u00b7zu\u00b7zau\u00b7bern", "den", "Hall", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da ruht er; doch kaum, dass der Blick", "tokens": ["Da", "ruht", "er", ";", "doch", "kaum", ",", "dass", "der", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ihm senket, so erwacht er.", "tokens": ["Sich", "ihm", "sen\u00b7ket", ",", "so", "er\u00b7wacht", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er erreicht die Farbe dich nicht, des Marmors", "tokens": ["Er", "er\u00b7reicht", "die", "Far\u00b7be", "dich", "nicht", ",", "des", "Mar\u00b7mors"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "$,", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Feilbare Last, G\u00f6ttin Sprache, dich nicht!", "tokens": ["Feil\u00b7ba\u00b7re", "Last", ",", "G\u00f6t\u00b7tin", "Spra\u00b7che", ",", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "NN", "$,", "PPER", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nur weniges bilden sie uns:", "tokens": ["Nur", "we\u00b7ni\u00b7ges", "bil\u00b7den", "sie", "uns", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und es zeigt sich uns auf Einmal.", "tokens": ["Und", "es", "zeigt", "sich", "uns", "auf", "Ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPER", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Erfinder, welcher durch dich des H\u00f6rers", "tokens": ["Dem", "Er\u00b7fin\u00b7der", ",", "wel\u00b7cher", "durch", "dich", "des", "H\u00f6\u00b7rers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Seele bewegt, that die Sch\u00f6pfung sich auf!", "tokens": ["See\u00b7le", "be\u00b7wegt", ",", "that", "die", "Sch\u00f6p\u00b7fung", "sich", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie D\u00fcften entschwebt, was er sagt,", "tokens": ["Wie", "D\u00fcf\u00b7ten", "ent\u00b7schwebt", ",", "was", "er", "sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit dem Reize der Erwartung,", "tokens": ["Mit", "dem", "Rei\u00b7ze", "der", "Er\u00b7war\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit der Menschenstimme Gewalt, mit ihrem", "tokens": ["Mit", "der", "Men\u00b7schen\u00b7stim\u00b7me", "Ge\u00b7walt", ",", "mit", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "H\u00f6heren Reiz, h\u00f6chsten, wenn sie Gesang", "tokens": ["H\u00f6\u00b7he\u00b7ren", "Reiz", ",", "h\u00f6chs\u00b7ten", ",", "wenn", "sie", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "NN"], "meter": "+--++-++-+", "measure": "dactylic.init"}, "line.3": {"text": "Hinstr\u00f6met, und inniger so", "tokens": ["Hins\u00b7tr\u00f6\u00b7met", ",", "und", "in\u00b7ni\u00b7ger", "so"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In die Seele sich ergiesset.", "tokens": ["In", "die", "See\u00b7le", "sich", "er\u00b7gies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch, Erfinder, t\u00e4usche dich nicht! F\u00fcr dich nur", "tokens": ["Doch", ",", "Er\u00b7fin\u00b7der", ",", "t\u00e4u\u00b7sche", "dich", "nicht", "!", "F\u00fcr", "dich", "nur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ist es gedacht, was zum Laute nicht wird,", "tokens": ["Ist", "es", "ge\u00b7dacht", ",", "was", "zum", "Lau\u00b7te", "nicht", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PRELS", "APPRART", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "F\u00fcr dich nur; wie tief auch, wie hell,", "tokens": ["F\u00fcr", "dich", "nur", ";", "wie", "tief", "auch", ",", "wie", "hell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$.", "PWAV", "ADJD", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wie begeisternd du es dachtest.", "tokens": ["Wie", "be\u00b7geis\u00b7ternd", "du", "es", "dach\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Gespielen sind ihr zu lieb der Sprache;", "tokens": ["Die", "Ge\u00b7spie\u00b7len", "sind", "ihr", "zu", "lieb", "der", "Spra\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Trenne sie nicht! Enge Fessel, geringt", "tokens": ["Tren\u00b7ne", "sie", "nicht", "!", "En\u00b7ge", "Fes\u00b7sel", ",", "ge\u00b7ringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "NE", "NN", "$,", "VVPP"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "An lemnischer Esse, vereint", "tokens": ["An", "lem\u00b7ni\u00b7scher", "Es\u00b7se", ",", "ver\u00b7eint"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ihr den Wohlklang, und den Verstanz", "tokens": ["Ihr", "den", "Wohl\u00b7klang", ",", "und", "den", "Ver\u00b7stanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Harmonie zu sondern, die so einstimmet,", "tokens": ["Har\u00b7mo\u00b7nie", "zu", "son\u00b7dern", ",", "die", "so", "ein\u00b7stim\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Meidet, wer weiss, welcher Zweck sie verband:", "tokens": ["Mei\u00b7det", ",", "wer", "weiss", ",", "wel\u00b7cher", "Zweck", "sie", "ver\u00b7band", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "$,", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die Trennungen zwingen zu viel", "tokens": ["Die", "Tren\u00b7nun\u00b7gen", "zwin\u00b7gen", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "PTKA", "PIS"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Des Gedachten zu verstummen.", "tokens": ["Des", "Ge\u00b7dach\u00b7ten", "zu", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Von dem Ausland, Deutsche, das Tanz des Liedes", "tokens": ["Von", "dem", "Aus\u00b7land", ",", "Deut\u00b7sche", ",", "das", "Tanz", "des", "Lie\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "$,", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klagend entbehrt, lernet ganz, was es ist,", "tokens": ["Kla\u00b7gend", "ent\u00b7behrt", ",", "ler\u00b7net", "ganz", ",", "was", "es", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem viele von euch, wie Athen", "tokens": ["Dem", "vie\u00b7le", "von", "euch", ",", "wie", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "APPR", "PPER", "$,", "PWAV", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihm auch horchte, noch so taub sind.", "tokens": ["Ihm", "auch", "horch\u00b7te", ",", "noch", "so", "taub", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und es schwebt doch k\u00fchn, und gewiss Teutona", "tokens": ["Und", "es", "schwebt", "doch", "k\u00fchn", ",", "und", "ge\u00b7wiss", "Teu\u00b7to\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "NE", "NE"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wendungen hin, die Hell\u00e4nis so gar", "tokens": ["Wen\u00b7dun\u00b7gen", "hin", ",", "die", "Hel\u00b7l\u00e4\u00b7nis", "so", "gar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht alle, mit stolzem Gef\u00fchl", "tokens": ["Nicht", "al\u00b7le", ",", "mit", "stol\u00b7zem", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Des Gelingens, sich erk\u00f6hre.", "tokens": ["Des", "Ge\u00b7lin\u00b7gens", ",", "sich", "er\u00b7k\u00f6h\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Den Gespielen lasset, und ihr der G\u00f6ttin", "tokens": ["Den", "Ge\u00b7spie\u00b7len", "las\u00b7set", ",", "und", "ihr", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blumen uns streun: Himmelschl\u00fcsseln dem Klang,", "tokens": ["Blu\u00b7men", "uns", "streun", ":", "Him\u00b7mel\u00b7schl\u00fcs\u00b7seln", "dem", "Klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dem Tanz Hiazinten, und ihr", "tokens": ["Dem", "Tanz", "Hi\u00b7a\u00b7zin\u00b7ten", ",", "und", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "PPOSAT"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Von den Rosen, die bemoost sind.", "tokens": ["Von", "den", "Ro\u00b7sen", ",", "die", "be\u00b7moost", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie entgl\u00fchen lieblicher, als der Schwestern", "tokens": ["Sie", "ent\u00b7gl\u00fc\u00b7hen", "lieb\u00b7li\u00b7cher", ",", "als", "der", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Bl\u00fchendster Busch, duften s\u00fcssern Geruch;", "tokens": ["Bl\u00fc\u00b7hends\u00b7ter", "Busch", ",", "duf\u00b7ten", "s\u00fcs\u00b7sern", "Ge\u00b7ruch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Auch schm\u00fcckt sie ihr moosig Gewand,", "tokens": ["Auch", "schm\u00fcckt", "sie", "ihr", "moo\u00b7sig", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und durchr\u00e4uchert ihr Ged\u00fcfte.", "tokens": ["Und", "durch\u00b7r\u00e4u\u00b7chert", "ihr", "Ge\u00b7d\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}