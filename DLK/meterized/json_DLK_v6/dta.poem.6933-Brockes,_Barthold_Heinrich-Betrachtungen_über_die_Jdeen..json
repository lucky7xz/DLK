{"dta.poem.6933": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen C\u00f6rpern nun das nimmer m\u00fcde Rennen,", "tokens": ["Von", "al\u00b7len", "C\u00f6r\u00b7pern", "nun", "das", "nim\u00b7mer", "m\u00fc\u00b7de", "Ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu suchen einen Zweck, den sie nicht kennen;", "tokens": ["Zu", "su\u00b7chen", "ei\u00b7nen", "Zweck", ",", "den", "sie", "nicht", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erweiset uns die Hand,", "tokens": ["Er\u00b7wei\u00b7set", "uns", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sie so wunderbar zusammen band.", "tokens": ["Die", "sie", "so", "wun\u00b7der\u00b7bar", "zu\u00b7sam\u00b7men", "band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der SCHOEPFFER wird erh\u00f6ht durch Seine Wunder-", "tokens": ["Der", "ScHOE\u00b7PF\u00b7FER", "wird", "er\u00b7h\u00f6ht", "durch", "Sei\u00b7ne", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Krafft,", "tokens": ["Krafft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Mit welcher er der Himmel-Creise misst,", "tokens": ["Mit", "wel\u00b7cher", "er", "der", "Him\u00b7mel\u00b7Crei\u00b7se", "misst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auch durch die tieffe Wissenschafft,", "tokens": ["Auch", "durch", "die", "tief\u00b7fe", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die in dem unsichtbarst- und kleinsten C\u00f6rper ist.", "tokens": ["Die", "in", "dem", "un\u00b7sicht\u00b7bar\u00b7st", "und", "kleins\u00b7ten", "C\u00f6r\u00b7per", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}