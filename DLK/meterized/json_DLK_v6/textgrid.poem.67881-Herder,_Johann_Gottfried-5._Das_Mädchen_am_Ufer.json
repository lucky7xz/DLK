{"textgrid.poem.67881": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5. Das M\u00e4dchen am Ufer", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im s\u00e4uselnden Winde, am murmelnden Bach", "tokens": ["Im", "s\u00e4u\u00b7seln\u00b7den", "Win\u00b7de", ",", "am", "mur\u00b7meln\u00b7den", "Bach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sa\u00df Lila auf Blumen und weinet' und sprach:", "tokens": ["Sa\u00df", "Li\u00b7la", "auf", "Blu\u00b7men", "und", "wei\u00b7net'", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbwas bl\u00fcht ihr, ihr Blumen? was s\u00e4uselst du West?", "tokens": ["\u00bb", "was", "bl\u00fcht", "ihr", ",", "ihr", "Blu\u00b7men", "?", "was", "s\u00e4u\u00b7selst", "du", "West", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was murmelst du Strom, der mich murmelnd verl\u00e4\u00dft?", "tokens": ["Was", "mur\u00b7melst", "du", "Strom", ",", "der", "mich", "mur\u00b7melnd", "ver\u00b7l\u00e4\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Lieber, er bl\u00fchte am Herzen mir hier,", "tokens": ["Mein", "Lie\u00b7ber", ",", "er", "bl\u00fch\u00b7te", "am", "Her\u00b7zen", "mir", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "War frisch wie die Welle, war lieblicher mir", "tokens": ["War", "frisch", "wie", "die", "Wel\u00b7le", ",", "war", "lieb\u00b7li\u00b7cher", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,", "VAFIN", "ADJD", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als Zephir; o Zephir, wo flohest du hin?", "tokens": ["Als", "Ze\u00b7phir", ";", "o", "Ze\u00b7phir", ",", "wo", "flo\u00b7hest", "du", "hin", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$.", "FM", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "O Blume der Liebe, du mustest verbl\u00fchn!\u00ab", "tokens": ["O", "Blu\u00b7me", "der", "Lie\u00b7be", ",", "du", "mus\u00b7test", "ver\u00b7bl\u00fchn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Vom Busen, vom Herzen ri\u00df ab sie den Straus,", "tokens": ["Vom", "Bu\u00b7sen", ",", "vom", "Her\u00b7zen", "ri\u00df", "ab", "sie", "den", "Straus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und seufzet und weinet die Seele sich aus.", "tokens": ["Und", "seuf\u00b7zet", "und", "wei\u00b7net", "die", "See\u00b7le", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was weinst in die Welle? Was seufzest in Wind?", "tokens": ["Was", "weinst", "in", "die", "Wel\u00b7le", "?", "Was", "seuf\u00b7zest", "in", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "O M\u00e4dchen, Wind, Welle und Leben zerrinnt.", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "Wind", ",", "Wel\u00b7le", "und", "Le\u00b7ben", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Strom kommt nicht wieder, der Westwind verweht,", "tokens": ["Der", "Strom", "kommt", "nicht", "wie\u00b7der", ",", "der", "West\u00b7wind", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Blume verwelket, die Jugend vergeht,", "tokens": ["Die", "Blu\u00b7me", "ver\u00b7wel\u00b7ket", ",", "die", "Ju\u00b7gend", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gib, M\u00e4dchen, die Blume dem Strome, dem West;", "tokens": ["Gib", ",", "M\u00e4d\u00b7chen", ",", "die", "Blu\u00b7me", "dem", "Stro\u00b7me", ",", "dem", "West", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es ist ja nicht Liebe, wenn Liebe verl\u00e4\u00dft.", "tokens": ["Es", "ist", "ja", "nicht", "Lie\u00b7be", ",", "wenn", "Lie\u00b7be", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "NN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}