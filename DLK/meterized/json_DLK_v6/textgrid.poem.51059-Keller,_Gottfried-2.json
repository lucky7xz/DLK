{"textgrid.poem.51059": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht Selbstsucht und nicht Eitelkeit,", "tokens": ["Es", "ist", "nicht", "Selbst\u00b7sucht", "und", "nicht", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was sehnend mir das Herz grab\u00fcber tr\u00e4gt;", "tokens": ["Was", "seh\u00b7nend", "mir", "das", "Herz", "grab\u00b7\u00fc\u00b7ber", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was mir die k\u00fchngeschwungne Br\u00fccke schl\u00e4gt,", "tokens": ["Was", "mir", "die", "k\u00fchn\u00b7ge\u00b7schwung\u00b7ne", "Br\u00fc\u00b7cke", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist wohl der Stolz, der mich vom Staub befreit.", "tokens": ["Ist", "wohl", "der", "Stolz", ",", "der", "mich", "vom", "Staub", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie ist so eng, die gr\u00fcne Erdenzeit,", "tokens": ["Sie", "ist", "so", "eng", ",", "die", "gr\u00fc\u00b7ne", "Er\u00b7den\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unendlich aber, was den Geist bewegt!", "tokens": ["Un\u00b7end\u00b7lich", "a\u00b7ber", ",", "was", "den", "Geist", "be\u00b7wegt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie wenig ist's, was ihr im Busen hegt,", "tokens": ["Wie", "we\u00b7nig", "ist's", ",", "was", "ihr", "im", "Bu\u00b7sen", "hegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da ihr so satt hier, so vergn\u00fcglich seid!", "tokens": ["Da", "ihr", "so", "satt", "hier", ",", "so", "ver\u00b7gn\u00fcg\u00b7lich", "seid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn auch einst die Freiheit ist errungen,", "tokens": ["Und", "wenn", "auch", "einst", "die", "Frei\u00b7heit", "ist", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschheit hoch wie eine Rose gl\u00fcht,", "tokens": ["Die", "Menschheit", "hoch", "wie", "ei\u00b7ne", "Ro\u00b7se", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ihr tiefster Kelch vom Sonnenlicht durchdrungen:", "tokens": ["Ihr", "tiefs\u00b7ter", "Kelch", "vom", "Son\u00b7nen\u00b7licht", "durch\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Sehnen bleibt, das uns hin\u00fcberzieht,", "tokens": ["Das", "Seh\u00b7nen", "bleibt", ",", "das", "uns", "hin\u00b7\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Nachtigallenlied ist nicht verklungen,", "tokens": ["Das", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7lied", "ist", "nicht", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei dessen Ton die Knospen sind erbl\u00fcht!", "tokens": ["Bei", "des\u00b7sen", "Ton", "die", "Knos\u00b7pen", "sind", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}