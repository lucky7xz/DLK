{"textgrid.poem.59819": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mirza-Schaffy! Nun werde vern\u00fcnftig,", "tokens": ["Mir\u00b7za\u00b7Schaffy", "!", "Nun", "wer\u00b7de", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "La\u00df deines Wesens Unstetigkeit \u2013", "tokens": ["La\u00df", "dei\u00b7nes", "We\u00b7sens", "Un\u00b7ste\u00b7tig\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zu ernsterem Gesch\u00e4fte k\u00fcnftig", "tokens": ["Zu", "erns\u00b7te\u00b7rem", "Ge\u00b7sch\u00e4f\u00b7te", "k\u00fcnf\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwende deine T\u00e4tigkeit!", "tokens": ["Ver\u00b7wen\u00b7de", "dei\u00b7ne", "T\u00e4\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh Mirza-Hadshi-Aghassi", "tokens": ["Sieh", "Mir\u00b7za\u00b7Hads\u00b7hi\u00b7Ag\u00b7has\u00b7si"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das ein Herr geworden ist!", "tokens": ["Was", "das", "ein", "Herr", "ge\u00b7wor\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War fr\u00fcher ein ganz gemeiner Mann,", "tokens": ["War", "fr\u00fc\u00b7her", "ein", "ganz", "ge\u00b7mei\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie er jetzt behangen mit Orden ist!", "tokens": ["Wie", "er", "jetzt", "be\u00b7han\u00b7gen", "mit", "Or\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Drum widme deine Kr\u00e4fte dem Staate,", "tokens": ["Drum", "wid\u00b7me", "dei\u00b7ne", "Kr\u00e4f\u00b7te", "dem", "Staa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr den sie sonst verloren sind,", "tokens": ["F\u00fcr", "den", "sie", "sonst", "ver\u00b7lo\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil meist die gr\u00f6\u00dften Herrn im Rate", "tokens": ["Weil", "meist", "die", "gr\u00f6\u00df\u00b7ten", "Herrn", "im", "Ra\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zugleich die gr\u00f6\u00dften Toren sind.", "tokens": ["Zu\u00b7gleich", "die", "gr\u00f6\u00df\u00b7ten", "To\u00b7ren", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich sprach: Viel andre werden schon", "tokens": ["Ich", "sprach", ":", "Viel", "and\u00b7re", "wer\u00b7den", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "PIS", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschickt zu solchem Platz sein,", "tokens": ["Ge\u00b7schickt", "zu", "sol\u00b7chem", "Platz", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schwerer d\u00fcrfte f\u00fcr meine Person", "tokens": ["Doch", "schwe\u00b7rer", "d\u00fcrf\u00b7te", "f\u00fcr", "mei\u00b7ne", "Per\u00b7son"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein passender Ersatz sein.", "tokens": ["Ein", "pas\u00b7sen\u00b7der", "Er\u00b7satz", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Darum: Zeigst du mir einen Mann,", "tokens": ["Da\u00b7rum", ":", "Zeigst", "du", "mir", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der jetzt im Rate Stimm' und Sitz hat", "tokens": ["Der", "jetzt", "im", "Ra\u00b7te", "Stimm'", "und", "Sitz", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und solche Lieder singen kann", "tokens": ["Und", "sol\u00b7che", "Lie\u00b7der", "sin\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich und meinen Geist und Witz hat:", "tokens": ["Wie", "ich", "und", "mei\u00b7nen", "Geist", "und", "Witz", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So lasse ich meine Unstetigkeit,", "tokens": ["So", "las\u00b7se", "ich", "mei\u00b7ne", "Un\u00b7ste\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Lasse Trinken, Singen und Dichtung", "tokens": ["Las\u00b7se", "Trin\u00b7ken", ",", "Sin\u00b7gen", "und", "Dich\u00b7tung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und gebe meiner T\u00e4tigkeit", "tokens": ["Und", "ge\u00b7be", "mei\u00b7ner", "T\u00e4\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sofort eine andere Richtung.", "tokens": ["So\u00b7fort", "ei\u00b7ne", "an\u00b7de\u00b7re", "Rich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}