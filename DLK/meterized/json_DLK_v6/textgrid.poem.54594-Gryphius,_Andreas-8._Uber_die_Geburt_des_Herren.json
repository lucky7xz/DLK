{"textgrid.poem.54594": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "8. Uber die Geburt des Herren", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch/ das Spill der Zeit verlohr die Ewigkeit/", "tokens": ["Der", "Mensch", "/", "das", "Spill", "der", "Zeit", "ver\u00b7lohr", "die", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gott der ewig ist nimt an sich Fleisch und Zeit/", "tokens": ["Und", "Gott", "der", "e\u00b7wig", "ist", "nimt", "an", "sich", "Fleisch", "und", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJD", "VAFIN", "VVFIN", "APPR", "PRF", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und tr\u00e4gt der Zeitten Fluch/ den Tod/ da\u00df er das Leben", "tokens": ["Und", "tr\u00e4gt", "der", "Zeit\u00b7ten", "Fluch", "/", "den", "Tod", "/", "da\u00df", "er", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem was hir sterblich ist auff ewig k\u00f6nne geben.", "tokens": ["Dem", "was", "hir", "sterb\u00b7lich", "ist", "auff", "e\u00b7wig", "k\u00f6n\u00b7ne", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ADV", "ADJD", "VAFIN", "APPR", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird/ was noch bi\u00dfher auf diser Welt gefehlt", "tokens": ["So", "wird", "/", "was", "noch", "bi\u00df\u00b7her", "auf", "di\u00b7ser", "Welt", "ge\u00b7fehlt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "PWS", "ADV", "ADV", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Die Zeitt und Ewigkeit! O Wunderding! verm\u00e4hlt.", "tokens": ["Die", "Zeitt", "und", "E\u00b7wig\u00b7keit", "!", "O", "Wun\u00b7der\u00b7ding", "!", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$.", "NE", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}