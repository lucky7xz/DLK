{"dta.poem.9447": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n Die Sechste Abtheilung.  \n Die Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich eylte zum Garten und wolte mich letzen/", "tokens": ["Ich", "eyl\u00b7te", "zum", "Gar\u00b7ten", "und", "wol\u00b7te", "mich", "let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich schaute die N\u00fcsse/ die liebliche Bach/", "tokens": ["Ich", "schau\u00b7te", "die", "N\u00fcs\u00b7se", "/", "die", "lieb\u00b7li\u00b7che", "Bach", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich wolte mein Hertze mit Reben erg\u00f6tzen/", "tokens": ["Ich", "wol\u00b7te", "mein", "Hert\u00b7ze", "mit", "Re\u00b7ben", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit lieblichen \u00e4pfeln/ ich eylte gemach;", "tokens": ["Mit", "lieb\u00b7li\u00b7chen", "\u00e4p\u00b7feln", "/", "ich", "eyl\u00b7te", "ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$(", "PPER", "VVFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Kont aber nicht wissen/", "tokens": ["Kont", "a\u00b7ber", "nicht", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie da\u00df ich gerissen", "tokens": ["Wie", "da\u00df", "ich", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zuricke von dar/", "tokens": ["Zu\u00b7ri\u00b7cke", "von", "dar", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Es hatt mich getragen", "tokens": ["Es", "hatt", "mich", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Amminadabs wagen;", "tokens": ["Am\u00b7mi\u00b7na\u00b7dabs", "wa\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Nun seh\u2019 ich die Pferde/ das hurtige paar.", "tokens": ["Nun", "seh'", "ich", "die", "Pfer\u00b7de", "/", "das", "hur\u00b7ti\u00b7ge", "paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}