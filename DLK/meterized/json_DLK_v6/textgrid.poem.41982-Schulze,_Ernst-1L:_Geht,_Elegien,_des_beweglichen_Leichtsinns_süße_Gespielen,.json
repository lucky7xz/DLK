{"textgrid.poem.41982": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geht, Elegien, des beweglichen Leichtsinns s\u00fc\u00dfe Gespielen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht, Elegien, des beweglichen Leichtsinns s\u00fc\u00dfe Gespielen,", "tokens": ["Geht", ",", "E\u00b7le\u00b7gi\u00b7en", ",", "des", "be\u00b7weg\u00b7li\u00b7chen", "Leicht\u00b7sinns", "s\u00fc\u00b7\u00dfe", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+---+--+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Geht Elegien, des Gef\u00fchls s\u00fc\u00dfe Gespielen auch ihr.", "tokens": ["Geht", "E\u00b7le\u00b7gi\u00b7en", ",", "des", "Ge\u00b7f\u00fchls", "s\u00fc\u00b7\u00dfe", "Ge\u00b7spie\u00b7len", "auch", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ART", "NN", "ADJA", "NN", "ADV", "PPER", "$."], "meter": "+-+-+-+-+--+-++", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Amor hat euch gezeugt, und die Grazie wiegte die Kindlein,", "tokens": ["A\u00b7mor", "hat", "euch", "ge\u00b7zeugt", ",", "und", "die", "Gra\u00b7zie", "wieg\u00b7te", "die", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,", "KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und mit Bl\u00fcthenged\u00fcft zog sie die Freundlichen auf.", "tokens": ["Und", "mit", "Bl\u00fc\u00b7then\u00b7ge\u00b7d\u00fcft", "zog", "sie", "die", "Freund\u00b7li\u00b7chen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fr\u00f6hlich umscherztet im \u00fcppigen Strahl muthwilliger Thorheit", "tokens": ["Fr\u00f6h\u00b7lich", "um\u00b7scherz\u00b7tet", "im", "\u00fcp\u00b7pi\u00b7gen", "Strahl", "mut\u00b7hwil\u00b7li\u00b7ger", "Thor\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Rings ihr den Freund, und den Ku\u00df gabt ihr dem Bittenden gern,", "tokens": ["Rings", "ihr", "den", "Freund", ",", "und", "den", "Ku\u00df", "gabt", "ihr", "dem", "Bit\u00b7ten\u00b7den", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wenn er euch leise genaht und euch fesselte rasch, wie des Herzens", "tokens": ["Wenn", "er", "euch", "lei\u00b7se", "ge\u00b7naht", "und", "euch", "fes\u00b7sel\u00b7te", "rasch", ",", "wie", "des", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP", "KON", "PPER", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Laun' und des leichten Gef\u00fchls wechselnder Hauch ihm gebot.", "tokens": ["Laun'", "und", "des", "leich\u00b7ten", "Ge\u00b7f\u00fchls", "wech\u00b7seln\u00b7der", "Hauch", "ihm", "ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Ach, dann lehrtet ihr mich s\u00fc\u00dft\u00f6nende Lieder, und Sehnsucht", "tokens": ["Ach", ",", "dann", "lehr\u00b7tet", "ihr", "mich", "s\u00fc\u00df\u00b7t\u00f6\u00b7nen\u00b7de", "Lie\u00b7der", ",", "und", "Sehn\u00b7sucht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJA", "NN", "$,", "KON", "NN"], "meter": "+-+-+-++--+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Lauscht' in dem einen, und Lust strahlt euch im anderen Blick.", "tokens": ["Lauscht'", "in", "dem", "ei\u00b7nen", ",", "und", "Lust", "strahlt", "euch", "im", "an\u00b7de\u00b7ren", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ART", "$,", "KON", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Aber vorbei sind Spiel und Gesang! Vor dem brausenden Nordhauch", "tokens": ["A\u00b7ber", "vor\u00b7bei", "sind", "Spiel", "und", "Ge\u00b7sang", "!", "Vor", "dem", "brau\u00b7sen\u00b7den", "Nord\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Zittert der Hain, ", "tokens": ["Zit\u00b7tert", "der", "Hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Amor schwand, und die Grazie weint; die verwaiseten Kinder", "tokens": ["A\u00b7mor", "schwand", ",", "und", "die", "Gra\u00b7zie", "weint", ";", "die", "ver\u00b7wai\u00b7se\u00b7ten", "Kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "M\u00fc\u00dft' ich, die reizenden, ach, kleiden in's Trauergewand.", "tokens": ["M\u00fc\u00dft'", "ich", ",", "die", "rei\u00b7zen\u00b7den", ",", "ach", ",", "klei\u00b7den", "in's", "Trau\u00b7er\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "ADJA", "$,", "ITJ", "$,", "ADJA", "APPRART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Doch Muthwillige ziemet ja nicht schmerzk\u00fcndender Flor, und", "tokens": ["Doch", "Mut\u00b7hwil\u00b7li\u00b7ge", "zie\u00b7met", "ja", "nicht", "schmerz\u00b7k\u00fcn\u00b7den\u00b7der", "Flor", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "PTKNEG", "ADJA", "NN", "$,", "KON"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "L\u00e4cheln w\u00fcrdet ihr doch, fleht' ich um Thr\u00e4nen euch an.", "tokens": ["L\u00e4\u00b7cheln", "w\u00fcr\u00b7det", "ihr", "doch", ",", "fleht'", "ich", "um", "Thr\u00e4\u00b7nen", "euch", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Auf dann! Fittige schenkte der Gott euch; flattert hinweg nun,", "tokens": ["Auf", "dann", "!", "Fit\u00b7ti\u00b7ge", "schenk\u00b7te", "der", "Gott", "euch", ";", "flat\u00b7tert", "hin\u00b7weg", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "NN", "VVFIN", "ART", "NN", "PPER", "$.", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Sucht mir den Vater und bringt hold den Verlohrnen zur\u00fcck!", "tokens": ["Sucht", "mir", "den", "Va\u00b7ter", "und", "bringt", "hold", "den", "Ver\u00b7lohr\u00b7nen", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Gern wohl leihet der Grazie Huld euch den G\u00fcrtel der Anmuth;", "tokens": ["Gern", "wohl", "lei\u00b7het", "der", "Gra\u00b7zie", "Huld", "euch", "den", "G\u00fcr\u00b7tel", "der", "An\u00b7muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Fesselt den Fl\u00fcchtigen nur, endlich belohnt er es selbst.", "tokens": ["Fes\u00b7selt", "den", "Fl\u00fcch\u00b7ti\u00b7gen", "nur", ",", "end\u00b7lich", "be\u00b7lohnt", "er", "es", "selbst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Sagt ihm Sch\u00f6nes von mir, da\u00df ich euch voll Treue gepflegt und", "tokens": ["Sagt", "ihm", "Sch\u00f6\u00b7nes", "von", "mir", ",", "da\u00df", "ich", "euch", "voll", "Treu\u00b7e", "ge\u00b7pflegt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "NN", "VVPP", "KON"], "meter": "+-+----+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Z\u00e4rtlich geliebt, da\u00df ich still, als ihr entschwunden, geweint.", "tokens": ["Z\u00e4rt\u00b7lich", "ge\u00b7liebt", ",", "da\u00df", "ich", "still", ",", "als", "ihr", "ent\u00b7schwun\u00b7den", ",", "ge\u00b7weint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Ach, dann kehrt er zur\u00fcck, und die Grazie l\u00e4chelt noch einmal,", "tokens": ["Ach", ",", "dann", "kehrt", "er", "zu\u00b7r\u00fcck", ",", "und", "die", "Gra\u00b7zie", "l\u00e4\u00b7chelt", "noch", "ein\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Und das verlassene Spiel spiel' ich von neuem mit euch.", "tokens": ["Und", "das", "ver\u00b7las\u00b7se\u00b7ne", "Spiel", "spiel'", "ich", "von", "neu\u00b7em", "mit", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "APPR", "PPER", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}