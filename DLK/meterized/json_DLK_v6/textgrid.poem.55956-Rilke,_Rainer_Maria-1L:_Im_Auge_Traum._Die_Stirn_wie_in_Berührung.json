{"textgrid.poem.55956": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Auge Traum. Die Stirn wie in Ber\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Auge Traum. Die Stirn wie in Ber\u00fchrung", "tokens": ["Im", "Au\u00b7ge", "Traum", ".", "Die", "Stirn", "wie", "in", "Be\u00b7r\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$.", "ART", "NN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit etwas Fernem. Um den Mund enorm", "tokens": ["mit", "et\u00b7was", "Fer\u00b7nem", ".", "Um", "den", "Mund", "en\u00b7orm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "$.", "KOUI", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "viel Jugend, ungel\u00e4chelte Verf\u00fchrung,", "tokens": ["viel", "Ju\u00b7gend", ",", "un\u00b7ge\u00b7l\u00e4\u00b7chel\u00b7te", "Ver\u00b7f\u00fch\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und vor der vollen schm\u00fcckenden Verschn\u00fcrung", "tokens": ["und", "vor", "der", "vol\u00b7len", "schm\u00fc\u00b7cken\u00b7den", "Ver\u00b7schn\u00fc\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "der schlanken adeligen Uniform", "tokens": ["der", "schlan\u00b7ken", "a\u00b7de\u00b7li\u00b7gen", "U\u00b7ni\u00b7form"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "der S\u00e4belkorb und beide H\u00e4nde \u2013, die", "tokens": ["der", "S\u00e4\u00b7bel\u00b7korb", "und", "bei\u00b7de", "H\u00e4n\u00b7de", "\u2013", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "$(", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "abwarten, ruhig, zu nichts hingedr\u00e4ngt.", "tokens": ["ab\u00b7war\u00b7ten", ",", "ru\u00b7hig", ",", "zu", "nichts", "hin\u00b7ge\u00b7dr\u00e4ngt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und nun fast nicht mehr sichtbar: als ob sie", "tokens": ["Und", "nun", "fast", "nicht", "mehr", "sicht\u00b7bar", ":", "als", "ob", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$.", "KOKOM", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "zuerst, die Fernes greifenden, verschw\u00e4nden.", "tokens": ["zu\u00b7erst", ",", "die", "Fer\u00b7nes", "grei\u00b7fen\u00b7den", ",", "ver\u00b7schw\u00e4n\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und alles andre mit sich selbst verh\u00e4ngt", "tokens": ["Und", "al\u00b7les", "and\u00b7re", "mit", "sich", "selbst", "ver\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PIS", "APPR", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und ausgel\u00f6scht als ob wirs nicht verst\u00e4nden", "tokens": ["und", "aus\u00b7ge\u00b7l\u00f6scht", "als", "ob", "wirs", "nicht", "ver\u00b7st\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "KOUS", "PIS", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "und tief aus seiner eignen Tiefe tr\u00fcb \u2013.", "tokens": ["und", "tief", "aus", "sei\u00b7ner", "eig\u00b7nen", "Tie\u00b7fe", "tr\u00fcb", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du schnell vergehendes Daguerreotyp", "tokens": ["Du", "schnell", "ver\u00b7ge\u00b7hen\u00b7des", "Da\u00b7guer\u00b7reo\u00b7typ"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in meinen langsamer vergehenden H\u00e4nden.", "tokens": ["in", "mei\u00b7nen", "lang\u00b7sa\u00b7mer", "ver\u00b7ge\u00b7hen\u00b7den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}}}}}