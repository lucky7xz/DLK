{"textgrid.poem.26160": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Wald ist jetzt eine dunkle Laube", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wald ist jetzt eine dunkle Laube.", "tokens": ["Der", "Wald", "ist", "jetzt", "ei\u00b7ne", "dunk\u00b7le", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er sitzt weit ab vom Alltagstaube", "tokens": ["Er", "sitzt", "weit", "ab", "vom", "All\u00b7tags\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In ewiger, wogender Festlichkeit", "tokens": ["In", "e\u00b7wi\u00b7ger", ",", "wo\u00b7gen\u00b7der", "Fest\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und vertreibt uns mit Liedern die Zeit.", "tokens": ["Und", "ver\u00b7treibt", "uns", "mit", "Lie\u00b7dern", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wir k\u00f6nnen dort unter den lautlosen Buchen", "tokens": ["Wir", "k\u00f6n\u00b7nen", "dort", "un\u00b7ter", "den", "laut\u00b7lo\u00b7sen", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Nicht nach dem schweren Golde suchen.", "tokens": ["Nicht", "nach", "dem", "schwe\u00b7ren", "Gol\u00b7de", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sehen den Spuren der Rehe nach,", "tokens": ["Wir", "se\u00b7hen", "den", "Spu\u00b7ren", "der", "Re\u00b7he", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die wohnen leicht unter des Waldes Dach", "tokens": ["Die", "woh\u00b7nen", "leicht", "un\u00b7ter", "des", "Wal\u00b7des", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und sind wie Verliebte im Mond nachtwandelnd wach.", "tokens": ["Und", "sind", "wie", "Ver\u00b7lieb\u00b7te", "im", "Mond", "nacht\u00b7wan\u00b7delnd", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "NN", "APPRART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}