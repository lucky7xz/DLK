{"textgrid.poem.46552": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als Knabe war mein gr\u00f6\u00dftes Wohlbehagen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Knabe war mein gr\u00f6\u00dftes Wohlbehagen,", "tokens": ["Als", "Kna\u00b7be", "war", "mein", "gr\u00f6\u00df\u00b7tes", "Wohl\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schwesterchen im Arm zu tragen,", "tokens": ["Ein", "Schwes\u00b7ter\u00b7chen", "im", "Arm", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gefl\u00fcchtet aus der engen Stub' hinaus,", "tokens": ["Ge\u00b7fl\u00fcch\u00b7tet", "aus", "der", "en\u00b7gen", "Stub'", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im weiten Garten hinterm Haus.", "tokens": ["Im", "wei\u00b7ten", "Gar\u00b7ten", "hin\u00b7term", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch hatte bald der Tod mein Wohlbehagen", "tokens": ["Doch", "hat\u00b7te", "bald", "der", "Tod", "mein", "Wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir aus dem Arm zu Grab getragen,", "tokens": ["Mir", "aus", "dem", "Arm", "zu", "Grab", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in des Lebens Braus verga\u00df der Knab'", "tokens": ["Und", "in", "des", "Le\u00b7bens", "Braus", "ver\u00b7ga\u00df", "der", "Knab'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Schwesterchen im stillen Grab.", "tokens": ["Das", "Schwes\u00b7ter\u00b7chen", "im", "stil\u00b7len", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch hab' ich mit wehm\u00fcthigem Behagen,", "tokens": ["Doch", "hab'", "ich", "mit", "weh\u00b7m\u00fct\u00b7hi\u00b7gem", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Zufall j\u00fcngst ins Dorf getragen,", "tokens": ["Vom", "Zu\u00b7fall", "j\u00fcngst", "ins", "Dorf", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich die Kinderjahre sah vergehn,", "tokens": ["Wo", "ich", "die", "Kin\u00b7der\u00b7jah\u00b7re", "sah", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach ihrem Grab mich umgesehn.", "tokens": ["Nach", "ih\u00b7rem", "Grab", "mich", "um\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Inzwischen hatt' ich gr\u00f6\u00dfres Wohlbehagen,", "tokens": ["I\u00b7nzwi\u00b7schen", "hatt'", "ich", "gr\u00f6\u00df\u00b7res", "Wohl\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ein T\u00f6chterchen im Arm zu tragen,", "tokens": ["Ein", "T\u00f6ch\u00b7ter\u00b7chen", "im", "Arm", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das, kommend still nach lauter Buben Tro\u00df,", "tokens": ["Das", ",", "kom\u00b7mend", "still", "nach", "lau\u00b7ter", "Bu\u00b7ben", "Tro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "ADJD", "APPR", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein halbes Dutzend lieblich schlo\u00df.", "tokens": ["Mein", "hal\u00b7bes", "Dut\u00b7zend", "lieb\u00b7lich", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun hat der finstre St\u00f6rer im Behagen,", "tokens": ["Nun", "hat", "der", "finst\u00b7re", "St\u00f6\u00b7rer", "im", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tod, auch dies davon getragen,", "tokens": ["Der", "Tod", ",", "auch", "dies", "da\u00b7von", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PDS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und an des Herzens leergewordnem Platz", "tokens": ["Und", "an", "des", "Her\u00b7zens", "leer\u00b7ge\u00b7word\u00b7nem", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ist zu hoffen f\u00fcr Ersatz?", "tokens": ["Was", "ist", "zu", "hof\u00b7fen", "f\u00fcr", "Er\u00b7satz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Soll ich noch mit Gro\u00dfvaterwohlbehagen", "tokens": ["Soll", "ich", "noch", "mit", "Gro\u00df\u00b7va\u00b7ter\u00b7wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Arm ein Enkelinnchen tragen?", "tokens": ["Im", "Arm", "ein", "En\u00b7ke\u00b7linn\u00b7chen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fcrchte, der die Beiden hat geraubt,", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "der", "die", "Bei\u00b7den", "hat", "ge\u00b7raubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er das Dritte nicht erlaubt.", "tokens": ["Da\u00df", "er", "das", "Drit\u00b7te", "nicht", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich f\u00fcrchte nicht, da\u00df er mit Unbehagen", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nicht", ",", "da\u00df", "er", "mit", "Un\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Enkelinnchen fort wird tragen;", "tokens": ["Das", "En\u00b7ke\u00b7linn\u00b7chen", "fort", "wird", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er selber wird zuvor mich f\u00fchren ein", "tokens": ["Er", "sel\u00b7ber", "wird", "zu\u00b7vor", "mich", "f\u00fch\u00b7ren", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu Schwesterchen und T\u00f6chterlein.", "tokens": ["Zu", "Schwes\u00b7ter\u00b7chen", "und", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von Beiden welches werd' ich mit Behagen", "tokens": ["Von", "Bei\u00b7den", "wel\u00b7ches", "werd'", "ich", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PWS", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am liebsten dort im Arme tragen?", "tokens": ["Am", "liebs\u00b7ten", "dort", "im", "Ar\u00b7me", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fcrchte, da\u00df die Schwester und das Kind", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "da\u00df", "die", "Schwes\u00b7ter", "und", "das", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dort meinem Arm entwachsen sind.", "tokens": ["Dort", "mei\u00b7nem", "Arm", "ent\u00b7wach\u00b7sen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ob ich sie werd', ob sie mich werden kennen?", "tokens": ["Ob", "ich", "sie", "werd'", ",", "ob", "sie", "mich", "wer\u00b7den", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ich sie werd' und sie mich nennen?", "tokens": ["Wie", "ich", "sie", "werd'", "und", "sie", "mich", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "KON", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich denke, da\u00df vorm gro\u00dfen Vater mu\u00df", "tokens": ["Ich", "den\u00b7ke", ",", "da\u00df", "vorm", "gro\u00b7\u00dfen", "Va\u00b7ter", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPRART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verstummen Vaterkindesgru\u00df.", "tokens": ["Ver\u00b7stum\u00b7men", "Va\u00b7ter\u00b7kin\u00b7des\u00b7gru\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch wird der Schwesterbrudergru\u00df noch gelten,", "tokens": ["Doch", "wird", "der", "Schwes\u00b7ter\u00b7bru\u00b7der\u00b7gru\u00df", "noch", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auch den Tausch werd' ich nicht schelten,", "tokens": ["Und", "auch", "den", "Tausch", "werd'", "ich", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn, die auf Erden meine Tochter war,", "tokens": ["Wenn", ",", "die", "auf", "Er\u00b7den", "mei\u00b7ne", "Toch\u00b7ter", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPR", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich dort mir stellt als Schwester dar.", "tokens": ["Sich", "dort", "mir", "stellt", "als", "Schwes\u00b7ter", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPER", "VVFIN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}