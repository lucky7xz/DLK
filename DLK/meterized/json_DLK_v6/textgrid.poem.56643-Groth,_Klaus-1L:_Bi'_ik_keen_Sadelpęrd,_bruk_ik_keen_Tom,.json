{"textgrid.poem.56643": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bi' ik keen Sadelp\u0119rd, bruk ik keen Tom,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bi' ik keen Sadelp\u0119rd, bruk ik keen Tom,", "tokens": ["Bi'", "ik", "ke\u00b7en", "Sa\u00b7del\u00b7p\u0119rd", ",", "bruk", "ik", "ke\u00b7en", "Tom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "$,", "NE", "NE", "VVFIN", "NE", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Pl\u00fcck ik min Ritpitsch af uten Wichelbom!", "tokens": ["Pl\u00fcck", "ik", "min", "Rit\u00b7pitsch", "af", "u\u00b7ten", "Wi\u00b7chel\u00b7bom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "NE", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nachtens wennt d\u00fcster is, stormt dat un ras't,", "tokens": ["Nach\u00b7tens", "wennt", "d\u00fcs\u00b7ter", "is", ",", "stormt", "dat", "un", "ras't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "FM", "$,", "VVFIN", "ART", "FM", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Min is dat beste P\u0119rd, wat de Wischen gras't!", "tokens": ["Min", "is", "dat", "bes\u00b7te", "P\u0119rd", ",", "wat", "de", "Wi\u00b7schen", "gras't", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "FM", "ART", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Black, reck de Poten ut! fleeg as de Wind!", "tokens": ["Black", ",", "reck", "de", "Po\u00b7ten", "ut", "!", "fleeg", "as", "de", "Wind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Drigg mi d\u0153r Storm un Nach bet min leefste Kind!", "tokens": ["Drigg", "mi", "d\u0153r", "Storm", "un", "Nach", "bet", "min", "leefs\u00b7te", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "APPR", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Weest du wa't Finster klickt? weest du wa't deit?", "tokens": ["Weest", "du", "wa't", "Fins\u00b7ter", "klickt", "?", "weest", "du", "wa't", "deit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "VVFIN", "$.", "VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Spring ik bi Leefste rin, spring du na de Weid! \u2013", "tokens": ["Spring", "ik", "bi", "Leefs\u00b7te", "rin", ",", "spring", "du", "na", "de", "Weid", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "PPER", "NE", "NE", "NE", "$.", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Lusti is't L\u0119ben ahn T\u0153gel un Tom!", "tokens": ["Lus\u00b7ti", "is't", "L\u0119\u00b7ben", "ahn", "T\u0153\u00b7gel", "un", "Tom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Vageln pl\u00fcckt K\u00fcrschen: wokeen h\u00f6rt de Bom?", "tokens": ["Va\u00b7geln", "pl\u00fcckt", "K\u00fcr\u00b7schen", ":", "wo\u00b7keen", "h\u00f6rt", "de", "Bom", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "PWAV", "VVFIN", "NE", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}