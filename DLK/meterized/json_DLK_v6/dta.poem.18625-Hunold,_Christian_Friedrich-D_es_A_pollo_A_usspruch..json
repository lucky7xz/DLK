{"dta.poem.18625": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D es  A pollo  A usspruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Apollo hatte kaum ihr klagen angeh\u00f6rt/", "tokens": ["A\u00b7pol\u00b7lo", "hat\u00b7te", "kaum", "ihr", "kla\u00b7gen", "an\u00b7ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPER", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sein entbrandter Grimm in diese Worte brach:", "tokens": ["Als", "sein", "ent\u00b7brand\u00b7ter", "Grimm", "in", "die\u00b7se", "Wor\u00b7te", "brach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NE", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie da\u00df die dumme Welt die Majest\u00e4t versehret/", "tokens": ["Wie", "da\u00df", "die", "dum\u00b7me", "Welt", "die", "Ma\u00b7jes\u00b7t\u00e4t", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und l\u00e4uffet straffbarlich der eignen Schande nach?", "tokens": ["Und", "l\u00e4uf\u00b7fet", "straff\u00b7bar\u00b7lich", "der", "eig\u00b7nen", "Schan\u00b7de", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hierauff so muste gleich Mercur die Fl\u00fcgel schwingen/", "tokens": ["Hier\u00b7auff", "so", "mus\u00b7te", "gleich", "Mer\u00b7cur", "die", "Fl\u00fc\u00b7gel", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "ADV", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er eilte wie ein Pfeil auff unser Saal-Athen/", "tokens": ["Er", "eil\u00b7te", "wie", "ein", "Pfeil", "auff", "un\u00b7ser", "Saa\u00b7lA\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des F\u00fcrsten wille war den Musen vorzubringen/", "tokens": ["Des", "F\u00fcrs\u00b7ten", "wil\u00b7le", "war", "den", "Mu\u00b7sen", "vor\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das kein Poete mehr gecr\u00f6net solte gehn.", "tokens": ["Das", "kein", "Poe\u00b7te", "mehr", "ge\u00b7cr\u00f6\u00b7net", "sol\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Es w\u00e4r nun \u00fcberall was gantz gemeines worden/", "tokens": ["Es", "w\u00e4r", "nun", "\u00fc\u00b7be\u00b7rall", "was", "gantz", "ge\u00b7mei\u00b7nes", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PWS", "ADV", "ADJA", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df itzt der Lorber-Crantz die Esels-Crone sey/", "tokens": ["Da\u00df", "itzt", "der", "Lor\u00b7ber\u00b7Crantz", "die", "E\u00b7sels\u00b7Cro\u00b7ne", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man schriebe sich hiermit in aller Hasen-Orden/", "tokens": ["Man", "schrie\u00b7be", "sich", "hier\u00b7mit", "in", "al\u00b7ler", "Ha\u00b7sen\u00b7Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PAV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es sey nichts schimpflichers/ als die Poeterey.", "tokens": ["Es", "sey", "nichts", "schimpf\u00b7li\u00b7chers", "/", "als", "die", "Poe\u00b7te\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJA", "$(", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Man sehe halb erstaunt bey den erblasten Leichen", "tokens": ["Man", "se\u00b7he", "halb", "er\u00b7staunt", "bey", "den", "er\u00b7blas\u00b7ten", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie mancher St\u00fcmper sich um seine Zeit gebracht/", "tokens": ["Wie", "man\u00b7cher", "St\u00fcm\u00b7per", "sich", "um", "sei\u00b7ne", "Zeit", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Hunde m\u00f6chten wohl den Versiver beseichen/", "tokens": ["Die", "Hun\u00b7de", "m\u00f6ch\u00b7ten", "wohl", "den", "Ver\u00b7si\u00b7ver", "be\u00b7sei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den seine Thorheit uns bi\u00dfher bekandt gemacht.", "tokens": ["Den", "sei\u00b7ne", "Thor\u00b7heit", "uns", "bi\u00df\u00b7her", "be\u00b7kandt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie solten ihre Kunst noch dieser Warnung richten/", "tokens": ["Sie", "sol\u00b7ten", "ih\u00b7re", "Kunst", "noch", "die\u00b7ser", "War\u00b7nung", "rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die ein gelehrter Mann vor seinen Abschied that:", "tokens": ["Die", "ein", "ge\u00b7lehr\u00b7ter", "Mann", "vor", "sei\u00b7nen", "Ab\u00b7schied", "that", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Man lasse mir doch nichts noch meinen Tode dichten/", "tokens": ["Man", "las\u00b7se", "mir", "doch", "nichts", "noch", "mei\u00b7nen", "To\u00b7de", "dich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PIS", "ADV", "PPOSAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als was Geschicklichkeit zu vor poliret hat.", "tokens": ["Als", "was", "Ge\u00b7schick\u00b7lich\u00b7keit", "zu", "vor", "po\u00b7li\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "PTKZU", "APPR", "NE", "VAFIN", "$."], "meter": "++-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Magister-Grillen-Kopff/ der sein Barethgen nehme/", "tokens": ["Ma\u00b7gis\u00b7ter\u00b7Gril\u00b7len\u00b7Kopff", "/", "der", "sein", "Ba\u00b7reth\u00b7gen", "neh\u00b7me", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der machte vielen auch den Narren-Kasten auff.", "tokens": ["Der", "mach\u00b7te", "vie\u00b7len", "auch", "den", "Nar\u00b7ren\u00b7Kas\u00b7ten", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja so ein Ignorant den Doctor-Hut bek\u00e4me/", "tokens": ["Ja", "so", "ein", "Ig\u00b7no\u00b7rant", "den", "Doc\u00b7tor\u00b7Hut", "be\u00b7k\u00e4\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So schimpffte man noch mehrdurch schlimme Verse drauff.", "tokens": ["So", "schimpff\u00b7te", "man", "noch", "mehr\u00b7durch", "schlim\u00b7me", "Ver\u00b7se", "drauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Di\u00df lie\u00df die Majest\u00e4t des F\u00fcrstens uns befehlen/", "tokens": ["Di\u00df", "lie\u00df", "die", "Ma\u00b7jes\u00b7t\u00e4t", "des", "F\u00fcrs\u00b7tens", "uns", "be\u00b7feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und sprach zur Poesie in heisser Lieb' entflammt:", "tokens": ["Und", "sprach", "zur", "Poe\u00b7sie", "in", "heis\u00b7ser", "Lieb'", "ent\u00b7flammt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Dir sol kein R\u00e4uber mehr den gr\u00fcnen Lorbeer stehlen/", "tokens": ["Dir", "sol", "kein", "R\u00e4u\u00b7ber", "mehr", "den", "gr\u00fc\u00b7nen", "Lor\u00b7beer", "steh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dieweil der edle Zweig von hohen G\u00f6ttern stammt.", "tokens": ["Die\u00b7weil", "der", "ed\u00b7le", "Zweig", "von", "ho\u00b7hen", "G\u00f6t\u00b7tern", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Musen/ so uns stets als ihren Schutz-Gott lieben/", "tokens": ["Die", "Mu\u00b7sen", "/", "so", "uns", "stets", "als", "ih\u00b7ren", "Schutz\u00b7Gott", "lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die wissen allb", "tokens": ["Die", "wis\u00b7sen", "allb"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Da\u00df du ein hoher Trieb des Geistes bist geblieben/", "tokens": ["Da\u00df", "du", "ein", "ho\u00b7her", "Trieb", "des", "Geis\u00b7tes", "bist", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df Klugheit und Verstand dich nur anbethen mu\u00df.", "tokens": ["Da\u00df", "Klug\u00b7heit", "und", "Ver\u00b7stand", "dich", "nur", "an\u00b7be\u00b7then", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Es sol kein Palatin mehr zehen Thaler nehmen/", "tokens": ["Es", "sol", "kein", "Pa\u00b7la\u00b7tin", "mehr", "ze\u00b7hen", "Tha\u00b7ler", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "CARD", "NN", "VVINF", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Denn dieses Kopff-Geld ist ein B\u00e4renhenter-Lohn.", "tokens": ["Denn", "die\u00b7ses", "Kopf\u00b7fGeld", "ist", "ein", "B\u00e4\u00b7ren\u00b7hen\u00b7ter\u00b7Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Musen werden sich dergleichen Thorheit sch\u00e4men/", "tokens": ["Die", "Mu\u00b7sen", "wer\u00b7den", "sich", "derg\u00b7lei\u00b7chen", "Thor\u00b7heit", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "PIS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn ihre Klugheit ehrt der Wei\u00dfheit edlen Thron.", "tokens": ["Denn", "ih\u00b7re", "Klug\u00b7heit", "ehrt", "der", "Wei\u00df\u00b7heit", "ed\u00b7len", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}