{"textgrid.poem.63596": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und doch, das ist der Dinge Lauf; auch du", "tokens": ["Und", "doch", ",", "das", "ist", "der", "Din\u00b7ge", "Lauf", ";", "auch", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$.", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erlebst es noch: ein jedes Leid am Ende,", "tokens": ["Er\u00b7lebst", "es", "noch", ":", "ein", "je\u00b7des", "Leid", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ART", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So furchtbar es gew\u00fctet, kommt zur Ruh'.", "tokens": ["So", "furcht\u00b7bar", "es", "ge\u00b7w\u00fc\u00b7tet", ",", "kommt", "zur", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Schmerz, so lang er jung ist, sind die W\u00e4nde", "tokens": ["Dem", "Schmerz", ",", "so", "lang", "er", "jung", "ist", ",", "sind", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Leibes viel zu eng, ihn einzuschlie\u00dfen.", "tokens": ["Des", "Lei\u00b7bes", "viel", "zu", "eng", ",", "ihn", "ein\u00b7zu\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er tobt umher, da\u00df er den Ausweg f\u00e4nde.", "tokens": ["Er", "tobt", "um\u00b7her", ",", "da\u00df", "er", "den", "Aus\u00b7weg", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Str\u00f6men mu\u00df er aus den Augen flie\u00dfen,", "tokens": ["In", "Str\u00f6\u00b7men", "mu\u00df", "er", "aus", "den", "Au\u00b7gen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir von den Lippen \u00e4chzen, auf die Stirn", "tokens": ["Dir", "von", "den", "Lip\u00b7pen", "\u00e4ch\u00b7zen", ",", "auf", "die", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "In kalten Tropfen perlend sich ergie\u00dfen.", "tokens": ["In", "kal\u00b7ten", "Trop\u00b7fen", "per\u00b7lend", "sich", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Am liebsten m\u00f6cht' er seiner Haft entschwirrn,", "tokens": ["Am", "liebs\u00b7ten", "m\u00f6cht'", "er", "sei\u00b7ner", "Haft", "ent\u00b7schwirrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VMFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zusamt der Seele, und dem Geier gleich", "tokens": ["Zu\u00b7samt", "der", "See\u00b7le", ",", "und", "dem", "Gei\u00b7er", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mit freiem Fl\u00fcgelschlag das All durchirrn.", "tokens": ["Mit", "frei\u00b7em", "Fl\u00fc\u00b7gel\u00b7schlag", "das", "All", "durch\u00b7irrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ermattet herrscht er dann in seinem Reich", "tokens": ["Er\u00b7mat\u00b7tet", "herrscht", "er", "dann", "in", "sei\u00b7nem", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gela\u00dfner, bricht nur selten aus den Augen", "tokens": ["Ge\u00b7la\u00df\u00b7ner", ",", "bricht", "nur", "sel\u00b7ten", "aus", "den", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und h\u00fcllt sich in Erinnern dumpf und weich.", "tokens": ["Und", "h\u00fcllt", "sich", "in", "E\u00b7rin\u00b7nern", "dumpf", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nun mag ihm nur die tiefste Stille taugen;", "tokens": ["Nun", "mag", "ihm", "nur", "die", "tiefs\u00b7te", "Stil\u00b7le", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Er haust im dunkelsten Verlie\u00df der Brust,", "tokens": ["Er", "haust", "im", "dun\u00b7kels\u00b7ten", "Ver\u00b7lie\u00df", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.18": {"text": "Begn\u00fcgt, dein Herzblut tropfenweis zu saugen.", "tokens": ["Be\u00b7gn\u00fcgt", ",", "dein", "Herz\u00b7blut", "trop\u00b7fen\u00b7weis", "zu", "sau\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die Mond' und Jahre fliehn ihm unbewu\u00dft;", "tokens": ["Die", "Mond'", "und", "Jah\u00b7re", "fliehn", "ihm", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Er ist gealtert, f\u00fchllos wie ein Greis,", "tokens": ["Er", "ist", "ge\u00b7al\u00b7tert", ",", "f\u00fchl\u00b7los", "wie", "ein", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Den kein Gewinn mehr k\u00fcmmert, noch Verlust.", "tokens": ["Den", "kein", "Ge\u00b7winn", "mehr", "k\u00fcm\u00b7mert", ",", "noch", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Doch wenn die Seele kaum noch von ihm wei\u00df,", "tokens": ["Doch", "wenn", "die", "See\u00b7le", "kaum", "noch", "von", "ihm", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Kaum des verschollnen Gastes N\u00e4h' empfindet,", "tokens": ["Kaum", "des", "ver\u00b7scholl\u00b7nen", "Gas\u00b7tes", "N\u00e4h'", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Tritt pl\u00f6tzlich er aus dem verborgnen Kreis,", "tokens": ["Tritt", "pl\u00f6tz\u00b7lich", "er", "aus", "dem", "ver\u00b7borg\u00b7nen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Erschrickt, da\u00df er die Welt verwandelt findet,", "tokens": ["Er\u00b7schrickt", ",", "da\u00df", "er", "die", "Welt", "ver\u00b7wan\u00b7delt", "fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und schilt die Seele, da\u00df sie ihn verachtet,", "tokens": ["Und", "schilt", "die", "See\u00b7le", ",", "da\u00df", "sie", "ihn", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und schilt sich selbst, da\u00df er verwelkt und schwindet.", "tokens": ["Und", "schilt", "sich", "selbst", ",", "da\u00df", "er", "ver\u00b7welkt", "und", "schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$,", "KOUS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Dann in die Kammer, drin er lang geschmachtet,", "tokens": ["Dann", "in", "die", "Kam\u00b7mer", ",", "drin", "er", "lang", "ge\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Schleicht er zur\u00fcck und sargt sich selber ein", "tokens": ["Schleicht", "er", "zu\u00b7r\u00fcck", "und", "sargt", "sich", "sel\u00b7ber", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PRF", "ADV", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und stirbt, von tiefster Einsamkeit umnachtet.", "tokens": ["Und", "stirbt", ",", "von", "tiefs\u00b7ter", "Ein\u00b7sam\u00b7keit", "um\u00b7nach\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Du aber, kannst du auch noch fr\u00f6hlich sein", "tokens": ["Du", "a\u00b7ber", ",", "kannst", "du", "auch", "noch", "fr\u00f6h\u00b7lich", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und wieder ausgef\u00fcllt von neuem Gl\u00fccke:", "tokens": ["Und", "wie\u00b7der", "aus\u00b7ge\u00b7f\u00fcllt", "von", "neu\u00b7em", "Gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Und Moderduft bleibt stets darin zur\u00fccke.", "tokens": ["Und", "Mo\u00b7der\u00b7duft", "bleibt", "stets", "da\u00b7rin", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}