{"dta.poem.2560": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschende Ode  \n  Bey Hoch Adel. Verm\u00e4hlung Hn. N. v. S.  \n u. Fr. M. N. g. v. F. im Namen A. S. den  \n 8. Febr. 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich opffern euren Flammen/", "tokens": ["Was", "soll", "ich", "opf\u00b7fern", "eu\u00b7ren", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch-Edle Zwey/ in Kindes Pflicht/", "tokens": ["Hoch\u00b7Ed\u00b7le", "Zwey", "/", "in", "Kin\u00b7des", "Pflicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tugend f\u00fchret euch zusammen/", "tokens": ["Die", "Tu\u00b7gend", "f\u00fch\u00b7ret", "euch", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und duppelt eurer Liebe Licht/", "tokens": ["Und", "dup\u00b7pelt", "eu\u00b7rer", "Lie\u00b7be", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Treu verbindet", "tokens": ["Die", "Treu", "ver\u00b7bin\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den Edlen Muth/", "tokens": ["Den", "Ed\u00b7len", "Muth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und hat entz\u00fcndet", "tokens": ["Und", "hat", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "So Geist als Blut", "tokens": ["So", "Geist", "als", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In gleicher Glut.", "tokens": ["In", "glei\u00b7cher", "Glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "So wird der Ruhm der grauen Ahnen/", "tokens": ["So", "wird", "der", "Ruhm", "der", "grau\u00b7en", "Ah\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eurer H\u00e4user Ewigkeit/", "tokens": ["Und", "eu\u00b7rer", "H\u00e4u\u00b7ser", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Eltern Ehren-Schild und Fahnen", "tokens": ["Der", "El\u00b7tern", "Eh\u00b7ren\u00b7Schild", "und", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von allem Untergang befreyt/", "tokens": ["Von", "al\u00b7lem", "Un\u00b7ter\u00b7gang", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil durch die Liebe", "tokens": ["Weil", "durch", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Bau der Welt", "tokens": ["Der", "Bau", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In stetem Triebe", "tokens": ["In", "ste\u00b7tem", "Trie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Sich unterh\u00e4lt", "tokens": ["Sich", "un\u00b7ter\u00b7h\u00e4lt"], "token_info": ["word", "word"], "pos": ["PRF", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und nicht zerf\u00e4llt.", "tokens": ["Und", "nicht", "zer\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich wolte meine Pflicht bezeigen", "tokens": ["Ich", "wol\u00b7te", "mei\u00b7ne", "Pflicht", "be\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In angenehmster Zierligkeit:", "tokens": ["In", "an\u00b7ge\u00b7nehms\u00b7ter", "Zier\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es solte nicht mein Mund verschweigen", "tokens": ["Es", "sol\u00b7te", "nicht", "mein", "Mund", "ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die schuldigste Beflissenheit/", "t