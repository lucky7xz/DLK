{"dta.poem.7747": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O wie kurzsichtig ist die Weisheit der Geschichte,", "tokens": ["O", "wie", "kurz\u00b7sich\u00b7tig", "ist", "die", "Weis\u00b7heit", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der du glaubst da\u00df sie gerecht die Todten richte.", "tokens": ["Von", "der", "du", "glaubst", "da\u00df", "sie", "ge\u00b7recht", "die", "Tod\u00b7ten", "rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu wandeln lieb' ich nicht in diesem Pantheon,", "tokens": ["Zu", "wan\u00b7deln", "lieb'", "ich", "nicht", "in", "die\u00b7sem", "Pan\u00b7the\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo, wie hier au\u00dfen, nur gereiht ist Thron an Thron.", "tokens": ["Wo", ",", "wie", "hier", "au\u00b7\u00dfen", ",", "nur", "ge\u00b7reiht", "ist", "Thron", "an", "Thron", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ADV", "ADV", "$,", "ADV", "VVPP", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Alsob nichts Gro\u00dfes sei, das nicht auf Thronen s\u00e4\u00dfe,", "tokens": ["Al\u00b7sob", "nichts", "Gro\u00b7\u00dfes", "sei", ",", "das", "nicht", "auf", "Thro\u00b7nen", "s\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich innrer Menschenwerth an \u00e4u\u00dferm Glanz nur m\u00e4\u00dfe.", "tokens": ["Sich", "inn\u00b7rer", "Men\u00b7schen\u00b7werth", "an", "\u00e4u\u00b7\u00dferm", "Glanz", "nur", "m\u00e4\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geh doch die Reihe durch der Einzigen, der Gro\u00dfen!", "tokens": ["Geh", "doch", "die", "Rei\u00b7he", "durch", "der", "Ein\u00b7zi\u00b7gen", ",", "der", "Gro\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wieviel sind die man nicht vom Throne sollte sto\u00dfen?", "tokens": ["Wie\u00b7viel", "sind", "die", "man", "nicht", "vom", "Thro\u00b7ne", "soll\u00b7te", "sto\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "PIS", "PTKNEG", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df Gro\u00dfes sie gethan mit gro\u00dfer Macht und Kraft,", "tokens": ["Da\u00df", "Gro\u00b7\u00dfes", "sie", "ge\u00b7than", "mit", "gro\u00b7\u00dfer", "Macht", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht das auf ewig sie f\u00fcr Menschen musterhaft?", "tokens": ["Macht", "das", "auf", "e\u00b7wig", "sie", "f\u00fcr", "Men\u00b7schen", "mus\u00b7ter\u00b7haft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ADJD", "PPER", "APPR", "NN", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Wo ist, wenn du auch das willst ziehen in Betrachtung,", "tokens": ["Wo", "ist", ",", "wenn", "du", "auch", "das", "willst", "zie\u00b7hen", "in", "Be\u00b7trach\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "KOUS", "PPER", "ADV", "PDS", "VMFIN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein F\u00fcnkchen Menschenlieb', ein K\u00f6rnchen Menschenachtung?", "tokens": ["Ein", "F\u00fcnk\u00b7chen", "Men\u00b7schen\u00b7lieb'", ",", "ein", "K\u00f6rn\u00b7chen", "Men\u00b7schen\u00b7ach\u00b7tung", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}