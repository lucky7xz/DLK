{"textgrid.poem.55304": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Freibeuter", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Haus hat kein T\u00fcr,", "tokens": ["Mein", "Haus", "hat", "kein", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mein T\u00fcr hat ke Haus:", "tokens": ["Mein", "T\u00fcr", "hat", "ke", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und immer mit Sch\u00e4tzel", "tokens": ["Und", "im\u00b7mer", "mit", "Sch\u00e4t\u00b7zel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hinein und heraus.", "tokens": ["Hin\u00b7ein", "und", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Mei K\u00fcch hat ke Herd,", "tokens": ["Mei", "K\u00fcch", "hat", "ke", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mei Herd hat ke K\u00fcch;", "tokens": ["Mei", "Herd", "hat", "ke", "K\u00fcch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da bratet's und siedet's", "tokens": ["Da", "bra\u00b7tet's", "und", "sie\u00b7det's"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr sich und f\u00fcr mich.", "tokens": ["F\u00fcr", "sich", "und", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Mei Bett hat ke Gstell,", "tokens": ["Mei", "Bett", "hat", "ke", "Gs\u00b7tell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mei Gstell hat ke Bett.", "tokens": ["Mei", "Gs\u00b7tell", "hat", "ke", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch w\u00fc\u00dft ich nit enen,", "tokens": ["Doch", "w\u00fc\u00dft", "ich", "nit", "e\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der's lustiger hett.", "tokens": ["Der's", "lus\u00b7ti\u00b7ger", "hett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Mei Keller is hoch,", "tokens": ["Mei", "Kel\u00b7ler", "is", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mei Scheuer is tief,", "tokens": ["Mei", "Scheu\u00b7er", "is", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "FM", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zuoberst zuunterst \u2013", "tokens": ["Zu\u00b7o\u00b7berst", "zu\u00b7un\u00b7terst", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da lag ich und schlief.", "tokens": ["Da", "lag", "ich", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und bin ich erwachen,", "tokens": ["Und", "bin", "ich", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da geht es so fort;", "tokens": ["Da", "geht", "es", "so", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mei Ort hat ke Bleibens,", "tokens": ["Mei", "Ort", "hat", "ke", "Blei\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein Bleibens ken Ort.", "tokens": ["Mein", "Blei\u00b7bens", "ken", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}