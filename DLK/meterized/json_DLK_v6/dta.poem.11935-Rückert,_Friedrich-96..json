{"dta.poem.11935": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der gr\u00f6ste Kummer ist im kummervollen Leben,", "tokens": ["Der", "gr\u00f6s\u00b7te", "Kum\u00b7mer", "ist", "im", "kum\u00b7mer\u00b7vol\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man das Gl\u00fcck erreicht nur das man aufgegeben.", "tokens": ["Da\u00df", "man", "das", "Gl\u00fcck", "er\u00b7reicht", "nur", "das", "man", "auf\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "ADV", "ART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo die Begierd' erlischt, ist auch der Arme reich,", "tokens": ["Wo", "die", "Be\u00b7gierd'", "er\u00b7lischt", ",", "ist", "auch", "der", "Ar\u00b7me", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo sie herrscht, da ist der F\u00fcrst dem Sklaven gleich.", "tokens": ["Und", "wo", "sie", "herrscht", ",", "da", "ist", "der", "F\u00fcrst", "dem", "Skla\u00b7ven", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wieviel du w\u00fcnschen magst, der Wunsch wird weiter gehn,", "tokens": ["Wie\u00b7viel", "du", "w\u00fcn\u00b7schen", "magst", ",", "der", "Wunsch", "wird", "wei\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gl\u00fcck ist da nur wo die W\u00fcnsche stille stehn.", "tokens": ["Und", "Gl\u00fcck", "ist", "da", "nur", "wo", "die", "W\u00fcn\u00b7sche", "stil\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADV", "PWAV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}