{"dta.poem.8558": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Purpurne Citronenw\u00e4lder", "tokens": ["Pur\u00b7pur\u00b7ne", "Cit\u00b7ro\u00b7nen\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "bl\u00fchen um blaue Meere.", "tokens": ["bl\u00fc\u00b7hen", "um", "blau\u00b7e", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Mit seidnen Segeln", "tokens": ["Mit", "seid\u00b7nen", "Se\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "saust", "tokens": ["saust"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "mein Drachenschiff.", "tokens": ["mein", "Dra\u00b7chen\u00b7schiff", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Fest,", "tokens": ["Fest", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "in den gr\u00fcnen Gischt,", "tokens": ["in", "den", "gr\u00fc\u00b7nen", "Gischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "dr\u00fcckt meine Faust das Steuer;", "tokens": ["dr\u00fcckt", "mei\u00b7ne", "Faust", "das", "Steu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "keine Wimper zuckt.", "tokens": ["kei\u00b7ne", "Wim\u00b7per", "zuckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Zu dir! Zu dir!", "tokens": ["Zu", "dir", "!", "Zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Unter meinem spiegelnden Goldpanzer,", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "spie\u00b7geln\u00b7den", "Gold\u00b7pan\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "aus dem die Sonne strahlt,", "tokens": ["aus", "dem", "die", "Son\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "klopft", "tokens": ["klopft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "mein Herz.", "tokens": ["mein", "Herz", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}