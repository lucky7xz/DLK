{"textgrid.poem.55671": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[was in der Schenke waren heute]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was in der Schenke waren heute", "tokens": ["Was", "in", "der", "Schen\u00b7ke", "wa\u00b7ren", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am fr\u00fchsten Morgen f\u00fcr Tumulte!", "tokens": ["Am", "fr\u00fchs\u00b7ten", "Mor\u00b7gen", "f\u00fcr", "Tu\u00b7mul\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wirt und M\u00e4dchen! Fackeln, Leute!", "tokens": ["Der", "Wirt", "und", "M\u00e4d\u00b7chen", "!", "Fa\u00b7ckeln", ",", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was gab's f\u00fcr H\u00e4ndel, f\u00fcr Insulte!", "tokens": ["Was", "gab's", "f\u00fcr", "H\u00e4n\u00b7del", ",", "f\u00fcr", "In\u00b7sul\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Fl\u00f6te klang, die Trommel scholl!", "tokens": ["Die", "Fl\u00f6\u00b7te", "klang", ",", "die", "Trom\u00b7mel", "scholl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war ein w\u00fcstes Wesen \u2013", "tokens": ["Es", "war", "ein", "w\u00fcs\u00b7tes", "We\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch bin ich, Lust und Liebe voll,", "tokens": ["Doch", "bin", "ich", ",", "Lust", "und", "Lie\u00b7be", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch selbst dabeigewesen.", "tokens": ["Auch", "selbst", "da\u00b7bei\u00b7ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich von Sitte nichts gelernt,", "tokens": ["Da\u00df", "ich", "von", "Sit\u00b7te", "nichts", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dar\u00fcber tadelt mich ein jeder;", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ta\u00b7delt", "mich", "ein", "je\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleib ich weislich weit entfernt", "tokens": ["Doch", "bleib", "ich", "weis\u00b7lich", "weit", "ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Streit der Schulen und Katheder.", "tokens": ["Vom", "Streit", "der", "Schu\u00b7len", "und", "Ka\u00b7the\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}