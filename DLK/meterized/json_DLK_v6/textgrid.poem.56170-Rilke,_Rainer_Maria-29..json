{"textgrid.poem.56170": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stiller Freund der vielen Fernen, f\u00fchle,", "tokens": ["Stil\u00b7ler", "Freund", "der", "vie\u00b7len", "Fer\u00b7nen", ",", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "ART", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie dein Atem noch den Raum vermehrt.", "tokens": ["wie", "dein", "A\u00b7tem", "noch", "den", "Raum", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Im Geb\u00e4lk der finstern Glockenst\u00fchle", "tokens": ["Im", "Ge\u00b7b\u00e4lk", "der", "fins\u00b7tern", "Glo\u00b7cken\u00b7st\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "la\u00df dich l\u00e4uten. Das, was an dir zehrt,", "tokens": ["la\u00df", "dich", "l\u00e4u\u00b7ten", ".", "Das", ",", "was", "an", "dir", "zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$.", "PDS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "wird ein Starkes \u00fcber dieser Nahrung.", "tokens": ["wird", "ein", "Star\u00b7kes", "\u00fc\u00b7ber", "die\u00b7ser", "Nah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geh in der Verwandlung aus und ein.", "tokens": ["Geh", "in", "der", "Ver\u00b7wand\u00b7lung", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was ist deine leidendste Erfahrung?", "tokens": ["Was", "ist", "dei\u00b7ne", "lei\u00b7dends\u00b7te", "Er\u00b7fah\u00b7rung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Ist dir Trinken bitter, werde Wein.", "tokens": ["Ist", "dir", "Trin\u00b7ken", "bit\u00b7ter", ",", "wer\u00b7de", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADJD", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sei in dieser Nacht aus \u00dcberma\u00df", "tokens": ["Sei", "in", "die\u00b7ser", "Nacht", "aus", "\u00dc\u00b7berm\u00b7a\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zauberkraft am Kreuzweg deiner Sinne,", "tokens": ["Zau\u00b7ber\u00b7kraft", "am", "Kreuz\u00b7weg", "dei\u00b7ner", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ihrer seltsamen Begegnung Sinn.", "tokens": ["ih\u00b7rer", "selt\u00b7sa\u00b7men", "Be\u00b7geg\u00b7nung", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und wenn dich das Irdische verga\u00df,", "tokens": ["Und", "wenn", "dich", "das", "Ir\u00b7di\u00b7sche", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "zu der stillen Erde sag: Ich rinne.", "tokens": ["zu", "der", "stil\u00b7len", "Er\u00b7de", "sag", ":", "Ich", "rin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zu dem raschen Wasser sprich: Ich bin.", "tokens": ["Zu", "dem", "ra\u00b7schen", "Was\u00b7ser", "sprich", ":", "Ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$.", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}