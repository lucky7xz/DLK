{"textgrid.poem.43308": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir waren in Knidos, meine holdselige Chloe und ich.", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir waren in Knidos, meine holdselige Chloe und ich.", "tokens": ["Wir", "wa\u00b7ren", "in", "Kni\u00b7dos", ",", "mei\u00b7ne", "hold\u00b7se\u00b7li\u00b7ge", "Chloe", "und", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$,", "PPOSAT", "ADJA", "NN", "KON", "PPER", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf ewig gr\u00fcnem Laube spielen", "tokens": ["Auf", "e\u00b7wig", "gr\u00fc\u00b7nem", "Lau\u00b7be", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier Scherz, und Lenz, und Z\u00e4rtlichkeit.", "tokens": ["Hier", "Scherz", ",", "und", "Lenz", ",", "und", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blumen k\u00fcssen, B\u00e4ume f\u00fchlen,", "tokens": ["Die", "Blu\u00b7men", "k\u00fcs\u00b7sen", ",", "B\u00e4u\u00b7me", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Grotten, welche Zephyrs k\u00fchlen,", "tokens": ["Und", "Grot\u00b7ten", ",", "wel\u00b7che", "Ze\u00b7phyrs", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verbergen manchen holden Streit,", "tokens": ["Ver\u00b7ber\u00b7gen", "man\u00b7chen", "hol\u00b7den", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn eine Dryas hier im Thale", "tokens": ["Wenn", "ei\u00b7ne", "Dryas", "hier", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dem jungen Faun zum erstenmale", "tokens": ["Dem", "jun\u00b7gen", "Faun", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit lautem Zwange K\u00fcsse weiht.", "tokens": ["Mit", "lau\u00b7tem", "Zwan\u00b7ge", "K\u00fcs\u00b7se", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit aufgel\u00f6stem G\u00fcrtel gingen", "tokens": ["Mit", "auf\u00b7ge\u00b7l\u00f6s\u00b7tem", "G\u00fcr\u00b7tel", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Grazien leicht vor ihr hin,", "tokens": ["Die", "Gra\u00b7zi\u00b7en", "leicht", "vor", "ihr", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Amor fliegt mit regen Schwingen", "tokens": ["Ein", "A\u00b7mor", "fliegt", "mit", "re\u00b7gen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schnell auf die Brust, schnell auf das Kinn,", "tokens": ["Schnell", "auf", "die", "Brust", ",", "schnell", "auf", "das", "Kinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sucht dort ein Kn\u00f6spchen anzubringen,", "tokens": ["Sucht", "dort", "ein", "Kn\u00f6spc\u00b7hen", "an\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und t\u00e4ndelt hier ein Gr\u00fcbchen hin:", "tokens": ["Und", "t\u00e4n\u00b7delt", "hier", "ein", "Gr\u00fcb\u00b7chen", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit himmlisch sanften Liebesschl\u00e4gen", "tokens": ["Mit", "himm\u00b7lisch", "sanf\u00b7ten", "Lie\u00b7bes\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Lohnt ihm die G\u00f6ttinn seine M\u00fch:", "tokens": ["Lohnt", "ihm", "die", "G\u00f6t\u00b7tinn", "sei\u00b7ne", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Froh flattert er der Straf entgegen,", "tokens": ["Froh", "flat\u00b7tert", "er", "der", "Straf", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und zur Vergeltung k\u00fc\u00dft er sie.", "tokens": ["Und", "zur", "Ver\u00b7gel\u00b7tung", "k\u00fc\u00dft", "er", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er wars, der im Ger\u00e4usch der Bl\u00e4tter", "tokens": ["Er", "wars", ",", "der", "im", "Ge\u00b7r\u00e4usch", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Untreue K\u00fcsse rauschen h\u00f6rt,", "tokens": ["Un\u00b7treu\u00b7e", "K\u00fcs\u00b7se", "rau\u00b7schen", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sicherheit in Furcht verkehrt,", "tokens": ["Der", "Si\u00b7cher\u00b7heit", "in", "Furcht", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sanftesten mit Wuth bewehrt,", "tokens": ["Die", "sanf\u00b7tes\u00b7ten", "mit", "Wuth", "be\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Edelsten Neid oder Argwohn lehrt,", "tokens": ["Die", "E\u00b7dels\u00b7ten", "Neid", "o\u00b7der", "Arg\u00b7wohn", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ach! jedes Gl\u00fcck der Liebe st\u00f6hrt:", "tokens": ["Ach", "!", "je\u00b7des", "Gl\u00fcck", "der", "Lie\u00b7be", "st\u00f6hrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! der gef\u00e4hrlichste der G\u00f6tter!", "tokens": ["Ach", "!", "der", "ge\u00b7f\u00e4hr\u00b7lichs\u00b7te", "der", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Mich nagt bey Chloens besten K\u00fcssen", "tokens": ["Mich", "nagt", "bey", "Chloens", "bes\u00b7ten", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein banger schrecklicher Verdacht:", "tokens": ["Ein", "ban\u00b7ger", "schreck\u00b7li\u00b7cher", "Ver\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwie, wenn bey diesen Nektark\u00fcssen", "tokens": ["\u00bb", "wie", ",", "wenn", "bey", "die\u00b7sen", "Nek\u00b7tar\u00b7k\u00fcs\u00b7sen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dritter oft mich still verlacht!", "tokens": ["Ein", "drit\u00b7ter", "oft", "mich", "still", "ver\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Chloe! sollt ich dieses wissen!\u00ab", "tokens": ["O", "Chloe", "!", "sollt", "ich", "die\u00b7ses", "wis\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$.", "VMFIN", "PPER", "PDS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann nenn ich Chloen den Verdacht,", "tokens": ["Dann", "nenn", "ich", "Chloen", "den", "Ver\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und Chloe weint; und ich mu\u00df reuig flehen:", "tokens": ["Und", "Chloe", "weint", ";", "und", "ich", "mu\u00df", "reu\u00b7ig", "fle\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "KON", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Denn weinen kann ich sie nicht sehen. \u2013", "tokens": ["Denn", "wei\u00b7nen", "kann", "ich", "sie", "nicht", "se\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ach! Venus, nimm den Gott zur\u00fcck!", "tokens": ["Ach", "!", "Ve\u00b7nus", ",", "nimm", "den", "Gott", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er bringt mich ewig um mein Gl\u00fcck.", "tokens": ["Er", "bringt", "mich", "e\u00b7wig", "um", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}