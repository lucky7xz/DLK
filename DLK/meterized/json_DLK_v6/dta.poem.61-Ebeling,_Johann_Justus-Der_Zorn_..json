{"dta.poem.61": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Zorn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein flatternd Feuer im Gem\u00fcte,", "tokens": ["Ein", "flat\u00b7ternd", "Feu\u00b7er", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00e4umend Wallen im Gebl\u00fcte,", "tokens": ["Ein", "sch\u00e4u\u00b7mend", "Wal\u00b7len", "im", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In unsrer Adern Wunderborn,", "tokens": ["In", "uns\u00b7rer", "A\u00b7dern", "Wun\u00b7der\u00b7born", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erregt der f\u00fcrchterliche Zorn:", "tokens": ["Er\u00b7regt", "der", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Zorn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Zorn entflammt die Eigen-", "tokens": ["Den", "Zorn", "ent\u00b7flammt", "die", "Ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Rachsucht st\u00e4rket seine Triebe,", "tokens": ["Die", "Rach\u00b7sucht", "st\u00e4r\u00b7ket", "sei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die z\u00e4rtliche Empfindlichkeit", "tokens": ["Die", "z\u00e4rt\u00b7li\u00b7che", "Emp\u00b7find\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Begleitet ihn zu jeder Zeit,", "tokens": ["Be\u00b7glei\u00b7tet", "ihn", "zu", "je\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Hofnung sucht durch spize Dornen,", "tokens": ["Die", "Hof\u00b7nung", "sucht", "durch", "spi\u00b7ze", "Dor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Jhn auch noch ferner anzuspornen.", "tokens": ["Jhn", "auch", "noch", "fer\u00b7ner", "an\u00b7zu\u00b7spor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist der Zorn, das Kind der H\u00f6llen,", "tokens": ["Das", "ist", "der", "Zorn", ",", "das", "Kind", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der das Gebl\u00fct pflegt aufzuschwellen,", "tokens": ["Der", "das", "Ge\u00b7bl\u00fct", "pflegt", "auf\u00b7zu\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und das Gem\u00fct in Wallung sezt,", "tokens": ["Und", "das", "Ge\u00b7m\u00fct", "in", "Wal\u00b7lung", "sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bald es sich nur d\u00fcnkt verlezt.", "tokens": ["So", "bald", "es", "sich", "nur", "d\u00fcnkt", "ver\u00b7lezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Wort sezt ihn in Dampf und Flammen,", "tokens": ["Ein", "Wort", "sezt", "ihn", "in", "Dampf", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da Stolz und Rachbegier zusammen,", "tokens": ["Da", "Stolz", "und", "Rach\u00b7be\u00b7gier", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Pech und Schweffel Feuer spein,", "tokens": ["Wie", "Pech", "und", "Schwef\u00b7fel", "Feu\u00b7er", "spein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Licht und Dampf und Stank ausstreun:", "tokens": ["Und", "Licht", "und", "Dampf", "und", "Stank", "aus\u00b7streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Zorn der pflegt bei seinen Wittern,", "tokens": ["Der", "Zorn", "der", "pflegt", "bei", "sei\u00b7nen", "Wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht andre, sondern sich zu splittern.", "tokens": ["Nicht", "and\u00b7re", ",", "son\u00b7dern", "sich", "zu", "split\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "$,", "KON", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O! st\u00fcrmisch rauhe Leidenschaften", "tokens": ["O", "!", "st\u00fcr\u00b7misch", "rau\u00b7he", "Lei\u00b7den\u00b7schaf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in der Menschen Seele haften,", "tokens": ["Die", "in", "der", "Men\u00b7schen", "See\u00b7le", "haf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Feinde der Zufriedenheit,", "tokens": ["Ihr", "Fein\u00b7de", "der", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unholden aller Menschlichkeit!", "tokens": ["Un\u00b7hol\u00b7den", "al\u00b7ler", "Menschlich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Entfernet euch aus denen Seelen,", "tokens": ["Ent\u00b7fer\u00b7net", "euch", "aus", "de\u00b7nen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRELS", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Jhr kommt das Herze nur zu qu\u00e4len,", "tokens": ["Ihr", "kommt", "das", "Her\u00b7ze", "nur", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ihr unsern Trieb vergn\u00fcgt,", "tokens": ["Und", "wenn", "ihr", "un\u00b7sern", "Trieb", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So habet ihr uns ganz besiegt,", "tokens": ["So", "ha\u00b7bet", "ihr", "uns", "ganz", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und macht uns gleich den wilden Thieren,", "tokens": ["Und", "macht", "uns", "gleich", "den", "wil\u00b7den", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da wir die Menschlichkeit verliehren.", "tokens": ["Da", "wir", "die", "Menschlich\u00b7keit", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Zorniger bei seinem Brennen,", "tokens": ["Ein", "Zor\u00b7ni\u00b7ger", "bei", "sei\u00b7nem", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht ein Mensche mehr zu nennen,", "tokens": ["Ist", "nicht", "ein", "Men\u00b7sche", "mehr", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein L\u00f6w, ein Tyger, Wolf und B\u00e4r,", "tokens": ["Ein", "L\u00f6w", ",", "ein", "Ty\u00b7ger", ",", "Wolf", "und", "B\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Basilisk, und was noch mehr", "tokens": ["Ein", "Ba\u00b7si\u00b7lisk", ",", "und", "was", "noch", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PWS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor f\u00fcrchterliche Thiere leben,", "tokens": ["Vor", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Thie\u00b7re", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die uns derselben Bildnis geben:", "tokens": ["Die", "uns", "der\u00b7sel\u00b7ben", "Bild\u00b7nis", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wo die Wuth das Herz erregt,", "tokens": ["Denn", "wo", "die", "Wuth", "das", "Herz", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Blut gleich Meereswellen schl\u00e4gt,", "tokens": ["Das", "Blut", "gleich", "Mee\u00b7res\u00b7wel\u00b7len", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kan die Vernunft ja nicht regieren,", "tokens": ["Kan", "die", "Ver\u00b7nunft", "ja", "nicht", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die bei uns mu\u00df das Ruder f\u00fchren?", "tokens": ["Die", "bei", "uns", "mu\u00df", "das", "Ru\u00b7der", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O! eilet alle aus dem Wege,", "tokens": ["O", "!", "ei\u00b7let", "al\u00b7le", "aus", "dem", "We\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier wird der Zorn im Adern rege,", "tokens": ["Hier", "wird", "der", "Zorn", "im", "A\u00b7dern", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er bricht schon los, und das Gesicht,", "tokens": ["Er", "bricht", "schon", "los", ",", "und", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umw\u00f6lkt das tieffe Augenlicht:", "tokens": ["Um\u00b7w\u00f6lkt", "das", "tief\u00b7fe", "Au\u00b7gen\u00b7licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Blut err\u00f6thet schon die Wangen,", "tokens": ["Das", "Blut", "er\u00b7r\u00f6t\u00b7het", "schon", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Auge wil gleich Feuer fangen,", "tokens": ["Das", "Au\u00b7ge", "wil", "gleich", "Feu\u00b7er", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Lebenssaft der schl\u00e4gt zur\u00fck;", "tokens": ["Der", "Le\u00b7bens\u00b7saft", "der", "schl\u00e4gt", "zu\u00b7r\u00fck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein blasser Basilisken Blik", "tokens": ["Ein", "blas\u00b7ser", "Ba\u00b7si\u00b7lis\u00b7ken", "Blik"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Glimmt durch, und ist nicht mehr zu z\u00e4umen,", "tokens": ["Glimmt", "durch", ",", "und", "ist", "nicht", "mehr", "zu", "z\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "O! sehet den Bese\u00dfnen sch\u00e4umen.", "tokens": ["O", "!", "se\u00b7het", "den", "Be\u00b7se\u00df\u00b7nen", "sch\u00e4u\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie raset nicht der blinde Eiffer,", "tokens": ["Wie", "ra\u00b7set", "nicht", "der", "blin\u00b7de", "Eif\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O! was vor Gift, o! was vor Geiffer", "tokens": ["O", "!", "was", "vor", "Gift", ",", "o", "!", "was", "vor", "Geif\u00b7fer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "APPR", "NN", "$,", "FM", "$.", "PWS", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sprizt wie ein J\u00e4scht aus seinen Mund", "tokens": ["Sprizt", "wie", "ein", "J\u00e4scht", "aus", "sei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht das b\u00f6se Herze kund.", "tokens": ["Und", "macht", "das", "b\u00f6\u00b7se", "Her\u00b7ze", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er flucht und schm\u00e4hlt, er tobt und bellet,", "tokens": ["Er", "flucht", "und", "schm\u00e4hlt", ",", "er", "tobt", "und", "bel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da er sich gleich dem Hunde stellet,", "tokens": ["Da", "er", "sich", "gleich", "dem", "Hun\u00b7de", "stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der grimmig in die Steine bei\u00dft,", "tokens": ["Der", "grim\u00b7mig", "in", "die", "Stei\u00b7ne", "bei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Damit man ihn verlezt und schmei\u00dft:", "tokens": ["Da\u00b7mit", "man", "ihn", "ver\u00b7lezt", "und", "schmei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er schilt, er l\u00e4stert, droht und heulet,", "tokens": ["Er", "schilt", ",", "er", "l\u00e4s\u00b7tert", ",", "droht", "und", "heu\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bis sich die Witterung zertheilet.", "tokens": ["Bis", "sich", "die", "Wit\u00b7te\u00b7rung", "zer\u00b7thei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gleich wie ein Meer wenn es erbittert,", "tokens": ["Gleich", "wie", "ein", "Meer", "wenn", "es", "er\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen Uffern schreklich wittert", "tokens": ["In", "sei\u00b7nen", "Uf\u00b7fern", "schrek\u00b7lich", "wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wellen welzt und tobend b\u00e4umt,", "tokens": ["Die", "Wel\u00b7len", "welzt", "und", "to\u00b7bend", "b\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die es in Zischen ausgesch\u00e4umt:", "tokens": ["Die", "es", "in", "Zi\u00b7schen", "aus\u00b7ge\u00b7sch\u00e4umt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wellen an die Klippen rennet,", "tokens": ["Die", "Wel\u00b7len", "an", "die", "Klip\u00b7pen", "ren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Daran der Fluthen Wuth zertrennet,", "tokens": ["Da\u00b7ran", "der", "Flut\u00b7hen", "Wuth", "zer\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wiederum von Wind erregt,", "tokens": ["Und", "wie\u00b7de\u00b7rum", "von", "Wind", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von neuen an dieselben schl\u00e4gt,", "tokens": ["Von", "neu\u00b7en", "an", "die\u00b7sel\u00b7ben", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bis es mit einem nassen Pochen,", "tokens": ["Bis", "es", "mit", "ei\u00b7nem", "nas\u00b7sen", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Riegel Damm entzwei gebrochen:", "tokens": ["Der", "Rie\u00b7gel", "Damm", "ent\u00b7zwei", "ge\u00b7bro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So w\u00fcten die mit Zorn und Grimme,", "tokens": ["So", "w\u00fc\u00b7ten", "die", "mit", "Zorn", "und", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald ihres Eifers Donner Stimme", "tokens": ["Bald", "ih\u00b7res", "Ei\u00b7fers", "Don\u00b7ner", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erheben, und b