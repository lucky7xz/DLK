{"dta.poem.9346": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Alles fl\u00fcchtig!  \n Alles nichtig!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Spiegel bricht/ dersch\u00f6nheit Zier vergeht/", "tokens": ["Der", "Spie\u00b7gel", "bricht", "/", "der\u00b7sch\u00f6n\u00b7heit", "Zier", "ver\u00b7geht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Leib nimbt ab/ die R\u00f6the selbst mu\u00df weichen/", "tokens": ["der", "Leib", "nimbt", "ab", "/", "die", "R\u00f6\u00b7the", "selbst", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das Alter naht/ die rothen Lippen bleichen/", "tokens": ["das", "Al\u00b7ter", "naht", "/", "die", "ro\u00b7then", "Lip\u00b7pen", "blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "was ist es dann/ das wohl an dier besteht?", "tokens": ["was", "ist", "es", "dann", "/", "das", "wohl", "an", "dier", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$(", "PDS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}