{"dta.poem.2931": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "130.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den h\u00f6chsten Menschensinn, das Augenlicht zu missen,", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Men\u00b7schen\u00b7sinn", ",", "das", "Au\u00b7gen\u00b7licht", "zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gefangen wohnend in best\u00e4nd'gen Finsternissen,", "tokens": ["Ge\u00b7fan\u00b7gen", "woh\u00b7nend", "in", "be\u00b7st\u00e4n\u00b7d'\u00b7gen", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ist doch, Erfahrung spricht, das h\u00f6chste Ungl\u00fcck nicht,", "tokens": ["Ist", "doch", ",", "Er\u00b7fah\u00b7rung", "spricht", ",", "das", "h\u00f6chs\u00b7te", "Un\u00b7gl\u00fcck", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil inneres ersetzt das \u00e4u\u00dferliche Licht.", "tokens": ["Weil", "in\u00b7ne\u00b7res", "er\u00b7setzt", "das", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7che", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der blindgewordene sieht in Erinnerungen,", "tokens": ["Der", "blind\u00b7ge\u00b7wor\u00b7de\u00b7ne", "sieht", "in", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der blindgeborene wird doch vom Licht durchdrungen;", "tokens": ["Der", "blind\u00b7ge\u00b7bo\u00b7re\u00b7ne", "wird", "doch", "vom", "Licht", "durch\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dolmetschen kannst du ihm den Stral, der ihn ber\u00fchrt,", "tokens": ["Dol\u00b7met\u00b7schen", "kannst", "du", "ihm", "den", "Stral", ",", "der", "ihn", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df der ein geistig Bild der Welt in ihm auff\u00fchrt.", "tokens": ["Da\u00df", "der", "ein", "geis\u00b7tig", "Bild", "der", "Welt", "in", "ihm", "auf\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJD", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Im Worte wird ihm kund die Weisheit aller Weisen,", "tokens": ["Im", "Wor\u00b7te", "wird", "ihm", "kund", "die", "Weis\u00b7heit", "al\u00b7ler", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKVZ", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kann mit Dichtermund die Wunder Gottes preisen.", "tokens": ["Er", "kann", "mit", "Dich\u00b7ter\u00b7mund", "die", "Wun\u00b7der", "Got\u00b7tes", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch diesen andern Sinn zu missen, den im Ohr,", "tokens": ["Doch", "die\u00b7sen", "an\u00b7dern", "Sinn", "zu", "mis\u00b7sen", ",", "den", "im", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entbehrend ewigen Weltharmonieenchor;", "tokens": ["Ent\u00b7beh\u00b7rend", "e\u00b7wi\u00b7gen", "Welt\u00b7har\u00b7mo\u00b7nie\u00b7en\u00b7chor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Verlust, der schwerer schien, ersetzen kann auch ihn", "tokens": ["Ver\u00b7lust", ",", "der", "schwe\u00b7rer", "schien", ",", "er\u00b7set\u00b7zen", "kann", "auch", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "VVFIN", "$,", "VVINF", "VMFIN", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Theilnahme doch der anschaubaren Harmonien.", "tokens": ["Theil\u00b7nah\u00b7me", "doch", "der", "an\u00b7schau\u00b7ba\u00b7ren", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Des Menschen Auge spricht dir und des Fr\u00fchlings Trift,", "tokens": ["Des", "Men\u00b7schen", "Au\u00b7ge", "spricht", "dir", "und", "des", "Fr\u00fch\u00b7lings", "Trift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sprache spricht dir selbst in ihrem Bild, der Schrift.", "tokens": ["Die", "Spra\u00b7che", "spricht", "dir", "selbst", "in", "ih\u00b7rem", "Bild", ",", "der", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dem taubgebornen auch, und darum stumm geboren,", "tokens": ["Dem", "taub\u00b7ge\u00b7bor\u00b7nen", "auch", ",", "und", "da\u00b7rum", "stumm", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "KON", "PAV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist alle F\u00e4higkeit der Bildung nicht verloren.", "tokens": ["Ist", "al\u00b7le", "F\u00e4\u00b7hig\u00b7keit", "der", "Bil\u00b7dung", "nicht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Zum handeln kannst du ihn, zum denken auch erziehn;", "tokens": ["Zum", "han\u00b7deln", "kannst", "du", "ihn", ",", "zum", "den\u00b7ken", "auch", "er\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VMFIN", "PPER", "PPER", "$,", "APPRART", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewi\u00df zum Dichter nur erziehst du niemals ihn.", "tokens": ["Ge\u00b7wi\u00df", "zum", "Dich\u00b7ter", "nur", "er\u00b7ziehst", "du", "nie\u00b7mals", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wer aber blind und taub zugleich ist uranf\u00e4nglich,", "tokens": ["Wer", "a\u00b7ber", "blind", "und", "taub", "zu\u00b7gleich", "ist", "u\u00b7ran\u00b7f\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der h\u00f6hern Menschheit scheint er Menschen unempf\u00e4nglich.", "tokens": ["Der", "h\u00f6\u00b7hern", "Menschheit", "scheint", "er", "Men\u00b7schen", "un\u00b7emp\u00b7f\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Gott, der ihn so gemacht, empf\u00e4nglich wird er machen", "tokens": ["Gott", ",", "der", "ihn", "so", "ge\u00b7macht", ",", "emp\u00b7f\u00e4ng\u00b7lich", "wird", "er", "ma\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn aus der Doppelnacht hier oder dort erwachen.", "tokens": ["Ihn", "aus", "der", "Dop\u00b7pel\u00b7nacht", "hier", "o\u00b7der", "dort", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Wer blind und taub nur ward, kann fort das Feuer sch\u00fcren", "tokens": ["Wer", "blind", "und", "taub", "nur", "ward", ",", "kann", "fort", "das", "Feu\u00b7er", "sch\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "ADV", "VAFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Innern, mag man auch nach au\u00dfen es nicht sp\u00fcren,", "tokens": ["Im", "In\u00b7nern", ",", "mag", "man", "auch", "nach", "au\u00b7\u00dfen", "es", "nicht", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VMFIN", "PIS", "ADV", "APPR", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Der Muschel gleich im Schlamm, Licht saugen mit Begier,", "tokens": ["Der", "Mu\u00b7schel", "gleich", "im", "Schlamm", ",", "Licht", "sau\u00b7gen", "mit", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zu viel sch\u00f6nrer Perl' in ihm wird als in ihr.", "tokens": ["Das", "zu", "viel", "sch\u00f6n\u00b7rer", "Perl'", "in", "ihm", "wird", "als", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "ADJA", "NN", "APPR", "PPER", "VAFIN", "KOKOM", "APPR", "PPOSAT", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "So sah ich einen Greis, an Aug' und Ohr verwittert,", "tokens": ["So", "sah", "ich", "ei\u00b7nen", "Greis", ",", "an", "Aug'", "und", "Ohr", "ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Lustentz\u00fcckungen im Fr\u00fchlingshain durchzittert.", "tokens": ["Von", "Lus\u00b7tent\u00b7z\u00fc\u00b7ckun\u00b7gen", "im", "Fr\u00fch\u00b7lings\u00b7hain", "durch\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.16": {"line.1": {"text": "Der Bl\u00fcten Duftgeruch, der Abendl\u00fcfte Wehn,", "tokens": ["Der", "Bl\u00fc\u00b7ten", "Duft\u00b7ge\u00b7ruch", ",", "der", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "Wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht ihm den Mund voll Preis, das Aug' in Thr\u00e4nen stehn.", "tokens": ["Macht", "ihm", "den", "Mund", "voll", "Preis", ",", "das", "Aug'", "in", "Thr\u00e4\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.17": {"line.1": {"text": "Er sog, was er nicht sah, und roch, was er nicht h\u00f6rte,", "tokens": ["Er", "sog", ",", "was", "er", "nicht", "sah", ",", "und", "roch", ",", "was", "er", "nicht", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "ADJD", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchlte Vollgenu\u00df und Andacht ungest\u00f6rte.", "tokens": ["Und", "f\u00fchl\u00b7te", "Voll\u00b7ge\u00b7nu\u00df", "und", "An\u00b7dacht", "un\u00b7ge\u00b7st\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "So sch\u00f6n ist Gottes Welt, da\u00df auch ein leises Fl\u00fcstern", "tokens": ["So", "sch\u00f6n", "ist", "Got\u00b7tes", "Welt", ",", "da\u00df", "auch", "ein", "lei\u00b7ses", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihr der Blindheit kann und Taubheit Nacht entd\u00fcstern.", "tokens": ["Von", "ihr", "der", "Blind\u00b7heit", "kann", "und", "Taub\u00b7heit", "Nacht", "ent\u00b7d\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "VMFIN", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}