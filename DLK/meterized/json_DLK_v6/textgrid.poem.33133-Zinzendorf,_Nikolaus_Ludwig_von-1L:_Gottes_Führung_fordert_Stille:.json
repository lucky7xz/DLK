{"textgrid.poem.33133": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gottes F\u00fchrung fordert Stille:", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottes F\u00fchrung fordert Stille:", "tokens": ["Got\u00b7tes", "F\u00fch\u00b7rung", "for\u00b7dert", "Stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Fu\u00df noch selber rauscht,", "tokens": ["Wo", "der", "Fu\u00df", "noch", "sel\u00b7ber", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird des ewgen Vaters Wille,", "tokens": ["Wird", "des", "ew\u00b7gen", "Va\u00b7ters", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der eignen Wahl vertauscht.", "tokens": ["Mit", "der", "eig\u00b7nen", "Wahl", "ver\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer da leben will, der sterbe:", "tokens": ["Wer", "da", "le\u00b7ben", "will", ",", "der", "ster\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer nicht stirbt, der lebet nicht:", "tokens": ["Wer", "nicht", "stirbt", ",", "der", "le\u00b7bet", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehe dann das Fleisch verderbe,", "tokens": ["E\u00b7he", "dann", "das", "Fleisch", "ver\u00b7der\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheinet uns kein wahres Licht.", "tokens": ["Schei\u00b7net", "uns", "kein", "wah\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was die andren Menschen wollen,", "tokens": ["Was", "die", "an\u00b7dren", "Men\u00b7schen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft der Sch\u00f6pfer noch geschehn:", "tokens": ["L\u00e4\u00dft", "der", "Sch\u00f6p\u00b7fer", "noch", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wenn die Kinder schmollen,", "tokens": ["A\u00b7ber", ",", "wenn", "die", "Kin\u00b7der", "schmol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft Er sie die Ruthe sehn.", "tokens": ["L\u00e4\u00dft", "Er", "sie", "die", "Ru\u00b7the", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle menschlichen Gesch\u00e4fte", "tokens": ["Al\u00b7le", "menschli\u00b7chen", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gehen \u00fcberhaupt nicht gut,", "tokens": ["Ge\u00b7hen", "\u00fc\u00b7ber\u00b7haupt", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo man sie durch eigne Kr\u00e4fte", "tokens": ["Wo", "man", "sie", "durch", "eig\u00b7ne", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht aus der Gnade thut.", "tokens": ["Und", "nicht", "aus", "der", "Gna\u00b7de", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "G\u00f6ttliche und innre Dinge", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "und", "inn\u00b7re", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lassen vollends gar nicht zu,", "tokens": ["Las\u00b7sen", "vol\u00b7lends", "gar", "nicht", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man sie mit Sturm erzwinge,", "tokens": ["Da\u00df", "man", "sie", "mit", "Sturm", "er\u00b7zwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern weisen uns zur Ruh.", "tokens": ["Son\u00b7dern", "wei\u00b7sen", "uns", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zeitlich, ewig, geist- und leiblich,", "tokens": ["Zeit\u00b7lich", ",", "e\u00b7wig", ",", "geist", "und", "leib\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "TRUNC", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beut sich oftermals die Hand:", "tokens": ["Beut", "sich", "of\u00b7ter\u00b7mals", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wie so unbeschreiblich", "tokens": ["A\u00b7ber", ",", "wie", "so", "un\u00b7be\u00b7schreib\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlie\u00dft sichs an das Ehe-Band.", "tokens": ["Schlie\u00dft", "sichs", "an", "das", "E\u00b7he\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Darum ist es unumg\u00e4nglich,", "tokens": ["Da\u00b7rum", "ist", "es", "un\u00b7um\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesus f\u00fchr uns erst hinein;", "tokens": ["Je\u00b7sus", "f\u00fchr", "uns", "erst", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}