{"dta.poem.18484": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines bequemen mannes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mehr kinderchen hatt ich als geldes in dem kasten:", "tokens": ["Mehr", "kin\u00b7der\u00b7chen", "hatt", "ich", "als", "gel\u00b7des", "in", "dem", "kas\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "KOUS", "ADJA", "APPR", "ART", "ADJA", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu allem schickte sich mein weib/ als nicht zum fasten/", "tokens": ["Zu", "al\u00b7lem", "schick\u00b7te", "sich", "mein", "weib", "/", "als", "nicht", "zum", "fas\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PRF", "PPOSAT", "NN", "$(", "KOUS", "PTKNEG", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fand selbe nicht bey mir/ was ihr den hunger nahm/", "tokens": ["Fand", "sel\u00b7be", "nicht", "bey", "mir", "/", "was", "ihr", "den", "hun\u00b7ger", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "PPER", "$(", "PWS", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So sah sie/ wo sie sonst zu naschen was bekam.", "tokens": ["So", "sah", "sie", "/", "wo", "sie", "sonst", "zu", "na\u00b7schen", "was", "be\u00b7kam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWAV", "PPER", "ADV", "PTKZU", "VVINF", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}