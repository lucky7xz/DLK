{"textgrid.poem.65181": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r's, mir zur Wahl, in meine Macht gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r's, mir zur Wahl, in meine Macht gegeben,", "tokens": ["W\u00e4r's", ",", "mir", "zur", "Wahl", ",", "in", "mei\u00b7ne", "Macht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs neue zu beginnen dieses Sein,", "tokens": ["Aufs", "neu\u00b7e", "zu", "be\u00b7gin\u00b7nen", "die\u00b7ses", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKZU", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wendete mich ab mit scheuem Beben", "tokens": ["Ich", "wen\u00b7de\u00b7te", "mich", "ab", "mit", "scheu\u00b7em", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und riefe schaudernd: Nein! und dreimal Nein!", "tokens": ["Und", "rie\u00b7fe", "schau\u00b7dernd", ":", "Nein", "!", "und", "drei\u00b7mal", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PTKANT", "$.", "KON", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df zu gut, wie nichtig alles Streben,", "tokens": ["Ich", "wei\u00df", "zu", "gut", ",", "wie", "nich\u00b7tig", "al\u00b7les", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PWAV", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und da\u00df nichts wirklich als der Schmerz allein.", "tokens": ["Und", "da\u00df", "nichts", "wirk\u00b7lich", "als", "der", "Schmerz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Dornen, die sich um das Leben winden,", "tokens": ["Die", "Dor\u00b7nen", ",", "die", "sich", "um", "das", "Le\u00b7ben", "win\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es ist genug, sie ", "tokens": ["Es", "ist", "ge\u00b7nug", ",", "sie"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein and'rer Wunsch, seht, ich gesteh's in Treuen!", "tokens": ["Ein", "an\u00b7d'\u00b7rer", "Wunsch", ",", "seht", ",", "ich", "ge\u00b7steh's", "in", "Treu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,", "PPER", "NE", "APPR", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Regt th\u00f6richt sich in meiner m\u00fcden Brust:", "tokens": ["Regt", "th\u00f6\u00b7richt", "sich", "in", "mei\u00b7ner", "m\u00fc\u00b7den", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf einen Tag nur m\u00f6cht' ich mich erneuen", "tokens": ["Auf", "ei\u00b7nen", "Tag", "nur", "m\u00f6cht'", "ich", "mich", "er\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In voller Jugendkraft und Jugendlust,", "tokens": ["In", "vol\u00b7ler", "Ju\u00b7gend\u00b7kraft", "und", "Ju\u00b7gend\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am heitern Glanz des Lebens mich erfreuen,", "tokens": ["Am", "hei\u00b7tern", "Glanz", "des", "Le\u00b7bens", "mich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Wehs, das er verschleiert, nicht bewu\u00dft,", "tokens": ["Des", "Wehs", ",", "das", "er", "ver\u00b7schlei\u00b7ert", ",", "nicht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mitten in so arglosem Genie\u00dfen", "tokens": ["Und", "mit\u00b7ten", "in", "so", "arg\u00b7lo\u00b7sem", "Ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das hold get\u00e4uschte Aug' im Tode schlie\u00dfen.", "tokens": ["Das", "hold", "ge\u00b7t\u00e4uschte", "Aug'", "im", "To\u00b7de", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}