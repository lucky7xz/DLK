{"dta.poem.12467": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "J.....?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wieder ob den Landen", "tokens": ["Und", "wie\u00b7der", "ob", "den", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lag j\u00fcngst ein schwerer Bann:", "tokens": ["Lag", "j\u00fcngst", "ein", "schwe\u00b7rer", "Bann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist ein Mann erstanden,", "tokens": ["Da", "ist", "ein", "Mann", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein ganzer, deutscher Mann;", "tokens": ["Ein", "gan\u00b7zer", ",", "deut\u00b7scher", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Deutscher und ein Freier,", "tokens": ["Ein", "Deut\u00b7scher", "und", "ein", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer h\u00e4tte das gedacht?", "tokens": ["Wer", "h\u00e4t\u00b7te", "das", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df selbst die deutsche Leier", "tokens": ["Da\u00df", "selbst", "die", "deut\u00b7sche", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus ihrem Schlaf erwacht.", "tokens": ["Aus", "ih\u00b7rem", "Schlaf", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Deutscher und ein Freier,", "tokens": ["Ein", "Deut\u00b7scher", "und", "ein", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ihr wohl selten schaut;", "tokens": ["Was", "ihr", "wohl", "sel\u00b7ten", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja wohl, ein k\u00fchner Freier", "tokens": ["Ja", "wohl", ",", "ein", "k\u00fch\u00b7ner", "Frei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um eine stolze Braut:", "tokens": ["Um", "ei\u00b7ne", "stol\u00b7ze", "Braut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der schwur gar laute Fehde", "tokens": ["Der", "schwur", "gar", "lau\u00b7te", "Feh\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der trotzigen Gewalt,", "tokens": ["Der", "trot\u00b7zi\u00b7gen", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df rings von seiner Rede", "tokens": ["Da\u00df", "rings", "von", "sei\u00b7ner", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Echo widerhallt:", "tokens": ["Das", "E\u00b7cho", "wi\u00b7der\u00b7hallt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201em\u00f6g' euch der Herr beh\u00fcten", "tokens": ["\u201e", "m\u00f6g'", "euch", "der", "Herr", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kronen lichten Glanz:", "tokens": ["Der", "Kro\u00b7nen", "lich\u00b7ten", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch flechtet aus den Bl\u00fcten", "tokens": ["Doch", "flech\u00b7tet", "aus", "den", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch endlich einen Kranz;", "tokens": ["Auch", "end\u00b7lich", "ei\u00b7nen", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um all die deutschen Sonnen", "tokens": ["Um", "all", "die", "deut\u00b7schen", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df auch Ein Himmel sein,", "tokens": ["Mu\u00df", "auch", "Ein", "Him\u00b7mel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er mu\u00df zu Einer Tonnen,", "tokens": ["Er", "mu\u00df", "zu", "Ei\u00b7ner", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der deutsche Feuerwein.\u201c", "tokens": ["Der", "deut\u00b7sche", "Feu\u00b7er\u00b7wein", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201edrum kommt, ihr Herrn, geschwinde,", "tokens": ["\u201e", "drum", "kommt", ",", "ihr", "Herrn", ",", "ge\u00b7schwin\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00dft uns zur Taufe gehn:", "tokens": ["La\u00dft", "uns", "zur", "Tau\u00b7fe", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei einem sch\u00f6nen Kinde", "tokens": ["Bei", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sollt ihr Gevatter stehn!", "tokens": ["Sollt", "ihr", "Ge\u00b7vat\u00b7ter", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wollt ihr den Namen wissen?", "tokens": ["Wollt", "ihr", "den", "Na\u00b7men", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr bindet in die Kissen", "tokens": ["Ihr", "bin\u00b7det", "in", "die", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm wohl die ", "tokens": ["Ihm", "wohl", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "\u201eund was ihr sonst versprochen:", "tokens": ["\u201e", "und", "was", "ihr", "sonst", "ver\u00b7spro\u00b7chen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebt auch die Rede frei!", "tokens": ["Gebt", "auch", "die", "Re\u00b7de", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wird ja doch zerbrochen,", "tokens": ["Er", "wird", "ja", "doch", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Stab der Tyrannei;", "tokens": ["Der", "Stab", "der", "Ty\u00b7ran\u00b7nei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nie wird sich mehr erheben", "tokens": ["Nie", "wird", "sich", "mehr", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis zu des Adlers Nest", "tokens": ["Bis", "zu", "des", "Ad\u00b7lers", "Nest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Wespe, die ihr Leben", "tokens": ["Die", "Wes\u00b7pe", ",", "die", "ihr", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit ihrem Stachel l\u00e4\u00dft.\u201c", "tokens": ["Mit", "ih\u00b7rem", "Sta\u00b7chel", "l\u00e4\u00dft", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201ees wird zunichte werden,", "tokens": ["\u201e", "es", "wird", "zu\u00b7nich\u00b7te", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sklaverei Phantom,", "tokens": ["Der", "Skla\u00b7ve\u00b7rei", "Phan\u00b7tom", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und frei rauscht durch die Erden", "tokens": ["Und", "frei", "rauscht", "durch", "die", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Freiheit Alpenstrom;", "tokens": ["Der", "Frei\u00b7heit", "Al\u00b7pen\u00b7strom", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Strom, der sich sein Bette", "tokens": ["Der", "Strom", ",", "der", "sich", "sein", "Bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur tiefer, tiefer w\u00fchlt,", "tokens": ["Nur", "tie\u00b7fer", ",", "tie\u00b7fer", "w\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis er die letzte Kette", "tokens": ["Bis", "er", "die", "letz\u00b7te", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Menschheit fortgesp\u00fclt.\u201c", "tokens": ["Der", "Menschheit", "fort\u00b7ge\u00b7sp\u00fclt", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "\u201evertraut doch eurem Volke,", "tokens": ["\u201e", "ver\u00b7traut", "doch", "eu\u00b7rem", "Vol\u00b7ke", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Seemann, der nie irrt", "tokens": ["Dem", "See\u00b7mann", ",", "der", "nie", "irrt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und wei\u00df, was euch die Wolke", "tokens": ["Und", "wei\u00df", ",", "was", "euch", "die", "Wol\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Abend bringen wird;", "tokens": ["Am", "A\u00b7bend", "brin\u00b7gen", "wird", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Schnitter, der die Garbe,", "tokens": ["Dem", "Schnit\u00b7ter", ",", "der", "die", "Gar\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die reife, wohl erkennt;", "tokens": ["Die", "rei\u00b7fe", ",", "wohl", "er\u00b7kennt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem Krieger, den die Narbe", "tokens": ["Dem", "Krie\u00b7ger", ",", "den", "die", "Nar\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor jedem Treffen brennt!\u201c", "tokens": ["Vor", "je\u00b7dem", "Tref\u00b7fen", "brennt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201ees kommt ein Sturm, drum gehen", "tokens": ["\u201e", "es", "kommt", "ein", "Sturm", ",", "drum", "ge\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Seelen auch so hoch,", "tokens": ["Die", "See\u00b7len", "auch", "so", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr m\u00fc\u00dft das Steuer drehen:", "tokens": ["Ihr", "m\u00fc\u00dft", "das", "Steu\u00b7er", "dre\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So h\u00f6rt, ihr F\u00fcrsten, doch!", "tokens": ["So", "h\u00f6rt", ",", "ihr", "F\u00fcrs\u00b7ten", ",", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier hilft kein Compa\u00dfregeln,", "tokens": ["Hier", "hilft", "kein", "Com\u00b7pa\u00df\u00b7re\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier hilft am Strand kein Thurm;", "tokens": ["Hier", "hilft", "am", "Strand", "kein", "Thurm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So rief er laute Fehde", "tokens": ["So", "rief", "er", "lau\u00b7te", "Feh\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der trotzigen Gewalt,", "tokens": ["Der", "trot\u00b7zi\u00b7gen", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df noch von seiner Rede", "tokens": ["Da\u00df", "noch", "von", "sei\u00b7ner", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Echo widerhallt.", "tokens": ["Das", "E\u00b7cho", "wi\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Weisen, den Geehrten,", "tokens": ["Den", "Wei\u00b7sen", ",", "den", "Ge\u00b7ehr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat's aber mi\u00dfbehagt:", "tokens": ["Hat's", "a\u00b7ber", "mi\u00df\u00b7be\u00b7hagt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gleich jenen Schriftgelehrten,", "tokens": ["Gleich", "je\u00b7nen", "Schrift\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn sie der Herr gefragt.", "tokens": ["Wenn", "sie", "der", "Herr", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}