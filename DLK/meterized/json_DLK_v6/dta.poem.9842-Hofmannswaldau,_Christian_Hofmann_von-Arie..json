{"dta.poem.9842": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Arie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ruhm durch tapfern muth erwirbt/", "tokens": ["Wer", "ruhm", "durch", "tap\u00b7fern", "muth", "er\u00b7wirbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird selber die tugend zum ehrenmahl haben.", "tokens": ["Wird", "sel\u00b7ber", "die", "tu\u00b7gend", "zum", "eh\u00b7ren\u00b7mahl", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch wer von blo\u00dfem drohen stirbt/", "tokens": ["Doch", "wer", "von", "blo\u00b7\u00dfem", "dro\u00b7hen", "stirbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den soll man mit esels-carthaunen begraben.", "tokens": ["Den", "soll", "man", "mit", "e\u00b7sels\u00b7cart\u00b7hau\u00b7nen", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "APPR", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Das ist ein mann/ der ist von helden art/", "tokens": ["Das", "ist", "ein", "mann", "/", "der", "ist", "von", "hel\u00b7den", "art", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer keine courage zur ehre nicht spart.", "tokens": ["Wer", "kei\u00b7ne", "cou\u00b7ra\u00b7ge", "zur", "eh\u00b7re", "nicht", "spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "APPRART", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Unverzagt!", "tokens": ["Un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wenn man sich will verlieben/", "tokens": ["Wenn", "man", "sich", "will", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So wird das auch zur losung vorgeschrieben.", "tokens": ["So", "wird", "das", "auch", "zur", "lo\u00b7sung", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ob schon das vorgesetzte ziel", "tokens": ["Ob", "schon", "das", "vor\u00b7ge\u00b7setz\u00b7te", "ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nicht gleich ergehen will/", "tokens": ["Nicht", "gleich", "er\u00b7ge\u00b7hen", "will", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So lasse man den muth nicht sincken.", "tokens": ["So", "las\u00b7se", "man", "den", "muth", "nicht", "sin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kein baum f\u00e4llt auf den ersten schlag.", "tokens": ["Kein", "baum", "f\u00e4llt", "auf", "den", "ers\u00b7ten", "schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Doch wenn das m\u00e4dchen gar nicht mag", "tokens": ["Doch", "wenn", "das", "m\u00e4d\u00b7chen", "gar", "nicht", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "PTKNEG", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und l\u00e4st sich etwas d\u00fcncken;", "tokens": ["Und", "l\u00e4st", "sich", "et\u00b7was", "d\u00fcn\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Nur unverzagt!", "tokens": ["Nur", "un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Man wird bald eine finden/", "tokens": ["Man", "wird", "bald", "ei\u00b7ne", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die nicht den tantz versagt.", "tokens": ["Die", "nicht", "den", "tantz", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Die lieb ist vorn wie hinten.", "tokens": ["Die", "lieb", "ist", "vorn", "wie", "hin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wofern ein ort nicht geht/", "tokens": ["Wo\u00b7fern", "ein", "ort", "nicht", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "So wird das andre rumb gedreht.", "tokens": ["So", "wird", "das", "and\u00b7re", "rumb", "ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "An solchen dingen ist kein mangel ie gewesen.", "tokens": ["An", "sol\u00b7chen", "din\u00b7gen", "ist", "kein", "man\u00b7gel", "ie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PIAT", "NN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Aus tausenden hat man zehnhundert auszulesen.", "tokens": ["Aus", "tau\u00b7sen\u00b7den", "hat", "man", "zehn\u00b7hun\u00b7dert", "aus\u00b7zu\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "PIS", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die sich am meisten sperrn/", "tokens": ["Die", "sich", "am", "meis\u00b7ten", "sperrn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "PIS", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Die habens dennoch gern.", "tokens": ["Die", "ha\u00b7bens", "den\u00b7noch", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}