{"textgrid.poem.47246": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Morgens weckte mich ein Hauch:", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgens weckte mich ein Hauch:", "tokens": ["Mor\u00b7gens", "weck\u00b7te", "mich", "ein", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling hat begonnen,", "tokens": ["Fr\u00fch\u00b7ling", "hat", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf und bade nun dich auch", "tokens": ["Auf", "und", "ba\u00b7de", "nun", "dich", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "VVFIN", "ADV", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Welt in Wonnen.", "tokens": ["Wie", "die", "Welt", "in", "Won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ging ich \u00fcbers Feld im Kreis", "tokens": ["Ging", "ich", "\u00fc\u00b7bers", "Feld", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchender Gestalten:", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "Ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eine sprach: \u00bbDu bist ein Greis,", "tokens": ["Ei\u00b7ne", "sprach", ":", "\u00bb", "Du", "bist", "ein", "Greis", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitze bei den Alten!\u00ab", "tokens": ["Sit\u00b7ze", "bei", "den", "Al\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch ich sagte. \u00bbLiebes Kind,", "tokens": ["Doch", "ich", "sag\u00b7te", ".", "\u00bb", "Lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh den Berg, den hohen,", "tokens": ["Sieh", "den", "Berg", ",", "den", "ho\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcber dessen Scheitel sind", "tokens": ["\u00dc\u00b7ber", "des\u00b7sen", "Schei\u00b7tel", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jahre viel geflohen.", "tokens": ["Jah\u00b7re", "viel", "ge\u00b7flo\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber aus dem Wintertraum", "tokens": ["A\u00b7ber", "aus", "dem", "Win\u00b7ter\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat er sich ger\u00fcttelt,", "tokens": ["Hat", "er", "sich", "ge\u00b7r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat von seines Kleides Saum", "tokens": ["Hat", "von", "sei\u00b7nes", "Klei\u00b7des", "Saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frost und Reif gesch\u00fcttelt.", "tokens": ["Frost", "und", "Reif", "ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nimmt f\u00fcrs graue Pelzgewand", "tokens": ["Nimmt", "f\u00fcrs", "grau\u00b7e", "Pelz\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sommergr\u00fcne Decken,", "tokens": ["Som\u00b7mer\u00b7gr\u00fc\u00b7ne", "De\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Um an k\u00fchler B\u00e4che Rand", "tokens": ["Um", "an", "k\u00fch\u00b7ler", "B\u00e4\u00b7che", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft sich hinzustrecken.", "tokens": ["Sanft", "sich", "hin\u00b7zu\u00b7stre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Alle Blumen ladet er,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "la\u00b7det", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm zu nahn mit D\u00fcften,", "tokens": ["Ihm", "zu", "nahn", "mit", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "APPR", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und mit Wonne badet er", "tokens": ["Und", "mit", "Won\u00b7ne", "ba\u00b7det", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den lauen L\u00fcften.", "tokens": ["In", "den", "lau\u00b7en", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Also la\u00dft mich zwischen euch", "tokens": ["Al\u00b7so", "la\u00dft", "mich", "zwi\u00b7schen", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhn und mit Behagen", "tokens": ["Ruhn", "und", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4umen unter Lenzgestr\u00e4uch", "tokens": ["Tr\u00e4u\u00b7men", "un\u00b7ter", "Lenz\u00b7ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von vergangnen Tagen.\u00ab", "tokens": ["Von", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}