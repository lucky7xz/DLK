{"textgrid.poem.44568": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[weh mir! ich bin verr\u00fcckt]", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh mir! ich bin verr\u00fcckt.", "tokens": ["Weh", "mir", "!", "ich", "bin", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich hoffs zu Deutschlands Ehre,", "tokens": ["Ich", "hoffs", "zu", "Deutschlands", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn, wenn ich es nicht w\u00e4re,", "tokens": ["Denn", ",", "wenn", "ich", "es", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4r Deutschland selbst verr\u00fcckt.", "tokens": ["W\u00e4r", "Deutschland", "selbst", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Denkt nur \u2013 ich sags mit Schaudern \u2013", "tokens": ["Denkt", "nur", "\u2013", "ich", "sags", "mit", "Schau\u00b7dern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der deutschen Weisheit Zier", "tokens": ["Der", "deut\u00b7schen", "Weis\u00b7heit", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Scheint mir ein leeres Plaudern,", "tokens": ["Scheint", "mir", "ein", "lee\u00b7res", "Plau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fast sch\u00e4m ich mich vor mir.", "tokens": ["Fast", "sch\u00e4m", "ich", "mich", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was ihre Philosophen", "tokens": ["Was", "ih\u00b7re", "Phi\u00b7lo\u00b7so\u00b7phen"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erschaut in Gottes Geist,", "tokens": ["Er\u00b7schaut", "in", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Scheint \u2013 ohne Vers und Strophen \u2013", "tokens": ["Scheint", "\u2013", "oh\u00b7ne", "Vers", "und", "Stro\u00b7phen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir Dichtung allermeist.", "tokens": ["Mir", "Dich\u00b7tung", "al\u00b7ler\u00b7meist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und zwar von schlechtster Sorte,", "tokens": ["Und", "zwar", "von", "schlechts\u00b7ter", "Sor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Meistrin Phantasie", "tokens": ["Wo", "Meis\u00b7trin", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Nebeln ballt die Worte,", "tokens": ["Zu", "Ne\u00b7beln", "ballt", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch zu Gestalten nie.", "tokens": ["Doch", "zu", "Ge\u00b7stal\u00b7ten", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Viel Wissen der Gelehrten,", "tokens": ["Viel", "Wis\u00b7sen", "der", "Ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielwisserei genannt,", "tokens": ["Viel\u00b7wis\u00b7se\u00b7rei", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimmt sie nicht als Gef\u00e4hrten", "tokens": ["Nimmt", "sie", "nicht", "als", "Ge\u00b7f\u00e4hr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOUS", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den leitenden Verstand.", "tokens": ["Den", "lei\u00b7ten\u00b7den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}