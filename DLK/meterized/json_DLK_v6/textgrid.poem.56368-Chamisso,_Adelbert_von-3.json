{"textgrid.poem.56368": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den heim sie bringen, haben sie beschuldigt,", "tokens": ["Den", "heim", "sie", "brin\u00b7gen", ",", "ha\u00b7ben", "sie", "be\u00b7schul\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df den Propheten er gel\u00e4stert habe", "tokens": ["Da\u00df", "den", "Pro\u00b7phe\u00b7ten", "er", "ge\u00b7l\u00e4s\u00b7tert", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihrem falschen Mahom nicht gehuldigt.", "tokens": ["Und", "ih\u00b7rem", "fal\u00b7schen", "Ma\u00b7hom", "nicht", "ge\u00b7hul\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der fremde Pilger ist's am Wanderstabe,", "tokens": ["Der", "frem\u00b7de", "Pil\u00b7ger", "ist's", "am", "Wan\u00b7der\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der b\u00fc\u00dfend unter diesen Palmen wallte", "tokens": ["Der", "b\u00fc\u00b7\u00dfend", "un\u00b7ter", "die\u00b7sen", "Pal\u00b7men", "wall\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und uns erz\u00e4hlte von dem heil'gen Grabe.", "tokens": ["Und", "uns", "er\u00b7z\u00e4hl\u00b7te", "von", "dem", "heil'\u00b7gen", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wird gegen ihre Henker dieser Alte", "tokens": ["Wird", "ge\u00b7gen", "ih\u00b7re", "Hen\u00b7ker", "die\u00b7ser", "Al\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bew\u00e4hren eines Christen festen Mut?", "tokens": ["Be\u00b7w\u00e4h\u00b7ren", "ei\u00b7nes", "Chris\u00b7ten", "fes\u00b7ten", "Mut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ihn st\u00e4rke Gott, da\u00df er am Glauben halte!", "tokens": ["Ihn", "st\u00e4r\u00b7ke", "Gott", ",", "da\u00df", "er", "am", "Glau\u00b7ben", "hal\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es glei\u00dfet arg verlockend zeitlich Gut;", "tokens": ["Es", "glei\u00b7\u00dfet", "arg", "ver\u00b7lo\u00b7ckend", "zeit\u00b7lich", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ihm ist's beschieden, l\u00e4\u00dft er sich verleiten,", "tokens": ["Ihm", "ist's", "be\u00b7schie\u00b7den", ",", "l\u00e4\u00dft", "er", "sich", "ver\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und bleibt er unersch\u00fcttert, flie\u00dft sein Blut.", "tokens": ["Und", "bleibt", "er", "un\u00b7er\u00b7sch\u00fct\u00b7tert", ",", "flie\u00dft", "sein", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Blickt dort nicht hin! Ein Gr\u00e4\u00dfliches bereiten", "tokens": ["Blickt", "dort", "nicht", "hin", "!", "Ein", "Gr\u00e4\u00df\u00b7li\u00b7ches", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKVZ", "$.", "ART", "NN", "VVINF"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.14": {"text": "Die blutgewohnten Schergen. Wehe, Wehe!", "tokens": ["Die", "blut\u00b7ge\u00b7wohn\u00b7ten", "Scher\u00b7gen", ".", "We\u00b7he", ",", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Vielleicht, da\u00df bald wir ihn dahin begleiten.", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "bald", "wir", "ihn", "da\u00b7hin", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "PPER", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Er kommt, \u2013 sie f\u00fchren ihn daher; ich sehe", "tokens": ["Er", "kommt", ",", "\u2013", "sie", "f\u00fch\u00b7ren", "ihn", "da\u00b7her", ";", "ich", "se\u00b7he"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "$(", "PPER", "VVFIN", "PPER", "PAV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie ein Geretteter, ihn freudig heiter,", "tokens": ["Wie", "ein", "Ge\u00b7ret\u00b7te\u00b7ter", ",", "ihn", "freu\u00b7dig", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Als ob er neuem Gl\u00fcck entgegen gehe.", "tokens": ["Als", "ob", "er", "neu\u00b7em", "Gl\u00fcck", "ent\u00b7ge\u00b7gen", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJA", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Hat er erkauft...? o nein! sie schreiten weiter", "tokens": ["Hat", "er", "er\u00b7kauft", "...", "?", "o", "nein", "!", "sie", "schrei\u00b7ten", "wei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "$(", "$.", "FM", "PTKANT", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der blut'gen St\u00e4tte zu; so war's gemeint!", "tokens": ["Der", "blut'\u00b7gen", "St\u00e4t\u00b7te", "zu", ";", "so", "wa\u00b7r's", "ge\u00b7meint", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Die Palme winkt dem starken Gottesstreiter. \u2013", "tokens": ["Die", "Pal\u00b7me", "winkt", "dem", "star\u00b7ken", "Got\u00b7tes\u00b7strei\u00b7ter", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbweint nicht! ich habe selber nicht geweint,", "tokens": ["\u00bb", "weint", "nicht", "!", "ich", "ha\u00b7be", "sel\u00b7ber", "nicht", "ge\u00b7weint", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Als ich ans Kreuz den sch\u00f6nen J\u00fcngling schlug;", "tokens": ["Als", "ich", "ans", "Kreuz", "den", "sch\u00f6\u00b7nen", "J\u00fcng\u00b7ling", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Mir war in meiner Brust das Herz versteint. \u2013", "tokens": ["Mir", "war", "in", "mei\u00b7ner", "Brust", "das", "Herz", "ver\u00b7steint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und angstgepeitscht begann den irren Zug", "tokens": ["Und", "angst\u00b7ge\u00b7peitscht", "be\u00b7gann", "den", "ir\u00b7ren", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der Frevler unter seiner S\u00fcnde Last,", "tokens": ["Der", "Frev\u00b7ler", "un\u00b7ter", "sei\u00b7ner", "S\u00fcn\u00b7de", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der Kains Zeichen an der Stirne trug. \u2013", "tokens": ["Der", "Kains", "Zei\u00b7chen", "an", "der", "Stir\u00b7ne", "trug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.28": {"text": "Der du f\u00fcr mich den Tod erduldet hast,", "tokens": ["Der", "du", "f\u00fcr", "mich", "den", "Tod", "er\u00b7dul\u00b7det", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Verf\u00fcgst du huldreich, da\u00df die Marter ende?", "tokens": ["Ver\u00b7f\u00fcgst", "du", "huld\u00b7reich", ",", "da\u00df", "die", "Mar\u00b7ter", "en\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Noch hofft ich, noch begehrt ich keine Rast.", "tokens": ["Noch", "hofft", "ich", ",", "noch", "be\u00b7gehrt", "ich", "kei\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVPP", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Unw\u00fcrdig, da\u00df dein Blick auf mich sich wende, \u2013", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", ",", "da\u00df", "dein", "Blick", "auf", "mich", "sich", "wen\u00b7de", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "PRF", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Der Tod, das Leben nicht, ist leicht zu tragen; \u2013", "tokens": ["Der", "Tod", ",", "das", "Le\u00b7ben", "nicht", ",", "ist", "leicht", "zu", "tra\u00b7gen", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKNEG", "$,", "VAFIN", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Nimm, Gott der Gnade, mich in deine H\u00e4nde.\u00ab", "tokens": ["Nimm", ",", "Gott", "der", "Gna\u00b7de", ",", "mich", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Als ihn die Schergen, ihn ans Kreuz zu schlagen,", "tokens": ["Als", "ihn", "die", "Scher\u00b7gen", ",", "ihn", "ans", "Kreuz", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ergriffen, schien es ihm erst wohl zu sein;", "tokens": ["Er\u00b7grif\u00b7fen", ",", "schien", "es", "ihm", "erst", "wohl", "zu", "sein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Die ihn umstanden nur erhoben Klagen.", "tokens": ["Die", "ihn", "um\u00b7stan\u00b7den", "nur", "er\u00b7ho\u00b7ben", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und als der Schmerz durchzuckte sein Gebein,", "tokens": ["Und", "als", "der", "Schmerz", "durch\u00b7zuck\u00b7te", "sein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Und er am Marterholz erhoben war,", "tokens": ["Und", "er", "am", "Mar\u00b7ter\u00b7holz", "er\u00b7ho\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Geno\u00df er Frieden vor der innern Pein.", "tokens": ["Ge\u00b7no\u00df", "er", "Frie\u00b7den", "vor", "der", "in\u00b7nern", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Ora pro nobis! betete die Schar", "tokens": ["O\u00b7ra", "pro", "no\u00b7bis", "!", "be\u00b7te\u00b7te", "die", "Schar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "VVFIN", "ART", "NN"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.41": {"text": "Der Gl\u00e4ub'gen, die am Fu\u00df des Kreuzes wachte;", "tokens": ["Der", "Gl\u00e4ub'\u00b7gen", ",", "die", "am", "Fu\u00df", "des", "Kreu\u00b7zes", "wach\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Sein Dulden war ein Beten immerdar.", "tokens": ["Sein", "Dul\u00b7den", "war", "ein", "Be\u00b7ten", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Der Tag, die Nacht vergingen, und es machte", "tokens": ["Der", "Tag", ",", "die", "Nacht", "ver\u00b7gin\u00b7gen", ",", "und", "es", "mach\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Der zweite Tag kein Ende seiner Qual;", "tokens": ["Der", "zwei\u00b7te", "Tag", "kein", "En\u00b7de", "sei\u00b7ner", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Die dritte Sonne schon den Lauf vollbrachte;", "tokens": ["Die", "drit\u00b7te", "Son\u00b7ne", "schon", "den", "Lauf", "voll\u00b7brach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Und wie sie scheidend warf den letzten Strahl,", "tokens": ["Und", "wie", "sie", "schei\u00b7dend", "warf", "den", "letz\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Versucht' er noch ins Auge sie zu fassen,", "tokens": ["Ver\u00b7sucht'", "er", "noch", "ins", "Au\u00b7ge", "sie", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Und rief, und atmete zum letzten Mal:", "tokens": ["Und", "rief", ",", "und", "at\u00b7me\u00b7te", "zum", "letz\u00b7ten", "Mal", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "\u00bbmein Gott, mein Gott, du hast mich nicht verlassen!\u00ab", "tokens": ["\u00bb", "mein", "Gott", ",", "mein", "Gott", ",", "du", "hast", "mich", "nicht", "ver\u00b7las\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}