{"dta.poem.6274": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und also h\u00e4rten sich die edelsten Metallen,", "tokens": ["Und", "al\u00b7so", "h\u00e4r\u00b7ten", "sich", "die", "e\u00b7dels\u00b7ten", "Me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und die so gl\u00e4ntzenden als k\u00f6stlichen Crystallen,", "tokens": ["Und", "die", "so", "gl\u00e4nt\u00b7zen\u00b7den", "als", "k\u00f6st\u00b7li\u00b7chen", "Crys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, wie gefrohrne Fluht in einem reinen Schein,", "tokens": ["Die", ",", "wie", "ge\u00b7frohr\u00b7ne", "Fluht", "in", "ei\u00b7nem", "rei\u00b7nen", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durchsicht\u2019ge C\u00f6rper seyn.", "tokens": ["Durch\u00b7sicht'\u00b7ge", "C\u00f6r\u00b7per", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}