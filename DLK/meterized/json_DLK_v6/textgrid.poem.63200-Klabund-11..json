{"textgrid.poem.63200": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4llt ein Blatt. Es st\u00fcrzt ein Baum.", "tokens": ["Es", "f\u00e4llt", "ein", "Blatt", ".", "Es", "st\u00fcrzt", "ein", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es steht der Mond. Es weht die Nacht.", "tokens": ["Es", "steht", "der", "Mond", ".", "Es", "weht", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber allem Traum und Raum", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7lem", "Traum", "und", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist eine Hoffnung sacht erwacht.", "tokens": ["Ist", "ei\u00b7ne", "Hoff\u00b7nung", "sacht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sucht nach Rast. Ein Falter fast.", "tokens": ["Sie", "sucht", "nach", "Rast", ".", "Ein", "Fal\u00b7ter", "fast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$.", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie st\u00e4ubt dahin, sie gl\u00e4nzt dahin.", "tokens": ["Sie", "st\u00e4ubt", "da\u00b7hin", ",", "sie", "gl\u00e4nzt", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer die Erde noch gehasst,", "tokens": ["Und", "wer", "die", "Er\u00b7de", "noch", "ge\u00b7hasst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bet\u00e4ubt geht und bekr\u00e4nzt er hin.", "tokens": ["Be\u00b7t\u00e4ubt", "geht", "und", "be\u00b7kr\u00e4nzt", "er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du, dem das Blut zum Halse stieg,", "tokens": ["Du", ",", "dem", "das", "Blut", "zum", "Hal\u00b7se", "stieg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der die goldne Sense schwang:", "tokens": ["Und", "der", "die", "gold\u00b7ne", "Sen\u00b7se", "schwang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stirne neig! Die Kniee bieg!", "tokens": ["Die", "Stir\u00b7ne", "neig", "!", "Die", "Kni\u00b7ee", "bieg", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gott geht seinen Donnergang!", "tokens": ["Der", "Gott", "geht", "sei\u00b7nen", "Don\u00b7ner\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}