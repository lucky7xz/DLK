{"textgrid.poem.65055": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: An deiner Brust ist meine Stelle,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deiner Brust ist meine Stelle,", "tokens": ["An", "dei\u00b7ner", "Brust", "ist", "mei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deinen Armen mein Asyl!", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "mein", "A\u00b7syl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich warf des Sturm's emp\u00f6rte Welle", "tokens": ["Mich", "warf", "des", "Stur\u00b7m's", "em\u00b7p\u00f6r\u00b7te", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An dieses bang ersehnte Ziel.", "tokens": ["An", "die\u00b7ses", "bang", "er\u00b7sehn\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gaben, die das Leben zieren,", "tokens": ["Die", "Ga\u00b7ben", ",", "die", "das", "Le\u00b7ben", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jedwedes Gut, das k\u00f6stlich hei\u00dft,", "tokens": ["Jed\u00b7we\u00b7des", "Gut", ",", "das", "k\u00f6st\u00b7lich", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich besa\u00df, mu\u00dft' ich verlieren,", "tokens": ["Was", "ich", "be\u00b7sa\u00df", ",", "mu\u00dft'", "ich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du fortan mir Alles sei'st.", "tokens": ["Da\u00df", "du", "for\u00b7tan", "mir", "Al\u00b7les", "sei'", "st."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt, da ich Alles hingegeben,", "tokens": ["Jetzt", ",", "da", "ich", "Al\u00b7les", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mir's durch dich zur\u00fcckgeschenkt,", "tokens": ["Wird", "mir's", "durch", "dich", "zu\u00b7r\u00fcck\u00b7ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn unter wonnevollem Beben", "tokens": ["Wenn", "un\u00b7ter", "won\u00b7ne\u00b7vol\u00b7lem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Mund auf meine Stirn' sich senkt.", "tokens": ["Dein", "Mund", "auf", "mei\u00b7ne", "Stirn'", "sich", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}