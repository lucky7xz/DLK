{"dta.poem.3317": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die Deutschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Deutschen, die an Ruhm ber\u00fchmtern V\u00e4tern wei-", "tokens": ["Ihr", "Deut\u00b7schen", ",", "die", "an", "Ruhm", "be\u00b7r\u00fchm\u00b7tern", "V\u00e4\u00b7tern", "wei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "chen!", "tokens": ["chen", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Verlangt ihr, gro\u00df zu seyn, so m\u00fc\u00dft ihr ihnen glei-", "tokens": ["Ver\u00b7langt", "ihr", ",", "gro\u00df", "zu", "seyn", ",", "so", "m\u00fc\u00dft", "ihr", "ih\u00b7nen", "glei"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "$,", "ADV", "VMFIN", "PPER", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "chen;", "tokens": ["chen", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Nicht an der alten Rauhigkeit!", "tokens": ["Nicht", "an", "der", "al\u00b7ten", "Rau\u00b7hig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Helden-Tugend jener Zeit", "tokens": ["Die", "Hel\u00b7den\u00b7Tu\u00b7gend", "je\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ruht nicht auf ungeschlachten Sitten,", "tokens": ["Ruht", "nicht", "auf", "un\u00b7ge\u00b7schlach\u00b7ten", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf nackter Armuth, nackten H\u00fctten.", "tokens": ["Auf", "nack\u00b7ter", "Ar\u00b7muth", ",", "nack\u00b7ten", "H\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Freundschaft Redlichkeit und ehrner Muth im", "tokens": ["In", "Freund\u00b7schaft", "Red\u00b7lich\u00b7keit", "und", "ehr\u00b7ner", "Muth", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Streite,", "tokens": ["Strei\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der ieden Tropfen Bluts dem Vaterlande weihte,", "tokens": ["Der", "ie\u00b7den", "Trop\u00b7fen", "Bluts", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jener unbewegte Sinn,", "tokens": ["Und", "je\u00b7ner", "un\u00b7be\u00b7weg\u00b7te", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der, taub zu niedrigem Gewinn,", "tokens": ["Der", ",", "taub", "zu", "nied\u00b7ri\u00b7gem", "Ge\u00b7winn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein der Ehre Stimme kannte,", "tokens": ["Al\u00b7lein", "der", "Eh\u00b7re", "Stim\u00b7me", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr Vaterland und Freyheit brannte:", "tokens": ["F\u00fcr", "Va\u00b7ter\u00b7land", "und", "Frey\u00b7heit", "brann\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das machte Deutschland gro\u00df; das eifert, nachzu-", "tokens": ["Das", "mach\u00b7te", "Deutschland", "gro\u00df", ";", "das", "ei\u00b7fert", ",", "nach\u00b7zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "NE", "ADJD", "$.", "PDS", "VVFIN", "$,", "TRUNC"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ahmen:", "tokens": ["ah\u00b7men", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So seyd ihr deutscher Art, nicht blo\u00df aus deutschem Saamen.", "tokens": ["So", "seyd", "ihr", "deut\u00b7scher", "Art", ",", "nicht", "blo\u00df", "aus", "deut\u00b7schem", "Saa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr starrt? ihr zittert und erbleicht?", "tokens": ["Ihr", "starrt", "?", "ihr", "zit\u00b7tert", "und", "er\u00b7bleicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum irrt euer Blick verscheucht?", "tokens": ["Wa\u00b7rum", "irrt", "eu\u00b7er", "Blick", "ver\u00b7scheucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ahndung hat mich nicht betrogen!", "tokens": ["Die", "Ahn\u00b7dung", "hat", "mich", "nicht", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Sklaven werdet ihr erzogen.", "tokens": ["Zu", "Skla\u00b7ven", "wer\u00b7det", "ihr", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O unsrer Schande Quell, Erziehung deutscher Ju-", "tokens": ["O", "uns\u00b7rer", "Schan\u00b7de", "Quell", ",", "Er\u00b7zie\u00b7hung", "deut\u00b7scher", "Ju"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gend!", "tokens": ["gend", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wer pflanzt in ihre Brust Empfindungen der Tugend", "tokens": ["Wer", "pflanzt", "in", "ih\u00b7re", "Brust", "Emp\u00b7fin\u00b7dun\u00b7gen", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Liebe f\u00fcr das Vaterland,", "tokens": ["Und", "Lie\u00b7be", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die unserm Hermann Lorbeern wand?", "tokens": ["Die", "un\u00b7serm", "Her\u00b7mann", "Lor\u00b7beern", "wand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer bildet ihre jungen Seelen,", "tokens": ["Wer", "bil\u00b7det", "ih\u00b7re", "jun\u00b7gen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch ehe sie das Laster w\u00e4hlen?", "tokens": ["Noch", "e\u00b7he", "sie", "das", "Las\u00b7ter", "w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man bildet nur den Leib: der J\u00fcngling lernt gefallen,", "tokens": ["Man", "bil\u00b7det", "nur", "den", "Leib", ":", "der", "J\u00fcng\u00b7ling", "lernt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$.", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lernt freyen Tanz und Spiel, in fremder Sprache lallen", "tokens": ["Lernt", "frey\u00b7en", "Tanz", "und", "Spiel", ",", "in", "frem\u00b7der", "Spra\u00b7che", "lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und buhlen, eh er mannbar ist,", "tokens": ["Und", "buh\u00b7len", ",", "eh", "er", "mann\u00b7bar", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00fcgen, die er kaum gek\u00fc\u00dft,", "tokens": ["Be\u00b7tr\u00fc\u00b7gen", ",", "die", "er", "kaum", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seinen Hals zu schlauen T\u00fccken", "tokens": ["Und", "sei\u00b7nen", "Hals", "zu", "schlau\u00b7en", "T\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Joche weicher Sitten b\u00fccken.", "tokens": ["Jm", "Jo\u00b7che", "wei\u00b7cher", "Sit\u00b7ten", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zur Ueppigkeit verw\u00f6hnt, wie kann er edel denken?", "tokens": ["Zur", "Uep\u00b7pig\u00b7keit", "ver\u00b7w\u00f6hnt", ",", "wie", "kann", "er", "e\u00b7del", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie soll er sich, als Mann, zur strengen Tugend lenken?", "tokens": ["Wie", "soll", "er", "sich", ",", "als", "Mann", ",", "zur", "stren\u00b7gen", "Tu\u00b7gend", "len\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "$,", "KOUS", "NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wird er, seiner Pflicht getreu,", "tokens": ["Und", "wird", "er", ",", "sei\u00b7ner", "Pflicht", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Schoose fauler Schwelgerey,", "tokens": ["Jm", "Schoo\u00b7se", "fau\u00b7ler", "Schwel\u00b7ge\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nie mit erkauften Uebelthaten", "tokens": ["Nie", "mit", "er\u00b7kauf\u00b7ten", "Ue\u00b7belt\u00b7ha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Vaterlandes Wohl verrathen?", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "Wohl", "ver\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Entkr\u00e4ftet vor der Zeit in Amors Myrthenstr\u00e4uchen,", "tokens": ["Ent\u00b7kr\u00e4f\u00b7tet", "vor", "der", "Zeit", "in", "A\u00b7mors", "Myr\u00b7then\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Baut er die Nachwelt an mit Kindern, die ihm gleichen,", "tokens": ["Baut", "er", "die", "Nach\u00b7welt", "an", "mit", "Kin\u00b7dern", ",", "die", "ihm", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "NN", "$,", "PRELS", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An einer gleichen Gattinn Brust,", "tokens": ["An", "ei\u00b7ner", "glei\u00b7chen", "Gat\u00b7tinn", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sorglos, unter eitler Lust,", "tokens": ["Die", "sorg\u00b7los", ",", "un\u00b7ter", "eit\u00b7ler", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur ihren Putz und Schooshund liebet,", "tokens": ["Nur", "ih\u00b7ren", "Putz", "und", "Schoos\u00b7hund", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihren Witz beym Spieltisch \u00fcbet.", "tokens": ["Und", "ih\u00b7ren", "Witz", "beym", "Spiel\u00b7tisch", "\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Aus bessrer Eltern Schoos entsprungen jene Helden,", "tokens": ["Aus", "bess\u00b7rer", "El\u00b7tern", "Schoos", "ent\u00b7sprun\u00b7gen", "je\u00b7ne", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von derer hellem Ruhm des Nachruhms B\u00fccher melden,", "tokens": ["Von", "de\u00b7rer", "hel\u00b7lem", "Ruhm", "des", "Nach\u00b7ruhms", "B\u00fc\u00b7cher", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die keinem Weltstrich unbekannt,", "tokens": ["Die", "kei\u00b7nem", "Welt\u00b7strich", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Geisseln in des Schicksals Hand,", "tokens": ["Als", "Geis\u00b7seln", "in", "des", "Schick\u00b7sals", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Rom, das feige Laster schw\u00e4chten,", "tokens": ["An", "Rom", ",", "das", "fei\u00b7ge", "Las\u00b7ter", "schw\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der halben Erde Knechtschaft r\u00e4chten:", "tokens": ["Der", "hal\u00b7ben", "Er\u00b7de", "Knecht\u00b7schaft", "r\u00e4ch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ein m\u00e4nnliches Geschlecht, stark, alles zu ertragen,", "tokens": ["Ein", "m\u00e4nn\u00b7li\u00b7ches", "Ge\u00b7schlecht", ",", "stark", ",", "al\u00b7les", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich streitbar, wann der S\u00fcd, in tr\u00e4gen Sommertagen,", "tokens": ["Gleich", "streit\u00b7bar", ",", "wann", "der", "S\u00fcd", ",", "in", "tr\u00e4\u00b7gen", "Som\u00b7mer\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die W\u00fcste Lybiens verlie\u00df;", "tokens": ["Die", "W\u00fcs\u00b7te", "Ly\u00b7bi\u00b7ens", "ver\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wann der alte Nordwind blies,", "tokens": ["Und", "wann", "der", "al\u00b7te", "Nord\u00b7wind", "blies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seine furchtbarn Fl\u00fcgel st\u00fcrmten,", "tokens": ["Und", "sei\u00b7ne", "furcht\u00b7barn", "Fl\u00fc\u00b7gel", "st\u00fcrm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schnee auf Schnee verderblich th\u00fcrmten.", "tokens": ["Die", "Schnee", "auf", "Schnee", "ver\u00b7derb\u00b7lich", "th\u00fcrm\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Zu welchem Wechsel ist der V\u00f6lker Gl\u00fcck verdammet!", "tokens": ["Zu", "wel\u00b7chem", "Wech\u00b7sel", "ist", "der", "V\u00f6l\u00b7ker", "Gl\u00fcck", "ver\u00b7dam\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein rauh verachtet Volk, das edler Muth entflammet,", "tokens": ["Ein", "rauh", "ver\u00b7ach\u00b7tet", "Volk", ",", "das", "ed\u00b7ler", "Muth", "ent\u00b7flam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Macht sich der Erde f\u00fcrchterlich,", "tokens": ["Macht", "sich", "der", "Er\u00b7de", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wird \u00fcppig und entkr\u00e4ftet sich,", "tokens": ["Wird", "\u00fcp\u00b7pig", "und", "ent\u00b7kr\u00e4f\u00b7tet", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und f\u00e4llt, nach kurzgenossnem Gl\u00fccke,", "tokens": ["Und", "f\u00e4llt", ",", "nach", "kurz\u00b7ge\u00b7noss\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schnell in sein erstes Nichts zur\u00fccke.", "tokens": ["Schnell", "in", "sein", "ers\u00b7tes", "Nichts", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}