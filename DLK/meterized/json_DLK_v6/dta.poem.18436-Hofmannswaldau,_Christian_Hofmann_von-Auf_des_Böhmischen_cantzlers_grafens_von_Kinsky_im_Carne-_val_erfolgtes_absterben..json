{"dta.poem.18436": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf des B\u00f6hmischen cantzlers/  \n grafens von Kinsky/ im Carne-  \n val erfolgtes absterben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich mitten in der lust und allgemeiner freuden/", "tokens": ["Gleich", "mit\u00b7ten", "in", "der", "lust", "und", "all\u00b7ge\u00b7mei\u00b7ner", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heist uns der tod das haupt der kl\u00fcgsten glieder meiden/", "tokens": ["Heist", "uns", "der", "tod", "das", "haupt", "der", "kl\u00fcgs\u00b7ten", "glie\u00b7der", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch kr\u00e4ucket euch nicht sehr", "tokens": ["Doch", "kr\u00e4u\u00b7cket", "euch", "nicht", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00f6stet euch vielmehr", "tokens": ["Und", "tr\u00f6s\u00b7tet", "euch", "viel\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bedienten/ hofeleut/ finantzer und soldaten/", "tokens": ["Be\u00b7dien\u00b7ten", "/", "ho\u00b7fe\u00b7leut", "/", "fi\u00b7nant\u00b7zer", "und", "sol\u00b7da\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "$(", "ADJA", "KON", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dencket nur/ wie weh euch seine geisseln thaten.", "tokens": ["Und", "den\u00b7cket", "nur", "/", "wie", "weh", "euch", "sei\u00b7ne", "geis\u00b7seln", "tha\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein kopff war gar so voll von klug erdachten lehren/", "tokens": ["Sein", "kopff", "war", "gar", "so", "voll", "von", "klug", "er\u00b7dach\u00b7ten", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df/ als er l\u00e4nger uns nicht konte sch\u00e4dlich seyn/", "tokens": ["Da\u00df", "/", "als", "er", "l\u00e4n\u00b7ger", "uns", "nicht", "kon\u00b7te", "sch\u00e4d\u00b7lich", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "ADJD", "PPER", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So schlieff er doch zuletzt im Carneval nur ein/", "tokens": ["So", "schlieff", "er", "doch", "zu\u00b7letzt", "im", "Car\u00b7ne\u00b7val", "nur", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Umb uns zum wenigsten das freuden-fest zu st\u00f6ren.", "tokens": ["Umb", "uns", "zum", "we\u00b7nigs\u00b7ten", "das", "freu\u00b7den\u00b7fest", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPRART", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}}}}