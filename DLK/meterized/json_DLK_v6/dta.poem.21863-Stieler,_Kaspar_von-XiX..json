{"dta.poem.21863": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Was Amor kaum gewust/ als er zwey Herzen", "tokens": ["Was", "A\u00b7mor", "kaum", "ge\u00b7wust", "/", "als", "er", "zwey", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "ADV", "VVPP", "$(", "KOUS", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und ihm des Bogens Krafft durch Streit", "tokens": ["und", "ihm", "des", "Bo\u00b7gens", "Krafft", "durch", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ersahr\u2019 ich selbst durch ihn. Ich sag\u2019 es A-", "tokens": ["er\u00b7sahr'", "ich", "selbst", "durch", "ihn", ".", "Ich", "sag'", "es", "A"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nu Amor in mir wohnt/ was er mir kund", "tokens": ["Nu", "A\u00b7mor", "in", "mir", "wohnt", "/", "was", "er", "mir", "kund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "APPR", "PPER", "VVFIN", "$(", "PWS", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}