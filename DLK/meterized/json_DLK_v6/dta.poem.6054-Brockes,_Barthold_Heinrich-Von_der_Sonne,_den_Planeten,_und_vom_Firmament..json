{"dta.poem.6054": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nun die Sonne nicht mehr l\u00e4ufft", "tokens": ["Wenn", "nun", "die", "Son\u00b7ne", "nicht", "mehr", "l\u00e4ufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der Eccliptica, und man von ihrem Stand", "tokens": ["In", "der", "Ec\u00b7clip\u00b7ti\u00b7ca", ",", "und", "man", "von", "ih\u00b7rem", "Stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "KON", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wahrheit recht begreifft,", "tokens": ["Die", "Wahr\u00b7heit", "recht", "be\u00b7greifft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ihre Ruh uns recht bekannt;", "tokens": ["Und", "ih\u00b7re", "Ruh", "uns", "recht", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So macht man mit geringrer M\u00fch", "tokens": ["So", "macht", "man", "mit", "ge\u00b7ring\u00b7rer", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt- und Land-Chart\u2019 unsrer Erden.", "tokens": ["Die", "Welt", "und", "Lan\u00b7dChart'", "uns\u00b7rer", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es d\u00fcrfen Poli nicht, Aequator, Tropici,", "tokens": ["Es", "d\u00fcr\u00b7fen", "Po\u00b7li", "nicht", ",", "A\u00b7e\u00b7qua\u00b7tor", ",", "Tro\u00b7pi\u00b7ci", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PTKNEG", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Am Firmamente sich mehr eingebildet werden.", "tokens": ["Am", "Fir\u00b7ma\u00b7men\u00b7te", "sich", "mehr", "ein\u00b7ge\u00b7bil\u00b7det", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}