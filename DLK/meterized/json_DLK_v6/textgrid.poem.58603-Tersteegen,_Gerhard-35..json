{"textgrid.poem.58603": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwahr, du bist ein verborgener Gott, du Gott Israels, der Heiland. Jes. 45, 15", "tokens": ["F\u00fcr\u00b7wahr", ",", "du", "bist", "ein", "ver\u00b7bor\u00b7ge\u00b7ner", "Gott", ",", "du", "Gott", "Is\u00b7raels", ",", "der", "Hei\u00b7land", ".", "Jes", ".", "45", ",", "15"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "NN", "NE", "$,", "ART", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-++-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "O du verborgner Gott, in dem wir alle leben,", "tokens": ["O", "du", "ver\u00b7borg\u00b7ner", "Gott", ",", "in", "dem", "wir", "al\u00b7le", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wirst du werden mir im Geiste offenbar?", "tokens": ["Wann", "wirst", "du", "wer\u00b7den", "mir", "im", "Geis\u00b7te", "of\u00b7fen\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Forschen der Vernunft mu\u00df sich gefangen geben,", "tokens": ["Mein", "For\u00b7schen", "der", "Ver\u00b7nunft", "mu\u00df", "sich", "ge\u00b7fan\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im dunkeln Grunde wird die Einfalt dein gewahr.", "tokens": ["Im", "dun\u00b7keln", "Grun\u00b7de", "wird", "die", "Ein\u00b7falt", "dein", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer kann, verborgner Gott, die Wunderweg' ergr\u00fcnden,", "tokens": ["Wer", "kann", ",", "ver\u00b7borg\u00b7ner", "Gott", ",", "die", "Wun\u00b7der\u00b7weg'", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch, mein Heiland, du die Deinen heiligst hier?", "tokens": ["Wo\u00b7durch", ",", "mein", "Hei\u00b7land", ",", "du", "die", "Dei\u00b7nen", "hei\u00b7ligst", "hier", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "PPER", "ART", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Volk verborgen ist, wer kann sich darin finden,", "tokens": ["Dein", "Volk", "ver\u00b7bor\u00b7gen", "ist", ",", "wer", "kann", "sich", "da\u00b7rin", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,", "PWS", "VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df Armut Kreuz und Schmach ist deiner Kinder Zier?", "tokens": ["Da\u00df", "Ar\u00b7mut", "Kreuz", "und", "Schmach", "ist", "dei\u00b7ner", "Kin\u00b7der", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}