{"dta.poem.8561": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um eine rote, gl\u00fchende Eisens\u00e4ule bis in den Himmel,", "tokens": ["Um", "ei\u00b7ne", "ro\u00b7te", ",", "gl\u00fc\u00b7hen\u00b7de", "Ei\u00b7sen\u00b7s\u00e4u\u00b7le", "bis", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "mit spitzen Glasscherben und Scheermessern gespickt,", "tokens": ["mit", "spit\u00b7zen", "Glas\u00b7scher\u00b7ben", "und", "Scheer\u00b7mes\u00b7sern", "ge\u00b7spickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "werde ich an unsichtbaren Ketten langsam rauf und runter gedreht.", "tokens": ["wer\u00b7de", "ich", "an", "un\u00b7sicht\u00b7ba\u00b7ren", "Ket\u00b7ten", "lang\u00b7sam", "rauf", "und", "run\u00b7ter", "ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+--+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Langsam, ruckweis und gr\u00fcndlich.", "tokens": ["Lang\u00b7sam", ",", "ruck\u00b7weis", "und", "gr\u00fcnd\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Ich st\u00f6hne, \u00e4chze, gurgle, br\u00fclle: Hosianna!", "tokens": ["Ich", "st\u00f6h\u00b7ne", ",", "\u00e4ch\u00b7ze", ",", "gurg\u00b7le", ",", "br\u00fcl\u00b7le", ":", "Ho\u00b7si\u00b7an\u00b7na", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "In sieben mal siebzig Ewigkeiten,", "tokens": ["In", "sie\u00b7ben", "mal", "sieb\u00b7zig", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wenn die Scherben zerm\u00fcrbt sind und die Messer nicht mehr k\u00f6nnen,", "tokens": ["wenn", "die", "Scher\u00b7ben", "zer\u00b7m\u00fcrbt", "sind", "und", "die", "Mes\u00b7ser", "nicht", "mehr", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "KON", "ART", "NN", "PTKNEG", "ADV", "VMFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "wird die S\u00e4ule schwarz stehn;", "tokens": ["wird", "die", "S\u00e4u\u00b7le", "schwarz", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "unten,", "tokens": ["un\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "in dem runden, stinkenden T\u00fcmpel um sie,", "tokens": ["in", "dem", "run\u00b7den", ",", "stin\u00b7ken\u00b7den", "T\u00fcm\u00b7pel", "um", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wird mein Hirn, meine Leber, mein Blut, der ganze Matsch geronnen liegen,", "tokens": ["wird", "mein", "Hirn", ",", "mei\u00b7ne", "Le\u00b7ber", ",", "mein", "Blut", ",", "der", "gan\u00b7ze", "Matsch", "ge\u00b7ron\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "und ich,", "tokens": ["und", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "\u201egel\u00e4utert\u201c,", "tokens": ["\u201e", "ge\u00b7l\u00e4u\u00b7tert", "\u201c", ","], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVPP", "$(", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "eine verkl\u00e4rte, selig gewordne Liebigb\u00fcchse,", "tokens": ["ei\u00b7ne", "ver\u00b7kl\u00e4r\u00b7te", ",", "se\u00b7lig", "ge\u00b7word\u00b7ne", "Lie\u00b7big\u00b7b\u00fcch\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "werde schluchzend", "tokens": ["wer\u00b7de", "schluch\u00b7zend"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "mit meinem letzten, \u00fcbrig gebliebenen Kn\u00f6chelchen", "tokens": ["mit", "mei\u00b7nem", "letz\u00b7ten", ",", "\u00fcb\u00b7rig", "ge\u00b7blie\u00b7be\u00b7nen", "Kn\u00f6\u00b7chel\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "an die Pforte des Paradieses klopfen!", "tokens": ["an", "die", "Pfor\u00b7te", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "klop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}