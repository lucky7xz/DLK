{"textgrid.poem.49944": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Fr\u00e4ulein Luise v. Breitschwert", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O eine kleine Welt voll Leben! Kenn ich sie?", "tokens": ["O", "ei\u00b7ne", "klei\u00b7ne", "Welt", "voll", "Le\u00b7ben", "!", "Kenn", "ich", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJD", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den schwachen Umri\u00df jener Tr\u00e4ume, wie?", "tokens": ["Den", "schwa\u00b7chen", "Um\u00b7ri\u00df", "je\u00b7ner", "Tr\u00e4u\u00b7me", ",", "wie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So konntest du ihn fassen, halten, sch\u00e4rfen?", "tokens": ["So", "konn\u00b7test", "du", "ihn", "fas\u00b7sen", ",", "hal\u00b7ten", ",", "sch\u00e4r\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 Sie m\u00fcssen leibhaft sein! nun zweifl' ich selber nicht,", "tokens": ["\u2013", "Sie", "m\u00fcs\u00b7sen", "leib\u00b7haft", "sein", "!", "nun", "zweifl'", "ich", "sel\u00b7ber", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADJD", "VAINF", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sie, bestrahlt von deinem Licht,", "tokens": ["Da", "sie", ",", "be\u00b7strahlt", "von", "dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entschiedne, holde Schatten werfen.", "tokens": ["Ent\u00b7schied\u00b7ne", ",", "hol\u00b7de", "Schat\u00b7ten", "wer\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund Kerner legte sich, im Reiseschattensinn,", "tokens": ["Freund", "Ker\u00b7ner", "leg\u00b7te", "sich", ",", "im", "Rei\u00b7se\u00b7schat\u00b7ten\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Album an, da quetscht er Dintendolken drin,", "tokens": ["Ein", "Al\u00b7bum", "an", ",", "da", "quetscht", "er", "Din\u00b7ten\u00b7dol\u00b7ken", "drin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zeichnet jeden Klecks nach seiner Phantasei", "tokens": ["Und", "zeich\u00b7net", "je\u00b7den", "Klecks", "nach", "sei\u00b7ner", "Phan\u00b7ta\u00b7sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit wen'gem aus und freut sich wie ein Kind dabei:", "tokens": ["Mit", "wen'\u00b7gem", "aus", "und", "freut", "sich", "wie", "ein", "Kind", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKVZ", "KON", "VVFIN", "PRF", "KOKOM", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird der nicht Augen machen, wenn er sieht,", "tokens": ["Wird", "der", "nicht", "Au\u00b7gen", "ma\u00b7chen", ",", "wenn", "er", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PTKNEG", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie anders dir der Spa\u00df geriet!", "tokens": ["Wie", "an\u00b7ders", "dir", "der", "Spa\u00df", "ge\u00b7riet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ach, was biet ich nun der K\u00fcnstlerin dagegen,", "tokens": ["Doch", "ach", ",", "was", "biet", "ich", "nun", "der", "K\u00fcnst\u00b7le\u00b7rin", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nicht etwa die ", "tokens": ["Wenn", "nicht", "et\u00b7wa", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ART"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Der gute ", "tokens": ["Der", "gu\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die H\u00e4lfte wenigstens, die goldne, w\u00e4re dein!", "tokens": ["Die", "H\u00e4lf\u00b7te", "we\u00b7nigs\u00b7tens", ",", "die", "gold\u00b7ne", ",", "w\u00e4\u00b7re", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}