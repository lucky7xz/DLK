{"textgrid.poem.44576": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[gedenkblatt f\u00fcr Meyerbeer]", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser Zeit, wo jeder will", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "wo", "je\u00b7der", "will"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PIS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und m\u00f6glichst hoch und m\u00f6glichst viel;", "tokens": ["Und", "m\u00f6g\u00b7lichst", "hoch", "und", "m\u00f6g\u00b7lichst", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo k\u00f6rperlos die Weltideen", "tokens": ["Wo", "k\u00f6r\u00b7per\u00b7los", "die", "Wel\u00b7ti\u00b7de\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wie Geister durch die Stra\u00dfen gehen,", "tokens": ["Wie", "Geis\u00b7ter", "durch", "die", "Stra\u00b7\u00dfen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, k\u00f6mmts zu bilden, was gedacht,", "tokens": ["Doch", ",", "k\u00f6mmts", "zu", "bil\u00b7den", ",", "was", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "PTKZU", "VVINF", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Wollen fehlt des Werkes Macht,", "tokens": ["Dem", "Wol\u00b7len", "fehlt", "des", "Wer\u00b7kes", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir von der Harmonie der Sph\u00e4ren", "tokens": ["Wir", "von", "der", "Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Reibung, nicht den Einklang h\u00f6ren:", "tokens": ["Die", "Rei\u00b7bung", ",", "nicht", "den", "Ein\u00b7klang", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da la\u00dft uns hoch den Meister ehren,", "tokens": ["Da", "la\u00dft", "uns", "hoch", "den", "Meis\u00b7ter", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Gro\u00dfes will und, als ein Mann,", "tokens": ["Der", "Gro\u00b7\u00dfes", "will", "und", ",", "als", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KON", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was er gewollt, auch machen kann!", "tokens": ["Was", "er", "ge\u00b7wollt", ",", "auch", "ma\u00b7chen", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMPP", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}