{"textgrid.poem.32599": {"metadata": {"author": {"name": "Shelley, Percy Bysshe", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf! auf! auf!", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! auf! auf!", "tokens": ["Auf", "!", "auf", "!", "auf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "$.", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Blut dampft von der Erde, die Brot euch versagt.", "tokens": ["Blut", "dampft", "von", "der", "Er\u00b7de", ",", "die", "Brot", "euch", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Um die Todten, die sanken zuhauf,", "tokens": ["Um", "die", "Tod\u00b7ten", ",", "die", "san\u00b7ken", "zu\u00b7hauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "ADJD", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sei aus str\u00f6menden Wunden ein Grablied geklagt.", "tokens": ["Sei", "aus", "str\u00f6\u00b7men\u00b7den", "Wun\u00b7den", "ein", "Grab\u00b7lied", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Keine andere Trauer sei ihnen gebracht!", "tokens": ["Kei\u00b7ne", "an\u00b7de\u00b7re", "Trau\u00b7er", "sei", "ih\u00b7nen", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sohn, Bruder und Gattin sind niedergemacht;", "tokens": ["Sohn", ",", "Bru\u00b7der", "und", "Gat\u00b7tin", "sind", "nie\u00b7der\u00b7ge\u00b7macht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wer sagt, da\u00df sie fielen in ehrlicher Schlacht?", "tokens": ["Wer", "sagt", ",", "da\u00df", "sie", "fie\u00b7len", "in", "ehr\u00b7li\u00b7cher", "Schlacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Erwacht! erwacht! erwacht!", "tokens": ["Er\u00b7wacht", "!", "er\u00b7wacht", "!", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVPP", "$.", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seit je befeinden Tyrann sich und Knecht.", "tokens": ["Seit", "je", "be\u00b7fein\u00b7den", "Ty\u00b7rann", "sich", "und", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "PRF", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Werft nieder die Ketten mit Macht", "tokens": ["Werft", "nie\u00b7der", "die", "Ket\u00b7ten", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In den Staub, da\u00df den Tod ihr der Br\u00fcder r\u00e4cht!", "tokens": ["In", "den", "Staub", ",", "da\u00df", "den", "Tod", "ihr", "der", "Br\u00fc\u00b7der", "r\u00e4cht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPOSAT", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Im Grabe wird regen sich ihr Gebein,", "tokens": ["Im", "Gra\u00b7be", "wird", "re\u00b7gen", "sich", "ihr", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJA", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn die Stimmen der Lieben im blutigen Schein", "tokens": ["Wenn", "die", "Stim\u00b7men", "der", "Lie\u00b7ben", "im", "blu\u00b7ti\u00b7gen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "APPRART", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Des heiligen Kampfes um Rache schrein.", "tokens": ["Des", "hei\u00b7li\u00b7gen", "Kamp\u00b7fes", "um", "Ra\u00b7che", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Hoch la\u00dft das Banner wehn,", "tokens": ["Hoch", "la\u00dft", "das", "Ban\u00b7ner", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn die Freiheit ladet zu Sieg und Tod,", "tokens": ["Wenn", "die", "Frei\u00b7heit", "la\u00b7det", "zu", "Sieg", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ob als Sklaven auch um sie stehn", "tokens": ["Ob", "als", "Skla\u00b7ven", "auch", "um", "sie", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "ADV", "APPR", "PPER", "VVINF"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hunger und Elend und seufzende Noth.", "tokens": ["Hun\u00b7ger", "und", "E\u00b7lend", "und", "seuf\u00b7zen\u00b7de", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Und ihr, die geschaart um ihr herrlich Gef\u00e4hrt,", "tokens": ["Und", "ihr", ",", "die", "ge\u00b7schaart", "um", "ihr", "herr\u00b7lich", "Ge\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADJD", "APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Z\u00fcckt nicht zuerst das mordende Schwert,", "tokens": ["Z\u00fcckt", "nicht", "zu\u00b7erst", "das", "mor\u00b7den\u00b7de", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Doch die Mutter zu sch\u00fctzen, seid mannlich bewehrt!", "tokens": ["Doch", "die", "Mut\u00b7ter", "zu", "sch\u00fct\u00b7zen", ",", "seid", "mann\u00b7lich", "be\u00b7wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Heil, Heil, Heil", "tokens": ["Heil", ",", "Heil", ",", "Heil"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Denen, die litten und Gro\u00dfes vollbracht!", "tokens": ["De\u00b7nen", ",", "die", "lit\u00b7ten", "und", "Gro\u00b7\u00dfes", "voll\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "VVFIN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Keinem wurde zu Theil", "tokens": ["Kei\u00b7nem", "wur\u00b7de", "zu", "Theil"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gr\u00f6\u00dferer Ruhm, als der ", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7rer", "Ruhm", ",", "als", "der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "ART"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.5": {"text": "Den Feind nur haben Erobrer bekriegt,", "tokens": ["Den", "Feind", "nur", "ha\u00b7ben", "E\u00b7rob\u00b7rer", "be\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dessen Stolz nun geb\u00e4ndigt zu Boden liegt: \u2013", "tokens": ["Des\u00b7sen", "Stolz", "nun", "ge\u00b7b\u00e4n\u00b7digt", "zu", "Bo\u00b7den", "liegt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "ADV", "VVPP", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ihr habt, siegreicher, euch selbst besiegt.", "tokens": ["Ihr", "habt", ",", "sieg\u00b7rei\u00b7cher", ",", "euch", "selbst", "be\u00b7siegt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Kr\u00e4nzt, kr\u00e4nzt eure Stirn", "tokens": ["Kr\u00e4nzt", ",", "kr\u00e4nzt", "eu\u00b7re", "Stirn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit Veilchen, Epheu und Tannengr\u00fcn;", "tokens": ["Mit", "Veil\u00b7chen", ",", "E\u00b7pheu", "und", "Tan\u00b7nen\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bedeckt das blutige Hirn", "tokens": ["Be\u00b7deckt", "das", "blu\u00b7ti\u00b7ge", "Hirn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mit Farben, wie g\u00f6ttlich im Lenz sie gl\u00fchn:", "tokens": ["Mit", "Far\u00b7ben", ",", "wie", "g\u00f6tt\u00b7lich", "im", "Lenz", "sie", "gl\u00fchn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADJD", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Gr\u00fcne Kraft, blaue Hoffnung und Ewigkeit,", "tokens": ["Gr\u00fc\u00b7ne", "Kraft", ",", "blau\u00b7e", "Hoff\u00b7nung", "und", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Doch Vergi\u00dfmeinnichtbl\u00fcmchen verbannet weit,", "tokens": ["Doch", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht\u00b7bl\u00fcm\u00b7chen", "ver\u00b7ban\u00b7net", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Bewahrt das Gedenken an euer Leid!", "tokens": ["Be\u00b7wahrt", "das", "Ge\u00b7den\u00b7ken", "an", "eu\u00b7er", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}