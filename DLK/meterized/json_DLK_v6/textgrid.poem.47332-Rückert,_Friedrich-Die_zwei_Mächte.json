{"textgrid.poem.47332": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei M\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wein und sch\u00f6ne M\u00e4dchen", "tokens": ["Wein", "und", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind zwei Zauberf\u00e4dchen,", "tokens": ["Sind", "zwei", "Zau\u00b7ber\u00b7f\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die auch die erfahrnen", "tokens": ["Die", "auch", "die", "er\u00b7fahr\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "V\u00f6gel gern umgarnen.", "tokens": ["V\u00f6\u00b7gel", "gern", "um\u00b7gar\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Becherrand und Lippen,", "tokens": ["Be\u00b7cher\u00b7rand", "und", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zwei Korallenklippen,", "tokens": ["Zwei", "Ko\u00b7ral\u00b7len\u00b7klip\u00b7pen", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo auch die gescheitern", "tokens": ["Wo", "auch", "die", "ge\u00b7schei\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schiffer gerne scheitern.", "tokens": ["Schif\u00b7fer", "ger\u00b7ne", "schei\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Kommst du in die Schenke,", "tokens": ["Kommst", "du", "in", "die", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf ein Knie dich senke!", "tokens": ["Auf", "ein", "Knie", "dich", "sen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn hier sitzen F\u00fcrsten,", "tokens": ["Denn", "hier", "sit\u00b7zen", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die nach Ruhme d\u00fcrsten.", "tokens": ["Die", "nach", "Ruh\u00b7me", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und die Liebeszettler", "tokens": ["Und", "die", "Lie\u00b7bes\u00b7zett\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schelte keine Bettler!", "tokens": ["Schel\u00b7te", "kei\u00b7ne", "Bett\u00b7ler", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jeder tr\u00e4gt von Schmerzen", "tokens": ["Je\u00b7der", "tr\u00e4gt", "von", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Einen Schatz im Herzen.", "tokens": ["Ei\u00b7nen", "Schatz", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Liebe und Herr Becher!", "tokens": ["Lie\u00b7be", "und", "Herr", "Be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freigeborner Zecher", "tokens": ["Frei\u00b7ge\u00b7bor\u00b7ner", "Ze\u00b7cher"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nigin und K\u00f6nig!", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "und", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Eurem Throne fr\u00f6n' ich.", "tokens": ["Eu\u00b7rem", "Thro\u00b7ne", "fr\u00f6n'", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Helfet ihr zu Rechte", "tokens": ["Hel\u00b7fet", "ihr", "zu", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Menschlichem Geschlechte,", "tokens": ["Menschli\u00b7chem", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wird es unter Tr\u00fcmmern", "tokens": ["Wird", "es", "un\u00b7ter", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Niemals gar verk\u00fcmmern.", "tokens": ["Nie\u00b7mals", "gar", "ver\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Gestern trat ein Weiser", "tokens": ["Ge\u00b7stern", "trat", "ein", "Wei\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vor des Himmels Kaiser,", "tokens": ["Vor", "des", "Him\u00b7mels", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Frug, wie lang' die n\u00e4rr'schen", "tokens": ["Frug", ",", "wie", "lang'", "die", "n\u00e4rr'\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Leute sollten herrschen?", "tokens": ["Leu\u00b7te", "soll\u00b7ten", "herr\u00b7schen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und Gott sprach: So lange", "tokens": ["Und", "Gott", "sprach", ":", "So", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$.", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eure Weisheit bange", "tokens": ["Eu\u00b7re", "Weis\u00b7heit", "ban\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird den Menschen machen,", "tokens": ["Wird", "den", "Men\u00b7schen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Soll die Thorheit lachen.", "tokens": ["Soll", "die", "Thor\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}