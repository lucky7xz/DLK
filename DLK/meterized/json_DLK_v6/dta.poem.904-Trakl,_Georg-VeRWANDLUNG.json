{"dta.poem.904": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "VeRWANDLUNG", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Entlang an G\u00e4rten, herbstlich, rotversengt:", "tokens": ["Ent\u00b7lang", "an", "G\u00e4r\u00b7ten", ",", "herbst\u00b7lich", ",", "rot\u00b7ver\u00b7sengt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier zeigt im Stillen sich ein t\u00fcchtig Leben.", "tokens": ["Hier", "zeigt", "im", "Stil\u00b7len", "sich", "ein", "t\u00fcch\u00b7tig", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PRF", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Menschen H\u00e4nde tragen braune Reben,", "tokens": ["Des", "Men\u00b7schen", "H\u00e4n\u00b7de", "tra\u00b7gen", "brau\u00b7ne", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indes der sanfte Schmerz im Blick sich senkt.", "tokens": ["In\u00b7des", "der", "sanf\u00b7te", "Schmerz", "im", "Blick", "sich", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Abend: Schritte gehn durch schwarzes Land", "tokens": ["Am", "A\u00b7bend", ":", "Schrit\u00b7te", "gehn", "durch", "schwar\u00b7zes", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erscheinender in roter Buchen Schweigen.", "tokens": ["Er\u00b7schei\u00b7nen\u00b7der", "in", "ro\u00b7ter", "Bu\u00b7chen", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein blaues Tier will sich vorm Tod verneigen", "tokens": ["Ein", "blau\u00b7es", "Tier", "will", "sich", "vorm", "Tod", "ver\u00b7nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und grauenvoll verf\u00e4llt ein leer Gewand.", "tokens": ["Und", "grau\u00b7en\u00b7voll", "ver\u00b7f\u00e4llt", "ein", "leer", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geruhiges vor einer Schenke spielt,", "tokens": ["Ge\u00b7ru\u00b7hi\u00b7ges", "vor", "ei\u00b7ner", "Schen\u00b7ke", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ein Antlitz ist berauscht ins Gras gesunken.", "tokens": ["Ein", "Ant\u00b7litz", "ist", "be\u00b7rauscht", "ins", "Gras", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hollunderfr\u00fcchte, Fl\u00f6ten weich und trunken,", "tokens": ["Hol\u00b7lun\u00b7der\u00b7fr\u00fcch\u00b7te", ",", "Fl\u00f6\u00b7ten", "weich", "und", "trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Resedenduft, der Weibliches umsp\u00fclt.", "tokens": ["Re\u00b7se\u00b7den\u00b7duft", ",", "der", "Weib\u00b7li\u00b7ches", "um\u00b7sp\u00fclt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}