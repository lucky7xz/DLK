{"textgrid.poem.52214": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Guten Morgen!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stand ich droben auf der Eifel K\u00e4mmen,", "tokens": ["Stand", "ich", "dro\u00b7ben", "auf", "der", "Ei\u00b7fel", "K\u00e4m\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als der Vollmond durch die Wolken brach;", "tokens": ["Als", "der", "Voll\u00b7mond", "durch", "die", "Wol\u00b7ken", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Breit und blendend sah ich \u00fcberschwemmen", "tokens": ["Breit", "und", "blen\u00b7dend", "sah", "ich", "\u00fc\u00b7bersc\u00b7hwem\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine Lichter See und Kloster Laach.", "tokens": ["Sei\u00b7ne", "Lich\u00b7ter", "See", "und", "Klos\u00b7ter", "Laach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Leiser Windhauch wehte durch die Tale,", "tokens": ["Lei\u00b7ser", "Wind\u00b7hauch", "weh\u00b7te", "durch", "die", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Laub und Rohr umfl\u00fcsterten den Strand,", "tokens": ["Laub", "und", "Rohr", "um\u00b7fl\u00fcs\u00b7ter\u00b7ten", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Flut entreckte sich die schmale,", "tokens": ["Und", "der", "Flut", "en\u00b7treck\u00b7te", "sich", "die", "schma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jene schmale, wei\u00dfe Nonnenhand.", "tokens": ["Je\u00b7ne", "schma\u00b7le", ",", "wei\u00b7\u00dfe", "Non\u00b7nen\u00b7hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Anzuschaun wie eine Blum' von ferne,", "tokens": ["An\u00b7zu\u00b7schaun", "wie", "ei\u00b7ne", "Blum'", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit den Wellen flog sie auf und ab;", "tokens": ["Mit", "den", "Wel\u00b7len", "flog", "sie", "auf", "und", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rings gespiegelt schwamm das Heer der Sterne: \u2013", "tokens": ["Rings", "ge\u00b7spie\u00b7gelt", "schwamm", "das", "Heer", "der", "Ster\u00b7ne", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Raffte sie's vom Himmel herab?", "tokens": ["Raff\u00b7te", "sie's", "vom", "Him\u00b7mel", "her\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Winkt' und winkte mir sodann die reine!", "tokens": ["Winkt'", "und", "wink\u00b7te", "mir", "so\u00b7dann", "die", "rei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie sich sch\u00fcttelnd rauscht' empor der See;", "tokens": ["Wie", "sich", "sch\u00fct\u00b7telnd", "rauscht'", "em\u00b7por", "der", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die Waldung huschten eigne Scheine;", "tokens": ["Durch", "die", "Wal\u00b7dung", "huschten", "eig\u00b7ne", "Schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00dcbern Kreuzweg sprang entsetzt das Reh.", "tokens": ["\u00dc\u00b7bern", "Kreuz\u00b7weg", "sprang", "ent\u00b7setzt", "das", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "War's die Hinde, die in ihren Tr\u00e4nen", "tokens": ["Wa\u00b7r's", "die", "Hin\u00b7de", ",", "die", "in", "ih\u00b7ren", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genoveven weiland sich gesellt?", "tokens": ["Ge\u00b7no\u00b7ve\u00b7ven", "wei\u00b7land", "sich", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach, mich fa\u00dfte schmerzlichs\u00fc\u00dfes Sehnen", "tokens": ["Ach", ",", "mich", "fa\u00df\u00b7te", "schmerz\u00b7lich\u00b7s\u00fc\u00b7\u00dfes", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nach der sel'gen alten M\u00e4rchenwelt!", "tokens": ["Nach", "der", "sel'\u00b7gen", "al\u00b7ten", "M\u00e4r\u00b7chen\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und beinahe jenem bleichen Finger", "tokens": ["Und", "bei\u00b7na\u00b7he", "je\u00b7nem", "blei\u00b7chen", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00e4r' gefolgt ich durch ihr offnes Tor;", "tokens": ["W\u00e4r'", "ge\u00b7folgt", "ich", "durch", "ihr", "off\u00b7nes", "Tor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch erwachend, mit mir selbst ein Ringer,", "tokens": ["Doch", "er\u00b7wa\u00b7chend", ",", "mit", "mir", "selbst", "ein", "Rin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rafft' ich stark und mutig mich empor!", "tokens": ["Rafft'", "ich", "stark", "und", "mu\u00b7tig", "mich", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "See und Kloster, T\u00fcrm' und Felsenspitzen,", "tokens": ["See", "und", "Klos\u00b7ter", ",", "T\u00fcrm'", "und", "Fel\u00b7sen\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wald und Schlucht, wo Genoveva litt \u2013", "tokens": ["Wald", "und", "Schlucht", ",", "wo", "Ge\u00b7no\u00b7ve\u00b7va", "litt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "NE", "VVFIN", "$("], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Einmal noch im Mondschein sah ich's blitzen,", "tokens": ["Ein\u00b7mal", "noch", "im", "Mond\u00b7schein", "sah", "ich's", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dann wandt' ich herzhaft meinen Schritt!", "tokens": ["Und", "dann", "wandt'", "ich", "herz\u00b7haft", "mei\u00b7nen", "Schritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Drauf verwirrend noch der Mondschein lag;", "tokens": ["Drauf", "ver\u00b7wir\u00b7rend", "noch", "der", "Mond\u00b7schein", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ging dem Morgen und dem Rhein entgegen,", "tokens": ["Ging", "dem", "Mor\u00b7gen", "und", "dem", "Rhein", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ging entgegen aus der Nacht dem Tag!", "tokens": ["Ging", "ent\u00b7ge\u00b7gen", "aus", "der", "Nacht", "dem", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Lie\u00df die Schatten d\u00e4mmernder Gesichte", "tokens": ["Lie\u00df", "die", "Schat\u00b7ten", "d\u00e4m\u00b7mern\u00b7der", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jubelnd fahren f\u00fcr die Wirklichkeit! \u2013", "tokens": ["Ju\u00b7belnd", "fah\u00b7ren", "f\u00fcr", "die", "Wirk\u00b7lich\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVINF", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, und vor mir hell im Sonnenlichte", "tokens": ["Sieh", ",", "und", "vor", "mir", "hell", "im", "Son\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "APPR", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zog der Rheinstrom, tief und gr\u00fcn und breit!", "tokens": ["Zog", "der", "Rhein\u00b7strom", ",", "tief", "und", "gr\u00fcn", "und", "breit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Zog der Rhein und r\u00fchrte sich das Leben \u2013", "tokens": ["Zog", "der", "Rhein", "und", "r\u00fchr\u00b7te", "sich", "das", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "KON", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ja, ins Leben ri\u00df mich ", "tokens": ["Ja", ",", "ins", "Le\u00b7ben", "ri\u00df", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht erhob er, mir den Gru\u00df zu geben,", "tokens": ["Nicht", "er\u00b7hob", "er", ",", "mir", "den", "Gru\u00df", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bleich und zitternd eine Totenhand!", "tokens": ["Bleich", "und", "zit\u00b7ternd", "ei\u00b7ne", "To\u00b7ten\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Doch den Handschlag bot er mir, den treuen,", "tokens": ["Doch", "den", "Hand\u00b7schlag", "bot", "er", "mir", ",", "den", "treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eines Volkes frank und unverstellt,", "tokens": ["Ei\u00b7nes", "Vol\u00b7kes", "frank", "und", "un\u00b7ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das \u2013 in Ehrfurcht, aber ohne Scheuen! \u2013", "tokens": ["Das", "\u2013", "in", "Ehr\u00b7furcht", ",", "a\u00b7ber", "oh\u00b7ne", "Scheu\u00b7en", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$(", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr sein Recht den Fu\u00df beim Male h\u00e4lt!", "tokens": ["F\u00fcr", "sein", "Recht", "den", "Fu\u00df", "beim", "Ma\u00b7le", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "O, der bannte, was von Spuk und Sorgen", "tokens": ["O", ",", "der", "bann\u00b7te", ",", "was", "von", "Spuk", "und", "Sor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "VVFIN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "N\u00e4chtlich noch auf meinem Herzen lag!", "tokens": ["N\u00e4cht\u00b7lich", "noch", "auf", "mei\u00b7nem", "Her\u00b7zen", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meinem Volke sagt' ich: \u00bbGuten Morgen!\u00ab \u2013", "tokens": ["Mei\u00b7nem", "Vol\u00b7ke", "sagt'", "ich", ":", "\u00bb", "Gu\u00b7ten", "Mor\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einst, so Gott will, sag' ich: \u00bbGuten Tag!\u00ab", "tokens": ["Einst", ",", "so", "Gott", "will", ",", "sag'", "ich", ":", "\u00bb", "Gu\u00b7ten", "Tag", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "VMFIN", "$,", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Guten Morgen denn! \u2013 Frei werd' ich stehen", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", "denn", "!", "\u2013", "Frei", "werd'", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "$.", "$(", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr das Volk und mit ihm in der Zeit!", "tokens": ["F\u00fcr", "das", "Volk", "und", "mit", "ihm", "in", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Also les' ich meinen Schiller heut!", "tokens": ["Al\u00b7so", "les'", "ich", "mei\u00b7nen", "Schil\u00b7ler", "heut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "NE", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}