{"textgrid.poem.64548": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "172. An Mademoiselle A.B.,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genie\u00df was dir noch nicht das neunte Jahr verwehrt,", "tokens": ["Ge\u00b7nie\u00df", "was", "dir", "noch", "nicht", "das", "neun\u00b7te", "Jahr", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unschuld Gl\u00fcck, mit Puppen froh zu spielen;", "tokens": ["Der", "Un\u00b7schuld", "Gl\u00fcck", ",", "mit", "Pup\u00b7pen", "froh", "zu", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wer verwegen sie entehrt,", "tokens": ["Doch", "wer", "ver\u00b7we\u00b7gen", "sie", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den la\u00df schon Witz und Stolz von achtzehn Jahren f\u00fchlen.", "tokens": ["Den", "la\u00df", "schon", "Witz", "und", "Stolz", "von", "acht\u00b7zehn", "Jah\u00b7ren", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "ADV", "NN", "KON", "NN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}