{"textgrid.poem.40137": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Im Traume", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, aus Tr\u00e4umen fahr' ich!", "tokens": ["Ach", ",", "aus", "Tr\u00e4u\u00b7men", "fahr'", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "in die graue Luft,", "tokens": ["in", "die", "grau\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "in die kalte, starr' ich;", "tokens": ["in", "die", "kal\u00b7te", ",", "starr'", "ich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "ach, dein Samum war ich,", "tokens": ["ach", ",", "dein", "Sa\u00b7mum", "war", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du mein Ambraduft!", "tokens": ["Du", "mein", "Am\u00b7bra\u00b7duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Durch die stumme W\u00fcste", "tokens": ["Durch", "die", "stum\u00b7me", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "bebtest du dahin,", "tokens": ["beb\u00b7test", "du", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "und dein Atem k\u00fc\u00dfte", "tokens": ["und", "dein", "A\u00b7tem", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "und dein Ku\u00df vers\u00fc\u00dfte", "tokens": ["und", "dein", "Ku\u00df", "ver\u00b7s\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Seele mir und Sinn.", "tokens": ["See\u00b7le", "mir", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Einsamkeiten hingen", "tokens": ["Ein\u00b7sam\u00b7kei\u00b7ten", "hin\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "tief ins wei\u00dfe Land;", "tokens": ["tief", "ins", "wei\u00b7\u00dfe", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "sonnenstill ein Ringen,", "tokens": ["son\u00b7nen\u00b7still", "ein", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "und mit hei\u00dfen Schwingen", "tokens": ["und", "mit", "hei\u00b7\u00dfen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "hielt ich dich umspannt,", "tokens": ["hielt", "ich", "dich", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "hielt ich dich umschlungen,", "tokens": ["hielt", "ich", "dich", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "hielt ich dich umgl\u00fcht, \u2013", "tokens": ["hielt", "ich", "dich", "um\u00b7gl\u00fcht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "und mit Flammenzungen", "tokens": ["und", "mit", "Flam\u00b7men\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "hab' ich dir gesungen", "tokens": ["hab'", "ich", "dir", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "meiner Wonne Lied.", "tokens": ["mei\u00b7ner", "Won\u00b7ne", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}