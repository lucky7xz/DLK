{"textgrid.poem.66593": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu dir, mein Fels, komm' ich gegangen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu dir, mein Fels, komm' ich gegangen", "tokens": ["Zu", "dir", ",", "mein", "Fels", ",", "komm'", "ich", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Stra\u00dfen, m\u00fchevoll und weit,", "tokens": ["Von", "Stra\u00b7\u00dfen", ",", "m\u00fc\u00b7he\u00b7voll", "und", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wald ist gr\u00fcn, die Blumen prangen,", "tokens": ["Der", "Wald", "ist", "gr\u00fcn", ",", "die", "Blu\u00b7men", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles wie in alter Zeit!", "tokens": ["Und", "Al\u00b7les", "wie", "in", "al\u00b7ter", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ich nur dir zuerst gesungen,", "tokens": ["Was", "ich", "nur", "dir", "zu\u00b7erst", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versteckt von deinem Schattenplan,", "tokens": ["Ver\u00b7steckt", "von", "dei\u00b7nem", "Schat\u00b7ten\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch alle Welt ist's hingeklungen,", "tokens": ["Durch", "al\u00b7le", "Welt", "ist's", "hin\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandert seine eigne Bahn.", "tokens": ["Und", "wan\u00b7dert", "sei\u00b7ne", "eig\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Jugend schwand, nun komm' ich wieder,", "tokens": ["Die", "Ju\u00b7gend", "schwand", ",", "nun", "komm'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vers\u00f6hnt aus manchem Lebensstreit,", "tokens": ["Ver\u00b7s\u00f6hnt", "aus", "man\u00b7chem", "Le\u00b7bens\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bring' im Herzen dir noch Lieder", "tokens": ["Und", "bring'", "im", "Her\u00b7zen", "dir", "noch", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und T\u00f6ne, wie in alter Zeit.", "tokens": ["Und", "T\u00f6\u00b7ne", ",", "wie", "in", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch du, mein Fels, bist von den Alten,", "tokens": ["Auch", "du", ",", "mein", "Fels", ",", "bist", "von", "den", "Al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jedem Jahr den Quell erneu'n;", "tokens": ["Die", "je\u00b7dem", "Jahr", "den", "Quell", "er\u00b7neu'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wollen wir's auch f\u00fcrder halten", "tokens": ["So", "wol\u00b7len", "wir's", "auch", "f\u00fcr\u00b7der", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und uns der jungen Bl\u00fcthen freu'n!", "tokens": ["Und", "uns", "der", "jun\u00b7gen", "Bl\u00fc\u00b7then", "freu'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}