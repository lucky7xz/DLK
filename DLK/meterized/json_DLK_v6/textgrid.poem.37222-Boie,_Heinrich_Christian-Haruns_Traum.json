{"textgrid.poem.37222": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Haruns Traum", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Harun al Raschid, der Khalif,", "tokens": ["Ha\u00b7run", "al", "Ra\u00b7schid", ",", "der", "Kha\u00b7lif", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "ART", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Verz\u00fcckt zur H\u00f6ll' im bangen Traume,", "tokens": ["Ver\u00b7z\u00fcckt", "zur", "H\u00f6ll'", "im", "ban\u00b7gen", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fand als sein Blick sie schnell durchlief,", "tokens": ["Fand", "als", "sein", "Blick", "sie", "schnell", "durch\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem matervollsten Raume", "tokens": ["In", "ih\u00b7rem", "ma\u00b7ter\u00b7volls\u00b7ten", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An eines K\u00f6niges die Hand", "tokens": ["An", "ei\u00b7nes", "K\u00f6\u00b7ni\u00b7ges", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von einem Derwisch fest geschlo\u00dfen,", "tokens": ["Von", "ei\u00b7nem", "Der\u00b7wisch", "fest", "ge\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und nur verwechselt ihr Gewand,", "tokens": ["Und", "nur", "ver\u00b7wech\u00b7selt", "ihr", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie gleicher H\u00f6llenpein Geno\u00dfen.", "tokens": ["Sie", "glei\u00b7cher", "H\u00f6l\u00b7len\u00b7pein", "Ge\u00b7no\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbsagt an, woher dies Urtheil,\u00ab rief", "tokens": ["\u00bb", "sagt", "an", ",", "wo\u00b7her", "dies", "Ur\u00b7theil", ",", "\u00ab", "rief"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PWAV", "PDS", "NN", "$,", "$(", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der h\u00f6chst betroffene Khalif,", "tokens": ["Der", "h\u00f6chst", "be\u00b7trof\u00b7fe\u00b7ne", "Kha\u00b7lif", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.11": {"text": "\u00bbda ihr so ganz verschieden scheinet,", "tokens": ["\u00bb", "da", "ihr", "so", "ganz", "ver\u00b7schie\u00b7den", "schei\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df gleiche Straf' euch hier vereinet?\u00ab", "tokens": ["Da\u00df", "glei\u00b7che", "Straf'", "euch", "hier", "ver\u00b7ei\u00b7net", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich hatte, sagte der Santon,", "tokens": ["Ich", "hat\u00b7te", ",", "sag\u00b7te", "der", "San\u00b7ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vom K\u00f6nigsehrgeiz wohl ein wenig.", "tokens": ["Vom", "K\u00f6\u00b7nig\u00b7sehr\u00b7geiz", "wohl", "ein", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbund ich, entgegnete der K\u00f6nig,", "tokens": ["\u00bb", "und", "ich", ",", "ent\u00b7geg\u00b7ne\u00b7te", "der", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vom Derwisch die Religion.\u00ab", "tokens": ["Vom", "Der\u00b7wisch", "die", "Re\u00b7li\u00b7gi\u00b7on", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}