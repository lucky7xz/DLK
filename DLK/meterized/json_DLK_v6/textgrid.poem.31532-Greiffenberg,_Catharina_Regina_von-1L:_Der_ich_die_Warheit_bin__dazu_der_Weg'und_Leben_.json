{"textgrid.poem.31532": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der ich die Warheit bin / dazu der Weg'und Leben /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich die Warheit bin / dazu der Weg'und Leben /", "tokens": ["Der", "ich", "die", "War\u00b7heit", "bin", "/", "da\u00b7zu", "der", "Weg'und", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "$(", "PAV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "zu und im Paradei\u00df / ich sage gn\u00e4digst dir:", "tokens": ["zu", "und", "im", "Pa\u00b7ra\u00b7dei\u00df", "/", "ich", "sa\u00b7ge", "gn\u00e4\u00b7digst", "dir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "APPRART", "NN", "$(", "PPER", "VVFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df / ob du mich und dich schon hangen siehst allhier /", "tokens": ["da\u00df", "/", "ob", "du", "mich", "und", "dich", "schon", "han\u00b7gen", "siehst", "all\u00b7hier", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "PRF", "KON", "PPER", "ADV", "VVINF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "du in demselben doch / sampt mir / heut noch solst schweben.", "tokens": ["du", "in", "dem\u00b7sel\u00b7ben", "doch", "/", "sampt", "mir", "/", "heut", "noch", "solst", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "ADV", "$(", "VVFIN", "PPER", "$(", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der / der es selber ist / kan ja das Leben geben!", "tokens": ["Der", "/", "der", "es", "sel\u00b7ber", "ist", "/", "kan", "ja", "das", "Le\u00b7ben", "ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "PPER", "ADV", "VAFIN", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "kein Gott-noch Lebens-Krafft sp\u00fcrstu zwar jetzt an mir:", "tokens": ["kein", "Got\u00b7tnoch", "Le\u00b7bens\u00b7Krafft", "sp\u00fcrs\u00b7tu", "zwar", "jetzt", "an", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "denn / als ein W\u00fcrmlein / ich erwirb die Himmels-Zier;", "tokens": ["denn", "/", "als", "ein", "W\u00fcrm\u00b7lein", "/", "ich", "er\u00b7wirb", "die", "Him\u00b7mels\u00b7Zier", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "mein tieffste Nidrigkeit kan Himmel-an erheben.", "tokens": ["mein", "tieffs\u00b7te", "Nid\u00b7rig\u00b7keit", "kan", "Him\u00b7mel\u00b7an", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer Gottes Kind / und mir ein treuer Knecht / will seyn /", "tokens": ["Wer", "Got\u00b7tes", "Kind", "/", "und", "mir", "ein", "treu\u00b7er", "Knecht", "/", "will", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "$(", "KON", "PPER", "ART", "ADJA", "NN", "$(", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der mu\u00df mein Creutz nit nur blo\u00df lieben / sondern tragen /", "tokens": ["der", "mu\u00df", "mein", "Creutz", "nit", "nur", "blo\u00df", "lie\u00b7ben", "/", "son\u00b7dern", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "VVINF", "$(", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und durch den Bach am Weg zum Himmel gehen ein.", "tokens": ["und", "durch", "den", "Bach", "am", "Weg", "zum", "Him\u00b7mel", "ge\u00b7hen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An denen hab ich nur mein Lust und wolbehagen /", "tokens": ["An", "de\u00b7nen", "hab", "ich", "nur", "mein", "Lust", "und", "wol\u00b7be\u00b7ha\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "die mir / wie du / am Creutz / auch wider allen Schein /", "tokens": ["die", "mir", "/", "wie", "du", "/", "am", "Creutz", "/", "auch", "wi\u00b7der", "al\u00b7len", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "PWAV", "PPER", "$(", "APPRART", "NN", "$(", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "vertrauen. Da\u00df du w\u00fcrdst erl\u00f6\u00df't / lie\u00df ich mich schlagen.", "tokens": ["ver\u00b7trau\u00b7en", ".", "Da\u00df", "du", "w\u00fcrdst", "er\u00b7l\u00f6\u00df't", "/", "lie\u00df", "ich", "mich", "schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$.", "KOUS", "PPER", "VAFIN", "VVPP", "$(", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}}}}