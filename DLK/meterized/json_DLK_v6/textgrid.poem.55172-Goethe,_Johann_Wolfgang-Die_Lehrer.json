{"textgrid.poem.55172": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Lehrer", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Diogenes still in seiner Tonne sich sonnte", "tokens": ["Als", "Dio\u00b7ge\u00b7nes", "still", "in", "sei\u00b7ner", "Ton\u00b7ne", "sich", "sonn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Calanus mit Lust stieg in das flammende Grab,", "tokens": ["Und", "Ca\u00b7la\u00b7nus", "mit", "Lust", "stieg", "in", "das", "flam\u00b7men\u00b7de", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Welche herrliche Lehre dem raschen Sohn des Philippus,", "tokens": ["Wel\u00b7che", "herr\u00b7li\u00b7che", "Leh\u00b7re", "dem", "ra\u00b7schen", "Sohn", "des", "Phil\u00b7ip\u00b7pus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "W\u00e4re der Herrscher der Welt nicht auch der Lehre zu gro\u00df!", "tokens": ["W\u00e4\u00b7re", "der", "Herr\u00b7scher", "der", "Welt", "nicht", "auch", "der", "Leh\u00b7re", "zu", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}