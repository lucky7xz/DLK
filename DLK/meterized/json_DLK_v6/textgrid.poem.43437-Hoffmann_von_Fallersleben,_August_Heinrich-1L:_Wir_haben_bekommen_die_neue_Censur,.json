{"textgrid.poem.43437": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir haben bekommen die neue Censur,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben bekommen die neue Censur,", "tokens": ["Wir", "ha\u00b7ben", "be\u00b7kom\u00b7men", "die", "neu\u00b7e", "Cen\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die hat eine stattliche sch\u00f6ne Frisur,", "tokens": ["Die", "hat", "ei\u00b7ne", "statt\u00b7li\u00b7che", "sch\u00f6\u00b7ne", "Fri\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ach, w\u00e4re die neue Frisur an dem Kopf", "tokens": ["Ach", ",", "w\u00e4\u00b7re", "die", "neu\u00b7e", "Fri\u00b7sur", "an", "dem", "Kopf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Nur etwas mehr als der alte Zopf!", "tokens": ["Nur", "et\u00b7was", "mehr", "als", "der", "al\u00b7te", "Zopf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Juchhe, juchhe, juchhe!", "tokens": ["Juch\u00b7he", ",", "juch\u00b7he", ",", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Dann riefen wir immer: juchhe!", "tokens": ["Dann", "rie\u00b7fen", "wir", "im\u00b7mer", ":", "juch\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die neue Frisur an unsrer Censur,", "tokens": ["Die", "neu\u00b7e", "Fri\u00b7sur", "an", "uns\u00b7rer", "Cen\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ist weiter nichts als 'ne falsche Tour,", "tokens": ["Ist", "wei\u00b7ter", "nichts", "als", "'ne", "fal\u00b7sche", "Tour", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Es zeigt sich dem Sachverst\u00e4ndigen klar,", "tokens": ["Es", "zeigt", "sich", "dem", "Sach\u00b7ver\u00b7st\u00e4n\u00b7di\u00b7gen", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es ist an ihr kein gutes Haar.", "tokens": ["Es", "ist", "an", "ihr", "kein", "gu\u00b7tes", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O weh, o weh, o weh!", "tokens": ["O", "weh", ",", "o", "weh", ",", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$,", "FM", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Drum rufen wir immer: o weh!", "tokens": ["Drum", "ru\u00b7fen", "wir", "im\u00b7mer", ":", "o", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}