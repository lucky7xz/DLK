{"textgrid.poem.39445": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Sch\u00f6pfer, der die Erde neu gestaltet,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Sch\u00f6pfer, der die Erde neu gestaltet,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", ",", "der", "die", "Er\u00b7de", "neu", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebenedeite! hast du ihr gegeben.", "tokens": ["Ge\u00b7be\u00b7ne\u00b7dei\u00b7te", "!", "hast", "du", "ihr", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du darfst dein Aug' als Anverm\u00e4hlte heben", "tokens": ["Du", "darfst", "dein", "Aug'", "als", "An\u00b7ver\u00b7m\u00e4hl\u00b7te", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Vater aller, der im Himmel waltet.", "tokens": ["Zum", "Va\u00b7ter", "al\u00b7ler", ",", "der", "im", "Him\u00b7mel", "wal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein guter Greis, des Treue nie veraltet,", "tokens": ["Ein", "gu\u00b7ter", "Greis", ",", "des", "Treu\u00b7e", "nie", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steht euer Pfleger v\u00e4terlich daneben.", "tokens": ["Steht", "eu\u00b7er", "Pfle\u00b7ger", "v\u00e4\u00b7ter\u00b7lich", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In deinem Sohne gl\u00fcht ein heilig Leben,", "tokens": ["In", "dei\u00b7nem", "Soh\u00b7ne", "gl\u00fcht", "ein", "hei\u00b7lig", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das spielend sich auf deinem Schoo\u00df entfaltet.", "tokens": ["Das", "spie\u00b7lend", "sich", "auf", "dei\u00b7nem", "Schoo\u00df", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mehr Lieb', als Kinder zu einander tragen,", "tokens": ["Mehr", "Lieb'", ",", "als", "Kin\u00b7der", "zu", "ein\u00b7an\u00b7der", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spricht des Geno\u00dfen feurige Geberde,", "tokens": ["Spricht", "des", "Ge\u00b7no\u00b7\u00dfen", "feu\u00b7ri\u00b7ge", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem Jesus zarte H\u00e4nd' entgegenbreitet.", "tokens": ["Dem", "Je\u00b7sus", "zar\u00b7te", "H\u00e4nd'", "ent\u00b7ge\u00b7gen\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der braungelockte Knabe scheint zu fragen:", "tokens": ["Der", "braun\u00b7ge\u00b7lock\u00b7te", "Kna\u00b7be", "scheint", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was thu' ich, da\u00df ich deiner w\u00fcrdig werde?", "tokens": ["Was", "thu'", "ich", ",", "da\u00df", "ich", "dei\u00b7ner", "w\u00fcr\u00b7dig", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gern sterb' ich, wenn ich dir den Weg bereitet.", "tokens": ["Gern", "sterb'", "ich", ",", "wenn", "ich", "dir", "den", "Weg", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}