{"textgrid.poem.37968": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gnadenbild Mari\u00e4-H\u00fclf bey Passau", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wohnt ein sch\u00f6nes Jungfr\u00e4ulein", "tokens": ["Es", "wohnt", "ein", "sch\u00f6\u00b7nes", "Jung\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bekleidet mit Sammt und Seiden,", "tokens": ["Be\u00b7klei\u00b7det", "mit", "Sammt", "und", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob Passau in ein Kirchel klein,", "tokens": ["Ob", "Pas\u00b7sau", "in", "ein", "Kir\u00b7chel", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer gr\u00fcnen Heiden,", "tokens": ["Auf", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dort auf dem Kapuziner-Berg,", "tokens": ["Dort", "auf", "dem", "Ka\u00b7pu\u00b7zi\u00b7ner\u00b7Berg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Gnaden sie verbleibet,", "tokens": ["In", "Gna\u00b7den", "sie", "ver\u00b7blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Zeichen und mit Wunderwerk", "tokens": ["Mit", "Zei\u00b7chen", "und", "mit", "Wun\u00b7der\u00b7werk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr meiste Zeit vertreibet.", "tokens": ["Ihr", "meis\u00b7te", "Zeit", "ver\u00b7trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus fremden Landen f\u00fchrt sie her,", "tokens": ["Aus", "frem\u00b7den", "Lan\u00b7den", "f\u00fchrt", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erzherzog Leopoldus,", "tokens": ["Erz\u00b7her\u00b7zog", "Leo\u00b7pol\u00b7dus", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ihr zu erzeigen alle Ehr,", "tokens": ["Ihr", "zu", "er\u00b7zei\u00b7gen", "al\u00b7le", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das war sein gr\u00f6ste Wollust.", "tokens": ["Das", "war", "sein", "gr\u00f6s\u00b7te", "Wol\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den sch\u00f6nen Sitz hat ihr bereit,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Sitz", "hat", "ihr", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein edler Herr von Schwendi,", "tokens": ["Ein", "ed\u00b7ler", "Herr", "von", "Schwen\u00b7di", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jezt genie\u00dft er in der Seligkeit,", "tokens": ["Jezt", "ge\u00b7nie\u00dft", "er", "in", "der", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ihr m\u00fctterliche H\u00e4nde.", "tokens": ["Ihr", "m\u00fct\u00b7ter\u00b7li\u00b7che", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf ihrem Haupt tr\u00e4gt sie ein Kron,", "tokens": ["Auf", "ih\u00b7rem", "Haupt", "tr\u00e4gt", "sie", "ein", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gold und Edelsteinen,", "tokens": ["Von", "Gold", "und", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Silber ist gemacht ihr Thron,", "tokens": ["Von", "Sil\u00b7ber", "ist", "ge\u00b7macht", "ihr", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dem thut sie erscheinen,", "tokens": ["Auf", "dem", "thut", "sie", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Jesus der wahre Gottes Sohn,", "tokens": ["Je\u00b7sus", "der", "wah\u00b7re", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihren Armen wohnet,", "tokens": ["In", "ih\u00b7ren", "Ar\u00b7men", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Seel, die ihm und ihr thut sch\u00f6n,", "tokens": ["Die", "Seel", ",", "die", "ihm", "und", "ihr", "thut", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bleibt wohl nicht unbelohnet.", "tokens": ["Bleibt", "wohl", "nicht", "un\u00b7be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "An ihr ist nichts denn Heiligkeit,", "tokens": ["An", "ihr", "ist", "nichts", "denn", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und majest\u00e4tisch Leben,", "tokens": ["Und", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ganz englisch ist ihr Reinigkeit,", "tokens": ["Ganz", "eng\u00b7lisch", "ist", "ihr", "Rei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem\u00fcthig doch darneben,", "tokens": ["De\u00b7m\u00fct\u00b7hig", "doch", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PAV", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Ihr Ursprung ist sehr adelich,", "tokens": ["Ihr", "Ur\u00b7sprung", "ist", "sehr", "a\u00b7de\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von k\u00f6niglichem Stamme,", "tokens": ["Von", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich darf sie nennen \u00f6ffentlich,", "tokens": ["Ich", "darf", "sie", "nen\u00b7nen", "\u00f6f\u00b7fent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Maria hei\u00dft ihr Namen.", "tokens": ["Ma\u00b7ria", "hei\u00dft", "ihr", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Vor ihr die Engel neigen sich,", "tokens": ["Vor", "ihr", "die", "En\u00b7gel", "nei\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil sie Gott selber ehret,", "tokens": ["Weil", "sie", "Gott", "sel\u00b7ber", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dienstwillig sie erzeigen sich,", "tokens": ["Dienst\u00b7wil\u00b7lig", "sie", "er\u00b7zei\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald sies nur begehret,", "tokens": ["So\u00b7bald", "sies", "nur", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Kaiser beugen ihre Knie,", "tokens": ["Die", "Kai\u00b7ser", "beu\u00b7gen", "ih\u00b7re", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die K\u00f6nig sie sch\u00f6n gr\u00fc\u00dfen,", "tokens": ["Die", "K\u00f6\u00b7nig", "sie", "sch\u00f6n", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrsten und Herrn r\u00fchmen sie,", "tokens": ["F\u00fcrs\u00b7ten", "und", "Herrn", "r\u00fch\u00b7men", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und fallen ihr zu F\u00fc\u00dfen.", "tokens": ["Und", "fal\u00b7len", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es stehn vor ihrem Angesicht,", "tokens": ["Es", "stehn", "vor", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel tapfre Edelknaben,", "tokens": ["Viel", "tapf\u00b7re", "E\u00b7del\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu ihrem Dienst dahin gericht,", "tokens": ["Zu", "ih\u00b7rem", "Dienst", "da\u00b7hin", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schild in H\u00e4nden haben.", "tokens": ["Die", "Schild", "in", "H\u00e4n\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Engel stehen ihr so nah,", "tokens": ["Wie", "En\u00b7gel", "ste\u00b7hen", "ihr", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Abla\u00df und die Gnade,", "tokens": ["Der", "Ab\u00b7la\u00df", "und", "die", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die gr\u00fc\u00dfen uns von Ferne da,", "tokens": ["Die", "gr\u00fc\u00b7\u00dfen", "uns", "von", "Fer\u00b7ne", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hin zu ihr uns laden.", "tokens": ["Und", "hin", "zu", "ihr", "uns", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit vielen zarten Bl\u00fcmelein,", "tokens": ["Mit", "vie\u00b7len", "zar\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist sie gar fein umstecket,", "tokens": ["Ist", "sie", "gar", "fein", "um\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit N\u00e4geln und mit R\u00f6selein", "tokens": ["Mit", "N\u00e4\u00b7geln", "und", "mit", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ihr Altar bedecket,", "tokens": ["Wird", "ihr", "Al\u00b7tar", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Davon das ganze Kirchel schier", "tokens": ["Da\u00b7von", "das", "gan\u00b7ze", "Kir\u00b7chel", "schier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ueberaus lieblich schmecket,", "tokens": ["Ue\u00b7be\u00b7raus", "lieb\u00b7lich", "schme\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Damit das Volk durch solche Zier", "tokens": ["Da\u00b7mit", "das", "Volk", "durch", "sol\u00b7che", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Andacht werd erwecket.", "tokens": ["Zur", "An\u00b7dacht", "werd", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Oft Musikklang und Orgelspiel", "tokens": ["Oft", "Mu\u00b7sik\u00b7klang", "und", "Or\u00b7gel\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut man da bey ihr h\u00f6ren,", "tokens": ["Thut", "man", "da", "bey", "ihr", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aemter und Litaneien viel,", "tokens": ["A\u00b7em\u00b7ter", "und", "Li\u00b7ta\u00b7nei\u00b7en", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Haltet man ihr zu Ehren,", "tokens": ["Hal\u00b7tet", "man", "ihr", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.