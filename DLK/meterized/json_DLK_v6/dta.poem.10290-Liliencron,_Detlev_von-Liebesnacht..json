{"dta.poem.10290": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Liebesnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Nun l\u00f6s' ich sanft die lieben H&#228;nde,             ", "tokens": ["Nun", "l\u00f6s'", "ich", "sanft", "die", "lie\u00b7ben", "H", "&#228;", "nde", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "XY", "$(", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die du mir um den Hals gelegt.", "tokens": ["Die", "du", "mir", "um", "den", "Hals", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich in deinen Augen finde,", "tokens": ["Da\u00df", "ich", "in", "dei\u00b7nen", "Au\u00b7gen", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dir das kleine Herz bewegt.", "tokens": ["Was", "dir", "das", "klei\u00b7ne", "Herz", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O sieh die Nacht, die wundervolle,", "tokens": ["O", "sieh", "die", "Nacht", ",", "die", "wun\u00b7der\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ferne L\u00e4nder zog der Tag.", "tokens": ["In", "fer\u00b7ne", "L\u00e4n\u00b7der", "zog", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Birke Zischellaub verstummte,", "tokens": ["Der", "Bir\u00b7ke", "Zi\u00b7schel\u00b7laub", "ver\u00b7stumm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rst du den Nachtigallenschlag?", "tokens": ["H\u00f6rst", "du", "den", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der wei\u00dfe Schlehdorn uns zu H\u00e4upten,", "tokens": ["Der", "wei\u00b7\u00dfe", "Schleh\u00b7dorn", "uns", "zu", "H\u00e4up\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist die liebste Bl\u00fcte mir.", "tokens": ["Es", "ist", "die", "liebs\u00b7te", "Bl\u00fc\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trenn\u2019 ab ein Zweiglein eh\u2019 wir scheiden,", "tokens": ["Trenn'", "ab", "ein", "Zwei\u00b7glein", "eh'", "wir", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dein\u2019 und meines Hutes Zier.", "tokens": ["Zu", "dein'", "und", "mei\u00b7nes", "Hu\u00b7tes", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df, M\u00e4dchen, uns die Nacht genie\u00dfen,", "tokens": ["La\u00df", ",", "M\u00e4d\u00b7chen", ",", "uns", "die", "Nacht", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein geh\u00f6rt sie mir und dir.", "tokens": ["Al\u00b7lein", "ge\u00b7h\u00f6rt", "sie", "mir", "und", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00fcte will ich aufbewahren", "tokens": ["Die", "Bl\u00fc\u00b7te", "will", "ich", "auf\u00b7be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An diese Fr\u00fchlingsstunde hier.", "tokens": ["An", "die\u00b7se", "Fr\u00fch\u00b7lings\u00b7stun\u00b7de", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}