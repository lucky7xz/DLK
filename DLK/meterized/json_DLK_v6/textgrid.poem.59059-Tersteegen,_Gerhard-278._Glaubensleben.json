{"textgrid.poem.59059": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "278. Glaubensleben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott schaun, verehren und umfassen,", "tokens": ["Gott", "schaun", ",", "ver\u00b7eh\u00b7ren", "und", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "KON", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht fern, nicht fremd, nicht zweifelhaft", "tokens": ["Nicht", "fern", ",", "nicht", "fremd", ",", "nicht", "zwei\u00b7fel\u00b7haft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihm sich innigst \u00fcberlassen:", "tokens": ["Und", "ihm", "sich", "in\u00b7nigst", "\u00fc\u00b7ber\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies ist des Glaubens edle Kraft.", "tokens": ["Dies", "ist", "des", "Glau\u00b7bens", "ed\u00b7le", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}