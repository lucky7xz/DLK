{"dta.poem.18765": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter und Timur .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "So umgab sie nun der Winter", "tokens": ["So", "um\u00b7gab", "sie", "nun", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit gewalt\u2019gem Grimme. Streuend", "tokens": ["Mit", "ge\u00b7walt'\u00b7gem", "Grim\u00b7me", ".", "Streu\u00b7end"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Eishauch zwischen alle,", "tokens": ["Sei\u00b7nen", "Eis\u00b7hauch", "zwi\u00b7schen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hetzt er die verschiednen Winde", "tokens": ["Hetzt", "er", "die", "ver\u00b7schied\u00b7nen", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerw\u00e4rtig auf sie ein.", "tokens": ["Wi\u00b7der\u00b7w\u00e4r\u00b7tig", "auf", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber sie gab er Gewaltkraft", "tokens": ["Ue\u00b7ber", "sie", "gab", "er", "Ge\u00b7walt\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Seinen frostgespitzten St\u00fcrmen,", "tokens": ["Sei\u00b7nen", "frost\u00b7ge\u00b7spitz\u00b7ten", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stieg in Timurs Rath hernieder,", "tokens": ["Stieg", "in", "Ti\u00b7murs", "Rath", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schrie ihn drohend an und sprach so:", "tokens": ["Schrie", "ihn", "dro\u00b7hend", "an", "und", "sprach", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Leise, langsam, Ungl\u00fccksel\u2019ger!", "tokens": ["Lei\u00b7se", ",", "lang\u00b7sam", ",", "Un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wandle du Tyrann des Unrechts;", "tokens": ["Wand\u00b7le", "du", "Ty\u00b7rann", "des", "Un\u00b7rechts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sollen l\u00e4nger noch die Herzen", "tokens": ["Sol\u00b7len", "l\u00e4n\u00b7ger", "noch", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sengen, brennen deinen Flammen?", "tokens": ["Sen\u00b7gen", ",", "bren\u00b7nen", "dei\u00b7nen", "Flam\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Bist du der verdammten Geister", "tokens": ["Bist", "du", "der", "ver\u00b7damm\u00b7ten", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Einer, wohl! ich bin der andre.", "tokens": ["Ei\u00b7ner", ",", "wohl", "!", "ich", "bin", "der", "and\u00b7re", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "$.", "PPER", "VAFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Du bist Greis, ich auch, erstarren", "tokens": ["Du", "bist", "Greis", ",", "ich", "auch", ",", "er\u00b7star\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "PPER", "ADV", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Machen wir so Land als Menschen.", "tokens": ["Ma\u00b7chen", "wir", "so", "Land", "als", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mars! Du bist\u2019s! ich bin Saturnus,", "tokens": ["Mars", "!", "Du", "bist's", "!", "ich", "bin", "Sa\u00b7tur\u00b7nus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "NE", "$.", "PPER", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Uebelth\u00e4tige Gestirne,", "tokens": ["Ue\u00b7belt\u00b7h\u00e4\u00b7ti\u00b7ge", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Im Verein die Schrecklichsten.", "tokens": ["Im", "Ver\u00b7ein", "die", "Schreck\u00b7lichs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.21": {"text": "T\u00f6dest du die Seele, k\u00e4ltest", "tokens": ["T\u00f6\u00b7dest", "du", "die", "See\u00b7le", ",", "k\u00e4l\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Du den Luftkreis; meine L\u00fcfte", "tokens": ["Du", "den", "Luft\u00b7kreis", ";", "mei\u00b7ne", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Sind noch k\u00e4lter als du seyn kannst.", "tokens": ["Sind", "noch", "k\u00e4l\u00b7ter", "als", "du", "seyn", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "VAINF", "VMFIN", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.24": {"text": "Qu\u00e4len deine wilden Heere", "tokens": ["Qu\u00e4\u00b7len", "dei\u00b7ne", "wil\u00b7den", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Gl\u00e4ubige mit tausend Martern;", "tokens": ["Gl\u00e4u\u00b7bi\u00b7ge", "mit", "tau\u00b7send", "Mar\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wohl, in meinen Tagen soll sich,", "tokens": ["Wohl", ",", "in", "mei\u00b7nen", "Ta\u00b7gen", "soll", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "VMFIN", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Geb es Gott! was schlimmres finden.", "tokens": ["Geb", "es", "Gott", "!", "was", "schlimm\u00b7res", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "PWS", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und bey Gott! Dir schenk\u2019 ich nichts", "tokens": ["Und", "bey", "Gott", "!", "Dir", "schenk'", "ich", "nichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "H\u00f6r\u2019 es Gott was ich dir biete!", "tokens": ["H\u00f6r'", "es", "Gott", "was", "ich", "dir", "bie\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ja bey Gott! von Todesk\u00e4lte", "tokens": ["Ja", "bey", "Gott", "!", "von", "To\u00b7des\u00b7k\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "APPR", "NN", "$.", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Nicht, o Greis, vertheid\u2019gen soll dich", "tokens": ["Nicht", ",", "o", "Greis", ",", "vert\u00b7hei\u00b7d'\u00b7gen", "soll", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "$,", "FM", "NN", "$,", "VVINF", "VMFIN", "PPER"], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Breite Kohlenglut vom Heerde,", "tokens": ["Brei\u00b7te", "Koh\u00b7len\u00b7glut", "vom", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Keine Flamme des Decembers.", "tokens": ["Kei\u00b7ne", "Flam\u00b7me", "des", "De\u00b7cem\u00b7bers", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}