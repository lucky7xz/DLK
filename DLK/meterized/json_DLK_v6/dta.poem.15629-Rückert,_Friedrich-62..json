{"dta.poem.15629": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol wird aus Ja und Ja sich nie ein Nein ergeben,", "tokens": ["Wol", "wird", "aus", "Ja", "und", "Ja", "sich", "nie", "ein", "Nein", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN", "PRF", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch dienet Nein und Nein einander aufzuheben.", "tokens": ["Doch", "die\u00b7net", "Nein", "und", "Nein", "ein\u00b7an\u00b7der", "auf\u00b7zu\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKANT", "KON", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Um Sprach' und Rechenkunst hat es ein gleich Bewendni\u00df,", "tokens": ["Um", "Sprach'", "und", "Re\u00b7chen\u00b7kunst", "hat", "es", "ein", "gleich", "Be\u00b7wend\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VAFIN", "PPER", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und kein ungleiches auch um Welt und Weltverst\u00e4ndni\u00df.", "tokens": ["Und", "kein", "un\u00b7glei\u00b7ches", "auch", "um", "Welt", "und", "Welt\u00b7ver\u00b7st\u00e4nd\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das B\u00f6se ist nur da, das Gute zu erproben;", "tokens": ["Das", "B\u00f6\u00b7se", "ist", "nur", "da", ",", "das", "Gu\u00b7te", "zu", "er\u00b7pro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dis bleibt, und jenes hat sich selber aufgehoben.", "tokens": ["Dis", "bleibt", ",", "und", "je\u00b7nes", "hat", "sich", "sel\u00b7ber", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PDS", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}