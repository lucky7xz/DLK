{"textgrid.poem.46756": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich geh' in Hain und Flur]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geh' in Hain und Flur", "tokens": ["Ich", "geh'", "in", "Hain", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nie ohne deine Spur;", "tokens": ["Nie", "oh\u00b7ne", "dei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn immer mu\u00df ich denken,", "tokens": ["Denn", "im\u00b7mer", "mu\u00df", "ich", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohin ich nur mag lenken,", "tokens": ["Wo\u00b7hin", "ich", "nur", "mag", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Die Augen und den Schritt:", "tokens": ["Die", "Au\u00b7gen", "und", "den", "Schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hier gingest du einst mit.", "tokens": ["Hier", "gin\u00b7gest", "du", "einst", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und werd' ich hingelangen,", "tokens": ["Und", "werd'", "ich", "hin\u00b7ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo du nie mitgegangen,", "tokens": ["Wo", "du", "nie", "mit\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So denk' ich wieder: hier", "tokens": ["So", "denk'", "ich", "wie\u00b7der", ":", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gingst du noch nie mit mir;", "tokens": ["Gingst", "du", "noch", "nie", "mit", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O k\u00f6nnt' ich mit dem neuen", "tokens": ["O", "k\u00f6nnt'", "ich", "mit", "dem", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Spaziergang dich erfreuen!", "tokens": ["Spa\u00b7zier\u00b7gang", "dich", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}