{"textgrid.poem.41487": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Ursache der Kriege", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein! sage mir, warum die F\u00fcrsten fechten?", "tokens": ["Mein", "!", "sa\u00b7ge", "mir", ",", "wa\u00b7rum", "die", "F\u00fcrs\u00b7ten", "fech\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fragt G\u00f6rgel den Gevatter Hein.", "tokens": ["Fragt", "G\u00f6r\u00b7gel", "den", "Ge\u00b7vat\u00b7ter", "Hein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der lacht und spricht: Wenn sie, wie wir, ged\u00e4chten;", "tokens": ["Der", "lacht", "und", "spricht", ":", "Wenn", "sie", ",", "wie", "wir", ",", "ge\u00b7d\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie stellten alle H\u00e4ndel ein.", "tokens": ["Sie", "stell\u00b7ten", "al\u00b7le", "H\u00e4n\u00b7del", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sie, wie wir, nur oft zusammen zechten,", "tokens": ["Wenn", "sie", ",", "wie", "wir", ",", "nur", "oft", "zu\u00b7sam\u00b7men", "zech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie w\u00fcrden Freund und Br\u00fcder sein.", "tokens": ["Sie", "w\u00fcr\u00b7den", "Freund", "und", "Br\u00fc\u00b7der", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}