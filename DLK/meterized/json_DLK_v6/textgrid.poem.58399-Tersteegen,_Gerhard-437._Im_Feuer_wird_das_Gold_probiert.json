{"textgrid.poem.58399": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "437. Im Feuer wird das Gold probiert", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man meint, man liebe Gott, und ist noch nicht probieret,", "tokens": ["Man", "meint", ",", "man", "lie\u00b7be", "Gott", ",", "und", "ist", "noch", "nicht", "pro\u00b7bie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "NN", "$,", "KON", "VAFIN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man schmecket dies und das und denkt, man liebe wohl;", "tokens": ["Man", "schme\u00b7cket", "dies", "und", "das", "und", "denkt", ",", "man", "lie\u00b7be", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "KON", "PDS", "KON", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer weder Gnad noch Trost, noch Gott, noch Gaben sp\u00fcret", "tokens": ["Wer", "we\u00b7der", "Gnad", "noch", "Trost", ",", "noch", "Gott", ",", "noch", "Ga\u00b7ben", "sp\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "KON", "NN", "ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und doch noch lieben kann, der liebet, wie er soll.", "tokens": ["Und", "doch", "noch", "lie\u00b7ben", "kann", ",", "der", "lie\u00b7bet", ",", "wie", "er", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}