{"textgrid.poem.65089": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die wei\u00dfen Wellenh\u00e4upter funkeln", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die wei\u00dfen Wellenh\u00e4upter funkeln", "tokens": ["Die", "wei\u00b7\u00dfen", "Wel\u00b7len\u00b7h\u00e4up\u00b7ter", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sonnenuntergang \u2013", "tokens": ["Im", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meiner Seele auch will's dunkeln,", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "auch", "will's", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz ist schwer und bang!", "tokens": ["Mein", "Herz", "ist", "schwer", "und", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es t\u00f6nt und rauschet aus der Tiefe", "tokens": ["Es", "t\u00f6nt", "und", "rau\u00b7schet", "aus", "der", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlockender Gesang,", "tokens": ["Ver\u00b7lo\u00b7cken\u00b7der", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob es mich herunter riefe \u2013", "tokens": ["Als", "ob", "es", "mich", "her\u00b7un\u00b7ter", "rie\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APZR", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz ist schwer und bang!", "tokens": ["Mein", "Herz", "ist", "schwer", "und", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von d\u00fcstern Sorgen, die es pressen,", "tokens": ["Von", "d\u00fcs\u00b7tern", "Sor\u00b7gen", ",", "die", "es", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist es so schwer und bang!", "tokens": ["Ist", "es", "so", "schwer", "und", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O nimmer werd' ich dich vergessen,", "tokens": ["O", "nim\u00b7mer", "werd'", "ich", "dich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hier mir's nicht gelang!", "tokens": ["Da", "hier", "mir's", "nicht", "ge\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}