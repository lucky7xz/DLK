{"textgrid.poem.31696": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo in den Buchen s\u00e4uselt der Wind", "tokens": ["Wo", "in", "den", "Bu\u00b7chen", "s\u00e4u\u00b7selt", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hoch auf den sieben Bergen hie,", "tokens": ["Hoch", "auf", "den", "sie\u00b7ben", "Ber\u00b7gen", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wohnte das feine Bauernkind,", "tokens": ["Da", "wohn\u00b7te", "das", "fei\u00b7ne", "Bau\u00b7ern\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die sch\u00f6ne Mimilie.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mi\u00b7mi\u00b7lie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da tanzte sie durch den lichten Wald", "tokens": ["Da", "tanz\u00b7te", "sie", "durch", "den", "lich\u00b7ten", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und lie\u00df die braunen Haare walln,", "tokens": ["Und", "lie\u00df", "die", "brau\u00b7nen", "Haa\u00b7re", "walln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sechzehn Jahre wurde sie alt", "tokens": ["Und", "sech\u00b7zehn", "Jah\u00b7re", "wur\u00b7de", "sie", "alt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wohl unter Rosen und Nachtigalln.", "tokens": ["Wohl", "un\u00b7ter", "Ro\u00b7sen", "und", "Nach\u00b7ti\u00b7galln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie blickte keck in die Wolken hinein,", "tokens": ["Sie", "blick\u00b7te", "keck", "in", "die", "Wol\u00b7ken", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da jauchzten die Falken mit wildem Schrei!", "tokens": ["Da", "jauchz\u00b7ten", "die", "Fal\u00b7ken", "mit", "wil\u00b7dem", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie blickte hinab in den gr\u00fcnen Rhein:", "tokens": ["Sie", "blick\u00b7te", "hin\u00b7ab", "in", "den", "gr\u00fc\u00b7nen", "Rhein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und stolzer rollten die Wogen vorbei!", "tokens": ["Und", "stol\u00b7zer", "roll\u00b7ten", "die", "Wo\u00b7gen", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und stolz auf die rheinische Dirne sah", "tokens": ["Und", "stolz", "auf", "die", "rhei\u00b7ni\u00b7sche", "Dir\u00b7ne", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Winzer im Feld und der Ferge im Kahn,", "tokens": ["Der", "Win\u00b7zer", "im", "Feld", "und", "der", "Fer\u00b7ge", "im", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und von K\u00f6nigswinter bis Honnef, da", "tokens": ["Und", "von", "K\u00f6\u00b7nigs\u00b7win\u00b7ter", "bis", "Hon\u00b7nef", ",", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "$,", "KOUS"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat manch armen Jungen sie's angetan.", "tokens": ["Hat", "manch", "ar\u00b7men", "Jun\u00b7gen", "sie's", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "NE", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und die Kunde drang durch das ganze Land,", "tokens": ["Und", "die", "Kun\u00b7de", "drang", "durch", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und jeder wollte die Sch\u00f6ne sehn.", "tokens": ["Und", "je\u00b7der", "woll\u00b7te", "die", "Sch\u00f6\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es lie\u00df der Student den staub'gen Foliant", "tokens": ["Es", "lie\u00df", "der", "Stu\u00b7dent", "den", "staub'\u00b7gen", "Fo\u00b7li\u00b7ant"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und kam und konnte nicht widerstehn", "tokens": ["Und", "kam", "und", "konn\u00b7te", "nicht", "wi\u00b7der\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und verga\u00df den Horaz und den alten Homer", "tokens": ["Und", "ver\u00b7ga\u00df", "den", "Ho\u00b7raz", "und", "den", "al\u00b7ten", "Ho\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NE"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und dachte an sie nur und nur an sie! \u2013", "tokens": ["Und", "dach\u00b7te", "an", "sie", "nur", "und", "nur", "an", "sie", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "KON", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und zog durch die sieben Berge daher", "tokens": ["Und", "zog", "durch", "die", "sie\u00b7ben", "Ber\u00b7ge", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "CARD", "NN", "PAV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lobte die sch\u00f6ne Mimilie. \u2013", "tokens": ["Und", "lob\u00b7te", "die", "sch\u00f6\u00b7ne", "Mi\u00b7mi\u00b7lie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "O Lust, o Liebe im frohen Mai,", "tokens": ["O", "Lust", ",", "o", "Lie\u00b7be", "im", "fro\u00b7hen", "Mai", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ist so schnell dein Zauber verbl\u00fcht!", "tokens": ["Wie", "ist", "so", "schnell", "dein", "Zau\u00b7ber", "ver\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es hallten die Berge von Wehgeschrei,", "tokens": ["Es", "hall\u00b7ten", "die", "Ber\u00b7ge", "von", "Weh\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als ach die sch\u00f6nste der Rosen schied.", "tokens": ["Als", "ach", "die", "sch\u00f6ns\u00b7te", "der", "Ro\u00b7sen", "schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Da h\u00f6rte der Falke zu jauchzen auf,", "tokens": ["Da", "h\u00f6r\u00b7te", "der", "Fal\u00b7ke", "zu", "jauch\u00b7zen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die Blumen starben entbl\u00e4ttert all,", "tokens": ["Und", "die", "Blu\u00b7men", "star\u00b7ben", "ent\u00b7bl\u00e4t\u00b7tert", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN", "PIAT", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dumpf brauste der Rhein den alten Lauf,", "tokens": ["Dumpf", "braus\u00b7te", "der", "Rhein", "den", "al\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und es schwieg im Walde die Nachtigall.", "tokens": ["Und", "es", "schwieg", "im", "Wal\u00b7de", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und der Winzer sah ernst in die Nacht hinaus,", "tokens": ["Und", "der", "Win\u00b7zer", "sah", "ernst", "in", "die", "Nacht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und es sanken dem Fergen die guten H\u00e4nd,", "tokens": ["Und", "es", "san\u00b7ken", "dem", "Fer\u00b7gen", "die", "gu\u00b7ten", "H\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und zu Bonn im hochgegiebelten Haus", "tokens": ["Und", "zu", "Bonn", "im", "hoch\u00b7ge\u00b7gie\u00b7bel\u00b7ten", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "APPRART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sa\u00df traurig wieder manch treuer Student.", "tokens": ["Sa\u00df", "trau\u00b7rig", "wie\u00b7der", "manch", "treu\u00b7er", "Stu\u00b7dent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}