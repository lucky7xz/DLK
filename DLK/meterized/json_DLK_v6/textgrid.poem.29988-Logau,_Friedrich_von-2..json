{"textgrid.poem.29988": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist alten Leuten gram und ehrt sie kaum mit einem Blicke,", "tokens": ["Die", "Welt", "ist", "al\u00b7ten", "Leu\u00b7ten", "gram", "und", "ehrt", "sie", "kaum", "mit", "ei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "ADJD", "KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Das macht: die Alten kummen drauff und weisen andren ihre T\u00fccke.", "tokens": ["Das", "macht", ":", "die", "Al\u00b7ten", "kum\u00b7men", "drauff", "und", "wei\u00b7sen", "an\u00b7dren", "ih\u00b7re", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}