{"textgrid.poem.37871": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Amor", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Nachts da bin ich gekommen,", "tokens": ["Des", "Nachts", "da", "bin", "ich", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Treibt mit mir ein B\u00fcbchen viel Scherz,", "tokens": ["Treibt", "mit", "mir", "ein", "B\u00fcb\u00b7chen", "viel", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie Amor mir ists vorgekommen,", "tokens": ["Wie", "A\u00b7mor", "mir", "ists", "vor\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwundet, verbindet mein Herz.", "tokens": ["Ver\u00b7wun\u00b7det", ",", "ver\u00b7bin\u00b7det", "mein", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich dacht, was sollt ich nun machen,", "tokens": ["Ich", "dacht", ",", "was", "sollt", "ich", "nun", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn ich mein klein B\u00fcbchen gedenk,", "tokens": ["Wenn", "ich", "mein", "klein", "B\u00fcb\u00b7chen", "ge\u00b7denk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So h\u00f6r ich die Flamme schon krachen,", "tokens": ["So", "h\u00f6r", "ich", "die", "Flam\u00b7me", "schon", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schier alle Minuten ihm schenk.", "tokens": ["Schier", "al\u00b7le", "Mi\u00b7nu\u00b7ten", "ihm", "schenk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ich kann es bei Tage nicht finden,", "tokens": ["Ich", "kann", "es", "bei", "Ta\u00b7ge", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Nachts da sucht es mich heim,", "tokens": ["Des", "Nachts", "da", "sucht", "es", "mich", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich will ihm die Augen verbinden,", "tokens": ["Ich", "will", "ihm", "die", "Au\u00b7gen", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann wird es bei Tage auch mein.", "tokens": ["Dann", "wird", "es", "bei", "Ta\u00b7ge", "auch", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "ADV", "PPOSAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}