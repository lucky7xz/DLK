{"textgrid.poem.44513": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die T\u00e4nzerin", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob Natur dich so beg\u00fcnstigt,", "tokens": ["Ob", "Na\u00b7tur", "dich", "so", "be\u00b7g\u00fcns\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob die Kunst dich so gelehrt?", "tokens": ["Ob", "die", "Kunst", "dich", "so", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberfl\u00fcssig scheint die Frage,", "tokens": ["\u00dc\u00b7berf\u00b7l\u00fcs\u00b7sig", "scheint", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Antwort nimmer wert.", "tokens": ["Und", "der", "Ant\u00b7wort", "nim\u00b7mer", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn Natur ist blind geboren,", "tokens": ["Denn", "Na\u00b7tur", "ist", "blind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kunst f\u00fcr sich, nicht blind, doch lahm,", "tokens": ["Kunst", "f\u00fcr", "sich", ",", "nicht", "blind", ",", "doch", "lahm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$,", "PTKNEG", "ADJD", "$,", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beide ganz erst, wenn das Auge", "tokens": ["Bei\u00b7de", "ganz", "erst", ",", "wenn", "das", "Au\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich den Fu\u00df zur St\u00fctze nahm.", "tokens": ["Sich", "den", "Fu\u00df", "zur", "St\u00fct\u00b7ze", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darum schwing die leichten Fl\u00fcgel,", "tokens": ["Da\u00b7rum", "schwing", "die", "leich\u00b7ten", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wieg des K\u00f6rpers sch\u00f6ne Last!", "tokens": ["Wieg", "des", "K\u00f6r\u00b7pers", "sch\u00f6\u00b7ne", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was auch soll dir unsre Meinung,", "tokens": ["Was", "auch", "soll", "dir", "uns\u00b7re", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du unsre Herzen hast?", "tokens": ["Wenn", "du", "uns\u00b7re", "Her\u00b7zen", "hast", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}