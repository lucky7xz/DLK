{"textgrid.poem.53530": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Achtundvierzig", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siebzig Jahre ist das nun her.", "tokens": ["Sieb\u00b7zig", "Jah\u00b7re", "ist", "das", "nun", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PDS", "ADV", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Siebzig Jahre wiegen so schwer.", "tokens": ["Sieb\u00b7zig", "Jah\u00b7re", "wie\u00b7gen", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schwarz-rot-goldene Fahnen flatterten,", "tokens": ["Schwa\u00b7rz\u00b7rot\u00b7gol\u00b7de\u00b7ne", "Fah\u00b7nen", "flat\u00b7ter\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-++--+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Vater Wrangels Musketen knatterten \u2013", "tokens": ["Va\u00b7ter", "Wran\u00b7gels", "Mus\u00b7ke\u00b7ten", "knat\u00b7ter\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVFIN", "$("], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wof\u00fcr?", "tokens": ["Wo\u00b7f\u00fcr", "?"], "token_info": ["word", "punct"], "pos": ["PWAV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Wie gl\u00fchten die Herzen! wie gl\u00fchten die K\u00f6pfe!", "tokens": ["Wie", "gl\u00fch\u00b7ten", "die", "Her\u00b7zen", "!", "wie", "gl\u00fch\u00b7ten", "die", "K\u00f6p\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kampf! Kampf gegen die B\u00fcrgertr\u00f6pfe,", "tokens": ["Kampf", "!", "Kampf", "ge\u00b7gen", "die", "B\u00fcr\u00b7ger\u00b7tr\u00f6p\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "gegen die nickenden Zipfelm\u00fctzen \u2013", "tokens": ["ge\u00b7gen", "die", "ni\u00b7cken\u00b7den", "Zip\u00b7fel\u00b7m\u00fct\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Klatschen in tr\u00fcbe F\u00fcrstenpf\u00fctzen \u2013", "tokens": ["Klat\u00b7schen", "in", "tr\u00fc\u00b7be", "F\u00fcrs\u00b7ten\u00b7pf\u00fct\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und dann?", "tokens": ["Und", "dann", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Der gro\u00dfe Sieg in den siebziger Jahren", "tokens": ["Der", "gro\u00b7\u00dfe", "Sieg", "in", "den", "sieb\u00b7zi\u00b7ger", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ist uns verdammt in die Krone gefahren.", "tokens": ["ist", "uns", "ver\u00b7dammt", "in", "die", "Kro\u00b7ne", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Die Krone glei\u00dfte. Die B\u00fcrger krochen.", "tokens": ["Die", "Kro\u00b7ne", "glei\u00df\u00b7te", ".", "Die", "B\u00fcr\u00b7ger", "kro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die treusten deutschen Herzen pochen", "tokens": ["Die", "treus\u00b7ten", "deut\u00b7schen", "Her\u00b7zen", "po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "im Proletariat.", "tokens": ["im", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7at", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und dann? Die versprochenen herrlichen Zeiten!", "tokens": ["Und", "dann", "?", "Die", "ver\u00b7spro\u00b7che\u00b7nen", "herr\u00b7li\u00b7chen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und dann? Wir wollen gen Frankreich reiten!", "tokens": ["Und", "dann", "?", "Wir", "wol\u00b7len", "gen", "Fran\u00b7kreich", "rei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VMFIN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dann? Wir k\u00e4mpfen gegen zwei Welten,", "tokens": ["Und", "dann", "?", "Wir", "k\u00e4mp\u00b7fen", "ge\u00b7gen", "zwei", "Wel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herz und Hirn haben den Deubel zu gelten \u2013", "tokens": ["Herz", "und", "Hirn", "ha\u00b7ben", "den", "Deu\u00b7bel", "zu", "gel\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Jetzt sitzt er in Holland.", "tokens": ["Jetzt", "sitzt", "er", "in", "Hol\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wof\u00fcr, mein Gott, hat die Freiheit geblutet?", "tokens": ["Wo\u00b7f\u00fcr", ",", "mein", "Gott", ",", "hat", "die", "Frei\u00b7heit", "ge\u00b7blu\u00b7tet", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wof\u00fcr wurden M\u00e4nner und M\u00e4dchen geknutet?", "tokens": ["Wo\u00b7f\u00fcr", "wur\u00b7den", "M\u00e4n\u00b7ner", "und", "M\u00e4d\u00b7chen", "ge\u00b7knu\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Spartakus! Deutsche! So \u00f6ffnet die Augen!", "tokens": ["Spar\u00b7ta\u00b7kus", "!", "Deut\u00b7sche", "!", "So", "\u00f6ff\u00b7net", "die", "Au\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sie warten, euch Blut aus den Adern zu saugen \u2013", "tokens": ["Sie", "war\u00b7ten", ",", "euch", "Blut", "aus", "den", "A\u00b7dern", "zu", "sau\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Feind steht rechts!", "tokens": ["Der", "Feind", "steht", "rechts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Zerfleischt euch nicht das eigene Herz!", "tokens": ["Zer\u00b7fleischt", "euch", "nicht", "das", "ei\u00b7ge\u00b7ne", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Denkt an die Barrikaden im M\u00e4rz \u2013!", "tokens": ["Denkt", "an", "die", "Bar\u00b7ri\u00b7ka\u00b7den", "im", "M\u00e4rz", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$(", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wir litten so viel.", "tokens": ["Wir", "lit\u00b7ten", "so", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wollen wir nicht endlich Weltb\u00fcrger werden?", "tokens": ["Wol\u00b7len", "wir", "nicht", "end\u00b7lich", "Welt\u00b7b\u00fcr\u00b7ger", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Wir haben nur einen Feind auf Erden:", "tokens": ["Wir", "ha\u00b7ben", "nur", "ei\u00b7nen", "Feind", "auf", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "den deutschen Schlemihl!", "tokens": ["den", "deut\u00b7schen", "Schle\u00b7mihl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}