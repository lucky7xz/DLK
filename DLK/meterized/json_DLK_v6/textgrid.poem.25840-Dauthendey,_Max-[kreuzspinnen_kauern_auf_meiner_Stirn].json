{"textgrid.poem.25840": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[kreuzspinnen kauern auf meiner Stirn]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kreuzspinnen kauern auf meiner Stirn", "tokens": ["Kreuz\u00b7spin\u00b7nen", "kau\u00b7ern", "auf", "mei\u00b7ner", "Stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lauern auf meine Gedanken.", "tokens": ["Und", "lau\u00b7ern", "auf", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und schwarzer Efeu um mein Gehirn", "tokens": ["Und", "schwar\u00b7zer", "E\u00b7feu", "um", "mein", "Ge\u00b7hirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit feinen, nagenden Ranken.", "tokens": ["Mit", "fei\u00b7nen", ",", "na\u00b7gen\u00b7den", "Ran\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf allen Wegen nur k\u00fcmmerlich Luft.", "tokens": ["Auf", "al\u00b7len", "We\u00b7gen", "nur", "k\u00fcm\u00b7mer\u00b7lich", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vor den Augen Regen, nur Regen.", "tokens": ["Vor", "den", "Au\u00b7gen", "Re\u00b7gen", ",", "nur", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}