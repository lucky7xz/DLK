{"textgrid.poem.60202": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund! Der Mahler? Gefunden", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund! Der Mahler? Gefunden", "tokens": ["O", "Freund", "!", "Der", "Mah\u00b7ler", "?", "Ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "ART", "NN", "$.", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat er im Auge mein Herz.", "tokens": ["Hat", "er", "im", "Au\u00b7ge", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Er fand mit sp\u00e4hendem Blicke", "tokens": ["Er", "fand", "mit", "sp\u00e4\u00b7hen\u00b7dem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Geist, und zeichnete ihn.", "tokens": ["Den", "Geist", ",", "und", "zeich\u00b7ne\u00b7te", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die sanft empfindende Seele", "tokens": ["Die", "sanft", "emp\u00b7fin\u00b7den\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Entwarf sein Pinsel, und nicht", "tokens": ["Ent\u00b7warf", "sein", "Pin\u00b7sel", ",", "und", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den Mund, die Wange, das L\u00e4cheln", "tokens": ["Den", "Mund", ",", "die", "Wan\u00b7ge", ",", "das", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir ohne Reize bekannt!", "tokens": ["Dir", "oh\u00b7ne", "Rei\u00b7ze", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "O dies zu sch\u00f6ne Gem\u00e4hlde", "tokens": ["O", "dies", "zu", "sch\u00f6\u00b7ne", "Ge\u00b7m\u00e4hl\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PDS", "PTKZU", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seh ich und kenne das Bild", "tokens": ["Seh", "ich", "und", "ken\u00b7ne", "das", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Von der unsterblichen Freundin", "tokens": ["Von", "der", "uns\u00b7terb\u00b7li\u00b7chen", "Freun\u00b7din"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die in mir denket, und f\u00fchlt.", "tokens": ["Die", "in", "mir", "den\u00b7ket", ",", "und", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mir von den G\u00f6ttern gesendet", "tokens": ["Mir", "von", "den", "G\u00f6t\u00b7tern", "ge\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ward sie, und lange verkannt", "tokens": ["Ward", "sie", ",", "und", "lan\u00b7ge", "ver\u00b7kannt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KON", "ADV", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Rief ihr aufstrebender Hunger", "tokens": ["Rief", "ihr", "auf\u00b7stre\u00b7ben\u00b7der", "Hun\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nicht Brod, nein Freunde f\u00fcr sich.", "tokens": ["Nicht", "Brod", ",", "nein", "Freun\u00b7de", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKANT", "NN", "APPR", "PRF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Gefunden hab ich euch endlich", "tokens": ["Ge\u00b7fun\u00b7den", "hab", "ich", "euch", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr von der Seele gew\u00fcnscht.", "tokens": ["Ihr", "von", "der", "See\u00b7le", "ge\u00b7w\u00fcnscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wer ihre Freude will kennen,", "tokens": ["Wer", "ih\u00b7re", "Freu\u00b7de", "will", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der komm und sehe mein Bild!", "tokens": ["Der", "komm", "und", "se\u00b7he", "mein", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Sie sitzt in sch\u00f6nstem Erstaunen", "tokens": ["Sie", "sitzt", "in", "sch\u00f6ns\u00b7tem", "Er\u00b7stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und denkt nicht Ehre, nicht Gold;", "tokens": ["Und", "denkt", "nicht", "Eh\u00b7re", ",", "nicht", "Gold", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Freund! ihre G\u00f6tter auf Erden", "tokens": ["Freund", "!", "ih\u00b7re", "G\u00f6t\u00b7ter", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Denkt sie, und denket auch dich!", "tokens": ["Denkt", "sie", ",", "und", "den\u00b7ket", "auch", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Da wo die Musen und Weisheit", "tokens": ["Da", "wo", "die", "Mu\u00b7sen", "und", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dir l\u00e4cheln, stelle sie hin;", "tokens": ["Dir", "l\u00e4\u00b7cheln", ",", "stel\u00b7le", "sie", "hin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und nenn' einst deiner Geliebten,", "tokens": ["Und", "nenn'", "einst", "dei\u00b7ner", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die auf der Schulter dir liegt,", "tokens": ["Die", "auf", "der", "Schul\u00b7ter", "dir", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Nenn ihr den zaubernden K\u00fcnstler", "tokens": ["Nenn", "ihr", "den", "zau\u00b7bern\u00b7den", "K\u00fcnst\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sprich: Das singende Weib", "tokens": ["Und", "sprich", ":", "Das", "sin\u00b7gen\u00b7de", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "War arm an \u00e4usserer Reizung", "tokens": ["War", "arm", "an", "\u00e4us\u00b7se\u00b7rer", "Rei\u00b7zung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und reich an s\u00fcssem Gef\u00fchl;", "tokens": ["Und", "reich", "an", "s\u00fcs\u00b7sem", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Mit zart geschaffenem Herzen", "tokens": ["Mit", "zart", "ge\u00b7schaf\u00b7fe\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ward sie einst Sapho genannt;", "tokens": ["Ward", "sie", "einst", "Sa\u00b7pho", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr waren Musen gef\u00e4llig,", "tokens": ["Ihr", "wa\u00b7ren", "Mu\u00b7sen", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sie war Freunden getreu.", "tokens": ["Und", "sie", "war", "Freun\u00b7den", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}