{"textgrid.poem.67516": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Germanien", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, schlummerst Du noch? Siehe, was rings um Dich,", "tokens": ["Deutschland", ",", "schlum\u00b7merst", "Du", "noch", "?", "Sie\u00b7he", ",", "was", "rings", "um", "Dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "$.", "VVIMP", "$,", "PRELS", "ADV", "APPR", "PPER", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Was Dir selber geschah! F\u00fchl es, ermuntre Dich,", "tokens": ["Was", "Dir", "sel\u00b7ber", "ge\u00b7schah", "!", "F\u00fchl", "es", ",", "er\u00b7munt\u00b7re", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$.", "NN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-++--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Eh die Sch\u00e4rfe des Siegers", "tokens": ["Eh", "die", "Sch\u00e4r\u00b7fe", "des", "Sie\u00b7gers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dir mit Hohne den Scheitel bl\u00f6\u00dft!", "tokens": ["Dir", "mit", "Hoh\u00b7ne", "den", "Schei\u00b7tel", "bl\u00f6\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Deine Nachbarin sieh, Polen, wie m\u00e4chtig einst", "tokens": ["Dei\u00b7ne", "Nach\u00b7ba\u00b7rin", "sieh", ",", "Po\u00b7len", ",", "wie", "m\u00e4ch\u00b7tig", "einst"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "NE", "$,", "PWAV", "ADJD", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und wie stolz! o sie kniet, ehren- und schmuckberaubt,", "tokens": ["Und", "wie", "stolz", "!", "o", "sie", "kniet", ",", "eh\u00b7ren", "und", "schmuck\u00b7be\u00b7raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$.", "FM", "PPER", "VVFIN", "$,", "TRUNC", "KON", "VVFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit zerrissenem Busen", "tokens": ["Mit", "zer\u00b7ris\u00b7se\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vor drei M\u00e4chtigen und verstummt.", "tokens": ["Vor", "drei", "M\u00e4ch\u00b7ti\u00b7gen", "und", "ver\u00b7stummt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ach, es halfen ihr nicht ihre Magnaten, nicht", "tokens": ["Ach", ",", "es", "hal\u00b7fen", "ihr", "nicht", "ih\u00b7re", "Mag\u00b7na\u00b7ten", ",", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihre Edeln, es half keiner der Namen ihr,", "tokens": ["Ih\u00b7re", "E\u00b7deln", ",", "es", "half", "kei\u00b7ner", "der", "Na\u00b7men", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PIS", "ART", "NN", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die aus tapferer Vorzeit", "tokens": ["Die", "aus", "tap\u00b7fe\u00b7rer", "Vor\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ewig gl\u00e4nzen am Sterngezelt.", "tokens": ["E\u00b7wig", "gl\u00e4n\u00b7zen", "am", "Stern\u00b7ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Und nun wende den Blick! Schau die zerfallenen", "tokens": ["Und", "nun", "wen\u00b7de", "den", "Blick", "!", "Schau", "die", "zer\u00b7fal\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "NN", "ART", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tr\u00fcmmer, welche man sonst Burgen der Freiheit hie\u00df,", "tokens": ["Tr\u00fcm\u00b7mer", ",", "wel\u00b7che", "man", "sonst", "Bur\u00b7gen", "der", "Frei\u00b7heit", "hie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Unzerst\u00f6rbare Nester;", "tokens": ["Un\u00b7zer\u00b7st\u00f6r\u00b7ba\u00b7re", "Nes\u00b7ter", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Weiter schaue! Du siehst, ferne in Osten steht", "tokens": ["Wei\u00b7ter", "schau\u00b7e", "!", "Du", "siehst", ",", "fer\u00b7ne", "in", "Os\u00b7ten", "steht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Dir ein Riese; Du selbst lehretest ihn, sein Schwert,", "tokens": ["Dir", "ein", "Rie\u00b7se", ";", "Du", "selbst", "leh\u00b7re\u00b7test", "ihn", ",", "sein", "Schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "PPER", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Seine Keule zu schwingen;", "tokens": ["Sei\u00b7ne", "Keu\u00b7le", "zu", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zorndorf probte sie auch an Dir.", "tokens": ["Zorn\u00b7dorf", "prob\u00b7te", "sie", "auch", "an", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Schau gen Westen! Es droht, fertig in jedem Kampf,", "tokens": ["Schau", "gen", "Wes\u00b7ten", "!", "Es", "droht", ",", "fer\u00b7tig", "in", "je\u00b7dem", "Kampf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vielgewandt und entgl\u00fcht, trotzend auf Gl\u00fcck und Macht,", "tokens": ["Viel\u00b7ge\u00b7wandt", "und", "ent\u00b7gl\u00fcht", ",", "trot\u00b7zend", "auf", "Gl\u00fcck", "und", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Dir ein anderer K\u00e4mpfer,", "tokens": ["Dir", "ein", "an\u00b7de\u00b7rer", "K\u00e4mp\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Dir schon eine Locke nahm.", "tokens": ["Der", "Dir", "schon", "ei\u00b7ne", "Lo\u00b7cke", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und Du s\u00e4umetest noch, Dich zu ermannen, Dich", "tokens": ["Und", "Du", "s\u00e4u\u00b7me\u00b7test", "noch", ",", "Dich", "zu", "er\u00b7man\u00b7nen", ",", "Dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klug zu einen? Du s\u00e4umst, kleinlich im Eigennutz,", "tokens": ["Klug", "zu", "ei\u00b7nen", "?", "Du", "s\u00e4umst", ",", "klein\u00b7lich", "im", "Ei\u00b7gen\u00b7nutz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Statt des polnischen Reichstags,", "tokens": ["Statt", "des", "pol\u00b7ni\u00b7schen", "Reichs\u00b7tags", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dich zu ordnen, ein m\u00e4chtig Volk?", "tokens": ["Dich", "zu", "ord\u00b7nen", ",", "ein", "m\u00e4ch\u00b7tig", "Volk", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ART", "ADJD", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Soll Dein Name verwehn? Willst Du zertheilet auch", "tokens": ["Soll", "Dein", "Na\u00b7me", "ver\u00b7wehn", "?", "Willst", "Du", "zer\u00b7thei\u00b7let", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$.", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Knien vor Fremden? Und ist keiner der V\u00e4ter Dir,", "tokens": ["Kni\u00b7en", "vor", "Frem\u00b7den", "?", "Und", "ist", "kei\u00b7ner", "der", "V\u00e4\u00b7ter", "Dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "KON", "VAFIN", "PIS", "ART", "NN", "PPER", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dir Dein eigenes ", "tokens": ["Dir", "Dein", "ei\u00b7ge\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Deine ", "tokens": ["Dei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Sprich, mit ", "tokens": ["Sprich", ",", "mit"], "token_info": ["word", "punct", "word"], "pos": ["VVIMP", "$,", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Sie zu tauschen? Dein Herz, soll es des Galliers,", "tokens": ["Sie", "zu", "tau\u00b7schen", "?", "Dein", "Herz", ",", "soll", "es", "des", "Gal\u00b7liers", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Des Kosacken, Kalmucken", "tokens": ["Des", "Ko\u00b7sa\u00b7cken", ",", "Kal\u00b7mu\u00b7cken"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Pulsschlag fr\u00f6hnen? Ermuntre Dich!", "tokens": ["Puls\u00b7schlag", "fr\u00f6h\u00b7nen", "?", "Er\u00b7munt\u00b7re", "Dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Wer sich selber nicht sch\u00fctzt, ist er der Freiheit werth?", "tokens": ["Wer", "sich", "sel\u00b7ber", "nicht", "sch\u00fctzt", ",", "ist", "er", "der", "Frei\u00b7heit", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "PTKNEG", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der gemaleten, die nur ihm geg\u00f6nnet ward.", "tokens": ["Der", "ge\u00b7ma\u00b7le\u00b7ten", ",", "die", "nur", "ihm", "ge\u00b7g\u00f6n\u00b7net", "ward", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach, die Pfeile des B\u00fcndels,", "tokens": ["Ach", ",", "die", "Pfei\u00b7le", "des", "B\u00fcn\u00b7dels", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Einzeln bricht sie der Knabe leicht.", "tokens": ["Ein\u00b7zeln", "bricht", "sie", "der", "Kna\u00b7be", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "H\u00f6fe sch\u00fctzen Dich nicht; ihre Magnaten fliehn,", "tokens": ["H\u00f6\u00b7fe", "sch\u00fct\u00b7zen", "Dich", "nicht", ";", "ih\u00b7re", "Mag\u00b7na\u00b7ten", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$.", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn kaum nahet der Feind; Inful und Mitra nicht.", "tokens": ["Wenn", "kaum", "na\u00b7het", "der", "Feind", ";", "In\u00b7ful", "und", "Mit\u00b7ra", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "$.", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wirf die l\u00e4hmende Deutschheit", "tokens": ["Wirf", "die", "l\u00e4h\u00b7men\u00b7de", "Deutschheit"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weg und sei ein Germanien!", "tokens": ["Weg", "und", "sei", "ein", "Ger\u00b7ma\u00b7ni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tr\u00e4um' ich, oder ich seh' welch einen Genius", "tokens": ["Tr\u00e4um'", "ich", ",", "o\u00b7der", "ich", "seh'", "welch", "ei\u00b7nen", "Ge\u00b7nius"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PWAT", "ART", "NN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Niederschweben? Er kn\u00fcpft, einig verkn\u00fcpfet er", "tokens": ["Nie\u00b7der\u00b7schwe\u00b7ben", "?", "Er", "kn\u00fcpft", ",", "ei\u00b7nig", "ver\u00b7kn\u00fcp\u00b7fet", "er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Zwei germanische Freundes-", "tokens": ["Zwei", "ger\u00b7ma\u00b7ni\u00b7sche", "Freun\u00b7des"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "TRUNC"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "H\u00e4nde, Preu\u00dfen und Oesterreich.", "tokens": ["H\u00e4n\u00b7de", ",", "Preu\u00b7\u00dfen", "und", "O\u00b7es\u00b7ter\u00b7reich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NE", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}