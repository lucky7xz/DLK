{"dta.poem.21670": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wie der Hirsch nach Wasser, lechzet", "tokens": ["Wie", "der", "Hirsch", "nach", "Was\u00b7ser", ",", "lech\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele, Gott, zu dir \u2014", "tokens": ["Mei\u00b7ne", "See\u00b7le", ",", "Gott", ",", "zu", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie verschmachtet und sie \u00e4chzet \u2014", "tokens": ["Sie", "ver\u00b7schmach\u00b7tet", "und", "sie", "\u00e4ch\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thr\u00e4nen nur sind Labung mir.", "tokens": ["Thr\u00e4\u00b7nen", "nur", "sind", "La\u00b7bung", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Galle bietet man allein", "tokens": ["Gal\u00b7le", "bie\u00b7tet", "man", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem Verschmachtenden als Wein.", "tokens": ["Dem", "Ver\u00b7schmach\u00b7ten\u00b7den", "als", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}