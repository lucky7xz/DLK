{"textgrid.poem.61238": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mag auch hei\u00df das Scheiden brennen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag auch hei\u00df das Scheiden brennen,", "tokens": ["Mag", "auch", "hei\u00df", "das", "Schei\u00b7den", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treuer Mut hat Trost und Licht;", "tokens": ["Treu\u00b7er", "Mut", "hat", "Trost", "und", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag auch Hand von Hand sich trennen,", "tokens": ["Mag", "auch", "Hand", "von", "Hand", "sich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe l\u00e4\u00dft von Liebe nicht.", "tokens": ["Lie\u00b7be", "l\u00e4\u00dft", "von", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Ferne darf uns kr\u00e4nken,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "darf", "uns", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn uns h\u00e4lt ein treu Gedenken.", "tokens": ["Denn", "uns", "h\u00e4lt", "ein", "treu", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist kein Wasser so ohn' Ende,", "tokens": ["Ist", "kein", "Was\u00b7ser", "so", "ohn'", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch so schmal ein Felsensteg,", "tokens": ["Noch", "so", "schmal", "ein", "Fel\u00b7sens\u00b7teg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nicht rechte Sehnsucht f\u00e4nde", "tokens": ["Da\u00df", "nicht", "rech\u00b7te", "Sehn\u00b7sucht", "f\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcberhin den sichern Weg.", "tokens": ["Dr\u00fc\u00b7berh\u00b7in", "den", "si\u00b7chern", "Weg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Keine Ferne darf uns kr\u00e4nken,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "darf", "uns", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn uns h\u00e4lt ein treu Gedenken.", "tokens": ["Denn", "uns", "h\u00e4lt", "ein", "treu", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00dcber Berg' und tiefe Tale,", "tokens": ["\u00dc\u00b7ber", "Ber\u00b7g'", "und", "tie\u00b7fe", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit den Wolken, mit dem Wind,", "tokens": ["Mit", "den", "Wol\u00b7ken", ",", "mit", "dem", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4glich, st\u00fcndlich tausend Male", "tokens": ["T\u00e4g\u00b7lich", ",", "st\u00fcnd\u00b7lich", "tau\u00b7send", "Ma\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df' ich dich, geliebtes Kind.", "tokens": ["Gr\u00fc\u00df'", "ich", "dich", ",", "ge\u00b7lieb\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Ferne darf uns kr\u00e4nken,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "darf", "uns", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn uns h\u00e4lt ein treu Gedenken.", "tokens": ["Denn", "uns", "h\u00e4lt", "ein", "treu", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Wind' und Wolken tragen", "tokens": ["Und", "die", "Wind'", "und", "Wol\u00b7ken", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Her zu mir die Liebe dein,", "tokens": ["Her", "zu", "mir", "die", "Lie\u00b7be", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gedanken, die da sagen:", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", ",", "die", "da", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin dein und du bist mein!", "tokens": ["Ich", "bin", "dein", "und", "du", "bist", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "KON", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Ferne darf uns kr\u00e4nken,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "darf", "uns", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn uns h\u00e4lt ein treu Gedenken.", "tokens": ["Denn", "uns", "h\u00e4lt", "ein", "treu", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00dcberall, wohin ich schreite,", "tokens": ["\u00dc\u00b7be\u00b7rall", ",", "wo\u00b7hin", "ich", "schrei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00fcr' ich, wie unsichtbarlich", "tokens": ["Sp\u00fcr'", "ich", ",", "wie", "un\u00b7sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Gebet mir zieht zur Seite", "tokens": ["Dein", "Ge\u00b7bet", "mir", "zieht", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Fl\u00fcgel schl\u00e4gt um mich.", "tokens": ["Und", "die", "Fl\u00fc\u00b7gel", "schl\u00e4gt", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Ferne darf uns kr\u00e4nken,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "darf", "uns", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn uns h\u00e4lt ein treu Gedenken.", "tokens": ["Denn", "uns", "h\u00e4lt", "ein", "treu", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so bin ich froh und stille,", "tokens": ["Und", "so", "bin", "ich", "froh", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich noch so ferne gehn;", "tokens": ["Mu\u00df", "ich", "noch", "so", "fer\u00b7ne", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Schritt - ist's Gottes Wille -", "tokens": ["Je\u00b7der", "Schritt", "ist's", "Got\u00b7tes", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "VAFIN", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Schritt zum Wiedersehn.", "tokens": ["Ist", "ein", "Schritt", "zum", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Ferne darf uns kr\u00e4nken,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "darf", "uns", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn uns h\u00e4lt ein treu Gedenken.", "tokens": ["Denn", "uns", "h\u00e4lt", "ein", "treu", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}