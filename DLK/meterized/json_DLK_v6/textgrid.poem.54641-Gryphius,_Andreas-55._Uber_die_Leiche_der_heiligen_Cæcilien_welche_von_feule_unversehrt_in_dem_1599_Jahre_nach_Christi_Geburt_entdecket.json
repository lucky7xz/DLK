{"textgrid.poem.54641": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "55. Uber die Leiche der heiligen C\u00e6cilien welche von feule unversehrt in dem 1599 Jahre nach Christi Geburt entdecket", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung; doch verst\u00e4ndig/ sch\u00f6n/ doch z\u00fcchtig/ reich/ doch rein", "tokens": ["Jung", ";", "doch", "ver\u00b7st\u00e4n\u00b7dig", "/", "sch\u00f6n", "/", "doch", "z\u00fcch\u00b7tig", "/", "reich", "/", "doch", "rein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJD", "$(", "ADJD", "$(", "ADV", "ADJD", "$(", "ADJD", "$(", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verm\u00e4hlt; doch Jungfraw schwach/ doch st\u00e4rcker denn die Pein.", "tokens": ["Ver\u00b7m\u00e4hlt", ";", "doch", "Jung\u00b7fraw", "schwach", "/", "doch", "st\u00e4r\u00b7cker", "denn", "die", "Pein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "NN", "VVFIN", "$(", "ADV", "ADJD", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey Engeln; auf der Welt geschmissen mit dem Schwerd/", "tokens": ["Bey", "En\u00b7geln", ";", "auf", "der", "Welt", "ge\u00b7schmis\u00b7sen", "mit", "dem", "Schwerd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dreymal; und nicht enthalst. Vergraben: nicht verzehrt.", "tokens": ["Drey\u00b7mal", ";", "und", "nicht", "ent\u00b7halst", ".", "Ver\u00b7gra\u00b7ben", ":", "nicht", "ver\u00b7zehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "PTKNEG", "VVFIN", "$.", "NN", "$.", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Verdeckt zw\u00f6lffhundert Jahr; doch nicht verkehrt in Erden.", "tokens": ["Ver\u00b7deckt", "zw\u00f6lf\u00b7fhun\u00b7dert", "Jahr", ";", "doch", "nicht", "ver\u00b7kehrt", "in", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$.", "ADV", "PTKNEG", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan was nicht irdisch ist wol Erd und Asche werden.", "tokens": ["Kan", "was", "nicht", "ir\u00b7disch", "ist", "wol", "Erd", "und", "A\u00b7sche", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWS", "PTKNEG", "ADJD", "VAFIN", "ADV", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}