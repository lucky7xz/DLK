{"textgrid.poem.57711": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und nun mu\u00dft du in den Tod, du jungjunges Blut,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun mu\u00dft du in den Tod, du jungjunges Blut,", "tokens": ["Und", "nun", "mu\u00dft", "du", "in", "den", "Tod", ",", "du", "jung\u00b7jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nun mu\u00dft du hinab in die Nacht,", "tokens": ["Und", "nun", "mu\u00dft", "du", "hin\u00b7ab", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und dein Mund, der singt ein leises Lied,", "tokens": ["Und", "dein", "Mund", ",", "der", "singt", "ein", "lei\u00b7ses", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und dein Mund, der singt und der lacht.", "tokens": ["Und", "dein", "Mund", ",", "der", "singt", "und", "der", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "KON", "ART", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und das Kalbfell, das dr\u00f6hnt und der Knecht fa\u00dft dich an,", "tokens": ["Und", "das", "Kalb\u00b7fell", ",", "das", "dr\u00f6hnt", "und", "der", "Knecht", "fa\u00dft", "dich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VVFIN", "KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und der M\u00f6nch, der spricht das Gebet.", "tokens": ["Und", "der", "M\u00f6nch", ",", "der", "spricht", "das", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du lachst so selig und stolz vor dich hin,", "tokens": ["Und", "du", "lachst", "so", "se\u00b7lig", "und", "stolz", "vor", "dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ein Mann, der ins Brautbett geht.", "tokens": ["Wie", "ein", "Mann", ",", "der", "ins", "Braut\u00b7bett", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Eine Rose fiel vom hohen Altan", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "fiel", "vom", "ho\u00b7hen", "Al\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und ein Ku\u00df kam von schneewei\u00dfer Hand,", "tokens": ["Und", "ein", "Ku\u00df", "kam", "von", "schnee\u00b7wei\u00b7\u00dfer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und du gehst den Weg in die dunkele Nacht,", "tokens": ["Und", "du", "gehst", "den", "Weg", "in", "die", "dun\u00b7ke\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Weg in das d\u00fcstere Land.", "tokens": ["Den", "Weg", "in", "das", "d\u00fcs\u00b7te\u00b7re", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und es lachen deine Augen und es l\u00e4chelt dein Mund,", "tokens": ["Und", "es", "la\u00b7chen", "dei\u00b7ne", "Au\u00b7gen", "und", "es", "l\u00e4\u00b7chelt", "dein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Und du gehst leise singend in den Tod.", "tokens": ["Und", "du", "gehst", "lei\u00b7se", "sin\u00b7gend", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einen Ku\u00df warf dir eine schneewei\u00dfe Hand", "tokens": ["Ei\u00b7nen", "Ku\u00df", "warf", "dir", "ei\u00b7ne", "schnee\u00b7wei\u00b7\u00dfe", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-++-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und die Rose, wie Liebe so rot.", "tokens": ["Und", "die", "Ro\u00b7se", ",", "wie", "Lie\u00b7be", "so", "rot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "NN", "ADV", "ADJD", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}