{"textgrid.poem.29187": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soldaten m\u00fcssen haben Sold, sollen sie thun Thaten;", "tokens": ["Sol\u00b7da\u00b7ten", "m\u00fcs\u00b7sen", "ha\u00b7ben", "Sold", ",", "sol\u00b7len", "sie", "thun", "Tha\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "VMFIN", "$,", "VMFIN", "PPER", "VVINF", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie mussen Thaten thun f\u00fcr Sold, wolln sie seyn Soldaten.", "tokens": ["Sie", "mus\u00b7sen", "Tha\u00b7ten", "thun", "f\u00fcr", "Sold", ",", "wolln", "sie", "seyn", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "APPR", "NE", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}