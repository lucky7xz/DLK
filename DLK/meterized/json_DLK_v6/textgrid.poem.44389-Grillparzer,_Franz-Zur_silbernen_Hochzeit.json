{"textgrid.poem.44389": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zur silbernen Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Silberne Hochzeit", "tokens": ["Sil\u00b7ber\u00b7ne", "Hoch\u00b7zeit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Feiern wir heute,", "tokens": ["Fei\u00b7ern", "wir", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fr\u00f6hliche Leute", "tokens": ["Fr\u00f6h\u00b7li\u00b7che", "Leu\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Stehn wir im Kreis.", "tokens": ["Stehn", "wir", "im", "Kreis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Hymen, er kn\u00fcpft mit", "tokens": ["Hy\u00b7men", ",", "er", "kn\u00fcpft", "mit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Eisernen Banden,", "tokens": ["Ei\u00b7ser\u00b7nen", "Ban\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Blumengirlanden", "tokens": ["Blu\u00b7men\u00b7gir\u00b7lan\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Windet er drum:", "tokens": ["Win\u00b7det", "er", "drum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Doch seine Blumen,", "tokens": ["Doch", "sei\u00b7ne", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ach, sie vergehen,", "tokens": ["Ach", ",", "sie", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bande bestehen,", "tokens": ["Ban\u00b7de", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Eiserne Last.", "tokens": ["Ei\u00b7ser\u00b7ne", "Last", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Aber der Wechsel", "tokens": ["A\u00b7ber", "der", "Wech\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Rollender Jahre", "tokens": ["Rol\u00b7len\u00b7der", "Jah\u00b7re"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bessert die Ware,", "tokens": ["Bes\u00b7sert", "die", "Wa\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Adelt Metall:", "tokens": ["A\u00b7delt", "Me\u00b7tall", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Nach f\u00fcnfundzwanzig", "tokens": ["Nach", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig"], "token_info": ["word", "word"], "pos": ["APPR", "CARD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht mehr von Eisen,", "tokens": ["Nicht", "mehr", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ketten als Schmuck.", "tokens": ["Ket\u00b7ten", "als", "Schmuck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Gleich viel der Jahre", "tokens": ["Gleich", "viel", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Lasset vergehen,", "tokens": ["Las\u00b7set", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dann sollt ihr sehen,", "tokens": ["Dann", "sollt", "ihr", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sind sie von ", "tokens": ["Sind", "sie", "von"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Kind auch und Enkel,", "tokens": ["Kind", "auch", "und", "En\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "T\u00f6chter und S\u00f6hne,", "tokens": ["T\u00f6ch\u00b7ter", "und", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Winden als sch\u00f6ne", "tokens": ["Win\u00b7den", "als", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOUS", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Blumen sich drum,", "tokens": ["Blu\u00b7men", "sich", "drum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "PAV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Und so am Ziele", "tokens": ["Und", "so", "am", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Seht ihr im Bande", "tokens": ["Seht", "ihr", "im", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Blumengirlande,", "tokens": ["Blu\u00b7men\u00b7gir\u00b7lan\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie beim Beginn.", "tokens": ["Wie", "beim", "Be\u00b7ginn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}