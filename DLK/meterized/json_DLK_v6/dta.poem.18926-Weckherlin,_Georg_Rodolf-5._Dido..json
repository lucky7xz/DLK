{"dta.poem.18926": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "5.  \n  Dido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wje bitter/ Dido/ war die frucht", "tokens": ["Wie", "bit\u00b7ter", "/", "Di\u00b7do", "/", "war", "die", "frucht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$(", "NE", "$(", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der beeden M\u00e4nner lieb denen du beygewohnet:", "tokens": ["Der", "bee\u00b7den", "M\u00e4n\u00b7ner", "lieb", "de\u00b7nen", "du", "bey\u00b7ge\u00b7woh\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Einen tod hat dich mit flucht/", "tokens": ["Des", "Ei\u00b7nen", "tod", "hat", "dich", "mit", "flucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "PRF", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des andern flucht mit tod belohnet.", "tokens": ["Des", "an\u00b7dern", "flucht", "mit", "tod", "be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}