{"textgrid.poem.64115": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch rauscht das Fest im Hippodrom", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch rauscht das Fest im Hippodrom", "tokens": ["Hoch", "rauscht", "das", "Fest", "im", "Hip\u00b7po\u00b7drom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Trier an dem Moselstrom:", "tokens": ["Zu", "Trier", "an", "dem", "Mo\u00b7sel\u00b7strom", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor\u00fcber jagten l\u00e4ngst die Renner,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "jag\u00b7ten", "l\u00e4ngst", "die", "Ren\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Weiber, lustberauscht, und M\u00e4nner", "tokens": ["Und", "Wei\u00b7ber", ",", "lust\u00b7be\u00b7rauscht", ",", "und", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Begehen in dem Marmorsaal,", "tokens": ["Be\u00b7ge\u00b7hen", "in", "dem", "Mar\u00b7mor\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im s\u00e4ulenstolzen Portikus,", "tokens": ["Im", "s\u00e4u\u00b7len\u00b7stol\u00b7zen", "Por\u00b7ti\u00b7kus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Versenkt, versunken im Genu\u00df,", "tokens": ["Ver\u00b7senkt", ",", "ver\u00b7sun\u00b7ken", "im", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein z\u00fcgelloses Bacchanal.", "tokens": ["Ein", "z\u00fc\u00b7gel\u00b7lo\u00b7ses", "Bac\u00b7cha\u00b7nal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun springt von des Tribunen Scho\u00df", "tokens": ["Nun", "springt", "von", "des", "Tri\u00b7bu\u00b7nen", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00fcppig Weib, die Br\u00fcste blo\u00df,", "tokens": ["Ein", "\u00fcp\u00b7pig", "Weib", ",", "die", "Br\u00fcs\u00b7te", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jauchzt und lacht, von Wein beladen:", "tokens": ["Und", "jauchzt", "und", "lacht", ",", "von", "Wein", "be\u00b7la\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbkennt ihr den Rauschtanz der M\u00e4naden,", "tokens": ["\u00bb", "kennt", "ihr", "den", "Rauschtanz", "der", "M\u00e4\u00b7na\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Wie ich ihn einst in Phrygia", "tokens": ["Wie", "ich", "ihn", "einst", "in", "Phry\u00b7gia"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beim Fest der gro\u00dfen G\u00f6ttin sah?", "tokens": ["Beim", "Fest", "der", "gro\u00b7\u00dfen", "G\u00f6t\u00b7tin", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schaut her, ich tanze vor!\u00ab Sie springt,", "tokens": ["Schaut", "her", ",", "ich", "tan\u00b7ze", "vor", "!", "\u00ab", "Sie", "springt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df hoch das Purpurhemde schwingt", "tokens": ["Da\u00df", "hoch", "das", "Pur\u00b7pur\u00b7hem\u00b7de", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und singt:", "tokens": ["Und", "singt", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "\u00bbh\u00f6rt, was die G\u00f6ttin mich selber gelehrt,", "tokens": ["\u00bb", "h\u00f6rt", ",", "was", "die", "G\u00f6t\u00b7tin", "mich", "sel\u00b7ber", "ge\u00b7lehrt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PRELS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Kybele, welche die Wonne gew\u00e4hrt.", "tokens": ["Ky\u00b7be\u00b7le", ",", "wel\u00b7che", "die", "Won\u00b7ne", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Schl\u00fcrfet des Augenblicks raschen Genu\u00df,", "tokens": ["Schl\u00fcr\u00b7fet", "des", "Au\u00b7gen\u00b7blicks", "ra\u00b7schen", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Schl\u00fcrfet den Becher und schl\u00fcrfet den Ku\u00df.", "tokens": ["Schl\u00fcr\u00b7fet", "den", "Be\u00b7cher", "und", "schl\u00fcr\u00b7fet", "den", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Ach, wie so bald schon sind wir gestorben!", "tokens": ["Ach", ",", "wie", "so", "bald", "schon", "sind", "wir", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "K\u00fchn um die Wonne des Rausches geworben", "tokens": ["K\u00fchn", "um", "die", "Won\u00b7ne", "des", "Rau\u00b7sches", "ge\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.16": {"text": "Bei Fl\u00f6tenget\u00f6n und Theorben!\u00ab", "tokens": ["Bei", "Fl\u00f6\u00b7ten\u00b7ge\u00b7t\u00f6n", "und", "The\u00b7or\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und die Tausende stimmen mit ein,", "tokens": ["Und", "die", "Tau\u00b7sen\u00b7de", "stim\u00b7men", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schwingen die Becher und schlingen den Reihn:", "tokens": ["Schwin\u00b7gen", "die", "Be\u00b7cher", "und", "schlin\u00b7gen", "den", "Reihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u00bbum Lust, um Rausch geworben", "tokens": ["\u00bb", "um", "Lust", ",", "um", "Rausch", "ge\u00b7wor\u00b7ben"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "KOUI", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Fl\u00f6ten und Theorben!\u00ab", "tokens": ["Bei", "Fl\u00f6\u00b7ten", "und", "The\u00b7or\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da warnt ein M\u00f6nch, ein hagrer Greis,", "tokens": ["Da", "warnt", "ein", "M\u00f6nch", ",", "ein", "hag\u00b7rer", "Greis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Blick so tief, sein Bart so wei\u00df:", "tokens": ["Sein", "Blick", "so", "tief", ",", "sein", "Bart", "so", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbverblendet Volk! La\u00df ab! Halt ein!", "tokens": ["\u00bb", "ver\u00b7blen\u00b7det", "Volk", "!", "La\u00df", "ab", "!", "Halt", "ein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$.", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Christus ist das Heil allein.", "tokens": ["In", "Chris\u00b7tus", "ist", "das", "Heil", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Alsbald, zur Strafe deiner S\u00fcnden, \u2013", "tokens": ["Als\u00b7bald", ",", "zur", "Stra\u00b7fe", "dei\u00b7ner", "S\u00fcn\u00b7den", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das l\u00e4\u00dft der Geist mich dir verk\u00fcnden: \u2013", "tokens": ["Das", "l\u00e4\u00dft", "der", "Geist", "mich", "dir", "ver\u00b7k\u00fcn\u00b7den", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wird Gottes Zorn die Stadt entz\u00fcnden.", "tokens": ["Wird", "Got\u00b7tes", "Zorn", "die", "Stadt", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Tut Bu\u00dfe!\u00ab ... Da, beim Schall der Lieder,", "tokens": ["Tut", "Bu\u00b7\u00dfe", "!", "\u00ab", "...", "Da", ",", "beim", "Schall", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "$(", "$(", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Tanzt schon ein wirbelnd Paar ihn nieder", "tokens": ["Tanzt", "schon", "ein", "wir\u00b7belnd", "Paar", "ihn", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und jauchzend, jubelnd schallt es wieder:", "tokens": ["Und", "jauch\u00b7zend", ",", "ju\u00b7belnd", "schallt", "es", "wie\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbum Lust und Rausch geworben", "tokens": ["\u00bb", "um", "Lust", "und", "Rausch", "ge\u00b7wor\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Bei Fl\u00f6ten und Theorben!\u00ab", "tokens": ["Bei", "Fl\u00f6\u00b7ten", "und", "The\u00b7or\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jetzt wirft der Richter strenge", "tokens": ["Jetzt", "wirft", "der", "Rich\u00b7ter", "stren\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Stab in das Gedr\u00e4nge:", "tokens": ["Den", "Stab", "in", "das", "Ge\u00b7dr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdrei Tage w\u00e4hrt nun dies Gepr\u00e4nge", "tokens": ["\u00bb", "drei", "Ta\u00b7ge", "w\u00e4hrt", "nun", "dies", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "CARD", "NN", "VVFIN", "ADV", "PDS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lasters und der L\u00fcste schon,", "tokens": ["Des", "Las\u00b7ters", "und", "der", "L\u00fcs\u00b7te", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verwaist steht l\u00e4ngst der Themis Thron:", "tokens": ["Ver\u00b7waist", "steht", "l\u00e4ngst", "der", "The\u00b7mis", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich ruf' euch auf im Geist der Alten", "tokens": ["Ich", "ruf'", "euch", "auf", "im", "Geist", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kommt, helfet mir, Gericht zu halten:", "tokens": ["Kommt", ",", "hel\u00b7fet", "mir", ",", "Ge\u00b7richt", "zu", "hal\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Rechts der R\u00f6mer la\u00dft uns walten!\u00ab", "tokens": ["Des", "Rechts", "der", "R\u00f6\u00b7mer", "la\u00dft", "uns", "wal\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch schon hat ihn hinweggeschoben", "tokens": ["Doch", "schon", "hat", "ihn", "hin\u00b7weg\u00b7ge\u00b7scho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Faunenmasken wildes Toben:", "tokens": ["Der", "Fau\u00b7nen\u00b7mas\u00b7ken", "wil\u00b7des", "To\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbdas Recht der R\u00f6mer ist uns bewu\u00dft!", "tokens": ["\u00bb", "das", "Recht", "der", "R\u00f6\u00b7mer", "ist", "uns", "be\u00b7wu\u00dft", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Recht der R\u00f6mer ist die Lust!", "tokens": ["Das", "Recht", "der", "R\u00f6\u00b7mer", "ist", "die", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "m