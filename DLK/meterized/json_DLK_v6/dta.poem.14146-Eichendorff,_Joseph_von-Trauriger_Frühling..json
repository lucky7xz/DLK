{"dta.poem.14146": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trauriger Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "sv:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Viel' V\u00f6glein lockend sangen", "tokens": ["Viel'", "V\u00f6\u00b7glein", "lo\u00b7ckend", "san\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im bl\u00fchenden Revier,", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich hatt' mir ein's gefangen,", "tokens": ["Ich", "hatt'", "mir", "ein's", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt ist es weit von mir,", "tokens": ["Jetzt", "ist", "es", "weit", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel V\u00f6glein drau\u00dfen sangen,", "tokens": ["Viel", "V\u00f6\u00b7glein", "drau\u00b7\u00dfen", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, h\u00e4tt' ich mein's nur hier!", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "mein's", "nur", "hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}