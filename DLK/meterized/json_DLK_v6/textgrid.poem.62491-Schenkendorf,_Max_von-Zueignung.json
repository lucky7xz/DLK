{"textgrid.poem.62491": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die mit mir zum gleichen hohen Ziele,", "tokens": ["Du", ",", "die", "mit", "mir", "zum", "glei\u00b7chen", "ho\u00b7hen", "Zie\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPER", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu frommer Wallfahrt gl\u00e4ubig sich verbunden,", "tokens": ["Zu", "from\u00b7mer", "Wall\u00b7fahrt", "gl\u00e4u\u00b7big", "sich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gleich mit mir der Dichtung zarte Spiele", "tokens": ["Die", "gleich", "mit", "mir", "der", "Dich\u00b7tung", "zar\u00b7te", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gleich des Lebens heil'gen Ernst empfunden,", "tokens": ["Und", "gleich", "des", "Le\u00b7bens", "heil'\u00b7gen", "Ernst", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du sprachst: Noch schlummern edler Kr\u00e4fte viele", "tokens": ["Du", "sprachst", ":", "Noch", "schlum\u00b7mern", "ed\u00b7ler", "Kr\u00e4f\u00b7te", "vie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In deinen Saiten; auf, sie zu erkunden!", "tokens": ["In", "dei\u00b7nen", "Sai\u00b7ten", ";", "auf", ",", "sie", "zu", "er\u00b7kun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du hast dein freies Vaterland gesungen,", "tokens": ["Du", "hast", "dein", "frei\u00b7es", "Va\u00b7ter\u00b7land", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fort sei um einen h\u00f6hern Preis gerungen!", "tokens": ["Fort", "sei", "um", "ei\u00b7nen", "h\u00f6\u00b7hern", "Preis", "ge\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So sei's! Und mir erscheint aus fernen Tagen", "tokens": ["So", "sei's", "!", "Und", "mir", "er\u00b7scheint", "aus", "fer\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$.", "KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nste menschlich-g\u00f6ttlichste Gestalt.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "menschlich\u00b7g\u00f6tt\u00b7lichs\u00b7te", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-++---+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wie soll ich k\u00fchn die goldnen Saiten schlagen,", "tokens": ["Wie", "soll", "ich", "k\u00fchn", "die", "gold\u00b7nen", "Sai\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Lieb' und Wehmuth mir im Busen wallt?", "tokens": ["Wenn", "Lieb'", "und", "Weh\u00b7muth", "mir", "im", "Bu\u00b7sen", "wallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur leise Kl\u00e4nge darf ich sch\u00fcchtern wagen,", "tokens": ["Nur", "lei\u00b7se", "Kl\u00e4n\u00b7ge", "darf", "ich", "sch\u00fcch\u00b7tern", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erliegend unter meines Lied's Gewalt.", "tokens": ["Er\u00b7lie\u00b7gend", "un\u00b7ter", "mei\u00b7nes", "Lie\u00b7d's", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Kein hoher Psalm! Nur Liebe, Reu' und Sehnen", "tokens": ["Kein", "ho\u00b7her", "Psalm", "!", "Nur", "Lie\u00b7be", ",", "Reu'", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "ADV", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Schmerzenfreude spricht aus diesen T\u00f6nen.", "tokens": ["Und", "Schmer\u00b7zen\u00b7freu\u00b7de", "spricht", "aus", "die\u00b7sen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So nimm, Geliebte! was der Geist gegeben,", "tokens": ["So", "nimm", ",", "Ge\u00b7lieb\u00b7te", "!", "was", "der", "Geist", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "NN", "$.", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bring' es freundlich zu dem holden Kreise,", "tokens": ["Und", "bring'", "es", "freund\u00b7lich", "zu", "dem", "hol\u00b7den", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo wir der Jugend frisches zartes Leben,", "tokens": ["Wo", "wir", "der", "Ju\u00b7gend", "fri\u00b7sches", "zar\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fraun und Jungfraun alte gute Weise,", "tokens": ["Der", "Fraun", "und", "Jung\u00b7fraun", "al\u00b7te", "gu\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aller Engel Macht und stilles Weben", "tokens": ["Und", "al\u00b7ler", "En\u00b7gel", "Macht", "und", "stil\u00b7les", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Innern oft gef\u00fchlt, so stark als leise.", "tokens": ["Im", "In\u00b7nern", "oft", "ge\u00b7f\u00fchlt", ",", "so", "stark", "als", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ob dich und jenen Chor mein Lied erbaute,", "tokens": ["Ob", "dich", "und", "je\u00b7nen", "Chor", "mein", "Lied", "er\u00b7bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PDAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das war das liebste Ziel, nach dem ich schaute.", "tokens": ["Das", "war", "das", "liebs\u00b7te", "Ziel", ",", "nach", "dem", "ich", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}