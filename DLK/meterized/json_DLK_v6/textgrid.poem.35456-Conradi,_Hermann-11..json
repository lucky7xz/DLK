{"textgrid.poem.35456": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh! Da\u00df mir doch ein Etwas \u2013 Schicksal \u2013 Gott \u2013", "tokens": ["Oh", "!", "Da\u00df", "mir", "doch", "ein", "Et\u00b7was", "\u2013", "Schick\u00b7sal", "\u2013", "Gott", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADV", "ART", "ADV", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nennt, wie Ihr's wollt! \u2013 die Kraft, die riesengro\u00dfe,", "tokens": ["Nennt", ",", "wie", "Ih\u00b7r's", "wollt", "!", "\u2013", "die", "Kraft", ",", "die", "rie\u00b7sen\u00b7gro\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "VMFIN", "$.", "$(", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Weltb\u00e4ndigende, g\u00e4be, da\u00df ich alles,", "tokens": ["Welt\u00b7b\u00e4n\u00b7di\u00b7gen\u00b7de", ",", "g\u00e4\u00b7be", ",", "da\u00df", "ich", "al\u00b7les", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "PIS", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Was sich entringt in Farben und Gestalten", "tokens": ["Was", "sich", "ent\u00b7ringt", "in", "Far\u00b7ben", "und", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem ewig unersch\u00f6pften Scho\u00dfe,", "tokens": ["Dem", "e\u00b7wig", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Scho\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erfassen und behalten k\u00f6nnte!", "tokens": ["Er\u00b7fas\u00b7sen", "und", "be\u00b7hal\u00b7ten", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df mir ergreifbar immer bliebe", "tokens": ["Da\u00df", "mir", "er\u00b7greif\u00b7bar", "im\u00b7mer", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der herbe Widerstreit der Elemente", "tokens": ["Der", "her\u00b7be", "Wi\u00b7der\u00b7streit", "der", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Das gro\u00dfe Schicksalsspiel von Ha\u00df und Liebe!", "tokens": ["Das", "gro\u00b7\u00dfe", "Schick\u00b7sals\u00b7spiel", "von", "Ha\u00df", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und was ich je voll G\u00f6tterlust durchf\u00fchlt \u2013", "tokens": ["Und", "was", "ich", "je", "voll", "G\u00f6t\u00b7ter\u00b7lust", "durch\u00b7f\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was mich durchw\u00fchlt \u2013", "tokens": ["Was", "mich", "durch\u00b7w\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit rohen ", "tokens": ["Mit", "ro\u00b7hen"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und, ", "tokens": ["Und", ","], "token_info": ["word", "punct"], "pos": ["KON", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "F\u00e4nd' in der Brandung Sturm und Dr\u00f6hnen", "tokens": ["F\u00e4nd'", "in", "der", "Bran\u00b7dung", "Sturm", "und", "Dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich dennoch tiefste Herzensstille", "tokens": ["Ich", "den\u00b7noch", "tiefs\u00b7te", "Her\u00b7zens\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und ein ents\u00fchnendes Vers\u00f6hnen!", "tokens": ["Und", "ein", "ent\u00b7s\u00fch\u00b7nen\u00b7des", "Ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ob die Sehnsucht mir die Brust zerbrennt:", "tokens": ["Und", "ob", "die", "Sehn\u00b7sucht", "mir", "die", "Brust", "zer\u00b7brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf irrer Spur", "tokens": ["Auf", "ir\u00b7rer", "Spur"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "L\u00e4\u00dft mich die Stunde nur", "tokens": ["L\u00e4\u00dft", "mich", "die", "Stun\u00b7de", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am ", "tokens": ["Am"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und fruchtlos send' ich meine Speere aus ...", "tokens": ["Und", "frucht\u00b7los", "send'", "ich", "mei\u00b7ne", "Spee\u00b7re", "aus", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein neues St\u00fcrmen und ein neu Erm\u00fcden \u2013", "tokens": ["Ein", "neu\u00b7es", "St\u00fcr\u00b7men", "und", "ein", "neu", "Er\u00b7m\u00fc\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein neues Trotzen \u2013 und doch kein Erklimmen \u2013", "tokens": ["Ein", "neu\u00b7es", "Trot\u00b7zen", "\u2013", "und", "doch", "kein", "Er\u00b7klim\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst verrollen meiner Seele Feuerfluten,", "tokens": ["Um\u00b7sonst", "ver\u00b7rol\u00b7len", "mei\u00b7ner", "See\u00b7le", "Feu\u00b7er\u00b7flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Leuchten sind im Niederglimmen ...", "tokens": ["Und", "ih\u00b7re", "Leuch\u00b7ten", "sind", "im", "Nie\u00b7der\u00b7glim\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja! Fruchtlos send' ich meine Speere aus \u2013 \u2013", "tokens": ["Ja", "!", "Frucht\u00b7los", "send'", "ich", "mei\u00b7ne", "Spee\u00b7re", "aus", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Oh! f\u00e4nd' ich endlich \u2013 endlich mich nach Haus!", "tokens": ["Oh", "!", "f\u00e4nd'", "ich", "end\u00b7lich", "\u2013", "end\u00b7lich", "mich", "nach", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "$(", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn unbez\u00e4hmbar in des Zwiestreits F\u00fclle", "tokens": ["Denn", "un\u00b7be\u00b7z\u00e4hm\u00b7bar", "in", "des", "Zwies\u00b7treits", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird mir der Drang zur Stille ...", "tokens": ["Wird", "mir", "der", "Drang", "zur", "Stil\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wo eins das andere verdr\u00e4ngt, enterbt,", "tokens": ["Wo", "eins", "das", "an\u00b7de\u00b7re", "ver\u00b7dr\u00e4ngt", ",", "en\u00b7terbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo jedes sich mit eignem Reize f\u00e4rbt,", "tokens": ["Wo", "je\u00b7des", "sich", "mit", "eig\u00b7nem", "Rei\u00b7ze", "f\u00e4rbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und keins sich rundet zur Gemeinsamkeit \u2013", "tokens": ["Und", "keins", "sich", "run\u00b7det", "zur", "Ge\u00b7mein\u00b7sam\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PRF", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird mir trotz meinem jungen Jahr zu weit ...", "tokens": ["Wird", "mir", "trotz", "mei\u00b7nem", "jun\u00b7gen", "Jahr", "zu", "weit", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Lie\u00df mich das Schicksal nicht ersteigen \u2013 \u2013", "tokens": ["Lie\u00df", "mich", "das", "Schick\u00b7sal", "nicht", "er\u00b7stei\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun denn! So scheid' ich unvers\u00f6hnt \u2013", "tokens": ["Nun", "denn", "!", "So", "scheid'", "ich", "un\u00b7ver\u00b7s\u00f6hnt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}