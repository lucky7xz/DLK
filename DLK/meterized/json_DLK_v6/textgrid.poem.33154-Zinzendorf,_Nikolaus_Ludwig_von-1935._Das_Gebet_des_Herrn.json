{"textgrid.poem.33154": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1935. Das Gebet des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater! itzo ist die zeit, deinen Sohn zu ehren; das gibt ihm gelegenheit, auch dich zu verkl\u00e4ren.", "tokens": ["Va\u00b7ter", "!", "it\u00b7zo", "ist", "die", "zeit", ",", "dei\u00b7nen", "Sohn", "zu", "eh\u00b7ren", ";", "das", "gibt", "ihm", "ge\u00b7le\u00b7gen\u00b7heit", ",", "auch", "dich", "zu", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "PDS", "VVFIN", "PPER", "NN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+---+--+-+-+--+---+--+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Und was ist das leben hie? Das ists: Wenn das seine zu Dir sagen kan: Eli! zu Ihm: Mein Gebeine!", "tokens": ["Und", "was", "ist", "das", "le\u00b7ben", "hie", "?", "Das", "ists", ":", "Wenn", "das", "sei\u00b7ne", "zu", "Dir", "sa\u00b7gen", "kan", ":", "E\u00b7li", "!", "zu", "Ihm", ":", "Mein", "Ge\u00b7bei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PDS", "VVFIN", "ADV", "$.", "PDS", "VAFIN", "$.", "KOUS", "PDS", "PPOSAT", "APPR", "PPER", "VVINF", "VMFIN", "$.", "NE", "$.", "APPR", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "-++-+----+-+--++--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Weil Er auf der erde war, hat er dich gezieret, und die arbeit macht' er gar, drein du ihn gef\u00fchret.", "tokens": ["Weil", "Er", "auf", "der", "er\u00b7de", "war", ",", "hat", "er", "dich", "ge\u00b7zie\u00b7ret", ",", "und", "die", "ar\u00b7beit", "macht'", "er", "gar", ",", "drein", "du", "ihn", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,", "VAFIN", "PPER", "PRF", "VVPP", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "--+-+--+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Alle sachen, die er gab, und die sie genommen, sahn sie an als eine haab, die von dir gekommen.", "tokens": ["Al\u00b7le", "sa\u00b7chen", ",", "die", "er", "gab", ",", "und", "die", "sie", "ge\u00b7nom\u00b7men", ",", "sahn", "sie", "an", "als", "ei\u00b7ne", "ha\u00b7ab", ",", "die", "von", "dir", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+--+-+-+-+-++--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Wu\u00dften sicherlich, da\u00df er von dir hergest\u00e4ndet; glaubten, da\u00df du ihn daher williglich gesendet.", "tokens": ["Wu\u00df\u00b7ten", "si\u00b7cher\u00b7lich", ",", "da\u00df", "er", "von", "dir", "her\u00b7ge\u00b7st\u00e4n\u00b7det", ";", "glaub\u00b7ten", ",", "da\u00df", "du", "ihn", "da\u00b7her", "wil\u00b7lig\u00b7lich", "ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$.", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PAV", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "F\u00fcr die bate dort das Lamm, nicht f\u00fcr alle leute; sondern f\u00fcr den gnaden-stamm, seiner siege beute.", "tokens": ["F\u00fcr", "die", "ba\u00b7te", "dort", "das", "Lamm", ",", "nicht", "f\u00fcr", "al\u00b7le", "leu\u00b7te", ";", "son\u00b7dern", "f\u00fcr", "den", "gna\u00b7den\u00b7stamm", ",", "sei\u00b7ner", "sie\u00b7ge", "beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADV", "ART", "NN", "$,", "PTKNEG", "APPR", "PIAT", "NN", "$.", "KON", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Da er annoch um sie war, nemlich hier auf erden, kont dein Vater-name zwar nicht verfremdet werden;", "tokens": ["Da", "er", "an\u00b7noch", "um", "sie", "war", ",", "nem\u00b7lich", "hier", "auf", "er\u00b7den", ",", "kont", "dein", "Va\u00b7ter\u00b7na\u00b7me", "zwar", "nicht", "ver\u00b7frem\u00b7det", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,", "ADV", "ADV", "APPR", "NN", "$,", "VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "--+-+-+-+-+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Er erhielt dir damals schon, was ihm zugeh\u00f6rte, bis zum ungerathnen sohn, der die schrift bew\u00e4hrte.", "tokens": ["Er", "er\u00b7hielt", "dir", "da\u00b7mals", "schon", ",", "was", "ihm", "zu\u00b7ge\u00b7h\u00f6r\u00b7te", ",", "bis", "zum", "un\u00b7ge\u00b7rath\u00b7nen", "sohn", ",", "der", "die", "schrift", "be\u00b7w\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Aber eh er in dein reich ging, wars ausgesprochen, da\u00df der j\u00fcnger freud an euch bleib unterbrochen.", "tokens": ["A\u00b7ber", "eh", "er", "in", "dein", "reich", "ging", ",", "wars", "aus\u00b7ge\u00b7spro\u00b7chen", ",", "da\u00df", "der", "j\u00fcn\u00b7ger", "freud", "an", "euch", "bleib", "un\u00b7ter\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "ADJD", "VVFIN", "$,", "VAFIN", "VVPP", "$,", "KOUS", "ART", "ADJA", "VVFIN", "APPR", "PPER", "VVFIN", "VVPP", "$."], "meter": "+--+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.10": {"line.1": {"text": "Da\u00df es sey den ganzen glast aller seiner gaben: weil du ihn geliebet hast, eh Ihr grund gegraben.", "tokens": ["Da\u00df", "es", "sey", "den", "gan\u00b7zen", "glast", "al\u00b7ler", "sei\u00b7ner", "ga\u00b7ben", ":", "weil", "du", "ihn", "ge\u00b7lie\u00b7bet", "hast", ",", "eh", "Ihr", "grund", "ge\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "ADJA", "NN", "PIAT", "PPOSAT", "VVFIN", "$.", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+--+-+-+-+-+-+---+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Treuer Vater! der welt hie fremde wie die todten, doch er kennet dich, und die kennen deinen boten.", "tokens": ["Treu\u00b7er", "Va\u00b7ter", "!", "der", "welt", "hie", "frem\u00b7de", "wie", "die", "tod\u00b7ten", ",", "doch", "er", "ken\u00b7net", "dich", ",", "und", "die", "ken\u00b7nen", "dei\u00b7nen", "bo\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "ADV", "ADJA", "KOKOM", "ART", "ADJA", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "KON", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Diesen leuten sagte er damals deine sachen, und wird ihnen dich nachher noch bekanter machen.", "tokens": ["Die\u00b7sen", "leu\u00b7ten", "sag\u00b7te", "er", "da\u00b7mals", "dei\u00b7ne", "sa\u00b7chen", ",", "und", "wird", "ih\u00b7nen", "dich", "nach\u00b7her", "noch", "be\u00b7kan\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "VVFIN", "$,", "KON", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "Bis die liebe, die ihn schon auf dem schoos umfangen, ist in ihnen, und der Sohn in sie eingegangen.", "tokens": ["Bis", "die", "lie\u00b7be", ",", "die", "ihn", "schon", "auf", "dem", "schoos", "um\u00b7fan\u00b7gen", ",", "ist", "in", "ih\u00b7nen", ",", "und", "der", "Sohn", "in", "sie", "ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "ADJD", "VVPP", "$,", "VAFIN", "APPR", "PPER", "$,", "KON", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+--+-+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}