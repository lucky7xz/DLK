{"textgrid.poem.47868": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Fakkel", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fakkel", "tokens": ["Die", "Fak\u00b7kel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Das mit verg\u00fcldten Haubt die braune Nacht zerbricht/", "tokens": ["Das", "mit", "ver\u00b7g\u00fcld\u00b7ten", "Haubt", "die", "brau\u00b7ne", "Nacht", "zer\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War noch nicht aufgewacht; es spielte durch das blaue", "tokens": ["War", "noch", "nicht", "auf\u00b7ge\u00b7wacht", ";", "es", "spiel\u00b7te", "durch", "das", "blau\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVPP", "$.", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gelblichrohte Braut/ die Mutter k\u00fchler Taue/", "tokens": ["Die", "gelb\u00b7li\u00b7ch\u00b7roh\u00b7te", "Braut", "/", "die", "Mut\u00b7ter", "k\u00fch\u00b7ler", "Tau\u00b7e", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die F\u00fcrstin des Gestirns/die neubegr\u00fcnte Welt", "tokens": ["Die", "F\u00fcrs\u00b7tin", "des", "Ge\u00b7stirns", "/", "die", "neu\u00b7be\u00b7gr\u00fcn\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lag noch wie fast vor tod in ihrem Nebelzelt.", "tokens": ["Lag", "noch", "wie", "fast", "vor", "tod", "in", "ih\u00b7rem", "Ne\u00b7bel\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der tieffe Hertzensschnitt/ das Sorgenvolle Wachen/", "tokens": ["Der", "tief\u00b7fe", "Hert\u00b7zens\u00b7schnitt", "/", "das", "Sor\u00b7gen\u00b7vol\u00b7le", "Wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein das fromme Weib Marien munter machen/", "tokens": ["Al\u00b7lein", "das", "from\u00b7me", "Weib", "Ma\u00b7ri\u00b7en", "mun\u00b7ter", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NE", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ist hinweggerafft/ nachdem sie hat gewacht", "tokens": ["Der", "ist", "hin\u00b7weg\u00b7ge\u00b7rafft", "/", "nach\u00b7dem", "sie", "hat", "ge\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht sonder Threnenflut den dritten Tag", "tokens": ["Nicht", "son\u00b7der", "Thre\u00b7nen\u00b7flut", "den", "drit\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wer tr\u00e4get wol ein Feur im Busen/ das nicht schmertze?", "tokens": ["Wer", "tr\u00e4\u00b7get", "wol", "ein", "Feur", "im", "Bu\u00b7sen", "/", "das", "nicht", "schmert\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$(", "PDS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Liebe birgt sich nicht/ der Mund verr\u00e4ht das Hertze.", "tokens": ["Die", "Lie\u00b7be", "birgt", "sich", "nicht", "/", "der", "Mund", "ver\u00b7r\u00e4ht", "das", "Hert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "$(", "ART", "NN", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der abgeseelte Leib/ das newe Todenhau\u00df/", "tokens": ["Der", "ab\u00b7ge\u00b7seel\u00b7te", "Leib", "/", "das", "ne\u00b7we", "To\u00b7den\u00b7hau\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die liegen jhr im Sinn/ dr\u00fcm bricht sie folgend au\u00df.", "tokens": ["Die", "lie\u00b7gen", "jhr", "im", "Sinn", "/", "dr\u00fcm", "bricht", "sie", "fol\u00b7gend", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$(", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}