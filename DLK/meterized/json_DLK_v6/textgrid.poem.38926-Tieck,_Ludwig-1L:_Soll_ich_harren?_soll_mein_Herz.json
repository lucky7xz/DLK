{"textgrid.poem.38926": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll ich harren? soll mein Herz", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich harren? soll mein Herz", "tokens": ["Soll", "ich", "har\u00b7ren", "?", "soll", "mein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich brechen?", "tokens": ["End\u00b7lich", "bre\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Soll ich niemals von dem Schmerz", "tokens": ["Soll", "ich", "nie\u00b7mals", "von", "dem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Busens sprechen?", "tokens": ["Mei\u00b7nes", "Bu\u00b7sens", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Warum Zittern? Warum Zagen?", "tokens": ["Wa\u00b7rum", "Zit\u00b7tern", "?", "Wa\u00b7rum", "Za\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "PWAV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4ges Weilen?", "tokens": ["Tr\u00e4\u00b7ges", "Wei\u00b7len", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Auf! dein h\u00f6chstes Gl\u00fcck zu wagen!", "tokens": ["Auf", "!", "dein", "h\u00f6chs\u00b7tes", "Gl\u00fcck", "zu", "wa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcgle deine Eile!", "tokens": ["Fl\u00fcg\u00b7le", "dei\u00b7ne", "Ei\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Suchen werd' ich: werd' ich finden?", "tokens": ["Su\u00b7chen", "werd'", "ich", ":", "werd'", "ich", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$.", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Ferne Ferne,", "tokens": ["Nach", "der", "Fer\u00b7ne", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Treibt das Herz; durch bl\u00fchnde Linden", "tokens": ["Treibt", "das", "Herz", ";", "durch", "bl\u00fchn\u00b7de", "Lin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4cheln dir die Sterne.", "tokens": ["L\u00e4\u00b7cheln", "dir", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}