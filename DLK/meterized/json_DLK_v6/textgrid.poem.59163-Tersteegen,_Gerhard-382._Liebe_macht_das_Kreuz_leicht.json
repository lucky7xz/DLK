{"textgrid.poem.59163": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "382. Liebe macht das Kreuz leicht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwahr, die Liebe f\u00fcrchtet nicht", "tokens": ["F\u00fcr\u00b7wahr", ",", "die", "Lie\u00b7be", "f\u00fcrch\u00b7tet", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Arbeit, M\u00fche und Beschwerden;", "tokens": ["Viel", "Ar\u00b7beit", ",", "M\u00fc\u00b7he", "und", "Be\u00b7schwer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob gleich das Kreuz so sauer sicht (sieht),", "tokens": ["Ob", "gleich", "das", "Kreuz", "so", "sau\u00b7er", "sicht", "(", "sieht", ")", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$(", "VVFIN", "$(", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Liebe kann's doch nimmer l\u00e4stig werden.", "tokens": ["Der", "Lie\u00b7be", "kann's", "doch", "nim\u00b7mer", "l\u00e4s\u00b7tig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}