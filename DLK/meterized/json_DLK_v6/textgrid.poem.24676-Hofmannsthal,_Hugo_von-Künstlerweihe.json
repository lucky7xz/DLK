{"textgrid.poem.24676": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstlerweihe", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wandern stumm, versch\u00fcchtert, bang geb\u00fcckt,", "tokens": ["Wir", "wan\u00b7dern", "stumm", ",", "ver\u00b7sch\u00fcch\u00b7tert", ",", "bang", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bergen scheu, was wir im Herzen hegen,", "tokens": ["Und", "ber\u00b7gen", "scheu", ",", "was", "wir", "im", "Her\u00b7zen", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reden Worte, die uns nicht bewegen,", "tokens": ["Und", "re\u00b7den", "Wor\u00b7te", ",", "die", "uns", "nicht", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tote Dinge preisen wir entz\u00fcckt.", "tokens": ["Und", "to\u00b7te", "Din\u00b7ge", "prei\u00b7sen", "wir", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Seele ist vergraben und erstickt ...", "tokens": ["Die", "See\u00b7le", "ist", "ver\u00b7gra\u00b7ben", "und", "er\u00b7stickt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verfaultes leuchtet fahl auf n\u00e4chtgen Wegen ...", "tokens": ["Ver\u00b7faul\u00b7tes", "leuch\u00b7tet", "fahl", "auf", "n\u00e4cht\u00b7gen", "We\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sind wir m\u00fcde, soll uns Kunst erregen,", "tokens": ["Und", "sind", "wir", "m\u00fc\u00b7de", ",", "soll", "uns", "Kunst", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis wir im Rausch der leeren Qual entr\u00fcckt.", "tokens": ["Bis", "wir", "im", "Rausch", "der", "lee\u00b7ren", "Qual", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "J\u00fcngst fiel mein Aug auf Meister Wolframs Buch", "tokens": ["J\u00fcngst", "fiel", "mein", "Aug", "auf", "Meis\u00b7ter", "Wolf\u00b7rams", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Parzival, und vor mir stand der Fluch,", "tokens": ["Vom", "Par\u00b7zi\u00b7val", ",", "und", "vor", "mir", "stand", "der", "Fluch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der vom verlornen Gral herniederklagt:", "tokens": ["Der", "vom", "ver\u00b7lor\u00b7nen", "Gral", "her\u00b7nie\u00b7der\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbunseliger, was hast du nicht gefragt?!\u00ab", "tokens": ["\u00bb", "un\u00b7se\u00b7li\u00b7ger", ",", "was", "hast", "du", "nicht", "ge\u00b7fragt", "?!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$.", "$("], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "In Mitleid ahnend stumme Qual befreie:", "tokens": ["In", "Mit\u00b7leid", "ah\u00b7nend", "stum\u00b7me", "Qual", "be\u00b7frei\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist die einzig wahre K\u00fcnstlerweihe!", "tokens": ["Das", "ist", "die", "ein\u00b7zig", "wah\u00b7re", "K\u00fcnst\u00b7ler\u00b7wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}