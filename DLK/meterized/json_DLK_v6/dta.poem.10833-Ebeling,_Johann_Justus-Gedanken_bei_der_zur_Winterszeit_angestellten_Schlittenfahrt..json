{"dta.poem.10833": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n bei  \n der zur Winterszeit angestellten  \n Schlittenfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah ein Masken-Volk in kalten Win-\nter brennen,", "tokens": ["Ich", "sah", "ein", "Mas\u00b7ken\u00b7Volk", "in", "kal\u00b7ten", "Win", "ter", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "TRUNC", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bei der Schlittenfahrt durch Stadt", "tokens": ["Und", "bei", "der", "Schlit\u00b7ten\u00b7fahrt", "durch", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es glimmte wie ein Feur, die aufgeglom-", "tokens": ["Es", "glimm\u00b7te", "wie", "ein", "Feur", ",", "die", "auf\u00b7ge\u00b7glom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In der, durch Schnee und Eis und Frost erstarrten", "tokens": ["In", "der", ",", "durch", "Schnee", "und", "Eis", "und", "Frost", "er\u00b7starr\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie froren wie man sah. Die lustigen Anstal-", "tokens": ["Sie", "fro\u00b7ren", "wie", "man", "sah", ".", "Die", "lus\u00b7ti\u00b7gen", "An\u00b7sta\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PIS", "VVFIN", "$.", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die st\u00e4rkten ihren Muth die K\u00e4lte auszuhal-", "tokens": ["Die", "st\u00e4rk\u00b7ten", "ih\u00b7ren", "Muth", "die", "K\u00e4l\u00b7te", "aus\u00b7zu\u00b7ha\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Es starrten Hand und Fu\u00df, bei ihrem eitlen", "tokens": ["Es", "starr\u00b7ten", "Hand", "und", "Fu\u00df", ",", "bei", "ih\u00b7rem", "eit\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was wirkte ihre Lust? ein schmerzliches Ge-", "tokens": ["Was", "wirk\u00b7te", "ih\u00b7re", "Lust", "?", "ein", "schmerz\u00b7li\u00b7ches", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es fiel mir dabei ein: Nun werden sie noch in-", "tokens": ["Es", "fiel", "mir", "da\u00b7bei", "ein", ":", "Nun", "wer\u00b7den", "sie", "noch", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie eitel ihre Fahrt, wie th\u00f6rigt ihr Begin-", "tokens": ["Wie", "ei\u00b7tel", "ih\u00b7re", "Fahrt", ",", "wie", "th\u00f6\u00b7rigt", "ihr", "Be\u00b7gin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So albern ist der Mensch, und sein verdorbnes", "tokens": ["So", "al\u00b7bern", "ist", "der", "Mensch", ",", "und", "sein", "ver\u00b7dorb\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sucht in der Unlust Lust, und Freude in dem", "tokens": ["Sucht", "in", "der", "Un\u00b7lust", "Lust", ",", "und", "Freu\u00b7de", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,", "KON", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}