{"textgrid.poem.33904": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Opfer des Fr\u00fchlings", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sah ich je ein Blau, wie droben", "tokens": ["Sah", "ich", "je", "ein", "Blau", ",", "wie", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klar und voll den Himmel schm\u00fcckt?", "tokens": ["Klar", "und", "voll", "den", "Him\u00b7mel", "schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht in Augen, sanft gehoben,", "tokens": ["Nicht", "in", "Au\u00b7gen", ",", "sanft", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht in Veilchen, still geb\u00fcckt!", "tokens": ["Nicht", "in", "Veil\u00b7chen", ",", "still", "ge\u00b7b\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leiser scheint der Flu\u00df zu wallen", "tokens": ["Lei\u00b7ser", "scheint", "der", "Flu\u00df", "zu", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter seinem Widerschein,", "tokens": ["Un\u00b7ter", "sei\u00b7nem", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "V\u00f6gel schweigen, und vor Allen", "tokens": ["V\u00f6\u00b7gel", "schwei\u00b7gen", ",", "und", "vor", "Al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "D\u00e4mmert meine Seele ein.", "tokens": ["D\u00e4m\u00b7mert", "mei\u00b7ne", "See\u00b7le", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch, es gilt auch eine Feier!", "tokens": ["Doch", ",", "es", "gilt", "auch", "ei\u00b7ne", "Fei\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut den Lenz im Morgenglanz!", "tokens": ["Schaut", "den", "Lenz", "im", "Mor\u00b7gen\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter grauer Nebel Schleier", "tokens": ["Hin\u00b7ter", "grau\u00b7er", "Ne\u00b7bel", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flocht der J\u00fcngling sich den Kranz.", "tokens": ["Flocht", "der", "J\u00fcng\u00b7ling", "sich", "den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sein Hauch, die Nebel theilend,", "tokens": ["Wenn", "sein", "Hauch", ",", "die", "Ne\u00b7bel", "thei\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn zu fr\u00fch' schon halb verrieth,", "tokens": ["Ihn", "zu", "fr\u00fch'", "schon", "halb", "ver\u00b7rieth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wich er scheu zur\u00fcck, enteilend", "tokens": ["Wich", "er", "scheu", "zu\u00b7r\u00fcck", ",", "ent\u00b7ei\u00b7lend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In ein dunkleres Gebiet.", "tokens": ["In", "ein", "dunk\u00b7le\u00b7res", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dennoch steh'n, ihn zu empfangen,", "tokens": ["Den\u00b7noch", "steh'n", ",", "ihn", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Kinder schon bereit:", "tokens": ["Sei\u00b7ne", "Kin\u00b7der", "schon", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rose mit den hei\u00dfen Wangen,", "tokens": ["Ro\u00b7se", "mit", "den", "hei\u00b7\u00dfen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mandelbaum im wei\u00dfen Kleid!", "tokens": ["Man\u00b7del\u00b7baum", "im", "wei\u00b7\u00dfen", "Kleid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Veilchen, die des Sommers Br\u00fcten", "tokens": ["Veil\u00b7chen", ",", "die", "des", "Som\u00b7mers", "Br\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald erstickt, sie harren auch,", "tokens": ["Bald", "er\u00b7stickt", ",", "sie", "har\u00b7ren", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keusche Lorbeern selbst ergl\u00fchten;", "tokens": ["Keu\u00b7sche", "Lor\u00b7beern", "selbst", "er\u00b7gl\u00fch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn sie Alle traf sein Hauch.", "tokens": ["Denn", "sie", "Al\u00b7le", "traf", "sein", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun, mit fast versch\u00e4mtem L\u00e4cheln,", "tokens": ["Nun", ",", "mit", "fast", "ver\u00b7sch\u00e4m\u00b7tem", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht er ein in's sch\u00f6ne Reich;", "tokens": ["Zieht", "er", "ein", "in's", "sch\u00f6\u00b7ne", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm die gl\u00fch'nde Stirn zu f\u00e4cheln,", "tokens": ["Ihm", "die", "gl\u00fch'n\u00b7de", "Stirn", "zu", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nah'n die Morgenwinde gleich.", "tokens": ["Nah'n", "die", "Mor\u00b7gen\u00b7win\u00b7de", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, ihn selber k\u00fchlend, stehlen", "tokens": ["Doch", ",", "ihn", "sel\u00b7ber", "k\u00fch\u00b7lend", ",", "steh\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PPER", "ADV", "ADJD", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie so viel der holden Glut,", "tokens": ["Sie", "so", "viel", "der", "hol\u00b7den", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als, die Blumen, die noch fehlen,", "tokens": ["Als", ",", "die", "Blu\u00b7men", ",", "die", "noch", "feh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu erwecken, n\u00f6thig thut.", "tokens": ["Zu", "er\u00b7we\u00b7cken", ",", "n\u00f6\u00b7thig", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Flugs nun auf den leichten Schwingen", "tokens": ["Flugs", "nun", "auf", "den", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilen sie durch Hain und Thal,", "tokens": ["Ei\u00b7len", "sie", "durch", "Hain", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor ihren K\u00fcssen springen", "tokens": ["Und", "vor", "ih\u00b7ren", "K\u00fcs\u00b7sen", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spr\u00f6de Knospen ohne Zahl.", "tokens": ["Spr\u00f6\u00b7de", "Knos\u00b7pen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Busch, wie sie ihn streifen,", "tokens": ["Je\u00b7der", "Busch", ",", "wie", "sie", "ihn", "strei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird zum bunten Bl\u00fctenstrau\u00df,", "tokens": ["Wird", "zum", "bun\u00b7ten", "Bl\u00fc\u00b7ten\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Wurzeln, die noch steifen,", "tokens": ["Und", "die", "Wur\u00b7zeln", ",", "die", "noch", "stei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Treiben erstes Gr\u00fcn heraus.", "tokens": ["Trei\u00b7ben", "ers\u00b7tes", "Gr\u00fcn", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch nun l\u00f6s't sich, alle Farben", "tokens": ["Doch", "nun", "l\u00f6s't", "sich", ",", "al\u00b7le", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu erh\u00f6h'n und allen Duft,", "tokens": ["Zu", "er\u00b7h\u00f6h'n", "und", "al\u00b7len", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das verschluckte Licht in Garben", "tokens": ["Das", "ver\u00b7schluck\u00b7te", "Licht", "in", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reinen Goldes aus der Luft.", "tokens": ["Rei\u00b7nen", "Gol\u00b7des", "aus", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind das Stralen? Sind das Sterne,", "tokens": ["Sind", "das", "Stra\u00b7len", "?", "Sind", "das", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$.", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der Tag in Flammen schmolz?", "tokens": ["Die", "der", "Tag", "in", "Flam\u00b7men", "schmolz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Alles funkelt, nah' und ferne,", "tokens": ["Al\u00b7les", "fun\u00b7kelt", ",", "nah'", "und", "fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Berg und Wald, ja Stein und Holz!", "tokens": ["Berg", "und", "Wald", ",", "ja", "Stein", "und", "Holz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Horcht! Vor diesem Glanze fahren", "tokens": ["Horcht", "!", "Vor", "die\u00b7sem", "Glan\u00b7ze", "fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die V\u00f6gel aus dem Traum,", "tokens": ["Auch", "die", "V\u00f6\u00b7gel", "aus", "dem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D'rin sie still versunken waren,", "tokens": ["D'\u00b7rin", "sie", "still", "ver\u00b7sun\u00b7ken", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder auf im blauen Raum;", "tokens": ["Wie\u00b7der", "auf", "im", "blau\u00b7en", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber dick und rauchend steigen", "tokens": ["A\u00b7ber", "dick", "und", "rau\u00b7chend", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wolken hei\u00dfen Dufts empor,", "tokens": ["Wol\u00b7ken", "hei\u00b7\u00dfen", "Dufts", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nun f\u00e4llt in's dumpfe Schweigen", "tokens": ["Und", "nun", "f\u00e4llt", "in's", "dump\u00b7fe", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neu bet\u00e4ubt zur\u00fcck ihr Chor.", "tokens": ["Neu", "be\u00b7t\u00e4ubt", "zu\u00b7r\u00fcck", "ihr", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "F\u00fcrder, immer f\u00fcrder schreitend,", "tokens": ["F\u00fcr\u00b7der", ",", "im\u00b7mer", "f\u00fcr\u00b7der", "schrei\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt der J\u00fcngling an den Flu\u00df,", "tokens": ["Kommt", "der", "J\u00fcng\u00b7ling", "an", "den", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, sich rings in's Land verbreitend,", "tokens": ["Der", ",", "sich", "rings", "in's", "Land", "ver\u00b7brei\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles tr\u00e4nkt, was trinken mu\u00df.", "tokens": ["Al\u00b7les", "tr\u00e4nkt", ",", "was", "trin\u00b7ken", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber heute m\u00f6ge d\u00fcrsten,", "tokens": ["A\u00b7ber", "heu\u00b7te", "m\u00f6\u00b7ge", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was da will, er h\u00e4lt sich an", "tokens": ["Was", "da", "will", ",", "er", "h\u00e4lt", "sich", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "$,", "PPER", "VVFIN", "PRF", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und versucht, ob er den F\u00fcrsten", "tokens": ["Und", "ver\u00b7sucht", ",", "ob", "er", "den", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch sein Bild nicht fesseln kann.", "tokens": ["Durch", "sein", "Bild", "nicht", "fes\u00b7seln", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Denn, wenn dieser, s\u00fc\u00df betroffen,", "tokens": ["Denn", ",", "wenn", "die\u00b7ser", ",", "s\u00fc\u00df", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PDS", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier sich selbst im Spiegel schaut,", "tokens": ["Hier", "sich", "selbst", "im", "Spie\u00b7gel", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00f6nt sein Blick das leise Hoffen,", "tokens": ["Kr\u00f6nt", "sein", "Blick", "das", "lei\u00b7se", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem die Welle still vertraut;", "tokens": ["Dem", "die", "Wel\u00b7le", "still", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei er noch so schnell und fl\u00fcchtig,", "tokens": ["Sei", "er", "noch", "so", "schnell", "und", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jene Lilje wird geweckt,", "tokens": ["Je\u00b7ne", "Lil\u00b7je", "wird", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die, wie keine, keusch und z\u00fcchtig,", "tokens": ["Die", ",", "wie", "kei\u00b7ne", ",", "keusch", "und", "z\u00fcch\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PIAT", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich in ihren Schoo\u00df versteckt.", "tokens": ["Sich", "in", "ih\u00b7ren", "Schoo\u00df", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wie sollte er nicht s\u00e4umen?", "tokens": ["Und", "wie", "soll\u00b7te", "er", "nicht", "s\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sieht er denn sich selber nur?", "tokens": ["Sieht", "er", "denn", "sich", "sel\u00b7ber", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zugleich, die seinen Tr\u00e4umen", "tokens": ["Nicht", "zu\u00b7gleich", ",", "die", "sei\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben gab, die bl\u00fch'nde Flur?", "tokens": ["Le\u00b7ben", "gab", ",", "die", "bl\u00fch'n\u00b7de", "Flur", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn's ihn auch vor\u00fcber triebe", "tokens": ["Wenn's", "ihn", "auch", "vor\u00b7\u00fc\u00b7ber", "trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An der eig'nen Huldgestalt,", "tokens": ["An", "der", "eig'\u00b7nen", "Huld\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fesselte ihn doch die Liebe", "tokens": ["Fes\u00b7sel\u00b7te", "ihn", "doch", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "An die Braut mit Allgewalt.", "tokens": ["An", "die", "Braut", "mit", "All\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, er z\u00f6gert wonnetrunken!", "tokens": ["Ach", ",", "er", "z\u00f6\u00b7gert", "won\u00b7ne\u00b7trun\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber lange bleibt er nicht", "tokens": ["A\u00b7ber", "lan\u00b7ge", "bleibt", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den s\u00fc\u00dfen Rausch versunken,", "tokens": ["In", "den", "s\u00fc\u00b7\u00dfen", "Rausch", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, er wendet das Gesicht!", "tokens": ["Nein", ",", "er", "wen\u00b7det", "das", "Ge\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ihm sagt ein inn'res Stocken,", "tokens": ["Denn", "ihm", "sagt", "ein", "inn'\u00b7res", "Sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die G\u00f6tter neidisch sind,", "tokens": ["Da\u00df", "die", "G\u00f6t\u00b7ter", "nei\u00b7disch", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihm d\u00e4ucht, mit seinen Locken", "tokens": ["Und", "ihm", "d\u00e4ucht", ",", "mit", "sei\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spiele schon ein and'rer Wind.", "tokens": ["Spie\u00b7le", "schon", "ein", "an\u00b7d'\u00b7rer", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Da beschleicht ihn dumpfe Trauer,", "tokens": ["Da", "be\u00b7schleicht", "ihn", "dump\u00b7fe", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm erlischt der Wange Roth,", "tokens": ["Ihm", "er\u00b7lischt", "der", "Wan\u00b7ge", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihn mahnt ein kalter Schauer", "tokens": ["Und", "ihn", "mahnt", "ein", "kal\u00b7ter", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Tod, den fr\u00fchen Tod;", "tokens": ["An", "den", "Tod", ",", "den", "fr\u00fc\u00b7hen", "Tod", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, von dem durchzuckt, entzittert,", "tokens": ["Doch", ",", "von", "dem", "durch\u00b7zuckt", ",", "ent\u00b7zit\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie von selbst, sein Kranz dem Haar,", "tokens": ["Wie", "von", "selbst", ",", "sein", "Kranz", "dem", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADV", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der die Ew'gen ihm erbittert,", "tokens": ["Der", "die", "Ew'\u00b7gen", "ihm", "er\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sein Fu\u00df zertritt ihn gar.", "tokens": ["Und", "sein", "Fu\u00df", "zer\u00b7tritt", "ihn", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Pl\u00f6tzlich Stille jetzt! Die Winde", "tokens": ["Pl\u00f6tz\u00b7lich", "Stil\u00b7le", "jetzt", "!", "Die", "Win\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh'n, wie auf ein Zauberwort,", "tokens": ["Ruh'n", ",", "wie", "auf", "ein", "Zau\u00b7ber\u00b7wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in jedem Fr\u00fchlingskinde", "tokens": ["Doch", "in", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bebt der Todesschauer fort,", "tokens": ["Bebt", "der", "To\u00b7des\u00b7schau\u00b7er", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein hast'ger Bl\u00fcten-Regen", "tokens": ["Und", "ein", "hast'\u00b7ger", "Bl\u00fc\u00b7ten\u00b7Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Macht das duft'ge Opfer voll,", "tokens": ["Macht", "das", "duft'\u00b7ge", "Op\u00b7fer", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das verhalt'nen Fluch in Segen,", "tokens": ["Das", "ver\u00b7halt'\u00b7nen", "Fluch", "in", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ha\u00df in Liebe wandeln soll.", "tokens": ["Ha\u00df", "in", "Lie\u00b7be", "wan\u00b7deln", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Aber nun den stolzen Wipfel", "tokens": ["A\u00b7ber", "nun", "den", "stol\u00b7zen", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Baum zur Erde neigt,", "tokens": ["Je\u00b7der", "Baum", "zur", "Er\u00b7de", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun auf hohem Berges-Gipfel", "tokens": ["Nun", "auf", "ho\u00b7hem", "Ber\u00b7ge\u00b7sGip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst der K\u00fchnste Demuth zeigt,", "tokens": ["Selbst", "der", "K\u00fchns\u00b7te", "De\u00b7muth", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun erhebt der J\u00fcngling wieder", "tokens": ["Nun", "er\u00b7hebt", "der", "J\u00fcng\u00b7ling", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanft das Haupt, das er gesenkt,", "tokens": ["Sanft", "das", "Haupt", ",", "das", "er", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Oelblatt s\u00e4uselt nieder,", "tokens": ["Und", "ein", "O\u00b7el\u00b7blatt", "s\u00e4u\u00b7selt", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das vers\u00f6hnt der Neid ihm schenkt.", "tokens": ["Das", "ver\u00b7s\u00f6hnt", "der", "Neid", "ihm", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}