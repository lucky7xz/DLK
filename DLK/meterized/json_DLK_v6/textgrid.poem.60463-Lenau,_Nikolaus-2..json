{"textgrid.poem.60463": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling, sch\u00f6nster Held auf Erden!", "tokens": ["Fr\u00fch\u00b7ling", ",", "sch\u00f6ns\u00b7ter", "Held", "auf", "Er\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wonniglich sind deine Kriege", "tokens": ["Won\u00b7nig\u00b7lich", "sind", "dei\u00b7ne", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen starre Todesm\u00e4chte,", "tokens": ["Ge\u00b7gen", "star\u00b7re", "To\u00b7des\u00b7m\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie holdselig deine Siege!", "tokens": ["Wie", "hold\u00b7se\u00b7lig", "dei\u00b7ne", "Sie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieh, dort kommt ein Held, ein rauher,", "tokens": ["Sieh", ",", "dort", "kommt", "ein", "Held", ",", "ein", "rau\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Walde zugeritten,", "tokens": ["Dei\u00b7nem", "Wal\u00b7de", "zu\u00b7ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig tanzt der Staub zum Himmel", "tokens": ["Freu\u00b7dig", "tanzt", "der", "Staub", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber seines Rosses Tritten.", "tokens": ["\u00dc\u00b7ber", "sei\u00b7nes", "Ros\u00b7ses", "Trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hei\u00dfe festlich ihn willkommen,", "tokens": ["Hei\u00b7\u00dfe", "fest\u00b7lich", "ihn", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenz, in deinen gr\u00fcnen Hallen,", "tokens": ["Lenz", ",", "in", "dei\u00b7nen", "gr\u00fc\u00b7nen", "Hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df ihm deine reinste Quelle", "tokens": ["La\u00df", "ihm", "dei\u00b7ne", "reins\u00b7te", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Huldigend zu F\u00fc\u00dfen fallen;", "tokens": ["Hul\u00b7di\u00b7gend", "zu", "F\u00fc\u00b7\u00dfen", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sprenge Duft aus Blumenkelchen,", "tokens": ["Spren\u00b7ge", "Duft", "aus", "Blu\u00b7men\u00b7kel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchre deine s\u00fc\u00dfen Fl\u00f6ten", "tokens": ["R\u00fch\u00b7re", "dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und entz\u00fcnde Freudenfackeln,", "tokens": ["Und", "ent\u00b7z\u00fcn\u00b7de", "Freu\u00b7den\u00b7fa\u00b7ckeln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pappeln an den Abendr\u00f6ten;", "tokens": ["Pap\u00b7peln", "an", "den", "A\u00b7bend\u00b7r\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bette Moos f\u00fcr seine Mannen,", "tokens": ["Bet\u00b7te", "Moos", "f\u00fcr", "sei\u00b7ne", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4nk und f\u00fcttre seine Rosse;", "tokens": ["Tr\u00e4nk", "und", "f\u00fctt\u00b7re", "sei\u00b7ne", "Ros\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Held, den du bewirtest,", "tokens": ["Denn", "der", "Held", ",", "den", "du", "be\u00b7wir\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchling, ist dein Stammgenosse. \u2013", "tokens": ["Fr\u00fch\u00b7ling", ",", "ist", "dein", "Stamm\u00b7ge\u00b7nos\u00b7se", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In die Buche holden Namen", "tokens": ["In", "die", "Bu\u00b7che", "hol\u00b7den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritzte hier verliebtes H\u00e4rmen,", "tokens": ["Ritz\u00b7te", "hier", "ver\u00b7lieb\u00b7tes", "H\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihn Bl\u00fctenhauche k\u00fcssen", "tokens": ["Da\u00df", "ihn", "Bl\u00fc\u00b7ten\u00b7hau\u00b7che", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die V\u00f6glein ihn umschw\u00e4rmen;", "tokens": ["Und", "die", "V\u00f6\u00b7glein", "ihn", "um\u00b7schw\u00e4r\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ziska will den Namen \u203aFreiheit\u2039,", "tokens": ["Zis\u00b7ka", "will", "den", "Na\u00b7men", "\u203a", "Frei\u00b7heit", "\u2039", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "$(", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sein Herz zu Taten schwellt,", "tokens": ["Der", "sein", "Herz", "zu", "Ta\u00b7ten", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief mit seinem Heldendegen", "tokens": ["Tief", "mit", "sei\u00b7nem", "Hel\u00b7den\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schneiden in das Mark der Welt.", "tokens": ["Schnei\u00b7den", "in", "das", "Mark", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seine Brautfahrt gilt der Freiheit,", "tokens": ["Sei\u00b7ne", "Braut\u00b7fahrt", "gilt", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rache ist die starre R\u00fcstung,", "tokens": ["Ra\u00b7che", "ist", "die", "star\u00b7re", "R\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die er tr\u00e4gt auf seinem Gange,", "tokens": ["Die", "er", "tr\u00e4gt", "auf", "sei\u00b7nem", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Werbung hei\u00dft Verw\u00fcstung.", "tokens": ["Sei\u00b7ne", "Wer\u00b7bung", "hei\u00dft", "Ver\u00b7w\u00fcs\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ziska bringt als Morgengabe", "tokens": ["Zis\u00b7ka", "bringt", "als", "Mor\u00b7gen\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Leichenschatz ihr dar,", "tokens": ["Sei\u00b7nen", "Lei\u00b7chen\u00b7schatz", "ihr", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hussens Schatten sei der Priester,", "tokens": ["Hus\u00b7sens", "Schat\u00b7ten", "sei", "der", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammen bauen den Altar.", "tokens": ["Flam\u00b7men", "bau\u00b7en", "den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fr\u00fchling, sieh, von seinem Rappen", "tokens": ["Fr\u00fch\u00b7ling", ",", "sieh", ",", "von", "sei\u00b7nem", "Rap\u00b7pen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Wilde sich geschwungen,", "tokens": ["Hat", "der", "Wil\u00b7de", "sich", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sucht ein kurzes Schlummern", "tokens": ["Und", "er", "sucht", "ein", "kur\u00b7zes", "Schlum\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Waldes D\u00e4mmerungen.", "tokens": ["In", "des", "Wal\u00b7des", "D\u00e4m\u00b7me\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Seine Krieger rings am Boden", "tokens": ["Sei\u00b7ne", "Krie\u00b7ger", "rings", "am", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben sich um ihn gelagert,", "tokens": ["Ha\u00b7ben", "sich", "um", "ihn", "ge\u00b7la\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gierig weiden schon die Rosse,", "tokens": ["Gie\u00b7rig", "wei\u00b7den", "schon", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcd, vom Schlachtenritt gemagert.", "tokens": ["M\u00fcd", ",", "vom", "Schlach\u00b7ten\u00b7ritt", "ge\u00b7ma\u00b7gert", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Mahlzeit halten die Hussiten", "tokens": ["Mahl\u00b7zeit", "hal\u00b7ten", "die", "Hus\u00b7si\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00f6hlich in der Abendk\u00fchle,", "tokens": ["Fr\u00f6h\u00b7lich", "in", "der", "A\u00b7bend\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es versinken ihre Panzer", "tokens": ["Es", "ver\u00b7sin\u00b7ken", "ih\u00b7re", "Pan\u00b7zer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Mooses weiche Pf\u00fchle.", "tokens": ["In", "des", "Moo\u00b7ses", "wei\u00b7che", "Pf\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "V\u00f6gel singen durch die Schatten,", "tokens": ["V\u00f6\u00b7gel", "sin\u00b7gen", "durch", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Locken Schlummer auf die Wimpern,", "tokens": ["Lo\u00b7cken", "Schlum\u00b7mer", "auf", "die", "Wim\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und melodisch s\u00e4uselnd, rauschend,", "tokens": ["Und", "me\u00b7lo\u00b7disch", "s\u00e4u\u00b7selnd", ",", "rau\u00b7schend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Gezweig die L\u00fcfte klimpern.", "tokens": ["Im", "Ge\u00b7zweig", "die", "L\u00fcf\u00b7te", "klim\u00b7pern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ziskas Auge blicket schl\u00e4frig", "tokens": ["Zis\u00b7kas", "Au\u00b7ge", "bli\u00b7cket", "schl\u00e4f\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durchs Entspinnen eines Traumes", "tokens": ["Durchs", "Ent\u00b7spin\u00b7nen", "ei\u00b7nes", "Trau\u00b7mes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem abendroten Stamme", "tokens": ["Nach", "dem", "a\u00b7ben\u00b7dro\u00b7ten", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort des alten Eichenbaumes;", "tokens": ["Dort", "des", "al\u00b7ten", "Ei\u00b7chen\u00b7bau\u00b7mes", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Zweifelnd mischen Aug und Seele", "tokens": ["Zwei\u00b7felnd", "mi\u00b7schen", "Aug", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihren Blick in eins zusammen:", "tokens": ["Ih\u00b7ren", "Blick", "in", "eins", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ists die Sonne? ists ein Blutstrom?", "tokens": ["Ist