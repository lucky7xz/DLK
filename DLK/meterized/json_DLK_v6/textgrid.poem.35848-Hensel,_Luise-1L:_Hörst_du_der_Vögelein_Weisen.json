{"textgrid.poem.35848": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rst du der V\u00f6gelein Weisen", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du der V\u00f6gelein Weisen", "tokens": ["H\u00f6rst", "du", "der", "V\u00f6\u00b7ge\u00b7lein", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Lieblich im gr\u00fcnenden Hain", "tokens": ["Lieb\u00b7lich", "im", "gr\u00fc\u00b7nen\u00b7den", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gott, den Allg\u00fctigen, preisen:", "tokens": ["Gott", ",", "den", "All\u00b7g\u00fc\u00b7ti\u00b7gen", ",", "prei\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Seele, o stimme mit ein!", "tokens": ["See\u00b7le", ",", "o", "stim\u00b7me", "mit", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "APPR", "ART", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Gr\u00fc\u00dft dich der farbige Bogen", "tokens": ["Gr\u00fc\u00dft", "dich", "der", "far\u00b7bi\u00b7ge", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Hoch dort vom Himmel herab:", "tokens": ["Hoch", "dort", "vom", "Him\u00b7mel", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Preise Ihn, der ihn gezogen,", "tokens": ["Prei\u00b7se", "Ihn", ",", "der", "ihn", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so viel Sch\u00f6nes dir gab!", "tokens": ["Der", "so", "viel", "Sch\u00f6\u00b7nes", "dir", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Winkt dir der Obstb\u00e4ume Segen,", "tokens": ["Winkt", "dir", "der", "Obst\u00b7b\u00e4u\u00b7me", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wogen dir nahe und fern", "tokens": ["Wo\u00b7gen", "dir", "na\u00b7he", "und", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Goldene Saaten entgegen:", "tokens": ["Gol\u00b7de\u00b7ne", "Saa\u00b7ten", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Danke dann freudig dem Herrn!", "tokens": ["Dan\u00b7ke", "dann", "freu\u00b7dig", "dem", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Siehst du die Sternlein ergl\u00fchen", "tokens": ["Siehst", "du", "die", "Stern\u00b7lein", "er\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zahllos und flimmernd und rein,", "tokens": ["Zahl\u00b7los", "und", "flim\u00b7mernd", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Friedlich die Mondsichel ziehen:", "tokens": ["Fried\u00b7lich", "die", "Mond\u00b7si\u00b7chel", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Seele, o denke dann Sein!", "tokens": ["See\u00b7le", ",", "o", "den\u00b7ke", "dann", "Sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schwinge dich auf aus dem Staube,", "tokens": ["Schwin\u00b7ge", "dich", "auf", "aus", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der dich belastend umgiebt;", "tokens": ["Der", "dich", "be\u00b7las\u00b7tend", "um\u00b7giebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Liebe und hoffe und glaube,", "tokens": ["Lie\u00b7be", "und", "hof\u00b7fe", "und", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da\u00df Gott als Vater dich liebt.", "tokens": ["Da\u00df", "Gott", "als", "Va\u00b7ter", "dich", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}