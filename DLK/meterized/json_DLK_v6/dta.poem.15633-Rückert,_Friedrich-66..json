{"dta.poem.15633": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Philosophie, wenn sie an der Religion", "tokens": ["Phi\u00b7lo\u00b7so\u00b7phie", ",", "wenn", "sie", "an", "der", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geheimnis r\u00fchrt, zergeht es oder sie davon;", "tokens": ["Ge\u00b7heim\u00b7nis", "r\u00fchrt", ",", "zer\u00b7geht", "es", "o\u00b7der", "sie", "da\u00b7von", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PPER", "KON", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob es begreiflich werd', ob unbegreiflich sie,", "tokens": ["Ob", "es", "be\u00b7greif\u00b7lich", "werd'", ",", "ob", "un\u00b7be\u00b7greif\u00b7lich", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob es zum Mythos, ob sie zur Mythologie.", "tokens": ["Ob", "es", "zum", "My\u00b7thos", ",", "ob", "sie", "zur", "My\u00b7tho\u00b7lo\u00b7gie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}