{"dta.poem.8687": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Zweifler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Freunde traten schweigend ein", "tokens": ["Zwei", "Freun\u00b7de", "tra\u00b7ten", "schwei\u00b7gend", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einen bl\u00fcthenvollen Hain.", "tokens": ["In", "ei\u00b7nen", "bl\u00fc\u00b7then\u00b7vol\u00b7len", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne lie\u00df den Strahl im Neigen", "tokens": ["Die", "Son\u00b7ne", "lie\u00df", "den", "Strahl", "im", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erzittern auf den Erlenzweigen;", "tokens": ["Er\u00b7zit\u00b7tern", "auf", "den", "Er\u00b7len\u00b7zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und Leben, Lieben \u00fcberall", "tokens": ["Und", "Le\u00b7ben", ",", "Lie\u00b7ben", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schien schwellend sich hervorzudr\u00e4ngen;", "tokens": ["Schien", "schwel\u00b7lend", "sich", "her\u00b7vor\u00b7zu\u00b7dr\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus B\u00fcschen ruft die Nachtigall", "tokens": ["Aus", "B\u00fc\u00b7schen", "ruft", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hervor in schmerzlich s\u00fc\u00dfen Kl\u00e4ngen,", "tokens": ["Her\u00b7vor", "in", "schmerz\u00b7lich", "s\u00fc\u00b7\u00dfen", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als ob die S\u00e4ngerin aus Eden", "tokens": ["Als", "ob", "die", "S\u00e4n\u00b7ge\u00b7rin", "aus", "E\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Tod sanft m\u00f6chte \u00fcberreden", "tokens": ["Den", "Tod", "sanft", "m\u00f6ch\u00b7te", "\u00fc\u00b7berr\u00b7e\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit ihrem Liede zaubervoll,", "tokens": ["Mit", "ih\u00b7rem", "Lie\u00b7de", "zau\u00b7ber\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df er den Lenz nicht rauben soll.", "tokens": ["Da\u00df", "er", "den", "Lenz", "nicht", "rau\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Freunde schwiegen, nur der Bach", "tokens": ["Die", "Freun\u00b7de", "schwie\u00b7gen", ",", "nur", "der", "Bach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In das Gefl\u00f6te murmelnd sprach;", "tokens": ["In", "das", "Ge\u00b7fl\u00f6\u00b7te", "mur\u00b7melnd", "sprach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Viel Blumen standen bunt herum", "tokens": ["Viel", "Blu\u00b7men", "stan\u00b7den", "bunt", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und wiegten ihre H\u00e4upter stumm,", "tokens": ["Und", "wieg\u00b7ten", "ih\u00b7re", "H\u00e4up\u00b7ter", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "In das geschw\u00e4tzig muntre Rauschen", "tokens": ["In", "das", "ge\u00b7schw\u00e4t\u00b7zig", "mun\u00b7tre", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Des Baches froh hinabzulauschen,", "tokens": ["Des", "Ba\u00b7ches", "froh", "hin\u00b7ab\u00b7zu\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wie Kinder lauschen, frohgespannt,", "tokens": ["Wie", "Kin\u00b7der", "lau\u00b7schen", ",", "froh\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dem Wandrer, der von fernem Land,", "tokens": ["Dem", "Wand\u00b7rer", ",", "der", "von", "fer\u00b7nem", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Von sch\u00f6nen Wundern viel erz\u00e4hlt", "tokens": ["Von", "sch\u00f6\u00b7nen", "Wun\u00b7dern", "viel", "er\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Auf seiner Irrfahrt durch die Welt. \u2014", "tokens": ["Auf", "sei\u00b7ner", "Irr\u00b7fahrt", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "O Nachtigall! du rufst vergebens", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "!", "du", "rufst", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Um Dauer dieses Wonnelebens!", "tokens": ["Um", "Dau\u00b7er", "die\u00b7ses", "Won\u00b7ne\u00b7le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Bald gl\u00fcht dein leztes Abendroth,", "tokens": ["Bald", "gl\u00fcht", "dein", "lez\u00b7tes", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "In seinem Durste wird der Tod", "tokens": ["In", "sei\u00b7nem", "Durs\u00b7te", "wird", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Hinweg die s\u00fc\u00dfen Lieder trinken,", "tokens": ["Hin\u00b7weg", "die", "s\u00fc\u00b7\u00dfen", "Lie\u00b7der", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Du wirst vom stillen Aste sinken!", "tokens": ["Du", "wirst", "vom", "stil\u00b7len", "As\u00b7te", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ihr lieben Bl\u00fcmlein! trauet nicht", "tokens": ["Ihr", "lie\u00b7ben", "Bl\u00fcm\u00b7lein", "!", "trau\u00b7et", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dem M\u00e4hrchen, das der Wandrer spricht;", "tokens": ["Dem", "M\u00e4hr\u00b7chen", ",", "das", "der", "Wand\u00b7rer", "spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Seht, seht, schon schwillt er brausend an,", "tokens": ["Seht", ",", "seht", ",", "schon", "schwillt", "er", "brau\u00b7send", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Im Walde schon die St\u00fcrme nah'n;", "tokens": ["Im", "Wal\u00b7de", "schon", "die", "St\u00fcr\u00b7me", "nah'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der Donner kommt, und voller schwillt", "tokens": ["Der", "Don\u00b7ner", "kommt", ",", "und", "vol\u00b7ler", "schwillt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der Bach, der immer lauter br\u00fcllt;", "tokens": ["Der", "Bach", ",", "der", "im\u00b7mer", "lau\u00b7ter", "br\u00fcllt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Er fa\u00dft euch an, er rei\u00dft euch los", "tokens": ["Er", "fa\u00dft", "euch", "an", ",", "er", "rei\u00dft", "euch", "los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Aus eurer Mutter gr\u00fcnem Schoos!", "tokens": ["Aus", "eu\u00b7rer", "Mut\u00b7ter", "gr\u00fc\u00b7nem", "Schoos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Wie dort die Rosenstaude bebt!", "tokens": ["Wie", "dort", "die", "Ro\u00b7sen\u00b7stau\u00b7de", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Nun sich zu ihr der Wilde hebt;", "tokens": ["Nun", "sich", "zu", "ihr", "der", "Wil\u00b7de", "hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PPOSAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Sie schwankt in ihrem Bl\u00fcthenkleid,", "tokens": ["Sie", "schwankt", "in", "ih\u00b7rem", "Bl\u00fc\u00b7then\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da sie der Strom frohlockend wiegt;", "tokens": ["Da", "sie", "der", "Strom", "froh\u00b7lo\u00b7ckend", "wiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "So wiegt der Bursche seine Maid',", "tokens": ["So", "wiegt", "der", "Bur\u00b7sche", "sei\u00b7ne", "Maid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Bevor mit ihr zum Tanz er fliegt.\u2014", "tokens": ["Be\u00b7vor", "mit", "ihr", "zum", "Tanz", "er", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PPOSAT", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der eine von den Freunden sann", "tokens": ["Der", "ei\u00b7ne", "von", "den", "Freun\u00b7den", "sann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Hinunter in den Wogendrang,", "tokens": ["Hin\u00b7un\u00b7ter", "in", "den", "Wo\u00b7gen\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und seine Stimme nun begann", "tokens": ["Und", "sei\u00b7ne", "Stim\u00b7me", "nun", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Zu t\u00f6nen, ernst, wie Grabgesang:", "tokens": ["Zu", "t\u00f6\u00b7nen", ",", "ernst", ",", "wie", "Grab\u00b7ge\u00b7sang", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Verg\u00e4nglichkeit! wie rauschen deine Wellen", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "wie", "rau\u00b7schen", "dei\u00b7ne", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Durch's weite Labyrinth des Lebens fort!", "tokens": ["Durch's", "wei\u00b7te", "La\u00b7by\u00b7rinth", "des", "Le\u00b7bens", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "In deine Wirbel fl\u00fcchten alle Quellen,", "tokens": ["In", "dei\u00b7ne", "Wir\u00b7bel", "fl\u00fcch\u00b7ten", "al\u00b7le", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Dir baut kein Damm entgegen sich, kein Hort!", "tokens": ["Dir", "baut", "kein", "Damm", "ent\u00b7ge\u00b7gen", "sich", ",", "kein", "Hort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PRF", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Es w\u00e4chst dein Strom mit jeglicher Minute,", "tokens": ["Es", "w\u00e4chst", "dein", "Strom", "mit", "jeg\u00b7li\u00b7cher", "Mi\u00b7nu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Stets lauter klagt der dumpfe Wellenschlag;", "tokens": ["Stets", "lau\u00b7ter", "klagt", "der", "dump\u00b7fe", "Wel\u00b7len\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Doch wie die Fluth auch unaufhaltsam fluthe,", "tokens": ["Doch", "wie", "die", "Fluth", "auch", "un\u00b7auf\u00b7halt\u00b7sam", "flut\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Ist Mancher doch, der sie nicht h\u00f6ren mag.", "tokens": ["Ist", "Man\u00b7cher", "doch", ",", "der", "sie", "nicht", "h\u00f6\u00b7ren", "mag."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "PIS", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Wenn auch die Wellen ihre Ufer fressen,", "tokens": ["Wenn", "auch", "die", "Wel\u00b7len", "ih\u00b7re", "U\u00b7fer", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Und du zum Meer hinwucherst, unermessen;", "tokens": ["Und", "du", "zum", "Meer", "hin\u00b7wuc\u00b7herst", ",", "un\u00b7er\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Doch steh'n an deinem Ufer frohe Thoren,", "tokens": ["Doch", "steh'n", "an", "dei\u00b7nem", "U\u00b7fer", "fro\u00b7he", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.58": {"text": "In ihren Traum \u201eUnsterblichkeit\u201c verloren.", "tokens": ["In", "ih\u00b7ren", "Traum", "\u201e", "U\u00b7nsterb\u00b7lich\u00b7keit", "\u201c", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "Am Ufer? \u2014 nein! es ist von deinem Bronnen", "tokens": ["Am", "U\u00b7fer", "?", "nein", "!", "es", "ist", "von", "dei\u00b7nem", "Bron\u00b7nen"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "$(", "PTKANT", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.60": {"text": "Tiefinnerst jede Creatur durchronnen;", "tokens": ["Tie\u00b7fin\u00b7nerst", "je\