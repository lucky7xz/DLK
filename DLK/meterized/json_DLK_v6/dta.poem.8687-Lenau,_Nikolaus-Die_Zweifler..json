66": {"text": "Da\u00df rettend meinen Geist sie einst empfangen:", "tokens": ["Da\u00df", "ret\u00b7tend", "mei\u00b7nen", "Geist", "sie", "einst", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.67": {"text": "Ich habe mich get\u00e4uscht! ich seh' erbleichen", "tokens": ["Ich", "ha\u00b7be", "mich", "ge\u00b7t\u00e4uscht", "!", "ich", "seh'", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$.", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.68": {"text": "Die Sterne selbst, und zitternd r\u00fcckw\u00e4rts weichen;", "tokens": ["Die", "Ster\u00b7ne", "selbst", ",", "und", "zit\u00b7ternd", "r\u00fcck\u00b7w\u00e4rts", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.69": {"text": "Sie h\u00f6ren, wie die Woge braust, sie ahnen,", "tokens": ["Sie", "h\u00f6\u00b7ren", ",", "wie", "die", "Wo\u00b7ge", "braust", ",", "sie", "ah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.70": {"text": "Da\u00df sie nicht sicher sind auf ihren Bahnen;", "tokens": ["Da\u00df", "sie", "nicht", "si\u00b7cher", "sind", "auf", "ih\u00b7ren", "Bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.71": {"text": "Sie sehen, wie es w\u00e4chst, das grause Meer,", "tokens": ["Sie", "se\u00b7hen", ",", "wie", "es", "w\u00e4chst", ",", "das", "grau\u00b7se", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.72": {"text": "Und ahnen wohl: \u2014 mir sagt's ihr banges Blinken \u2014", "tokens": ["Und", "ah\u00b7nen", "wohl", ":", "mir", "sagt's", "ihr", "ban\u00b7ges", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.73": {"text": "Einst wird vom raschen Flug ihr strahlend Heer", "tokens": ["Einst", "wird", "vom", "ra\u00b7schen", "Flug", "ihr", "strah\u00b7lend", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.74": {"text": "Ein m\u00fcdes Schwalbenvolk heruntersinken.", "tokens": ["Ein", "m\u00fc\u00b7des", "Schwal\u00b7ben\u00b7volk", "her\u00b7un\u00b7ter\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.75": {"text": "Dann br\u00fctet auf dem Ocean die Nacht,", "tokens": ["Dann", "br\u00fc\u00b7tet", "auf", "dem", "O\u00b7cean", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.76": {"text": "Dann ist des 