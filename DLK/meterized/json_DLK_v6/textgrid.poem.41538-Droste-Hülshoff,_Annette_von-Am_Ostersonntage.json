{"textgrid.poem.41538": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Ostersonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, jauchze, Welt, du hast ihn wieder,", "tokens": ["O", ",", "jauch\u00b7ze", ",", "Welt", ",", "du", "hast", "ihn", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Himmel hielt ihn nicht zur\u00fcck!", "tokens": ["Sein", "Him\u00b7mel", "hielt", "ihn", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O jauchzet! jauchzet! singet Lieder!", "tokens": ["O", "jauch\u00b7zet", "!", "jauch\u00b7zet", "!", "sin\u00b7get", "Lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dunkelst du, mein sel'ger Blick?", "tokens": ["Was", "dun\u00b7kelst", "du", ",", "mein", "sel'\u00b7ger", "Blick", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist zu viel, man kann nur weinen,", "tokens": ["Es", "ist", "zu", "viel", ",", "man", "kann", "nur", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "$,", "PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Freude steht wie Kummer da;", "tokens": ["Die", "Freu\u00b7de", "steht", "wie", "Kum\u00b7mer", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer kann so gro\u00dfer Lust sich einen,", "tokens": ["Wer", "kann", "so", "gro\u00b7\u00dfer", "Lust", "sich", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJA", "NN", "PRF", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der all so gro\u00dfe Trauer sah!", "tokens": ["Der", "all", "so", "gro\u00b7\u00dfe", "Trau\u00b7er", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Unendlich Heil hab' ich erfahren", "tokens": ["Un\u00b7end\u00b7lich", "Heil", "hab'", "ich", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ein Geheimnis voller Schmerz,", "tokens": ["Durch", "ein", "Ge\u00b7heim\u00b7nis", "vol\u00b7ler", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie es kein Menschensinn bewahren,", "tokens": ["Wie", "es", "kein", "Men\u00b7schen\u00b7sinn", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empfinden kann kein Menschenherz.", "tokens": ["Emp\u00b7fin\u00b7den", "kann", "kein", "Men\u00b7schen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vom Grabe ist mein Herr erstanden,", "tokens": ["Vom", "Gra\u00b7be", "ist", "mein", "Herr", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00fc\u00dfet alle die da sein,", "tokens": ["Und", "gr\u00fc\u00b7\u00dfet", "al\u00b7le", "die", "da", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wir sind frei von Tod und Banden,", "tokens": ["Und", "wir", "sind", "frei", "von", "Tod", "und", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der S\u00fcnde Moder rein.", "tokens": ["Und", "von", "der", "S\u00fcn\u00b7de", "Mo\u00b7der", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den eignen Leib hat er zerrissen,", "tokens": ["Den", "eig\u00b7nen", "Leib", "hat", "er", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu waschen uns mit seinem Blut,", "tokens": ["Zu", "wa\u00b7schen", "uns", "mit", "sei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer kann um dies Geheimnis wissen,", "tokens": ["Wer", "kann", "um", "dies", "Ge\u00b7heim\u00b7nis", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schmelzen nicht in Liebesglut!", "tokens": ["Und", "schmel\u00b7zen", "nicht", "in", "Lie\u00b7bes\u00b7glut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich soll mich freun an diesem Tage", "tokens": ["Ich", "soll", "mich", "freun", "an", "die\u00b7sem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner ganzen Christenheit,", "tokens": ["Mit", "dei\u00b7ner", "gan\u00b7zen", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist mir doch, als ob ich wage,", "tokens": ["Und", "ist", "mir", "doch", ",", "als", "ob", "ich", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da Unnennbares mich erfreut.", "tokens": ["Da", "Un\u00b7nenn\u00b7ba\u00b7res", "mich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit Todesqualen hat gerungen", "tokens": ["Mit", "To\u00b7des\u00b7qua\u00b7len", "hat", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seligkeit von Ewigkeit,", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "von", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich S\u00fcndern hat das Graun bezwungen", "tokens": ["Gleich", "S\u00fcn\u00b7dern", "hat", "das", "Graun", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ewige Vollkommenheit.", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Voll\u00b7kom\u00b7men\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein Gott, was konnte dich bewegen", "tokens": ["Mein", "Gott", ",", "was", "konn\u00b7te", "dich", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dieser grenzenlosen Huld!", "tokens": ["Zu", "die\u00b7ser", "gren\u00b7zen\u00b7lo\u00b7sen", "Huld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich darf nicht die Gedanken regen", "tokens": ["Ich", "darf", "nicht", "die", "Ge\u00b7dan\u00b7ken", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf unsre unerme\u00dfne Schuld.", "tokens": ["Auf", "uns\u00b7re", "un\u00b7er\u00b7me\u00df\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, sind denn aller Menschen Seelen", "tokens": ["Ach", ",", "sind", "denn", "al\u00b7ler", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl sonst ein \u00fcberk\u00f6stlich Gut,", "tokens": ["Wohl", "sonst", "ein", "\u00fc\u00b7ber\u00b7k\u00f6st\u00b7lich", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind sie es wert, da\u00df Gott sich qu\u00e4len,", "tokens": ["Sind", "sie", "es", "wert", ",", "da\u00df", "Gott", "sich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "KOUS", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ersterben mu\u00df in Angst und Glut!", "tokens": ["E\u00b7rster\u00b7ben", "mu\u00df", "in", "Angst", "und", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und sind nicht aller Menschen Seelen", "tokens": ["Und", "sind", "nicht", "al\u00b7ler", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor ihm nur eines Mundes Hauch?", "tokens": ["Vor", "ihm", "nur", "ei\u00b7nes", "Mun\u00b7des", "Hauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ganz befleckt von Schmach und Fehlen,", "tokens": ["Und", "ganz", "be\u00b7fleckt", "von", "Schmach", "und", "Feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein getr\u00fcbter dunkler Rauch?", "tokens": ["Wie", "ein", "ge\u00b7tr\u00fcb\u00b7ter", "dunk\u00b7ler", "Rauch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mein Geist, o wolle nicht ergr\u00fcnden,", "tokens": ["Mein", "Geist", ",", "o", "wol\u00b7le", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was einmal unergr\u00fcndlich ist;", "tokens": ["Was", "ein\u00b7mal", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Stein des Falles harrt des Blinden,", "tokens": ["Der", "Stein", "des", "Fal\u00b7les", "harrt", "des", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er die Wege Gottes mi\u00dft.", "tokens": ["Wenn", "er", "die", "We\u00b7ge", "Got\u00b7tes", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mein Jesus hat sie wert befunden", "tokens": ["Mein", "Je\u00b7sus", "hat", "sie", "wert", "be\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Liebe und Gerechtigkeit;", "tokens": ["In", "Lie\u00b7be", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was will ich ferner noch erkunden?", "tokens": ["Was", "will", "ich", "fer\u00b7ner", "noch", "er\u00b7kun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Wille bleibt in Ewigkeit!", "tokens": ["Sein", "Wil\u00b7le", "bleibt", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So darf ich glauben und vertrauen", "tokens": ["So", "darf", "ich", "glau\u00b7ben", "und", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meiner Seele Herrlichkeit!", "tokens": ["Auf", "mei\u00b7ner", "See\u00b7le", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So darf ich auf zum Himmel schauen,", "tokens": ["So", "darf", "ich", "auf", "zum", "Him\u00b7mel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meines Gottes \u00c4hnlichkeit!", "tokens": ["In", "mei\u00b7nes", "Got\u00b7tes", "\u00c4hn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ich soll mich freun an diesem Tage:", "tokens": ["Ich", "soll", "mich", "freun", "an", "die\u00b7sem", "Ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich freue mich, mein Jesu Christ,", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "mein", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn im Aug' ich Tr\u00e4nen trage,", "tokens": ["Und", "wenn", "im", "Aug'", "ich", "Tr\u00e4\u00b7nen", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wei\u00dft doch, da\u00df es Freude ist!", "tokens": ["Du", "wei\u00dft", "doch", ",", "da\u00df", "es", "Freu\u00b7de", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}