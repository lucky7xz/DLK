{"textgrid.poem.34947": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Messe wird man singen,", "tokens": ["Kei\u00b7ne", "Mes\u00b7se", "wird", "man", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinen Kadosch wird man sagen,", "tokens": ["Kei\u00b7nen", "Ka\u00b7dosch", "wird", "man", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts gesagt und nichts gesungen", "tokens": ["Nichts", "ge\u00b7sagt", "und", "nichts", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVPP", "KON", "PIS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird an meinen Sterbetagen.", "tokens": ["Wird", "an", "mei\u00b7nen", "Ster\u00b7be\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch vielleicht an solchem Tage,", "tokens": ["Doch", "viel\u00b7leicht", "an", "sol\u00b7chem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Wetter sch\u00f6n und milde,", "tokens": ["Wenn", "das", "Wet\u00b7ter", "sch\u00f6n", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht spazieren auf Montmartre", "tokens": ["Geht", "spa\u00b7zie\u00b7ren", "auf", "Mont\u00b7mar\u00b7tre"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mit Paulinen Frau Mathilde.", "tokens": ["Mit", "Pau\u00b7li\u00b7nen", "Frau", "Ma\u00b7thil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit dem Kranz von Immortellen", "tokens": ["Mit", "dem", "Kranz", "von", "Im\u00b7mor\u00b7tel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt sie, mir das Grab zu schm\u00fccken,", "tokens": ["Kommt", "sie", ",", "mir", "das", "Grab", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie seufzet: \u00bbPauvre homme!\u00ab", "tokens": ["Und", "sie", "seuf\u00b7zet", ":", "\u00bb", "Pauv\u00b7re", "hom\u00b7me", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feuchte Wehmut in den Blicken.", "tokens": ["Feuch\u00b7te", "Weh\u00b7mut", "in", "den", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Leider wohn ich viel zu hoch,", "tokens": ["Lei\u00b7der", "wohn", "ich", "viel", "zu", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich habe meiner S\u00fc\u00dfen", "tokens": ["Und", "ich", "ha\u00b7be", "mei\u00b7ner", "S\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Stuhl hier anzubieten;", "tokens": ["Kei\u00b7nen", "Stuhl", "hier", "an\u00b7zu\u00b7bie\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! sie schwankt mit m\u00fcden F\u00fc\u00dfen.", "tokens": ["Ach", "!", "sie", "schwankt", "mit", "m\u00fc\u00b7den", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00dfes, dickes Kind, du darfst", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "di\u00b7ckes", "Kind", ",", "du", "darfst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht zu Fu\u00df nach Hause gehen;", "tokens": ["Nicht", "zu", "Fu\u00df", "nach", "Hau\u00b7se", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Barrieregitter", "tokens": ["An", "dem", "Bar\u00b7ri\u00b7e\u00b7re\u00b7git\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehst du die Fiaker stehen.", "tokens": ["Siehst", "du", "die", "Fi\u00b7a\u00b7ker", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}