{"textgrid.poem.59706": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Herzenssaitenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was spielte so klingende Saiten", "tokens": ["Was", "spiel\u00b7te", "so", "klin\u00b7gen\u00b7de", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf dir, mein altes Herz,", "tokens": ["Auf", "dir", ",", "mein", "al\u00b7tes", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus fernsten Tiefen und Weiten", "tokens": ["Aus", "ferns\u00b7ten", "Tie\u00b7fen", "und", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zugleich mit Schmerz und Scherz?", "tokens": ["Zu\u00b7gleich", "mit", "Schmerz", "und", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es flie\u00dfen die Stunden, die R\u00e4ume", "tokens": ["Es", "flie\u00b7\u00dfen", "die", "Stun\u00b7den", ",", "die", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zusammen in dem Gewirr", "tokens": ["Zu\u00b7sam\u00b7men", "in", "dem", "Ge\u00b7wirr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Schattenspiele der Tr\u00e4ume", "tokens": ["Und", "Schat\u00b7ten\u00b7spie\u00b7le", "der", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im leichten Fl\u00fcgelgeschwirr.", "tokens": ["Im", "leich\u00b7ten", "Fl\u00fc\u00b7gel\u00b7ge\u00b7schwirr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Bald spielt es wie im Reigen", "tokens": ["Bald", "spielt", "es", "wie", "im", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hell auf zum lustigen Tanz,", "tokens": ["Hell", "auf", "zum", "lus\u00b7ti\u00b7gen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Sonn' und Bl\u00fcten neigen", "tokens": ["Und", "Sonn'", "und", "Bl\u00fc\u00b7ten", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dar\u00fcber Fr\u00fchlingsglanz;", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "Fr\u00fch\u00b7lings\u00b7glanz", ";"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bald bl\u00e4st wie \u00fcber Leichen", "tokens": ["Bald", "bl\u00e4st", "wie", "\u00fc\u00b7ber", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die tiefe Fl\u00f6te Weh,", "tokens": ["Die", "tie\u00b7fe", "Fl\u00f6\u00b7te", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie hohle T\u00f6ne streichen", "tokens": ["Wie", "hoh\u00b7le", "T\u00f6\u00b7ne", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fernher auf tiefer See.", "tokens": ["Fern\u00b7her", "auf", "tie\u00b7fer", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Das ist's, die Tiefen, die Weiten,", "tokens": ["Das", "ist's", ",", "die", "Tie\u00b7fen", ",", "die", "Wei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ist's, das meint der Klang,", "tokens": ["Das", "ist's", ",", "das", "meint", "der", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das jauchzen, das klingen die Saiten.", "tokens": ["Das", "jauch\u00b7zen", ",", "das", "klin\u00b7gen", "die", "Sai\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sei drum, mein Herz, nicht bang.", "tokens": ["Sei", "drum", ",", "mein", "Herz", ",", "nicht", "bang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Sonnen und die Erden \u2013", "tokens": ["Die", "Son\u00b7nen", "und", "die", "Er\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer misset Flug und Schritt? \u2013", "tokens": ["Wer", "mis\u00b7set", "Flug", "und", "Schritt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M\u00fcssen Flieger und T\u00e4nzer werden:", "tokens": ["M\u00fcs\u00b7sen", "Flie\u00b7ger", "und", "T\u00e4n\u00b7zer", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Du tanze lustig mit.", "tokens": ["Du", "tan\u00b7ze", "lus\u00b7tig", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und la\u00df sie spielen, die Saiten", "tokens": ["Und", "la\u00df", "sie", "spie\u00b7len", ",", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf dir, du altes Herz,", "tokens": ["Auf", "dir", ",", "du", "al\u00b7tes", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und frage nicht N\u00e4hen noch Weiten,", "tokens": ["Und", "fra\u00b7ge", "nicht", "N\u00e4\u00b7hen", "noch", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Spielt alles doch himmelw\u00e4rts.", "tokens": ["Spielt", "al\u00b7les", "doch", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.8": {"line.1": {"text": "So fliege mit tanzenden Himmeln", "tokens": ["So", "flie\u00b7ge", "mit", "tan\u00b7zen\u00b7den", "Him\u00b7meln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und glaube, die Welt ist dein;", "tokens": ["Und", "glau\u00b7be", ",", "die", "Welt", "ist", "dein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo G\u00f6tter und Sonnen sich wimmeln,", "tokens": ["Wo", "G\u00f6t\u00b7ter", "und", "Son\u00b7nen", "sich", "wim\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Rolle mit in dem Klang und Schein.", "tokens": ["Rol\u00b7le", "mit", "in", "dem", "Klang", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}