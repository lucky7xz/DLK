{"dta.poem.16269": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Ruh im Leben hat, wer Sch\u00e4tz' hat in der Truhe;", "tokens": ["Nicht", "Ruh", "im", "Le\u00b7ben", "hat", ",", "wer", "Sch\u00e4tz'", "hat", "in", "der", "Tru\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPRART", "NN", "VAFIN", "$,", "PWS", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer den Schatz vergr\u00e4bt, hat auch im Tod nicht Ruhe.", "tokens": ["Und", "wer", "den", "Schatz", "ver\u00b7gr\u00e4bt", ",", "hat", "auch", "im", "Tod", "nicht", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "APPRART", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Solange mu\u00df er gehn als Geist um jeden Platz,", "tokens": ["So\u00b7lan\u00b7ge", "mu\u00df", "er", "gehn", "als", "Geist", "um", "je\u00b7den", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "KOKOM", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er den Schatz vergrub, bis jemand hob den Schatz.", "tokens": ["Wo", "er", "den", "Schatz", "ver\u00b7grub", ",", "bis", "je\u00b7mand", "hob", "den", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Freu dich, da\u00df du kanst Ruh im Tod und Leben haben,", "tokens": ["Freu", "dich", ",", "da\u00df", "du", "kanst", "Ruh", "im", "Tod", "und", "Le\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "NN", "APPRART", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du hast keinen Schatz verschlossen noch vergraben.", "tokens": ["Weil", "du", "hast", "kei\u00b7nen", "Schatz", "ver\u00b7schlos\u00b7sen", "noch", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIAT", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}