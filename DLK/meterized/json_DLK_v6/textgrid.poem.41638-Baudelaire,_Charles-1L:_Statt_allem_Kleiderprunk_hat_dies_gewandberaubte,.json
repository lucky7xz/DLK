{"textgrid.poem.41638": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Statt allem Kleiderprunk hat dies gewandberaubte,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Statt allem Kleiderprunk hat dies gewandberaubte,", "tokens": ["Statt", "al\u00b7lem", "Klei\u00b7der\u00b7prunk", "hat", "dies", "ge\u00b7wand\u00b7be\u00b7raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "NN", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entsetzliche Phantom auf seinem Knochenhaupte", "tokens": ["Ent\u00b7setz\u00b7li\u00b7che", "Phan\u00b7tom", "auf", "sei\u00b7nem", "Kno\u00b7chen\u00b7haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein gr\u00e4\u00dflich Diadem, wie es zum Fasching pa\u00dft.", "tokens": ["Ein", "gr\u00e4\u00df\u00b7lich", "Di\u00b7a\u00b7dem", ",", "wie", "es", "zum", "Fa\u00b7sching", "pa\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+----+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ohn Sporn und Peitsche treibt in atemloser Hast", "tokens": ["Ohn", "Sporn", "und", "Peit\u00b7sche", "treibt", "in", "a\u00b7tem\u00b7lo\u00b7ser", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ein gespenstig Ro\u00df, apokalyptisch-d\u00fcster,", "tokens": ["Es", "ein", "ge\u00b7spens\u00b7tig", "Ro\u00df", ",", "a\u00b7po\u00b7ka\u00b7lyp\u00b7tischd\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Fallsuchtskranken gleich Schaum spr\u00fcht aus seiner N\u00fcster.", "tokens": ["Das", "Fall\u00b7suchts\u00b7kran\u00b7ken", "gleich", "Schaum", "spr\u00fcht", "aus", "sei\u00b7ner", "N\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "Den gro\u00dfen Weltenraum durchqueren sie zu zweit", "tokens": ["Den", "gro\u00b7\u00dfen", "Wel\u00b7ten\u00b7raum", "durch\u00b7que\u00b7ren", "sie", "zu", "zweit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und stampfen k\u00fchnen Tritts die Unerme\u00dflichkeit.", "tokens": ["Und", "stamp\u00b7fen", "k\u00fch\u00b7nen", "Tritts", "die", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Reiter schwingt ein Schwert, das Feuerflammen wettert", "tokens": ["Der", "Rei\u00b7ter", "schwingt", "ein", "Schwert", ",", "das", "Feu\u00b7er\u00b7flam\u00b7men", "wet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auf die Legionen, die der Huf des Pferds zerschmettert,", "tokens": ["Auf", "die", "Le\u00b7gi\u00b7o\u00b7nen", ",", "die", "der", "Huf", "des", "Pferds", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Und reitet, wie ein F\u00fcrst, der sein Gebiet beschaut,", "tokens": ["Und", "rei\u00b7tet", ",", "wie", "ein", "F\u00fcrst", ",", "der", "sein", "Ge\u00b7biet", "be\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch eisges Grabfeld, das kein Horizont umblaut.", "tokens": ["Durch", "eis\u00b7ges", "Grab\u00b7feld", ",", "das", "kein", "Ho\u00b7ri\u00b7zont", "um\u00b7blaut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dort wesen, hingestreckt in fahlem Sonnenlichte,", "tokens": ["Dort", "we\u00b7sen", ",", "hin\u00b7ge\u00b7streckt", "in", "fah\u00b7lem", "Son\u00b7nen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die V\u00f6lker neuer und antiker Weltgeschichte.", "tokens": ["Die", "V\u00f6l\u00b7ker", "neu\u00b7er", "und", "an\u00b7ti\u00b7ker", "Welt\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}