{"dta.poem.10285": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Erwartung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Turm und Thor und Mauerkranz,             ", "tokens": ["Auf", "Turm", "und", "Thor", "und", "Mau\u00b7er\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf rauschende, dunkle Tannen,", "tokens": ["Auf", "rau\u00b7schen\u00b7de", ",", "dunk\u00b7le", "Tan\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00e4llt Flammenschein und Lichtertanz", "tokens": ["F\u00e4llt", "Flam\u00b7men\u00b7schein", "und", "Lich\u00b7ter\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Fackeln und aus Pfannen.", "tokens": ["Von", "Fa\u00b7ckeln", "und", "aus", "Pfan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Weib steht an des S\u00f6llers Rand,", "tokens": ["Ein", "Weib", "steht", "an", "des", "S\u00f6l\u00b7lers", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es nimmt der Wind ihre Rede:", "tokens": ["Es", "nimmt", "der", "Wind", "ih\u00b7re", "Re\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Trauter zog in\u2019s Niederland,", "tokens": ["Mein", "Trau\u00b7ter", "zog", "in's", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er zog in die blutige Fehde.", "tokens": ["Er", "zog", "in", "die", "blu\u00b7ti\u00b7ge", "Feh\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und h\u00f6rt sie nicht Zinken und Siegesgeschrei,", "tokens": ["Und", "h\u00f6rt", "sie", "nicht", "Zin\u00b7ken", "und", "Sie\u00b7ges\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und sieht seinen Helm sie nicht blinken?", "tokens": ["Und", "sieht", "sei\u00b7nen", "Helm", "sie", "nicht", "blin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Walde nur singt auf der Wiese die Fei,", "tokens": ["Im", "Wal\u00b7de", "nur", "singt", "auf", "der", "Wie\u00b7se", "die", "Fei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Stern th\u00e4t niedersinken.", "tokens": ["Ein", "Stern", "th\u00e4t", "nie\u00b7der\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Morgen graut, die Welt ist so leer,", "tokens": ["Der", "Mor\u00b7gen", "graut", ",", "die", "Welt", "ist", "so", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Welt ist voll Herzeleide.", "tokens": ["Die", "Welt", "ist", "voll", "Her\u00b7ze\u00b7lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wen tragen auf langen Spie\u00dfen sie her,", "tokens": ["Wen", "tra\u00b7gen", "auf", "lan\u00b7gen", "Spie\u00b7\u00dfen", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie fanden ihn tot auf der Heide.", "tokens": ["Sie", "fan\u00b7den", "ihn", "tot", "auf", "der", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}