{"textgrid.poem.56167": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ergreift uns der Vogelschrei...", "tokens": ["Wie", "er\u00b7greift", "uns", "der", "Vo\u00b7gel\u00b7schrei", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Irgend ein einmal erschaffenes Schreien.", "tokens": ["Ir\u00b7gend", "ein", "ein\u00b7mal", "er\u00b7schaf\u00b7fe\u00b7nes", "Schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aber die Kinder schon, spielend im Freien,", "tokens": ["A\u00b7ber", "die", "Kin\u00b7der", "schon", ",", "spie\u00b7lend", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "schreien an wirklichen Schreien vorbei.", "tokens": ["schrei\u00b7en", "an", "wirk\u00b7li\u00b7chen", "Schrei\u00b7en", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Schreien den Zufall. In Zwischenr\u00e4ume", "tokens": ["Schrei\u00b7en", "den", "Zu\u00b7fall", ".", "In", "Zwi\u00b7schen\u00b7r\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "dieses, des Weltraums, (in welchen der heile", "tokens": ["die\u00b7ses", ",", "des", "Welt\u00b7raums", ",", "(", "in", "wel\u00b7chen", "der", "hei\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "$,", "ART", "NN", "$,", "$(", "APPR", "PWAT", "ART", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Vogelschrei eingeht, wie Menschen in Tr\u00e4ume \u2013)", "tokens": ["Vo\u00b7gel\u00b7schrei", "ein\u00b7geht", ",", "wie", "Men\u00b7schen", "in", "Tr\u00e4u\u00b7me", "\u2013", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN", "APPR", "NN", "$(", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "treiben sie ihre, des Kreischens, Keile.", "tokens": ["trei\u00b7ben", "sie", "ih\u00b7re", ",", "des", "Krei\u00b7schens", ",", "Kei\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "$,", "ART", "NN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wehe, wo sind wir? Immer noch freier,", "tokens": ["We\u00b7he", ",", "wo", "sind", "wir", "?", "Im\u00b7mer", "noch", "frei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "$.", "ADV", "ADV", "ADJD", "$,"], "meter": "+-++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "wie die losgerissenen Drachen", "tokens": ["wie", "die", "los\u00b7ge\u00b7ris\u00b7se\u00b7nen", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "jagen wir halbhoch, mit R\u00e4ndern von Lachen,", "tokens": ["ja\u00b7gen", "wir", "halb\u00b7hoch", ",", "mit", "R\u00e4n\u00b7dern", "von", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "windig zerfetzten. \u2013 Ordne die Schreier,", "tokens": ["win\u00b7dig", "zer\u00b7fetz\u00b7ten", ".", "\u2013", "Ord\u00b7ne", "die", "Schrei\u00b7er", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$(", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "singender Gott! da\u00df sie rauschend erwachen,", "tokens": ["sin\u00b7gen\u00b7der", "Gott", "!", "da\u00df", "sie", "rau\u00b7schend", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "tragend als Str\u00f6mung das Haupt und die Leier.", "tokens": ["tra\u00b7gend", "als", "Str\u00f6\u00b7mung", "das", "Haupt", "und", "die", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}