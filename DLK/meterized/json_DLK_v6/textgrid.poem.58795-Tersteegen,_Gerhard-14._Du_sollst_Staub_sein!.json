{"textgrid.poem.58795": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "14. Du sollst Staub sein!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Staub will gerne niedrig bleiben;", "tokens": ["Der", "Staub", "will", "ger\u00b7ne", "nied\u00b7rig", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wenn der Wind nur bl\u00e4set drein,", "tokens": ["Doch", "wenn", "der", "Wind", "nur", "bl\u00e4\u00b7set", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So l\u00e4\u00dft er sich auch willig treiben:", "tokens": ["So", "l\u00e4\u00dft", "er", "sich", "auch", "wil\u00b7lig", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Also sollst du dem Herren sein.", "tokens": ["Al\u00b7so", "sollst", "du", "dem", "Her\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}