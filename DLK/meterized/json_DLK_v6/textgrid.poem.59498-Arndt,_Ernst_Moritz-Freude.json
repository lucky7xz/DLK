{"textgrid.poem.59498": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Freude", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlich leuchten dir Sonne, Mond und Sterne,", "tokens": ["Freund\u00b7lich", "leuch\u00b7ten", "dir", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Freundlich schimmert das Blumenkleid der Erde,", "tokens": ["Freund\u00b7lich", "schim\u00b7mert", "das", "Blu\u00b7men\u00b7kleid", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tiefer rauschet das Meer mit seinen Wellen", "tokens": ["Tie\u00b7fer", "rau\u00b7schet", "das", "Meer", "mit", "sei\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Furchtbar und lieblich.", "tokens": ["Furcht\u00b7bar", "und", "lieb\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Droben kreist in Sonnenglut der Adler,", "tokens": ["Dro\u00b7ben", "kreist", "in", "Son\u00b7nen\u00b7glut", "der", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Drunten sumset der K\u00e4fer und die Milbe,", "tokens": ["Drun\u00b7ten", "sum\u00b7set", "der", "K\u00e4\u00b7fer", "und", "die", "Mil\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Aus den B\u00fcschen t\u00f6nen der Nachtigallen", "tokens": ["Aus", "den", "B\u00fc\u00b7schen", "t\u00f6\u00b7nen", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Z\u00e4rtliche Lieder.", "tokens": ["Z\u00e4rt\u00b7li\u00b7che", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, du bist sch\u00f6n und golden, Mutter Erde,", "tokens": ["Ja", ",", "du", "bist", "sch\u00f6n", "und", "gol\u00b7den", ",", "Mut\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJA", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00f6n in deinen rosigen Abendlocken,", "tokens": ["Sch\u00f6n", "in", "dei\u00b7nen", "ro\u00b7si\u00b7gen", "A\u00b7ben\u00b7dlo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Duftig in deines Erwachens Silberschimmer,", "tokens": ["Duf\u00b7tig", "in", "dei\u00b7nes", "Er\u00b7wa\u00b7chens", "Sil\u00b7ber\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Br\u00e4utlich und z\u00fcchtig.", "tokens": ["Br\u00e4ut\u00b7lich", "und", "z\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Lustig h\u00fcpfest du hin im Weltentanze,", "tokens": ["Lus\u00b7tig", "h\u00fcp\u00b7fest", "du", "hin", "im", "Wel\u00b7ten\u00b7tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alle deine Kinder am warmen Herzen,", "tokens": ["Al\u00b7le", "dei\u00b7ne", "Kin\u00b7der", "am", "war\u00b7men", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wandelst freudig dahin in deiner Sonne", "tokens": ["Wan\u00b7delst", "freu\u00b7dig", "da\u00b7hin", "in", "dei\u00b7ner", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PAV", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Funkelndem Reigen.", "tokens": ["Fun\u00b7keln\u00b7dem", "Rei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Lustig sei und lachend des Menschen Stirne!", "tokens": ["Lus\u00b7tig", "sei", "und", "la\u00b7chend", "des", "Men\u00b7schen", "Stir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Nur dem Fr\u00f6hlichen bl\u00fcht der Baum des Lebens,", "tokens": ["Nur", "dem", "Fr\u00f6h\u00b7li\u00b7chen", "bl\u00fcht", "der", "Baum", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dem Unschuldigen rinnt der Born der Jugend", "tokens": ["Dem", "Un\u00b7schul\u00b7di\u00b7gen", "rinnt", "der", "Born", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch noch im Alter.", "tokens": ["Auch", "noch", "im", "Al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Heiter schwimmet die Luft mit ihren Sternen", "tokens": ["Hei\u00b7ter", "schwim\u00b7met", "die", "Luft", "mit", "ih\u00b7ren", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf dem Busen des sanftbewegten Meeres,", "tokens": ["Auf", "dem", "Bu\u00b7sen", "des", "sanft\u00b7be\u00b7weg\u00b7ten", "Mee\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Doch gestaltlos zittern auf wilden Wogen", "tokens": ["Doch", "ge\u00b7stalt\u00b7los", "zit\u00b7tern", "auf", "wil\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Bleichende Schimmer.", "tokens": ["Blei\u00b7chen\u00b7de", "Schim\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}