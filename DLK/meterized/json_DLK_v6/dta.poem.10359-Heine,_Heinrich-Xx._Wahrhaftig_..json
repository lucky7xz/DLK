{"dta.poem.10359": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  \n  Wahrhaftig .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Fr\u00fchling kommt mit dem Sonnenschein,", "tokens": ["Wenn", "der", "Fr\u00fch\u00b7ling", "kommt", "mit", "dem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann knospen und bl\u00fchen die Bl\u00fcmlein auf;", "tokens": ["Dann", "knos\u00b7pen", "und", "bl\u00fc\u00b7hen", "die", "Bl\u00fcm\u00b7lein", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn der Mond beginnt seinen Stralenlauf,", "tokens": ["Wenn", "der", "Mond", "be\u00b7ginnt", "sei\u00b7nen", "Stra\u00b7len\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann schwimmen die Sternlein hintendrein;", "tokens": ["Dann", "schwim\u00b7men", "die", "Stern\u00b7lein", "hin\u00b7ten\u00b7drein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn der S\u00e4nger zwei s\u00fc\u00dfe Aeuglein sieht,", "tokens": ["Wenn", "der", "S\u00e4n\u00b7ger", "zwei", "s\u00fc\u00b7\u00dfe", "A\u00b7e\u00b7u\u00b7glein", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Dann quellen ihm Lieder aus tiefem Gem\u00fcth; \u2014", "tokens": ["Dann", "quel\u00b7len", "ihm", "Lie\u00b7der", "aus", "tie\u00b7fem", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Doch Lieder und Sterne und Bl\u00fcmelein,", "tokens": ["Doch", "Lie\u00b7der", "und", "Ster\u00b7ne", "und", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und Aeuglein und Mondglanz und Sonnenschein,", "tokens": ["Und", "A\u00b7e\u00b7u\u00b7glein", "und", "Mond\u00b7glanz", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wie sehr das Zeug auch gef\u00e4llt,", "tokens": ["Wie", "sehr", "das", "Zeug", "auch", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "So macht's doch noch lang keine Welt.", "tokens": ["So", "macht's", "doch", "noch", "lang", "kei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}