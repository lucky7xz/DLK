{"dta.poem.9645": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Warum er in der Jrrl\u00e4ndischen schlacht  \n bleiben m\u00fcssen?  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich bey der ersten schlacht f\u00fcr der Britanner kronen/", "tokens": ["Gleich", "bey", "der", "ers\u00b7ten", "schlacht", "f\u00fcr", "der", "Bri\u00b7tan\u00b7ner", "kro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt Schomberg im gefecht/ und Wilhelm siegt allein.", "tokens": ["Bleibt", "Schom\u00b7berg", "im", "ge\u00b7fecht", "/", "und", "Wil\u00b7helm", "siegt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "VVPP", "$(", "KON", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warum? weil dieser held den sieg und seine thronen", "tokens": ["Wa\u00b7rum", "?", "weil", "die\u00b7ser", "held", "den", "sieg", "und", "sei\u00b7ne", "thro\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PDS", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst keinem/ als sich selbst/ hat sollen schuldig seyn.", "tokens": ["Sonst", "kei\u00b7nem", "/", "als", "sich", "selbst", "/", "hat", "sol\u00b7len", "schul\u00b7dig", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word"