{"textgrid.poem.28445": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott sollstu mehr dann dich, wie dich den Nechsten lieben;", "tokens": ["Gott", "soll\u00b7stu", "mehr", "dann", "dich", ",", "wie", "dich", "den", "Nechs\u00b7ten", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADV", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann eine Liebe bleibt, so sind sie beyde blieben;", "tokens": ["Wann", "ei\u00b7ne", "Lie\u00b7be", "bleibt", ",", "so", "sind", "sie", "bey\u00b7de", "blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann Gott und Nechsten sind verkn\u00fcpfft in eines Band,", "tokens": ["Dann", "Gott", "und", "Nechs\u00b7ten", "sind", "ver\u00b7kn\u00fcpfft", "in", "ei\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer da sich hat getrennt, der hat sich dort getrant.", "tokens": ["Wer", "da", "sich", "hat", "ge\u00b7trennt", ",", "der", "hat", "sich", "dort", "ge\u00b7trant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VAFIN", "VVPP", "$,", "PRELS", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}