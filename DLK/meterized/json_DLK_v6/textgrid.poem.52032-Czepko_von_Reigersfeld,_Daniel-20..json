{"textgrid.poem.52032": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb ist auch eine Pest, die aus der Gruben f\u00e4hrt,", "tokens": ["Lieb", "ist", "auch", "ei\u00b7ne", "Pest", ",", "die", "aus", "der", "Gru\u00b7ben", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "In welche man dich sieht durch Th\u00fcr und Fenster dringen:", "tokens": ["In", "wel\u00b7che", "man", "dich", "sieht", "durch", "Th\u00fcr", "und", "Fens\u00b7ter", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist wie Curtius: Der Sprung ist nur verkehrt,", "tokens": ["Du", "bist", "wie", "Cur\u00b7tius", ":", "Der", "Sprung", "ist", "nur", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NE", "$.", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihn sahe man ins Loch, dich aus dem Loche springen.", "tokens": ["Ihn", "sa\u00b7he", "man", "ins", "Loch", ",", "dich", "aus", "dem", "Lo\u00b7che", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$,", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}