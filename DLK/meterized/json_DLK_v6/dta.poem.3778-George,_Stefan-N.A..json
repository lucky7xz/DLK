{"dta.poem.3778": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Lose geschmeide \u00b7", "tokens": ["Lo\u00b7se", "ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["XY", "XY", "XY"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Frauen dirnen", "tokens": ["Frau\u00b7en", "dir\u00b7nen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schlanke schenken", "tokens": ["Schlan\u00b7ke", "schen\u00b7ken"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "M\u00fcde sich senken", "tokens": ["M\u00fc\u00b7de", "sich", "sen\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRF", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ledig die lende", "tokens": ["Le\u00b7dig", "die", "len\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Busen und h\u00fcfte", "tokens": ["Bu\u00b7sen", "und", "h\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Um die stirnen", "tokens": ["Um", "die", "stir\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Der kr\u00e4nze rest", "tokens": ["Der", "kr\u00e4n\u00b7ze", "rest"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schl\u00e4fernder broden", "tokens": ["Schl\u00e4\u00b7fern\u00b7der", "bro\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Traufender d\u00fcfte", "tokens": ["Trau\u00b7fen\u00b7der", "d\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Weink\u00f6nig scheide!", "tokens": ["Wein\u00b7k\u00f6\u00b7nig", "schei\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Aller ende", "tokens": ["Al\u00b7ler", "en\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Endet das fest", "tokens": ["En\u00b7det", "das", "fest"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Rosen regnen", "tokens": ["Ro\u00b7sen", "reg\u00b7nen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Purpurne satte", "tokens": ["Pur\u00b7pur\u00b7ne", "sat\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die liebkosen?", "tokens": ["Die", "lieb\u00b7ko\u00b7sen", "?"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Weisse matte", "tokens": ["Weis\u00b7se", "mat\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Euch zu laben?", "tokens": ["Euch", "zu", "la\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Malvenrote", "tokens": ["Mal\u00b7ven\u00b7ro\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Gelbe tote:", "tokens": ["Gel\u00b7be", "to\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Manen-k\u00fcsse", "tokens": ["Ma\u00b7nen\u00b7k\u00fcs\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Euch zu segnen", "tokens": ["Euch", "zu", "seg\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Auf die schleusen!", "tokens": ["Auf", "die", "schleu\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und aus reusen", "tokens": ["Und", "aus", "reu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Regnen rosen", "tokens": ["Reg\u00b7nen", "ro\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "G\u00fcsse fl\u00fcsse", "tokens": ["G\u00fcs\u00b7se", "fl\u00fcs\u00b7se"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Die begraben", "tokens": ["Die", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}}}}