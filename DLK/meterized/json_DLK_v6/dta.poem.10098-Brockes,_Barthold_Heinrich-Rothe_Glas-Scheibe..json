{"dta.poem.10098": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rothe Glas-Scheibe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann mein ausgekl\u00e4rter Blick \u00fcber gr\u00fcne Wiesen\nschiesset,", "tokens": ["Wann", "mein", "aus\u00b7ge\u00b7kl\u00e4r\u00b7ter", "Blick", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "Wie\u00b7sen", "schies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Welche mit belaubten B\u00e4umen hin und wieder ausge-", "tokens": ["Wel\u00b7che", "mit", "be\u00b7laub\u00b7ten", "B\u00e4u\u00b7men", "hin", "und", "wie\u00b7der", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ADJA", "NN", "PTKVZ", "KON", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Sonderlich, wann ihre Sch\u00f6nheit von der Sonnen ange-", "tokens": ["Son\u00b7der\u00b7lich", ",", "wann", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "von", "der", "Son\u00b7nen", "an\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "PPOSAT", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und das durch derselben Pracht ein sich schl\u00e4ngelnd B\u00e4chlein", "tokens": ["Und", "das", "durch", "der\u00b7sel\u00b7ben", "Pracht", "ein", "sich", "schl\u00e4n\u00b7gelnd", "B\u00e4ch\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PDAT", "NN", "ART", "PRF", "ADJD", "NN"], "meter": "----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das den himmlischen Saphir, hin und wieder sch\u00f6n ver-", "tokens": ["Das", "den", "himm\u00b7li\u00b7schen", "Sa\u00b7phir", ",", "hin", "und", "wie\u00b7der", "sch\u00f6n", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "$,", "ADV", "KON", "ADV", "ADJD", "TRUNC"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Durch der Wolcken Gegen-Schlag, als im glatten Spiegel,", "tokens": ["Durch", "der", "Wol\u00b7cken", "Ge\u00b7gen\u00b7Schlag", ",", "als", "im", "glat\u00b7ten", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Scheints, da\u00df sich der Erden Pracht mit der Himmels-Pracht", "tokens": ["Scheints", ",", "da\u00df", "sich", "der", "Er\u00b7den", "Pracht", "mit", "der", "Him\u00b7mels\u00b7Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PRF", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Diese Sch\u00f6nheit stellt sich mir", "tokens": ["Die\u00b7se", "Sch\u00f6n\u00b7heit", "stellt", "sich", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PRF", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein herrliches Spectackel, und als einen Schau-Platz, f\u00fcr,", "tokens": ["Als", "ein", "herr\u00b7li\u00b7ches", "Spec\u00b7ta\u00b7ckel", ",", "und", "als", "ei\u00b7nen", "Schau\u00b7Platz", ",", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "KON", "KOUS", "ART", "NN", "$,", "APPR", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Welcher von des Sch\u00f6pfers Hand, auf uns unbekannte", "tokens": ["Wel\u00b7cher", "von", "des", "Sch\u00f6p\u00b7fers", "Hand", ",", "auf", "uns", "un\u00b7be\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "NN", "NN", "$,", "APPR", "PPER", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Uns zur Anmuth, Jhm zum Preise,", "tokens": ["Uns", "zur", "An\u00b7muth", ",", "Jhm", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wunderbarlich vorgestellt.", "tokens": ["Wun\u00b7der\u00b7bar\u00b7lich", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als ich j\u00fcngst mich dergestalt an der sch\u00f6nen Welt", "tokens": ["Als", "ich", "j\u00fcngst", "mich", "der\u00b7ge\u00b7stalt", "an", "der", "sch\u00f6\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und, zu meiner Augen-Lust, ein ", "tokens": ["Und", ",", "zu", "mei\u00b7ner", "Au\u00b7gen\u00b7Lust", ",", "ein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "$,", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah\u2019 ich dieser Landschafft Pracht, durch ein rothes Scheibgen", "tokens": ["Sah'", "ich", "die\u00b7ser", "Land\u00b7schafft", "Pracht", ",", "durch", "ein", "ro\u00b7thes", "Scheib\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Mit Erstaunen, mit Vergn\u00fcgen, aber halb mit Schrecken, an.", "tokens": ["Mit", "Er\u00b7stau\u00b7nen", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ",", "a\u00b7ber", "halb", "mit", "Schre\u00b7cken", ",", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Alles Gr\u00fcne war dahin, roth war alles Laub und Gras,", "tokens": ["Al\u00b7les", "Gr\u00fc\u00b7ne", "war", "da\u00b7hin", ",", "roth", "war", "al\u00b7les", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PAV", "$,", "ADJD", "VAFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "H\u00fcgel, Th\u00e4ler, Wasser, W\u00e4lder,", "tokens": ["H\u00fc\u00b7gel", ",", "Th\u00e4\u00b7ler", ",", "Was\u00b7ser", ",", "W\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aecker, H\u00e4user, G\u00e4rten, Felder.", "tokens": ["A\u00b7e\u00b7cker", ",", "H\u00e4u\u00b7ser", ",", "G\u00e4r\u00b7ten", ",", "Fel\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Himmel, Wolcken, Thier\u2019 und Menschen, alle Vorw\u00fcrff\u2019, die", "tokens": ["Him\u00b7mel", ",", "Wol\u00b7cken", ",", "Thier'", "und", "Men\u00b7schen", ",", "al\u00b7le", "Vor\u00b7w\u00fcrff'", ",", "die"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "PIAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Fern und nah,", "tokens": ["Fern", "und", "nah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Waren alle, wie Rubinen, feurig, doch nicht minder sch\u00f6n,", "tokens": ["Wa\u00b7ren", "al\u00b7le", ",", "wie", "Ru\u00b7bi\u00b7nen", ",", "feu\u00b7rig", ",", "doch", "nicht", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PWAV", "NN", "$,", "ADJD", "$,", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Als vorhero, anzusehn.", "tokens": ["Als", "vor\u00b7he\u00b7ro", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "VVIZU", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Ob mich die Ver\u00e4ndrung nun gleich im Anfang sehr ergetzte,", "tokens": ["Ob", "mich", "die", "Ver\u00b7\u00e4n\u00b7drung", "nun", "gleich", "im", "An\u00b7fang", "sehr", "er\u00b7getz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Und, f\u00fcr lieblicher Verwirrung, gleichsam aus mir selber", "tokens": ["Und", ",", "f\u00fcr", "lieb\u00b7li\u00b7cher", "Ver\u00b7wir\u00b7rung", ",", "gleich\u00b7sam", "aus", "mir", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Fiel mir dennoch, voll Erstaunen, mitten in der Freude, bey,", "tokens": ["Fiel", "mir", "den\u00b7noch", ",", "voll", "Er\u00b7stau\u00b7nen", ",", "mit\u00b7ten", "in", "der", "Freu\u00b7de", ",", "bey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJD", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "APPR", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Wie die\u00df eine grosse Probe irdscher Ungewi\u00dfheit sey.", "tokens": ["Wie", "die\u00df", "ei\u00b7ne", "gros\u00b7se", "Pro\u00b7be", "ird\u00b7scher", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}