{"textgrid.poem.42230": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Das Goldst\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier an dieser \u00f6den St\u00e4tte", "tokens": ["Hier", "an", "die\u00b7ser", "\u00f6\u00b7den", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich rasten, bis es tagt;", "tokens": ["Will", "ich", "ras\u00b7ten", ",", "bis", "es", "tagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welker Rasen ist ein Bette,", "tokens": ["Wel\u00b7ker", "Ra\u00b7sen", "ist", "ein", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's mir eben recht behagt.", "tokens": ["Wie's", "mir", "e\u00b7ben", "recht", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neben mir die Wogen brausen,", "tokens": ["Ne\u00b7ben", "mir", "die", "Wo\u00b7gen", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber mir die Wolken sausen,", "tokens": ["\u00dc\u00b7ber", "mir", "die", "Wol\u00b7ken", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Keiner milden Stimme Klang", "tokens": ["Kei\u00b7ner", "mil\u00b7den", "Stim\u00b7me", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "T\u00f6nt den d\u00fcstren Hag entlang.", "tokens": ["T\u00f6nt", "den", "d\u00fcst\u00b7ren", "Hag", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles habt ihr mir genommen,", "tokens": ["Al\u00b7les", "habt", "ihr", "mir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ihr mir gegeben habt;", "tokens": ["Was", "ihr", "mir", "ge\u00b7ge\u00b7ben", "habt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nackend bin ich hergekommen,", "tokens": ["Na\u00b7ckend", "bin", "ich", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nackend bin ich hingetrabt,", "tokens": ["Na\u00b7ckend", "bin", "ich", "hin\u00b7ge\u00b7trabt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Str\u00fcmpfe, Stiefel, Hosen \u2013", "tokens": ["Oh\u00b7ne", "Str\u00fcmp\u00b7fe", ",", "Stie\u00b7fel", ",", "Ho\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Lebens lichte Rosen,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "lich\u00b7te", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meiner Jugend muntrer Sinn,", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "mun\u00b7trer", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles, alles ist dahin.", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "ist", "da\u00b7hin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob es schon ein Ziel mir setzte,", "tokens": ["Ob", "es", "schon", "ein", "Ziel", "mir", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu erforschen vom Geschick,", "tokens": ["Zu", "er\u00b7for\u00b7schen", "vom", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werf ich in die Luft dies letzte", "tokens": ["Werf", "ich", "in", "die", "Luft", "dies", "letz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "PDS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blanke goldgepr\u00e4gte St\u00fcck,", "tokens": ["Blan\u00b7ke", "gold\u00b7ge\u00b7pr\u00e4g\u00b7te", "St\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df es, auf des Kopfes Seite", "tokens": ["Da\u00df", "es", ",", "auf", "des", "Kop\u00b7fes", "Sei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fallend meinen Tod bedeute;", "tokens": ["Fal\u00b7lend", "mei\u00b7nen", "Tod", "be\u00b7deu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn das Bild gen Himmel schaut,", "tokens": ["Wenn", "das", "Bild", "gen", "Him\u00b7mel", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei noch bessrer Zeit vertraut.", "tokens": ["Sei", "noch", "bess\u00b7rer", "Zeit", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und es steigt, es f\u00e4llt, es klingelt,", "tokens": ["Und", "es", "steigt", ",", "es", "f\u00e4llt", ",", "es", "klin\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, zum Himmel starrt die Zahl!", "tokens": ["Sieh", ",", "zum", "Him\u00b7mel", "starrt", "die", "Zahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein erbebend Herz umzingelt", "tokens": ["Mein", "er\u00b7be\u00b7bend", "Herz", "um\u00b7zin\u00b7gelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Todesangst zum letztenmal. \u2013", "tokens": ["To\u00b7de\u00b7sangst", "zum", "letz\u00b7ten\u00b7mal", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eingedenk der Abschiedsflasche", "tokens": ["Ein\u00b7ge\u00b7denk", "der", "Ab\u00b7schieds\u00b7fla\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steck ich's schweigend in die Tasche;", "tokens": ["Steck", "ich's", "schwei\u00b7gend", "in", "die", "Ta\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre Dauer sei mein Ma\u00df,", "tokens": ["Ih\u00b7re", "Dau\u00b7er", "sei", "mein", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eins des andern Stundenglas.", "tokens": ["Eins", "des", "an\u00b7dern", "Stun\u00b7den\u00b7glas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sp\u00e4t am Tage schlendr ich weiter", "tokens": ["Sp\u00e4t", "am", "Ta\u00b7ge", "schlendr", "ich", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Sonne fahlem Glanz.", "tokens": ["In", "der", "Son\u00b7ne", "fah\u00b7lem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Such dir r\u00fcstigern Begleiter,", "tokens": ["Such", "dir", "r\u00fcs\u00b7ti\u00b7gern", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandrer du im Efeukranz!", "tokens": ["Wand\u00b7rer", "du", "im", "E\u00b7feu\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Vieles m\u00f6chtest du vers\u00e4umen,", "tokens": ["Vie\u00b7les", "m\u00f6ch\u00b7test", "du", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich darf rasten, ich darf tr\u00e4umen;", "tokens": ["Ich", "darf", "ras\u00b7ten", ",", "ich", "darf", "tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was das Schicksal mir verspricht,", "tokens": ["Was", "das", "Schick\u00b7sal", "mir", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "J\u00fcngling, das enteilt mir nicht.", "tokens": ["J\u00fcng\u00b7ling", ",", "das", "ent\u00b7eilt", "mir", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}