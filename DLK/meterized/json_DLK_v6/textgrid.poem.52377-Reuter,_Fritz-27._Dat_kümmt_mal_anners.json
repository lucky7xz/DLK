{"textgrid.poem.52377": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "27. Dat k\u00fcmmt mal anners", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oll Moses Itzig un oll Slaume Lessen,", "tokens": ["Oll", "Mo\u00b7ses", "It\u00b7zig", "un", "oll", "Slau\u00b7me", "Les\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "De sitten recht bequem in ehren Wagen", "tokens": ["De", "sit\u00b7ten", "recht", "be\u00b7quem", "in", "eh\u00b7ren", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Un f\u00fchr'n nah Leipzig up de Messen.", "tokens": ["Un", "f\u00fchr'n", "nah", "Leip\u00b7zig", "up", "de", "Mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "NE", "NE", "NE", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sei s\u00fcnd nu grad dorbi un klagen,", "tokens": ["Sei", "s\u00fcnd", "nu", "grad", "dor\u00b7bi", "un", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "ADV", "FM", "FM", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wo dat de Judenschaft doch wir so unnerdr\u00fcckt", "tokens": ["Wo", "dat", "de", "Ju\u00b7den\u00b7schaft", "doch", "wir", "so", "un\u00b7ner\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "NN", "KON", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Un dat en jeder ehr an't T\u00fcg wat flickt", "tokens": ["Un", "dat", "en", "je\u00b7der", "ehr", "an't", "T\u00fcg", "wat", "flickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Un dat dat so nich bliwen k\u00fcnn;", "tokens": ["Un", "dat", "dat", "so", "nich", "bli\u00b7wen", "k\u00fcnn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Dat m\u00fc\u00dft doch mal eins anners warden,", "tokens": ["Dat", "m\u00fc\u00dft", "doch", "mal", "eins", "an\u00b7ners", "war\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "ADV", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn s' Rothschildten taum K\u00f6nig hadden.", "tokens": ["Wenn", "s'", "Roth\u00b7schild\u00b7ten", "taum", "K\u00f6\u00b7nig", "had\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Drup reden sei von den Gewinn", "tokens": ["Drup", "re\u00b7den", "sei", "von", "den", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Un von dat pr\u00e4chtige Gesch\u00e4ft,", "tokens": ["Un", "von", "dat", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Wat s' beid in dat vergangen Johr", "tokens": ["Wat", "s'", "beid", "in", "dat", "ver\u00b7gan\u00b7gen", "Johr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Heww'n makt, un wat s' verk\u00f6fft", "tokens": ["Hew\u00b7w'n", "makt", ",", "un", "wat", "s'", "ver\u00b7k\u00f6fft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "FM", "FM", "FM", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un wat s' behollen von de Wohr;", "tokens": ["Un", "wat", "s'", "be\u00b7hol\u00b7len", "von", "de", "Wohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Wat ehr't Gesch\u00e4ftche in hadd dragen", "tokens": ["Wat", "ehr't", "Ge\u00b7sch\u00e4ft\u00b7che", "in", "hadd", "dra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Un wovel Geld s' tausamen slagen", "tokens": ["Un", "wo\u00b7vel", "Geld", "s'", "tau\u00b7sa\u00b7men", "sla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "NE", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Un wat s' nu maken wullen f\u00f6r en Rebbes.", "tokens": ["Un", "wat", "s'", "nu", "ma\u00b7ken", "wul\u00b7len", "f\u00f6r", "en", "Reb\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "So f\u00fchr'n sei nu denn de Schossen entlang,", "tokens": ["So", "f\u00fchr'n", "sei", "nu", "denn", "de", "Schos\u00b7sen", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "ADV", "ADV", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Un an de Strat s\u00fcnd de Schossehsteinkl\u00f6ppers,", "tokens": ["Un", "an", "de", "Strat", "s\u00fcnd", "de", "Schos\u00b7seh\u00b7stein\u00b7kl\u00f6p\u00b7pers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "De kloppen dor in Hitt un Stohm ehr Stein", "tokens": ["De", "klop\u00b7pen", "dor", "in", "Hitt", "un", "Stohm", "ehr", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Un sweiten dorbi as de Boren.", "tokens": ["Un", "swei\u00b7ten", "dor\u00b7bi", "as", "de", "Bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "As de de Juden nu gewohren", "tokens": ["As", "de", "de", "Ju\u00b7den", "nu", "ge\u00b7woh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NN", "ADV", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Un Slaumen sine N\u00e4s' sei seihn,", "tokens": ["Un", "Slau\u00b7men", "si\u00b7ne", "N\u00e4s'", "sei", "seihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dunn fang'n sei an \u00bbHepp! Hepp!\u00ab tau schrigen", "tokens": ["Dunn", "fang'n", "sei", "an", "\u00bb", "Hepp", "!", "Hepp", "!", "\u00ab", "tau", "schri\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "ITJ", "$.", "NE", "$.", "$(", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Un \u00bbnimm mer ", "tokens": ["Un", "\u00bb", "nimm", "mer"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$(", "VVIMP", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Oll Slaume kann doch allseindag' dit", "tokens": ["Oll", "Slau\u00b7me", "kann", "doch", "all\u00b7sein\u00b7dag'", "dit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nich so passieren lat'n; hei ward dat Schellen kriegen", "tokens": ["Nich", "so", "pas\u00b7sie\u00b7ren", "lat'n", ";", "hei", "ward", "dat", "Schel\u00b7len", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "VVINF", "$.", "ADJD", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Un schimpt herute ut den Plan,", "tokens": ["Un", "schimpt", "he\u00b7ru\u00b7te", "ut", "den", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Un slimm wir't unsre Juden gahn,", "tokens": ["Un", "slimm", "wir't", "uns\u00b7re", "Ju\u00b7den", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hadd Moses sick nich twischen leggt.", "tokens": ["Hadd", "Mo\u00b7ses", "sick", "nich", "twi\u00b7schen", "leggt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "De ritt oll Slaumen t'r\u00fcgg un seggt:", "tokens": ["De", "ritt", "oll", "Slau\u00b7men", "t'\u00b7r\u00fcgg", "un", "seggt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbla\u00df gut sein, Schlaume, es wird k\u00fcmmen!", "tokens": ["\u00bb", "la\u00df", "gut", "sein", ",", "Schlau\u00b7me", ",", "es", "wird", "k\u00fcm\u00b7men", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADJD", "VAINF", "$,", "NN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.16": {"text": "Pa\u00df Achtung! Es wird k\u00fcmm'n die Zeit,", "tokens": ["Pa\u00df", "Ach\u00b7tung", "!", "Es", "wird", "k\u00fcm\u00b7m'n", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VAFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Da\u00df unser Fett tut oben schwimmen,", "tokens": ["Da\u00df", "un\u00b7ser", "Fett", "tut", "o\u00b7ben", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Denn werd'n mer aach sein unge\u00dfogen.", "tokens": ["Denn", "werd'n", "mer", "aach", "sein", "un\u00b7ge\u00b7\u00dfo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Pa\u00df Achtung! Es wird k\u00fcmm'n de Zeit,", "tokens": ["Pa\u00df", "Ach\u00b7tung", "!", "Es", "wird", "k\u00fcm\u00b7m'n", "de", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VAFIN", "NE", "NE", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Wo wir se denn aach 'runterm\u00f6ppern.", "tokens": ["Wo", "wir", "se", "denn", "aach", "'r\u00b7un\u00b7ter\u00b7m\u00f6p\u00b7pern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "FM", "KON", "NE", "NE", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Die werden denn Schossehstein kl\u00f6ppern!\u00ab", "tokens": ["Die", "wer\u00b7den", "denn", "Schos\u00b7seh\u00b7stein", "kl\u00f6p\u00b7pern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}