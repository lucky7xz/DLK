{"textgrid.poem.42837": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6pfe und R\u00fcmpfe trennen sich", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6pfe und R\u00fcmpfe trennen sich", "tokens": ["K\u00f6p\u00b7fe", "und", "R\u00fcmp\u00b7fe", "tren\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00dcberall im Blut.", "tokens": ["\u00dc\u00b7be\u00b7rall", "im", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcberall bekennen sich", "tokens": ["\u00dc\u00b7be\u00b7rall", "be\u00b7ken\u00b7nen", "sich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leute zum Henkersmut.", "tokens": ["Leu\u00b7te", "zum", "Hen\u00b7kers\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "\u00dcberall wird die Rache satt.", "tokens": ["\u00dc\u00b7be\u00b7rall", "wird", "die", "Ra\u00b7che", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "\u00dcberall tut sich ein Recht,", "tokens": ["\u00dc\u00b7be\u00b7rall", "tut", "sich", "ein", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Birgt sich, wenn es \u00c4ngste hat,", "tokens": ["Birgt", "sich", ",", "wenn", "es", "\u00c4ngs\u00b7te", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter einem beschr\u00e4nkten Knecht.", "tokens": ["Hin\u00b7ter", "ei\u00b7nem", "be\u00b7schr\u00e4nk\u00b7ten", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ferne Unwetter grollen.", "tokens": ["Fer\u00b7ne", "Un\u00b7wet\u00b7ter", "grol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Es gruselt dumpf:", "tokens": ["Es", "gru\u00b7selt", "dumpf", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was werden die K\u00f6pfe wollen,", "tokens": ["Was", "wer\u00b7den", "die", "K\u00f6p\u00b7fe", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn sie wieder hupfen auf ihren Rumpf?", "tokens": ["Wenn", "sie", "wie\u00b7der", "hup\u00b7fen", "auf", "ih\u00b7ren", "Rumpf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}