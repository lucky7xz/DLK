{"textgrid.poem.36460": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nachtigall sang Elegien,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall sang Elegien,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "sang", "E\u00b7le\u00b7gi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Oden, oder Threnodien,", "tokens": ["Und", "O\u00b7den", ",", "o\u00b7der", "Thre\u00b7no\u00b7di\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem ganzen V\u00f6gel-Chor", "tokens": ["Dem", "gan\u00b7zen", "V\u00f6\u00b7gel\u00b7Chor"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In einem stillen Walde vor.", "tokens": ["In", "ei\u00b7nem", "stil\u00b7len", "Wal\u00b7de", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht weit davon hebt sich die Lerche hoch empor", "tokens": ["Nicht", "weit", "da\u00b7von", "hebt", "sich", "die", "Ler\u00b7che", "hoch", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "PAV", "VVFIN", "PRF", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In ihre freie Luft,", "tokens": ["In", "ih\u00b7re", "frei\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fingt, (indes der Kuckuck ruft,)", "tokens": ["Und", "fingt", ",", "(", "in\u00b7des", "der", "Ku\u00b7ckuck", "ruft", ",", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "$(", "ADV", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrer kleinen, hellen Kehle,", "tokens": ["Mit", "ih\u00b7rer", "klei\u00b7nen", ",", "hel\u00b7len", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lust und Zufriedenheit dem Wandrer in die Seele.", "tokens": ["Lust", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "dem", "Wand\u00b7rer", "in", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Die Nachtigall singt trauriger und b\u00e4nger", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "trau\u00b7ri\u00b7ger", "und", "b\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Schmerzenslied!", "tokens": ["Ihr", "Schmer\u00b7zens\u00b7lied", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Lerche, die sich \u00fcberwunden sieht,", "tokens": ["Die", "Ler\u00b7che", ",", "die", "sich", "\u00fc\u00b7berw\u00b7un\u00b7den", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rt auf, und will gestreng, die Nachtigall gestrenger", "tokens": ["H\u00f6rt", "auf", ",", "und", "will", "ge\u00b7streng", ",", "die", "Nach\u00b7ti\u00b7gall", "ge\u00b7stren\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VMFIN", "ADJD", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gerichtet sein!", "tokens": ["Ge\u00b7rich\u00b7tet", "sein", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Kein Richter meldet sich, zu richten diese S\u00e4nger!", "tokens": ["Kein", "Rich\u00b7ter", "mel\u00b7det", "sich", ",", "zu", "rich\u00b7ten", "die\u00b7se", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis endlich noch ein Denker, ein Uhu,", "tokens": ["Bis", "end\u00b7lich", "noch", "ein", "Den\u00b7ker", ",", "ein", "U\u00b7hu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus einem hohlen Baume spricht:", "tokens": ["Aus", "ei\u00b7nem", "hoh\u00b7len", "Bau\u00b7me", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Nachtigall! und Lerche, du!", "tokens": ["Du", "Nach\u00b7ti\u00b7gall", "!", "und", "Ler\u00b7che", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$.", "KON", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vollkommen singt ihr nicht!", "tokens": ["Voll\u00b7kom\u00b7men", "singt", "ihr", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach wie so schwer trifft man die Mittelstra\u00dfe doch!", "tokens": ["Ach", "wie", "so", "schwer", "trifft", "man", "die", "Mit\u00b7tel\u00b7stra\u00b7\u00dfe", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "VVFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der eine f\u00e4llt zu ", "tokens": ["Der", "ei\u00b7ne", "f\u00e4llt", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ihr guten S\u00e4nger! welch ein Richter!", "tokens": ["Ihr", "gu\u00b7ten", "S\u00e4n\u00b7ger", "!", "welch", "ein", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Von meinem Uz, dem Liederdichter,", "tokens": ["Von", "mei\u00b7nem", "Uz", ",", "dem", "Lie\u00b7der\u00b7dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meinem Klopstock, der, ein Adler, sich erhebt,", "tokens": ["Und", "mei\u00b7nem", "Klops\u00b7tock", ",", "der", ",", "ein", "Ad\u00b7ler", ",", "sich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "$,", "ART", "NN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Gottes Sonne sieht, hoch \u00fcber Wolken schwebt,", "tokens": ["In", "Got\u00b7tes", "Son\u00b7ne", "sieht", ",", "hoch", "\u00fc\u00b7ber", "Wol\u00b7ken", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sprach, schon vor zwanzig Jahren, am Parna\u00df,", "tokens": ["Sprach", ",", "schon", "vor", "zwan\u00b7zig", "Jah\u00b7ren", ",", "am", "Par\u00b7na\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "CARD", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Uhu eben das!", "tokens": ["Ein", "U\u00b7hu", "e\u00b7ben", "das", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PDS", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}