{"textgrid.poem.36505": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4. \u00c4sop und der Abderit", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprich doch einmal mit deinen Tieren!", "tokens": ["Sprich", "doch", "ein\u00b7mal", "mit", "dei\u00b7nen", "Tie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach zum \u00c4sop ein Abderit;", "tokens": ["Sprach", "zum", "\u00c4\u00b7sop", "ein", "Ab\u00b7de\u00b7rit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sie stehn um dich herum, auf Zweien und auf Vieren,", "tokens": ["Sie", "stehn", "um", "dich", "he\u00b7rum", ",", "auf", "Zwei\u00b7en", "und", "auf", "Vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wer sie sieht,", "tokens": ["Und", "wer", "sie", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sieht wohl, da\u00df sie nicht sprechen k\u00f6nnen,", "tokens": ["Sieht", "wohl", ",", "da\u00df", "sie", "nicht", "spre\u00b7chen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht an dem Fuchse keine List!", "tokens": ["Sieht", "an", "dem", "Fuch\u00b7se", "kei\u00b7ne", "List", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du wollest, sprach \u00c4sop, weil du gespr\u00e4chig bist,", "tokens": ["Du", "wol\u00b7lest", ",", "sprach", "\u00c4\u00b7sop", ",", "weil", "du", "ge\u00b7spr\u00e4\u00b7chig", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "NE", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ihnen ihre Stummheit g\u00f6nnen!", "tokens": ["Doch", "ih\u00b7nen", "ih\u00b7re", "Stumm\u00b7heit", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}