{"textgrid.poem.54937": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe so viel vergessen.", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe so viel vergessen.", "tokens": ["Ich", "ha\u00b7be", "so", "viel", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wei\u00df nicht mehr", "tokens": ["Ich", "wei\u00df", "nicht", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Woher ich komme.", "tokens": ["Wo\u00b7her", "ich", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich sa\u00df in einer Laube", "tokens": ["Ich", "sa\u00df", "in", "ei\u00b7ner", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von gro\u00dfen gr\u00fcnen Smaragden;", "tokens": ["Von", "gro\u00b7\u00dfen", "gr\u00fc\u00b7nen", "Sma\u00b7rag\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie schimmerten wie Gl\u00fchwurmlicht.", "tokens": ["Sie", "schim\u00b7mer\u00b7ten", "wie", "Gl\u00fch\u00b7wurm\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mehr aber wei\u00df ich nicht.", "tokens": ["Mehr", "a\u00b7ber", "wei\u00df", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es war ganz hinten im Raume", "tokens": ["Es", "war", "ganz", "hin\u00b7ten", "im", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und fast wie in dem Traume,", "tokens": ["Und", "fast", "wie", "in", "dem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der uns der allerliebste ist.", "tokens": ["Der", "uns", "der", "al\u00b7ler\u00b7liebs\u00b7te", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}