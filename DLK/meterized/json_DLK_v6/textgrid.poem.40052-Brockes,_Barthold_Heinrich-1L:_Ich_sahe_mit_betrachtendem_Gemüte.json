{"textgrid.poem.40052": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sahe mit betrachtendem Gem\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe mit betrachtendem Gem\u00fcte", "tokens": ["Ich", "sa\u00b7he", "mit", "be\u00b7trach\u00b7ten\u00b7dem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "J\u00fcngst einen Kirsch-Baum, welcher bl\u00fch'te,", "tokens": ["J\u00fcngst", "ei\u00b7nen", "Kir\u00b7schBaum", ",", "wel\u00b7cher", "bl\u00fch'\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In k\u00fcler Nacht beym Monden-Schein;", "tokens": ["In", "k\u00fc\u00b7ler", "Nacht", "beym", "Mon\u00b7den\u00b7Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich glaubt', es k\u00f6nne nichts von gr\u00f6ss'rer Weisse seyn.", "tokens": ["Ich", "glaubt'", ",", "es", "k\u00f6n\u00b7ne", "nichts", "von", "gr\u00f6ss'\u00b7rer", "Weis\u00b7se", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es schien, ob w\u00e4r' ein Schnee gefallen.", "tokens": ["Es", "schien", ",", "ob", "w\u00e4r'", "ein", "Schnee", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder, auch der klein'ste Ast", "tokens": ["Ein", "je\u00b7der", ",", "auch", "der", "klein'\u00b7ste", "Ast"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Trug gleichsam eine rechte Last", "tokens": ["Trug", "gleich\u00b7sam", "ei\u00b7ne", "rech\u00b7te", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von zierlich-weissen runden Ballen.", "tokens": ["Von", "zier\u00b7lich\u00b7weis\u00b7sen", "run\u00b7den", "Bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es ist kein Schwan so wei\u00df, da nemlich jedes Blat,", "tokens": ["Es", "ist", "kein", "Schwan", "so", "wei\u00df", ",", "da", "nem\u00b7lich", "je\u00b7des", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$,", "KOUS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indem daselbst des Mondes sanftes Licht", "tokens": ["In\u00b7dem", "da\u00b7selbst", "des", "Mon\u00b7des", "sanf\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Selbst durch die zarten Bl\u00e4tter bricht,", "tokens": ["Selbst", "durch", "die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So gar den Schatten wei\u00df und sonder Schw\u00e4rze hat.", "tokens": ["So", "gar", "den", "Schat\u00b7ten", "wei\u00df", "und", "son\u00b7der", "Schw\u00e4r\u00b7ze", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Unm\u00f6glich, dacht' ich, kann auf Erden", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", ",", "dacht'", "ich", ",", "kann", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Was weissers ausgefunden werden.", "tokens": ["Was", "weis\u00b7sers", "aus\u00b7ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Indem ich nun bald hin bald her", "tokens": ["In\u00b7dem", "ich", "nun", "bald", "hin", "bald", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Im Schatten dieses Baumes gehe:", "tokens": ["Im", "Schat\u00b7ten", "die\u00b7ses", "Bau\u00b7mes", "ge\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sah' ich von ungefehr", "tokens": ["Sah'", "ich", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Durch alle Bluhmen in die H\u00f6he", "tokens": ["Durch", "al\u00b7le", "Bluh\u00b7men", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und ward noch einen weissern Schein,", "tokens": ["Und", "ward", "noch", "ei\u00b7nen", "weis\u00b7sern", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der tausend mal so wei\u00df, der tausend mal so klar,", "tokens": ["Der", "tau\u00b7send", "mal", "so", "wei\u00df", ",", "der", "tau\u00b7send", "mal", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADV", "ADV", "VVFIN", "$,", "PRELS", "CARD", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Fast halb darob erstaunt, gewahr.", "tokens": ["Fast", "halb", "da\u00b7rob", "er\u00b7staunt", ",", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Bl\u00fchte Schnee schien schwarz zu seyn", "tokens": ["Der", "Bl\u00fch\u00b7te", "Schnee", "schien", "schwarz", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bey diesem weissen Glanz. Es fiel mir ins Gesicht", "tokens": ["Bey", "die\u00b7sem", "weis\u00b7sen", "Glanz", ".", "Es", "fiel", "mir", "ins", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von einem hellen Stern ein weisses Licht,", "tokens": ["Von", "ei\u00b7nem", "hel\u00b7len", "Stern", "ein", "weis\u00b7ses", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Das mir recht in die Sele stral'te.", "tokens": ["Das", "mir", "recht", "in", "die", "Se\u00b7le", "stral'\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Wie sehr ich mich an GOtt im Irdischen ergetze,", "tokens": ["Wie", "sehr", "ich", "mich", "an", "Gott", "im", "Ir\u00b7di\u00b7schen", "er\u00b7get\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dacht' ich, hat Er dennoch weit gr\u00f6s're Sch\u00e4tze.", "tokens": ["Dacht'", "ich", ",", "hat", "Er", "den\u00b7noch", "weit", "gr\u00f6s'\u00b7re", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.28": {"text": "Die gr\u00f6ste Sch\u00f6nheit dieser Erden", "tokens": ["Die", "gr\u00f6s\u00b7te", "Sch\u00f6n\u00b7heit", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Kann mit der himmlischen doch nicht verglichen werden.", "tokens": ["Kann", "mit", "der", "himm\u00b7li\u00b7schen", "doch", "nicht", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}