{"textgrid.poem.50381": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "38. Zweyfacher Sieg", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leichtlich wird ein Mann vom Gegner \u00fcberwunden,", "tokens": ["Wie", "leicht\u00b7lich", "wird", "ein", "Mann", "vom", "Geg\u00b7ner", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dieser, ", "tokens": ["Wenn", "die\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PDS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Erdr\u00fcck' ihn! Er verdient's; es steht dir alles frey:", "tokens": ["Er\u00b7dr\u00fcck", "ihn", "!", "Er", "ver\u00b7dient's", ";", "es", "steht", "dir", "al\u00b7les", "frey", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wo du ihm vergiebst, so ", "tokens": ["Doch", "wo", "du", "ihm", "ver\u00b7giebst", ",", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}