{"textgrid.poem.61112": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlicher Greis, hab' Dank! Du erquicktest die durstigen Wandrer,", "tokens": ["Freund\u00b7li\u00b7cher", "Greis", ",", "hab'", "Dank", "!", "Du", "er\u00b7quick\u00b7test", "die", "durs\u00b7ti\u00b7gen", "Wand\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Die auf felsigem Steig deiner Behausung genaht.", "tokens": ["Die", "auf", "fel\u00b7si\u00b7gem", "Steig", "dei\u00b7ner", "Be\u00b7hau\u00b7sung", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+---++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Selbst zwar arm, doch ludest du uns in des gr\u00fcnenden Weindachs", "tokens": ["Selbst", "zwar", "arm", ",", "doch", "lu\u00b7dest", "du", "uns", "in", "des", "gr\u00fc\u00b7nen\u00b7den", "Wein\u00b7dachs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Schatten und brachtest uns gern, was du besa\u00dfest, herbei;", "tokens": ["Schat\u00b7ten", "und", "brach\u00b7test", "uns", "gern", ",", "was", "du", "be\u00b7sa\u00b7\u00dfest", ",", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sorglich lasest du selbst im Garten die saftigsten Trauben,", "tokens": ["Sorg\u00b7lich", "la\u00b7sest", "du", "selbst", "im", "Gar\u00b7ten", "die", "saf\u00b7tigs\u00b7ten", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Aus dem erfrischenden Quell sch\u00f6pftest du selber den Trunk.", "tokens": ["Aus", "dem", "er\u00b7fri\u00b7schen\u00b7den", "Quell", "sch\u00f6pf\u00b7test", "du", "sel\u00b7ber", "den", "Trunk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Freundlicher Greis, hab' Dank! Zwar schlugst du das Gegengeschenk aus,", "tokens": ["Freund\u00b7li\u00b7cher", "Greis", ",", "hab'", "Dank", "!", "Zwar", "schlugst", "du", "das", "Ge\u00b7gen\u00b7ge\u00b7schenk", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Aber den segnenden Wunsch halt' ich vergebens zur\u00fcck:", "tokens": ["A\u00b7ber", "den", "seg\u00b7nen\u00b7den", "Wunsch", "halt'", "ich", "ver\u00b7ge\u00b7bens", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "M\u00f6ge der Stock dir bl\u00fchn von den k\u00f6stlichsten Beeren, und t\u00e4glich", "tokens": ["M\u00f6\u00b7ge", "der", "Stock", "dir", "bl\u00fchn", "von", "den", "k\u00f6st\u00b7lichs\u00b7ten", "Bee\u00b7ren", ",", "und", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Streue der Palme Gezweig dichteren Schatten umher.", "tokens": ["Streu\u00b7e", "der", "Pal\u00b7me", "Ge\u00b7zweig", "dich\u00b7te\u00b7ren", "Schat\u00b7ten", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Nimmer versiege der labende Quell, und nimmer im Fasse", "tokens": ["Nim\u00b7mer", "ver\u00b7sie\u00b7ge", "der", "la\u00b7ben\u00b7de", "Quell", ",", "und", "nim\u00b7mer", "im", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADV", "APPRART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Gehe der Weizen dir aus, nimmer im Kruge das \u00d6l;", "tokens": ["Ge\u00b7he", "der", "Wei\u00b7zen", "dir", "aus", ",", "nim\u00b7mer", "im", "Kru\u00b7ge", "das", "\u00d6l", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "PTKVZ", "$,", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Doch uns m\u00f6ge der Wanderer Gott noch oft es gew\u00e4hren,", "tokens": ["Doch", "uns", "m\u00f6\u00b7ge", "der", "Wan\u00b7de\u00b7rer", "Gott", "noch", "oft", "es", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "NN", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Solch ein traulich Gem\u00fct wiederzufinden wie deins!", "tokens": ["Solch", "ein", "trau\u00b7lich", "Ge\u00b7m\u00fct", "wie\u00b7der\u00b7zu\u00b7fin\u00b7den", "wie", "deins", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "NN", "VVFIN", "KOKOM", "NE", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}